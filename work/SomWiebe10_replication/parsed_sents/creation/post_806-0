(ROOT
  (S
    (NP
      (NP (NN Everything))
      (SBAR
        (S
          (NP (PRP I))
          (VP (VBP say)))))
    (VP (VBZ is)
      (ADJP (JJ correct))
      (, ,)
      (ADVP (RB regardless)
        (PP (IN of)
          (NP (NN logic)
            (CC or)
            (NN evidence))))
      (SBAR (IN that)
        (S
          (NP (NNS attempts))
          (VP (TO to)
            (VP (VB prove)
              (S
                (NP (PRP me))
                (ADJP (JJ wrong))))))))
    (. .)))

dobj(say-3, Everything-1)
nsubj(correct-5, Everything-1)
nsubj(say-3, I-2)
rcmod(Everything-1, say-3)
cop(correct-5, is-4)
root(ROOT-0, correct-5)
prep_regardless_of(correct-5, logic-9)
prep_regardless_of(correct-5, evidence-11)
conj_or(logic-9, evidence-11)
complm(prove-15, that-12)
nsubj(prove-15, attempts-13)
aux(prove-15, to-14)
ccomp(correct-5, prove-15)
xcomp(correct-5, prove-15)
nsubj(wrong-17, me-16)
xcomp(prove-15, wrong-17)

