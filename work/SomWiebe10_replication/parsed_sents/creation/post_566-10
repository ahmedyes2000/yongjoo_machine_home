(ROOT
  (S ('' '')
    (PP
      (ADVP (RB LivvyExcept))
      (IN for) (IN in)
      (NP
        (NP (DT the) (JJ fossil) (NN record))
        (, ,)
        (SBAR
          (WHADVP (WRB where))
          (S
            (VP (VBZ transitional)
              (NP (NNS elements)))))))
    (VP (VBP are)
      (ADJP (RB quite) (JJ prominent)))
    (. .)))

advmod(prominent-14, LivvyExcept-2)
dep(prominent-14, for-3)
det(record-7, the-5)
amod(record-7, fossil-6)
prep_in(prominent-14, record-7)
advmod(transitional-10, where-9)
