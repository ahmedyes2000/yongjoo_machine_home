(ROOT
  (S
    (NP (DT That))
    (VP (MD would)
      (VP (VB be)
        (NP (DT an) (JJ interesting) (NN position)
          (S
            (VP (TO to)
              (VP (VB hold)
                (S
                  (VP (VBG considering)
                    (SBAR (IN that)
                      (S
                        (VP (VBZ 's)
                          (SBAR
                            (WHNP (WP what))
                            (S
                              (NP (NN science))
                              (VP (VBZ is) (RB all)
                                (ADJP (IN about))))))))))))))))
    (. .)))

nsubj(position-6, That-1)
aux(position-6, would-2)
cop(position-6, be-3)
det(position-6, an-4)
amod(position-6, interesting-5)
root(ROOT-0, position-6)
aux(hold-8, to-7)
infmod(position-6, hold-8)
xcomp(hold-8, considering-9)
complm('s-11, that-10)
ccomp(considering-9, 's-11)
dobj(about-16, what-12)
nsubj(about-16, science-13)
cop(about-16, is-14)
advmod(about-16, all-15)
ccomp('s-11, about-16)

