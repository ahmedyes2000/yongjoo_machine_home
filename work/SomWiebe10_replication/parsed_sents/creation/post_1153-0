(ROOT
  (S
    (NP (NNP Well) (NN evolution))
    (VP (VBZ is)
      (PP (IN by)
        (NP
          (NP (RB far) (DT the)
            (ADJP (RBS most) (JJ logical))
            (NN idea))
          (SBAR
            (S
              (NP (PRP I))
              (VP (VBP have)
                (VP (VBN heard)
                  (PP (IN about)
                    (SBAR
                      (WHADVP (WRB where))
                      (S
                        (NP (NN man))
                        (VP (VBD came)
                          (PP (IN from)))))))))))))
    (: ...)))

nn(evolution-2, Well-1)
nsubj(is-3, evolution-2)
root(ROOT-0, is-3)
advmod(idea-9, far-5)
det(idea-9, the-6)
advmod(logical-8, most-7)
amod(idea-9, logical-8)
