(ROOT
  (S (CC And)
    (NP (DT that))
    (VP (VBD was)
      (VP (VBG being)
        (ADJP (JJ able)
          (S
            (VP (TO to)
              (VP (VB write)
                (NP (DT a)
                  (ADJP (RB gramatically) (JJ correct))
                  (NN sentence) (NN .5))))))))
    (. .)))

cc(able-5, And-1)
