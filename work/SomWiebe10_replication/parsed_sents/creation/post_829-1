(ROOT
  (S
    (S
      (NP (PRP I))
      (VP (VBP 'm)
        (ADJP (JJ sorry))))
    (CC but)
    (S
      (NP (DT this))
      (VP (VBD took)
        (SBAR
          (S
            (NP (NNS SERIOUS))
            (VP (VBZ cahones)
              (S
                (VP (TO to)
                  (VP (VB write)
                    (PP (IN as)
                      (NP
                        (NP (DT a) (NN defense))
                        (PP (IN of)
                          (NP (PRP$ your) (NNS beliefs)))))))))))))
    (. .)))

nsubj(sorry-3, I-1)
cop(sorry-3, 'm-2)
root(ROOT-0, sorry-3)
nsubj(took-6, this-5)
conj_but(sorry-3, took-6)
