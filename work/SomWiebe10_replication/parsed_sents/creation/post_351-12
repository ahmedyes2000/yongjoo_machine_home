(ROOT
  (S
    (ADVP (RB Thus))
    (NP (JJ general) (JJ non-Gaussian) (NNS detections))
    (VP (MD can)
      (ADVP (RB easily))
      (VP (VB be)
        (VP (VBN dismissed))))
    (. .)))

advmod(dismissed-8, Thus-1)
amod(detections-4, general-2)
amod(detections-4, non-Gaussian-3)
nsubjpass(dismissed-8, detections-4)
aux(dismissed-8, can-5)
advmod(dismissed-8, easily-6)
auxpass(dismissed-8, be-7)
root(ROOT-0, dismissed-8)

