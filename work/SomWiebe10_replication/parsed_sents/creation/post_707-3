(ROOT
  (S
    (S
      (NP (JJ Christian) (NNS archeologists))
      (VP (VBP have)
        (VP (VBN been)
          (VP (VBG searching)
            (PP (IN for)
              (NP
                (NP (JJ verifiable) (NN evidence))
                (PP (IN of)
                  (NP
                    (NP (DT the) (JJ supernatural) (NNS claims))
                    (PP (IN in)
                      (NP (DT the) (NNP Bible)))))))
            (PP (IN for)
              (NP (NNS centuries)))
            (ADVP (RB now))))))
    (CC and)
    (S
      (ADVP (RB so) (RB far))
      (NP (PRP they))
      (VP (VBP have)
        (ADVP (RB either))
        (VP (VBN found)
          (NP
            (NP (DT no) (JJ clear) (NN proof))
            (, ,) (RB only)
            (NP (JJ contradictory) (NN proof))))))
    (. .)))

