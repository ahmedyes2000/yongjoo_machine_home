                  (NP
                    (NP (NNP God))
                    (PP (IN like)
                      (NP (NNS grasshoppers)))))))))))
    (: ...)
    (ADVP (RB well))
    (, ,)
    (S
      (NP (DT that))
      (VP (VBZ is)
        (NP (DT a) (NN metaphor))))
