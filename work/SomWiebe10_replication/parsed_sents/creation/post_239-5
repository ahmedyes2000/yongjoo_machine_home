    (NP
      (NP
        (NP (JJ Intelligent) (NN design) (NN creationism) (POS 's))
        (VBN continued)
        (ADJP (JJ visible))
        (NN support))
      (PP (IN from)
        (NP (JJ fundamentalist) (NNS Christians))))
    (VP (VBZ provides)
      (NP
        (NP (DT an) (NN illustration))
        (PP (IN of)
          (SBAR
            (WHNP (WP what))
            (S
              (NP (PRP$ its) (JJ actual) (NN design))
              (ADVP (RB really))
              (VP (VBZ is)))))))
    (. .)))

