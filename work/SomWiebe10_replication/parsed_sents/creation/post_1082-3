(ROOT
  (S
    (NP (DT A) (JJ human) (NN being))
    (VP (VBZ is)
      (VP (VBN created)
        (PP (IN from)
          (NP (DT a) (NN sperm)))))
    (. .)))

det(being-3, A-1)
amod(being-3, human-2)
nsubjpass(created-5, being-3)
auxpass(created-5, is-4)
root(ROOT-0, created-5)
det(sperm-8, a-7)
prep_from(created-5, sperm-8)

