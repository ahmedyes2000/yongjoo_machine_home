(ROOT
  (S
    (NP (DT This) (JJ entire) (NN argument))
    (VP (VBZ is)
      (VP (VBN based)
        (PP (IN on)
          (NP
            (NP (PRP$ our) (NNS limitations))
            (CC AND)
            (NP (NNS projections))))))
    (. .)))

det(argument-3, This-1)
amod(argument-3, entire-2)
nsubjpass(based-5, argument-3)
auxpass(based-5, is-4)
root(ROOT-0, based-5)
poss(limitations-8, our-7)
prep_on(based-5, limitations-8)
prep_on(based-5, projections-10)
conj_and(limitations-8, projections-10)

