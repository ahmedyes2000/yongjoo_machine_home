(ROOT
  (S
    (NP (PRP You))
    (VP (VBP claim)
      (SBAR (IN that)
        (S
          (NP (PRP it))
          (VP (VBZ makes)
            (NP (JJ mathematical) (NN sense)))))
      (, ,)
      (S (RB then)
        (VP (VB prove)
          (NP (PRP it)))))
    (. !)))

nsubj(claim-2, You-1)
root(ROOT-0, claim-2)
complm(makes-5, that-3)
nsubj(makes-5, it-4)
ccomp(claim-2, makes-5)
amod(sense-7, mathematical-6)
dobj(makes-5, sense-7)
advmod(prove-10, then-9)
ccomp(claim-2, prove-10)
dobj(prove-10, it-11)

