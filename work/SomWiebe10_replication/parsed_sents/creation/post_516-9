(ROOT
  (S
    (NP (PRP They))
    (ADVP (RB simply))
    (VP (VBP want)
      (S
        (VP (TO to)
          (VP (VB show)
            (SBAR (IN that)
              (S
                (NP (EX there))
                (VP (VBZ is)
                  (NP
                    (NP (JJ good) (JJ scientific) (NN evidence))
                    (PP (IN for)
                      (S
                        (VP (VBG considering)
                          (NP
                            (NP (DT the) (NN role))
                            (PP (IN of)
                              (NP (NN intelligence))))
                          (PP (IN in)
                            (NP
                              (NP (DT the) (NN emergence))
                              (PP (IN of)
                                (NP (NN life))))))))))))))))
    (. .)))

nsubj(want-3, They-1)
xsubj(show-5, They-1)
advmod(want-3, simply-2)
root(ROOT-0, want-3)
aux(show-5, to-4)
xcomp(want-3, show-5)
complm(is-8, that-6)
expl(is-8, there-7)
ccomp(show-5, is-8)
amod(evidence-11, good-9)
amod(evidence-11, scientific-10)
nsubj(is-8, evidence-11)
prepc_for(evidence-11, considering-13)
det(role-15, the-14)
dobj(considering-13, role-15)
prep_of(role-15, intelligence-17)
det(emergence-20, the-19)
prep_in(considering-13, emergence-20)
prep_of(emergence-20, life-22)

