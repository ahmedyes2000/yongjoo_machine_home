(ROOT
  (S
    (S
      (NP (EX There))
      (VP (VBP are)
        (NP (CD two))))
    (, ,)
    (CC and)
    (S
      (NP (PRP they))
      (VP (VBP are)
        (ADJP (RB fundamentally) (JJ different))))
    (. .)))

expl(are-2, There-1)
root(ROOT-0, are-2)
nsubj(are-2, two-3)
nsubj(different-9, they-6)
cop(different-9, are-7)
advmod(different-9, fundamentally-8)
conj_and(are-2, different-9)

