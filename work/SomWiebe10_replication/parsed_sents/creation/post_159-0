(ROOT
  (NP (: --)
    (NP
      (NP (NNS Taxibound2))
      (: --)
      (S
        (NP (PRP I))
        (VP (MD must)
          (VP (VB say)
            (SBAR (IN that)
              (S
                (NP (NNP PvM))
                (VP (VBZ has)
                  (NP
                    (NP (DT a) (NN way))
                    (PP (IN with)
                      (NP (NNS words)))))))))))
    (. .)))

root(ROOT-0, Taxibound2-2)
nsubj(say-6, I-4)
aux(say-6, must-5)
dep(Taxibound2-2, say-6)
complm(has-9, that-7)
nsubj(has-9, PvM-8)
