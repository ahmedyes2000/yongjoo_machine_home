    (NP
      (NP (DT The) (NN evidence))
      (SBAR
        (WHNP (WDT that))
        (S
          (VP (VBZ supports)
            (NP (NN evolution))))))
    (VP (VBZ is) (RB not)
      (ADJP
        (ADJP (RB only) (RB significantly) (JJ abundant))
        (CC but)
        (ADJP (RB predominately) (JJ irrefutable))))
    (. .)))

