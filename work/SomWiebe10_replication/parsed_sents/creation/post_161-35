(ROOT
  (S
    (SBAR
      (WHADVP (WRB When))
      (S
        (VP (VBN held)
          (PP (TO to)
            (NP
              (NP (DT the) (NNS standards))
              (PP (IN of)
                (NP
                  (NP (NN proof))
                  (VP (VBN required)
                    (PP (IN for)
                      (NP (DT all) (JJ other) (JJ scientific) (NNS theories)))))))))))
    (, ,)
    (NP (NNP ID))
    (VP (VBZ fails))
    (. .)))

advmod(held-2, When-1)
advcl(fails-16, held-2)
det(standards-5, the-4)
prep_to(held-2, standards-5)
prep_of(standards-5, proof-7)
partmod(proof-7, required-8)
det(theories-13, all-10)
amod(theories-13, other-11)
amod(theories-13, scientific-12)
prep_for(required-8, theories-13)
nsubj(fails-16, ID-15)
root(ROOT-0, fails-16)

