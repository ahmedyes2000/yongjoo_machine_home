(ROOT
  (S
    (NP (PRP They))
    (VP (VBP keep)
      (SBAR
        (S
          (S
            (VP (VBG saying) ('' '')
              (NP (NNS Scientists))))
          (VP (MD could)
            (VP (VB be)
              (ADJP (JJ wrong) ('' '')))))))
    (. .)))

nsubj(keep-2, They-1)
root(ROOT-0, keep-2)
csubj(wrong-8, saying-3)
dobj(saying-3, Scientists-5)
aux(wrong-8, could-6)
cop(wrong-8, be-7)
ccomp(keep-2, wrong-8)

