(ROOT
  (S
    (NP (NNP God))
    (VP (VBZ is)
      (ADVP (RB just))
      (NP
        (NP (DT an) (JJ imaginary) (NN friend))
        (PP (IN for)
          (NP (NNS adults)))))))

nsubj(friend-6, God-1)
cop(friend-6, is-2)
