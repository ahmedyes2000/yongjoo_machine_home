(ROOT
  (NP
    (NP (NN Use))
    (PP (IN of)
      (NP (NN god)
        (CC or)
        (NN supernatural)))
    (PP (IN as)
      (NP (DT the) (JJ primary) (NN explanation)))
    (. .)))

root(ROOT-0, Use-1)
prep_of(Use-1, god-3)
prep_of(Use-1, supernatural-5)
conj_or(god-3, supernatural-5)
det(explanation-9, the-7)
amod(explanation-9, primary-8)
prep_as(Use-1, explanation-9)

