(ROOT
  (S
    (NP (PRP I))
    (VP (MD can) (RB not)
      (VP
        (VP (VB go)
          (PP (IN into)
            (NP (NN school)))
          (NP (NN tomorrow)))
        (CC and)
        (VP
          (VP (VB tell)
            (NP
              (NP (PDT all) (DT the) (NNS children))
              (PP (IN in)
                (NP (DT the) (NN class)))))
          (CC and)
          (VP (VB express)
            (NP (JJ political) (NNS opinions))))))
    (. .)))

nsubj(go-4, I-1)
nsubj(tell-9, I-1)
aux(go-4, can-2)
neg(go-4, not-3)
root(ROOT-0, go-4)
prep_into(go-4, school-6)
tmod(go-4, tomorrow-7)
conj_and(go-4, tell-9)
predet(children-12, all-10)
det(children-12, the-11)
