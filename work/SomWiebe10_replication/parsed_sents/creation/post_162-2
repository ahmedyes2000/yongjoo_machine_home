(ROOT
  (S
    (PP (IN As)
      (NP (JJ such)))
    (NP (JJ evolutionary) (NN theory))
    (VP (VBZ has)
      (VP (VBN done)
        (ADVP (RB quite) (RB well))))
    (. .)))

prep_such_as(done-6, such-2)
amod(theory-4, evolutionary-3)
nsubj(done-6, theory-4)
aux(done-6, has-5)
root(ROOT-0, done-6)
advmod(well-8, quite-7)
advmod(done-6, well-8)

