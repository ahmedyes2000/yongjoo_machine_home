(ROOT
  (S
    (NP (PRP I))
    (VP (VBD found)
      (S
        (NP (PRP myself))
        (ADJP (JJ wrong)
          (PP (IN on)
            (NP (NN everything))))))
    (. .)))

nsubj(found-2, I-1)
root(ROOT-0, found-2)
nsubj(wrong-4, myself-3)
xcomp(found-2, wrong-4)
prep_on(wrong-4, everything-6)

