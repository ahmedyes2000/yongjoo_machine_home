(ROOT
  (S
    (NP (RB Only) (NNPS Darwinists))
    (VP (VBP want)
      (S
        (VP (TO to)
          (VP (VB censor)
            (NP (DT the) (NN debate))))))
    (. .)))

advmod(Darwinists-2, Only-1)
nsubj(want-3, Darwinists-2)
xsubj(censor-5, Darwinists-2)
root(ROOT-0, want-3)
aux(censor-5, to-4)
xcomp(want-3, censor-5)
det(debate-7, the-6)
dobj(censor-5, debate-7)

