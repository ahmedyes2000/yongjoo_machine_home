(ROOT
  (S
    (NP (PRP I))
    (VP (VBP suggest)
      (S
        (NP (PRP you))
        (VP (TO to)
          (VP (VB read)
            (NP
              (NP (DT the) (NNP Bible))
              (PP (IN in)
                (NP (PRP$ its) (NN entirety))))))))
    (. .)))

nsubj(suggest-2, I-1)
root(ROOT-0, suggest-2)
nsubj(read-5, you-3)
aux(read-5, to-4)
xcomp(suggest-2, read-5)
det(Bible-7, the-6)
dobj(read-5, Bible-7)
poss(entirety-10, its-9)
prep_in(Bible-7, entirety-10)

