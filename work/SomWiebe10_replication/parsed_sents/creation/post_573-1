(ROOT
  (S ('' '')
    (ADVP (RB Possibly))
    (, ,)
    (ADVP (RB though))
    (NP (PRP it))
    (VP (VBZ depends)
      (PP (IN on)
        (NP (NN you) (NN definition))))
    (. .)))

advmod(depends-6, Possibly-2)
advmod(depends-6, though-4)
nsubj(depends-6, it-5)
root(ROOT-0, depends-6)
nn(definition-9, you-8)
prep_on(depends-6, definition-9)

