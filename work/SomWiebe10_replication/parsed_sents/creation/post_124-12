(ROOT
  (S
    (VP (VBG '') (`` `)
      (SBAR
        (SBARQ
          (WHADVP (WRB how))
          (SQ (MD would)
            (NP (DT the) (NN judge))
            (VP (VB know)
              (NP (DT that)))))))
    (: ...)))

advmod(know-7, how-3)
aux(know-7, would-4)
det(judge-6, the-5)
