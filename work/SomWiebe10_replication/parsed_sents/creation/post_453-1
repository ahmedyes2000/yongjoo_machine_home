  (S
    (PP (IN As)
      (NP (DT a) (JJ Christian) (NN myself)))
    (, ,)
    (NP (PRP I))
    (VP (VBP have)
      (NP
        (NP (DT a) (JJ hard) (NN time) (NN understanding))
        (SBAR
          (WHADVP (WRB why))
          (S (`` ``)
            (NP (NN bait)
              (CC and)
              (NN switch))
            ('' '')
            (VP (MD would)
              (VP (VB further)
                (NP (DT a) (JJ Christian) (NN message))))))))
    (. .)))

