(ROOT
  (S (CC And)
    (NP (PRP I))
    (VP (VBP say)
      (SBAR
        (S
          (NP (DT the) (NN Earth))
          (VP (VBD was) (RB n't)
            (VP (VBN created)
              (PP (IN in)
                (NP (CD six) (NNS days))))))))
    (. .)))

cc(say-3, And-1)
nsubj(say-3, I-2)
root(ROOT-0, say-3)
det(Earth-5, the-4)
nsubjpass(created-8, Earth-5)
auxpass(created-8, was-6)
neg(created-8, n't-7)
ccomp(say-3, created-8)
num(days-11, six-10)
prep_in(created-8, days-11)

