  (S
    (NP (DT A) (NNS hypothesis))
    (VP (VBZ is)
      (NP
        (NP
          (NP (DT an) (NN assumption))
          (SBAR
            (WHNP (WDT that))
            (S
              (VP (VBZ is) (RB not)
                (VP (VBN taken)
                  (PP (IN as) (`` ``)
                    (NP (NN truth))
                    ('' '')))))))
        (, ,)
        (CONJP (CC but)
          (RB as))
        (NP (NN speculation))))
    (. .)))

det(hypothesis-2, A-1)
nsubj(assumption-5, hypothesis-2)
cop(assumption-5, is-3)
det(assumption-5, an-4)
