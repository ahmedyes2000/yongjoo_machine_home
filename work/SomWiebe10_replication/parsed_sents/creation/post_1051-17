(ROOT
  (S
    (ADVP (RB Well))
    (, ,)
    (NP
      (NP (PRP$ its)
        (ADJP
          (QP (RB just) (RB as)))
        (NN faith))
      (PP (VBN based)
        (PP (IN as)
          (NP (NN evolution)))))
    (VP (VBZ is))
    (. .)))

advmod(is-10, Well-1)
poss(faith-6, its-3)
quantmod(as-5, just-4)
num(faith-6, as-5)
nsubj(is-10, faith-6)
prep(faith-6, based-7)
pcomp(based-7, as-8)
prep_as(based-7, evolution-9)
root(ROOT-0, is-10)

(ROOT
  (NP (NN No.)))

root(ROOT-0, No.-1)

