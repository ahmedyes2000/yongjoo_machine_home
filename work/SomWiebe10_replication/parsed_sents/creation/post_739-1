(ROOT
  (S
    (NP (DT The) (NNP Bible))
    (VP (VBZ says)
      (SBAR (IN that)
        (S
          (NP (DT the) (NNPS Jews))
          (VP (VBP are)
            (NP (PRP$ his) (VBN chosen) (NNS people))))))
    (. .)))

det(Bible-2, The-1)
nsubj(says-3, Bible-2)
root(ROOT-0, says-3)
complm(people-10, that-4)
det(Jews-6, the-5)
nsubj(people-10, Jews-6)
dep(people-10, are-7)
poss(people-10, his-8)
amod(people-10, chosen-9)
ccomp(says-3, people-10)

