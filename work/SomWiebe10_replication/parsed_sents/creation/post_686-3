(ROOT
  (S
    (ADVP (IN Before))
    (, ,)
    (NP (NNP God) (-LRB- -LRB-)
      (NP (NNS gods))
      (-RRB- -RRB-))
    (VP (VBD were)
      (VP (VBN caught)
        (PP (IN in)
          (NP
            (NP (DT the) (NN web))
            (PP (IN of)
              (NP (NN circumstance)))))
        (PP (IN as)
          (ADVP (RB surely)))
        (PP (IN as)
          (NP (NN man)))))
    (. .)))

advmod(caught-8, Before-1)
nsubjpass(caught-8, God-3)
dep(God-3, gods-5)
auxpass(caught-8, were-7)
root(ROOT-0, caught-8)
det(web-11, the-10)
prep_in(caught-8, web-11)
prep_of(web-11, circumstance-13)
prep(caught-8, as-14)
pcomp(as-14, surely-15)
prep_as(caught-8, man-17)

(ROOT
  (S
    (PP (IN By)
      (S
        (VP (VBG taking)
          (NP
            (NP (NNP God))
            (PP (IN as)
              (NP
                (NP (DT an) (JJ outside) (NN force))
                (VP (VBG creating)
                  (NP (DT all)))))))))
    (, ,)
    (NP (DT the) (NNPS Hebrews))
    (VP (VBD set)
      (NP (DT a) (JJ new) (NN tone)))
    (. .)))

prepc_by(set-13, taking-2)
dobj(taking-2, God-3)
det(force-7, an-5)
