(ROOT
  (S
    (NP (DT All))
    (SBAR (IN because)
      (S
        (NP (EX there))
        (VP (VBZ exists)
          (NP
            (NP (DT a)
              (ADJP (RB well) (VBN established))
              (NN trail))
            (PP (IN of)
              (NP
                (NP (NN evidence))
                (VP (VBG linking)
                  (NP (PRP them))
                  (ADVP (DT all) (RB together)))))))))
    (. .)))

dep(exists-4, All-1)
mark(exists-4, because-2)
expl(exists-4, there-3)
root(ROOT-0, exists-4)
det(trail-8, a-5)
advmod(established-7, well-6)
amod(trail-8, established-7)
dobj(exists-4, trail-8)
nsubj(exists-4, trail-8)
prep_of(trail-8, evidence-10)
partmod(evidence-10, linking-11)
dobj(linking-11, them-12)
dep(together-14, all-13)
advmod(linking-11, together-14)

