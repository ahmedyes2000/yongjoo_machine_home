(ROOT
  (S (CC but)
    (ADVP (RB really))
    (NP (PRP I))
    (VP (MD ca) (RB n't)
      (VP (VB devote)
        (NP (PRP$ my) (NN life))
        (PP (TO to)
          (NP (DT this) (NN conversation)))))
    (: ...)))

cc(devote-6, but-1)
advmod(devote-6, really-2)
nsubj(devote-6, I-3)
aux(devote-6, ca-4)
neg(devote-6, n't-5)
root(ROOT-0, devote-6)
poss(life-8, my-7)
