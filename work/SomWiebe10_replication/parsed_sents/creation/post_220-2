(ROOT
  (S
    (SBAR (IN Once)
      (S
        (NP (JJ prolific) (NNS scientists))
        (VP (VBP become)
          (NP (NNP ID) (NNS advocates)))))
    (, ,)
    (NP (PRP they))
    (VP (VBP stop)
      (S
        (VP (VBG producing)
          (NP
            (NP (NN anything))
            (PP (IN of)
              (NP (NN merit)))))))
    (. .)))

