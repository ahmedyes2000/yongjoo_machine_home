(ROOT
  (S
    (VP
      (VP
        (VP (VB \/) (-RRB- -RRB-)
          (NP (NNS \/))
          (-RRB- -RRB-))
        (SBAR (IN If)
          (S
            (SBAR
              (WHADVP (RB not) (WRB why))
              (S
                (VP (VBD did)
                  (NP (PRP$ your) (NN god)))))
            (VP (VBD created)
              (NP (PRP it))))))
      (, ,)
      (SBAR (IN if)
        (S
          (NP (PRP it))
          (VP (VBZ has)
            (NP
              (NP (DT no) (NN purpose))
              (CC and)
              (NP (DT no) (NN use)))))))
    (. .)))

root(ROOT-0, \/-1)
dobj(\/-1, \/-3)
mark(created-11, If-5)
dep(why-7, not-6)
advmod(did-8, why-7)
advcl(created-11, did-8)
