poss(god-10, your-9)
dobj(did-8, god-10)
advcl(\/-1, created-11)
dobj(created-11, it-12)
mark(has-16, if-14)
nsubj(has-16, it-15)
advcl(\/-1, has-16)
det(purpose-18, no-17)
dobj(has-16, purpose-18)
det(use-21, no-20)
