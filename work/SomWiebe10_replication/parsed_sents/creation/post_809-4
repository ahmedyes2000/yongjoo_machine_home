(ROOT
  (S
    (SBAR (RB Once) (IN upon)
      (S
        (NP (DT a) (NN time) (, ,) (JJ intelligent) (NN design))
        (VP (VBD did))))
    (VP (VBD made)
      (NP (DT a) (NN prediction))
      (SBAR (IN that)
        (S
          (NP (PRP we))
          (VP (MD could)
            (VP (VB measure)
              (CC and)
              (VB verify))))))
    (. .)))

advmod(did-8, Once-1)
dep(did-8, upon-2)
det(design-7, a-3)
nn(design-7, time-4)
amod(design-7, intelligent-6)
nsubj(did-8, design-7)
advcl(made-9, did-8)
csubj(made-9, did-8)
root(ROOT-0, made-9)
det(prediction-11, a-10)
dobj(made-9, prediction-11)
complm(measure-15, that-12)
nsubj(measure-15, we-13)
nsubj(verify-17, we-13)
aux(measure-15, could-14)
ccomp(made-9, measure-15)
ccomp(made-9, verify-17)
conj_and(measure-15, verify-17)

