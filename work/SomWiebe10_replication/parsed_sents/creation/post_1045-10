(ROOT
  (S
    (ADVP (IN So) (RB there))
    (, ,)
    (S
      (NP (PRP it))
      (VP (VBZ 's) (RB not)
        (NP (NNS feathers))))
    (CC but)
    (S
      (NP (PRP it))
      (VP (VBZ is)
        (NP (NN evolution))))
    (. .)))

advmod(feathers-7, So-1)
advmod(So-1, there-2)
nsubj(feathers-7, it-4)
cop(feathers-7, 's-5)
neg(feathers-7, not-6)
root(ROOT-0, feathers-7)
nsubj(evolution-11, it-9)
cop(evolution-11, is-10)
conj_but(feathers-7, evolution-11)

