(ROOT
  (S
    (S
      (NP (PRP I))
      (VP (MD could)
        (VP (VB go)
          (PRT (RP on)))))
    (, ,)
    (CC but)
    (S
      (NP (DT this) (NN argument))
      (VP (VBZ is)
        (ADVP (RB already))
        (ADJP (RB too) (JJ long))))
    (. .)))

nsubj(go-3, I-1)
aux(go-3, could-2)
root(ROOT-0, go-3)
prt(go-3, on-4)
det(argument-8, this-7)
nsubj(long-12, argument-8)
cop(long-12, is-9)
advmod(long-12, already-10)
advmod(long-12, too-11)
conj_but(go-3, long-12)

