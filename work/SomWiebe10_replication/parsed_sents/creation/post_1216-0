(ROOT
  (SBARQ
    (WHNP (WP Who))
    (SQ
      (VP (VBZ enters)
        (PP (IN into)
          (NP
            (NP (DT a) (JJ scientific) (NN debate))
            (PP (IN about)
              (NP (NN evolution)))))
        (S
          (VP (VBG asking)
            (ADVP (RB only))
            (PP (IN for)
              (NP (NNS opinions)))))))
    (. ?)))

nsubj(enters-2, Who-1)
root(ROOT-0, enters-2)
det(debate-6, a-4)
amod(debate-6, scientific-5)
prep_into(enters-2, debate-6)
prep_about(debate-6, evolution-8)
xcomp(enters-2, asking-9)
advmod(asking-9, only-10)
prep_for(asking-9, opinions-12)

