(ROOT
  (S ('' '')
    (NP (PRP I))
    (ADVP (RB neither))
    (VP
      (VP (VBZ worship)
        (NP (NNP Darwin)))
      (CC nor)
      (VP (VBP accept)
        (NP (PRP$ his) (NNS writings))
        (PP (IN as)
          (NP (JJ absolute) (NN truth)))))
    (. .)))

nsubj(worship-4, I-2)
nsubj(accept-7, I-2)
advmod(worship-4, neither-3)
root(ROOT-0, worship-4)
dobj(worship-4, Darwin-5)
conj_nor(worship-4, accept-7)
poss(writings-9, his-8)
dobj(accept-7, writings-9)
amod(truth-12, absolute-11)
prep_as(accept-7, truth-12)

