(ROOT
  (S
    (NP (PRP$ Your) (NN methodology))
    (VP (VBZ has)
      (VP (VBN infected)
        (NP (PRP$ your) (NN worldview))))
    (. .)))

poss(methodology-2, Your-1)
nsubj(infected-4, methodology-2)
aux(infected-4, has-3)
root(ROOT-0, infected-4)
poss(worldview-6, your-5)
dobj(infected-4, worldview-6)

