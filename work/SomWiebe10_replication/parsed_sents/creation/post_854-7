(ROOT
  (S
    (NP (DT This))
    (VP (VBD left)
      (S
        (NP (PRP me))
        (VP (VBG wondering)
          (PP (IN about)
            (NP
              (NP (DT the) (NN motivation))
              (PP (IN of)
                (NP
                  (NP (NNS organizations))
                  (PP (IN like)
                    (NP (NNP AiG))))))))))
    (. .)))

nsubj(left-2, This-1)
root(ROOT-0, left-2)
nsubj(wondering-4, me-3)
dep(left-2, wondering-4)
det(motivation-7, the-6)
prep_about(wondering-4, motivation-7)
prep_of(motivation-7, organizations-9)
prep_like(organizations-9, AiG-11)

