(ROOT
  (S ('' '')
    (S
      (NP (CD One) (NN study))
      (VP (MD would) (RB not)
        (VP (VB be)
          (ADJP (JJ enough)))))
    (: ...)
    (CC but)
    (S
      (SBAR
        (WHADVP (WRB when))
        (S
          (VP (VBN combined)
            (PP (IN with)
              (NP
                (NP (DT the) (NN body))
                (PP (IN of)
                  (NP
                    (NP (NN evidence))
                    (PP (IN for)
                      (NP (NNP Evolution))))))))))
      (NP (PRP it))
      (VP (VBZ is)
        (NP
          (NP (DT a)
            (ADJP (RB very) (JJ powerful))
            (NN demonstration))
          (PP (IN of)
            (NP (DT the) (NN change))))))
    (. .)))

num(study-3, One-2)
nsubj(enough-7, study-3)
aux(enough-7, would-4)
neg(enough-7, not-5)
cop(enough-7, be-6)
root(ROOT-0, enough-7)
advmod(combined-11, when-10)
advcl(demonstration-24, combined-11)
det(body-14, the-13)
prep_with(combined-11, body-14)
prep_of(body-14, evidence-16)
prep_for(evidence-16, Evolution-18)
nsubj(demonstration-24, it-19)
cop(demonstration-24, is-20)
det(demonstration-24, a-21)
advmod(powerful-23, very-22)
amod(demonstration-24, powerful-23)
conj_but(enough-7, demonstration-24)
det(change-27, the-26)
prep_of(demonstration-24, change-27)

