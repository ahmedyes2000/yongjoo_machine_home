(ROOT
  (S (: --)
    (NP (NNP Joe) (NNP GI))
    (ADVP (RB never))
    (VP (VBD said)
      (NP (NN anything))
      (PP (IN about)
        (NP
          (NP (NNP Einstein) (POS 's))
          (JJ religious) (NNS convictions))))
    (. .)))

nn(GI-3, Joe-2)
nsubj(said-5, GI-3)
neg(said-5, never-4)
root(ROOT-0, said-5)
dobj(said-5, anything-6)
poss(convictions-11, Einstein-8)
amod(convictions-11, religious-10)
prep_about(said-5, convictions-11)

