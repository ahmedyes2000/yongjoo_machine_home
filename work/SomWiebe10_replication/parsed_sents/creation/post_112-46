(ROOT
  (NP
    (NP
      (QP (CC or)
        (DT a) (NN mentioning)))
    (PP (IN of)
      (NP
        (NP (DT the) (NN world))
        (VP (VBG hanging)
          (PP (IN on)
            (NP (NN nothing))))))
    (. .)))

