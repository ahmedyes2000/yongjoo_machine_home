(ROOT
  (SQ (VBZ Is)
    (NP (DT this))
    (NP
      (NP (DT the) (NN conclusion))
      (PP (IN of)
        (NP (DT a) (JJ scientific) (JJ peer-reviewed) (NN study))))
    (. ?)))

cop(conclusion-4, Is-1)
nsubj(conclusion-4, this-2)
det(conclusion-4, the-3)
root(ROOT-0, conclusion-4)
det(study-9, a-6)
amod(study-9, scientific-7)
amod(study-9, peer-reviewed-8)
prep_of(conclusion-4, study-9)

