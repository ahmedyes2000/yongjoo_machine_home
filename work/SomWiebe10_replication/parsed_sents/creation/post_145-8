(ROOT
  (S
    (NP (PRP I))
    (VP (VBP 've)
      (VP (VBN found)
        (NP (NN purpose)
          (CC and)
          (NN happiness))
        (ADVP (RB too))))
    (. .)))

nsubj(found-3, I-1)
aux(found-3, 've-2)
root(ROOT-0, found-3)
dobj(found-3, purpose-4)
dobj(found-3, happiness-6)
conj_and(purpose-4, happiness-6)
advmod(found-3, too-7)

