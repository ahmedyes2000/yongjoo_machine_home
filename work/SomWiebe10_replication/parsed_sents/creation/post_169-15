(ROOT
  (S ('' '')
    (NP (DT That))
    (VP (MD would)
      (ADVP (RB pretty) (RB much))
      (VP (VB apply)
        (PP (TO to)
          (NP (NN anything)))
        (NP
          (NP (RB else) (JJ outside))
          (PP (IN of)
            (NP (JJ molecular) (NNS physics))))))
    (. .)))

nsubj(apply-6, That-2)
aux(apply-6, would-3)
advmod(much-5, pretty-4)
advmod(apply-6, much-5)
root(ROOT-0, apply-6)
prep_to(apply-6, anything-8)
advmod(outside-10, else-9)
dobj(apply-6, outside-10)
amod(physics-13, molecular-12)
prep_of(outside-10, physics-13)

