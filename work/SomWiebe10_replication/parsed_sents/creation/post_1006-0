(ROOT
  (FRAG
    (PP (RB Just) (IN like)
      (NP
        (NP
          (NP (NNP Pluto))
          (VP (VBG orbiting)
            (NP (DT the) (NN sun))
            (ADVP (RB completely))))
        (CC and)
        (NP (NN gravity))))
    (. !)))

advmod(like-2, Just-1)
root(ROOT-0, like-2)
pobj(like-2, Pluto-3)
partmod(Pluto-3, orbiting-4)
det(sun-6, the-5)
dobj(orbiting-4, sun-6)
advmod(orbiting-4, completely-7)
pobj(like-2, gravity-9)
conj_and(Pluto-3, gravity-9)

