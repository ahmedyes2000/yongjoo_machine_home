(ROOT
  (S (RB Then)
    (ADVP (RB again))
    (, ,)
    (NP (PRP I))
    (ADVP (RB often))
    (VP (VBD wondered)
      (PP (IN as)
        (NP (DT a) (NN child)))
      (SBAR
        (WHADVP (WRB why)
          (CC and)
          (WRB how))
        (S
          (NP (DT the) (JJ whole) (NN universe))
          (VP (VBD was)
            (VP (VBN created)
              (PP (IN in)
                (NP (DT the) (JJ first) (NN place))))))))
    (. .)))

advmod(wondered-6, Then-1)
advmod(wondered-6, again-2)
nsubj(wondered-6, I-4)
advmod(wondered-6, often-5)
root(ROOT-0, wondered-6)
det(child-9, a-8)
prep_as(wondered-6, child-9)
advmod(created-17, why-10)
dep(why-10, and-11)
dep(why-10, how-12)
det(universe-15, the-13)
amod(universe-15, whole-14)
nsubjpass(created-17, universe-15)
auxpass(created-17, was-16)
advcl(wondered-6, created-17)
ccomp(wondered-6, created-17)
det(place-21, the-19)
amod(place-21, first-20)
prep_in(created-17, place-21)

