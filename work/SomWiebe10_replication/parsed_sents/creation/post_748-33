(ROOT
  (S
    (NP (DT This))
    (VP (VBZ is) (RB not)
      (ADVP (RB even))
      (PP (IN in)
        (NP (DT the) (JJ same) (NN category))))
    (. .)))

nsubj(is-2, This-1)
root(ROOT-0, is-2)
neg(is-2, not-3)
advmod(is-2, even-4)
det(category-8, the-6)
amod(category-8, same-7)
prep_in(is-2, category-8)

