(ROOT
  (S
    (NP (PRP I))
    (VP (VBP believe)
      (SBAR
        (S
          (NP (DT this))
          (VP (VBD happened)
            (ADVP (RB just))
            (PP (IN by)
              (NP (NN accident)))
            (PP (IN like)
              (NP (NN evolution)))))))
    (. .)))

nsubj(believe-2, I-1)
root(ROOT-0, believe-2)
nsubj(happened-4, this-3)
ccomp(believe-2, happened-4)
advmod(happened-4, just-5)
prep_by(happened-4, accident-7)
prep_like(happened-4, evolution-9)

