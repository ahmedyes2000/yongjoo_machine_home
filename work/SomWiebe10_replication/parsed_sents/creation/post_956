(ROOT
  (S
    (NP
      (NP (DT The) (NN theory))
      (PP (IN behind)
        (NP (NN evolution))))
    (VP (VBZ is) (RB not)
      (SBAR
        (SBAR (IN that)
          (S
            (NP (DT an) (NN organism))
            (VP (VBZ evolves)
              (PP (IN during)
                (NP (PRP$ its) (NN lifetime))))))
        (, ,)
        (CC but)
        (SBAR (IN that)
          (S
            (NP (DT the) (NNS species))
            (VP (VBZ evolves)
              (PP (IN over)
                (NP
                  (NP
                    (NP (NNS thousands))
                    (, ,)
                    (CONJP (IN if) (RB not))
                    (NP (NNS millions))
                    (, ,))
                  (PP (IN of)
                    (NP (NNS years)
                      (CC or)
                      (NNS generations))))))))))
    (. .)))

det(theory-2, The-1)
nsubj(is-5, theory-2)
prep_behind(theory-2, evolution-4)
root(ROOT-0, is-5)
neg(is-5, not-6)
complm(evolves-10, that-7)
det(organism-9, an-8)
nsubj(evolves-10, organism-9)
ccomp(is-5, evolves-10)
poss(lifetime-13, its-12)
prep_during(evolves-10, lifetime-13)
complm(evolves-19, that-16)
det(species-18, the-17)
nsubj(evolves-19, species-18)
ccomp(is-5, evolves-19)
conj_but(evolves-10, evolves-19)
prep_over(evolves-19, thousands-21)
prep_over(evolves-19, millions-25)
conj_negcc(thousands-21, millions-25)
prep_of(thousands-21, years-28)
prep_of(thousands-21, generations-30)
conj_or(years-28, generations-30)

(ROOT
  (S
    (NP (NNP Evolution))
    (VP (VBZ takes)
      (NP
        (NP (NN time))
        (, ,)
        (NP
          (NP (NN survival))
          (PP (IN of)
            (NP (DT the) (JJS fittest))))
        (, ,)
        (CC and)
        (NP (JJ genetic) (NN variation)))
      (, ,)
      (PP (IN among)
        (NP (JJ other) (NNS things))))
    (. .)))

nsubj(takes-2, Evolution-1)
root(ROOT-0, takes-2)
dobj(takes-2, time-3)
dobj(takes-2, survival-5)
conj_and(time-3, survival-5)
det(fittest-8, the-7)
