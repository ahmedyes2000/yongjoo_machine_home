(ROOT
  (S (IN So)
    (NP (JJ much) (NN pressure))
    (VP (VBD was)
      (VP (VBN put)
        (PP (IN on)
          (NP (DT the) (NN magazine)))
        (SBAR (IN that)
          (S
            (NP (PRP they))
            (VP (VBD promised)
              (ADVP (RB never))
              (S
                (VP (TO to)
                  (VP (VB publish)
                    (NP (PDT such) (DT a) (NN piece))
                    (ADVP (RB again))))))))))
    (. .)))

dep(put-5, So-1)
amod(pressure-3, much-2)
nsubjpass(put-5, pressure-3)
auxpass(put-5, was-4)
root(ROOT-0, put-5)
det(magazine-8, the-7)
prep_on(put-5, magazine-8)
complm(promised-11, that-9)
nsubj(promised-11, they-10)
xsubj(publish-14, they-10)
ccomp(put-5, promised-11)
neg(promised-11, never-12)
aux(publish-14, to-13)
xcomp(promised-11, publish-14)
predet(piece-17, such-15)
det(piece-17, a-16)
dobj(publish-14, piece-17)
advmod(publish-14, again-18)

