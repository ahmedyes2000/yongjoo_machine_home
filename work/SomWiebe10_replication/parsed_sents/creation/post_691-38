(ROOT
  (S
    (NP (DT The) (NN laity))
    (VP (TO to)
      (VP (VB have)
        (NP (NN faith))
        (PP (IN in)
          (NP
            (NP (DT the) (NNS deacons))
            (, ,)
            (NP (DT the) (NNS deacons))))
        (S
          (VP (TO to)
            (VP (VB have)
              (NP
                (NP (NN faith))
                (PP (IN in)
                  (NP (DT the) (NNS bishops)))))))))
    (. .)))

det(laity-2, The-1)
nsubj(have-4, laity-2)
xsubj(have-13, laity-2)
