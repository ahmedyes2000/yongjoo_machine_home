(ROOT
  (S
    (S
      (NP (EX There))
      (VP (VBZ is)
        (NP
          (NP (DT no) (NN controversy))
          (PP (IN among)
            (NP (NNS professionals))))))
    (: :)
    (S
      (NP (NN evolution))
      (VP (VBD happened)))
    (. .)))

expl(is-2, There-1)
root(ROOT-0, is-2)
det(controversy-4, no-3)
nsubj(is-2, controversy-4)
