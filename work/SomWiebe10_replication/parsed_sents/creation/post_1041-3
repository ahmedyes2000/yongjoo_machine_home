(ROOT
  (S
    (NP
      (NP (DT A)
        (ADJP (RBR more) (JJ accurate))
        (NN term))
      (PP (TO to)
        (NP (NN use))))
    (VP (MD would)
      (VP (VB be)
        (NP (NN scientist))
        (SBAR (IN because)
          (S
            (NP
              (NP
                (QP (IN over) (CD 99))
                (NN %))
              (PP (IN of)
                (NP (NNS scientists))))
            (VP (VBP agree)
              (PP (IN with)
                (NP (NN evolution))))))))
    (. .)))

det(term-4, A-1)
advmod(accurate-3, more-2)
amod(term-4, accurate-3)
nsubj(scientist-9, term-4)
prep_to(term-4, use-6)
aux(scientist-9, would-7)
cop(scientist-9, be-8)
root(ROOT-0, scientist-9)
mark(agree-16, because-10)
quantmod(99-12, over-11)
num(%-13, 99-12)
nsubj(agree-16, %-13)
prep_of(%-13, scientists-15)
advcl(scientist-9, agree-16)
prep_with(agree-16, evolution-18)

