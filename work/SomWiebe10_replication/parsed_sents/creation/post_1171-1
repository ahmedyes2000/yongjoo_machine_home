(ROOT
  (SINV (RB So)
    (VP (VBZ does))
    (NP
      (NP (DT the) (JJ vast) (NN majority))
      (PP (IN of)
        (NP (DT the) (JJ scientific) (NN community))))
    (. .)))

advmod(does-2, So-1)
root(ROOT-0, does-2)
det(majority-5, the-3)
amod(majority-5, vast-4)
nsubj(does-2, majority-5)
det(community-9, the-7)
amod(community-9, scientific-8)
prep_of(majority-5, community-9)

