  (S
    (S
      (NP (DT The) (NNP Bible))
      (VP (VBD was)
        (VP (VBN written)
          (SBAR (IN as)
            (S
              (VP (VBZ is) (, ,)
                (SBAR (IN because)
                  (S
                    (ADVP (RB otherwise))
                    (NP (PRP it))
                    (VP (MD will)
                      (VP (VB be)
                        (ADJP (JJ one-sided))))))))))))
    (, ,)
    (NP (DT no) (NN one))
    (VP (VBZ believes)
      (NP (DT a) (JJ one-sided) (NN story)))
    (. .)))

