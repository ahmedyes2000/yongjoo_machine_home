(ROOT
  (S
    (NP
      (QP (RB As) (JJ long) (IN as) (CD one)))
    (VP (VBZ uses)
      (NP
        (NP (DT the) (JJ anthropic) (NNS principles))
        (SBAR
          (WHNP (WDT which))
          (S
            (NP (PRP I))
            (VP (VBD described)
              (PP (IN in)
                (NP (JJR more) (NN detail))))))))
    (. .)))

quantmod(one-4, As-1)
quantmod(one-4, long-2)
quantmod(one-4, as-3)
nsubj(uses-5, one-4)
root(ROOT-0, uses-5)
det(principles-8, the-6)
amod(principles-8, anthropic-7)
dobj(uses-5, principles-8)
dobj(described-11, principles-8)
rel(described-11, which-9)
nsubj(described-11, I-10)
rcmod(principles-8, described-11)
amod(detail-14, more-13)
prep_in(described-11, detail-14)

