(ROOT
  (S ('' '')
    (S
      (NP (NNP Intelligent) (NN design) (NN creationism))
      (VP (MD may)
        (VP (VB have)
          (NP (JJ religious) (NN merit)))))
    (, ,)
    (CC but)
    (S
      (NP (PRP it))
      (ADVP (RB certainly))
      (VP (VBZ has)
        (NP
          (ADJP (RB utterly) (DT no))
          (JJ scientific) (NN merit))))
    (. .)))

nn(creationism-4, Intelligent-2)
nn(creationism-4, design-3)
nsubj(have-6, creationism-4)
aux(have-6, may-5)
root(ROOT-0, have-6)
amod(merit-8, religious-7)
dobj(have-6, merit-8)
nsubj(has-13, it-11)
advmod(has-13, certainly-12)
conj_but(have-6, has-13)
advmod(no-15, utterly-14)
amod(merit-17, no-15)
amod(merit-17, scientific-16)
dobj(has-13, merit-17)

