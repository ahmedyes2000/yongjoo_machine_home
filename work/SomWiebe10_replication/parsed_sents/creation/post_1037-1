(ROOT
  (S
    (ADVP (RB Though))
    (, ,)
    (NP (DT both))
    (ADVP (RB still))
    (VP (VBZ remains)
      (PP (IN as)
        (NP (DT a) (NN theory)))
      (SBAR (IN because)
        (S
          (NP (PRP it))
          (VP (VBZ is)
            (ADVP (RB still))
            (RB not)
            (ADJP (JJ proven)
              (S
                (VP (TO to)
                  (VP (VB be)
                    (ADVP (RB otherwise))))))))))
    (. .)))

advmod(remains-5, Though-1)
nsubj(remains-5, both-3)
advmod(remains-5, still-4)
root(ROOT-0, remains-5)
det(theory-8, a-7)
prep_as(remains-5, theory-8)
mark(proven-14, because-9)
nsubj(proven-14, it-10)
cop(proven-14, is-11)
advmod(proven-14, still-12)
neg(proven-14, not-13)
advcl(remains-5, proven-14)
aux(be-16, to-15)
xcomp(proven-14, be-16)
advmod(be-16, otherwise-17)

