(ROOT
  (S
    (NP (PRP You))
    (VP (MD might)
      (ADVP (RB as) (RB well))
      (VP (VB ask)
        (NP (DT a) (NN plumber)
          (S
            (VP (TO to)
              (VP (VB falsify)
                (NP (NN string) (NN theory))))))))
    (. .)))

nsubj(ask-5, You-1)
aux(ask-5, might-2)
advmod(well-4, as-3)
mwe(well-4, as-3)
advmod(ask-5, well-4)
root(ROOT-0, ask-5)
det(plumber-7, a-6)
dobj(ask-5, plumber-7)
aux(falsify-9, to-8)
infmod(plumber-7, falsify-9)
nn(theory-11, string-10)
dobj(falsify-9, theory-11)

