(ROOT
  (S (CC But)
    (NP (NNP ID))
    (VP (VBZ is)
      (ADJP (JJ unscientific)
        (PP (IN for)
          (NP
            (NP
              (ADJP (RB entirely) (JJ different))
              (NNS reasons))
            (PRN (-LRB- -LRB-)
              (ADJP (RB as) (JJ explored)
                (PP (IN in)
                  (NP
                    (NP (DT the) (JJ other) (NNS articles))
                    (PP (IN on)
                      (NP (DT this) (NN site))))))
              (-RRB- -RRB-))))))
    (. .)))

cc(unscientific-4, But-1)
nsubj(unscientific-4, ID-2)
cop(unscientific-4, is-3)
root(ROOT-0, unscientific-4)
advmod(different-7, entirely-6)
amod(reasons-8, different-7)
prep_for(unscientific-4, reasons-8)
advmod(explored-11, as-10)
dep(reasons-8, explored-11)
det(articles-15, the-13)
amod(articles-15, other-14)
prep_in(explored-11, articles-15)
det(site-18, this-17)
prep_on(articles-15, site-18)

