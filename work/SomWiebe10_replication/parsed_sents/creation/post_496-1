(ROOT
  (S (CC And)
    (PP (IN for)
      (NP
        (NP (JJ good) (NN reason))
        (SBAR (IN because)
          (S
            (NP (EX there))
            (VP (VBP are)
              (ADVP (RB trivially))
              (ADJP (JJ few)
                (SBAR (IN if)
                  (FRAG (DT any)))))))))
    (, ,)
    (VP (VBP ID)
      (NP (JJ relevant) (JJ scientific) (NNS papers)))
    (. .)))

cc(ID-13, And-1)
amod(reason-4, good-3)
prep_for(ID-13, reason-4)
mark(are-7, because-5)
expl(are-7, there-6)
dep(reason-4, are-7)
advmod(are-7, trivially-8)
acomp(are-7, few-9)
mark(any-11, if-10)
dep(few-9, any-11)
root(ROOT-0, ID-13)
amod(papers-16, relevant-14)
amod(papers-16, scientific-15)
dobj(ID-13, papers-16)

