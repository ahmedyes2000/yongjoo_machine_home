(ROOT
  (S
    (S
      (NP (PRP I))
      (VP (VBP have)
        (NP
          (NP (CD two))
          (PP (IN in)
            (NP (PRP$ my) (NN house))))))
    (, ,)
    (CC and)
    (S
      (NP (DT the) (JJ modern) (NN translation))
      (VP (VBZ is)
        (ADJP (JJ different))))
    (. .)))

nsubj(have-2, I-1)
root(ROOT-0, have-2)
dobj(have-2, two-3)
poss(house-6, my-5)
prep_in(two-3, house-6)
