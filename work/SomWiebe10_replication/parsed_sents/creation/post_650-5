(ROOT
  (S
    (NP
      (NP (DT No) (NN wonder))
      (, ,)
      (NP (DT the) (JJ abrupt) (NN appearance\/creation)))
    (VP (VBZ is)
      (NP
        (NP (DT a) (JJ standard) (JJ religious) (NN explanation))
        (SBAR
          (WHNP (WDT which))
          (S
            (VP (VBZ ignores)
              (NP
                (NP (DT the) (NNS facts))
                (PP (IN of)
                  (NP (NN science)))))))))
    (. .)))

det(wonder-2, No-1)
nsubj(explanation-11, wonder-2)
det(appearance\/creation-6, the-4)
amod(appearance\/creation-6, abrupt-5)
appos(wonder-2, appearance\/creation-6)
cop(explanation-11, is-7)
det(explanation-11, a-8)
amod(explanation-11, standard-9)
amod(explanation-11, religious-10)
root(ROOT-0, explanation-11)
nsubj(ignores-13, explanation-11)
rcmod(explanation-11, ignores-13)
det(facts-15, the-14)
dobj(ignores-13, facts-15)
prep_of(facts-15, science-17)

