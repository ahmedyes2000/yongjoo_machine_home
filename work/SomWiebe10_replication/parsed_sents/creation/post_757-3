(ROOT
  (S (CC So)
    (S
      (ADVP (RB eventually))
      (NP (PRP you))
      (VP (VBP come)
        (PP (TO to)
          (NP
            (NP (DT the) (NN end))
            (PP (IN of)
              (NP (DT that) (NN argument)))))))
    (CC and)
    (S
      (NP (PRP you))
      (VP (MD must)
        (VP (VB have)
          (NP (DT an) (JJ ULTIMATE) (NN authority)))))
    (. .)))

advmod(come-4, eventually-2)
nsubj(come-4, you-3)
root(ROOT-0, come-4)
det(end-7, the-6)
prep_to(come-4, end-7)
det(argument-10, that-9)
prep_of(end-7, argument-10)
nsubj(have-14, you-12)
aux(have-14, must-13)
conj_and(come-4, have-14)
det(authority-17, an-15)
amod(authority-17, ULTIMATE-16)
dobj(have-14, authority-17)

