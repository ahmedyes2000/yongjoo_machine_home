(ROOT
  (S
    (NP (PRP I))
    (VP (MD will)
      (VP (VB start)
        (VP (VB be)
          (VP (VBG quoting)
            (NP (DT a) (JJ bible) (NN verse))))))
    (. .)))

nsubj(start-3, I-1)
aux(start-3, will-2)
root(ROOT-0, start-3)
aux(quoting-5, be-4)
dep(start-3, quoting-5)
det(verse-8, a-6)
amod(verse-8, bible-7)
dobj(quoting-5, verse-8)

