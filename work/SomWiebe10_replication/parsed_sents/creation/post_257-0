(ROOT
  (S (`` ``)
    (NP (NNP ID))
    (VP (VBZ is)
      (UCP
        (ADVP (RB ridiculously))
        (CC and)
        (PP
          (ADVP (RB obviously))
          (RB not)
          (NP (NN science)))))
    (. .)))

nsubj(is-3, ID-2)
root(ROOT-0, is-3)
dep(is-3, ridiculously-4)
dep(is-3, obviously-6)
