(ROOT
  (S
    (VP (VB turn)
      (FRAG
        (NP
          (NP (DT the) (JJ other) (NN cheek))
          (SBAR
            (S
              (VP (VBD was)
                (NP
                  (NP (DT the) (NN law))
                  (SBAR
                    (WHADVP (WRB when))
                    (S
                      (NP (NNP Christ))
                      (VP (VBD came))))))))
          (. .))))
    (. .)))

root(ROOT-0, turn-1)
det(cheek-4, the-2)
amod(cheek-4, other-3)
dep(turn-1, cheek-4)
cop(law-7, was-5)
det(law-7, the-6)
rcmod(cheek-4, law-7)
tmod(came-10, law-7)
advmod(came-10, when-8)
nsubj(came-10, Christ-9)
dep(law-7, came-10)

