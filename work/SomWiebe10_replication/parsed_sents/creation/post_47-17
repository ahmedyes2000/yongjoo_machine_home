(ROOT
  (S
    (S
      (NP (PRP It))
      (VP (VBZ has)
        (S
          (VP (TO to)))
        (, ,)
        (PP (IN at)
          (NP
            (NP (NN risk))
            (PP (IN of)
              (NP (NN incoherence)))))))
    (: ;)
    (CC and)
    (S
      (ADVP (RB yet)
        (PP (IN by)
          (S
            (VP (VBG doing)
              (ADVP (RB so))))))
      (, ,)
      (NP (PRP it))
      (VP (VBZ collapses)
        (PP (IN into)
          (NP (NN incoherence)))))
    (. .)))

