(ROOT
  (S
    (NP (PRP$ Your) (NNS comments))
    (VP (VBD showed)
      (SBAR (IN that)
        (S
          (NP (PRP you))
          (ADVP (RB also))
          (VP (VBP understand)
            (SBAR (IN that)
              (S
                (NP (NN fitness))
                (VP (VBZ is) (RB not)
                  (VP (VBN determined)
                    (PP (IN by)
                      (NP (DT a) (JJ single) (NN cause\/effect)))))))))))
    (. .)))

poss(comments-2, Your-1)
nsubj(showed-3, comments-2)
root(ROOT-0, showed-3)
complm(understand-7, that-4)
