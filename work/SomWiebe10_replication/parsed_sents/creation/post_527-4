(ROOT
  (S (RB So)
    (VP
      (VP (VB surprise)
        (NP (PRP me)))
      (, ,)
      (VP (VB explain)
        (PP (TO to)
          (NP (PRP us)))
        (SBAR
          (WHADVP (WRB how))
          (S
            (NP (NNP ID))
            (VP (VBZ explains)
              (NP (DT the) (JJ genetic) (NN code)))))))
    (. .)))

advmod(surprise-2, So-1)
root(ROOT-0, surprise-2)
dobj(surprise-2, me-3)
dep(surprise-2, explain-5)
prep_to(explain-5, us-7)
advmod(explains-10, how-8)
nsubj(explains-10, ID-9)
ccomp(explain-5, explains-10)
det(code-13, the-11)
amod(code-13, genetic-12)
dobj(explains-10, code-13)

