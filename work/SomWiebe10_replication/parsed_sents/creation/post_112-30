(ROOT
  (S
    (SBAR (IN Since)
      (S
        (NP (PRP they))
        (VP (VBP are)
          (NP (JJ personal) (NNS experiences)))))
    (NP (PRP they))
    (VP (VBP do) (RB n't)
      (VP (VB have)
        (S
          (VP (VB weight)
            (PP (IN with)
              (NP (JJ other) (NNS people)))
            (. .)))))
    (. .)))

mark(experiences-5, Since-1)
nsubj(experiences-5, they-2)
cop(experiences-5, are-3)
amod(experiences-5, personal-4)
advcl(have-9, experiences-5)
nsubj(have-9, they-6)
aux(have-9, do-7)
neg(have-9, n't-8)
root(ROOT-0, have-9)
