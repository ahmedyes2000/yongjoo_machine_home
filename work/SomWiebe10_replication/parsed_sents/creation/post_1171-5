(ROOT
  (S
    (ADVP (RB As) (RB long)
      (SBAR (IN as)
        (S
          (NP (PRP they))
          (VP (MD can)
            (VP (VB accept))))))
    (NP (DT that) (NN evolution))
    (VP
      (VP (VBD happened))
      (, ,)
      (CC and)
      (VP (VBZ does) (RB not)
        (VP (VB turn)
          (NP (DT a)
            (ADJP (JJ blind)
              (PP (TO to)
                (NP (DT the) (NN blaring))))
            (NN evidence)))))
    (. .)))

advmod(long-2, As-1)
advmod(happened-9, long-2)
mark(accept-6, as-3)
nsubj(accept-6, they-4)
aux(accept-6, can-5)
ccomp(long-2, accept-6)
det(evolution-8, that-7)
nsubj(happened-9, evolution-8)
nsubj(turn-14, evolution-8)
root(ROOT-0, happened-9)
aux(turn-14, does-12)
neg(turn-14, not-13)
conj_and(happened-9, turn-14)
det(evidence-20, a-15)
amod(evidence-20, blind-16)
det(blaring-19, the-18)
