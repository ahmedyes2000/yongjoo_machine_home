(ROOT
  (S ('' '')
    (NP (PRP I))
    (VP (VBD defined)
      (NP (JJ purposeful) (JJR earlier))
      (PP (IN in)
        (NP (DT this) (NN comment))))
    (. .)))

nsubj(defined-3, I-2)
root(ROOT-0, defined-3)
amod(earlier-5, purposeful-4)
dobj(defined-3, earlier-5)
det(comment-8, this-7)
prep_in(defined-3, comment-8)

