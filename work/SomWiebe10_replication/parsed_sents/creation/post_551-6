(ROOT
  (S ('' '')
    (NP (EX There))
    (VP (VBZ is)
      (NP
        (NP
          (NP (DT no) (NN mention))
          (PP (IN of)
            (NP (DT the) (NNP Christian) (NNP God))))
        (CC or)
        (NP
          (NP (DT any) (JJ other) (NN God))
          (PP (IN in)
            (NP (DT this) (NN definition))))))
    (. .)))

expl(is-3, There-2)
root(ROOT-0, is-3)
det(mention-5, no-4)
nsubj(is-3, mention-5)
det(God-9, the-7)
nn(God-9, Christian-8)
prep_of(mention-5, God-9)
det(God-13, any-11)
amod(God-13, other-12)
nsubj(is-3, God-13)
conj_or(mention-5, God-13)
det(definition-16, this-15)
prep_in(God-13, definition-16)

