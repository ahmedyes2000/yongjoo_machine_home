(ROOT
  (S
    (NP (EX There))
    (VP (VBZ 's)
      (NP
        (NP (DT a) (JJ definite) (NN difference))
        (PP (IN between)
          (NP (NN micro-evolution)
            (CC and)
            (NN macro-evolution)))
        (PP (IN in)
          (NP (DT the) (NN sense))))
      (SBAR (IN that)
        (S
          (PP (IN with)
            (NP
              (NP (DT the) (NNS moths))
              (CC and)
              (NP (DT the) (NNS finches))))
          (, ,)
          (NP (EX there))
          (VP (VBP are)
            (NP
              (NP (JJ minor) (NNS changes))
              (SBAR
                (WHNP (WDT that))
                (S
                  (VP (VBP happen)))))))))
    (. .)))

expl('s-2, There-1)
root(ROOT-0, 's-2)
det(difference-5, a-3)
amod(difference-5, definite-4)
nsubj('s-2, difference-5)
prep_between(difference-5, micro-evolution-7)
prep_between(difference-5, macro-evolution-9)
conj_and(micro-evolution-7, macro-evolution-9)
det(sense-12, the-11)
prep_in(difference-5, sense-12)
complm(are-22, that-13)
det(moths-16, the-15)
prep_with(are-22, moths-16)
det(finches-19, the-18)
conj_and(moths-16, finches-19)
prep_with(are-22, finches-19)
expl(are-22, there-21)
ccomp('s-2, are-22)
amod(changes-24, minor-23)
nsubj(are-22, changes-24)
nsubj(happen-26, changes-24)
rcmod(changes-24, happen-26)

