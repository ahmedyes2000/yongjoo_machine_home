(ROOT
  (S
    (NP
      (NP (JJ Many) (NNS people))
      (PP (VBG including)
        (NP (DT the) (NNP NCSE))))
    (VP (VBP have)
      (VP (VBN been)
        (VP (VBN involved)
          (S
            (VP (VBG addressing)
              (NP (DT the) (NNS arguments)))))))
    (. .)))

amod(people-2, Many-1)
nsubjpass(involved-8, people-2)
det(NCSE-5, the-4)
prep_including(people-2, NCSE-5)
aux(involved-8, have-6)
auxpass(involved-8, been-7)
root(ROOT-0, involved-8)
xcomp(involved-8, addressing-9)
det(arguments-11, the-10)
dobj(addressing-9, arguments-11)

