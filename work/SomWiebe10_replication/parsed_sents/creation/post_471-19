(ROOT
  (S
    (NP (DT This))
    (VP (VBZ is)
      (NP (DT an)
        (ADJP (JJ arbitrary)
          (CC and)
          (JJ illogical))
        (NN conclusion)))
    (. .)))

nsubj(conclusion-7, This-1)
cop(conclusion-7, is-2)
det(conclusion-7, an-3)
amod(conclusion-7, arbitrary-4)
conj_and(arbitrary-4, illogical-6)
amod(conclusion-7, illogical-6)
root(ROOT-0, conclusion-7)

