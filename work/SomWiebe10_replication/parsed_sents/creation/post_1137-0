(ROOT
  (S
    (NP (NNP Evolution))
    (VP (VBZ is)
      (ADVP (RB simply))
      (NP
        (NP (DT the) (JJ genetic) (NN change))
        (PP (IN in)
          (NP
            (NP (NNS populations))
            (PP (IN of)
              (NP
                (NP (NNS organisms))
                (PP (IN over)
                  (NP (JJ successive) (NNS generations)))))))))
    (. .)))

nsubj(change-6, Evolution-1)
cop(change-6, is-2)
advmod(change-6, simply-3)
det(change-6, the-4)
amod(change-6, genetic-5)
root(ROOT-0, change-6)
prep_in(change-6, populations-8)
prep_of(populations-8, organisms-10)
amod(generations-13, successive-12)
prep_over(organisms-10, generations-13)

