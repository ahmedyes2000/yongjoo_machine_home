(ROOT
  (S
    (NP (DT This))
    (VP (VBZ is)
      (SBAR (IN because)
        (S
          (NP
            (NP (PRP$ his) (NNS conclusions))
            (PP (VBG concerning)
              (NP (DT the) (NNS hypothesis))))
          (VP (VBD were)
            (PP (IN in)
              (NP (DT the) (JJ negative)))))))
    (. .)))

nsubj(is-2, This-1)
root(ROOT-0, is-2)
mark(were-9, because-3)
poss(conclusions-5, his-4)
nsubj(were-9, conclusions-5)
det(hypothesis-8, the-7)
prep_concerning(conclusions-5, hypothesis-8)
advcl(is-2, were-9)
det(negative-12, the-11)
prep_in(were-9, negative-12)

