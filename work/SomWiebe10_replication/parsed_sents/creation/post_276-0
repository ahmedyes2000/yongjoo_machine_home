                (NP (NNS hurricanes))
                (, ,) (FW etc.)))))))
    (. .)))

nsubjpass(recognized-4, Design-2)
auxpass(recognized-4, is-3)
root(ROOT-0, recognized-4)
det(arrangement-8, the-6)
