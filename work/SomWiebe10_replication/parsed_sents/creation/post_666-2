  (S (CC But)
    (S
      (NP (DT another) (NN opinion))
      (VP (VBZ is)))
    (: :) (`` ``)
    (S
      (NP (DT The) (NN book))
      (VP (VBZ argues)
        (SBAR (IN that)
          (S
            (NP
              (NP (DT the) (NN origin))
              (PP (IN of)
                (NP (JJ new) (NNS organisms))))
            (VP (VBZ is) (`` ``)
              (PP (IN in)
                (NP (DT an) (JJ immaterial) (NN cause))))))
        (: :)
        (PP (IN in)
          (NP
            (NP (DT a) (NN blueprint))
            (, ,)
            (NP (DT a) (NN plan))
            (, ,)
            (NP (DT a) (NN pattern))
            (, ,)
            (VP (VBN devised)
              (PP (IN by)
                (NP (DT an) (JJ intelligent) (NN agent))))))))
    (. .)))

cc(is-4, But-1)
det(opinion-3, another-2)
nsubj(is-4, opinion-3)
root(ROOT-0, is-4)
