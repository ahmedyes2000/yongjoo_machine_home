(ROOT
  (S
    (ADVP (RB So))
    (SINV (VBP do)
      (NP (PRP I)))
    (, ,)
    (CC but)
    (S
      (NP (PRP I))
      (ADVP (RB also))
      (VP (VBP see)
        (NP
          (NP
            (NP (DT the) (NN evidence))
            (PP (IN of)))
          (, ,)
          (CC and)
          (NP
            (NP (DT the) (NN logic))
            (PP (IN for)
              (NP (NN design)))))))
    (. .)))

advmod(do-2, So-1)
root(ROOT-0, do-2)
nsubj(do-2, I-3)
nsubj(see-8, I-6)
advmod(see-8, also-7)
conj_but(do-2, see-8)
det(evidence-10, the-9)
dobj(see-8, evidence-10)
prep(evidence-10, of-11)
det(logic-15, the-14)
dobj(see-8, logic-15)
conj_and(evidence-10, logic-15)
prep_for(logic-15, design-17)

