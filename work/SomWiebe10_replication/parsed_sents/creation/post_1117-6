(ROOT
  (SINV
    (ADVP (RB Similarly))
    (VP (VBZ is))
    (NP
      (NP (DT the) (NN case))
      (PP (IN of)
        (NP
          (NP (DT every) (VBG living) (NN thing))
          (PP (IN on)
            (NP (DT this) (NN earth))))))
    (. .)))

advmod(is-2, Similarly-1)
root(ROOT-0, is-2)
det(case-4, the-3)
nsubj(is-2, case-4)
det(thing-8, every-6)
amod(thing-8, living-7)
prep_of(case-4, thing-8)
det(earth-11, this-10)
prep_on(thing-8, earth-11)

