(ROOT
  (S
    (NP
      (NP (NN Everything))
      (SBAR
        (S
          (NP (PRP you))
          (VP (VBD said)))))
    (VP (VBZ sounds)
      (ADJP (RB so) (JJ ridiculous)
        (SBAR
          (S
            (NP (PRP it))
            (VP (VBZ borders)
              (NP (NN made-up)))))))
    (. .)))

dobj(said-3, Everything-1)
nsubj(sounds-4, Everything-1)
nsubj(said-3, you-2)
rcmod(Everything-1, said-3)
root(ROOT-0, sounds-4)
advmod(ridiculous-6, so-5)
acomp(sounds-4, ridiculous-6)
nsubj(borders-8, it-7)
dep(ridiculous-6, borders-8)
dobj(borders-8, made-up-9)

