(ROOT
  (S
    (S
      (NP (NNP ID))
      (VP (MD may)
        (VP (VB assert))))
    (CC but)
    (S
      (PP (IN as)
        (NP (DT a) (NN science)))
      (NP (PRP$ their) (NNS contributions))
      (VP (VBP remain)
        (ADVP (RB largely))
        (PP (IN with)
          (NP (NN content)))))
    (. .)))

nsubj(assert-3, ID-1)
aux(assert-3, may-2)
root(ROOT-0, assert-3)
det(science-7, a-6)
prep_as(remain-10, science-7)
poss(contributions-9, their-8)
nsubj(remain-10, contributions-9)
conj_but(assert-3, remain-10)
advmod(remain-10, largely-11)
prep_with(remain-10, content-13)

