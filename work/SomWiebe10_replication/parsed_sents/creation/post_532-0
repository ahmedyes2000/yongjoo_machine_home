(ROOT
  (S
    (NP (PRP I))
    (ADVP (RB never))
    (VP (VBD said)
      (NP (NN anything))
      (PP (IN about)
        (NP
          (NP (NNP Einstein) (POS 's))
          (JJ religious) (NNS convictions))))
    (. .)))

nsubj(said-3, I-1)
neg(said-3, never-2)
root(ROOT-0, said-3)
dobj(said-3, anything-4)
poss(convictions-9, Einstein-6)
amod(convictions-9, religious-8)
prep_about(said-3, convictions-9)

