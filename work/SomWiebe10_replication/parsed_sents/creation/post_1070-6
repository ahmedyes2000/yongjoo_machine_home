(ROOT
  (S
    (PP (VBG According)
      (PP (TO to)
        (NP (DT the) (NNP Bible))))
    (, ,)
    (NP (NN earth))
    (VP (VBZ is)
      (ADJP (JJ flat))
      (SBAR (CC but)
        (S
          (NP (DT no) (NN one))
          (VP (MD would)
            (VP (VB believe)
              (NP (DT this)))))))
    (. .)))

prepc_according_to(flat-8, to-2)
det(Bible-4, the-3)
pobj(flat-8, Bible-4)
nsubj(flat-8, earth-6)
cop(flat-8, is-7)
root(ROOT-0, flat-8)
cc(believe-13, but-9)
