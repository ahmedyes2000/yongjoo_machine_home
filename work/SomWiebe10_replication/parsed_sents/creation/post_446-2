(ROOT
  (S
    (NP (PRP$ Their) (JJ only) (NN concern))
    (VP (VBZ is)
      (SBAR (IN if)
        (S
          (NP (DT an) (NN explanation))
          (VP (VBZ leads)
            (PP (TO to)
              (NP (NNS predictions)))))))
    (. .)))

poss(concern-3, Their-1)
amod(concern-3, only-2)
nsubj(is-4, concern-3)
root(ROOT-0, is-4)
mark(leads-8, if-5)
det(explanation-7, an-6)
nsubj(leads-8, explanation-7)
advcl(is-4, leads-8)
prep_to(leads-8, predictions-10)

