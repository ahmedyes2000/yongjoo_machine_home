  (S
    (S
      (NP (NNS Prokaryotes))
      (VP (VBP are)
        (PP (IN by)
          (NP
            (NP (RB far) (DT the)
              (ADJP (RBS most) (JJ successful))
              (NN species))
            (PP (IN on)
              (NP (DT the) (NN planet) (NN earth)))))))
    (CC and)
    (S
      (NP (PRP they))
      (VP (VBP do) (RB n't)
        (ADVP (RB even))
        (VP (VB have)
          (NP (NN cell) (NNS nuclei)))))
    (. .)))

