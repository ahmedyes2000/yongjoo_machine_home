(ROOT
  (S
    (NP (DT That))
    (VP (MD would)
      (VP (VB be)
        (VP (VBN closed-minded)
          (PP (IN toward)
            (NP (DT the) (NN subject))))))
    (. .)))

nsubjpass(closed-minded-4, That-1)
aux(closed-minded-4, would-2)
auxpass(closed-minded-4, be-3)
root(ROOT-0, closed-minded-4)
det(subject-7, the-6)
prep_toward(closed-minded-4, subject-7)

