(ROOT
  (X (CC But)
    (NP (RB now))
    (NP
      (NP (DT the) (JJ real) (NN problem))
      (PP (IN with)
        (NP (DT this) (NN argument))))
    (SBAR (IN that)
      (S
        (NP (NNS AUSCS))
        (VP (VBZ makes))))
    (. .)))

dep(problem-5, But-1)
dep(problem-5, now-2)
det(problem-5, the-3)
amod(problem-5, real-4)
root(ROOT-0, problem-5)
