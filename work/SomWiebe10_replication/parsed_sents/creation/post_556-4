(ROOT
  (S
    (PP (IN Because)
      (NP (PRP we)))
    (NP (DT all))
    (VP (VBP know)
      (SBAR
        (S
          (NP (DT the) (NN answer))
          (VP (MD may)
            (VP (VB lay)
              (NP
                (NP (RB outside))
                (PP (IN of)
                  (NP (JJ methodological) (NN naturalism)))))))))
    (. .)))

prep_because(know-4, we-2)
nsubj(know-4, all-3)
root(ROOT-0, know-4)
det(answer-6, the-5)
nsubj(lay-8, answer-6)
aux(lay-8, may-7)
ccomp(know-4, lay-8)
dobj(lay-8, outside-9)
amod(naturalism-12, methodological-11)
prep_of(outside-9, naturalism-12)

