(ROOT
  (S
    (ADVP (RB then))
    (NP (PRP you))
    (ADVP (RB obviously))
    (VP (VBP believe)
      (SBAR
        (S
          (NP (NNP God))
          (VP (VBD had)
            (NP
              (NP (DT no) (NN hand))
              (PP (IN in)
                (NP (PRP it))))))))
    (. .)))

advmod(believe-4, then-1)
nsubj(believe-4, you-2)
advmod(believe-4, obviously-3)
root(ROOT-0, believe-4)
nsubj(had-6, God-5)
ccomp(believe-4, had-6)
det(hand-8, no-7)
dobj(had-6, hand-8)
prep_in(hand-8, it-10)

