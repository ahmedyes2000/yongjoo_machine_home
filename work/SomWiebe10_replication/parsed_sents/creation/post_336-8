(ROOT
  (S
    (NP (JJ Previous) (NNS claims))
    (VP (VBP are)
      (ADVP (RB thus))
      (VP (VBN supported)
        (PP (IN by)
          (NP (PRP$ our) (NN analysis)))))
    (. .)))

amod(claims-2, Previous-1)
nsubjpass(supported-5, claims-2)
auxpass(supported-5, are-3)
advmod(supported-5, thus-4)
root(ROOT-0, supported-5)
poss(analysis-8, our-7)
agent(supported-5, analysis-8)

