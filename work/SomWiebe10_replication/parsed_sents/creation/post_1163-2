(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (ADVP (RB actually))
        (VP (VBD read)
          (NP (JJ scientific) (NNS papers))
          (PP (IN about)
            (NP (NN evolution))))))
    (NP (PRP they))
    (VP (VBP do) (RB not)
      (VP (VB claim)
        (S
          (VP (TO to)
            (VP (VB be)
              (NP (NN law))
              (SBAR (IN because)
                (S
                  (NP (PRP it))
                  (VP (VBZ is)
                    (ADVP (RB still))
                    (NP (DT a) (JJ working) (NN theory))))))))))
    (. .)))

mark(read-4, If-1)
nsubj(read-4, you-2)
advmod(read-4, actually-3)
advcl(claim-12, read-4)
amod(papers-6, scientific-5)
dobj(read-4, papers-6)
prep_about(read-4, evolution-8)
nsubj(claim-12, they-9)
xsubj(law-15, they-9)
aux(claim-12, do-10)
neg(claim-12, not-11)
root(ROOT-0, claim-12)
aux(law-15, to-13)
cop(law-15, be-14)
xcomp(claim-12, law-15)
mark(theory-22, because-16)
nsubj(theory-22, it-17)
cop(theory-22, is-18)
advmod(theory-22, still-19)
det(theory-22, a-20)
amod(theory-22, working-21)
advcl(law-15, theory-22)

