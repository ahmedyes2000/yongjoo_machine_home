  (S
    (NP (PRP You))
    (VP (VBP know)
      (SBAR
        (S
          (PRN (, ,)
            (S
              (S
                (NP (PRP I))
                (ADVP (RB neither))
                (VP (VBD saw)
                  (NP (DT that) (NN statement))))
              (, ,)
              (S
                (NP (NN nobody))
                (VP (VBD gave)
                  (NP (PRP him))
                  (NP (DT that) (NN information))))
              (, ,)
              (CC and)
              (S
                (NP (PRP I))
                (VP (VBD did) (RB n't)))))
          (NP (PRP I))
          (ADVP (RB certainly))
          (VP (VBD did) (RB n't)
            (VP (VB approve)
              (NP (PRP it)))))))
    (. .)))

nsubj(know-2, You-1)
root(ROOT-0, know-2)
nsubj(saw-6, I-4)
