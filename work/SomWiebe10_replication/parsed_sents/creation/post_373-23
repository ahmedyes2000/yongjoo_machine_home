(ROOT
  (S
    (FRAG
      (ADJP (RB Same) (JJ old)))
    ('' '')
    (NP (PRP I))
    (VP (VBP know)
      (NP (NN design))
      (SBAR
        (WHADVP (WRB when))
        (S
          (NP (PRP I))
          (VP (VBP see)
            (NP (PRP it))
            ('' '')
            (ADVP (VBD nonsense))))))
    (. .)))

advmod(old-2, Same-1)
dep(know-5, old-2)
nsubj(know-5, I-4)
root(ROOT-0, know-5)
dobj(know-5, design-6)
advmod(see-9, when-7)
nsubj(see-9, I-8)
advcl(know-5, see-9)
dobj(see-9, it-10)
advmod(see-9, nonsense-12)

