(ROOT
  (S
    (NP (NNP Darwin))
    (VP (VBZ deserves)
      (NP
        (NP (DT a) (NN lot))
        (PP (IN of)
          (NP
            (NP (NN credit))
            (PP (IN for)
              (NP (DT that)))))))
    (. .)))

nsubj(deserves-2, Darwin-1)
root(ROOT-0, deserves-2)
det(lot-4, a-3)
dobj(deserves-2, lot-4)
prep_of(lot-4, credit-6)
prep_for(credit-6, that-8)

