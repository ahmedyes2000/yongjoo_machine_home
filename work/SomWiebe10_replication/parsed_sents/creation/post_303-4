(ROOT
  (S (CC And)
    (ADVP (RB apparently))
    (NP (`` ``) (NN tell-tale) (NNS signs) ('' ''))
    (VP (VBP are)
      (ADVP (RB also))
      (PP (IN in)
        (NP
          (NP (DT the) (NN eye))
          (PP (IN of)
            (NP (DT the) (NN beholder))))))
    (. .)))

cc(are-7, And-1)
advmod(are-7, apparently-2)
nn(signs-5, tell-tale-4)
nsubj(are-7, signs-5)
root(ROOT-0, are-7)
advmod(are-7, also-8)
det(eye-11, the-10)
