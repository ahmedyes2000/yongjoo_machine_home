(ROOT
  (S (IN So)
    (SBAR (IN if)
      (S
        (NP (PRP it))
        (VP (VBZ is)
          (ADJP (JJ wrong)))))
    (, ,)
    (NP (EX there))
    (VP (MD should)
      (VP (VB be)
        (NP
          (NP (NN evidence))
          (, ,)
          (NP
            (NP (JJ scientific) (NN evidence))
            (SBAR
              (WHNP (WDT that))
              (S
                (VP (VBZ does) (RB not)
                  (VP (VB fit)
                    (PP (IN with)
                      (NP (DT that) (NN worldview)))))))))))
    (. .)))

dep(evidence-10, So-1)
mark(wrong-5, if-2)
nsubj(wrong-5, it-3)
cop(wrong-5, is-4)
