(ROOT
  (S (: --)
    (NP
      (NP (NNP SChen))
      (: --)
      (NP (FW Finally) (, ,)
        (S
          (VP (TO to)
            (VP (VB claim)
              (SBAR (IN that)
                (S
                  (NP (NNP ID))
                  (VP (VBZ has)
                    (NP
                      (NP (DT no) (NN secular) (NNS purposes))
                      (SBAR
                        (WHNP (WDT that))
                        (S
                          (VP (VBP are) (RB not)
                            (ADJP (JJ shams))))))))))))))
    (VP (VBZ is)
      (NP (RB quite) (DT a) (NN claim)
        (S
          (VP (TO to)
            (VP (VB make))))))
    (. .)))

nsubj(claim-21, SChen-2)
dep(SChen-2, Finally-4)
aux(claim-7, to-6)
dep(Finally-4, claim-7)
complm(has-10, that-8)
nsubj(has-10, ID-9)
ccomp(claim-7, has-10)
det(purposes-13, no-11)
nn(purposes-13, secular-12)
dobj(has-10, purposes-13)
nsubj(shams-17, purposes-13)
cop(shams-17, are-15)
neg(shams-17, not-16)
rcmod(purposes-13, shams-17)
cop(claim-21, is-18)
advmod(claim-21, quite-19)
det(claim-21, a-20)
