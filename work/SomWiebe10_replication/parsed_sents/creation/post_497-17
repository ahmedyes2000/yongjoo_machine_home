(ROOT
  (S ('' '')
    (NP (EX There))
    (VP (VBZ is)
      (NP
        (NP (DT no) (JJ established) (`` ``) (NN belief-system) ('' ''))
        (VP (VBN presented)
          (PP (IN as)
            (NP
              (NP (NN evidence))
              (PP (IN of)
                (NP (NN design))))))))
    (. .)))

expl(is-3, There-2)
root(ROOT-0, is-3)
det(belief-system-7, no-4)
amod(belief-system-7, established-5)
nsubj(is-3, belief-system-7)
partmod(belief-system-7, presented-9)
prep_as(presented-9, evidence-11)
prep_of(evidence-11, design-13)

