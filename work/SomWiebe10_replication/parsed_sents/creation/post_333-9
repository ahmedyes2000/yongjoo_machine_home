(ROOT
  (S
    (NP (NNP ID))
    (VP (VBZ does) (RB n't)
      (VP (VB offer)
        (NP
          (NP (JJ new) (NN evidence))
          (SBAR
            (S
              (NP (PRP it))
              (ADVP (RB merely))
              (VP (VBZ relies)
                (PP (IN on)
                  (NP
                    (NP (DT the) (NNS gaps))
                    (PP (IN in)
                      (NP (JJ current) (JJ evolutionary) (NN knowledge)))))))))))
    (. .)))

nsubj(offer-4, ID-1)
aux(offer-4, does-2)
neg(offer-4, n't-3)
root(ROOT-0, offer-4)
amod(evidence-6, new-5)
dobj(offer-4, evidence-6)
dobj(relies-9, evidence-6)
nsubj(relies-9, it-7)
advmod(relies-9, merely-8)
rcmod(evidence-6, relies-9)
det(gaps-12, the-11)
prep_on(relies-9, gaps-12)
amod(knowledge-16, current-14)
amod(knowledge-16, evolutionary-15)
prep_in(gaps-12, knowledge-16)

