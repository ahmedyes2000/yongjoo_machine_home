(ROOT
  (S
    (NP (PRP It))
    (VP (MD will)
      (ADVP (RB never))
      (VP (VB end)
        (SBAR (IN because)
          (S
            (NP (EX there))
            (VP (MD will)
              (ADVP (RB always))
              (VP (VB be)
                (NP (JJR more) (NNS questions))))))))
    (. .)))

nsubj(end-4, It-1)
aux(end-4, will-2)
neg(end-4, never-3)
root(ROOT-0, end-4)
mark(questions-11, because-5)
expl(questions-11, there-6)
aux(questions-11, will-7)
advmod(questions-11, always-8)
cop(questions-11, be-9)
amod(questions-11, more-10)
advcl(end-4, questions-11)

