(ROOT
  (S
    (NP (PRP We))
    (ADVP (RB just))
    (VP (MD ca) (RB n't)
      (VP (VB let)
        (S
          (NP (EX there))
          (VP (VB be)
            (NP
              (NP (NNP Evolution))
              (CC and)
              (ADVP (RB then))
              (NP (NNP Christian) (NNP Creationism)))))))
    (. .)))

nsubj(let-5, We-1)
advmod(let-5, just-2)
aux(let-5, ca-3)
neg(let-5, n't-4)
root(ROOT-0, let-5)
expl(be-7, there-6)
