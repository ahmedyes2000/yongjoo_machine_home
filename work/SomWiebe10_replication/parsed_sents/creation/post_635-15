(ROOT
  (S
    (NP (PRP He))
    (VP (VBD was) (RB n't)
      (ADJP (RB even)
        (PP (IN from)
          (NP
            (NP (DT the) (NN state))
            (PP (IN of)
              (NP (NNP Texas)))))))
    (. .)))

nsubj(even-4, He-1)
cop(even-4, was-2)
neg(even-4, n't-3)
root(ROOT-0, even-4)
det(state-7, the-6)
prep_from(even-4, state-7)
prep_of(state-7, Texas-9)

