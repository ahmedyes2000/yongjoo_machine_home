(ROOT
  (S
    (NP (DT The) (NNP ACLU))
    (VP (VBZ is)
      (NP
        (NP (NN anti) (NN religion))
        (PP (IN for)
          (NP (DT the) (JJS most) (NN part))))
      (, ,)
      (SBAR (IN although)
        (S (, ,)
          (PP (TO to)
            (NP (PRP$ their) (NN credit)))
          (, ,)
          (SBAR (IN once)
            (FRAG
              (PP (IN in)
                (NP (DT a) (JJ great) (NN while)))))
          (, ,)
          (NP (PRP they))
          (VP (MD will)
            (VP (VB take)
              (PRT (RP up))
              (NP
                (NP (DT the) (NN cause))
                (PP (IN of)
                  (NP (DT a) (JJ religious) (NN person)))))))))
    (. .)))

det(ACLU-2, The-1)
nsubj(religion-5, ACLU-2)
cop(religion-5, is-3)
nn(religion-5, anti-4)
root(ROOT-0, religion-5)
