mark(in-18, once-17)
advcl(take-25, in-18)
det(while-21, a-19)
amod(while-21, great-20)
pobj(in-18, while-21)
nsubj(take-25, they-23)
aux(take-25, will-24)
advcl(religion-5, take-25)
prt(take-25, up-26)
det(cause-28, the-27)
