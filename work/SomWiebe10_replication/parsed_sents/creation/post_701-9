(ROOT
  (S
    (NP (PRP They))
    (VP
      (VP (MD can)
        (ADVP (RB now))
        (VP (VB reproduce)
          (NP (NN ear) (NNS organs))
          (PP (IN off)
            (NP (DT a) (NN rodent)))))
      (CC and)
      (VP
        (ADVP (RB now))
        (VBP are)
        (VP (VBG finding)
          (NP (JJR more) (NNS planets)))))
    (. .)))

nsubj(reproduce-4, They-1)
nsubj(finding-13, They-1)
aux(reproduce-4, can-2)
advmod(reproduce-4, now-3)
root(ROOT-0, reproduce-4)
nn(organs-6, ear-5)
dobj(reproduce-4, organs-6)
det(rodent-9, a-8)
prep_off(reproduce-4, rodent-9)
advmod(finding-13, now-11)
aux(finding-13, are-12)
conj_and(reproduce-4, finding-13)
amod(planets-15, more-14)
dobj(finding-13, planets-15)

