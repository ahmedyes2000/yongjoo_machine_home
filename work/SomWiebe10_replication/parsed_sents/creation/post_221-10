(ROOT
  (S
    (NP
      (NP
        (QP (CD 3) (CD billion))
        (NNS years))
      (PP (IN of)
        (NP (NN evolution))))
    (VP (VBZ is)
      (NP (DT a) (JJ loooong) (NN time)))
    (. .)))

number(billion-2, 3-1)
num(years-3, billion-2)
nsubj(time-9, years-3)
prep_of(years-3, evolution-5)
cop(time-9, is-6)
det(time-9, a-7)
amod(time-9, loooong-8)
root(ROOT-0, time-9)

