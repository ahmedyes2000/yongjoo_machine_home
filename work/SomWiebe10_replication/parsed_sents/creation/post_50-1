(ROOT
  (S
    (PP (IN In)
      (NP
        (NP (NN order))
        (PP (IN for)
          (NP (NN something)))
        (S
          (VP (TO to)
            (VP (VB be)
              (VP (VBN designed)))))))
    (, ,)
    (NP (PRP it))
    (VP (VBZ needs)
      (S
        (VP (TO to)
          (VP (VB be)
            (UCP
              (VP (VBN specified))
              (CC and)
              (ADJP (RB sufficiently) (JJ complex)))))))
    (. .)))

prep_in(needs-10, order-2)
prep_for(order-2, something-4)
aux(designed-7, to-5)
auxpass(designed-7, be-6)
infmod(order-2, designed-7)
