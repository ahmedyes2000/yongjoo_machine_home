(ROOT
  (S
    (NP (EX There))
    (VP (VBZ is)
      (NP
        (NP (NN nothing))
        (PP (IN in)
          (NP
            (NP (NN science))
            (PP (IN beyond)
              (NP (DT a) (NN theory)))))))
    (. .)))

expl(is-2, There-1)
root(ROOT-0, is-2)
nsubj(is-2, nothing-3)
prep_in(nothing-3, science-5)
det(theory-8, a-7)
prep_beyond(science-5, theory-8)

