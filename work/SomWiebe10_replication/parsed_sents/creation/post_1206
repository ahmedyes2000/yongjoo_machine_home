(ROOT
  (S
    (NP (NNP Evolution))
    (VP (VBZ is)
      (ADVP (RB just))
      (NP
        (NP (DT an) (NN excuse))
        (PP (IN for)
          (NP (NN biology))))
      (S
        (VP (TO to)
          (VP (VB occupy)
            (NP (DT the) (JJ adolescent) (JJ young) (NN mind))))))
    (. .)))

nsubj(excuse-5, Evolution-1)
cop(excuse-5, is-2)
advmod(excuse-5, just-3)
det(excuse-5, an-4)
