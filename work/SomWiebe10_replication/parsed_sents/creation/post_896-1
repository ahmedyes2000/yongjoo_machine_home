(ROOT
  (S ('' '')
    (S
      (NP (DT The) (NNP Bible))
      (VP (VBD was) (RB n't)
        (VP (VBN written)
          (PP (IN in)
            (NP (NNP English))))))
    (, ,)
    (NP (PRP it))
    (VP (VBD was)
      (VP (VBN written)
        (PP (IN in)
          (NP (JJ ancient) (NNP Hebrew)))))
    (. .)))

det(Bible-3, The-2)
nsubjpass(written-6, Bible-3)
auxpass(written-6, was-4)
neg(written-6, n't-5)
ccomp(written-12, written-6)
prep_in(written-6, English-8)
nsubjpass(written-12, it-10)
auxpass(written-12, was-11)
root(ROOT-0, written-12)
amod(Hebrew-15, ancient-14)
prep_in(written-12, Hebrew-15)

