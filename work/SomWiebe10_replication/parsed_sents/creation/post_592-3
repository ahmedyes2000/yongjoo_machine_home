(ROOT
  (S
    (ADVP (RB However))
    (NP (PRP we))
    (VP (MD can)
      (VP (VB observe)
        (NP (JJ small) (NNS changes))
        (SBAR (IN as)
          (S
            (NP (NNS species))
            (VP (VBP adapt))))))
    (. .)))

advmod(observe-4, However-1)
nsubj(observe-4, we-2)
aux(observe-4, can-3)
root(ROOT-0, observe-4)
amod(changes-6, small-5)
dobj(observe-4, changes-6)
mark(adapt-9, as-7)
nsubj(adapt-9, species-8)
advcl(observe-4, adapt-9)

