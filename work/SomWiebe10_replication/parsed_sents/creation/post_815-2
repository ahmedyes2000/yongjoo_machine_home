(ROOT
  (S
    (NP (PRP They))
    (VP (MD should)
      (VP (VB have)
        (S
          (NP (PRP$ their) (NNS degrees))
          (VP (VBD retracted)
            (PP (IN for)
              (S
                (VP (VBG spewing)
                  (NP (JJ such) (JJ utter) (NN nonsense)))))))))
    (. !)))

