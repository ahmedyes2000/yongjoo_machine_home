(ROOT
  (S
    (S
      (NP (DT That))
      (VP (VBZ is) (RB n't)
        (NP (NN faith))))
    (: ;)
    (S
      (NP (PRP it))
      (VP (VBZ 's)
        (NP (DT the) (NN courage)
          (S
            (VP (TO to)
              (VP (VB face)
                (NP (NN reality))))))))
    (. .)))

nsubj(faith-4, That-1)
cop(faith-4, is-2)
neg(faith-4, n't-3)
root(ROOT-0, faith-4)
nsubj(courage-9, it-6)
cop(courage-9, 's-7)
det(courage-9, the-8)
parataxis(faith-4, courage-9)
aux(face-11, to-10)
infmod(courage-9, face-11)
dobj(face-11, reality-12)

