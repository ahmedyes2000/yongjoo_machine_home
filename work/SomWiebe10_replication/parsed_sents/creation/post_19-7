(ROOT
  (S
    (NP (DT This) (NN principle))
    (VP
      (VP (VBZ is)
        (VP (VBN known)
          (PP (IN as) ('' '')
            (NP
              (NP (NNP Freedom))
              (PP (IN of)
                (NP (NNP Religion)))))))
      ('' '')
      (CC and)
      (VP (VBZ is)
        (VP (VBN protected)
          (PP (IN in)
            (NP (DT the) (NNP United) (NNPS States)))
          (PP (IN by)
            (NP (DT the) (NNP Establishment) (NN Clause))))))
    (. .)))

det(principle-2, This-1)
nsubjpass(known-4, principle-2)
nsubjpass(protected-13, principle-2)
auxpass(known-4, is-3)
root(ROOT-0, known-4)
prep_as(known-4, Freedom-7)
prep_of(Freedom-7, Religion-9)
auxpass(protected-13, is-12)
conj_and(known-4, protected-13)
det(States-17, the-15)
nn(States-17, United-16)
prep_in(protected-13, States-17)
det(Clause-21, the-19)
nn(Clause-21, Establishment-20)
agent(protected-13, Clause-21)

