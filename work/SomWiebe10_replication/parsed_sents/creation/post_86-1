  (S
    (NP (EX There))
    (VP (VBP are)
      (NP
        (NP (NNS questions))
        (SBAR
          (WHPP (IN for)
            (WHNP (WDT which) (NN religion)))
          (S
            (NP (NNP \/))
            (ADVP (RB philosophy\/spirituality))
            (VP (VBZ has)
              (NP (DT no) (NNS answers))))))
      (SBAR (RB just) (IN as)
        (S
          (NP (EX there))
          (VP (VBP are)
            (NP
              (NP (NNS questions))
              (SBAR
                (WHNP (WDT which))
                (S
                  (VP (VBP are)
                    (ADJP (JJ irrelevant)
                      (PP (TO to)
                        (NP (NN science))))))))))))
    (. .)))

expl(are-2, There-1)
root(ROOT-0, are-2)
nsubj(are-2, questions-3)
det(religion-6, questions-3)
