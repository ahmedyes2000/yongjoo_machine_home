(ROOT
  (S
    (NP
      (NP (DT No) (NN evidence))
      (PP (IN of)
        (NP (DT a) (NN designer))))
    (VP (VBZ has)
      (VP (VBN been)
        (VP (VBN given)
          (PP (IN beyond)
            (SBAR (IN that)
              (S
                (NP (DT the) (NNS mechanisms))
                (VP (VBP seem)
                  (S
                    (VP (TO to)
                      (VP (VB be)
                        (VP (VBN designed))))))))))))
    (. .)))

det(evidence-2, No-1)
nsubjpass(given-8, evidence-2)
det(designer-5, a-4)
prep_of(evidence-2, designer-5)
aux(given-8, has-6)
auxpass(given-8, been-7)
root(ROOT-0, given-8)
complm(seem-13, that-10)
det(mechanisms-12, the-11)
