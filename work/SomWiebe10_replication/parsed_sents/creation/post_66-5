(ROOT
  (S
    (VP (VB Let)
      (NP (PRP me))
      (VP (VB show)
        (NP (PRP you))
        (: :)
        (SBARQ
          (WHADVP (WRB How))
          (SQ (VBZ does)
            (NP (NNP ID))
            (VP (VB explain)
              (NP (DT the) (JJ bacterial) (NN flagella)))))))
    (. .)))

root(ROOT-0, Let-1)
dobj(Let-1, me-2)
dep(Let-1, show-3)
dobj(show-3, you-4)
advmod(explain-9, How-6)
aux(explain-9, does-7)
nsubj(explain-9, ID-8)
