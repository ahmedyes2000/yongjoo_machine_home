(ROOT
  (S
    (NP (NNS Archeologists))
    (VP (VBP use)
      (S
        (NP (JJ various) (NNS methods))
        (VP (TO to)
          (VP (VB determine)
            (NP (NN design))
            (PP (IN in)
              (NP
                (NP (NNS things))
                (SBAR
                  (S
                    (NP (PRP they))
                    (VP (VBP find))))))))))
    (. .)))

nsubj(use-2, Archeologists-1)
root(ROOT-0, use-2)
amod(methods-4, various-3)
nsubj(determine-6, methods-4)
aux(determine-6, to-5)
xcomp(use-2, determine-6)
dobj(determine-6, design-7)
prep_in(determine-6, things-9)
dobj(find-11, things-9)
nsubj(find-11, they-10)
rcmod(things-9, find-11)

