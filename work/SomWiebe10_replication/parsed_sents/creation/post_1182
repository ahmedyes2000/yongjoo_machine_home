(ROOT
  (S
    (NP
      (NP (NN Evidence))
      (VP (VBG indicating)
        (NP (NNP Intelligent) (NNP Design))))
    (VP (VBZ is)
      (ADJP (JJ abundant)
        (CC and)
        (JJ overwhelming)))
    (. .)))

nsubj(abundant-6, Evidence-1)
nsubj(overwhelming-8, Evidence-1)
partmod(Evidence-1, indicating-2)
nn(Design-4, Intelligent-3)
dobj(indicating-2, Design-4)
cop(abundant-6, is-5)
root(ROOT-0, abundant-6)
conj_and(abundant-6, overwhelming-8)

