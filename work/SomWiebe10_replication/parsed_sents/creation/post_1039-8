(ROOT
  (SBARQ
    (WHADVP
      (ADVP (RB Then))
      (WRB why))
    (SQ (VBP have)
      (NP (JJ fossilized) (NNS whales))
      (VP (VBN been)
        (VP (VBN found)
          (S
            (VP (VBG standing)
              (ADVP (RB vertically))
              (PP (IN on)
                (NP (PRP$ their) (NNS tails))))))))
    (. ?)))

advmod(why-2, Then-1)
advmod(found-7, why-2)
aux(found-7, have-3)
amod(whales-5, fossilized-4)
nsubjpass(found-7, whales-5)
auxpass(found-7, been-6)
root(ROOT-0, found-7)
xcomp(found-7, standing-8)
advmod(standing-8, vertically-9)
poss(tails-12, their-11)
prep_on(standing-8, tails-12)

