(ROOT
  (S
    (NP (PRP They))
    (VP (VBP deal)
      (PP (IN in)
        (NP
          (NP (JJ real) (NNS issues))
          (PP (JJ such) (IN as)
            (NP (NNS means) (, ,) (NNS motives) (, ,) (NNS opportunities) (, ,)
              (CC and)
              (NNP identify) (NNS suspects))))))
    (. .)))

nsubj(deal-2, They-1)
root(ROOT-0, deal-2)
amod(issues-5, real-4)
prep_in(deal-2, issues-5)
nn(opportunities-12, means-8)
dep(opportunities-12, motives-10)
prep_such_as(issues-5, opportunities-12)
nn(suspects-16, identify-15)
prep_such_as(issues-5, suspects-16)
conj_and(opportunities-12, suspects-16)

