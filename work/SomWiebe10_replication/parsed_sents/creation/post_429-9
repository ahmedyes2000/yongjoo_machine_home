(ROOT
  (S
    (ADVP (RB Just))
    (VP (VB ask)
      (NP (DT any) (NN IDer))
      (, ,)
      (SBAR
        (WHADVP (WRB how))
        (S
          (VP (VBZ does)
            (SBAR
              (S
                (S
                  (NP (NNP ID))
                  (VP (VBP explain)
                    (NP (DT the) (JJ bacterial) (NN flagella))))
                (CC and)
                (S
                  (NP (PRP you))
                  (VP (MD will)
                    (VP (VB be)
                      (VP (VBN amazed)
                        (SBAR (IN that)
                          (S
                            (NP (PRP they))
                            (VP (VBP provide)
                              (NP (NN nothing)))))))))))))))
    (. .)))

advmod(ask-2, Just-1)
root(ROOT-0, ask-2)
det(IDer-4, any-3)
dobj(ask-2, IDer-4)
