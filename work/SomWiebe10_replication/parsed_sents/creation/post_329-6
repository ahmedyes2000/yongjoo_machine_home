(ROOT
  (S
    (NP (PRP He))
    (VP (MD would)
      (VP (VB be)
        (NP
          (NP (DT an) (JJ utter) (NN failure))
          (PP (IN as)
            (NP (DT a) (NN biologist))))))
    (. .)))

nsubj(failure-6, He-1)
aux(failure-6, would-2)
cop(failure-6, be-3)
det(failure-6, an-4)
amod(failure-6, utter-5)
root(ROOT-0, failure-6)
det(biologist-9, a-8)
prep_as(failure-6, biologist-9)

