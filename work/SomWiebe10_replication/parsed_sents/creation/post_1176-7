(ROOT
  (S
    (NP (PRP You))
    (VP (MD 'll)
      (VP (VB find)
        (SBAR (IN that)
          (S
            (NP (NNS species))
            (VP (VBP are)
              (VP
                (VP (VBG changing))
                (CC and)
                (VP (VBG forming)
                  (NP (PDT all) (DT the) (NN time))
                  (PP (IN as)
                    (NP
                      (NP (NNS changes))
                      (PP (IN in)
                        (NP (DT the) (NN environment) (NN force))))))
                (NP (PRP them))
                (PP (TO to))))))))
    (. .)))

nsubj(find-3, You-1)
aux(find-3, 'll-2)
root(ROOT-0, find-3)
complm(changing-7, that-4)
nsubj(changing-7, species-5)
nsubj(forming-9, species-5)
aux(changing-7, are-6)
ccomp(find-3, changing-7)
ccomp(find-3, forming-9)
conj_and(changing-7, forming-9)
predet(time-12, all-10)
det(time-12, the-11)
dobj(forming-9, time-12)
prep_as(forming-9, changes-14)
det(force-18, the-16)
nn(force-18, environment-17)
prep_in(changes-14, force-18)
dobj(changing-7, them-19)
prep(changing-7, to-20)

