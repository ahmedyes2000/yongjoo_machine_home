  (S
    (S
      (PP (IN In)
        (NP (NN case)))
      (NP (PRP you))
      (VP (VBP didnt)
        (S
          (VP (VB see)
            (NP (PRP it))
            (SBAR
              (S
                (NP (PRP$ their))
                (VP (VBD was)
                  (NP
                    (NP (DT an) (NN atheist))
                    (CC or)
                    (NP
                      (NP (CD two))
                      (PP (IN in)
                        (NP (DT those) (NNS foxholes))))))))))))
    (, ,)
    (CC but)
    (FRAG (RB not)
      (ADJP (IN for) (RB very) (RB long)))
    (. !)))

