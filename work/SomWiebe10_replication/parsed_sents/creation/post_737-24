(ROOT
  (S
    (NP (DT The) (NN world))
    (VP (VBD was)
      (VP (VBN created)
        (ADVP (RB exactly))
        (SBAR (IN as)
          (S
            (NP (DT the) (NNP Bible))
            (VP (VBZ says)
              (SBAR
                (S
                  (NP (PRP it))
                  (VP (VBD was) (, ,)
                    (PP (IN over)
                      (NP (NNS billions)))
                    (PP (IN of)
                      (NP (NNS years)))))))))))
    (. .)))

det(world-2, The-1)
nsubjpass(created-4, world-2)
auxpass(created-4, was-3)
root(ROOT-0, created-4)
advmod(created-4, exactly-5)
mark(says-9, as-6)
det(Bible-8, the-7)
nsubj(says-9, Bible-8)
advcl(created-4, says-9)
nsubj(was-11, it-10)
ccomp(says-9, was-11)
prep_over(was-11, billions-14)
prep_of(was-11, years-16)

