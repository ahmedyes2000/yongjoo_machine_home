(ROOT
  (S
    (S
      (NP (PRP He))
      (ADVP (RB then)))
    (, ,)
    (PP (IN on)
      (NP (DT that) (JJ same) (NN day)))
    (, ,)
    (VP (VBD seperated)
      (NP
        (NP (DT the) (NNS waters))
        (PP (IN of)
          (NP (DT the) (NN earth))))
      (PP (IN from)
        (NP
          (NP (DT the) (NNS waters))
          (PP (IN in)
            (NP (DT the) (NNS heavens))))))
    (. .)))

ccomp(seperated-9, He-1)
advmod(He-1, then-2)
det(day-7, that-5)
amod(day-7, same-6)
prep_on(seperated-9, day-7)
root(ROOT-0, seperated-9)
det(waters-11, the-10)
dobj(seperated-9, waters-11)
det(earth-14, the-13)
prep_of(waters-11, earth-14)
det(waters-17, the-16)
prep_from(seperated-9, waters-17)
det(heavens-20, the-19)
prep_in(waters-17, heavens-20)

