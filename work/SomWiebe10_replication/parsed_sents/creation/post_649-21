(ROOT
  (S ('' '')
    (PP (IN In)
      (NP (NN addition)))
    (, ,)
    (NP
      (NP (DT the) (NNS objections))
      (PP (TO to)
        (NP (JJ evolutionary) (NN theory))))
    (VP (VBP mirror)
      (NP
        (NP (DT the) (JJ same) (JJ flawed) (NNS criticisms))
        (PP (IN of)
          (NP (RBR earlier) (JJ religious) (NN opposition)))))
    (. .)))

prep_in(mirror-10, addition-3)
det(objections-6, the-5)
nsubj(mirror-10, objections-6)
amod(theory-9, evolutionary-8)
prep_to(objections-6, theory-9)
root(ROOT-0, mirror-10)
det(criticisms-14, the-11)
amod(criticisms-14, same-12)
amod(criticisms-14, flawed-13)
dobj(mirror-10, criticisms-14)
advmod(opposition-18, earlier-16)
amod(opposition-18, religious-17)
prep_of(criticisms-14, opposition-18)

