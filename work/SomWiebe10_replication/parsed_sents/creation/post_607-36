(ROOT
  (S
    (ADVP (RB Right) (RB now))
    (NP (DT many))
    (VP (VBP are)
      (VP (VBG using)
        (NP (NN evolution))
        (SBAR (RB as) (IN if)
          (S
            (NP (PRP it))
            (ADVP (RB already))
            (VP (VBZ has)
              (VP (VBN proven)
                (SBAR (IN that)
                  (S
                    (NP (NNP God))
                    (VP (VBZ has)
                      (NP (NN nothing)
                        (S
                          (VP (TO to)
                            (VP (VB do)
                              (PP (IN with)
                                (NP (PRP$ our) (NN existence))))))))))))))))
    (. .)))

