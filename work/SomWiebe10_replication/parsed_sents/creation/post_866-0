(ROOT
  (S
    (NP (PRP I))
    (VP (VBP am)
      (NP
        (NP (DT a) (JJ strong) (NN believer))
        (PP (IN in)
          (NP
            (NP (DT the) (NN authority)
              (CC and)
              (NN inspiration))
            (PP (IN of)
              (NP
                (NP (DT the) (NN word))
                (PP (IN of)
                  (NP (NNP God)))))))))
    (. .)))

nsubj(believer-5, I-1)
cop(believer-5, am-2)
det(believer-5, a-3)
