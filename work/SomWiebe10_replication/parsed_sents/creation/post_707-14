(ROOT
  (S
    (ADVP
      (NP (DT Any) (JJ metaphorical) (NNS meanings))
      (RB aside))
    (NP (DT the) (NN text))
    (VP (VBZ is)
      (ADJP (JJ literal)))
    (. .)))

det(meanings-3, Any-1)
amod(meanings-3, metaphorical-2)
npadvmod(aside-4, meanings-3)
advmod(literal-8, aside-4)
det(text-6, the-5)
nsubj(literal-8, text-6)
cop(literal-8, is-7)
root(ROOT-0, literal-8)

(ROOT
  (SQ (MD Can)
    (NP (NN someone))
    (VP (`` ``) (VB prove) ('' '')
      (S
        (NP (DT the) (NNP Bible))
        (VP (VBZ means)
          (NP (DT a) (JJ literal) (CD 6) (NN day))
          (FRAG
            (NP (NN creation))
            (. .)))))
    (. .)))

aux(prove-4, Can-1)
nsubj(prove-4, someone-2)
root(ROOT-0, prove-4)
det(Bible-7, the-6)
nsubj(means-8, Bible-7)
ccomp(prove-4, means-8)
det(day-12, a-9)
amod(day-12, literal-10)
num(day-12, 6-11)
tmod(means-8, day-12)
dep(means-8, creation-13)

