(ROOT
  (S
    (NP (PRP they))
    (VP (VBP are)
      (PP (IN in)
        (NP
          (NP (PRP$ my) (NN opinion))
          (X
            (ADVP (RB clearly))
            (NP (JJ inferior) (NNS versions))
            (. .)))))
    (. .)))

nsubj(are-2, they-1)
root(ROOT-0, are-2)
poss(opinion-5, my-4)
prep_in(are-2, opinion-5)
dep(versions-8, clearly-6)
amod(versions-8, inferior-7)
dep(opinion-5, versions-8)

