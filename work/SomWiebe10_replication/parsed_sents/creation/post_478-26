(ROOT
  (S
    (NP (PRP They))
    (VP (VBP assume)
      (NP (PRP$ their) (NN theory)
        (S
          (VP (TO to)
            (VP (VB prove)
              (NP (PRP$ their) (NN theory)))))))
    (. .)))

nsubj(assume-2, They-1)
root(ROOT-0, assume-2)
poss(theory-4, their-3)
dobj(assume-2, theory-4)
aux(prove-6, to-5)
infmod(theory-4, prove-6)
poss(theory-8, their-7)
dobj(prove-6, theory-8)

