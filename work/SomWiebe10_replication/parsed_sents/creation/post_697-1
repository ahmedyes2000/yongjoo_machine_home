(ROOT
  (S (CC And)
    (NP (DT the) (JJ same) (NN God))
    (VP (VB condemn)
      (NP (PRP it))
      (ADVP
        (NP (NNS centuries))
        (RB later)))
    (. .)))

cc(condemn-5, And-1)
det(God-4, the-2)
amod(God-4, same-3)
nsubj(condemn-5, God-4)
root(ROOT-0, condemn-5)
dobj(condemn-5, it-6)
npadvmod(later-8, centuries-7)
advmod(condemn-5, later-8)

