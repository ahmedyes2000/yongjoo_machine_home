  (S
    (SBAR (IN If)
      (S
        (NP (JJ scientific) (NN inquiry))
        (VP (VBZ has)
          (VP (VBN shown)
            (ADVP (RB conclusively))
            (SBAR (IN that)
              (S
                (NP (NNP Genesis))
                (VP (VBZ is)
                  (ADJP
                    (ADVP (IN at) (JJS best))
                    (JJ allegorical)))))))))
    (, ,)
    (ADVP (RB then))
    (NP (NNS Christians))
    (VP (MD should)
      (VP (VB reconsider)
        (NP
          (NP (NNS beliefs))
