(ROOT
  (S
    (S
      (NP (PRP We))
      (VP (VB don)
        (NP (NN t))
        (VP (VB have)
          (NP (PDT such) (DT a) (NN theory))
          (ADVP (RB right) (RB now)))))
    (, ,)
    (CC and)
    (S
      (NP (DT that))
      (VP (VBZ s)
        (NP (DT a) (NN problem))))
    (. .)))

nsubj(don-2, We-1)
root(ROOT-0, don-2)
dobj(don-2, t-3)
dep(don-2, have-4)
predet(theory-7, such-5)
det(theory-7, a-6)
