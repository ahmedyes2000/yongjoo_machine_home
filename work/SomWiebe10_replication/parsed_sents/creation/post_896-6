                (NP (DT this) (NN topic) (NN titled) ('' '') (NN Think))
                (PP (IN about)
                  (NP (DT this)))
                ('' '')))
            (-RRB- -RRB-)))
        (ADVP (RB Also))))
    (, ,)
    (NP (DT the) (NNP Kent) (NNP Hovind) (NN thing))
    (VP (VBD was)
      (NP
        (NP (DT an) (NN ad) (NN hominem))
        (, ,) (RB not)
        (NP (DT a) (NN straw) (NN man))))
    (. .)))

