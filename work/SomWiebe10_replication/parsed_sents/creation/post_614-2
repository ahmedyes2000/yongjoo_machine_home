(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is)
      (NP
        (NP (DT a) (NN part))
        (PP (IN of)
          (NP (NN science))))
      (SBAR (IN if)
        (S
          (NP (NN science))
          (VP (VBZ is)
            (NP
              (NP (DT a) (NN search))
              (PP (IN for)
                (NP (DT the) (NN truth))))))))
    (. .)))

nsubj(part-4, It-1)
cop(part-4, is-2)
det(part-4, a-3)
root(ROOT-0, part-4)
prep_of(part-4, science-6)
mark(search-11, if-7)
