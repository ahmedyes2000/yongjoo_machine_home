(ROOT
  (S
    (S
      (VP (VBG '')))
    (: ...)
    (CC and)
    (S
      (VP (VBP are)
        (VP (VBG conducting)
          (NP (JJ scientific) (NN research))
          (PP (IN into)
            (NP
              (NP (NNP ID))
              (`` ``)
              (NP (NNP Riiiight)))))))
    (. .)))

aux(conducting-5, are-4)
conj_and(''-1, conducting-5)
amod(research-7, scientific-6)
dobj(conducting-5, research-7)
prep_into(conducting-5, ID-9)
dep(ID-9, Riiiight-11)

