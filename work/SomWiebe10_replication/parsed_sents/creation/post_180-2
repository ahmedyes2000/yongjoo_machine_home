(ROOT
  (S ('' '')
    (S
      (NP (NNP ID))
      (VP (VBD did)
        (VP (VB make)
          (NP (DT a) (NN prediction)))))
    (, ,)
    (CC and)
    (S
      (NP (DT the) (NN prediction))
      (VP (VBD was)
        (ADJP (JJ accurate))))
    (. .)))

nsubj(make-4, ID-2)
aux(make-4, did-3)
root(ROOT-0, make-4)
det(prediction-6, a-5)
dobj(make-4, prediction-6)
det(prediction-10, the-9)
nsubj(accurate-12, prediction-10)
cop(accurate-12, was-11)
conj_and(make-4, accurate-12)

