(ROOT
  (S ('' '')
    (S
      (SBAR
        (WHADVP (WRB Where))
        (S
          (VP (VBZ does)
            (NP (JJ evolutionary) (NN theory)))))
      (VP (VBP say)))
    (IN that)
    (S
      (NP (PRP it))
      (VP (MD will)
        (VP (VB be)
          (ADJP (JJ slow)
            (CC or)
            (JJ gradual)))))
    (. ?)))

advmod(does-3, Where-2)
advcl(say-6, does-3)
csubj(say-6, does-3)
amod(theory-5, evolutionary-4)
dobj(does-3, theory-5)
root(ROOT-0, say-6)
dep(say-6, that-7)
nsubj(slow-11, it-8)
nsubj(gradual-13, it-8)
aux(slow-11, will-9)
cop(slow-11, be-10)
ccomp(say-6, slow-11)
ccomp(say-6, gradual-13)
conj_or(slow-11, gradual-13)

