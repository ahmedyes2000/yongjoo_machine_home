(ROOT
  (S
    (NP
      (NP (NNS Supporters))
      (PP (IN of)
        (NP (NNP Creationism))))
    (ADVP (RB then))
    (VP (VBP cite)
      (S
        (VP (VBG supporting)
          (NP (NN evidence))
          (PP (IN for)
            (NP (DT this) (NN theory))))))
    (. .)))

nsubj(cite-5, Supporters-1)
prep_of(Supporters-1, Creationism-3)
advmod(cite-5, then-4)
root(ROOT-0, cite-5)
xcomp(cite-5, supporting-6)
dobj(supporting-6, evidence-7)
det(theory-10, this-9)
prep_for(supporting-6, theory-10)

