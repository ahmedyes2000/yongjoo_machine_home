(ROOT
  (FRAG ('' '')
    (NP (RB Seems) (JJ reasonable))
    (PP (TO to)
      (NP (PRP me)))
    (. .)))

advmod(reasonable-3, Seems-2)
root(ROOT-0, reasonable-3)
dep(reasonable-3, to-4)
pobj(to-4, me-5)

(ROOT
  (S (CC And)
    (ADVP (RB since))
    (NP (DT that))
    (VP (VBZ is)
      (ADJP (RB still)
        (ADJP (JJ true))
        (ADVP (RB today))
        (, ,)))
    (: ...)))

cc(true-6, And-1)
advmod(true-6, since-2)
nsubj(true-6, that-3)
cop(true-6, is-4)
advmod(true-6, still-5)
root(ROOT-0, true-6)
advmod(true-6, today-7)

