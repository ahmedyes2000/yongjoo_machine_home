(ROOT
  (S
    (NP (PRP I))
    (ADVP (RB just))
    (VP (VBD wanted)
      (S
        (VP (TO to)
          (VP (VB get)
            (NP (DT a) (JJR better) (NN handle))
            (PP (IN on)
              (SBAR
                (WHADVP (WRB where))
                (S
                  (NP (PRP you))
                  (VP (VBD were)
                    (VP (VBG coming)
                      (PP (IN from)
                        (PP (IN with)
                          (NP (PRP$ your) (NN argument)))))))))))))
    (. .)))

nsubj(wanted-3, I-1)
xsubj(get-5, I-1)
advmod(wanted-3, just-2)
root(ROOT-0, wanted-3)
aux(get-5, to-4)
xcomp(wanted-3, get-5)
det(handle-8, a-6)
amod(handle-8, better-7)
dobj(get-5, handle-8)
advmod(coming-13, where-10)
nsubj(coming-13, you-11)
aux(coming-13, were-12)
prepc_on(get-5, coming-13)
