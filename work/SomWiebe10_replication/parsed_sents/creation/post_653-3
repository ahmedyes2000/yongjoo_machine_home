(ROOT
  (S ('' '')
    (PP (IN If)
      (S
        (NP (PRP you))
        (VP (VBP are)
          (ADJP (JJ interested)
            (PP (IN in) (`` ``)
              (NP (JJ continuing) (DT the) (NN debate))
              ('' ''))))))
    (VP
      (ADVP (RB then))
      (VBP present)
      (NP (PRP$ your) (NNS arguments)))
    (. .)))

nsubj(interested-5, you-3)
cop(interested-5, are-4)
prepc_if(present-13, interested-5)
amod(debate-10, continuing-8)
det(debate-10, the-9)
prep_in(interested-5, debate-10)
advmod(present-13, then-12)
root(ROOT-0, present-13)
poss(arguments-15, your-14)
dobj(present-13, arguments-15)

