(ROOT
  (S ('' '')
    (NP (DT This))
    (VP (VBZ is)
      (ADJP (RB as) (JJ good))
      (SBAR (IN as)
        (S
          (NP (PRP it))
          (VP (VBZ gets) (, ,)
            (ADVP (RB straight))
            (PP (IN from)
              (NP
                (NP (PRP$ their) (NN prophet) (POS 's))
                (NN mouth)))))))
    (. .)))

nsubj(good-5, This-2)
cop(good-5, is-3)
advmod(good-5, as-4)
root(ROOT-0, good-5)
mark(gets-8, as-6)
nsubj(gets-8, it-7)
advcl(good-5, gets-8)
advmod(gets-8, straight-10)
poss(prophet-13, their-12)
poss(mouth-15, prophet-13)
prep_from(gets-8, mouth-15)

