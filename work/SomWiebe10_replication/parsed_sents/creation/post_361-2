(ROOT
  (S (CC But)
    (NP
      (NP (NNP Christianity))
      (CC and)
      (NP (JJ other) (JJ theistic) (NNS religions)))
    (VP (VBP maintain)
      (SBAR (IN that)
        (S
          (NP (NNP God))
          (VP (VBD created)
            (NP (NN heaven)
              (CC and)
              (NN earth))
            (, ,)
            (PP (IN in)
              (NP
                (NP (JJ other) (NNS words))
                (, ,)
                (NP (NN everything))))))))
    (. .)))

cc(maintain-7, But-1)
nsubj(maintain-7, Christianity-2)
amod(religions-6, other-4)
amod(religions-6, theistic-5)
conj_and(Christianity-2, religions-6)
nsubj(maintain-7, religions-6)
root(ROOT-0, maintain-7)
