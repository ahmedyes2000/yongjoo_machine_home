(ROOT
  (S (CC and)
    (ADVP (RB then))
    (VP (VBD mimicking)
      (NP (PRP it))
      (SBAR
        (S
          (NP (DT The) (NN dragonfly))
          (VP (VBD gave)
            (NP (PRP us))
            (NP
              (NP (NNS ideas))
              (PP (IN on)
                (SBAR
                  (WHADVP (WRB how))
                  (S
                    (VP (TO to)
                      (VP (VB build)
                        (NP (DT a) (NN helicopter))))))))))))
    (: ...)))

cc(mimicking-3, and-1)
advmod(mimicking-3, then-2)
root(ROOT-0, mimicking-3)
