(ROOT
  (S
    (S
      (ADVP (RB Certainly))
      (NP (DT the) (JJ individual) (NN mind))
      (VP (VBZ has)
        (NP
          (NP (DT no) (NN value))
          (PP (IN in)
            (NP (JJ todays) (NN society))))))
    (, ,)
    (CC and)
    (S
      (NP (RB only) (DT the) (NNP GROUP) (NNP MIND))
      (VP (MD can)
        (VP (VB come)
          (PP (TO to)
            (NP
              (NP (DT any) (JJ real) (NN conclusion))
              (PP (IN in)
                (NP (DT the) (JJ modern) (NN sense))))))))
    (. .)))

advmod(has-5, Certainly-1)
det(mind-4, the-2)
amod(mind-4, individual-3)
nsubj(has-5, mind-4)
root(ROOT-0, has-5)
det(value-7, no-6)
dobj(has-5, value-7)
amod(society-10, todays-9)
prep_in(value-7, society-10)
advmod(MIND-16, only-13)
det(MIND-16, the-14)
nn(MIND-16, GROUP-15)
nsubj(come-18, MIND-16)
aux(come-18, can-17)
conj_and(has-5, come-18)
det(conclusion-22, any-20)
amod(conclusion-22, real-21)
prep_to(come-18, conclusion-22)
det(sense-26, the-24)
amod(sense-26, modern-25)
prep_in(conclusion-22, sense-26)

