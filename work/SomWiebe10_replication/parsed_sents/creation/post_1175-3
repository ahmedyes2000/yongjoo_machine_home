(ROOT
  (S
    (NP (PRP You))
    (ADVP (RB either))
    (VP (VBP know)
      (SBAR
        (S
          (S
            (NP (PRP he))
            (VP (VBZ does) (RB n't)
              (VP (VB exist))))
          (CC or)
          (S
            (NP (PRP you))
            (VP (VBP are)
              (VP (VBN deluded)))))))
    (. .)))

nsubj(know-3, You-1)
advmod(know-3, either-2)
root(ROOT-0, know-3)
nsubj(exist-7, he-4)
aux(exist-7, does-5)
neg(exist-7, n't-6)
dep(know-3, exist-7)
nsubjpass(deluded-11, you-9)
auxpass(deluded-11, are-10)
dep(know-3, deluded-11)
conj_or(exist-7, deluded-11)

