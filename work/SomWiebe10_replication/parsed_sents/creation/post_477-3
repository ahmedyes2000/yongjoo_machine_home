(ROOT
  (SBAR
    (WHADVP (WRB Why))
    (S
      (SBAR (IN if)
        (S
          (NP (PRP it))
          (VP (VBZ is)
            (ADJP (JJ true)))))
      (VP (VBZ ?!!!)))))

advmod(?!!!-6, Why-1)
mark(true-5, if-2)
nsubj(true-5, it-3)
cop(true-5, is-4)
csubj(?!!!-6, true-5)
root(ROOT-0, ?!!!-6)

