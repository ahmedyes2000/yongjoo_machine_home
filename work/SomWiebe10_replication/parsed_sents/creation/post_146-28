(ROOT
  (NP
    (NP (DT some) (NN form))
    (PP (IN of)
      (NP (NN reason)))
    (S
      (VP (TO to)
        (VP (VB exist)
          (FRAG
            (PP (IN of)
              (NP (NNS sorts)))
            (. .)))))
    (. .)))

det(form-2, some-1)
root(ROOT-0, form-2)
prep_of(form-2, reason-4)
aux(exist-6, to-5)
infmod(form-2, exist-6)
dep(exist-6, of-7)
pobj(of-7, sorts-8)

