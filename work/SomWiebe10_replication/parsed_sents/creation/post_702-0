(ROOT
  (S
    (NP (JJ Several) (NNS scientists))
    (VP (VBP have)
      (ADVP (RB already))
      (VP (VBN admitted)
        (SBAR (IN that)
          (S
            (NP
              (NP (DT the) (NN account))
              (PP (IN of)
                (NP
                  (NP (NN Creation) (RB as))
                  (VP (VBN described)
                    (PP (IN in)
                      (NP (NNP Genesis)))))))
            (VP (VBZ has)
              (NP (JJ definite) (NN validity)))))))
    (. .)))

amod(scientists-2, Several-1)
nsubj(admitted-5, scientists-2)
aux(admitted-5, have-3)
advmod(admitted-5, already-4)
root(ROOT-0, admitted-5)
complm(has-15, that-6)
det(account-8, the-7)
nsubj(has-15, account-8)
