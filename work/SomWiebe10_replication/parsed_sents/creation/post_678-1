(ROOT
  (S
    (NP (JJ Coherent) (NNS arguments))
    (VP (VBP are) (RB not)
      (ADVP (RB really))
      (NP
        (NP (DT a) (NN requirement))
        (PP (IN for)
          (NP (NNP ID)))))
    (. .)))

amod(arguments-2, Coherent-1)
nsubj(requirement-7, arguments-2)
cop(requirement-7, are-3)
neg(requirement-7, not-4)
advmod(requirement-7, really-5)
det(requirement-7, a-6)
root(ROOT-0, requirement-7)
prep_for(requirement-7, ID-9)

