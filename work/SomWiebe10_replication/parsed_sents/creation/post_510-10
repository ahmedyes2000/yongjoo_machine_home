(ROOT
  (S
    (NP (NNS Irons))
    (VP (VBZ uses)
      (NP (JJ poor) (NNS sources))
      (PP (IN for)
        (NP
          (NP (JJ many))
          (PP (IN of)
            (NP (PRP$ his) (NNS claims)))
          (, ,)
          (SBAR
            (WHNP (WDT which))
            (S
              (VP (MD may)
                (VP (VB be)
                  (SBAR
                    (WHADVP (WRB why))
                    (S
                      (NP (PRP he))
                      (VP (VBZ is)
                        (ADJP (RB so) (JJ inaccurate))))))))))))
    (. .)))

nsubj(uses-2, Irons-1)
root(ROOT-0, uses-2)
amod(sources-4, poor-3)
dobj(uses-2, sources-4)
prep_for(uses-2, many-6)
nsubj(be-13, many-6)
poss(claims-9, his-8)
prep_of(many-6, claims-9)
aux(be-13, may-12)
rcmod(many-6, be-13)
advmod(inaccurate-18, why-14)
nsubj(inaccurate-18, he-15)
cop(inaccurate-18, is-16)
advmod(inaccurate-18, so-17)
advcl(be-13, inaccurate-18)

