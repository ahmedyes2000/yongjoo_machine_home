(ROOT
  (S
    (NP
      (NP (DT The) (JJ following) (NN statement))
      (PP (IN by)
        (NP
          (NP (DT the) (NN employee))
          (PP (IN of)
            (NP (NNP NCSE))))))
    (VP (VBZ is)
      (NP
        (NP (DT the) (JJ exact) (NN form))
        (PP (IN of)
          (NP
            (NP (NN inference))
            (PP (TO to)
              (NP (DT the) (JJS best) (NN explanation))))))
      (SBAR (IN that)
        (S
          (NP (NNP ID) (NNS scientists))
          (VP (VBP use)
            (S
              (VP (TO to)
                (VP (VB demonstrate)
                  (SBAR (IN that)
                    (S
                      (NP (EX there))
                      (VP (VBZ is)
                        (NP
                          (NP (NN intelligence))
                          (PP (IN behind)
                            (NP (NN life))))))))))))))
    (. .)))

