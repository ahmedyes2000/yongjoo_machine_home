(ROOT
  (S
    (VP (VB Look)
      (PRT (RP away))
      (PP (IN from)
        (NP
          (NP (PRP$ your) (NN computer) (NN screen))
          (PP (IN for)
            (NP (DT a) (NN moment)
              (CC and)
              (NN glance)))))
      (PP (IN out)
        (NP (PRP$ your) (NN window))))
    (. .)))

root(ROOT-0, Look-1)
poss(screen-6, your-4)
