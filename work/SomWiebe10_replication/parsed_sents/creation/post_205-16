(ROOT
  (S
    (NP (NNP Science))
    (VP (MD would)
      (ADVP (RB always))
      (VP (VB say) ('' '')
        (S
          (NP (PRP I))
          (VP (VBP do) (RB n't)
            (VP (VB know))))
        (SBAR
          (WHADVP (WRB why))
          (S
            (NP (DT this))
            (VP (VBD happened))))))
    (. .)))

nsubj(say-4, Science-1)
aux(say-4, would-2)
advmod(say-4, always-3)
root(ROOT-0, say-4)
nsubj(know-9, I-6)
aux(know-9, do-7)
neg(know-9, n't-8)
ccomp(say-4, know-9)
advmod(happened-12, why-10)
nsubj(happened-12, this-11)
advcl(say-4, happened-12)

