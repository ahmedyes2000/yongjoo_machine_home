(ROOT
  (S
    (NP (EX There))
    (VP (VBZ is)
      (NP (DT no) (NN reason))
      (SBAR (IN that)
        (S
          (NP
            (NP (DT both) (NNS sides))
            (PP (IN of)
              (NP (DT the) (NN issue))))
          (VP (MD can) (RB not)
            (VP (VB be)
              (VP (VBN talked) (IN about)
                (PP (IN in)
                  (NP (NN class)))))))))
    (. .)))

expl(is-2, There-1)
root(ROOT-0, is-2)
det(reason-4, no-3)
nsubj(is-2, reason-4)
complm(talked-14, that-5)
det(sides-7, both-6)
