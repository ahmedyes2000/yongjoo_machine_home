(ROOT
  (S
    (ADVP (RB Thus))
    (NP (PRP we))
    (VP (VBP start)
      (S
        (VP (TO to)
          (VP (VB understand)
            (NP (DT the) (NNP Goldilocks) (NN aspect))
            (ADVP (RBR better))))))
    (. .)))

advmod(start-3, Thus-1)
nsubj(start-3, we-2)
xsubj(understand-5, we-2)
root(ROOT-0, start-3)
aux(understand-5, to-4)
xcomp(start-3, understand-5)
det(aspect-8, the-6)
nn(aspect-8, Goldilocks-7)
dobj(understand-5, aspect-8)
advmod(understand-5, better-9)

