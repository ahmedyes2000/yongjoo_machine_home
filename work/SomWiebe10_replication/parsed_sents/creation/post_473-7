(ROOT
  (S ('' '')
    (NP (NNP ID))
    (VP (VBZ is)
      (PP (IN about)
        (S
          (VP (VBG showing)
            (NP (NN evidence))
            (S
              (VP (TO to)
                (VP (VB support)
                  (NP (DT this) (NN idea)))))))))
    (. .)))

nsubj(is-3, ID-2)
root(ROOT-0, is-3)
prepc_about(is-3, showing-5)
dobj(showing-5, evidence-6)
aux(support-8, to-7)
xcomp(showing-5, support-8)
det(idea-10, this-9)
dobj(support-8, idea-10)

