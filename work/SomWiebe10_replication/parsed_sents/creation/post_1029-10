(ROOT
  (S
    (NP (PRP I))
    (VP (MD would)
      (VP (VB be)
        (ADJP (JJ interested)
          (PP (IN in)
            (S
              (VP (VBG hearing)
                (NP (PRP$ their) (NN opinion))))))))
    (. .)))

nsubj(interested-4, I-1)
aux(interested-4, would-2)
cop(interested-4, be-3)
root(ROOT-0, interested-4)
prepc_in(interested-4, hearing-6)
poss(opinion-8, their-7)
dobj(hearing-6, opinion-8)

