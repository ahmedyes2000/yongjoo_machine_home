(ROOT
  (S
    (ADVP (RB Surely))
    (PP (IN with)
      (NP
        (NP (CD 90,000) (NN scientist))
        (PP (IN on)
          (NP (PRP$ their) (NN side)))))
    (NP (PRP they))
    (VP (MD can)
      (VP (VB do)
        (ADVP (RBR better))
        (PP (IN than)
          (NP (DT that)))))
    (. .)))

advmod(do-10, Surely-1)
num(scientist-4, 90,000-3)
prep_with(do-10, scientist-4)
poss(side-7, their-6)
prep_on(scientist-4, side-7)
nsubj(do-10, they-8)
aux(do-10, can-9)
root(ROOT-0, do-10)
advmod(do-10, better-11)
prep_than(do-10, that-13)

