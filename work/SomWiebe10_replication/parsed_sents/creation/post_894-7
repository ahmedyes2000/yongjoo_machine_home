(ROOT
  (S
    (NP (DT That))
    (VP (VBZ makes)
      (SBAR
        (S
          (NP (DT the) (JJ whole) (NN thing))
          (VP (VBP seem)
            (ADJP (RB even) (RBR more) (JJ ridiculous))))))
    (. .)))

nsubj(makes-2, That-1)
root(ROOT-0, makes-2)
det(thing-5, the-3)
amod(thing-5, whole-4)
nsubj(ridiculous-9, thing-5)
cop(ridiculous-9, seem-6)
advmod(ridiculous-9, even-7)
advmod(ridiculous-9, more-8)
ccomp(makes-2, ridiculous-9)

