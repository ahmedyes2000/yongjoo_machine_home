(ROOT
  (S
    (NP
      (NP (NNP BTW) (NN everything))
      (PP (IN in)
        (NP (PRP$ your) (NN position))))
    (VP (VBZ is)
      (PP (IN beyond)
        (NP (NN observation))))
    (. .)))

nn(everything-2, BTW-1)
nsubj(is-6, everything-2)
poss(position-5, your-4)
prep_in(everything-2, position-5)
root(ROOT-0, is-6)
prep_beyond(is-6, observation-8)

