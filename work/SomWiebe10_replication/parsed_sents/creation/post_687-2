(ROOT
  (S
    (NP (NNP AND) (DT the) (NNP EARTH))
    (VP (VBD was)
      (VP (VBN recreated)
        (PP (IN at)
          (NP
            (NP (DT some) (NN point))
            (PP (IN in)
              (NP
                (NP (DT the) (NN kingdom))
                (PP (IN of)
                  (NP
                    (NP (NNP HEAVEN) (POS 's))
                    (NN time) (NN line)))))))))
    (: ...)))

nn(EARTH-3, AND-1)
det(EARTH-3, the-2)
nsubjpass(recreated-5, EARTH-3)
auxpass(recreated-5, was-4)
root(ROOT-0, recreated-5)
det(point-8, some-7)
prep_at(recreated-5, point-8)
det(kingdom-11, the-10)
prep_in(point-8, kingdom-11)
poss(line-16, HEAVEN-13)
nn(line-16, time-15)
prep_of(kingdom-11, line-16)

