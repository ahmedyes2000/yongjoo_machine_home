(ROOT
  (S
    (NP (NNP Biblical) (NNP Hebrew))
    (VP (VBZ is)
      (ADJP (RB considerably) (JJ different)
        (PP (IN from)
          (NP
            (NP (DT any) (NN language))
            (VP (VBN spoken)
              (NP (NN today)))))))
    (. .)))

nn(Hebrew-2, Biblical-1)
nsubj(different-5, Hebrew-2)
cop(different-5, is-3)
advmod(different-5, considerably-4)
root(ROOT-0, different-5)
det(language-8, any-7)
prep_from(different-5, language-8)
partmod(language-8, spoken-9)
tmod(spoken-9, today-10)

