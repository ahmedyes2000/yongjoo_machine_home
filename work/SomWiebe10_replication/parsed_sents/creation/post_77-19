(ROOT
  (S
    (NP (NNP ID) (NNS scientists))
    (VP (VBP do) (RB not)
      (VP (VB have)
        (NP
          (NP (DT a) (NN problem))
          (PP (IN with)
            (NP
              (NP (NN definition))
              (ADJP (# #) (CD 1)))))))
    (. .)))

nn(scientists-2, ID-1)
nsubj(have-5, scientists-2)
aux(have-5, do-3)
neg(have-5, not-4)
root(ROOT-0, have-5)
det(problem-7, a-6)
dobj(have-5, problem-7)
prep_with(problem-7, definition-9)
