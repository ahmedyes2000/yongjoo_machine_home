  (SQ (MD Can)
    (NP (PRP I))
    (VP (VB do)
      (NP (DT the) (JJ same) (NN thing))
      (PP (IN with)
        (NP
          (NP (DT a) (NN bird))
          (CC or)
          (NP (DT a) (NN lizard)))))
    (. ?)))

aux(do-3, Can-1)
