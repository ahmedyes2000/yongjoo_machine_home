(ROOT
  (S
    (SBAR (IN Because)
      (S
        (NP (PRP$ our) (NN planet))
        (VP (VBZ is)
          (ADVP (RB currently))
          (VP (VBG changing)))))
    (PRN (, ,)
      (S
        (NP (DT some) (NNS animals))
        (VP (VBP need)
          (S
            (VP (TO to)
              (VP (VB adapt)
                (PP (TO to)
                  (NP (DT that))))))))
      (, ,))
    (SBAR (IN if)
      (S
        (NP (PRP they))
        (VP
          (VP (VBP do) (RB n't))
          (CC or)
          (VP (VBP are)
            (ADJP (JJ incapable))))))
    (ADVP (RB then))
    (NP (PRP they))
    (VP (MD can)
      (VP (VB become)
        (ADJP (JJ extinct))))
    (. .)))

mark(changing-6, Because-1)
poss(planet-3, our-2)
nsubj(changing-6, planet-3)
aux(changing-6, is-4)
advmod(changing-6, currently-5)
advcl(extinct-27, changing-6)
det(animals-9, some-8)
nsubj(need-10, animals-9)
xsubj(adapt-12, animals-9)
parataxis(extinct-27, need-10)
aux(adapt-12, to-11)
xcomp(need-10, adapt-12)
prep_to(adapt-12, that-14)
mark(do-18, if-16)
nsubj(do-18, they-17)
nsubj(incapable-22, they-17)
dep(extinct-27, do-18)
neg(do-18, n't-19)
cop(incapable-22, are-21)
conj_or(do-18, incapable-22)
dep(extinct-27, incapable-22)
advmod(extinct-27, then-23)
nsubj(extinct-27, they-24)
aux(extinct-27, can-25)
cop(extinct-27, become-26)
root(ROOT-0, extinct-27)

