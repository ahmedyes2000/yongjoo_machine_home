  (S
    (PP (IN In)
      (NP (JJ similar) (NN news)))
    (, ,)
    (NP
      (NP (JJ noted) (NNS scientists))
      (CC and)
      (NP (NNP Jesuit) (NNP astronomer) (NNP George) (NNP Coyne)))
    (VP (VBZ is)
      (PP (IN on)
        (NP (DT a) (NN mission)
          (S
            (VP (TO to)
              (VP (VB expose)
                (NP
                  (NP (DT the)
                    (ADJP (JJ scientific)
                      (CC and)
                      (JJ theological))
                    (NN vacuity))
                  (PP (IN of)
                    (NP (NNP Intelligent) (NNP Design))))))))))
    (. .)))

