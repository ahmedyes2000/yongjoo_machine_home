(ROOT
  (SBARQ
    (WHADVP (WRB Why))
    (SQ (VBZ is)
      (NP (PRP it))
      (ADJP (RB so) (JJ hard)
        (PP (IN for)
          (NP (NNP ID) (NNS proponents)
            (S
              (VP (TO to)
                (VP (VB address)
                  (NP (DT these) (JJ simple) (NNS facts)))))))))
    (. .)))

advmod(hard-5, Why-1)
cop(hard-5, is-2)
nsubj(hard-5, it-3)
advmod(hard-5, so-4)
root(ROOT-0, hard-5)
nn(proponents-8, ID-7)
prep_for(hard-5, proponents-8)
aux(address-10, to-9)
infmod(proponents-8, address-10)
det(facts-13, these-11)
amod(facts-13, simple-12)
dobj(address-10, facts-13)

