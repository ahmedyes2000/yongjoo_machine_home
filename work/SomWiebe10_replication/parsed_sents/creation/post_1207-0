(ROOT
  (S
    (VP (VB Go)
      (VP (VBN read) ('' '')
        (PP (IN On)
          (NP
            (NP (DT the) (NNP Origin))
            (PP (IN of)
              (NP (NNP Species)))))
        ('' '')))
    (: ...)))

root(ROOT-0, Go-1)
dep(Go-1, read-2)
det(Origin-6, the-5)
prep_on(read-2, Origin-6)
prep_of(Origin-6, Species-8)

