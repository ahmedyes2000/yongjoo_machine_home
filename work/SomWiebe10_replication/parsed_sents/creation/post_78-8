(ROOT
  (S
    (SBAR (IN While)
      (S
        (NP (NN chance))
        (VP (MD may)
          (VP (VB need)
            (NP (DT no) (JJ further) (NN explanation))))))
    (, ,)
    (NP (NN regularity))
    (VP (VBZ refers)
      (PP (TO to)
        (NP
          (NP (DT a) (NN law))
          (PP (IN like)
            (NP (NN process))))))
    (. .)))

