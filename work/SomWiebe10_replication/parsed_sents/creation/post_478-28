(ROOT
  (S
    (NP (DT The) (NN evidence))
    (VP (VBZ is)
      (ADVP (RB really))
      (ADJP
        (ADJP
          (NP (DT a) (RB lot))
          (RBR less) (JJ convincing))
        (SBAR (IN that)
          (S
            (NP (PRP you))
            (VP (VBP seem)
              (S
                (VP (TO to)
                  (VP (VB think)))))))))
    (. .)))

det(evidence-2, The-1)
nsubj(convincing-8, evidence-2)
cop(convincing-8, is-3)
advmod(convincing-8, really-4)
det(lot-6, a-5)
dep(convincing-8, lot-6)
advmod(convincing-8, less-7)
root(ROOT-0, convincing-8)
complm(seem-11, that-9)
nsubj(seem-11, you-10)
xsubj(think-13, you-10)
ccomp(convincing-8, seem-11)
aux(think-13, to-12)
xcomp(seem-11, think-13)

