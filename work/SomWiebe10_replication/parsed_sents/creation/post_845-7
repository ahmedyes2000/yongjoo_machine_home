(ROOT
  (S
    (NP
      (NP (DT An) (NN observer))
      (PP (IN on)
        (NP (NNP Betelgeuse))))
    (VP (MD could)
      (VP (VB make)
        (NP
          (NP (DT a) (JJ crude) (NN estimate))
          (PP (IN of)
            (NP
              (NP (DT the) (NN earth) (POS 's))
              (NN age))))
        (PP (IN by)
          (S
            (VP (VBG comparing)
              (NP (PRP$ our) (NN sun))
              (PP (IN with)
                (NP (JJ other) (JJ main) (NN sequence) (NNS stars))))))))
    (. .)))

