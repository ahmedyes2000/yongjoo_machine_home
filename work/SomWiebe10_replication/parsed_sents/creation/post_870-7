(ROOT
  (S ('' '')
    (PP (IN After)
      (NP (DT all)))
    (, ,)
    (VP (VBZ is) (RB n't)
      (NP (NN religion))
      (PP
        (PP (IN about)
          (NP
            (NP (JJ spiritual) (NNS matters))
            (, ,)
            (NP
              (NP (NN man) (POS 's))
              (NN soul))))
        (, ,)
        (CC and)
        (RB not)
        (PP (IN about)
          (NP (DT the) (NN material) (NN world)))))
    (. ?)))

prep_after(religion-7, all-3)
cop(religion-7, is-5)
neg(religion-7, n't-6)
root(ROOT-0, religion-7)
prep(religion-7, about-8)
amod(matters-10, spiritual-9)
pobj(about-8, matters-10)
poss(soul-14, man-12)
