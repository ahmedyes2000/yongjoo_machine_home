(ROOT
  (S ('' '')
    (NP (DT This))
    (VP (VBZ is) (, ,)
      (NP
        (PP (IN of)
          (NP (NN course)))
        (, ,)
        (NP (DT a)
          (ADJP (RB completely) (JJ unwarranted))
          (NN conclusion))))
    (. .)))

nsubj(conclusion-11, This-2)
cop(conclusion-11, is-3)
prep_of(conclusion-11, course-6)
det(conclusion-11, a-8)
advmod(unwarranted-10, completely-9)
amod(conclusion-11, unwarranted-10)
root(ROOT-0, conclusion-11)

