(ROOT
  (S
    (NP (PRP You))
    (ADVP (RB always))
    (VP (VBP pick)
      (NP (DT the) (JJS narrowest) (NN interpretation))
      (SBAR (RB as) (IN if)
        (S
          (NP (PRP it))
          (VP (VBD was)
            (NP (DT the) (JJ only) (NN interpretation))))))
    (. .)))

nsubj(pick-3, You-1)
advmod(pick-3, always-2)
root(ROOT-0, pick-3)
det(interpretation-6, the-4)
amod(interpretation-6, narrowest-5)
dobj(pick-3, interpretation-6)
advmod(interpretation-13, as-7)
dep(interpretation-13, if-8)
nsubj(interpretation-13, it-9)
