(ROOT
  (S
    (VP
      (VP (VB Explain)
        (SBAR
          (WHADVP (WRB how))
          (S
            (NP (DT this) (`` ``) (NN information) ('' ''))
            (VP (VBZ is)
              (VP (VBN transferred)
                (CC and)
                (VBN adapted)
                (PP (IN between)
                  (NP (VBG living) (NNS organisms))))))))
      (CC and)
      (VP (VB provide)
        (NP (NNS examples))))
    (. .)))

root(ROOT-0, Explain-1)
advmod(transferred-8, how-2)
det(information-5, this-3)
nsubjpass(transferred-8, information-5)
nsubjpass(adapted-10, information-5)
auxpass(transferred-8, is-7)
ccomp(Explain-1, transferred-8)
ccomp(Explain-1, adapted-10)
conj_and(transferred-8, adapted-10)
amod(organisms-13, living-12)
prep_between(transferred-8, organisms-13)
conj_and(Explain-1, provide-15)
dobj(provide-15, examples-16)

