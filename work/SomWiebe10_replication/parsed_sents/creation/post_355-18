(ROOT
  (S
    (NP (PRP I))
    (VP (VBP think)
      (SBAR
        (S
          (NP (PRP they))
          (VP (VBP are)
            (VP (VBG shooting)
              (NP (PRP themselves))
              (PP (IN in)
                (NP
                  (NP (DT the) (NN foot))
                  (SBAR
                    (WHADVP (WRB when))
                    (S
                      (NP (PRP they))
                      (VP (VBP do)
                        (NP (DT this))))))))))))
    (. .)))

nsubj(think-2, I-1)
root(ROOT-0, think-2)
nsubj(shooting-5, they-3)
