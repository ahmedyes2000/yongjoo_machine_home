(ROOT
  (S
    (PP (IN For)
      (NP (PRP$ our) (NN discussion)))
    (NP (JJ faux) (NN rust))
    (VP (VBZ is)
      (NP (DT the) (JJS most) (NN germaine)))
    (. .)))

poss(discussion-3, our-2)
prep_for(germaine-9, discussion-3)
amod(rust-5, faux-4)
nsubj(germaine-9, rust-5)
cop(germaine-9, is-6)
det(germaine-9, the-7)
amod(germaine-9, most-8)
root(ROOT-0, germaine-9)

