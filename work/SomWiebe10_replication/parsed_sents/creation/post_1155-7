(ROOT
  (S
    (NP (DT The) (NN following))
    (VP (VBP are)
      (NP
        (NP (DT some))
        (PP (IN of)
          (NP
            (NP (DT the) (NNS phenomena))
            (VP (VBN involved))))))
    (. .)))

det(following-2, The-1)
nsubj(some-4, following-2)
cop(some-4, are-3)
root(ROOT-0, some-4)
det(phenomena-7, the-6)
