(ROOT
  (S
    (NP (DT Neither))
    (VP (MD can)
      (NP (PRP it))
      (S
        (VP (VB change)
          (NP (DT a) (NN cart))
          (PP (IN into)
            (NP (DT a) (NN boat)
              (CC or)
              (NN airplane))))))
    (. .)))

nsubj(can-2, Neither-1)
root(ROOT-0, can-2)
dobj(can-2, it-3)
dep(can-2, change-4)
det(cart-6, a-5)
dobj(change-4, cart-6)
det(boat-9, a-8)
prep_into(change-4, boat-9)
prep_into(change-4, airplane-11)
conj_or(boat-9, airplane-11)

