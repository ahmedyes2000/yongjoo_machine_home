    (VP (VB Show)
      (NP (PRP us))
      (ADVP (RB either))
      (SBAR
        (SBAR
          (WHADVP (WRB how))
          (S
            (NP (NNP ID))
            (VP (VBZ explains)
              (NP (NNP UcD)))))
        (CC or)
        (SBAR
          (WHADVP (WRB how))
          (S
            (NP (NN mutation) (NNS rates))
            (VP (VBP need)
              (S
                (VP (TO to)
                  (VP (VB be)
                    (ADJP (RB too) (JJ high)
                      (PP (IN for)
                        (NP (NNP UcD))))))))))))
    (. .)))

