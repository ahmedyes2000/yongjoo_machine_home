(ROOT
  (S
    (S
      (NP (EX There))
      (VP (VBP are)
        (ADVP (RB so))
        (NP
          (NP
            (ADJP (JJ many) (JJ illogical))
            (NNS flaws))
          (PP (IN in)
            (NP (DT that) (NN theory))))))
    (, ,)
    (NP (PRP it))
    (VP (VBZ 's)
      (ADJP (JJ unbelievable)))
    (. .)))

expl(are-2, There-1)
ccomp('s-12, are-2)
advmod(are-2, so-3)
amod(illogical-5, many-4)
amod(flaws-6, illogical-5)
nsubj(are-2, flaws-6)
det(theory-9, that-8)
prep_in(flaws-6, theory-9)
nsubj('s-12, it-11)
root(ROOT-0, 's-12)
acomp('s-12, unbelievable-13)

