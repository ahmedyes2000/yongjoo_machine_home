(ROOT
  (S
    (NP (DT That))
    (VP (VBZ s)
      (NP
        (NP (DT all))
        (SBAR
          (S
            (NP (PRP I))
            (VP (VBP have)
              (NP (NN time)
                (S
                  (VP (TO to)
                    (VP (VB write)
                      (ADVP (RB right) (RB now)))))))))))
    (. .)))

nsubj(all-3, That-1)
cop(all-3, s-2)
root(ROOT-0, all-3)
nsubj(have-5, I-4)
rcmod(all-3, have-5)
dobj(have-5, time-6)
aux(write-8, to-7)
infmod(time-6, write-8)
advmod(now-10, right-9)
advmod(write-8, now-10)

(ROOT
  (NP (NN tj)))

root(ROOT-0, tj-1)

