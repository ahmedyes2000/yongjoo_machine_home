(ROOT
  (S
    (PP (IN In)
      (NP (NN essence)))
    (, ,)
    (NP (PRP it))
    (VP (VBZ is)
      (NP (DT a) (NN claim))
      (SBAR (IN that)
        (S
          (NP (NN evolution))
          (VP (MD could) (RB not)
            (ADVP (RB possibly))
            (VP (VB work)
              (ADVP (RB properly)))))))
    (. .)))

prep_in(claim-7, essence-2)
nsubj(claim-7, it-4)
cop(claim-7, is-5)
det(claim-7, a-6)
root(ROOT-0, claim-7)
complm(work-13, that-8)
nsubj(work-13, evolution-9)
aux(work-13, could-10)
neg(work-13, not-11)
advmod(work-13, possibly-12)
ccomp(claim-7, work-13)
advmod(work-13, properly-14)

