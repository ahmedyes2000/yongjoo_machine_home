(ROOT
  (S
    (NP (DT This))
    (VP (VBZ has)
      (VP (VBN been)
        (NP
          (NP (DT the) (NN battle))
          (PP (IN of)
            (NP (NNS philosophers))))
        (PP (IN for)
          (NP
            (NP (DT all))
            (PP (IN of)
              (NP (NN time)))))))
    (. .)))

nsubj(battle-5, This-1)
aux(battle-5, has-2)
cop(battle-5, been-3)
det(battle-5, the-4)
root(ROOT-0, battle-5)
prep_of(battle-5, philosophers-7)
prep_for(battle-5, all-9)
prep_of(all-9, time-11)

