(ROOT
  (SBARQ
    (WHADVP (RB '') (WRB How))
    (SQ (VBZ does)
      (NP (NNP Genesis))
      (VP (VB help)
        (S
          (NP (NN oil) (NNS companies))
          (VP (VB find)
            (NP (NN oil))))))
    (. ?)))

advmod(help-5, How-2)
aux(help-5, does-3)
nsubj(help-5, Genesis-4)
root(ROOT-0, help-5)
nn(companies-7, oil-6)
