(ROOT
  (S
    (S
      (NP (PRP$ Their) (NN bias))
      (VP (VBZ reveals)
        (NP (PRP them))))
    (CC and)
    (S
      (ADVP (RB admittedly))
      (NP (DT that))
      (VP (VBZ happens)
        (PP (IN on)
          (NP (DT both) (NNS sides)))))
    (. .)))

poss(bias-2, Their-1)
nsubj(reveals-3, bias-2)
root(ROOT-0, reveals-3)
dobj(reveals-3, them-4)
advmod(happens-8, admittedly-6)
nsubj(happens-8, that-7)
conj_and(reveals-3, happens-8)
det(sides-11, both-10)
prep_on(happens-8, sides-11)

