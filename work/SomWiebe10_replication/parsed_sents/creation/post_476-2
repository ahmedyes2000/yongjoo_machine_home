(ROOT
  (S
    (NP (EX There))
    (VP (VBP are)
      (NP
        (NP (DT a) (NN lot))
        (PP (IN of)
          (NP
            (ADJP (JJ inconsistent))
            (NNS evolutionists))))
      (ADVP (IN around) (RB as) (RB well)))
    (. .)))

expl(are-2, There-1)
root(ROOT-0, are-2)
det(lot-4, a-3)
nsubj(are-2, lot-4)
amod(evolutionists-7, inconsistent-6)
prep_of(lot-4, evolutionists-7)
advmod(are-2, around-8)
advmod(around-8, as-9)
mwe(around-8, as-9)
advmod(around-8, well-10)

