(ROOT
  (S
    (NP (EX There))
    (VP (VBZ is)
      (NP
        (NP (DT no) (NN reason))
        (PP (IN for)
          (NP (PRP them))))
      (ADVP (RB ever))
      (S
        (VP (TO to)
          (VP
            (ADVP (RB even))
            (VB consider)
            (NP (NNP Christianity))
            (SBAR (IN if)
              (S
                (NP (PRP$ their) (NN theory))
                (VP (VBZ is)
                  (ADJP (JJ right)))))))))
    (. .)))

expl(is-2, There-1)
xsubj(consider-10, There-1)
root(ROOT-0, is-2)
det(reason-4, no-3)
nsubj(is-2, reason-4)
prep_for(reason-4, them-6)
advmod(is-2, ever-7)
aux(consider-10, to-8)
advmod(consider-10, even-9)
xcomp(is-2, consider-10)
dobj(consider-10, Christianity-11)
mark(right-16, if-12)
poss(theory-14, their-13)
nsubj(right-16, theory-14)
cop(right-16, is-15)
advcl(consider-10, right-16)

