(ROOT
  (S (`` ``)
    (ADVP (RB Now))
    (NP (DT that))
    (VP (VBZ makes)
      (ADJP (JJ good)
        (SBAR
          (S
            (NP (JJ scientific) (NN sense) (`` ``) (NN chance) ('' ''))
            (VP (VBD was)
              (ADJP (JJ sufficient)))))))
    (. .)))

advmod(makes-4, Now-2)
nsubj(makes-4, that-3)
root(ROOT-0, makes-4)
acomp(makes-4, good-5)
