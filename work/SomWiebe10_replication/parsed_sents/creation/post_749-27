(ROOT
  (S (CC and)
    (ADVP (RB misunderstood) (RB yet))
    (, ,)
    (NP (DT the) (NN fact))
    (VP (VBZ remains)
      (SBAR (IN that)
        (S
          (NP (DT the) (NN rule))
          (ADVP (PRP itself))
          (VP (VBZ is)
            (ADJP (JJ correct))))))
    (. .)))

cc(remains-7, and-1)
advmod(yet-3, misunderstood-2)
advmod(remains-7, yet-3)
det(fact-6, the-5)
nsubj(remains-7, fact-6)
root(ROOT-0, remains-7)
complm(correct-13, that-8)
det(rule-10, the-9)
