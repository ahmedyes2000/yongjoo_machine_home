(ROOT
  (S
    (NP (NNP Science))
    (VP
      (VP (MD will)
        (ADVP (RB only))
        (VP (VB have)
          (NP
            (NP (JJR more) (NNS questions))
            (CC and)
            (NP (JJR more) (NNS discoveries)))))
      (CC but)
      (VP (MD will)
        (ADVP (RB still))
        (VP (VB find)
          (NP
            (ADJP (RBR more) (JJ dead))
            (NNS ends)))))
    (. .)))

nsubj(have-4, Science-1)
nsubj(find-13, Science-1)
aux(have-4, will-2)
advmod(have-4, only-3)
root(ROOT-0, have-4)
amod(questions-6, more-5)
dobj(have-4, questions-6)
amod(discoveries-9, more-8)
dobj(have-4, discoveries-9)
conj_and(questions-6, discoveries-9)
aux(find-13, will-11)
advmod(find-13, still-12)
conj_but(have-4, find-13)
advmod(dead-15, more-14)
amod(ends-16, dead-15)
dobj(find-13, ends-16)

