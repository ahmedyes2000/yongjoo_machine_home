(ROOT
  (S
    (NP (PRP We))
    (VP (VBP have)
      (S
        (VP (TO to)
          (VP (VB be)
            (ADJP (JJ able)
              (S
                (VP (TO to)
                  (VP (VB test)
                    (NP (PRP$ our) (NNS ideas))))))))))
    (. .)))

nsubj(have-2, We-1)
xsubj(able-5, We-1)
root(ROOT-0, have-2)
aux(able-5, to-3)
cop(able-5, be-4)
xcomp(have-2, able-5)
aux(test-7, to-6)
xcomp(able-5, test-7)
poss(ideas-9, our-8)
dobj(test-7, ideas-9)

