(ROOT
  (S
    (NP (NNP God))
    (VP (VBZ is)
      (UCP
        (ADJP (JJ unique))
        (CC and)
        (NP
          (NP (PRP He))
          (SBAR
            (WHNP (WP who))
            (S
              (VP (TO to)
                (VP (VB be)
                  (VP (VBN worshiped)
                    (ADVP (RB only))))))))))
    (. .)))

nsubj(unique-3, God-1)
cop(unique-3, is-2)
root(ROOT-0, unique-3)
conj_and(unique-3, He-5)
nsubjpass(worshiped-9, He-5)
aux(worshiped-9, to-7)
auxpass(worshiped-9, be-8)
infmod(He-5, worshiped-9)
rcmod(He-5, worshiped-9)
advmod(worshiped-9, only-10)

