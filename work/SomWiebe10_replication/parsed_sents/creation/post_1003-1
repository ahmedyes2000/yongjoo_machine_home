(ROOT
  (S
    (NP (PRP I))
    (VP (MD would)
      (VP (VB argue)
        (SBAR (IN that)
          (S
            (NP (JJ common) (NN design))
            (VP (VBZ is)
              (ADJP (RB far) (RBR more) (JJ reasonable)))))))
    (. .)))

nsubj(argue-3, I-1)
aux(argue-3, would-2)
root(ROOT-0, argue-3)
complm(reasonable-10, that-4)
amod(design-6, common-5)
nsubj(reasonable-10, design-6)
cop(reasonable-10, is-7)
advmod(reasonable-10, far-8)
advmod(reasonable-10, more-9)
ccomp(argue-3, reasonable-10)

