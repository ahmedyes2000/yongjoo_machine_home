  (S
    (S
      (PP (IN Like)
        (S
          (NP (PRP I))
          (VP (VBD said))))
      (, ,)
      (NP (PRP I))
      (VP (VBP have)
        (NP (PRP$ my) (JJ own) (NNS theories))))
    (, ,)
    (CC but)
    (S
      (NP (PRP they))
      (VP (VBP 're)
