(ROOT
  (S
    (NP
      (NP (NNP AiG) (POS 's))
      (NN argument))
    (VP (VBZ rests)
      (PP (IN on)
        (S
          (VP (VBG accepting)
            (S
              (S
                (NP (DT the) (NN text))
                (VP (VBZ is)
                  (ADJP (JJ perfect))))
              (CC and)
              (S
                (NP (PRP$ their) (NN understanding))
                (VP (VBZ is)
                  (ADJP (JJ perfect)))))))))
    (. .)))

poss(argument-3, AiG-1)
nsubj(rests-4, argument-3)
root(ROOT-0, rests-4)
prepc_on(rests-4, accepting-6)
det(text-8, the-7)
nsubj(perfect-10, text-8)
cop(perfect-10, is-9)
dep(accepting-6, perfect-10)
poss(understanding-13, their-12)
nsubj(perfect-15, understanding-13)
cop(perfect-15, is-14)
dep(accepting-6, perfect-15)
conj_and(perfect-10, perfect-15)

