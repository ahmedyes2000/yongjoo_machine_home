(ROOT
  (S
    (NP (DT The) (NNP Board))
    (VP (VBD contacted)
      (NP
        (NP (DT no) (NNS scientists))
        (CC or)
        (NP (JJ scientific) (NNS organizations))))
    (. .)))

det(Board-2, The-1)
nsubj(contacted-3, Board-2)
root(ROOT-0, contacted-3)
det(scientists-5, no-4)
dobj(contacted-3, scientists-5)
amod(organizations-8, scientific-7)
dobj(contacted-3, organizations-8)
conj_or(scientists-5, organizations-8)

