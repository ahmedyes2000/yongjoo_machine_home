(ROOT
  (S
    (NP (PRP You))
    (VP (VBP have)
      (S
        (NP (NNP GOT))
        (VP (TO to)
          (VP (VB be)
            (VP (VBG kidding)
              (NP (PRP me)))))))
    (. .)))

nsubj(have-2, You-1)
root(ROOT-0, have-2)
nsubj(kidding-6, GOT-3)
aux(kidding-6, to-4)
aux(kidding-6, be-5)
ccomp(have-2, kidding-6)
xcomp(have-2, kidding-6)
dobj(kidding-6, me-7)

