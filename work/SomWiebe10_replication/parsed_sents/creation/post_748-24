(ROOT
  (S
    (NP (PRP I))
    (ADVP (RB never))
    (VP (VBD learned)
      (NP
        (NP (NN discipline))
        (ADVP (CC and)
          (RB because))
        (SBAR
          (WHPP (IN of)
            (WHNP (WDT that)))
          (PRN
            (S
              (NP (PRP I))
              (VP (VBP 'm)
                (ADJP (RB terribly) (JJ irresponsible))))
            (-RRB- -RRB-))
          (S
            (NP (DT These))
            (VP (VBP are) (RB all)
              (NP (JJ good) (NNS rules)))
            (. .)))))
    (. .)))

nsubj(learned-3, I-1)
neg(learned-3, never-2)
root(ROOT-0, learned-3)
dobj(learned-3, discipline-4)
cc(because-6, and-5)
advmod(discipline-4, because-6)
rel(rules-18, that-8)
nsubj(irresponsible-12, I-9)
