(ROOT
  (S
    (VP (VB Say)
      (SBAR
        (S
          (NP (DT the) (JJ aforementioned) (NNS alleles))
          (VP (VBZ exists)
            (NP
              (NP (DT a) (NN population))
              (PP (IN of)
                (NP (NN yeast) (NNS cells))))))))
    (. .)))

root(ROOT-0, Say-1)
det(alleles-4, the-2)
