(ROOT
  (S
    (NP (PRP I))
    (VP (MD 'll)
      (VP (VB give)
        (NP (PRP you))
        (NP (PRP$ my) (NN email))
        (SBAR (IN if)
          (S
            (VP (VBD needed))))))
    (. .)))

nsubj(give-3, I-1)
aux(give-3, 'll-2)
root(ROOT-0, give-3)
iobj(give-3, you-4)
poss(email-6, my-5)
dobj(give-3, email-6)
mark(needed-8, if-7)
advcl(give-3, needed-8)

