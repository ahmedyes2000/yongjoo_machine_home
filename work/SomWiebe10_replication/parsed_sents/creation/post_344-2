(ROOT
  (S
    (NP (PRP I))
    (VP (VBD found)
      (NP (DT the) (JJ original) (NN source))
      (S
        (VP (TO to)
          (VP (VB be)
            (ADJP (RB far) (RBR more) (JJ useful))))))))

nsubj(found-2, I-1)
root(ROOT-0, found-2)
det(source-5, the-3)
amod(source-5, original-4)
dobj(found-2, source-5)
aux(useful-10, to-6)
cop(useful-10, be-7)
advmod(useful-10, far-8)
advmod(useful-10, more-9)
xcomp(found-2, useful-10)

