(ROOT
  (S
    (NP (PRP It))
    (ADVP (RB totally))
    (VP (VBZ rings)
      (ADJP (JJ true)
        (PP (TO to)
          (NP (PRP$ my) (NN heart)))))
    (. .)))

nsubj(rings-3, It-1)
advmod(rings-3, totally-2)
root(ROOT-0, rings-3)
acomp(rings-3, true-4)
poss(heart-7, my-6)
prep_to(true-4, heart-7)

