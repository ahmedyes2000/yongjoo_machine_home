(ROOT
  (S
    (NP (PRP They))
    (VP
      (VP (VBP operate)
        (PP (IN on)
          (NP (DT the) (JJ same) (NNS principles))))
      (CC and)
      (VP (VBP look)
        (PP (IN for)
          (NP
            (NP (DT the) (JJ same) (NNS kinds))
            (PP (IN of)
              (NP (NN evidence)))))))
    (. .)))

nsubj(operate-2, They-1)
nsubj(look-8, They-1)
root(ROOT-0, operate-2)
det(principles-6, the-4)
amod(principles-6, same-5)
prep_on(operate-2, principles-6)
conj_and(operate-2, look-8)
det(kinds-12, the-10)
amod(kinds-12, same-11)
prep_for(look-8, kinds-12)
prep_of(kinds-12, evidence-14)

