(ROOT
  (S
    (NP (DT That))
    (VP (VBZ 's)
      (SBAR
        (WHNP (WP what))
        (S
          (NP (DT the) (NN guy))
          (VP (VBZ cares)
            (PRT (RP about))))))
    (. .)))

nsubj('s-2, That-1)
root(ROOT-0, 's-2)
dobj(cares-6, what-3)
det(guy-5, the-4)
nsubj(cares-6, guy-5)
ccomp('s-2, cares-6)
prt(cares-6, about-7)

