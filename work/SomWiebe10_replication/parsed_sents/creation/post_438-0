(ROOT
  (S
    (NP (RB Even) (DT the) (JJ anti) (NNS IDists))
    (VP (VBP require)
      (NP (NN something))
      (PP (IN beyond)
        (NP (NN nature)))
      (S
        (VP (TO to)
          (VP (VB explain)
            (NP
              (NP (DT the) (NN origin))
              (PP (IN of)
                (NP (NN nature))))))))
    (. .)))

advmod(IDists-4, Even-1)
det(IDists-4, the-2)
amod(IDists-4, anti-3)
nsubj(require-5, IDists-4)
xsubj(explain-10, IDists-4)
root(ROOT-0, require-5)
dobj(require-5, something-6)
prep_beyond(require-5, nature-8)
aux(explain-10, to-9)
xcomp(require-5, explain-10)
det(origin-12, the-11)
dobj(explain-10, origin-12)
prep_of(origin-12, nature-14)

