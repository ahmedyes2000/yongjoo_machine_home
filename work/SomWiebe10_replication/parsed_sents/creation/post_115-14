(ROOT
  (S
    (VP (VBZ Is)
      (ADVP (RB also))
      (ADJP (JJ unlikely)
        (S
          (VP (TO to)
            (VP (VB have)
              (VP (VBN happened)))))))
    (. .)))

cop(unlikely-3, Is-1)
advmod(unlikely-3, also-2)
root(ROOT-0, unlikely-3)
aux(happened-6, to-4)
aux(happened-6, have-5)
xcomp(unlikely-3, happened-6)

