(ROOT
  (S ('' '')
    (ADVP (RB Indeed))
    (, ,)
    (NP (PRP you))
    (VP (VBP 've)
      (VP (VBN spent)
        (NP
          (NP (DT a) (NN lot))
          (PP (IN of)
            (NP (NN time))))
        (S
          (VP (VBG saying)
            (SBAR
              (S
                (NP (NNP ID))
                (VP (VBZ is) (RB not)
                  (NP (NN science)))))))))
    (. .)))

advmod(spent-6, Indeed-2)
nsubj(spent-6, you-4)
aux(spent-6, 've-5)
root(ROOT-0, spent-6)
det(lot-8, a-7)
dobj(spent-6, lot-8)
prep_of(lot-8, time-10)
xcomp(spent-6, saying-11)
nsubj(science-15, ID-12)
cop(science-15, is-13)
neg(science-15, not-14)
ccomp(saying-11, science-15)

