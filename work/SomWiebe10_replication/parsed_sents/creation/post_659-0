(ROOT
  (S
    (VP
      (VP (VB Stop)
        (S
          (VP (VBG eating)
            (PP (IN at)
              (NP
                (NP (NNP creationist) (POS 's))
                (NNS heads))))))
      (CC and)
      (VP (VB settle)
        (PRT (RP down))
        (NP (DT a) (NN tad))))
    (. .)))

root(ROOT-0, Stop-1)
