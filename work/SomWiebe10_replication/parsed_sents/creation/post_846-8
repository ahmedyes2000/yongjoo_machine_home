(ROOT
  (S ('' '')
    (NP (NNP Please))
    (VP (VBZ clarify)
      (SBAR
        (WHADVP (WRB how))
        (S
          (NP (DT this))
          (VP (VBZ negates)
            (ADVP (RB relativity))))))
    (. .)))

nsubj(clarify-3, Please-2)
root(ROOT-0, clarify-3)
advmod(negates-6, how-4)
nsubj(negates-6, this-5)
ccomp(clarify-3, negates-6)
advmod(negates-6, relativity-7)

