(ROOT
  (S
    (NP (DT The) (NNP ID) (NNS promoters))
    (ADVP (RB delibrately))
    (VP (VBP leave)
      (SBAR
        (S
          (NP (JJ such) (NNS questions))
          (VP (VBD unanswered)
            (PP (IN in)
              (NP (PRP$ their) (JJ official) (NNS pronouncements)))
            (PP (IN about)
              (NP (NNP ID)))))))
    (. .)))

det(promoters-3, The-1)
nn(promoters-3, ID-2)
nsubj(leave-5, promoters-3)
advmod(leave-5, delibrately-4)
root(ROOT-0, leave-5)
amod(questions-7, such-6)
nsubj(unanswered-8, questions-7)
ccomp(leave-5, unanswered-8)
poss(pronouncements-12, their-10)
amod(pronouncements-12, official-11)
prep_in(unanswered-8, pronouncements-12)
prep_about(unanswered-8, ID-14)

(ROOT
  (FRAG (WRB Why) (. ?)))

