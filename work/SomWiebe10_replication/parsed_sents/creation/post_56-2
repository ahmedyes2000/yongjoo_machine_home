(ROOT
  (S
    (INTJ (RB Sure))
    (, ,)
    (ADVP (RB always))
    (VP (VBZ ready)
      (S
        (VP (TO to)
          (VP (VB please)
            (NP (DT the) (NN audience))))))
    (. .)))

dep(ready-4, Sure-1)
advmod(ready-4, always-3)
root(ROOT-0, ready-4)
aux(please-6, to-5)
xcomp(ready-4, please-6)
det(audience-8, the-7)
dobj(please-6, audience-8)

