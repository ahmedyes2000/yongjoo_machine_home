(ROOT
  (S
    (NP (NNP Evolution))
    (ADVP (RB also))
    (VP (VBZ believes)
      (NP (DT the))
      (SBAR (IN that) (, ,)
        (S (`` ``)
          (NP
            (NP (NN out))
            (PP (IN of)
              (NP (NN nothing))))
          ('' '')
          (VP (VBD came)
            (NP (NN everything))))))
    (. .)))

nsubj(believes-3, Evolution-1)
advmod(believes-3, also-2)
root(ROOT-0, believes-3)
dobj(believes-3, the-4)
complm(came-12, that-5)
nsubj(came-12, out-8)
prep_of(out-8, nothing-10)
ccomp(believes-3, came-12)
dobj(came-12, everything-13)

