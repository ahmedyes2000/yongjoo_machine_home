(ROOT
  (S
    (NP (DT This))
    (VP (VBZ occurs)
      (ADVP (RB everywhere))
      (, ,)
      (PP (IN from)
        (NP
          (NP (DT the) (JJ main) (NNS arguments))
          (PP (TO to)
            (NP (DT the) (NNS comments))))))
    (. .)))

nsubj(occurs-2, This-1)
root(ROOT-0, occurs-2)
advmod(occurs-2, everywhere-3)
det(arguments-8, the-6)
amod(arguments-8, main-7)
prep_from(occurs-2, arguments-8)
det(comments-11, the-10)
prep_to(arguments-8, comments-11)

