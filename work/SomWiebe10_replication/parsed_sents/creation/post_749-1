(ROOT
  (S
    (S
      (NP (PRP he))
      (VP (VBD knew)
        (SBAR
          (WHNP (DT all)
            (WHPP (IN of)
              (WHNP (WDT that))))
          (S
            (NP (PRP he))
            (VP (VBD knew)
              (SBAR
                (S
                  (NP (PRP we))
                  (VP (MD would)
                    (VP (VB fall))))))))))
    (: ...)
    (S
      (NP (PRP he))
      (VP (VBD knew)
        (SBAR
          (S
            (NP (DT the) (NNS translations))
            (VP (MD would)
              (VP (VB vary))))))
      (. .))
    (. .)))

nsubj(knew-2, he-1)
root(ROOT-0, knew-2)
dep(of-4, all-3)
dep(knew-7, of-4)
dobj(fall-10, of-4)
