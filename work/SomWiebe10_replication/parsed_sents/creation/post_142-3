(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP (VBP 're)
          (VP (VBG looking)
            (PP (IN for)
              (NP
                (NP (DT a) (NN catalyst))
                (PP (IN for)
                  (NP (DT a) (JJ happier) (NN life)))))))))
    (, ,)
    (NP (PRP you))
    (VP (MD 'd)
      (VP (VB need)
        (S
          (VP (TO to)
            (VP (VB look)
              (ADVP (RB elsewhere)))))))
    (. .)))

