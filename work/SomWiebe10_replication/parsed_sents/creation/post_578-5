(ROOT
  (S
    (VP (VB Sad)
      (ADVP (RB really))
      (PP (CC but)
        (NP
          (NP (NN par))
          (PP (IN for)
            (NP (DT the) (NN course))))))
    (. .)))

root(ROOT-0, Sad-1)
advmod(Sad-1, really-2)
prep(Sad-1, but-3)
dep(but-3, par-4)
det(course-7, the-6)
prep_for(par-4, course-7)

