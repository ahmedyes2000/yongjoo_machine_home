(ROOT
  (S ('' '')
    (S
      (PP (IN If))
      (NP (PRP it))
      (VP (VBZ is)
        (ADJP (JJ consistent))))
    (RB then)
    (S
      (NP (PRP it))
      (VP (MD can) (RB not)
        (VP (VB be)
          (VP (VBN inconstant)
            (PP (IN at)
              (NP (DT the) (JJ same) (NN time)))))))
    (. .)))

prep(consistent-5, If-2)
nsubj(consistent-5, it-3)
cop(consistent-5, is-4)
root(ROOT-0, consistent-5)
advmod(consistent-5, then-6)
nsubjpass(inconstant-11, it-7)
aux(inconstant-11, can-8)
neg(inconstant-11, not-9)
auxpass(inconstant-11, be-10)
ccomp(consistent-5, inconstant-11)
det(time-15, the-13)
