(ROOT
  (S
    (PP (IN On)
      (NP (NNP Day) (CD 5)))
    (NP (NNP God))
    (VP (VBZ creates)
      (NP
        (NP (DT the) (NN sea) (NNS creatures))
        (PRN (-LRB- -LRB-)
          (SBAR
            (WHNP (WDT which))
            (S
              (VP (VBD came)
                (ADVP (RB first)))))
          (-RRB- -RRB-))
        (CC and)
        (NP
          (NP (DT the) (NNS birds))
          (PRN (-LRB- -LRB-)
            (SBAR
              (WHNP (WDT which))
              (S (, ,)
                (PP (IN along)
                  (PP (IN with)
                    (NP (NNS reptiles))))
                (, ,)
                (VP (VBD followed))))
            (-RRB- -RRB-)))))
    (. .)))

prep_on(creates-5, Day-2)
num(Day-2, 5-3)
nsubj(creates-5, God-4)
root(ROOT-0, creates-5)
det(creatures-8, the-6)
nn(creatures-8, sea-7)
dobj(creates-5, creatures-8)
nsubj(came-11, which-10)
dep(creatures-8, came-11)
advmod(came-11, first-12)
det(birds-16, the-15)
dobj(creates-5, birds-16)
conj_and(creatures-8, birds-16)
nsubj(followed-24, which-18)
prepc_along_with(followed-24, with-21)
pobj(followed-24, reptiles-22)
dep(birds-16, followed-24)

