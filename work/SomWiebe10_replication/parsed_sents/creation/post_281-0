(ROOT
  (S (: ...)
    (NP (PRP we))
    (ADVP (RB absolutely))
    (VP (VBP exclude)
      (NP (JJ natural) (NN selection))
      (PP (IN as)
        (NP (DT a) (NN designer))))
    (. !)))

