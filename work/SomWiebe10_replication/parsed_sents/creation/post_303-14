  (S
    (SBAR (IN If)
      (S
        (NP (NN science))
        (VP (MD can)
          (VP (VB explain)
            (NP (NN reality))
            (PP (IN without)
              (S
                (VP (VBG resorting)
                  (PP (TO to)
                    (NP
                      (NP (DT a) (NN miracle))
                      (CC or)
                      (NP (DT the) (NN supernatural)))))))))))
    (, ,)
    (NP (RB then) (NN science))
    (VP (VBZ is)
      (ADJP (JJ satisfactory)))
    (. .)))

