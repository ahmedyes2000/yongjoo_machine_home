(ROOT
  (S ('' '')
    (S
      (SBAR (RB Even) (IN if)
        (S
          (NP (NN e.) (NNS coli))
          (VP (VBD did))))
      (VP (VBP change)
        (PP (IN into)
          (NP (DT a) (JJ different) (NN species)))))
    (, ,)
    (NP (DT the) (NN study))
    (VP (MD would) (RB not)
      (VP (VB justify)
        (S
          (VP (VBG inferring)
            (SBAR (IN that)
              (S
                (NP (DT all) (NN life))
                (VP (VBZ changes)
                  (PP (IN into)
                    (NP
                      (NP (JJ new) (NNS species))
                      (PP (IN via)
                        (NP (JJ non-intelligent) (NNS processes))))))))))))
    (. .)))

advmod(did-6, Even-2)
dep(did-6, if-3)
nn(coli-5, e.-4)
nsubj(did-6, coli-5)
advcl(change-7, did-6)
csubj(change-7, did-6)
ccomp(justify-17, change-7)
det(species-11, a-9)
amod(species-11, different-10)
prep_into(change-7, species-11)
det(study-14, the-13)
nsubj(justify-17, study-14)
aux(justify-17, would-15)
neg(justify-17, not-16)
root(ROOT-0, justify-17)
xcomp(justify-17, inferring-18)
complm(changes-22, that-19)
det(life-21, all-20)
nsubj(changes-22, life-21)
ccomp(inferring-18, changes-22)
amod(species-25, new-24)
prep_into(changes-22, species-25)
amod(processes-28, non-intelligent-27)
prep_via(species-25, processes-28)

