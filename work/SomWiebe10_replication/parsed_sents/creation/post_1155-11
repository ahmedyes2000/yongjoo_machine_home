(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ has)
      (NP
        (NP (JJ useful) (NNS applications))
        (PP (IN in)
          (NP
            (NP (NN epidemiology))
            (, ,)
            (NP (JJ pest) (NN control))
            (, ,)
            (NP (NN drug) (NN discovery))
            (, ,)
            (CC and)
            (NP (JJ other) (NNS areas))))))
    (. .)))

nsubj(has-2, It-1)
root(ROOT-0, has-2)
amod(applications-4, useful-3)
dobj(has-2, applications-4)
prep_in(applications-4, epidemiology-6)
amod(control-9, pest-8)
prep_in(applications-4, control-9)
conj_and(epidemiology-6, control-9)
nn(discovery-12, drug-11)
prep_in(applications-4, discovery-12)
conj_and(epidemiology-6, discovery-12)
amod(areas-16, other-15)
prep_in(applications-4, areas-16)
conj_and(epidemiology-6, areas-16)

