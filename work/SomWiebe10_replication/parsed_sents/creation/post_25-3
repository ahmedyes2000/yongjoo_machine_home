(ROOT
  (S
    (ADVP (RB Therefore))
    (, ,)
    (NP (DT neither))
    (VP (MD should)
      (VP (VB creationism)
        (VP (VB be)
          (VP (VBN taught)
            (PP (IN as)
              (NP
                (NP (DT a) (NN theory))
                (VP (VBG explaining)
                  (NP
                    (NP (DT the) (NN origin))
                    (PP (IN of)
                      (NP (DT the) (NN earth)))))))))))
    (. .)))

advmod(creationism-5, Therefore-1)
nsubj(creationism-5, neither-3)
aux(creationism-5, should-4)
root(ROOT-0, creationism-5)
auxpass(taught-7, be-6)
dep(creationism-5, taught-7)
det(theory-10, a-9)
prep_as(taught-7, theory-10)
partmod(theory-10, explaining-11)
det(origin-13, the-12)
dobj(explaining-11, origin-13)
det(earth-16, the-15)
prep_of(origin-13, earth-16)

