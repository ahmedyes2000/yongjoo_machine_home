(ROOT
  (SBARQ
    (WHADVP (WRB How))
    (SQ (MD would)
      (NP (PRP we))
      (VP (VB know) (: ...)
        (NP (RB so) (JJ few) (NN try))))
    (. .)))

advmod(know-4, How-1)
aux(know-4, would-2)
nsubj(know-4, we-3)
root(ROOT-0, know-4)
advmod(try-8, so-6)
amod(try-8, few-7)
dobj(know-4, try-8)

