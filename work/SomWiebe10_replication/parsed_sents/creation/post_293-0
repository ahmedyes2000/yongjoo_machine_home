(ROOT
  (S
    (NP (NNP ICDESIGN))
    (VP (VBZ is)
      (ADVP (RB suddenly))
      (RB not)
      (ADJP (JJ interested)
        (PP (IN in)
          (S
            (VP (VBG hearing)
              (PP (IN about)
                (NP (NN science)))
              (ADVP (RB anymore)))))))
    (. .)))

nsubj(interested-5, ICDESIGN-1)
cop(interested-5, is-2)
advmod(interested-5, suddenly-3)
neg(interested-5, not-4)
root(ROOT-0, interested-5)
prepc_in(interested-5, hearing-7)
prep_about(hearing-7, science-9)
advmod(hearing-7, anymore-10)

