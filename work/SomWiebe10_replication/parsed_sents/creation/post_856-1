(ROOT
  (S ('' '')
    (NP (NNP Please))
    (VP (VBP learn)
      (PP (IN about)
        (NP
          (NP (DT the) (NNS things))
          (SBAR
            (S
              (NP (PRP you))
              (VP (VBP attempt)
                (S
                  (VP (TO to)
                    (VP (VB talk)
                      (PRT (RP about)))))))))))
    (. .)))

nsubj(learn-3, Please-2)
root(ROOT-0, learn-3)
det(things-6, the-5)
prep_about(learn-3, things-6)
dobj(attempt-8, things-6)
nsubj(attempt-8, you-7)
xsubj(talk-10, you-7)
rcmod(things-6, attempt-8)
aux(talk-10, to-9)
xcomp(attempt-8, talk-10)
prt(talk-10, about-11)

