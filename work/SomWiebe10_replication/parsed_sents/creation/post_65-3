(ROOT
  (S
    (VP (VB Thank)
      (NP (PRP you))
      (PP (IN for)
        (S
          (VP (VBG demonstrating)
            (NP (PRP$ your) (NN dishonesty))))))
    (. .)))

root(ROOT-0, Thank-1)
dobj(Thank-1, you-2)
prepc_for(Thank-1, demonstrating-4)
poss(dishonesty-6, your-5)
dobj(demonstrating-4, dishonesty-6)

