(ROOT
  (S
    (NP (EX There))
    (VP (VBP are)
      (NP
        (NP (JJ atheistic) (NNS philosophies))
        (SBAR
          (WHNP (WDT that))
          (S
            (VP (MD can)
              (VP (VB fill)
                (NP (PDT such) (DT a) (NN gap))))))))
    (. .)))

expl(are-2, There-1)
root(ROOT-0, are-2)
amod(philosophies-4, atheistic-3)
nsubj(are-2, philosophies-4)
nsubj(fill-7, philosophies-4)
aux(fill-7, can-6)
rcmod(philosophies-4, fill-7)
predet(gap-10, such-8)
det(gap-10, a-9)
dobj(fill-7, gap-10)

