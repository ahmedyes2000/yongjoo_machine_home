(ROOT
  (S
    (VP (VB Allow)
      (NP (PRP me))
      (SBAR
        (S
          (NP (DT an) (NN opportunity))
          (, ,)
          (ADVP (RB here)
            (CC and)
            (RB now))
          (, ,)
          (VP (TO to)
            (VP (VB describe)
              (PP (IN in)
                (NP (NN part)))
              (NP
                (NP (PRP$ my) (NN approach))
                (PP (IN in)
                  (S
                    (VP (VBG answering)
                      (NP
                        (NP (DT the) (NN question))
                        (PP (IN of)
                          (NP (JJ intelligent) (NN cause)))))))))))))
    (. .)))

root(ROOT-0, Allow-1)
dobj(Allow-1, me-2)
det(opportunity-4, an-3)
nsubj(describe-11, opportunity-4)
