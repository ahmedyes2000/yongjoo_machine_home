      (S
        (NP (PRP they))
        (VP (VBP play)
          (SBAR
            (S
              (NP (PRP$ their))
              (VP (VBD had)
                (ADVP (RB properly))))))))
    (, ,)
    (NP (PRP they))
    (VP (MD could)
      (VP
        (VP (VB show)
          (NP (PRP$ their) (NN stuff))
          (PP (TO to)
            (NP (DT the) (NN public))))
        (CC and)
        (VP (VB settle)
          (NP (DT the) (NN issue))
          (UCP
            (ADVP (RB once))
            (CC and)
            (PP (IN for)
              (NP (DT all)))))))
    (. .)))

