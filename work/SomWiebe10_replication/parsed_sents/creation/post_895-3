(ROOT
  (S
    (NP (PRP I))
    (VP (VBP have)
      (NP
        (NP (DT no) (NN prediction))
        (PP (IN on)
          (NP
            (NP (DT the) (NN age))
            (PP (IN of)
              (NP
                (NP (DT the) (NN earth))
                (, ,)
                (NP
                  (NP (DT a) (NN mystery))
                  (SBAR
                    (WHNP (WDT that))
                    (S
                      (VP (MD will)
                        (ADVP (RB always))
                        (VP (VB be)
                          (VP (VBN unsolved)))))))))))))
    (. .)))

nsubj(have-2, I-1)
root(ROOT-0, have-2)
det(prediction-4, no-3)
dobj(have-2, prediction-4)
det(age-7, the-6)
prep_on(prediction-4, age-7)
det(earth-10, the-9)
prep_of(age-7, earth-10)
det(mystery-13, a-12)
