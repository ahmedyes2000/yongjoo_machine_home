  (S
    (ADVP (RB Thus))
    (, ,)
    (SBAR (IN if)
      (S
        (NP (PRP I))
        (VP (VBP hypothesize)
          (SBAR (IN that)
            (S
              (NP (NNS species))
              (VP (VBP have)
                (VP (VBN changed)
                  (PP (IN over)
                    (NP (NN time))))))))))
    (, ,)
    (ADVP (RB then))
    (NP (PRP I))
    (VP (VBP predict)
      (SBAR (IN that)
        (S
          (NP (JJ million-year-old) (NNS fossils))
          (VP (MD will)
            (VP (VB indicate)
              (NP
