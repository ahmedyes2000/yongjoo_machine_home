(ROOT
  (S
    (NP (NNS Irons))
    (ADVP (RB repeatedly))
    (VP (VBZ misrepresents)
      (CC and)
      (VBZ misstates)
      (NP (NNS facts)))
    (. .)))

nsubj(misrepresents-3, Irons-1)
nsubj(misstates-5, Irons-1)
advmod(misrepresents-3, repeatedly-2)
root(ROOT-0, misrepresents-3)
conj_and(misrepresents-3, misstates-5)
dobj(misrepresents-3, facts-6)

