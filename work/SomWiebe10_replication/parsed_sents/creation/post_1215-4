(ROOT
  (S
    (ADVP (RB Now))
    (VP (VB think)
      (PP (IN about)
        (SBAR
          (WHADVP (WRB how))
          (S
            (NP (NNS theories))
            (VP (VBP are)
              (VP (VBN created)))))))
    (. .)))

advmod(think-2, Now-1)
root(ROOT-0, think-2)
advmod(created-7, how-4)
nsubjpass(created-7, theories-5)
auxpass(created-7, are-6)
prepc_about(think-2, created-7)

