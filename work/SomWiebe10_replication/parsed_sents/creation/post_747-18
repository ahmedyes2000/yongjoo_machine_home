(ROOT
  (S ('' '')
    (S
      (S
        (VP (VBG Beating)
          (NP (PRP$ your) (NNS children))))
      (VP (VBZ is)))
    (RB somewhat)
    (S
      (NP (NN frowned))
      (VP (VBD upon)
        (NP (DT these) (NNS days))))
    (. .)))

csubj(is-5, Beating-2)
poss(children-4, your-3)
dobj(Beating-2, children-4)
root(ROOT-0, is-5)
advmod(is-5, somewhat-6)
nsubj(upon-8, frowned-7)
ccomp(is-5, upon-8)
det(days-10, these-9)
tmod(upon-8, days-10)

