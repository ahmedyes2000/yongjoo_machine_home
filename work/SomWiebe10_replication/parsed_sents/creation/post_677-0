  (S (IN So)
    (NP (NNP Luskin))
    (VP (VBZ is)
      (VP (VBG accusing)
        (NP
          (NP (NNP NCSE))
          (PP (IN of)
            (S
              (VP (VBG being)
                (NP (DT both) (JJ anti) (NN religion)
                  (CC and)
                  (NN pro-religion))))))))
    (. .)))

dep(accusing-4, So-1)
