(ROOT
  (S
    (NP (NNP ETERNITY))
    (VP (VBZ is)
      (NP
        (NP (CD one))
        (CC &)
        (NP
          (NP (DT the) (JJ same) (NN DAY))
          (PP (IN in)
            (NP
              (NP (NNS terms))
              (PP (IN of)
                (NP (NNP GODs) (NN existance))))))))
    (: ...)))

