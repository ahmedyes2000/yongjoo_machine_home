(ROOT
  (S
    (S
      (NP (PRP He))
      (VP (MD could) (RB n't)
        (VP (VB explain)
          (NP (NNS things))
          (PP (TO to)
            (S
              (NP (PRP them))
              (VP (VBG using)
                (NP (JJ complex) (JJ scientific) (NNS terms))))))))
    (, ,) (IN so)
    (S
      (NP (PRP he))
      (VP (VBZ simplified)
        (NP (PRP it))))
    (. .)))

nsubj(explain-4, He-1)
aux(explain-4, could-2)
neg(explain-4, n't-3)
root(ROOT-0, explain-4)
dobj(explain-4, things-5)
nsubj(using-8, them-7)
prepc_to(explain-4, using-8)
amod(terms-11, complex-9)
amod(terms-11, scientific-10)
dobj(using-8, terms-11)
dep(explain-4, so-13)
nsubj(simplified-15, he-14)
ccomp(explain-4, simplified-15)
dobj(simplified-15, it-16)

