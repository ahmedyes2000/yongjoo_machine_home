(ROOT
  (S
    (S
      (INTJ (DT That) (VBZ is))
      (VP (VB design)))
    (, ,)
    (ADVP (RB however))
    (, ,)
    (NP (PRP it))
    (VP (VBZ is)
      (NP
        (NP (NN life))
        (VP (VBG designing)
          (NP (NN life)))))
    (. .)))

dep(design-3, That-1)
dep(That-1, is-2)
ccomp(life-9, design-3)
advmod(life-9, however-5)
nsubj(life-9, it-7)
cop(life-9, is-8)
root(ROOT-0, life-9)
