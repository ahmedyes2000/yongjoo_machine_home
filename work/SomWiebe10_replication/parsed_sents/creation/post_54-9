(ROOT
  (S
    (NP (NNP Highlander))
    (VP (VBZ asks)
      (SBAR (IN for)
        (S
          (NP (DT another))
          (VP (VBP cite) (: --)
            (NP (NNP Okay) (NNP PvM))))))
    (. .)))

nsubj(asks-2, Highlander-1)
root(ROOT-0, asks-2)
mark(cite-5, for-3)
nsubj(cite-5, another-4)
advcl(asks-2, cite-5)
nn(PvM-8, Okay-7)
