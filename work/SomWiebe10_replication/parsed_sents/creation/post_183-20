(ROOT
  (S
    (NP (JJ Unobserved) (NNS entities))
    (VP (VBP are)
      (VP (VBN allowed)
        (PP (IN in)
          (NP
            (NP (NN science))
            (, ,)
            (NP (JJ such)
              (ADJP (RB as) (JJ dark))
              (NN matter) (NN \/) (NN energy))))))
    (. .)))

amod(entities-2, Unobserved-1)
nsubjpass(allowed-4, entities-2)
auxpass(allowed-4, are-3)
root(ROOT-0, allowed-4)
