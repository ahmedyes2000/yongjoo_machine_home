(ROOT
  (S
    (S
      (ADVP (RB Now))
      (VP (VB let)
        (S
          (NP (POS 's))
          (VP
            (VP (VB see))
            (, ,)
            (NP
              (QP (CD 100) (CD billion))
              (NNS galaxies))))))
    (: ...)
    (S (CC but)
      (ADVP (RB nah))
      (, ,)
      (NP (NN mankind))
      (VP (VBZ is)
        (ADJP
          (NP (DT the) (RB only))
          (JJ intelligent))
        (S
          (VP (VBG being)
            (PP (IN in)
              (NP (NN existence)))))))
    (. .)))

advmod(let-2, Now-1)
root(ROOT-0, let-2)
nsubj(see-4, 's-3)
ccomp(let-2, see-4)
number(billion-7, 100-6)
num(galaxies-8, billion-7)
dobj(see-4, galaxies-8)
cc(intelligent-17, but-10)
advmod(intelligent-17, nah-11)
nsubj(intelligent-17, mankind-13)
cop(intelligent-17, is-14)
det(only-16, the-15)
npadvmod(intelligent-17, only-16)
parataxis(let-2, intelligent-17)
xcomp(intelligent-17, being-18)
prep_in(being-18, existence-20)

