(ROOT
  (S
    (NP (NNP IOW))
    (VP (VBP thank)
      (NP (PRP you))
      (PP (IN for)
        (S
          (VP (VBG demonstrating)
            (SBAR (IN that)
              (S
                (NP
                  (NP (NNS critics))
                  (PP (IN of)
                    (NP (NNP ID))))
                (VP (VBP base)
                  (NP (PRP$ their) (NN criticism))
                  (PP (IN on)
                    (NP (NN ignorance))))))))))
    (. .)))

nsubj(thank-2, IOW-1)
root(ROOT-0, thank-2)
dobj(thank-2, you-3)
prepc_for(thank-2, demonstrating-5)
complm(base-10, that-6)
nsubj(base-10, critics-7)
prep_of(critics-7, ID-9)
ccomp(demonstrating-5, base-10)
poss(criticism-12, their-11)
dobj(base-10, criticism-12)
prep_on(base-10, ignorance-14)

