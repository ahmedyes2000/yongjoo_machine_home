(ROOT
  (S
    (NP (PRP I))
    (VP (MD 'll)
      (VP (VB give)
        (NP (PRP you))
        (PP (IN as)
          (NP (JJ much) (NN time)))
        (SBAR (IN as)
          (S
            (NP (PRP you))
            (VP (VBP need)
              (S
                (VP (TO to)
                  (VP (VB fulfill)
                    (NP (PRP$ my) (NN challenge))))))))))
    (. .)))

nsubj(give-3, I-1)
aux(give-3, 'll-2)
root(ROOT-0, give-3)
dobj(give-3, you-4)
amod(time-7, much-6)
prep_as(give-3, time-7)
mark(need-10, as-8)
nsubj(need-10, you-9)
xsubj(fulfill-12, you-9)
advcl(give-3, need-10)
aux(fulfill-12, to-11)
xcomp(need-10, fulfill-12)
poss(challenge-14, my-13)
dobj(fulfill-12, challenge-14)

