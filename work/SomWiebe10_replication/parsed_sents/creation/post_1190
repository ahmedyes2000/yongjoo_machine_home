(ROOT
  (S
    (INTJ (UH No))
    (, ,)
    (NP (PRP it))
    (VP (VBZ is) (RB not)
      (VP (VBG falling)
        (ADVP (RB apart))))
    (. .)))

dep(falling-6, No-1)
nsubj(falling-6, it-3)
aux(falling-6, is-4)
neg(falling-6, not-5)
root(ROOT-0, falling-6)
advmod(falling-6, apart-7)

(ROOT
  (S
    (NP (PRP$ Your) (NN statement))
    (VP (VBZ is)
      (NP
        (ADJP (RB just) (JJ worthless))
        (JJ penny-stock) (NN church) (NN rhetoric)))
    (. .)))

poss(statement-2, Your-1)
nsubj(rhetoric-8, statement-2)
cop(rhetoric-8, is-3)
advmod(worthless-5, just-4)
amod(rhetoric-8, worthless-5)
amod(rhetoric-8, penny-stock-6)
nn(rhetoric-8, church-7)
root(ROOT-0, rhetoric-8)

