(ROOT
  (S
    (NP (PRP I))
    (VP (VBP do) (RB n't)
      (VP (VB believe)
        (PP (IN in)
          (NP (NN evolution) (, ,)
            (CC or)
            (NN anything) (NN contemporary)))
        (PP (IN beyond)
          (NP
            (NP (DT the) (NN dictionary) (NN definition))
            (PP (IN of)
              (NP (PRP it)))))))
    (. .)))

nsubj(believe-4, I-1)
aux(believe-4, do-2)
neg(believe-4, n't-3)
root(ROOT-0, believe-4)
prep_in(believe-4, evolution-6)
nn(contemporary-10, anything-9)
prep_in(believe-4, contemporary-10)
conj_or(evolution-6, contemporary-10)
det(definition-14, the-12)
nn(definition-14, dictionary-13)
prep_beyond(believe-4, definition-14)
prep_of(definition-14, it-16)

