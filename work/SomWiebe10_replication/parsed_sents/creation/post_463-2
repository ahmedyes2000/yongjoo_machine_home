(ROOT
  (S
    (NP (PRP He))
    (VP (VBD lost)
      (NP
        (NP (PRP$ his) (NN job))
        (CC and)
        (NP (PRP$ his) (NN reputation))))
    (. .)))

nsubj(lost-2, He-1)
root(ROOT-0, lost-2)
poss(job-4, his-3)
dobj(lost-2, job-4)
poss(reputation-7, his-6)
dobj(lost-2, reputation-7)
conj_and(job-4, reputation-7)

