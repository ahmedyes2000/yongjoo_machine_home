(ROOT
  (SBARQ
    (WHADVP (WRB How)
      (CC and)
      (WRB why))
    (SQ (VBP are)
      (NP
        (NP (DT the) (JJ great) (NNS questions))
        (PP (IN of)
          (NP (PRP$ our) (NN time)))))
    (. .)))

advmod(are-4, How-1)
dep(How-1, and-2)
dep(How-1, why-3)
root(ROOT-0, are-4)
det(questions-7, the-5)
amod(questions-7, great-6)
nsubj(are-4, questions-7)
poss(time-10, our-9)
prep_of(questions-7, time-10)

