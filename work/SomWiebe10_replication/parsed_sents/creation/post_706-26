(ROOT
  (S
    (NP
      (NP (NNS Parables))
      (CC and)
      (NP (DT the) (JJ like)))
    (VP (MD can)
      (VP (VB be)
        (VP (VBN considered)
          (ADVP (RB literally))
          (NP
            (UCP
              (ADJP (`` ``) (JJ true) ('' ''))
              (CC but)
              (ADVP (RB yet)))
            (NNS metaphors)))))
    (. .)))

nsubjpass(considered-7, Parables-1)
det(like-4, the-3)
conj_and(Parables-1, like-4)
nsubjpass(considered-7, like-4)
aux(considered-7, can-5)
auxpass(considered-7, be-6)
root(ROOT-0, considered-7)
advmod(considered-7, literally-8)
amod(metaphors-14, true-10)
advmod(true-10, yet-13)
conj_but(true-10, yet-13)
amod(metaphors-14, yet-13)
dobj(considered-7, metaphors-14)

