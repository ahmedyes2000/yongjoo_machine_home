(ROOT
  (S
    (NP (DT The) (NNS evolutionists))
    (VP (VBP state)
      (SBAR (IN that)
        (S
          (NP (EX there))
          (VP (VBZ is)
            (NP
              (NP (JJ unequivocal) (NN evidence))
              (PP (IN of)
                (NP
                  (NP (NN life))
                  (PP (IN before)
                    (NP (NN man))))))))))
    (. .)))

det(evolutionists-2, The-1)
nsubj(state-3, evolutionists-2)
root(ROOT-0, state-3)
complm(is-6, that-4)
expl(is-6, there-5)
ccomp(state-3, is-6)
amod(evidence-8, unequivocal-7)
nsubj(is-6, evidence-8)
prep_of(evidence-8, life-10)
prep_before(life-10, man-12)

(ROOT
  (FRAG
    (NP (PRP$ Their) (NN right))
    (. !)))

poss(right-2, Their-1)
root(ROOT-0, right-2)

