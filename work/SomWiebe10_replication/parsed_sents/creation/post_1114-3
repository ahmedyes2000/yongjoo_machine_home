(ROOT
  (S
    (VP
      (VP (VB \/) (-RRB- -RRB-)
        (NP (NNS \/))
        (-RRB- -RRB-))
      (CC Let)
      (VP
        (ADVP (RB there))
        (VP (VB be)
          (ADJP (JJ light)
            (PP (IN over)
              (NP
                (NP (DT the) (NNS darkness))
                (PP (IN of)
                  (NP (NN ignorance)))))))))
    (. .)))

root(ROOT-0, \/-1)
dobj(\/-1, \/-3)
