(ROOT
  (NP ('' '')
    (CC Can)
    (NP (DT the) (NNS designers)
      (S
        (NP (JJ next) (NNS actions))
        (VP (VB be)
          (VP (VBN determined)))))
    (. ?)))

cc(designers-4, Can-2)
det(designers-4, the-3)
root(ROOT-0, designers-4)
amod(actions-6, next-5)
nsubjpass(determined-8, actions-6)
auxpass(determined-8, be-7)
dep(designers-4, determined-8)

