(ROOT
  (SINV (RB So)
    (ADVP (RB then))
    (, ,)
    (VP (VBZ is)
      (NP (PRP he)))
    (NP (DT the) (JJ intelligent) (NN designer))
    (. ?)))

advmod(he-5, So-1)
