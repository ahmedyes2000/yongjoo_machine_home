(ROOT
  (S ('' '')
    (NP (PRP It))
    (VP (VBZ does)
      (VP (VB require)
        (NP (NN faith))
        (S
          (VP (TO to)
            (VP (VB believe)
              (PP (IN in)
                (NP (NNP God)))
              (SBAR (IN because)
                (S
                  (NP (PRP He))
                  (VP (VBZ is)
                    (ADJP
                      (ADJP (JJR greater))
                      (PP (IN than)
                        (NP
                          (NP (NN anything))
                          (VP (VBN conceived)
                            (PP (IN on)
                              (NP (DT the) (NN earth)))))))))))))))
    (. .)))

nsubj(require-4, It-2)
aux(require-4, does-3)
root(ROOT-0, require-4)
dobj(require-4, faith-5)
aux(believe-7, to-6)
xcomp(require-4, believe-7)
prep_in(believe-7, God-9)
mark(greater-13, because-10)
nsubj(greater-13, He-11)
cop(greater-13, is-12)
advcl(believe-7, greater-13)
prep_than(greater-13, anything-15)
partmod(anything-15, conceived-16)
det(earth-19, the-18)
prep_on(conceived-16, earth-19)

