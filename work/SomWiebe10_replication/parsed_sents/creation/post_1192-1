(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is)
      (ADVP (RB nowhere))
      (PP (IN near)
        (NP
          (NP (DT a) (NN theory))
          (PP (IN in)
            (NP (NN crisis))))))
    (. .)))

nsubj(is-2, It-1)
root(ROOT-0, is-2)
advmod(is-2, nowhere-3)
det(theory-6, a-5)
prep_near(is-2, theory-6)
prep_in(theory-6, crisis-8)

