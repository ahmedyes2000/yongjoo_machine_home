(ROOT
  (S
    (FRAG
      (ADJP (JJ '')))
    ('' '')
    (NP (DT The) (NN court))
    (VP (VBD provided)
      (NP
        (NP (DT a) (NN list))
        (PP (IN of)
          (NP (JJ such) (NNS signs))))
      (, ,)
      (PP (VBG including)
        (NP
          (NP (NN liturgy))
          (, ,)
          (NP (NNS clergy))
          (, ,)
          (CC and)
          (NP
            (NP (NN observance))
            (PP (IN of)
              (NP (NNS holidays)))))))
    (. .)))

det(court-4, The-3)
nsubj(provided-5, court-4)
root(ROOT-0, provided-5)
det(list-7, a-6)
dobj(provided-5, list-7)
amod(signs-10, such-9)
prep_of(list-7, signs-10)
prep_including(provided-5, liturgy-13)
prep_including(provided-5, clergy-15)
conj_and(liturgy-13, clergy-15)
prep_including(provided-5, observance-18)
conj_and(liturgy-13, observance-18)
prep_of(observance-18, holidays-20)

