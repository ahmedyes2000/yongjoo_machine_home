  (SBARQ
    (ADVP (RB Now))
    (WHNP
      (WHADJP (WRB how) (JJ many))
      (NP
        (NP (NNP ID) (NNS proponents))
        (CC or)
        (NP (NNS enthusiasts))))
    (SQ
      (VP (VBP are)
        (ADJP (JJ familiar)
          (PP (IN with)
            (NP (DT this) (JJ hidden) (NN gem))))))
    (. ?)))

advmod(familiar-9, Now-1)
