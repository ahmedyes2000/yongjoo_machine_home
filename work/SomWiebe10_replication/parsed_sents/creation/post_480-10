(ROOT
  (S ('' '')
    (S
      (SBAR (IN If)
        (S
          (NP (DT this))
          (VP (VBZ is)
            (WHNP (WP what)))))
      (NP (PRP you))
      (ADVP (RB really))
      (VP (VBP want)
        (S
          (NP (PRP us))
          (VP (TO to)
            (VP (VB believe))))))
    (, ,) (RB then)
    (S
      (NP (PRP I))
      (VP (VBP want)
        (S
          (VP (TO to)
            (VP (VB hear)
              (NP (PRP it))
              (PP (RB straight) (IN from)
                (NP (PRP$ your) (NN mouth))))))))
    (. .)))

mark(what-5, If-2)
nsubj(what-5, this-3)
cop(what-5, is-4)
advcl(want-8, what-5)
nsubj(want-8, you-6)
advmod(want-8, really-7)
root(ROOT-0, want-8)
nsubj(believe-11, us-9)
aux(believe-11, to-10)
xcomp(want-8, believe-11)
advmod(want-8, then-13)
nsubj(want-15, I-14)
xsubj(hear-17, I-14)
ccomp(want-8, want-15)
aux(hear-17, to-16)
xcomp(want-15, hear-17)
dobj(hear-17, it-18)
advmod(hear-17, straight-19)
poss(mouth-22, your-21)
prep_from(hear-17, mouth-22)

