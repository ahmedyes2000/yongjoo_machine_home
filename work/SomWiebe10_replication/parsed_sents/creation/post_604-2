(ROOT
  (S
    (ADVP (RB No) (RB because))
    (NP (DT these))
    (VP (VBP are)
      (NP (VBN stated) (NNS requirements)))
    (. .)))

advmod(because-2, No-1)
advmod(requirements-6, because-2)
nsubj(requirements-6, these-3)
dep(requirements-6, are-4)
amod(requirements-6, stated-5)
root(ROOT-0, requirements-6)

