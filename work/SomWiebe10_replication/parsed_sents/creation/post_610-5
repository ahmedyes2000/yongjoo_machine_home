(ROOT
  (S
    (NP (DT The) (NN rejection))
    (VP (VBZ is)
      (VP (VBN based)
        (PP (IN on)
          (NP
            (NP (JJ philosophical) (NNS assumptions))
            (, ,) (RB not)
            (NP (NN science))))))
    (. .)))

det(rejection-2, The-1)
nsubjpass(based-4, rejection-2)
auxpass(based-4, is-3)
root(ROOT-0, based-4)
amod(assumptions-7, philosophical-6)
prep_on(based-4, assumptions-7)
neg(assumptions-7, not-9)
dep(assumptions-7, science-10)

