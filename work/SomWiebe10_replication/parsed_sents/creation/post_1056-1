(ROOT
  (S
    (NP (PRP You))
    (VP (VBP 're)
      (NP (DT the)
        (VP
          (NP
            (QP (RB only) (CD one)))
          (S
            (VP (VBG using)
              (NP (NN faith))
              (ADVP (RB here)))))
        (NN buddy)))
    (. .)))

nsubj(buddy-9, You-1)
cop(buddy-9, 're-2)
det(buddy-9, the-3)
quantmod(one-5, only-4)
dep(buddy-9, one-5)
xcomp(one-5, using-6)
dobj(using-6, faith-7)
advmod(using-6, here-8)
root(ROOT-0, buddy-9)

