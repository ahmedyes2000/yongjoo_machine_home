(ROOT
  (S
    (NP (NNP Ignorance))
    (VP (VBZ is)
      (NP
        (NP (DT a) (JJ powerful) (NN foundation))
        (PP (IN for)
          (NP
            (NP (NNP ID))
            (VP (VBG causing)
              (S
                (NP (PRP it))
                (VP (TO to)
                  (VP (VB remain)
                    (ADJP (RB scientifically) (JJ irrelevant))))))))))
    (. .)))

nsubj(foundation-5, Ignorance-1)
cop(foundation-5, is-2)
det(foundation-5, a-3)
amod(foundation-5, powerful-4)
root(ROOT-0, foundation-5)
prep_for(foundation-5, ID-7)
partmod(ID-7, causing-8)
nsubj(irrelevant-13, it-9)
