(ROOT
  (SBARQ
    (WHADVP (WRB How))
    (SQ (VBZ does)
      (NP (NNP Genesis))
      (VP (VB help)
        (S
          (NP (NN oil) (NNS companies))
          (VP (VB find)
            (NP (NN oil))))))
    (. ?)))

