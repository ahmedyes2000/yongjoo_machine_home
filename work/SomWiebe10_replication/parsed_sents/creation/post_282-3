(ROOT
  (S
    (S
      (VP (VB bet)
        (SBAR
          (S
            (NP (PRP you))
            (VP (MD ca) (RB n't)
              (VP (VB do)
                (NP (PRP it))))))))
    (: --)
    (S
      (NP (PRP You))
      (VP (MD should) (RB not)
        (VP (VB be)
          (NP (DT a) (VBG betting) (NN man))
          (SBAR
            (WHADVP (WRB when))
            (S
              (NP (PRP you))
              (VP (VBP are)
                (VP (VBG bluffing)
                  (ADVP
                    (ADVP (RB so) (RB obviously))
                    (PRN (-LRB- -LRB-)
                      (NP
                        (NP (NN hint))
                        (: :)
                        (NP (DT all) (NNS caps)))
                      (-RRB- -RRB-))))))))))
    (. .)))

root(ROOT-0, bet-1)
nsubj(do-5, you-2)
aux(do-5, ca-3)
neg(do-5, n't-4)
ccomp(bet-1, do-5)
dobj(do-5, it-6)
