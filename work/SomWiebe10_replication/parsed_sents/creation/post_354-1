(ROOT
  (NP ('' '')
    (CC And)
    (NP
      (NP (RB absolutely) (DT no) (JJ scientific))
      (SBAR
        (S
          (VP (VBP plausibility)))))
    (. .)))

cc(scientific-5, And-2)
advmod(scientific-5, absolutely-3)
det(scientific-5, no-4)
root(ROOT-0, scientific-5)
rcmod(scientific-5, plausibility-6)

