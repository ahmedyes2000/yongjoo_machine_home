(ROOT
  (S ('' '')
    (NP (NNP Gravity)
      (CC and)
      (JJ other) (JJ universal) (NN cosmological) (NNS constants))
    (VP (VBD were) (RB not)
      (VP
        (ADVP (RB intelligently))
        (VBN designed)
        (PP (IN for)
          (NP
            (NP (DT the) (NN convenience))
            (PP (IN of)
              (NP (NN life)))))
        (PP (IN on)
          (NP (NN earth)))))
    (. .)))

nn(constants-7, Gravity-2)
conj_and(Gravity-2, other-4)
nn(constants-7, other-4)
amod(constants-7, universal-5)
nn(constants-7, cosmological-6)
nsubjpass(designed-11, constants-7)
auxpass(designed-11, were-8)
neg(designed-11, not-9)
advmod(designed-11, intelligently-10)
root(ROOT-0, designed-11)
det(convenience-14, the-13)
prep_for(designed-11, convenience-14)
prep_of(convenience-14, life-16)
prep_on(designed-11, earth-18)

