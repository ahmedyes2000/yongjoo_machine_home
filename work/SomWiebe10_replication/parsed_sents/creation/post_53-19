(ROOT
  (S
    (S
      (NP (PRP I))
      (VP (VBP 'm)
        (ADVP (RB just))
        (VP (VBG addressing)
          (NP
            (NP (DT the) (JJ simple) (NN core))
            (PP (IN of)
              (NP (NNP ID)))))))
    (: :)
    (S
      (NP (NNP Design))
      (VP (VBZ eliminates)
        (NP (NN chance))
        (PP (IN as)
          (NP (DT a) (NN cause)))))
    (. .)))

nsubj(addressing-4, I-1)
aux(addressing-4, 'm-2)
advmod(addressing-4, just-3)
root(ROOT-0, addressing-4)
det(core-7, the-5)
