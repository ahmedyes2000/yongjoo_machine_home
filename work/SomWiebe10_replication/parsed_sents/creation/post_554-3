(ROOT
  (S
    (VP (VB Read)
      (NP
        (NP (DT the) (JJ other) (NN side) (POS 's))
        (NN answer))
      (PP (TO to)
        (NP (DT this) (NN attack))))
    (. .)))

root(ROOT-0, Read-1)
det(side-4, the-2)
amod(side-4, other-3)
poss(answer-6, side-4)
dobj(Read-1, answer-6)
det(attack-9, this-8)
prep_to(Read-1, attack-9)

