(ROOT
  (S
    (NP
      (NP (RB Even) (DT the) (NN theory))
      (PP (IN of)
        (NP (NN gravity))))
    (ADVP (RB still))
    (VP (VBZ receives)
      (NP (JJ serious) (NNS challenges)))
    (. .)))

advmod(theory-3, Even-1)
det(theory-3, the-2)
nsubj(receives-7, theory-3)
prep_of(theory-3, gravity-5)
advmod(receives-7, still-6)
root(ROOT-0, receives-7)
amod(challenges-9, serious-8)
dobj(receives-7, challenges-9)

