(ROOT
  (S ('' '')
    (NP (NNS Scientists))
    (ADVP (RB then))
    (VP (VBP perform)
      (NP (JJ experimental) (NNS tests))
      (PP (IN upon)
        (NP (JJ natural) (NNS objects)))
      (S
        (VP (TO to)
          (VP (VB determine)
            (SBAR (IN if)
              (S
                (NP (PRP they))
                (VP (VBP contain)
                  (NP
                    (NP (NN complex))
                    (CC and)
                    (NP (VBN specified) (NN information))))))))))
    (. .)))

nsubj(perform-4, Scientists-2)
xsubj(determine-11, Scientists-2)
advmod(perform-4, then-3)
root(ROOT-0, perform-4)
amod(tests-6, experimental-5)
dobj(perform-4, tests-6)
amod(objects-9, natural-8)
prep_upon(perform-4, objects-9)
aux(determine-11, to-10)
xcomp(perform-4, determine-11)
mark(contain-14, if-12)
nsubj(contain-14, they-13)
advcl(determine-11, contain-14)
dobj(contain-14, complex-15)
amod(information-18, specified-17)
dobj(contain-14, information-18)
conj_and(complex-15, information-18)

