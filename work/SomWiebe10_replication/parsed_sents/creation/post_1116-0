(ROOT
  (S
    (NP (PRP I))
    (VP (VBP feel)
      (S
        (NP (DT the) (NN need))
        (VP (TO to)
          (VP (VB distinguish)
            (PP (IN between)
              (NP (NN micro-evolution)
                (CC and)
                (NN macro-evolution)))))))
    (. .)))

nsubj(feel-2, I-1)
root(ROOT-0, feel-2)
det(need-4, the-3)
nsubj(distinguish-6, need-4)
aux(distinguish-6, to-5)
xcomp(feel-2, distinguish-6)
prep_between(distinguish-6, micro-evolution-8)
prep_between(distinguish-6, macro-evolution-10)
conj_and(micro-evolution-8, macro-evolution-10)

