(ROOT
  (S
    (INTJ (UH True))
    (, ,)
    (NP (EX there))
    (VP (MD may)
      (VP (VB be)
        (ADJP (JJ dots)
          (S
            (VP (TO to)
              (VP (VB be)
                (VP (VBN connected))))))))
    (. .)))

