(ROOT
  (S
    (PP (IN In)
      (NP (PRP$ their) (JJ second) (NN paragraph)))
    (, ,)
    (NP (NNP NCSE))
    (VP (VBZ presents)
      (NP
        (NP (DT a)
          (ADJP (JJ distorted))
          (NN definition))
        (PP (IN of)
          (NP (JJ intelligent) (NN design)))))
    (. .)))

poss(paragraph-4, their-2)
amod(paragraph-4, second-3)
prep_in(presents-7, paragraph-4)
nsubj(presents-7, NCSE-6)
root(ROOT-0, presents-7)
det(definition-10, a-8)
amod(definition-10, distorted-9)
dobj(presents-7, definition-10)
amod(design-13, intelligent-12)
prep_of(definition-10, design-13)

