(ROOT
  (S
    (S
      (NP (PRP He))
      (VP (VBZ is)
        (ADVP (IN before) (DT all))
        (NP (NNS things))))
    (, ,)
    (CC and)
    (S
      (PP (IN in)
        (NP (PRP him)))
      (NP (DT all) (NNS things))
      (VP (VBP hold)
        (ADVP (RB together))))
    (. .)))

nsubj(things-5, He-1)
cop(things-5, is-2)
advmod(things-5, before-3)
dep(before-3, all-4)
root(ROOT-0, things-5)
prep_in(hold-12, him-9)
det(things-11, all-10)
nsubj(hold-12, things-11)
conj_and(things-5, hold-12)
