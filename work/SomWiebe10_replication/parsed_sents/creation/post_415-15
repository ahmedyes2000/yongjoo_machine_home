(ROOT
  (S ('' '')
    (S
      (SBAR (IN If)
        (S
          (NP (PRP we))
          (VP (VBP know)
            (NP (DT the) (NN Earth)))))
      (VP (VBZ is)
        (ADJP (JJ flat))))
    (, ,)
    (S
      (SBAR
        (WHADVP (WRB how))
        (S
          (VP (MD can))))
      (NP (PRP it))
      (VP (VBP be)
        (NP (NN round))))
    (. ?)))

mark(know-4, If-2)
nsubj(know-4, we-3)
csubj(flat-8, know-4)
det(Earth-6, the-5)
