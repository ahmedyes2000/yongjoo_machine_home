(ROOT
  (S
    (NP (NNP THAT) (NNP ALONE))
    (VP (VBZ is)
      (NP (NN evidence))
      (SBAR (IN that)
        (S
          (NP (PRP we))
          (VP (VBP 're) (RB all)
            (ADJP (JJ apes))))))
    (. .)))

nn(ALONE-2, THAT-1)
nsubj(evidence-4, ALONE-2)
cop(evidence-4, is-3)
root(ROOT-0, evidence-4)
complm(apes-9, that-5)
nsubj(apes-9, we-6)
cop(apes-9, 're-7)
advmod(apes-9, all-8)
ccomp(evidence-4, apes-9)

