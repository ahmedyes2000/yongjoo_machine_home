(ROOT
  (S
    (ADVP (IN So) (RB far))
    (, ,)
    (NP (PRP it))
    (VP (VBZ fits)
      (ADVP (RB together))
      (ADVP (RB fairly) (RB well)))
    (. .)))

advmod(fits-5, So-1)
advmod(So-1, far-2)
nsubj(fits-5, it-4)
root(ROOT-0, fits-5)
advmod(fits-5, together-6)
advmod(well-8, fairly-7)
advmod(fits-5, well-8)

