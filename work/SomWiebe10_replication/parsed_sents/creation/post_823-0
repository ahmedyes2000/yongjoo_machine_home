(ROOT
  (SQ (VBD Was)
    (NP (DT the) (NN earth))
    (VP (VBN created)
      (PP (IN in)
        (NP
          (NP (CD six) (NNS days))
          (CC or)
          (NP
            (QP (CD six) (CD thousand))
            (NNS years)))))
    (. ?)))

auxpass(created-4, Was-1)
det(earth-3, the-2)
nsubjpass(created-4, earth-3)
root(ROOT-0, created-4)
num(days-7, six-6)
prep_in(created-4, days-7)
number(thousand-10, six-9)
num(years-11, thousand-10)
prep_in(created-4, years-11)
conj_or(days-7, years-11)

