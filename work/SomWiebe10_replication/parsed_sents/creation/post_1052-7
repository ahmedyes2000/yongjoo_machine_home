(ROOT
  (S
    (NP (PRP You))
    (VP (VBP 're)
      (VP (VBP are)
        (ADJP (JJ right)
          (PP (IN about)
            (ADVP
              (NP (CD one) (NN thing))
              (IN though))))))
    (. .)))

nsubj(right-4, You-1)
aux(right-4, 're-2)
cop(right-4, are-3)
root(ROOT-0, right-4)
prep(right-4, about-5)
num(thing-7, one-6)
npadvmod(though-8, thing-7)
pcomp(about-5, though-8)

