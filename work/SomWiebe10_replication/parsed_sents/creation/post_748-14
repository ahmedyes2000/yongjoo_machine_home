(ROOT
  (S
    (NP (DT these) (NNS things))
    (VP (VBP are)
      (ADJP (JJ hard)
        (PP (IN for)
          (FRAG
            (NP
              (NP (JJS most))
              (SBAR
                (S
                  (VP (TO to)
                    (VP (VB understand)))))
              (. .))))))
    (. .)))

det(things-2, these-1)
nsubj(hard-4, things-2)
cop(hard-4, are-3)
root(ROOT-0, hard-4)
prep(hard-4, for-5)
dep(for-5, most-6)
aux(understand-8, to-7)
infmod(most-6, understand-8)

