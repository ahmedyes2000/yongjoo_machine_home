(ROOT
  (S
    (SBAR (IN That)
      (S
        (NP (PRP you))
        (VP (VBP continue)
          (S
            (VP (TO to)
              (VP (VB argue)
                (PP (IN for)
                  (NP (PRP it)))
                (PP (IN in)
                  (NP
                    (NP (NN spite))
                    (PP (IN of)
                      (NP (DT this)))))))))))
    (VP (VBZ shows)
      (SBAR (IN that)
        (S
          (NP (PRP you))
          (VP (VBP have)
            (NP (DT some) (JJ other) (NN agenda))))))
    (. .)))

complm(continue-3, That-1)
nsubj(continue-3, you-2)
xsubj(argue-5, you-2)
csubj(shows-12, continue-3)
aux(argue-5, to-4)
xcomp(continue-3, argue-5)
prep_for(argue-5, it-7)
prep_in_spite_of(argue-5, this-11)
root(ROOT-0, shows-12)
complm(have-15, that-13)
nsubj(have-15, you-14)
ccomp(shows-12, have-15)
det(agenda-18, some-16)
amod(agenda-18, other-17)
dobj(have-15, agenda-18)

