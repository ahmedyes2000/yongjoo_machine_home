(ROOT
  (S
    (PP (IN Of)
      (NP (NN course)))
    (NP (PRP we))
    (VP (VBP have)
      (NP
        (NP (DT a) (JJ bloody) (NN idea))
        (PP (IN of)
          (NP (DT a) (JJ defined) (JJ developmental) (NN line)))))
    (. .)))

prep_of(have-4, course-2)
nsubj(have-4, we-3)
root(ROOT-0, have-4)
det(idea-7, a-5)
amod(idea-7, bloody-6)
dobj(have-4, idea-7)
det(line-12, a-9)
amod(line-12, defined-10)
amod(line-12, developmental-11)
prep_of(idea-7, line-12)

