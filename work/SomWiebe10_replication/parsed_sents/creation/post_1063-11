(ROOT
  (S ('' '')
    (NP (NNP Aithiesm))
    (VP (VBZ constitutes)
      (SBAR (IN that)
        (S
          (NP (NNP NOTHING))
          (VP (VBD created)
            (NP (DT the) (NN universe))))))
    (. .)))

nsubj(constitutes-3, Aithiesm-2)
root(ROOT-0, constitutes-3)
complm(created-6, that-4)
nsubj(created-6, NOTHING-5)
ccomp(constitutes-3, created-6)
det(universe-8, the-7)
dobj(created-6, universe-8)

