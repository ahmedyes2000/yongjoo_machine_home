(ROOT
  (S
    (NP (PRP I))
    (VP (MD would)
      (VP (VB love)
        (S
          (VP (TO to)
            (VP (VB hear)
              (NP (PRP it)))))))
    (. .)))

nsubj(love-3, I-1)
xsubj(hear-5, I-1)
aux(love-3, would-2)
root(ROOT-0, love-3)
aux(hear-5, to-4)
xcomp(love-3, hear-5)
dobj(hear-5, it-6)

