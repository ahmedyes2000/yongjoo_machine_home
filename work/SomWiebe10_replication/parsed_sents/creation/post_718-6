(ROOT
  (S ('' '')
    (S
      (NP (DT The) (NN moon))
      (VP (VBZ is) (RB not)
        (ADJP (JJ necessary)
          (PP (IN for)
            (NP (NN light))))
        (PP (IN during)
          (NP (DT the) (NN night)))))
    (: ...)
    (S
      (SBAR (IN because)
        (S
          (NP (PRP it))
          (VP (VBZ is)
            (ADVP (RB there))
            (NP (NNS creatures)))))
      (VP
        (VP (VBP have)
          (VP (VBN adapted)
            (PP (TO to)
              (NP
                (NP (PRP it) (POS 's))
                (NN light)))))
        (CC and)
        (VP (VBP use)
          (NP (PRP it)))))
    (. .)))

det(moon-3, The-2)
nsubj(necessary-6, moon-3)
cop(necessary-6, is-4)
neg(necessary-6, not-5)
root(ROOT-0, necessary-6)
prep_for(necessary-6, light-8)
det(night-11, the-10)
prep_during(necessary-6, night-11)
mark(creatures-17, because-13)
nsubj(creatures-17, it-14)
cop(creatures-17, is-15)
advmod(creatures-17, there-16)
csubj(adapted-19, creatures-17)
csubj(use-25, creatures-17)
aux(adapted-19, have-18)
parataxis(necessary-6, adapted-19)
poss(light-23, it-21)
prep_to(adapted-19, light-23)
parataxis(necessary-6, use-25)
conj_and(adapted-19, use-25)
dobj(use-25, it-26)

