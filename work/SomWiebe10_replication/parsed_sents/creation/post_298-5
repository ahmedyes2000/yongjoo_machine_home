      (NP
        (NP (DT Both) (NNS kinds))
        (PP (IN of)
          (NP (NNS circuits))))
      (VP (VBP work)))
    (, ,)
    (CC but)
    (S
      (NP (PRP they))
      (VP (VBP have)
        (NP
          (ADJP (RB distinctly) (JJ different))
          (NNS characteristics))))
    (. .)))

det(kinds-2, Both-1)
nsubj(work-5, kinds-2)
prep_of(kinds-2, circuits-4)
root(ROOT-0, work-5)
