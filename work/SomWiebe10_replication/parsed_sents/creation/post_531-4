(ROOT
  (S
    (NP (PRP It))
    (ADVP (RB always))
    (VP (VBZ helps)
      (S
        (VP (TO to)
          (VP (VB comprehend)
            (NP
              (NP (DT the) (NN context))
              (SBAR
                (WHPP (IN in)
                  (WHNP (WDT which)))
                (S
                  (NP (DT these) (NN quote) (NNS mines))
                  (VP (VBD were)
                    (VP (VBN produced))))))))))
    (. .)))

nsubj(helps-3, It-1)
xsubj(comprehend-5, It-1)
advmod(helps-3, always-2)
root(ROOT-0, helps-3)
aux(comprehend-5, to-4)
xcomp(helps-3, comprehend-5)
det(context-7, the-6)
dobj(comprehend-5, context-7)
rel(produced-14, which-9)
det(mines-12, these-10)
nn(mines-12, quote-11)
nsubjpass(produced-14, mines-12)
auxpass(produced-14, were-13)
rcmod(context-7, produced-14)

