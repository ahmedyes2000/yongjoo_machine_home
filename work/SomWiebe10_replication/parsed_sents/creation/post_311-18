(ROOT
  (S
    (S
      (VP (VBG -RRB-) ('' '')))
    (VP (VBZ Is)
      (ADVP (RB there))
      (NP
        (NP (NN evidence))
        (PP (IN for)
          (NP
            (NP (NNP ID))
            (PP (IN in)
              (NP (NN nature)))))))
    (. .)))

cop(evidence-5, Is-3)
