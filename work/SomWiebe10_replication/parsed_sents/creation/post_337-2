(ROOT
  (S
    (NP (DT The) (NNS data))
    (VP (VBZ shows)
      (NP
        (NP (NN evidence))
        (PP (IN for)
          (NP
            (NP (JJ systematic) (NNS differences))
            (PP (IN between)
              (NP (DT the) (NNP Northern)
                (CC and)
                (NNP Southern) (NNS regions)))))
        (PP (IN of)
          (NP (DT the) (NN sky)))))
    (. .)))

det(data-2, The-1)
nsubj(shows-3, data-2)
root(ROOT-0, shows-3)
dobj(shows-3, evidence-4)
amod(differences-7, systematic-6)
prep_for(evidence-4, differences-7)
det(regions-13, the-9)
nn(regions-13, Northern-10)
conj_and(Northern-10, Southern-12)
nn(regions-13, Southern-12)
prep_between(differences-7, regions-13)
det(sky-16, the-15)
prep_of(evidence-4, sky-16)

