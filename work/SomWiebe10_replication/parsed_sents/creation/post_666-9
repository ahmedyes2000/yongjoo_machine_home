(ROOT
  (S
    (S
      (NP (DT The) (NNS philosophers))
      (VP (VBD did) (RB n't)
        (VP (VB have)
          (NP
            (NP (DT a) (NN clue))
            (PP (IN about)
              (NP (NN evolution))))
          (ADVP (RB back) (RB then)))))
    (CC and)
    (S
      (NP (PRP you))
      (ADVP (RB still))
      (VP (VBP do) (RB n't)
        (VP (VB have)
          (NP (DT a) (NN clue))
          (NP (NN today)))))
    (. .)))

