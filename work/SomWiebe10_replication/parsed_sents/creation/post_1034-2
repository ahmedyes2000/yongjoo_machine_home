(ROOT
  (S
    (ADVP (RB Socrates))
    (NP (PRP I))
    (VP (VBP like)
      (ADVP
        (NP (DT a) (NN lot))
        (RB too))
      (SBAR (RB mainly) (IN because)
        (S
          (NP (PRP I))
          (VP (VBP think)
            (PP (IN like)
              (NP (PRP him)))
            (PP (IN on)
              (SBAR
                (WHADVP (WRB how))
                (S
                  (NP (EX there))
                  (VP (VBZ is)
                    (NP
                      (NP (DT no) (JJ real) (NN answer))
                      (PP (IN for)
                        (NP (NN anything))))))))))))
    (. .)))

advmod(like-3, Socrates-1)
nsubj(like-3, I-2)
root(ROOT-0, like-3)
det(lot-5, a-4)
npadvmod(too-6, lot-5)
advmod(like-3, too-6)
