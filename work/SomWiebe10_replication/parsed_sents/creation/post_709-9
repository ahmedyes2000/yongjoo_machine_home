(ROOT
  (S
    (NP (NNP Science))
    (VP (MD ca) (RB n't)
      (VP (VB help)
        (NP (PRP you))
        (PP (IN with)
          (NP (PRP$ your) (NN faith)))
        (, ,)
        (SBAR (IN since)
          (S
            (NP (PRP it))
            (VP (VBZ is)
              (PP (IN by)
                (NP (NN nature)))
              (NP
                (NP (DT the) (NNS antithesis))
                (PP (IN of)
                  (NP (NN faith)))))))))
    (. .)))

nsubj(help-4, Science-1)
aux(help-4, ca-2)
neg(help-4, n't-3)
root(ROOT-0, help-4)
dobj(help-4, you-5)
poss(faith-8, your-7)
prep_with(help-4, faith-8)
mark(antithesis-16, since-10)
nsubj(antithesis-16, it-11)
cop(antithesis-16, is-12)
prep_by(antithesis-16, nature-14)
det(antithesis-16, the-15)
advcl(help-4, antithesis-16)
prep_of(antithesis-16, faith-18)

