  (S
    (NP (DT The) (NN fact))
    (VP (VBZ remains) (, ,)
      (SBAR (IN that)
        (S
          (PRN (, ,)
            (ADVP (RB even))
            (PP (IN in)
              (NP (PRP$ your) (NN response)))
            (, ,))
          (NP (PRP you))
          (VP (VBP have)
            (VP (VBN failed)
              (S
                (VP (TO to)
                  (VP (VB show)
                    (SBAR (IN that)
                      (S
                        (NP (NNP ID))
                        (VP (VBZ is)
                          (ADJP (RB scientifically) (JJ relevant)))))))))))))
    (. .)))

det(fact-2, The-1)
nsubj(remains-3, fact-2)
root(ROOT-0, remains-3)
complm(failed-14, that-5)
dep(in-8, even-7)
dep(failed-14, in-8)
