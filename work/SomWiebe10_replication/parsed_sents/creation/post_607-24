(ROOT
  (S
    (ADVP (RB Therefor))
    (, ,)
    (NP (DT these) (NNS machines))
    (VP (VBP are) (RB not)
      (VP (VBN designed)))
    (. .)))

advmod(designed-7, Therefor-1)
det(machines-4, these-3)
nsubjpass(designed-7, machines-4)
auxpass(designed-7, are-5)
neg(designed-7, not-6)
root(ROOT-0, designed-7)

