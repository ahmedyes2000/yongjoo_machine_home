(ROOT
  (S
    (ADVP (RB Finally))
    (, ,)
    (NP (PRP they))
    (VP (VBP want)
      (S
        (NP (NNP God))
        (VP (TO to)
          (VP (VB be)
            (ADJP (RB so) (JJ mysterious))
            (SBAR (IN that)
              (S
                (NP (DT no) (NN one))
                (VP (MD can)
                  (VP (VB understand)
                    (NP (PRP him))))))))))
    (. .)))

advmod(want-4, Finally-1)
nsubj(want-4, they-3)
root(ROOT-0, want-4)
nsubj(mysterious-9, God-5)
aux(mysterious-9, to-6)
cop(mysterious-9, be-7)
advmod(mysterious-9, so-8)
xcomp(want-4, mysterious-9)
complm(understand-14, that-10)
det(one-12, no-11)
nsubj(understand-14, one-12)
aux(understand-14, can-13)
ccomp(mysterious-9, understand-14)
dobj(understand-14, him-15)

