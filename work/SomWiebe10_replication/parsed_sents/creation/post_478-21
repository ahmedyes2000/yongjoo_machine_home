(ROOT
  (S
    (NP (PRP They))
    (VP (VBP look)
      (PP (IN at)
        (NP (NN everything)))
      (PP (IN through)
        (NP
          (NP (PRP$ their) (NN faith))
          (PP (IN in)
            (NP (NN evolution))))))
    (. .)))

nsubj(look-2, They-1)
root(ROOT-0, look-2)
prep_at(look-2, everything-4)
poss(faith-7, their-6)
prep_through(look-2, faith-7)
prep_in(faith-7, evolution-9)

