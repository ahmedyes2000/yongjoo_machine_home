(ROOT
  (S
    (NP (PRP I))
    (VP (VBP question)
      (SBAR (IN whether)
        (S
          (NP (NNP NCSE))
          (VP (VBZ is)
            (ADJP (VBN qualified)
              (S
                (VP (TO to)
                  (VP (VB participate)
                    (PP (IN in)
                      (NP (DT this) (NN debate)))))))))))
    (. .)))

nsubj(question-2, I-1)
root(ROOT-0, question-2)
complm(qualified-6, whether-3)
nsubjpass(qualified-6, NCSE-4)
auxpass(qualified-6, is-5)
ccomp(question-2, qualified-6)
aux(participate-8, to-7)
xcomp(qualified-6, participate-8)
det(debate-11, this-10)
prep_in(participate-8, debate-11)

