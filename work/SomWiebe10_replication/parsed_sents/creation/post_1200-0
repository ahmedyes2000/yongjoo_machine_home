(ROOT
  (S
    (NP (EX There))
    (VP (VBZ is)
      (NP
        (NP (JJ positive) (NN evidence))
        (PP (IN against)
          (NP (NN macroevolution)))))
    (. .)))

expl(is-2, There-1)
root(ROOT-0, is-2)
amod(evidence-4, positive-3)
nsubj(is-2, evidence-4)
prep_against(evidence-4, macroevolution-6)

