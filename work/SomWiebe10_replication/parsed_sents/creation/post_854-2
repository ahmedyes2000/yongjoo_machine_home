  (S
    (SBAR (IN If)
      (S
        (NP (DT the) (JJ fossil) (NN record))
        (VP (VBZ is) (RB n't)
          (SBAR
            (WHNP (WP what))
            (S
              (S
                (NP (PRP it))
                (VP (VBZ appears)
                  (S
                    (VP (TO to)
                      (VP (VB be))))))
              (CC and)
              (S
                (NP (NNP AiG))
                (VP (MD can)
                  (VP (VB demonstrate)
                    (NP (PRP it))))))))))
    (, ,)
    (NP (DT the) (NNS scientists))
    (VP (MD will)
      (VP (VB listen)))
    (. .)))

