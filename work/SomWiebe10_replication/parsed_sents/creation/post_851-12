(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (DT The) (NNP LORD))
        (VP (VBD chose)
          (S
            (VP (TO to)
              (VP (VB do)
                (NP (NN something))))))))
    (, ,)
    (PP (IN like)
      (NP (DT that)))
    (, ,)
    (NP (PRP He))
    (VP (MD could)
      (VP (VB do)
        (NP (DT an)
          (ADJP (RB infinitely) (JJR better))
          (NN job))
        (SBAR (IN than)
          (S
            (NP (PRP we) (NNS humans))
            (VP (MD can))))))
    (. .)))

mark(chose-4, If-1)
det(LORD-3, The-2)
nsubj(chose-4, LORD-3)
xsubj(do-6, LORD-3)
advcl(do-14, chose-4)
aux(do-6, to-5)
xcomp(chose-4, do-6)
dobj(do-6, something-7)
prep_like(do-14, that-10)
nsubj(do-14, He-12)
aux(do-14, could-13)
root(ROOT-0, do-14)
det(job-18, an-15)
advmod(better-17, infinitely-16)
