(ROOT
  (S
    (ADVP (RB However))
    (, ,)
    (NP
      (NP (NNS books))
      (PP (IN like)
        (NP
          (NP (NNP Leviticus))
          (PP (IN in)
            (NP
              (NP (NNS chapters) (CD 18)
                (CC and)
                (CD 20))
              (, ,)
              (NP (NN incest)))))))
    (VP (VBZ is)
      (ADVP (RB clearly))
      (VP (VBN condemned)))
    (. .)))

advmod(condemned-15, However-1)
nsubjpass(condemned-15, books-3)
prep_like(books-3, Leviticus-5)
prep_in(Leviticus-5, chapters-7)
num(chapters-7, 18-8)
prep_in(Leviticus-5, 20-10)
conj_and(chapters-7, 20-10)
appos(chapters-7, incest-12)
auxpass(condemned-15, is-13)
advmod(condemned-15, clearly-14)
root(ROOT-0, condemned-15)

