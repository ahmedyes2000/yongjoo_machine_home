(ROOT
  (S
    (NP (NNP Isaac))
    (VP (VBD became)
      (NP
        (NP (NN son))
        (PP (IN of)
          (NP (NNP Abraham))))
      (, ,)
      (PP (IN with)
        (S
          (NP (NNP Esau)
            (CC and)
            (NNP Jacob))
          (VP (VBG becoming)
            (SBAR
              (S
                (NP (NNP Isaac))
                (VP (VBZ s)
                  (NP (NNS sons)))))))))
    (. .)))

nsubj(son-3, Isaac-1)
cop(son-3, became-2)
root(ROOT-0, son-3)
prep_of(son-3, Abraham-5)
nsubj(becoming-11, Esau-8)
conj_and(Esau-8, Jacob-10)
nsubj(becoming-11, Jacob-10)
prepc_with(son-3, becoming-11)
nsubj(sons-14, Isaac-12)
cop(sons-14, s-13)
