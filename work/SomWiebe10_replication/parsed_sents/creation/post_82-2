(ROOT
  (S
    (NP (PRP They))
    (VP (VBP are)
      (PP
        (PP (IN about)
          (S
            (VP (VBG explaining)
              (NP (JJ basic) (JJ human) (NNS needs)
                (CC and)
                (NNS emotions)))))
        (CC and)
        (PP (IN about)
          (SBAR
            (WHADVP (WRB how))
            (S
              (NP (NN man))
              (VP (VBZ fits)
                (PP (IN with)
                  (NP
                    (NP (DT both) (NN nature))
                    (CC and)
                    (NP (PRP himself))))))))))
    (. .)))

nsubj(are-2, They-1)
root(ROOT-0, are-2)
prepc_about(are-2, explaining-4)
amod(needs-7, basic-5)
amod(needs-7, human-6)
dobj(explaining-4, needs-7)
dobj(explaining-4, emotions-9)
conj_and(needs-7, emotions-9)
advmod(fits-14, how-12)
nsubj(fits-14, man-13)
prepc_about(are-2, fits-14)
conj_and(explaining-4, fits-14)
det(nature-17, both-16)
prep_with(fits-14, nature-17)
prep_with(fits-14, himself-19)
conj_and(nature-17, himself-19)

