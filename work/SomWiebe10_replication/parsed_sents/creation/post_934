(ROOT
  (S
    (S
      (VP (VBG Citing)
        (NP
          (NP (DT the) (NN bible))
          (CC or)
          (NP (DT any) (JJ other) (JJ religious) (NN text)))
        (PP (IN as)
          (NP (NN proof)))
        (PP (TO to)
          (NP (DT the) (NN contrary)))))
    (VP (VBZ is)
      (ADJP (JJ akin)
        (PP (TO to)
          (S
            (VP (VBG citing)
              (NP
                (NP (NNP Aesop) (POS 's))
                (NNS fables))
              (PP (IN as)
                (NP
                  (NP (NN proof))
                  (PP (IN for)
                    (NP (VBG talking) (NNS animals))))))))))
    (. .)))

