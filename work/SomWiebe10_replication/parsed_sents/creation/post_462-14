(ROOT
  (S
    (NP
      (NP (NNS Scientists))
      (CC and)
      (NP (PRP$ his) (NNS peers)))
    (VP (VBD jumped)
      (NP
        (NP (DT all))
        (PP (IN over)
          (NP (PRP him)))))
    (. .)))

nsubj(jumped-5, Scientists-1)
poss(peers-4, his-3)
conj_and(Scientists-1, peers-4)
nsubj(jumped-5, peers-4)
root(ROOT-0, jumped-5)
dobj(jumped-5, all-6)
prep_over(all-6, him-8)

