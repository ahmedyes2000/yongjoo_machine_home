    (VP (VBZ is)
      (SBAR (IN that)
        (S
          (S
            (NP (RB about) (CD 90,000) (NN scientist))
            (VP (VBP do) (RB n't)
              (VP (VB believe)
                (PP (IN in)
                  (NP (NNP ID))))))
          (CC and)
          (S
            (NP (PRP you))
            (VP (MD should) (RB n't)))
          (ADVP (RB either)))))
    (. .)))

