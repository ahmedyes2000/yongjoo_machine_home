(ROOT
  (S (CC but)
    (VP (VB have)
      (VP (VBN made)
        (NP (DT an) (JJ imperfect) (NN being))
        (: ...)
        (FRAG
          (ADJP (VBN doomed)
            (PP (TO to)
              (NP (NNS mistakes)
                (CC and)
                (NN suffering))))
          (. .))))
    (. .)))

cc(made-3, but-1)
aux(made-3, have-2)
root(ROOT-0, made-3)
det(being-6, an-4)
amod(being-6, imperfect-5)
dobj(made-3, being-6)
dep(made-3, doomed-8)
prep_to(doomed-8, mistakes-10)
