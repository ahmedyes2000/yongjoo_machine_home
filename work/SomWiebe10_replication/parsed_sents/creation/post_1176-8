(ROOT
  (S
    (NP (PRP You))
    (VP (VBP have)
      (S
        (VP (TO to)
          (VP (VB understand)
            (NP (NN adaptation))
            (SBAR (IN before)
              (S
                (NP (PRP you))
                (VP (VB debate)
                  (NP (NN evolution)))))))))
    (. .)))

nsubj(have-2, You-1)
xsubj(understand-4, You-1)
root(ROOT-0, have-2)
aux(understand-4, to-3)
xcomp(have-2, understand-4)
dobj(understand-4, adaptation-5)
mark(debate-8, before-6)
nsubj(debate-8, you-7)
advcl(understand-4, debate-8)
dobj(debate-8, evolution-9)

