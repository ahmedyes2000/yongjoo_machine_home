(ROOT
  (S (CC And)
    (VP (VB let)
      (S
        (NP (POS 's))
        (VP
          (ADVP (RB just))
          (VB say)
          (PP (IN for)
            (NP
              (NP (NN sake))
              (PP (IN of)
                (NP (NN argument)))))
          (SBAR (IN that)
            (S
              (NP (EX there))
              (VP (VBZ is)
                (NP (DT a) (NNP Creator))))))))
    (. .)))

cc(let-2, And-1)
root(ROOT-0, let-2)
nsubj(say-5, 's-3)
advmod(say-5, just-4)
ccomp(let-2, say-5)
prep_for(say-5, sake-7)
