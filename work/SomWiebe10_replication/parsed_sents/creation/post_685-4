(ROOT
  (S
    (NP (PRP We))
    (VP (VBP 're)
      (ADJP
        (ADVP (RBR smarter) (IN than))
        (SBAR (IN that)
          (FRAG
            (ADVP (RB now))))))
    (. .)))

nsubj(than-4, We-1)
cop(than-4, 're-2)
advmod(than-4, smarter-3)
root(ROOT-0, than-4)
complm(now-6, that-5)
dep(than-4, now-6)

