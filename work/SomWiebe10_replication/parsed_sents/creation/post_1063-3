(ROOT
  (S
    (S
      (NP (DT All))
      (VP (VBP are)
        (NP (NNS theories))))
    (: ...)
    (S
      (NP (DT all))
      (VP (VBP are)
        (ADVP (RB firmly))
        (VP (VBN founded))))
    (: ...)
    (S
      (NP (DT all))
      (VP (VBP are)
        (VP (VBN accepted)
          (PP (IN as)
            (NP
              (NP (JJ valid) (NNS descriptions))
              (PP (IN of)
                (NP (WDT whatever) (NN aspect)))
              (PP (IN of)
                (NP (DT the) (NN universe))))))))
    (. .)))

nsubj(theories-3, All-1)
cop(theories-3, are-2)
root(ROOT-0, theories-3)
nsubjpass(founded-8, all-5)
auxpass(founded-8, are-6)
advmod(founded-8, firmly-7)
parataxis(theories-3, founded-8)
nsubjpass(accepted-12, all-10)
auxpass(accepted-12, are-11)
parataxis(theories-3, accepted-12)
amod(descriptions-15, valid-14)
prep_as(accepted-12, descriptions-15)
dep(aspect-18, whatever-17)
prep_of(descriptions-15, aspect-18)
det(universe-21, the-20)
prep_of(descriptions-15, universe-21)

