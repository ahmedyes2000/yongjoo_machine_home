(ROOT
  (S
    (NP (PRP I))
    (VP (VBP trust)
      (NP (PRP him))
      (PP (RB not)
        (NP (PRP$ his) (JJ flawed) (NN creation))))
    (. .)))

nsubj(trust-2, I-1)
root(ROOT-0, trust-2)
dobj(trust-2, him-3)
prep(trust-2, not-4)
poss(creation-7, his-5)
amod(creation-7, flawed-6)
pobj(not-4, creation-7)

