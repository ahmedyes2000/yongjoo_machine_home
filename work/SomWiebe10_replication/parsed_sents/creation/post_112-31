(ROOT
  (NP
    (NP (JJ only))
    (PP (IN for)
      (NP (PRP me)))
    (. .)))

root(ROOT-0, only-1)
prep_for(only-1, me-3)

(ROOT
  (S
    (NP (PRP It))
    (VP (VBD left)
      (SBAR
        (S
          (NP
            (NP (NN nothing))
            (ADJP (JJ physical) (DT no)))
          (VP (VBZ faces)
            (PP (IN of)
              (NP
                (NP (NNP Jesus))
                (PP (IN in)
                  (NP (JJ pancake) (NN batter))))))
          (. .))))
    (. .)))

nsubj(left-2, It-1)
root(ROOT-0, left-2)
nsubj(faces-6, nothing-3)
amod(nothing-3, physical-4)
dep(physical-4, no-5)
ccomp(left-2, faces-6)
prep_of(faces-6, Jesus-8)
amod(batter-11, pancake-10)
prep_in(Jesus-8, batter-11)

