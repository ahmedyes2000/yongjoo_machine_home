  (S
    (PP (IN Of)
      (NP
        (NP (NN course) (RB not))
        (, ,)
        (SBAR (IN because)
          (S
            (NP (NNP Darwinism))
            (VP (VBZ is)
              (NP (DT a) (JJ scientific) (NN theory)))))))
    (NP (DT that))
    (VP (VBZ stands)
      (PP (IN on)
        (NP (PRP$ its) (JJ own) (NN evidence))))
    (. .)))

