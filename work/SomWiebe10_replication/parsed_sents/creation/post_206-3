(ROOT
  (S
    (S
      (NP (DT The) (NN problem))
      (VP (VBZ is)
        (SBAR (IN that)
          (S
            (NP
              (NP (DT the) (NN origin))
              (PP (IN of)
                (NP (NN life))))
            (VP (VBZ is) (RB not)
              (NP (NN science)))))))
    (: ;)
    (S
      (NP (PRP it))
      (VP (VBD was)
        (NP (DT an) (JJ historical) (NN event))))
    (. .)))

