(ROOT
  (S
    (NP (PRP You))
    (ADVP (RB really))
    (VP (VBP expect)
      (S
        (NP (DT the) (NN world))
        (VP (TO to)
          (VP (VB look)
            (NP (DT the) (JJ same) (CD 6,000) (NNS years))
            (ADVP (RB later))))))
    (. ?)))

nsubj(expect-3, You-1)
advmod(expect-3, really-2)
root(ROOT-0, expect-3)
det(world-5, the-4)
nsubj(look-7, world-5)
aux(look-7, to-6)
xcomp(expect-3, look-7)
det(years-11, the-8)
amod(years-11, same-9)
num(years-11, 6,000-10)
tmod(look-7, years-11)
advmod(look-7, later-12)

