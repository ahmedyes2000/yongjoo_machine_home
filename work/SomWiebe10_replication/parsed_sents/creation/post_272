  (S
    (NP (NNP Science))
    (VP (VBD was)
      (ADJP (JJ able)
        (S
          (VP (TO to)
            (VP
              (ADVP (RB intelligently))
              (VB design)
              (NP (NN flagellar) (NN function)))))))
    (. .)))

