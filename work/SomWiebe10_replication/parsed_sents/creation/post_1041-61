(ROOT
  (S
    (NP (NN Yesterday))
    (NP (DT the) (NN temperature))
    (VP (VBD was)
      (NP
        (NP (CD one) (NN degree))
        (JJR colder))
      (ADVP (IN than) (NN today)))
    (. .)))

tmod(colder-7, Yesterday-1)
det(temperature-3, the-2)
nsubj(colder-7, temperature-3)
cop(colder-7, was-4)
num(degree-6, one-5)
dep(colder-7, degree-6)
root(ROOT-0, colder-7)
advmod(colder-7, than-8)
dep(than-8, today-9)

