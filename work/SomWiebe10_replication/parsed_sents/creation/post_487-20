(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ teaches)
      (S
        (NP (NNS students))
        (VP (TO to)
          (VP (VB think)
            (NP (NNS critically))))))
    (. .)))

nsubj(teaches-2, It-1)
root(ROOT-0, teaches-2)
nsubj(think-5, students-3)
aux(think-5, to-4)
xcomp(teaches-2, think-5)
dobj(think-5, critically-6)

