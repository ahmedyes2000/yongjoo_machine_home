(ROOT
  (S ('' '')
    (PP (VBG Nothing)
      (PP (IN in)
        (NP
          (NP (DT the) (NN passage))
          (PP (IN of)
            (NP (DT the) (NN ruling))))))
    (VP (VBZ mentions)
      (SBAR (IN that)
        (S
          (NP (PRP$ their) (NN argument))
          (VP (VBD was)
            (VP (VBN heard))))))
    (. .)))

prep(mentions-9, Nothing-2)
pcomp(Nothing-2, in-3)
det(passage-5, the-4)
prep_in(Nothing-2, passage-5)
det(ruling-8, the-7)
prep_of(passage-5, ruling-8)
root(ROOT-0, mentions-9)
complm(heard-14, that-10)
poss(argument-12, their-11)
nsubjpass(heard-14, argument-12)
auxpass(heard-14, was-13)
ccomp(mentions-9, heard-14)

