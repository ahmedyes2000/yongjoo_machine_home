  (S
    (NP
      (NP (NNP Newton) (, ,) (NNP Pascal) (, ,) (NNP Einstein) (, ,))
      (CC and)
      (NP (JJ many) (NNS others)))
    (VP (VBD put)
      (NP (NN stock))
      (PP (IN in)
        (NP (DT the) (NNP ID) (NN theory))))
    (. .)))

