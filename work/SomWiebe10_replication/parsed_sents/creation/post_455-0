(ROOT
  (S
    (PP (IN In)
      (NP (NN science)))
    (, ,)
    (S
      (VP (VBG testing)
        (NP (DT a) (NN hypothesis)
          (CC or)
          (NN theory))))
    (VP (VBZ means)
      (S
        (VP (VBG deriving)
          (NP (NNS predictions))
          (PP (IN from)
            (NP (PRP them))))))
    (. .)))

prep_in(means-9, science-2)
csubj(means-9, testing-4)
det(hypothesis-6, a-5)
dobj(testing-4, hypothesis-6)
dobj(testing-4, theory-8)
conj_or(hypothesis-6, theory-8)
root(ROOT-0, means-9)
xcomp(means-9, deriving-10)
dobj(deriving-10, predictions-11)
prep_from(deriving-10, them-13)

