(ROOT
  (S
    (ADVP (RB Sometimes))
    (NP (PRP they))
    (VP (VBD died)
      (PRT (RP out))
      (SBAR (IN because)
        (S
          (PP (IN of)
            (NP
              (NP (JJ climatic) (NNS changes))
              (PP (IN in)
                (NP (DT the) (NNS areas)))))
          (NP (PRP they))
          (VP (VBD lived)
            (PRT (RP in))))))
    (. .)))

advmod(died-3, Sometimes-1)
nsubj(died-3, they-2)
root(ROOT-0, died-3)
prt(died-3, out-4)
mark(lived-13, because-5)
amod(changes-8, climatic-7)
prep_of(lived-13, changes-8)
det(areas-11, the-10)
prep_in(changes-8, areas-11)
nsubj(lived-13, they-12)
advcl(died-3, lived-13)
prt(lived-13, in-14)

