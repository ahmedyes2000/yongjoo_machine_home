(ROOT
  (S (RBR Further) (, ,)
    (NP (PRP I))
    (VP (VBP have) (RB not)
      (VP (VBN attacked)
        (NP
          (NP (DT any) (NN individual))
          (PP (RB because) (IN of)
            (NP (PRP$ their) (JJ religious) (NN belief))))))
    (. .)))

advmod(attacked-6, Further-1)
nsubj(attacked-6, I-3)
aux(attacked-6, have-4)
neg(attacked-6, not-5)
root(ROOT-0, attacked-6)
det(individual-8, any-7)
dobj(attacked-6, individual-8)
poss(belief-13, their-11)
amod(belief-13, religious-12)
prep_because_of(individual-8, belief-13)

