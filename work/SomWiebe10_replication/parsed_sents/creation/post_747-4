    (S
      (NP (NNP God))
      (VP
        (VP (VBD created)
          (NP (NNP Adam)))
        (CC and)
        (VP (VBD saw)
          (SBAR (IN that)
            (S
              (NP (PRP he))
              (VP (VBD was)
                (ADJP (JJ lonely))))))))
    (: ...)
    (S (CC but)
      (NP (NNP God))
      (ADVP (RB already))
      (VP (VBD knew)
        (SBAR (IN that)
          (S
            (NP (NNP Adam))
            (VP (VBD was)
              (VP (VBG going)
                (S
                  (VP (TO to)
                    (VP (VB be)
                      (ADJP (JJ lonely)))))))))))
    (. .)))

