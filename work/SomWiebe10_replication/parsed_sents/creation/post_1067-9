(ROOT
  (S
    (NP (PRP I))
    (VP (VBP 'm)
      (ADJP (JJ sorry)
        (SBAR (IN if)
          (S
            (NP (PRP I))
            (VP (VBD offended)
              (NP (PRP you)))))))
    (: ...)))

nsubj(sorry-3, I-1)
cop(sorry-3, 'm-2)
root(ROOT-0, sorry-3)
mark(offended-6, if-4)
nsubj(offended-6, I-5)
ccomp(sorry-3, offended-6)
dobj(offended-6, you-7)

