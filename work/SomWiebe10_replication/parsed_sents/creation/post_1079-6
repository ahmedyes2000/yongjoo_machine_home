(ROOT
  (S
    (NP (NN Creation))
    (VP (VBZ is)
      (ADVP (RB just))
      (ADJP (RB too) (RB darn) (JJ beautiful))
      (S
        (VP (TO to)
          (VP (VB believe)
            (SBAR (IN that)
              (S
                (NP (PRP it))
                (ADVP (DT all))
                (VP (VBD evolved)
                  (CC and)
                  (VBD happened)
                  (PP (IN by)
                    (NP (NN chance))))))))))
    (. .)))

nsubj(beautiful-6, Creation-1)
xsubj(believe-8, Creation-1)
cop(beautiful-6, is-2)
advmod(beautiful-6, just-3)
advmod(beautiful-6, too-4)
