(ROOT
  (S
    (NP (PRP He))
    (VP (VBD thought)
      (NP (DT a) (NN bit))
      (PP (IN on)
        (NP
          (NP (DT the) (NNS lines))
          (PP (IN of)
            (NP
              (NP
                (NP (NNP Pantheism))
                (, ,)
                (ADVP (RB actually))
                (, ,))
              (NP (DT a) (NN lot)))))))
    (. .)))

nsubj(thought-2, He-1)
root(ROOT-0, thought-2)
det(bit-4, a-3)
dobj(thought-2, bit-4)
det(lines-7, the-6)
prep_on(thought-2, lines-7)
prep_of(lines-7, Pantheism-9)
advmod(Pantheism-9, actually-11)
det(lot-14, a-13)
dep(Pantheism-9, lot-14)

