(ROOT
  (S (IN So)
    (NP (PRP we))
    (VP (MD can)
      (ADVP (RB still))
      (VP (VB be)
        (NP
          (NP (DT a) (JJ diverse) (NN country))
          (PP (IN of)
            (NP
              (NP (NNS beliefs))
              (, ,)
              (CONJP (RB not) (IN of))
              (NP (JJ stupid) (NN sheep)))))))
    (. .)))

dep(country-8, So-1)
nsubj(country-8, we-2)
aux(country-8, can-3)
advmod(country-8, still-4)
cop(country-8, be-5)
det(country-8, a-6)
amod(country-8, diverse-7)
root(ROOT-0, country-8)
prep_of(country-8, beliefs-10)
amod(sheep-15, stupid-14)
prep_of(country-8, sheep-15)
conj_negcc(beliefs-10, sheep-15)

