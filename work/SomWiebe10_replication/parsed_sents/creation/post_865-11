(ROOT
  (S
    (NP
      (NP (DT The) (NN creation) (NN myth))
      (PP (IN of)
        (NP (DT the) (NNPS Canaanites))))
    (ADVP (RB therefore))
    (VP (VBZ speaks)
      (PP (IN of)
        (NP (DT the) (JJ dry) (, ,) (JJ arid) (NN land)))
      (SBAR (IN that)
        (S
          (NP (PRP$ their) (NN God))
          (VP (VB bless)
            (PP (IN with)
              (NP (NN rain)
                (CC and)
                (NNS wells)))
            (S
              (VP (VBG breaking)
                (ADVP (RB forth))))))))
    (. .)))

