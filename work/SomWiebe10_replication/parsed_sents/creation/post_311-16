(ROOT
  (S ('' '')
    (NP (NNP Breathe))
    (ADVP (RB easy))
    (NP
      (NP (NNP PvM))
      (, ,)
      (ADVP (RB no)
        (NP (CD one))))
    (VP (VBZ is)
      (VP (VBG advocating)
        (NP (JJ holocaust) (NN denial))
        (ADVP (RB here))))
    (. .)))

nsubj(advocating-9, Breathe-2)
advmod(advocating-9, easy-3)
nsubj(advocating-9, PvM-4)
advmod(PvM-4, no-6)
pobj(no-6, one-7)
aux(advocating-9, is-8)
root(ROOT-0, advocating-9)
amod(denial-11, holocaust-10)
dobj(advocating-9, denial-11)
advmod(advocating-9, here-12)

