(ROOT
  (S (CC and)
    (ADVP (RB now))
    (NP (PRP I))
    (VP (VBP believe)
      (ADVP (RB wholeheartedly))
      (PP (IN in)
        (NP (NNP God))))
    (. .)))

cc(believe-4, and-1)
advmod(believe-4, now-2)
nsubj(believe-4, I-3)
root(ROOT-0, believe-4)
advmod(believe-4, wholeheartedly-5)
prep_in(believe-4, God-7)

