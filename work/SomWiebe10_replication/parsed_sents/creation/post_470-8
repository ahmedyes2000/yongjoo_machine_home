(ROOT
  (S
    (SBAR (IN Whether)
      (S
        (NP
          (QP (CC or)
            (RB not))
          (DT the) (NN debate))
        (VP (VBD was)
          (ADVP (RB ever))
          (PP
            (ADVP (RB primarily))
            (IN over)
            (NP (NN science))))))
    (, ,)
    (NP (DT the) (NN film))
    (VP (VBZ is)
      (ADJP (JJ correct)
        (PP (IN in)
          (S
            (VP (VBG identifying)
              (NP (PRP it))
              (PP (IN as)
                (S
                  (VP (VBG being)
                    (NP
                      (NP (DT a) (JJ world-view) (NN conflict))
                      (SBAR
                        (WHNP (WDT that))
                        (S
                          (VP (VBZ is)
                            (ADJP (RB largely) (JJ religious)
                              (PP (IN in)
                                (NP (NN character))))))))))))))))
    (. .)))

