      (, ,))
    (S
      (NP
        (NP (DT The) (NN conflict))
        (PP (IN over)
          (NP
            (NP (DT the) (NNS principles))
            (PP (IN of)
              (NP (NN evolution))))))
      (VP (VBZ has)
        (VP (VBN become)
          (NP (DT a) (JJ religious) (NN war)))))
    (: ;)
    (S
      (NP (PRP it))
      (VP (VBZ is)
        (ADVP (RB no) (RB longer))
        (NP
          (NP (DT a) (NN conflict))
          (PP (IN over)
            (NP (NN science))))))
    (. .)))

