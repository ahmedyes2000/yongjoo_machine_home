(ROOT
  (S ('' '')
    (S
      (SBAR (IN If)
        (S
          (NP (PRP it))
          (VP (MD can) (RB not))))
      (VP (VBP make)
        (NP (NNS predictions))))
    (RB then)
    (S
      (NP (PRP it))
      (VP (VBZ is) (RB not)
        (NP (DT a) (JJ viable) (NN theory))))
    (. .)))

mark(can-4, If-2)
nsubj(can-4, it-3)
csubj(make-6, can-4)
neg(can-4, not-5)
root(ROOT-0, make-6)
dobj(make-6, predictions-7)
advmod(make-6, then-8)
nsubj(theory-14, it-9)
cop(theory-14, is-10)
neg(theory-14, not-11)
det(theory-14, a-12)
amod(theory-14, viable-13)
ccomp(make-6, theory-14)

