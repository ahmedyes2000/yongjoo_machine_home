(ROOT
  (S
    (NP (PRP I))
    (VP (VBP am) (RB not)
      (ADJP (VB welcome)
        (PP (IN at)
          (NP
            (NP (NNP UcD))
            (, ,)
            (NP
              (NP (DT a) (NN site))
              (SBAR
                (WHNP (WDT which))
                (S
