(ROOT
  (S
    (PP (IN As)
      (NP (DT every) (NN thing)))
    (NP (PRP we))
    (VP (VBP have)
      (VP (VBN observed)
        (PP (IN in)
          (NP (DT the) (NN universe)))
        (ADVP (RB so) (RB far))))
    (. .)))

det(thing-3, every-2)
prep_as(observed-6, thing-3)
nsubj(observed-6, we-4)
aux(observed-6, have-5)
root(ROOT-0, observed-6)
det(universe-9, the-8)
prep_in(observed-6, universe-9)
advmod(far-11, so-10)
advmod(observed-6, far-11)

