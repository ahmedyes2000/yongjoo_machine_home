  (S
    (ADVP (JJR Worse))
    (, ,)
    (NP (PRP he))
    (VP (VBZ continues)
      (NP
        (NP (DT a) (JJ fine) (NN tradition))
        (PP (IN of)
          (NP (NN quote) (NN mining) (NNP Gould)))))
    (. .)))

