(ROOT
  (S
    (NP (PRP You))
    (VP (MD ca) (RB n't)
      (VP (VB use)
        (S
          (NP (NN something) (NNP LESS) (NNP authoritative))
          (VP (TO to)
            (VP (VB prove)
              (NP (DT a) (NN point)))))))
    (. .)))

nsubj(use-4, You-1)
aux(use-4, ca-2)
neg(use-4, n't-3)
root(ROOT-0, use-4)
nn(authoritative-7, something-5)
nn(authoritative-7, LESS-6)
nsubj(prove-9, authoritative-7)
aux(prove-9, to-8)
xcomp(use-4, prove-9)
det(point-11, a-10)
dobj(prove-9, point-11)

