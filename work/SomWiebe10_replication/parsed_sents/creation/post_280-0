(ROOT
  (NP (: --)
    (NP
      (NP (NNP ICDEsig))
      (: --)
      (NP (: ...)
        (NP (DT all) (NN design))
        (PP (VBZ is)
          (NP (JJ intelligent)))))
    (. .)))

root(ROOT-0, ICDEsig-2)
det(design-6, all-5)
dep(ICDEsig-2, design-6)
prep(design-6, is-7)
dep(is-7, intelligent-8)

