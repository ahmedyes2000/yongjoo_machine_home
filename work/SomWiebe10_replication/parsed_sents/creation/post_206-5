(ROOT
  (S
    (NP (DT No) (NN human))
    (VP (VBD observed)
      (NP
        (NP (DT the) (NN origin))
        (PP (IN of)
          (NP (NN life)))))
    (. .)))

det(human-2, No-1)
nsubj(observed-3, human-2)
root(ROOT-0, observed-3)
det(origin-5, the-4)
dobj(observed-3, origin-5)
prep_of(origin-5, life-7)

