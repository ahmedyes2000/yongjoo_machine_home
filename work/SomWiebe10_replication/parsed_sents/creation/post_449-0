(ROOT
  (S
    (NP (PRP I))
    (VP (VBP do) (RB n't)
      (ADVP (RB really))
      (VP (VB believe)
        (PP (IN in)
          (NP (DT the) (NN supernatural)))
        (SBAR (IN unless)
          (S
            (NP (PRP you))
            (VP (VBD wanted)
              (S
                (VP (TO to)
                  (VP (VB say)
                    (SBAR (IN that)
                      (S
                        (NP
                          (NP (DT the) (JJ first) (NN cause))
                          (PP (IN behind)
                            (NP (DT the) (JJ big) (NN bang))))
                        (VP (VBZ is)
                          (PP (IN beyond)
                            (NP (DT the) (JJ observable) (NN world)
                              (CC and)
                              (NN therefor) (NN supernatural))))))))))))))
    (. .)))

