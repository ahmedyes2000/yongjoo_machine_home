(ROOT
  (S
    (NP (DT The) (NNP Bible))
    (VP (VBZ is)
      (ADJP (JJ rife)
        (PP (IN with)
          (NP
            (NP (JJ vague) (NNS messages))
            (CC and)
            (NP (JJ contradictory) (NNS passages))))))
    (. .)))

det(Bible-2, The-1)
nsubj(rife-4, Bible-2)
cop(rife-4, is-3)
root(ROOT-0, rife-4)
amod(messages-7, vague-6)
prep_with(rife-4, messages-7)
amod(passages-10, contradictory-9)
prep_with(rife-4, passages-10)
conj_and(messages-7, passages-10)

