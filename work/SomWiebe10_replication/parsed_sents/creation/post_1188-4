(ROOT
  (S
    (NP (DT That))
    (VP (MD ca) (RB n't)
      (VP (VB be)
        (VP (VBN said)
          (PP (IN for)
            (NP
              (NP (NN dog) (NN boy)
                (CC or)
                (NN ibanex))
              (CC and)
              (NP (DT a) (JJ few) (JJ other)))))))
    (. .)))

nsubjpass(said-5, That-1)
aux(said-5, ca-2)
neg(said-5, n't-3)
auxpass(said-5, be-4)
root(ROOT-0, said-5)
nn(boy-8, dog-7)
prep_for(said-5, boy-8)
prep_for(said-5, ibanex-10)
conj_or(boy-8, ibanex-10)
det(other-14, a-12)
amod(other-14, few-13)
prep_for(said-5, other-14)
conj_and(boy-8, other-14)

