(ROOT
  (S ('' '')
    (S
      (SBAR
        (WHADVP (WRB How))
        (S
          (VP (VBP do)
            (NP (PRP you)))))
      (, ,)
      (VP (VB objectively)))
    (, ,)
    (VP (VBP demonstrate)
      (SBAR
        (WHNP (WDT which))
        (S
          (NP (NN understanding))
          (VP (VBZ is)
            (NP (DT the) (JJ correct) (NN one))))))
    (. ?)))

advmod(do-3, How-2)
dep(objectively-6, do-3)
dobj(do-3, you-4)
ccomp(demonstrate-8, objectively-6)
root(ROOT-0, demonstrate-8)
dobj(one-14, which-9)
nsubj(one-14, understanding-10)
cop(one-14, is-11)
det(one-14, the-12)
amod(one-14, correct-13)
ccomp(demonstrate-8, one-14)

