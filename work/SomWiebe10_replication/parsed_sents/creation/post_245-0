(ROOT
  (S
    (SBAR (IN In) (NN order)
      (S
        (VP (TO to)
          (VP (VB have)
            (NP (NNS purposefulness)
              (CC and)
              (NN intentionality))))))
    (NP (PRP you))
    (VP (VBP need)
      (NP (NN self-awareness)))
    (. .)))

mark(have-4, In-1)
dep(have-4, order-2)
aux(have-4, to-3)
dep(need-9, have-4)
dobj(have-4, purposefulness-5)
dobj(have-4, intentionality-7)
conj_and(purposefulness-5, intentionality-7)
nsubj(need-9, you-8)
root(ROOT-0, need-9)
dobj(need-9, self-awareness-10)

