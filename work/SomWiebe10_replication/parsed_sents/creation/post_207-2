(ROOT
  (S
    (NP (PRP I))
    (ADVP (RB often))
    (VP (VBP go)
      (S
        (VP (TO to)
          (VP (VB read)
            (NP
              (NP (PRP$ its) (NNS comments)
                (CC and)
                (NNS periodicals))
              (CC and)
              (NP (NNS newsletters)))))))
    (. .)))

nsubj(go-3, I-1)
xsubj(read-5, I-1)
advmod(go-3, often-2)
root(ROOT-0, go-3)
aux(read-5, to-4)
xcomp(go-3, read-5)
poss(comments-7, its-6)
dobj(read-5, comments-7)
dobj(read-5, periodicals-9)
conj_and(comments-7, periodicals-9)
dobj(read-5, newsletters-11)
conj_and(comments-7, newsletters-11)

