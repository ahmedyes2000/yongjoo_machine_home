(ROOT
  (S
    (NP (DT That))
    (VP (VBZ is)
      (SBAR
        (S
          (NP (PRP they))
          (VP (VBP are)
            (NP
              (NP (NNS data))
              (VP (VBG carrying)
                (NP (NNS mediums))))))))
    (. .)))

nsubj(is-2, That-1)
root(ROOT-0, is-2)
nsubj(data-5, they-3)
cop(data-5, are-4)
ccomp(is-2, data-5)
partmod(data-5, carrying-6)
dobj(carrying-6, mediums-7)

