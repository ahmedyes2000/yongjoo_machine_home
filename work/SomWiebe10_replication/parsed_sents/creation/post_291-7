(ROOT
  (S
    (VP (VB Faith))
    (. !)))

root(ROOT-0, Faith-1)

(ROOT
  (S
    (NP (JJ =) (NN scienceThe) (NN evidence))
    (VP (VBZ is)
      (NP (NNP NOT))
      (ADVP (RB everywhere)))
    (. .)))

amod(evidence-3, =-1)
nn(evidence-3, scienceThe-2)
nsubj(NOT-5, evidence-3)
cop(NOT-5, is-4)
root(ROOT-0, NOT-5)
advmod(NOT-5, everywhere-6)

