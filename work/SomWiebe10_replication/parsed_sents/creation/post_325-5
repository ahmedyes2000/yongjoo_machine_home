(ROOT
  (S ('' '')
    (S (CC But)
      (NP (IN if) (DT the))
      (VP (VBZ universe)))
    (PRN
      (NP (NN can))
      (VP (VBP provide)))
    (S
      (NP (VBG evidence))
      (PP (IN against)
        (NP (NN design))
        (, ,)))
    (. ...)))

cc(universe-5, But-2)
amod(the-4, if-3)
nsubj(universe-5, the-4)
root(ROOT-0, universe-5)
nsubj(provide-7, can-6)
dep(universe-5, provide-7)
ccomp(universe-5, evidence-8)
dep(evidence-8, against-9)
pobj(against-9, design-10)

