(ROOT
  (SINV
    (ADVP (IN Along))
    (VBZ comes)
    (NP
      (NP (DT a) (JJ creationist) (NN asks))
      (SBAR
        (WHNP (WP what))
        (S
          (NP (PRP they))
          (VP (VBP 're)
            (VP (VBG doing))))))
    (. .)))

advmod(comes-2, Along-1)
root(ROOT-0, comes-2)
det(asks-5, a-3)
amod(asks-5, creationist-4)
nsubj(comes-2, asks-5)
dobj(doing-9, asks-5)
rel(doing-9, what-6)
nsubj(doing-9, they-7)
aux(doing-9, 're-8)
rcmod(asks-5, doing-9)

(ROOT
  (S
    (NP (DT The) (NN evolutionist))
    (VP (VBD reply)
      (SBAR
        (S
          (NP (PRP we))
          (VP (VBP 're)
            (VP (VBG trying)
              (S
                (VP (TO to)
                  (VP (VB find)
                    (NP
                      (NP (DT the) (NN height))
                      (PP (IN of)
                        (NP (DT the) (NN pole))))))))))))
    (. .)))

det(evolutionist-2, The-1)
nsubj(reply-3, evolutionist-2)
root(ROOT-0, reply-3)
nsubj(trying-6, we-4)
xsubj(find-8, we-4)
aux(trying-6, 're-5)
