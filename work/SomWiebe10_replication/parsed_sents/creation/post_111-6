(ROOT
  (S (CC but)
    (NP (PRP it))
    (VP (VBD was) (RB n't)
      (VP (VBN accepted)
        (PP (IN as)
          (NP (DT a) (VBN given)))))
    (. .)))

cc(accepted-5, but-1)
nsubjpass(accepted-5, it-2)
auxpass(accepted-5, was-3)
neg(accepted-5, n't-4)
root(ROOT-0, accepted-5)
prep_as(accepted-5, a-7)
amod(a-7, given-8)

(ROOT
  (S (CC Yet)
    (NP
      (NP (DT the) (NN concept))
      (PP (IN of)
        (NP (DT a) (JJ circular) (NN Earth))))
    (VP (VBZ is)
      (ADJP (RB far) (JJR older))
      (ADVP (RB then) (RB that)))
    (. .)))

cc(older-10, Yet-1)
det(concept-3, the-2)
nsubj(older-10, concept-3)
det(Earth-7, a-5)
amod(Earth-7, circular-6)
prep_of(concept-3, Earth-7)
cop(older-10, is-8)
advmod(older-10, far-9)
root(ROOT-0, older-10)
advmod(that-12, then-11)
advmod(older-10, that-12)

