(ROOT
  (S
    (NP (PRP I))
    (VP (VBD was)
      (VP (VBG disagreeing)
        (PP (IN with)
          (NP
            (NP (PRP$ your) (NN generalization))
            (PP (IN of)
              (NP (NNP ID) (NN belief)))))))
    (. .)))

nsubj(disagreeing-3, I-1)
aux(disagreeing-3, was-2)
root(ROOT-0, disagreeing-3)
poss(generalization-6, your-5)
prep_with(disagreeing-3, generalization-6)
nn(belief-9, ID-8)
prep_of(generalization-6, belief-9)

