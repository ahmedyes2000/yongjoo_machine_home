(ROOT
  (S
    (S
      (VP (VB Remember)))
    (, ,)
    (NP (NN truth) (NNS claims))
    (VP (VBP have)
      (ADVP (NN nothing))
      (S
        (VP (TO to)
          (VP (VB do)
            (PP (IN with)
              (SBAR
                (WHADVP (WRB how))
                (S
                  (S
                    (NP (PRP they))
                    (VP (VBP are)
                      (VP (VBN presented))))
                  (CC and)
                  (S
                    (NP (DT neither))
                    (VP (VBZ does)
                      (NP (NN constitutionality)))))))))))
    (. .)))

ccomp(have-5, Remember-1)
nn(claims-4, truth-3)
nsubj(have-5, claims-4)
xsubj(do-8, claims-4)
root(ROOT-0, have-5)
advmod(have-5, nothing-6)
aux(do-8, to-7)
xcomp(have-5, do-8)
advmod(presented-13, how-10)
nsubjpass(presented-13, they-11)
auxpass(presented-13, are-12)
prepc_with(do-8, presented-13)
nsubj(does-16, neither-15)
