(ROOT
  (S
    (NP
      (NP (DT The) (NN one))
      (SBAR
        (WHNP (WDT that))
        (S
          (VP (VBZ suits)
            (SBAR
              (S
                (NP (PRP it))
                (VP (VBZ 's)
                  (NP (JJ current) (NN environment)))))))))
    (VP
      (VP (VBZ surveys))
      (CC and)
      (VP (VBP grow)
        (PRT (RP up))
        (S
          (VP (TO to)
            (VP (VB breed))))))
    (. .)))

det(one-2, The-1)
nsubj(suits-4, one-2)
nsubj(surveys-9, one-2)
nsubj(grow-11, one-2)
xsubj(breed-14, one-2)
rcmod(one-2, suits-4)
nsubj(environment-8, it-5)
cop(environment-8, 's-6)
amod(environment-8, current-7)
ccomp(suits-4, environment-8)
root(ROOT-0, surveys-9)
conj_and(surveys-9, grow-11)
prt(grow-11, up-12)
aux(breed-14, to-13)
xcomp(grow-11, breed-14)

