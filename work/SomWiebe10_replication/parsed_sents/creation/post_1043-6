  (S
    (NP (DT This))
    (VP (VBZ is)
      (NP
        (NP (CD one))
        (PP (IN of)
          (NP
            (NP (DT the) (JJ common) (NNS misconceptions))
            (PP (IN about)
              (NP (NN evolution)))))))
    (. .)))

