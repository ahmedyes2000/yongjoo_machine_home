(ROOT
  (S
    (ADVP (RB Yes))
    (NP (PRP I))
    (VP (VBP believe)
      (SBAR
        (S
          (NP
            (NP (DT the) (NN theory))
            (PP (IN of)
              (NP (NN evolution))))
          (VP (VBZ is)
            (ADJP (JJ wrong))
            (SBAR (IN as)
              (S
                (NP (EX there))
                (VP (VBP are)
                  (NP
                    (NP (DT no) (JJ scientific) (NNS facts))
                    (PP (IN about)
                      (NP (DT that)))))))))))
    (. .)))

advmod(believe-3, Yes-1)
nsubj(believe-3, I-2)
root(ROOT-0, believe-3)
det(theory-5, the-4)
nsubj(wrong-9, theory-5)
prep_of(theory-5, evolution-7)
cop(wrong-9, is-8)
ccomp(believe-3, wrong-9)
mark(are-12, as-10)
expl(are-12, there-11)
advcl(wrong-9, are-12)
det(facts-15, no-13)
amod(facts-15, scientific-14)
nsubj(are-12, facts-15)
prep_about(facts-15, that-17)

