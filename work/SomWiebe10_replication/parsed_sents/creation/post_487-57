(ROOT
  (S
    (S
      (NP (PRP It))
      (VP (VBZ is)
        (NP
          (NP (DT no) (NN wonder))
          (SBAR
            (S
              (NP (PRP they))
              (VP (VBP believe)
                (S
                  (NP (NNP ID))
                  (VP (TO to)
                    (VP (VB be)
                      (ADJP (JJ unconstitutional)))))))))))
    (: --)
    (S
      (NP (PRP they))
      (VP (VBP are)
        (VP (VBG using)
          (NP (JJ scientific) (NNS methods))
          (S
            (VP (TO to)
              (VP (VB measure)
                (NP (DT the) (NN constitution))))))))
    (. .)))

nsubj(wonder-4, It-1)
cop(wonder-4, is-2)
det(wonder-4, no-3)
root(ROOT-0, wonder-4)
dobj(believe-6, wonder-4)
nsubj(believe-6, they-5)
rcmod(wonder-4, believe-6)
nsubj(unconstitutional-10, ID-7)
aux(unconstitutional-10, to-8)
cop(unconstitutional-10, be-9)
xcomp(believe-6, unconstitutional-10)
nsubj(using-14, they-12)
aux(using-14, are-13)
parataxis(wonder-4, using-14)
amod(methods-16, scientific-15)
dobj(using-14, methods-16)
aux(measure-18, to-17)
xcomp(using-14, measure-18)
det(constitution-20, the-19)
dobj(measure-18, constitution-20)

