(ROOT
  (S
    (S
      (NP (PRP I))
      (VP (VBP 've)
        (VP (VBN asked)
          (PP (IN for)
            (NP (PRP them)))
          (ADVP (RB repeatedly)))))
    (CC and)
    (S
      (NP (PRP he))
      (VP (VBZ 's)
        (VP (VBG dodging))))
    (. .)))

