(ROOT
  (S (`` ``)
    (PP (IN Because)
      (ADVP (RB nearly)))
    (NP (DT all) (NNS mountains))
    (VP (VBD used)
      (S
        (VP (TO to)
          (VP (VB be)
            (NP (JJ ancient) (NN sea) (NN floor) (NN genius))))))
    (. .)))

prep(used-6, Because-2)
pcomp(Because-2, nearly-3)
det(mountains-5, all-4)
nsubj(used-6, mountains-5)
xsubj(genius-12, mountains-5)
root(ROOT-0, used-6)
aux(genius-12, to-7)
cop(genius-12, be-8)
