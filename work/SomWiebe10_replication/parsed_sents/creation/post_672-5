  (S
    (NP (DT The) (JJ obvious) (NN answer))
    (VP (VBZ is)
      (ADVP (RB no)
        (SBAR (IN because)
          (S
            (NP (PRP it))
            (VP (VBZ explains)
              (FRAG
                (NP (NN everything)))
              (PRN (-LRB- -LRB-)
                (S
                  (NP (DT every) (NN living) (NN thing))
                  (VP (VBZ is)
                    (VP (VBN designed))))
                (. ?) (-RRB- -RRB-)))))))))

