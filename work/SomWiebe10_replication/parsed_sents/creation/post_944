  (FRAG
    (ADJP
      (ADJP (RB Not) (VBN stupid))
      (, ,)
      (CC but)
      (ADJP
        (ADJP (JJ ignorant))
        (CC and)
        (ADJP (RB perhaps) (RB even) (JJ foolish))))
    (. .)))

