(ROOT
  (S
    (NP (NNP Faith))
    (VP (VBZ is)
      (VP (VBG believeing)
        (PP (IN in)
          (NP (NN something)))
        (PP (IN without)
          (NP (NN evidence)))))
    (. .)))

nsubj(believeing-3, Faith-1)
aux(believeing-3, is-2)
root(ROOT-0, believeing-3)
prep_in(believeing-3, something-5)
prep_without(believeing-3, evidence-7)

