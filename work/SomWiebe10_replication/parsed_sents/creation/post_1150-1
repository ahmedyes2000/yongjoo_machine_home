(ROOT
  (FRAG
    (NP (DT This))
    (, ,)
    (PP (IN despite)
      (NP
        (NP (DT the) (NN fact))
        (SBAR (IN that)
          (S
            (NP (RB nearly) (DT the) (NN debate))
            (VP (VBZ is)
              (ADVP (RB actually))
              (ADJP (RB very) (JJ close)
                (PP (IN in)
                  (NP
                    (NP (NNS terms))
                    (PP (IN of)
                      (NP
                        (NP (NNS numbers))
                        (PP (IN on)
                          (NP (DT each) (NN side)))))))))))))
    (. .)))

root(ROOT-0, This-1)
dep(This-1, despite-3)
det(fact-5, the-4)
pobj(despite-3, fact-5)
complm(close-13, that-6)
