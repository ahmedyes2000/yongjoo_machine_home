(ROOT
  (S
    (NP (VBG Teaching) (NN evolution))
    (ADVP (RB exclusively))
    (VP (VBZ is)
      (ADJP (JJ dangerous)
        (PP (TO to)
          (NP (NN science)
            (CC and)
            (NN reason)))))
    (. .)))

amod(evolution-2, Teaching-1)
nsubj(dangerous-5, evolution-2)
advmod(dangerous-5, exclusively-3)
cop(dangerous-5, is-4)
root(ROOT-0, dangerous-5)
prep_to(dangerous-5, science-7)
prep_to(dangerous-5, reason-9)
conj_and(science-7, reason-9)

