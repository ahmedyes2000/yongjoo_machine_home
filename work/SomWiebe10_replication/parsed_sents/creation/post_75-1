(ROOT
  (S
    (NP (NNP Luskin))
    (ADVP (RB however))
    (VP (VBZ continues)
      (NP (DT the)
        (ADJP (RB somewhat) (JJ ignorant))
        (NN statement))
      (SBAR (IN that)
        (S
          (NP (DT this))
          (VP (VBZ shows)
            (SBAR
              (WHADVP (WRB how))
              (S
                (NP (NN junk) (NN dna))
                (VP (VBZ is) (RB not)
                  (ADVP (RB always))
                  (NP (NN junk)))))))))
    (. .)))

nsubj(continues-3, Luskin-1)
advmod(continues-3, however-2)
root(ROOT-0, continues-3)
det(statement-7, the-4)
advmod(ignorant-6, somewhat-5)
amod(statement-7, ignorant-6)
dobj(continues-3, statement-7)
complm(shows-10, that-8)
nsubj(shows-10, this-9)
ccomp(continues-3, shows-10)
advmod(junk-17, how-11)
nn(dna-13, junk-12)
nsubj(junk-17, dna-13)
cop(junk-17, is-14)
neg(junk-17, not-15)
advmod(junk-17, always-16)
ccomp(shows-10, junk-17)

