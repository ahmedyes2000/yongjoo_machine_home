(ROOT
  (S
    (NP (PRP it))
    (VP (VBD was)
      (ADJP (JJ necessary)
        (PP (IN at)
          (NP
            (NP (CD one) (NN point))
            (PP (IN in)
              (NP (NN history))))))
      (PP (IN for)
        (NP (NNP God)))
      (S
        (VP (TO to)
          (VP (VB make)
            (NP
              (NP (PRP himself))
              (ADJP (RBR more) (RB clearly) (VBN known)))))))
    (. .) (. .)))

nsubj(necessary-3, it-1)
xsubj(make-12, it-1)
cop(necessary-3, was-2)
root(ROOT-0, necessary-3)
