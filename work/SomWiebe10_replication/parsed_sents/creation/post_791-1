(ROOT
  (S
    (NP (DT This))
    (VP (VBZ is)
      (NP
        (NP (DT the) (JJ major) (NN reason))
        (SBAR
          (WHADVP (WRB why))
          (S
            (NP (JJS most) (NNS people))
            (VP (VBP question)
              (NP
                (NP (DT the) (NN length))
                (PP (IN of)
                  (NP
                    (NP (DT the) (NNS days))
                    (PP (IN of)
                      (NP (NN creation))))))
              (PP (IN in)
                (NP
                  (NP (DT the) (NN book))
                  (PP (IN of)
                    (NP (NNP Genesis))))))))))
    (. .)))

nsubj(reason-5, This-1)
cop(reason-5, is-2)
det(reason-5, the-3)
amod(reason-5, major-4)
root(ROOT-0, reason-5)
advmod(question-9, why-6)
amod(people-8, most-7)
nsubj(question-9, people-8)
rcmod(reason-5, question-9)
det(length-11, the-10)
dobj(question-9, length-11)
det(days-14, the-13)
prep_of(length-11, days-14)
prep_of(days-14, creation-16)
det(book-19, the-18)
prep_in(question-9, book-19)
prep_of(book-19, Genesis-21)

