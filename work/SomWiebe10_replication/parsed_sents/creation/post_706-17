(ROOT
  (S
    (NP (PRP You))
    (VP (MD ca) (RB n't)
      (ADVP (RB technically))
      (VP (VB have)
        (NP
          (NP (DT a) (`` ``) (JJ full) (NN understanding) ('' ''))
          (PP (IN of)
            (NP (NN anything))))))
    (: ...)))

nsubj(have-5, You-1)
aux(have-5, ca-2)
neg(have-5, n't-3)
advmod(have-5, technically-4)
root(ROOT-0, have-5)
det(understanding-9, a-6)
amod(understanding-9, full-8)
dobj(have-5, understanding-9)
prep_of(understanding-9, anything-12)

