(ROOT
  (S
    (NP (EX There))
    (VP (VBP are)
      (NP
        (NP (VBN documented) (NNS CASES))
        (PP (IN of)
          (NP
            (NP (JJ specific) (NNS species))
            (VP (VBG adapting)
              (PP (IN over)
                (NP (NN time))))))))
    (. .)))

expl(are-2, There-1)
root(ROOT-0, are-2)
amod(CASES-4, documented-3)
nsubj(are-2, CASES-4)
amod(species-7, specific-6)
prep_of(CASES-4, species-7)
partmod(species-7, adapting-8)
prep_over(adapting-8, time-10)

