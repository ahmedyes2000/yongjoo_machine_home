(ROOT
  (S
    (ADVP (RB Sometimes))
    (, ,)
    (NP (NNS people))
    (VP (VBP think)
      (SBAR (IN that)
        (S
          (NP (NN creationism))
          (VP (VBZ is)
            (NP (NN science))))))
    (. .)))

advmod(think-4, Sometimes-1)
nsubj(think-4, people-3)
root(ROOT-0, think-4)
complm(science-8, that-5)
nsubj(science-8, creationism-6)
cop(science-8, is-7)
ccomp(think-4, science-8)

