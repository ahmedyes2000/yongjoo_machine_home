(ROOT
  (S
    (S
      (NP (NNP ID))
      (VP (VBZ is)
        (NP (NN nonsense))))
    (, ,)
    (CC and)
    (S
      (NP (PRP it))
      (VP (VBZ 's)
        (ADJP (RB so) (RB far)
          (PP (IN from)
            (NP (NN science))))
        (SBAR (IN that)
          (S
            (S
              (VP (VBG claiming)
                (SBAR
                  (S
                    (NP (PRP it))
                    (VP (VBZ follows)
                      (NP (JJ scientific) (NN method))
                      (ADVP (RB immediately)))))))
            (VP (VBZ invalidates)
              (NP
                (NP (CD one) (POS 's))
                (NN opinion)))))))
    (. .)))

nsubj(nonsense-3, ID-1)
cop(nonsense-3, is-2)
root(ROOT-0, nonsense-3)
nsubj(far-9, it-6)
cop(far-9, 's-7)
advmod(far-9, so-8)
conj_and(nonsense-3, far-9)
prep_from(far-9, science-11)
complm(invalidates-19, that-12)
csubj(invalidates-19, claiming-13)
nsubj(follows-15, it-14)
ccomp(claiming-13, follows-15)
amod(method-17, scientific-16)
dobj(follows-15, method-17)
advmod(follows-15, immediately-18)
ccomp(far-9, invalidates-19)
poss(opinion-22, one-20)
dobj(invalidates-19, opinion-22)

