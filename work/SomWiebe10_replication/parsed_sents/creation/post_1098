(ROOT
  (S
    (INTJ (UH No))
    (, ,)
    (NP (PRP we))
    (VP (VBD were)
      (VP (VBN put)
        (ADVP (RB here))
        (PP (IN by)
          (NP
            (NP (NNS aliens))
            (PP (IN as)
              (NP (DT an) (JJ alternative) (NN food) (NN source)))))))
    (. .)))

dep(put-5, No-1)
nsubjpass(put-5, we-3)
auxpass(put-5, were-4)
root(ROOT-0, put-5)
advmod(put-5, here-6)
agent(put-5, aliens-8)
det(source-13, an-10)
amod(source-13, alternative-11)
nn(source-13, food-12)
prep_as(aliens-8, source-13)

