(ROOT
  (S (CC And)
    (SBAR (IN if)
      (S
        (NP (PRP you))
        (VP (MD could)
          (VP (VB spell)
            (ADVP (RB properly))))))
    (, ,)
    (NP (DT that))
    (VP (MD would)
      (VP (VB be)
        (ADJP (JJ good))
        (ADVP (RB also))))
    (. .)))

cc(good-11, And-1)
mark(spell-5, if-2)
nsubj(spell-5, you-3)
aux(spell-5, could-4)
advcl(good-11, spell-5)
advmod(spell-5, properly-6)
nsubj(good-11, that-8)
aux(good-11, would-9)
cop(good-11, be-10)
root(ROOT-0, good-11)
advmod(good-11, also-12)

