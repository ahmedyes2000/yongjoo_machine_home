(ROOT
  (S
    (NP (PRP I))
    (VP (MD would)
      (VP (VB be)
        (VP (VBN worried)
          (PP (IN about)
            (NP
              (NP (DT any) (NN idea))
              (SBAR
                (WHADVP (WRB where))
                (S
                  (NP (NNS atheists))
                  (VP (VBP are)
                    (PP (IN in)
                      (NP
                        (NP (NN agreement))
                        (PP (IN with)
                          (NP (JJ young-earth) (NNS creationist)))))))))))))
    (. .)))

nsubjpass(worried-4, I-1)
aux(worried-4, would-2)
auxpass(worried-4, be-3)
root(ROOT-0, worried-4)
det(idea-7, any-6)
prep_about(worried-4, idea-7)
advmod(are-10, where-8)
nsubj(are-10, atheists-9)
rcmod(idea-7, are-10)
prep_in(are-10, agreement-12)
amod(creationist-15, young-earth-14)
prep_with(agreement-12, creationist-15)

