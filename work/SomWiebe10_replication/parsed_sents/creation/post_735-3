(ROOT
  (S
    (NP (NNP Energy))
    (VP (MD can) (RB not)
      (VP (VB be)
        (VP (VBN created)
          (CC or)
          (VBN destroyed))))
    (. .)))

nsubjpass(created-5, Energy-1)
nsubjpass(destroyed-7, Energy-1)
aux(created-5, can-2)
neg(created-5, not-3)
auxpass(created-5, be-4)
root(ROOT-0, created-5)
conj_or(created-5, destroyed-7)

