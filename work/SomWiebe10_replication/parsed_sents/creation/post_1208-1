          (PP (IN of)
            (NP (DT the) (NN matter))))
        (VP (VBZ is)
          (SBAR (IN that)
            (S
              (NP (EX there))
              (VP (VBZ is)
                (NP
                  (NP (JJR more) (NN evidence))
                  (PP (IN than)
                    (NP (PRP you))))))))))
    (VP (MD could)
      (VP (VB examine)
        (PP (IN in)
          (NP
            (NP (JJ several) (NNS lifetimes))
            (PP (IN of)
              (NP (NN study)))))))
    (. .)))

