(ROOT
  (S
    (NP (DT This))
    (ADVP (RB completely))
    (VP (VBZ misrepresents)
      (NP (DT the) (NNP Pancer) (NN paper)))
    (. .)))

nsubj(misrepresents-3, This-1)
advmod(misrepresents-3, completely-2)
root(ROOT-0, misrepresents-3)
det(paper-6, the-4)
nn(paper-6, Pancer-5)
dobj(misrepresents-3, paper-6)

