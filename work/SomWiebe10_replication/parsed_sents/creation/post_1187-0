(ROOT
  (S
    (NP (PRP I))
    (VP (VBP agree)
      (SBAR (IN that)
        (S
          (NP (PRP it))
          (VP (VBZ is)
            (ADVP (RB slowly))
            (VP (VBG falling)
              (ADVP (RB apart)))))))
    (. .)))

nsubj(agree-2, I-1)
root(ROOT-0, agree-2)
complm(falling-7, that-3)
nsubj(falling-7, it-4)
aux(falling-7, is-5)
advmod(falling-7, slowly-6)
ccomp(agree-2, falling-7)
advmod(falling-7, apart-8)

