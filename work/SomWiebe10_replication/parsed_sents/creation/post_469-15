(ROOT
  (S
    (ADVP (RB First))
    (, ,)
    (SBAR (IN if)
      (S
        (NP (PRP he))
        (VP (MD can)
          (VP (VB do)
            (NP (JJ good) (JJ standard) (NN science))))))
    (, ,)
    (NP (DT that))
    (VP (VBZ 's)
      (NP
        (NP (DT all))
        (SBAR
          (S
            (NP (PRP we))
            (VP (VBP care)
              (PRT (RP about)))))))
    (. .)))

advmod(all-13, First-1)
mark(do-6, if-3)
nsubj(do-6, he-4)
aux(do-6, can-5)
advcl(all-13, do-6)
amod(science-9, good-7)
amod(science-9, standard-8)
dobj(do-6, science-9)
nsubj(all-13, that-11)
cop(all-13, 's-12)
root(ROOT-0, all-13)
dobj(care-15, all-13)
nsubj(care-15, we-14)
rcmod(all-13, care-15)
prt(care-15, about-16)

