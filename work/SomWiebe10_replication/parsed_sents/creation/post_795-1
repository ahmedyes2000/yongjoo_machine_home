(ROOT
  (S
    (NP (PRP I))
    (VP (VBP see)
      (NP (PRP it))
      (PP (IN as)
        (NP (DT a) (JJ rational) (NN way)))
      (S
        (VP (TO to)
          (VP (VB understand)
            (NP (DT the) (NN world))))))
    (. .)))

nsubj(see-2, I-1)
xsubj(understand-9, I-1)
root(ROOT-0, see-2)
dobj(see-2, it-3)
det(way-7, a-5)
amod(way-7, rational-6)
prep_as(see-2, way-7)
aux(understand-9, to-8)
xcomp(see-2, understand-9)
det(world-11, the-10)
dobj(understand-9, world-11)

