(ROOT
  (S (-LRB- -LRB-)
    (VP
      (ADVP (RB just))
      (VB make)
      (NP
        (NP (JJ sure))
        (SBAR
          (S
            (NP (NNP Eugenie) (NNP Scott))
            (VP (VBZ is)
              (ADJP (JJ aware)))))))
    (: ...)))

advmod(make-3, just-2)
root(ROOT-0, make-3)
dobj(make-3, sure-4)
nn(Scott-6, Eugenie-5)
nsubj(aware-8, Scott-6)
cop(aware-8, is-7)
rcmod(sure-4, aware-8)

