(ROOT
  (S
    (VP
      (VP (VB Perform)
        (NP (NNS experiments)))
      (, ,)
      (CC or)
      (VP
        (VP (VB find)
          (NP
            (NP (JJ new) (NNS data))
            (SBAR
              (WHNP (WDT that))
              (S
                (VP (VBZ validates)
                  (NP (PRP$ your) (NNS hypothesis)))))))
        (, ,)
        (CC or)
        (VP (VB go)
          (NP (NN home)))))
    (. .)))

root(ROOT-0, Perform-1)
dobj(Perform-1, experiments-2)
conj_or(Perform-1, find-5)
amod(data-7, new-6)
dobj(find-5, data-7)
nsubj(validates-9, data-7)
rcmod(data-7, validates-9)
poss(hypothesis-11, your-10)
dobj(validates-9, hypothesis-11)
conj_or(Perform-1, go-14)
conj_or(find-5, go-14)
dobj(go-14, home-15)

