(ROOT
  (S
    (NP (PRP We))
    (VP (VBP know)
      (SBAR
        (S
          (NP (DT the) (JJ big) (NN bang))
          (VP (VBD happened) (, ,)
            (PP (IN for)
              (NP (WDT whatever) (NN reason)))))))
    (. .)))

nsubj(know-2, We-1)
root(ROOT-0, know-2)
det(bang-5, the-3)
amod(bang-5, big-4)
nsubj(happened-6, bang-5)
ccomp(know-2, happened-6)
dep(reason-10, whatever-9)
prep_for(happened-6, reason-10)

