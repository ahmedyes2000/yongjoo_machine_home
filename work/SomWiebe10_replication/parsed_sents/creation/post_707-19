(ROOT
  (S
    (PP (IN Except)
      (PP (IN for)
        (NP
          (NP (DT the) (NN mountain))
          (PP (IN of)
            (NP
              (NP (NN evidence))
              (PP (IN against)
                (NP (DT a) (JJ literal) (CD 6) (NN day) (NN creation))))))))
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP (VBP want)
          (S
            (VP (TO to)
              (VP (VB say)
                (S
                  (NP (PRP I))
                  (VP (VB believe)
                    (NP
                      (NP (DT that))
                      (PP (IN in)
                        (NP (DT a) (CD six) (NN day) (NN creation))))))))))))
    (ADVP (RB then))
    (NP (PRP I))
    (VP (MD can) (RB not)
      (VP (VB argue)
        (PP (IN with)
          (NP (PRP$ your) (NNS beliefs)))))
    (. .)))

