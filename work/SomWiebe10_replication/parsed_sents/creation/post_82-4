(ROOT
  (S
    (NP
      (NP (NNS People))
      (VP (VBG taking)
        (NP (PRP them))))
    (ADVP (RB literally))
    (VP (VBP miss)
      (NP (DT the) (JJ authors) ('' '') (NN intent)))
    (. .)))

nsubj(miss-5, People-1)
partmod(People-1, taking-2)
dobj(taking-2, them-3)
advmod(miss-5, literally-4)
root(ROOT-0, miss-5)
det(intent-9, the-6)
amod(intent-9, authors-7)
dobj(miss-5, intent-9)

