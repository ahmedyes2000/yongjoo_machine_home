(ROOT
  (S
    (NP (PRP I))
    (VP (VBP think)
      (SBAR
        (S
          (NP (PRP it))
          (VP (VBZ is)
            (ADJP (JJR better))
            (S
              (VP (TO to)
                (VP (VB show)
                  (S
                    (NP (PRP them))
                    (NP
                      (NP (DT a) (JJ different) (NN interpretation))
                      (PP (IN of)
                        (NP (DT the) (JJ same) (NN evidence))))))))
            (SBAR (IN that)
              (S
                (NP (PRP they))
                (VP (VB interpret)
                  (PP (IN through)
                    (NP (PRP$ their) (JJ Darwinian) (NNS glasses))))))))))
    (. .)))

nsubj(think-2, I-1)
root(ROOT-0, think-2)
nsubj(better-5, it-3)
xsubj(show-7, it-3)
cop(better-5, is-4)
ccomp(think-2, better-5)
aux(show-7, to-6)
xcomp(better-5, show-7)
