(ROOT
  (S
    (S
      (VP (TO To)
        (VP (VB punish)
          (NP (PRP him))
          (PP (IN for)
            (NP
              (NP (DT a) (NN crime))
              (SBAR
                (S
                  (NP (PRP he))
                  (VP (VBD committed)
                    (SBAR (IN before)
                      (S
                        (NP (PRP he))
                        (VP (VBD was)
                          (VP (VBN born)))))))))))))
    (VP (VBZ is)
      (NP
        (NP (DT a) (NN mockery))
        (PP (IN of)
          (NP (NN justice)))))
    (. .)))

