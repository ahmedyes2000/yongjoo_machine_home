  (S
    (SBAR (IN That)
      (S
        (SBAR (IN if)
          (S
            (NP (PRP you))
            (VP (VBP look)
              (PP (IN into)
                (NP (DT all) (NNS beliefs)
                  (CC and)
                  (NNS theories))))))
        (, ,)
        (NP (PRP you))
        (VP (MD will)
          (VP (VB find)
            (NP (DT a) (NNS flawThis))))))
    (VP (VBZ is)
      (ADJP (RB very) (JJ true)
        (PP (IN for)
          (NP
            (QP (RB almost) (DT all))
            (NNS theories)))))
    (. .)))

