  (S (CC But)
    (, ,)
    (S
      (NP (NNP let) (POS 's))
      (ADVP (RB just))
      (VP (VBP say)
        (PP (IN for)
          (NP
            (NP (NN sake))
            (PP (IN of)
              (NP (NN argument)))))
        (, ,)
        (SBAR (IN that)
          (S
            (NP (EX there))
            (VP (VBZ is)
              (NP (DT a) (NNP Creator)))))))
    (. .)))

cc(say-6, But-1)
nsubj(say-6, let-3)
possessive(let-3, 's-4)
