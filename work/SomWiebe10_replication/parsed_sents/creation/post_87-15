(ROOT
  (S
    (S
      (VP (VBG Taking)
        (NP (CD two))))
    (VP (VBZ steps)
      (ADVP (RB backward))
      (PP (IN for)
        (NP (DT every) (NN step)))
      (ADVP (RB forward)))
    (. .)))

csubj(steps-3, Taking-1)
dobj(Taking-1, two-2)
root(ROOT-0, steps-3)
advmod(steps-3, backward-4)
det(step-7, every-6)
prep_for(steps-3, step-7)
advmod(steps-3, forward-8)

