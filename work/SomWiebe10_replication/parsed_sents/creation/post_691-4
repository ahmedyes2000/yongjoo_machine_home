(ROOT
  (S
    (PP (IN In)
      (NP (DT the) (JJ other)))
    (, ,)
    (S
      (NP (NN man))
      (VP (VBZ is)
        (VP (VBN created)
          (ADVP (RB far) (RBR earlier)))))
    (CC and)
    (S
      (NP
        (NP (DT the) (NN rest))
        (PP (IN of)
          (NP (NNP Eden))))
      (VP (VBZ is)
        (VP (VBN created)
          (PP (IN for)
            (NP (PRP him))))))
    (. .)))

det(other-3, the-2)
prep_in(created-7, other-3)
nsubjpass(created-7, man-5)
auxpass(created-7, is-6)
root(ROOT-0, created-7)
advmod(earlier-9, far-8)
advmod(created-7, earlier-9)
det(rest-12, the-11)
nsubjpass(created-16, rest-12)
prep_of(rest-12, Eden-14)
auxpass(created-16, is-15)
conj_and(created-7, created-16)
prep_for(created-16, him-18)

