  (S
    (NP (EX There))
    (VP (VBZ is)
      (ADVP (RB actually))
      (ADJP
        (ADVP (RB far) (RBR more))
        (JJ evidence)
        (PRN (, ,)
          (CC and)
          (PP (IN from)
            (NP (JJ many) (JJ different) (NNS fields)))))
      (, ,)
      (PP (IN for)
        (NP
          (NP (DT the) (NNP Theory))
          (PP (IN of)
            (NP (NNP Evolution)))))
      (SBAR (IN than)
        (S
          (NP (EX there))
          (VP (VBP are)
            (PP (IN for)
              (NP
                (NP (DT the) (NNPS Theories))
                (PP (IN of)
                  (NP (NNP Gravity) (, ,) (NNP Relativity) (, ,)
                    (CC or)
                    (NN quantum) (NNS mechanics)))))))))
    (. .)))

