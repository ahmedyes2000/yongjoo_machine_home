(ROOT
  (S
    (PP (IN On)
      (NP (NNP Day) (CD 3)))
    (, ,)
    (S
      (NP (NNP God))
      (VP (VBZ seperates)
        (NP (DT the) (NN water))
        (PP (IN into)
          (NP (CD one) (JJ great) (NN basin)))))
    (, ,)
    (CC and)
    (S
      (NP (JJ dry) (NN land))
      (VP (VBZ appears)))
    (. .)))

prep_on(seperates-6, Day-2)
num(Day-2, 3-3)
nsubj(seperates-6, God-5)
root(ROOT-0, seperates-6)
det(water-8, the-7)
dobj(seperates-6, water-8)
num(basin-12, one-10)
amod(basin-12, great-11)
prep_into(seperates-6, basin-12)
amod(land-16, dry-15)
nsubj(appears-17, land-16)
conj_and(seperates-6, appears-17)

