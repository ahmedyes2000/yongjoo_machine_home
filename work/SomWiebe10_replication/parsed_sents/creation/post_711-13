(ROOT
  (S ('' '')
    (NP (DT The) (NN bible))
    (VP (VBZ is)
      (NP
        (NP (RB just) (CD one))
        (PP (IN of)
          (NP
            (NP (JJ many) (JJ religious) (NNS texts))
            (VP (VBG explaining)
              (NP
                (NP (DT the) (NN history))
                (PP (IN of)
                  (NP (DT the) (NN world)))))))))
    (. .)))

det(bible-3, The-2)
nsubj(one-6, bible-3)
cop(one-6, is-4)
advmod(one-6, just-5)
root(ROOT-0, one-6)
amod(texts-10, many-8)
amod(texts-10, religious-9)
prep_of(one-6, texts-10)
partmod(texts-10, explaining-11)
det(history-13, the-12)
dobj(explaining-11, history-13)
det(world-16, the-15)
prep_of(history-13, world-16)

