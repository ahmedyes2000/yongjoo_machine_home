(ROOT
  (S
    (NP (PRP You))
    (VP (VBP say)
      (SBAR (IN that)
        (S
          (NP (NN evolution))
          (VP (VBZ is)
            (ADJP (JJ observed))))))
    (. .)))

nsubj(say-2, You-1)
root(ROOT-0, say-2)
complm(observed-6, that-3)
nsubj(observed-6, evolution-4)
cop(observed-6, is-5)
ccomp(say-2, observed-6)

