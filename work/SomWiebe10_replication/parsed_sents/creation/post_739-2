(ROOT
  (S
    (SBAR
      (WHADVP (WRB Why))
      (S
        (NP (PRP he))
        (VP (VBD chose)
          (NP (PRP them)))))
    (, ,)
    (NP (NNS Christians))
    (VP (VBP do) (RB n't)
      (VP (VB know)))
    (. .)))

advmod(chose-3, Why-1)
nsubj(chose-3, he-2)
advcl(know-9, chose-3)
dobj(chose-3, them-4)
nsubj(know-9, Christians-6)
aux(know-9, do-7)
neg(know-9, n't-8)
root(ROOT-0, know-9)

