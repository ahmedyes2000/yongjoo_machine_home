(ROOT
  (S
    (ADVP (RB Maybe))
    (NP (DT that))
    (VP (VBZ 's)
      (SBAR
        (WHADVP (WRB why))
        (S
          (NP (NN religion))
          (VP (VBZ is) (RB n't)
            (NP
              (NP (DT a) (NN category))
              (PP (IN on)
                (NP (DT this) (NN site))))))))
    (. .)))

advmod('s-3, Maybe-1)
nsubj('s-3, that-2)
root(ROOT-0, 's-3)
advmod(category-9, why-4)
nsubj(category-9, religion-5)
cop(category-9, is-6)
neg(category-9, n't-7)
det(category-9, a-8)
advcl('s-3, category-9)
det(site-12, this-11)
prep_on(category-9, site-12)

