(ROOT
  (S
    (VP (VB Let)
      (SBAR
        (S
          (NP (PRP 's))
          (VP (VBP hope)
            (SBAR
              (S
                (NP (PRP they))
                (VP (VBP do) (RB n't)
                  (VP (VB hate)
                    (NP (PRP me))
                    (PP (IN for)
                      (S
                        (VP (VBG starting)
                          (NP (DT an) (NNP IDEA) (NN center)))))))))))))
    (: ...)))

root(ROOT-0, Let-1)
nsubj(hope-3, 's-2)
ccomp(Let-1, hope-3)
nsubj(hate-7, they-4)
aux(hate-7, do-5)
neg(hate-7, n't-6)
ccomp(hope-3, hate-7)
dobj(hate-7, me-8)
prepc_for(hate-7, starting-10)
det(center-13, an-11)
nn(center-13, IDEA-12)
dobj(starting-10, center-13)

