(ROOT
  (S
    (NP (WDT Which))
    (VP (VBZ is)
      (ADVP (RB pretty) (RB much))
      (NP
        (NP (DT the) (JJ exact) (JJ same) (NN thing))
        (PP (IN as)
          (NP
            (NP (JJ critical) (NN analysis))
            (PP (IN of)
              (NP (NN evolution)))))))
    (. .)))

nsubj(thing-8, Which-1)
cop(thing-8, is-2)
advmod(much-4, pretty-3)
advmod(thing-8, much-4)
det(thing-8, the-5)
amod(thing-8, exact-6)
amod(thing-8, same-7)
root(ROOT-0, thing-8)
amod(analysis-11, critical-10)
prep_as(thing-8, analysis-11)
prep_of(analysis-11, evolution-13)

