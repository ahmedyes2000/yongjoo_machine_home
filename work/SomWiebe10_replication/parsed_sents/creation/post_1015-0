(ROOT
  (S
    (PP (TO To)
      (NP (PRP me)))
    (NP (DT this))
    (VP (VBZ is)
      (NP
        (NP (DT a) (JJ simple) (NN one))
        (VP (VBG Evolution)
          (NP (PDT all) (DT the) (NN way)))))
    (. .)))

prep_to(one-7, me-2)
nsubj(one-7, this-3)
cop(one-7, is-4)
det(one-7, a-5)
amod(one-7, simple-6)
root(ROOT-0, one-7)
