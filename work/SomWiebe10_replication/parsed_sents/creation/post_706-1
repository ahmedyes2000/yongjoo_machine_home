(ROOT
  (S
    (ADVP (JJ first) (RP off))
    (, ,)
    (NP (NNS words))
    (VP (VBP are)
      (NP
        (NP (NNS metaphors))
        (SBAR
          (S
            (NP (PRP they))
            (VP (VBP are)
              (NP
                (NP (NNS symbols))
                (PP (IN of)
                  (NP (DT an) (NN object)))))
            (. .)))))
    (. .)))

advmod(metaphors-6, first-1)
dep(first-1, off-2)
nsubj(metaphors-6, words-4)
cop(metaphors-6, are-5)
root(ROOT-0, metaphors-6)
nsubj(symbols-9, they-7)
cop(symbols-9, are-8)
rcmod(metaphors-6, symbols-9)
det(object-12, an-11)
prep_of(symbols-9, object-12)

