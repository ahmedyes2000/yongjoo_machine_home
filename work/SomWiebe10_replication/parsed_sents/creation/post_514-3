(ROOT
  (S (-LRB- -LRB-)
    (VP (VB Do) (RB n't)
      (VP (VB say)
        (NP
          (NP (NN space) (NNS aliens)
            (CC or)
            (NN time) (NNS travelers))
          (SBAR
            (S
              (NP (PRP you))
              (ADVP (RB only))
              (VP (VBP make)
                (SBAR
                  (S
                    (NP (PRP yourself))
                    (VP (VBP look)
                      (ADJP (JJ foolish))
                      (PP (IN with)
                        (NP (JJ such) (NNS claims))))))))))))
    (. .) (-RRB- -RRB-)))

aux(say-4, Do-2)
neg(say-4, n't-3)
root(ROOT-0, say-4)
nn(aliens-6, space-5)
dobj(say-4, aliens-6)
nn(travelers-9, time-8)
dobj(say-4, travelers-9)
conj_or(aliens-6, travelers-9)
nsubj(make-12, you-10)
advmod(make-12, only-11)
rcmod(aliens-6, make-12)
nsubj(look-14, yourself-13)
ccomp(make-12, look-14)
acomp(look-14, foolish-15)
amod(claims-18, such-17)
