(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is)
      (ADJP (JJ possible)
        (S
          (VP (TO to)
            (VP (VB teach)
              (NP
                (NP (DT both) (NN creationism)
                  (CC and)
                  (NN evolution))
                (PP (IN in)
                  (NP (DT the) (JJ same) (NN school)))))))))
    (. .)))

nsubj(possible-3, It-1)
cop(possible-3, is-2)
root(ROOT-0, possible-3)
aux(teach-5, to-4)
xcomp(possible-3, teach-5)
preconj(creationism-7, both-6)
dobj(teach-5, creationism-7)
dobj(teach-5, evolution-9)
