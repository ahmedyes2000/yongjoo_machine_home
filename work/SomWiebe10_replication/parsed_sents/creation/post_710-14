(ROOT
  (S (CC And)
    (NP (DT the) (JJ fossil) (NN record))
    (VP (VBZ tells)
      (NP (PRP us))
      (SBAR (IN that)
        (S
          (NP (EX there))
          (VP (VBZ s)
            (VP (VBN been)
              (NP (DT a) (JJ tremendous) (NN change)))))))
    (. .)))

cc(tells-5, And-1)
det(record-4, the-2)
amod(record-4, fossil-3)
nsubj(tells-5, record-4)
root(ROOT-0, tells-5)
