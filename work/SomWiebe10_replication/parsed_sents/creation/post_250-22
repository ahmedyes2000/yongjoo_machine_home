  (S
    (NP
      (NP (DT The) (NN question))
      (PP (IN at)
        (NP (NN hand))))
    (VP (VBZ is)
      (SBAR (IN whether)
        (CC or)
        (RB not)
        (S
          (NP
            (NP (NNP Darwin) (POS 's))
            (NN mechanism))
          (VP (MD can)
            (VP (VB produce)
              (NP
