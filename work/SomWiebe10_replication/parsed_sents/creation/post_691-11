(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (NNP God))
        (VP (VBZ speaks)
          (PP (TO to)
            (NP (PRP us))))))
    (, ,)
    (S
      (ADVP (RB then))
      (NP (PRP he))
      (VP (MD must)
        (VP (VB use)
          (NP
            (NP (DT the) (NNS words))
            (SBAR
              (S
                (NP (PRP we))
                (VP (MD can)
                  (VP (VB understand)))))))))
    (CC or)
    (S
      (NP (PRP we))
      (VP (MD can) (RB not)
        (VP (VB be)
          (VP (VBN expected)
            (S
              (VP (TO to)
                (VP (VB heed)
                  (NP (PRP them)))))))))
    (. .)))

mark(speaks-3, If-1)
nsubj(speaks-3, God-2)
advcl(use-10, speaks-3)
prep_to(speaks-3, us-5)
