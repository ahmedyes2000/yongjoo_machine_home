(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ 's)
      (NP
        (NP (DT the) (JJ religious) (NN motivation))
        (SBAR
          (WHNP (WDT which))
          (S
            (VP (VBZ has)
              (VP (VBN caused)
                (S
                  (NP (NNP ID))
                  (VP (TO to)
                    (VP (VB remain)
                      (NP (JJ scientifically) (NNS vacuous)))))))))))
    (. .)))

nsubj(motivation-5, It-1)
cop(motivation-5, 's-2)
det(motivation-5, the-3)
amod(motivation-5, religious-4)
root(ROOT-0, motivation-5)
nsubj(caused-8, motivation-5)
aux(caused-8, has-7)
rcmod(motivation-5, caused-8)
nsubj(vacuous-13, ID-9)
aux(vacuous-13, to-10)
cop(vacuous-13, remain-11)
amod(vacuous-13, scientifically-12)
xcomp(caused-8, vacuous-13)

