  (S
    (NP
      (NP (CD 99) (NN %))
      (PP (IN of)
        (NP
          (NP (DT the) (NN species))
          (CC and)
          (NP
            (NP (CD 50) (NN %))
            (PP (IN of)
              (NP
                (NP (DT the) (NNS biomass))
                (PP (IN on)
                  (NP (DT this) (NN planet)))))))))
    (VP (VBP are)
      (ADJP (JJ prokaryotes)))
    (. .)))

