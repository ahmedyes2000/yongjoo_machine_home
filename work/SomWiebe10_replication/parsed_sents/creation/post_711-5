(ROOT
  (S
    (NP (DT The) (JJ fossil) (NN record))
    (VP (VBZ is)
      (ADJP (RB very) (JJ clear))
      (SBAR (IN that)
        (S
          (NP (DT these) (NNS animals))
          (ADVP (DT all))
          (VP
            (VP (VBD died)
              (ADVP (RB suddenly)))
            (CC and)
            (VP (VBD were)
              (VP (VBN covered)
                (PP (IN with)
                  (NP
                    (NP (NNS deposits))
                    (PP (IN of)
                      (NP (NN mud)))))
                (ADVP (RB instantly))))))))
    (. .)))

det(record-3, The-1)
amod(record-3, fossil-2)
nsubj(clear-6, record-3)
cop(clear-6, is-4)
advmod(clear-6, very-5)
root(ROOT-0, clear-6)
complm(died-11, that-7)
det(animals-9, these-8)
nsubj(died-11, animals-9)
nsubjpass(covered-15, animals-9)
advmod(died-11, all-10)
ccomp(clear-6, died-11)
advmod(died-11, suddenly-12)
auxpass(covered-15, were-14)
ccomp(clear-6, covered-15)
conj_and(died-11, covered-15)
prep_with(covered-15, deposits-17)
prep_of(deposits-17, mud-19)
advmod(covered-15, instantly-20)

