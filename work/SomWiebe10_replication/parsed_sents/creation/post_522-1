(ROOT
  (S
    (NP (DT No) (JJ real) (NN scientist))
    (VP (MD would)
      (VP (VB waste)
        (ADVP
          (NP (RB even) (CD 30) (NNS seconds))
          (RB there))))
    (. .)))

det(scientist-3, No-1)
amod(scientist-3, real-2)
nsubj(waste-5, scientist-3)
aux(waste-5, would-4)
root(ROOT-0, waste-5)
advmod(seconds-8, even-6)
num(seconds-8, 30-7)
npadvmod(there-9, seconds-8)
advmod(waste-5, there-9)

