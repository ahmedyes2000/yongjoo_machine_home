  (S
    (SBAR
      (ADVP (RB Well))
      (IN once)
      (S
        (NP (PRP you))
        (VP (VBP accept)
          (NP
            (NP (DT the) (NN existence))
            (PP (IN of)
              (NP
                (NP (DT an)
                  (ADJP (RB all) (JJ powerful))
                  (NN deity))
                (ADVP (RB then))))))))
    (NP (NN anything))
    (VP (VBZ is)
      (ADJP (JJ possible)))
    (. ...)))

