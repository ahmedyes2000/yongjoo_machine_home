(ROOT
  (S
    (PP (IN If)
      (ADVP (RB so)))
    (, ,)
    (NP (NNP let) (POS 's))
    (VP (VBP dismiss)
      (NP
        (NP (PDT all) (DT the) (NN evidence))
        (SBAR
          (WHNP (WDT that))
          (S
            (VP (VBZ lends)
              (VP (VB support)
                (PP (TO to)
                  (NP
                    (NP (DT the) (JJ religious) (NNS views))
                    (PP (IN of)
                      (NP (DT the) (NNPS Darwinists)))))
                (ADVP (RB as) (RB well))))))))
    (. .)))

prep(dismiss-6, If-1)
pcomp(If-1, so-2)
nsubj(dismiss-6, let-4)
possessive(let-4, 's-5)
root(ROOT-0, dismiss-6)
predet(evidence-9, all-7)
det(evidence-9, the-8)
dobj(dismiss-6, evidence-9)
nsubj(lends-11, evidence-9)
rcmod(evidence-9, lends-11)
dep(lends-11, support-12)
det(views-16, the-14)
amod(views-16, religious-15)
prep_to(support-12, views-16)
det(Darwinists-19, the-18)
prep_of(views-16, Darwinists-19)
advmod(well-21, as-20)
mwe(well-21, as-20)
advmod(support-12, well-21)

