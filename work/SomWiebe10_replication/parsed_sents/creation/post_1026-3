    (NP (NNP ID))
    (VP (VBZ does) (RB n't)
      (VP (VB say)
        (NP
          (NP
            (NP (NNP God))
            (ADVP (RB directly)))
          (PRN (-LRB- -LRB-)
            (VP (VBN anymore) (, ,)
              (SBAR (IN unless)
                (S
                  (NP (PRP you))
                  (VP (VBP are)
                    (NP
                      (NP (DT a) (NN creationist))
                      (SBAR
                        (WHNP (WP who))
                        (S
                          (VP (VBZ believes)
                            (PP (IN in)
                              (NP (NNP ID)))))))))))
            (-RRB- -RRB-)))))
    (. .)))

