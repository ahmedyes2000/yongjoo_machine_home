(ROOT
  (S
    (NP (PRP It))
    (VP (MD can)
      (ADVP (RB never))
      (VP (VB reveal)
        (NP (DT the) (NN why))
        (ADVP (RB either))))
    (. .)))

nsubj(reveal-4, It-1)
aux(reveal-4, can-2)
neg(reveal-4, never-3)
root(ROOT-0, reveal-4)
det(why-6, the-5)
dobj(reveal-4, why-6)
advmod(reveal-4, either-7)

