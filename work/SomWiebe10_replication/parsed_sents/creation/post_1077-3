(ROOT
  (S
    (PP (IN In)
      (NP (JJR earlier) (NNS times)))
    (, ,)
    (NP (PRP it))
    (VP (VBD was)
      (ADJP (JJ enough)
        (S
          (VP (TO to)
            (VP (VB keep)
              (NP
                (NP (NNS adults))
                (VP (VBN entertained)
                  (CC and)
                  (VBN bedazzled))))))))
    (. .)))

amod(times-3, earlier-2)
prep_in(enough-7, times-3)
nsubj(enough-7, it-5)
cop(enough-7, was-6)
root(ROOT-0, enough-7)
aux(keep-9, to-8)
xcomp(enough-7, keep-9)
dobj(keep-9, adults-10)
partmod(adults-10, entertained-11)
partmod(adults-10, bedazzled-13)
conj_and(entertained-11, bedazzled-13)

