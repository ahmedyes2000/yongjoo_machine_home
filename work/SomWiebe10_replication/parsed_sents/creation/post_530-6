  (S
    (PP (IN After)
      (NP (DT all)))
    (, ,)
    (NP (DT the) (NN heart))
    (VP (VBD was)
      (VP
        (ADVP (RB historically))
        (VBN seen)
        (PP (IN as)
          (NP
            (NP (DT a) (NN pump))
            (, ,)
            (NP (JJ leading) (NN science))))
        (S
          (VP (TO to)
            (VP (VB proceed)
              (PP (IN in)
                (NP (DT a) (JJ fruitful) (NN area))))))))
    (. .)))

prep_after(seen-8, all-2)
det(heart-5, the-4)
nsubjpass(seen-8, heart-5)
xsubj(proceed-16, heart-5)
