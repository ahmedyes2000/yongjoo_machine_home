(ROOT
  (S
    (S
      (NP (PRP They))
      (VP (VBD succeeded)))
    (CC and)
    (S
      (NP (PRP they))
      (VP (VBD were) (RB not)
        (VP (VBN punished)
          (PP (IN for)
            (NP
              (NP (DT this))
              (ADVP (IN at) (DT all)))))))
    (. .)))

nsubj(succeeded-2, They-1)
root(ROOT-0, succeeded-2)
nsubjpass(punished-7, they-4)
auxpass(punished-7, were-5)
neg(punished-7, not-6)
conj_and(succeeded-2, punished-7)
prep_for(punished-7, this-9)
advmod(this-9, at-10)
pobj(at-10, all-11)

