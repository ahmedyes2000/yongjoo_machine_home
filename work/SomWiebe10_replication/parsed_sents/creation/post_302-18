(ROOT
  (S ('' '')
    (NP (PRP I))
    (VP (VBP think)
      (SBAR
        (S
          (NP (PRP he))
          (ADVP (RB already))
          (VP (VBD took)
            (NP
              (NP (NN care))
              (PP (IN of)
                (NP (DT that) (NN one))))))))
    (. .)))

nsubj(think-3, I-2)
root(ROOT-0, think-3)
nsubj(took-6, he-4)
advmod(took-6, already-5)
ccomp(think-3, took-6)
dobj(took-6, care-7)
det(one-10, that-9)
prep_of(care-7, one-10)

