(ROOT
  (S ('' '')
    (S
      (SBAR
        (WHADVP (WRB When))
        (S
          (NP (NN evidence))
          (VP (VBD was)
            (VP (VBN produced)
              (PP (TO to)
                (NP (DT the) (NN contrary)))))))
      (, ,)
      (NP (DT that) (NN evolution))
      (VP (MD can)
        (VP (VB occur)
          (ADVP (RB suddenly)))))
    (, ,)
    (NP (DT the) (NN theory))
    (VP (VBD was)
      (VP (VBN adjusted)
        (S
          (VP (TO to)
            (VP (VB reflect)
              (NP (DT that) (NN information)))))))
    (. .)))

advmod(produced-5, When-2)
nsubjpass(produced-5, evidence-3)
auxpass(produced-5, was-4)
advcl(occur-13, produced-5)
det(contrary-8, the-7)
prep_to(produced-5, contrary-8)
det(evolution-11, that-10)
nsubj(occur-13, evolution-11)
aux(occur-13, can-12)
ccomp(adjusted-19, occur-13)
advmod(occur-13, suddenly-14)
det(theory-17, the-16)
nsubjpass(adjusted-19, theory-17)
xsubj(reflect-21, theory-17)
auxpass(adjusted-19, was-18)
root(ROOT-0, adjusted-19)
aux(reflect-21, to-20)
xcomp(adjusted-19, reflect-21)
det(information-23, that-22)
dobj(reflect-21, information-23)

