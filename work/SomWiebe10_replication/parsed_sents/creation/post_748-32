(ROOT
  (S
    (NP (NNP and))
    (VP (VBD wrote)
      (NP
        (NP (DT a) (NN book))
        (PP (IN of)
          (SBAR
            (WHADVP (WRB how))
            (S
              (NP (NNS people))
              (VP (MD should)
                (VP (VB live))))))
        (: ...)
        (PP (IN from)
          (NP (PRP$ his) (JJ deep) (NN understanding)))))
    (. .)))

nsubj(wrote-2, and-1)
root(ROOT-0, wrote-2)
det(book-4, a-3)
dobj(wrote-2, book-4)
advmod(live-9, how-6)
nsubj(live-9, people-7)
aux(live-9, should-8)
prepc_of(book-4, live-9)
poss(understanding-14, his-12)
amod(understanding-14, deep-13)
prep_from(book-4, understanding-14)

