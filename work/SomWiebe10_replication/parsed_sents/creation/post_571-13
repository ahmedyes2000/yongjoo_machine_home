(ROOT
  (S ('' '')
    (NP (DT That))
    (VP (VBZ 's)
      (VP (VBN taken)
        (NP
          (NP (NN care))
          (PP (IN of)))
        (, ,)
        (SBAR (RB now)
          (FRAG
            (WHNP (WP what))))))
    (. .)))

nsubjpass(taken-4, That-2)
auxpass(taken-4, 's-3)
root(ROOT-0, taken-4)
dobj(taken-4, care-5)
prep(care-5, of-6)
advmod(what-9, now-8)
dep(taken-4, what-9)

