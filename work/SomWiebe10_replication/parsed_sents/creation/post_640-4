(ROOT
  (S ('' '')
    (S (CC But)
      (NP (PRP they))
      (VP (VBD were) (RB n't)
        (NP (NNS predecessors))))
    (, ,)
    (NP (PRP you))
    (VP (VBP know)
      (SBAR (IN that)
        (S
          (NP
            (NP
              (QP (RB just) (RB as) (JJ much)))
            (PP (IN as)
              (NP (PRP I))))
          (VP (VBP do)))))
    (. .)))

cc(predecessors-6, But-2)
nsubj(predecessors-6, they-3)
cop(predecessors-6, were-4)
neg(predecessors-6, n't-5)
ccomp(know-9, predecessors-6)
nsubj(know-9, you-8)
root(ROOT-0, know-9)
complm(do-16, that-10)
quantmod(much-13, just-11)
quantmod(much-13, as-12)
nsubj(do-16, much-13)
prep_as(much-13, I-15)
ccomp(know-9, do-16)

