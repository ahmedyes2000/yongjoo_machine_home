(ROOT
  (S
    (ADVP
      (NP (RB Just) (DT a) (JJ few) (NNS weeks))
      (RB ago))
    (NP (PRP they))
    (VP
      (VP (VBD found)
        (NP (DT the) (JJS best) (JJ transitional) (NN fossil))
        (ADVP (RB yet)))
      (, ,)
      (NP
        (NP (DT a) (NN creature))
        (PP (IN with)
          (NP (DT both) (NN reptile)
            (CC and)
            (NN amphibian) (NNS features)))))
    (. .)))

advmod(weeks-4, Just-1)
det(weeks-4, a-2)
amod(weeks-4, few-3)
npadvmod(ago-5, weeks-4)
advmod(found-7, ago-5)
nsubj(found-7, they-6)
root(ROOT-0, found-7)
det(fossil-11, the-8)
amod(fossil-11, best-9)
amod(fossil-11, transitional-10)
dobj(found-7, fossil-11)
advmod(found-7, yet-12)
det(creature-15, a-14)
dobj(found-7, creature-15)
preconj(features-21, both-17)
nn(features-21, reptile-18)
conj_and(reptile-18, amphibian-20)
nn(features-21, amphibian-20)
prep_with(creature-15, features-21)

