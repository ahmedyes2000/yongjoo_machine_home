  (S
    (ADVP (RB As) (RB far)
      (SBAR (IN as)
        (S
          (NP (JJ scientific) (NNS findings))
          (VP (VBP go)
            (NP
              (NP (DT the) (NNS numbers))
              (PP (IN of)
                (NP (VBG evolutionbelieving) (NN scientist))))))))
    (NP (PRP 's))
    (VP (VBZ is)
      (PP (IN on)
        (NP (DT a) (JJ major) (NN decline))))
    (. .)))

