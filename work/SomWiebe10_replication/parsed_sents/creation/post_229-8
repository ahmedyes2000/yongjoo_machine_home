(ROOT
  (S ('' '')
    (NP (NNP Ape) (NNS feet))
    (VP (VBP are)
      (ADJP (JJ prehensile))
      (, ,)
      (PP (IN with)
        (NP
          (NP (NNS thumbs))
          (CC and)
          (NP (JJ great) (NN dexterity)))))
    (. .)))

nn(feet-3, Ape-2)
nsubj(prehensile-5, feet-3)
cop(prehensile-5, are-4)
root(ROOT-0, prehensile-5)
prep_with(prehensile-5, thumbs-8)
amod(dexterity-11, great-10)
prep_with(prehensile-5, dexterity-11)
conj_and(thumbs-8, dexterity-11)

