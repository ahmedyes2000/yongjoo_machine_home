(ROOT
  (S
    (NP (PRP They))
    (VP (VBP believe)
      (SBAR
        (S
          (NP (DT the) (NN world))
          (VP (VBZ is)
            (NP (DT a)
              (ADJP
                (NP
                  (QP (JJ little) (IN over) (CD 6,000))
                  (NNS years))
                (JJ old))
              (NN !!))))))))

nsubj(believe-2, They-1)
root(ROOT-0, believe-2)
det(world-4, the-3)
nsubj(!!-12, world-4)
cop(!!-12, is-5)
det(!!-12, a-6)
advmod(over-8, little-7)
quantmod(6,000-9, over-8)
num(years-10, 6,000-9)
npadvmod(old-11, years-10)
amod(!!-12, old-11)
ccomp(believe-2, !!-12)

