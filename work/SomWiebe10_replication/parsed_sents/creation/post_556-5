(ROOT
  (S
    (NP (PRP You))
    (VP (MD might)
      (VP (VB be)
        (VP (VBG fishing)
          (PP (IN in)
            (NP (DT a) (NN pond)))
          (SBAR
            (WHADVP (WRB where))
            (S
              (NP (EX there))
              (VP (VBZ is)
                (NP (DT no) (NN fish))))))))
    (. .)))

nsubj(fishing-4, You-1)
aux(fishing-4, might-2)
aux(fishing-4, be-3)
root(ROOT-0, fishing-4)
det(pond-7, a-6)
prep_in(fishing-4, pond-7)
advmod(is-10, where-8)
expl(is-10, there-9)
advcl(fishing-4, is-10)
det(fish-12, no-11)
nsubj(is-10, fish-12)

