(ROOT
  (S
    (NP (PRP You))
    (VP (VBP are)
      (VP (VBG ignoring)
        (NP (PRP$ my) (JJ main) (NN point))
        (SBAR (IN that)
          (S
            (NP (NNP Catastrophism))
            (VP (VBD was)
              (ADJP
                (ADJP (JJR closer))
                (PP (TO to)
                  (NP (DT the) (NN truth))))
              (PP (RB then)
                (NP (NNP Darwin))))))))
    (. .)))

