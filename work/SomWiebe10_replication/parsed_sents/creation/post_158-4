(ROOT
  (S
    (NP (DT Both) (NNS claims))
    (VP (VBP are)
      (ADVP (RB scientifically)
        (ADJP (JJ testable)))
      (VP (VBG using)
        (NP
          (NP (JJ scientific) (NNS methods))
          (VP (VBN employed)
            (PP (IN by)
              (NP (JJ standard) (JJ historical) (NNS sciences)))))))
    (. .)))

det(claims-2, Both-1)
nsubj(using-6, claims-2)
aux(using-6, are-3)
advmod(using-6, scientifically-4)
pobj(scientifically-4, testable-5)
root(ROOT-0, using-6)
amod(methods-8, scientific-7)
dobj(using-6, methods-8)
partmod(methods-8, employed-9)
amod(sciences-13, standard-11)
amod(sciences-13, historical-12)
agent(employed-9, sciences-13)

