(ROOT
  (FRAG
    (ADVP (RB Then))
    (SBAR
      (WHADVP (WRB how))
      (S
        (VP (TO to)
          (VP (VB explain)
            (NP (DT the) (JJS closest) (NN star))
            (, ,)
            (PP (IN in)
              (NP (JJ light) (NNS years)))))))
    (. .)))

root(ROOT-0, Then-1)
advmod(explain-4, how-2)
aux(explain-4, to-3)
dep(Then-1, explain-4)
det(star-7, the-5)
amod(star-7, closest-6)
dobj(explain-4, star-7)
amod(years-11, light-10)
prep_in(explain-4, years-11)

