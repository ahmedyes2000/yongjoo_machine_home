(ROOT
  (S
    (NP
      (NP (NNP Think))
      (PP (IN of)
        (NP (NN life))))
    (VP
      (ADVP (IN as) (DT a))
      (VBP choose)
      (NP (PRP$ your) (JJ own) (NN adventure) (NN book)))
    (. .)))

nsubj(choose-6, Think-1)
prep_of(Think-1, life-3)
advmod(choose-6, as-4)
dep(as-4, a-5)
root(ROOT-0, choose-6)
poss(book-10, your-7)
amod(book-10, own-8)
nn(book-10, adventure-9)
dobj(choose-6, book-10)

