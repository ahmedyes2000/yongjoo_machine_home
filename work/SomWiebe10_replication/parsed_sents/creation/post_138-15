(ROOT
  (S
    (S
      (VP (VB Ask)
        (NP (DT any) (NN doctor))))
    (CC and)
    (S
      (NP (PRP they))
      (VP (MD will)
        (VP (VB tell)
          (NP (PRP you))
          (SBAR (IN that)
            (S
              (NP (DT the) (JJ human) (NN body))
              (VP (VBZ is)
                (NP (DT a) (JJ remarkable) (NN machine))))))))
    (. .)))

