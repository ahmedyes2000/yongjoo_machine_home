(ROOT
  (S ('' '')
    (S
      (NP
        (NP (: ...)
          (NP (IN to))
          (PP (VB try)
            (SBAR (IN to)
              (S
                (VP (VBP allow)
                  (NP (NNS creationists)))))))
        (PP (TO to)))
      (VP (VBP claim)))
    (IN that)
    (S
      (NP (NN evolution))
      (VP (MD might)
        (VP (VB be)
          (ADJP (JJ wrong)))))
    (. .)))

nsubj(claim-9, to-3)
prep(to-3, try-4)
mark(allow-6, to-5)
pcomp(try-4, allow-6)
dobj(allow-6, creationists-7)
prep(to-3, to-8)
root(ROOT-0, claim-9)
dep(claim-9, that-10)
nsubj(wrong-14, evolution-11)
aux(wrong-14, might-12)
cop(wrong-14, be-13)
ccomp(claim-9, wrong-14)

