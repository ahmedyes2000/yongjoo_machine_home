(ROOT
  (S
    (NP (DT This))
    (VP (VBZ is)
      (VP (VBG saying)
        (SBAR (IN that)
          (S
            (NP
              (NP (DT the) (CD 100) (NNS years))
              (PP (IN of)
                (NP (DT a) (JJ human) (NN life))))
            (VP (VBZ is)
              (PP (IN like)
                (NP
                  (NP (DT a) (NN second))
                  (PP (TO to)
                    (NP (NNP God))))))))))
    (. .)))

nsubj(saying-3, This-1)
aux(saying-3, is-2)
root(ROOT-0, saying-3)
complm(is-12, that-4)
det(years-7, the-5)
num(years-7, 100-6)
nsubj(is-12, years-7)
det(life-11, a-9)
amod(life-11, human-10)
prep_of(years-7, life-11)
ccomp(saying-3, is-12)
det(second-15, a-14)
prep_like(is-12, second-15)
