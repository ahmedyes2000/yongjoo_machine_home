(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP (VBP do) (RB n't)
          (VP (VB accept)
            (NP (DT the) (JJ first))))))
    (NP (PRP I))
    (VP (VBP am)
      (ADJP (JJ powerless)
        (S
          (VP (TO to)
            (VP (VB change)
              (NP (PRP$ your) (NN denial)))))))
    (. .)))

mark(accept-5, If-1)
nsubj(accept-5, you-2)
aux(accept-5, do-3)
neg(accept-5, n't-4)
