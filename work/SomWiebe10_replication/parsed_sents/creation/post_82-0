(ROOT
  (S
    (NP (NNP Science))
    (VP (VBZ is)
      (PP (IN about)
        (SBAR
          (WHADVP (WRB how))
          (S
            (NP (NNS things))
            (VP (VBP work))))))
    (. .)))

nsubj(is-2, Science-1)
root(ROOT-0, is-2)
advmod(work-6, how-4)
nsubj(work-6, things-5)
prepc_about(is-2, work-6)

