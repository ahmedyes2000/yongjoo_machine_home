(ROOT
  (S
    (NP (DT That))
    (VP (VBZ is)
      (SBAR
        (WHADVP (WRB how))
        (S
          (NP (PRP$ your) (NN comparison))
          (VP (VBZ fails)))))
    (. .)))

nsubj(is-2, That-1)
root(ROOT-0, is-2)
advmod(fails-6, how-3)
poss(comparison-5, your-4)
nsubj(fails-6, comparison-5)
ccomp(is-2, fails-6)

