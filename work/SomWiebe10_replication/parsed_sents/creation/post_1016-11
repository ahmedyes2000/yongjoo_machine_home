(ROOT
  (S ('' '')
    (S
      (NP
        (NP (DT The) (NN origin))
        (PP (IN of)
          (NP (NN life))))
      (VP (VBZ is)
        (NP
          (NP (DT a) (JJ necessary) (NN precursor))
          (PP (IN for)
            (NP (JJ biological) (NN evolution))))))
    (, ,)
    (CC but)
    (S
      (S
        (VP (VBG understanding)
          (NP (DT that) (NN evolution))))
      (VP (VBD occurred)
        (SBAR (IN once)
          (S
            (NP (NNS organisms))
            (VP (VBD appeared))))))
    (. ...)))

det(origin-3, The-2)
nsubj(precursor-9, origin-3)
prep_of(origin-3, life-5)
cop(precursor-9, is-6)
det(precursor-9, a-7)
amod(precursor-9, necessary-8)
root(ROOT-0, precursor-9)
amod(evolution-12, biological-11)
prep_for(precursor-9, evolution-12)
csubj(occurred-18, understanding-15)
det(evolution-17, that-16)
dobj(understanding-15, evolution-17)
conj_but(precursor-9, occurred-18)
mark(appeared-21, once-19)
nsubj(appeared-21, organisms-20)
advcl(occurred-18, appeared-21)

