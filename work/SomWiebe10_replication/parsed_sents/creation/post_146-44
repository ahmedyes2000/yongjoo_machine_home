(ROOT
  (S
    (S
      (NP (PRP$ their) (JJ perfect) (NN creator))
      (VP (VBP ahhh)))
    (: ...)
    (CC but)
    (S
      (NP (EX there))
      (VP (VBZ 's)
        (NP (DT a) (NN problem)))
      (. .))
    (. .)))

poss(creator-3, their-1)
amod(creator-3, perfect-2)
nsubj(ahhh-4, creator-3)
root(ROOT-0, ahhh-4)
expl('s-8, there-7)
conj_but(ahhh-4, 's-8)
det(problem-10, a-9)
nsubj('s-8, problem-10)

