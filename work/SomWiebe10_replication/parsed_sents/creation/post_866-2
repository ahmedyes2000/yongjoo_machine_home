(ROOT
  (S
    (S
      (NP (DT This))
      (VP (VBZ is)
        (ADJP (JJ confusing))
        (S
          (VP (VBG considering)
            (NP (NN land))))))
    (CC and)
    (S
      (NP (NNP vegetation))
      (VP (VBD was)
        (VP (VBN made)
          (PP (IN on)
            (NP
              (NP (NN day) (CD 3))
              (CC and)
              (NP (NN man))))
          (PP (IN on)
            (NP (NN day) (CD 6))))))
    (. .)))

nsubj(confusing-3, This-1)
cop(confusing-3, is-2)
root(ROOT-0, confusing-3)
xcomp(confusing-3, considering-4)
dobj(considering-4, land-5)
nsubjpass(made-9, vegetation-7)
auxpass(made-9, was-8)
conj_and(confusing-3, made-9)
prep_on(made-9, day-11)
num(day-11, 3-12)
prep_on(made-9, man-14)
conj_and(day-11, man-14)
prep_on(made-9, day-16)
num(day-16, 6-17)

