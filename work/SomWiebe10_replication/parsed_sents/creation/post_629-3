(ROOT
  (S
    (NP (NNP Nature))
    (VP (VBZ picks)
      (NP (VBG living) (NNS things))
      (S
        (VP (TO to)
          (VP (VB fill)
            (NP (JJ ecological) (NNS niches))))))
    (. ?)))

nsubj(picks-2, Nature-1)
root(ROOT-0, picks-2)
amod(things-4, living-3)
dobj(picks-2, things-4)
aux(fill-6, to-5)
xcomp(picks-2, fill-6)
amod(niches-8, ecological-7)
dobj(fill-6, niches-8)

