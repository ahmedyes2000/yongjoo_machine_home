(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP it))
        (VP (MD can) (RB not))))
    (, ,)
    (ADVP (RB then))
    (NP (PRP it))
    (VP (VBZ is)
      (ADJP (JJ weak)))
    (. .)))

mark(can-3, If-1)
nsubj(can-3, it-2)
advcl(weak-9, can-3)
neg(can-3, not-4)
advmod(weak-9, then-6)
nsubj(weak-9, it-7)
cop(weak-9, is-8)
root(ROOT-0, weak-9)

