(ROOT
  (S
    (PP (IN As)
      (NP (DT an) (NN archaeologist)))
    (NP (PRP I))
    (VP (VBP have)
      (S
        (VP (TO to)
          (VP (VB say)
            (SBAR (IN that)
              (S
                (NP (NN creationism))
                (VP (VBZ is)
                  (ADJP (JJ wrong)))))))))
    (. .)))

det(archaeologist-3, an-2)
prep_as(have-5, archaeologist-3)
nsubj(have-5, I-4)
xsubj(say-7, I-4)
root(ROOT-0, have-5)
aux(say-7, to-6)
xcomp(have-5, say-7)
complm(wrong-11, that-8)
nsubj(wrong-11, creationism-9)
cop(wrong-11, is-10)
ccomp(say-7, wrong-11)

