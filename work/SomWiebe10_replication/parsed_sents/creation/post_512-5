(ROOT
  (S
    (NP
      (NP (DT The) (NN government) (POS 's))
      (NN action))
    (VP (MD must) (RB not)
      (VP (VB have)
        (NP
          (NP (DT the) (JJ primary) (NN effect))
          (PP (IN of)
            (NP (DT either) (VBG advancing)
              (CC or)
              (VBG inhibiting) (NN religion)))
          (: ;)
          (NP (CD 3)))))
    (. .)))

det(government-2, The-1)
poss(action-4, government-2)
nsubj(have-7, action-4)
aux(have-7, must-5)
neg(have-7, not-6)
root(ROOT-0, have-7)
det(effect-10, the-8)
amod(effect-10, primary-9)
dobj(have-7, effect-10)
preconj(religion-16, either-12)
nn(religion-16, advancing-13)
conj_or(advancing-13, inhibiting-15)
nn(religion-16, inhibiting-15)
prep_of(effect-10, religion-16)
dep(effect-10, 3-18)

