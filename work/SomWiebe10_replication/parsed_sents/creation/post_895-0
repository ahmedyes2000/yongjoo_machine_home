(ROOT
  (S
    (PP (IN In)
      (NP (NNP Genesis)))
    (, ,)
    (NP (PRP we))
    (VP (VBP have)
      (NP (DT no) (NN reason)
        (S
          (VP (TO to)
            (VP (VB believe)
              (SBAR (IN that)
                (S (`` ``)
                  (NP (CD one) (NN day))
                  ('' '')
                  (VP (VBD meant)
                    (ADVP (DT any) (RBR more))
                    (PP (IN than) (`` ``)
                      (NP (CD one) (NN day))
                      ('' ''))
                    (NP (NN today))))))))))
    (. .)))

prep_in(have-5, Genesis-2)
nsubj(have-5, we-4)
root(ROOT-0, have-5)
det(reason-7, no-6)
