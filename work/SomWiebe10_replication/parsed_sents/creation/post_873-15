  (S
    (ADJP (VB No.))
    (. .)))

root(ROOT-0, No.-1)

(ROOT
  (S
    (NP
      (QP (CD Six) (CD billion))
      (NNS YEARS))
    (VP (MD would)
      (VP (VB be)
        (ADJP
          (ADJP (JJR closer))
          (, ,)
          (CC but)
          (ADJP (JJ only)
            (PP (IN about)
              (NP (NN half)))))))
    (. .)))

