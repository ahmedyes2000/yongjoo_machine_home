(ROOT
  (S
    (NP (PRP You))
    (VP (MD would)
      (VP (VB need)
        (S
          (VP (TO to)
            (VP (VB live)
              (S
                (VP (TO to)
                  (VP (VB do)
                    (ADVP (RB so))))))))))
    (: ...)))

nsubj(need-3, You-1)
xsubj(live-5, You-1)
aux(need-3, would-2)
root(ROOT-0, need-3)
aux(live-5, to-4)
xcomp(need-3, live-5)
aux(do-7, to-6)
xcomp(live-5, do-7)
advmod(do-7, so-8)

