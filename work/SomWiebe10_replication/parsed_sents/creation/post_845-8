(ROOT
  (S
    (ADVP (RB Furthermore))
    (, ,)
    (NP (PRP they))
    (VP (MD could)
      (ADVP (RB precisely))
      (VP (VB measure)
        (NP
          (NP (DT the) (NN rate))
          (SBAR
            (WHPP (IN at)
              (WHNP (WDT which)))
            (S
              (NP (NN time))
              (VP (VBZ passes)
                (ADVP (RB here))
                (PP (IN by)
                  (S
                    (VP (VBG running)
                      (NP (NN light))
                      (PP (IN from)
                        (NP (PRP$ our) (NN star)))
                      (PP (IN through)
                        (NP (DT a) (NN spectrometer))))))))))))
    (. .)))

advmod(measure-6, Furthermore-1)
nsubj(measure-6, they-3)
aux(measure-6, could-4)
advmod(measure-6, precisely-5)
root(ROOT-0, measure-6)
det(rate-8, the-7)
dobj(measure-6, rate-8)
rel(passes-12, which-10)
nsubj(passes-12, time-11)
rcmod(rate-8, passes-12)
advmod(passes-12, here-13)
prepc_by(passes-12, running-15)
dobj(running-15, light-16)
poss(star-19, our-18)
prep_from(running-15, star-19)
det(spectrometer-22, a-21)
prep_through(running-15, spectrometer-22)

