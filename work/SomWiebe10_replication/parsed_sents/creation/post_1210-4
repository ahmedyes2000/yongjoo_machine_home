(ROOT
  (NP (FW .) (. .)))


(ROOT
  (S
    (NP
      (NP (DT the) (JJ average) (NN height))
      (PP (IN of)
        (NP
          (NP (DT a) (JJ grown) (NN man))
          (PP (IN in)
            (NP (CD 1800))))))
    (VP (VBD was)
      (ADJP
        (NP (DT a) (RB lot))
        (JJR shorter)
        (PP (IN than)
          (NP
            (NP (DT that))
            (PP (IN of)
              (NP (DT a) (NN man))))))
      (NP (NN today)))
    (. .)))

det(height-3, the-1)
amod(height-3, average-2)
nsubj(shorter-13, height-3)
det(man-7, a-5)
