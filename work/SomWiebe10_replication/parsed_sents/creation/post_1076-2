(ROOT
  (S
    (NP (DT That))
    (VP (VBZ is)
      (ADJP
        (ADJP (JJ laughably))
        (CC and)
        (ADJP (RB provably) (JJ false))))
    (. .)))

nsubj(laughably-3, That-1)
nsubj(false-6, That-1)
cop(laughably-3, is-2)
root(ROOT-0, laughably-3)
advmod(false-6, provably-5)
conj_and(laughably-3, false-6)

