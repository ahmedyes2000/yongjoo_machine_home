(ROOT
  (S
    (NP (PRP$ My) (NN logic))
    (VP (VBZ is)
      (SBAR
        (S
          (NP (EX there))
          (VP (VBZ is)
            (NP (DT no) (JJ good) (NN reason)
              (S
                (VP (TO to) (RB not)
                  (VP (VB believe)
                    (PP (IN in)
                      (NP (CD 6) (NNS days)))
                    (SBAR (IN as)
                      (S
                        (NP (DT the) (NN bible))
                        (VP (VBZ says))))))))))))
    (. .)))

poss(logic-2, My-1)
nsubj(is-3, logic-2)
root(ROOT-0, is-3)
expl(is-5, there-4)
ccomp(is-3, is-5)
det(reason-8, no-6)
amod(reason-8, good-7)
nsubj(is-5, reason-8)
aux(believe-11, to-9)
neg(believe-11, not-10)
infmod(reason-8, believe-11)
num(days-14, 6-13)
prep_in(believe-11, days-14)
mark(says-18, as-15)
det(bible-17, the-16)
nsubj(says-18, bible-17)
