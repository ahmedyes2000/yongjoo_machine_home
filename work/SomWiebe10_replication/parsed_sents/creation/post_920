(ROOT
  (S
    (NP (NN Everyone))
    (VP (MD should)
      (VP (VB be)
        (ADJP (JJ willing)
          (S
            (VP (TO to)
              (VP (VB question)
                (SBAR
                  (WHNP (WP what))
                  (S
                    (NP (PRP they))
                    (VP (VBP believe))))))))))
    (. .)))

