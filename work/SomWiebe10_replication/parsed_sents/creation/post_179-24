(ROOT
  (S ('' '')
    (NP (RB Yet) (PRP it))
    (VP (VBD demonstrated)
      (NP (DT a) (JJ radical) (NN change))
      (PP (IN over)
        (NP
          (NP (DT the)
            (ADJP (RB comparatively) (JJ short))
            (NN span))
          (PP (IN of)
            (NP (CD twenty) (NNS years))))))
    (. .)))

advmod(it-3, Yet-2)
nsubj(demonstrated-4, it-3)
root(ROOT-0, demonstrated-4)
det(change-7, a-5)
amod(change-7, radical-6)
dobj(demonstrated-4, change-7)
det(span-12, the-9)
advmod(short-11, comparatively-10)
amod(span-12, short-11)
prep_over(demonstrated-4, span-12)
num(years-15, twenty-14)
prep_of(span-12, years-15)

