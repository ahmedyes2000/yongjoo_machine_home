(ROOT
  (S
    (NP (DT that) (NN reply))
    (VP (VBD was)
      (PP (TO to)
        (NP (NN allogic))))
    (: ...)))

det(reply-2, that-1)
nsubj(was-3, reply-2)
root(ROOT-0, was-3)
prep_to(was-3, allogic-5)

(ROOT
  (S
    (NP (PRP I))
    (ADVP (RB clearly))
    (VP (MD should)
      (VP (VB have)
        (VP (VBN clarified))))
    (. .)))

nsubj(clarified-5, I-1)
advmod(clarified-5, clearly-2)
aux(clarified-5, should-3)
aux(clarified-5, have-4)
root(ROOT-0, clarified-5)

