(ROOT
  (S
    (S
      (NP (PRP You))
      (VP (VBP believe)
        (PP (IN in)
          (NP (NNP Biblical) (NNP Genesis)))))
    (, ,)
    (CC but)
    (S
      (NP (PRP I))
      (VP (VBP 've)
        (ADVP (RB yet))
        (S
          (VP (TO to)
            (VP (VB see)
              (NP (DT any) (NN evidence)
                (S
                  (VP
                    (VP (TO to)
                      (VP (VB support)
                        (NP (PRP it))))
                    (: ...)
                    (CC or)
                    (VP (TO to)
                      (VP (VB give)
                        (NP (PRP it))
                        (NP (JJR more) (NN credence))
                        (PP (RB then)
                          (NP (DT the) (JJ Egyptian) (, ,) (JJ Native) (NNP American)))
                        (, ,)
                        (CC or)
                        (NP (JJ Viking) (NN creation) (NNS stories))))))))))))
    (. .)))

nsubj(believe-2, You-1)
root(ROOT-0, believe-2)
nn(Genesis-5, Biblical-4)
prep_in(believe-2, Genesis-5)
nsubj('ve-9, I-8)
xsubj(see-12, I-8)
conj_but(believe-2, 've-9)
advmod('ve-9, yet-10)
aux(see-12, to-11)
xcomp('ve-9, see-12)
det(evidence-14, any-13)
dobj(see-12, evidence-14)
aux(support-16, to-15)
dep(evidence-14, support-16)
dobj(support-16, it-17)
aux(give-21, to-20)
dep(evidence-14, give-21)
conj_or(support-16, give-21)
iobj(give-21, it-22)
amod(credence-24, more-23)
dobj(give-21, credence-24)
prep(give-21, then-25)
det(American-30, the-26)
amod(American-30, Egyptian-27)
amod(American-30, Native-29)
pobj(then-25, American-30)
amod(stories-35, Viking-33)
nn(stories-35, creation-34)
