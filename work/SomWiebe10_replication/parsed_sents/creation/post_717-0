(ROOT
  (S
    (NP (PRP You))
    (VP (VBP are)
      (ADJP (RB exactly) (JJ right)
        (SBAR
          (S
            (NP (NN evidence))
            (VP (VBZ is) (RB all)
              (PP (IN around)
                (NP (PRP you)))
              (, ,)
              (NP
                (NP (NNS mountains))
                (PP (IN of)
                  (NP (PRP it)))
                (PP (IN in)
                  (NP (NN fact)))))))))
    (. .)))

nsubj(right-4, You-1)
cop(right-4, are-2)
advmod(right-4, exactly-3)
root(ROOT-0, right-4)
nsubj(mountains-11, evidence-5)
cop(mountains-11, is-6)
advmod(mountains-11, all-7)
prep_around(mountains-11, you-9)
dep(right-4, mountains-11)
prep_of(mountains-11, it-13)
prep_in(mountains-11, fact-15)

