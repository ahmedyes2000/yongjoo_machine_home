(ROOT
  (S
    (NP (DT The) (JJ evangelical) (NNS Christians))
    (VP (VBP believe)
      (PP (IN in)
        (NP
          (NP (DT the) (JJ literal) (NNS translations))
          (PP (IN of)
            (NP (DT the) (NNP Bible)))))
      (SBAR (IN that)
        (S
          (NP (NNP God))
          (VP (VBD made)
            (NP
              (NP (DT the) (NNS heavens)
                (CC and)
                (NN earth))
              (PP (IN in)
                (NP (CD six) (NNS days))))))))
    (. .)))

det(Christians-3, The-1)
amod(Christians-3, evangelical-2)
nsubj(believe-4, Christians-3)
root(ROOT-0, believe-4)
det(translations-8, the-6)
amod(translations-8, literal-7)
prep_in(believe-4, translations-8)
det(Bible-11, the-10)
prep_of(translations-8, Bible-11)
complm(made-14, that-12)
nsubj(made-14, God-13)
ccomp(believe-4, made-14)
det(heavens-16, the-15)
dobj(made-14, heavens-16)
dobj(made-14, earth-18)
conj_and(heavens-16, earth-18)
num(days-21, six-20)
prep_in(heavens-16, days-21)

