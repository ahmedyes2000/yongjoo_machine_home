  (NP
    (NP (DT The) (JJ first) (NN day))
    (, ,)
    (NP (DT the) (JJ second) (NN day))
    (, ,)
    (NP (DT the) (JJ third) (NN day))
    (, ,)
    (CC and)
    (RB so)
    (NP (NNP forth))
    (. .)))

