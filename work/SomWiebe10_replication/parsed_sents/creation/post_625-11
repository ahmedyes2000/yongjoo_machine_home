(ROOT
  (S ('' '')
    (ADVP (RB Science))
    (, ,)
    (NP (PRP it))
    (VP (VBZ appears)
      (PP (IN as)
        (NP (JJ many) (NNS definitions))))
    (. .)))

advmod(appears-5, Science-2)
nsubj(appears-5, it-4)
root(ROOT-0, appears-5)
amod(definitions-8, many-7)
prep_as(appears-5, definitions-8)

