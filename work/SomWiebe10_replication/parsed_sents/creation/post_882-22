(ROOT
  (S
    (S
      (NP (PRP It))
      (VP (VBZ is)
        (ADJP (RB almost) (JJ certain))
        (SBAR (IN that)
          (S
            (NP (NNP Mars))
            (VP (VBD had)
              (NP
                (NP (JJ liquid) (NN water))
                (PP (IN at)
                  (NP (DT some) (NN point)))))))))
    (, ,) (IN so)
    (S
      (NP (PRP it))
      (VP (VBD was)
        (NP
          (NP (DT the) (JJ right) (NN temperature))
          (PP (IN for)
            (NP (NN life)))
          (S
            (VP (TO to)
              (VP (VB exist)))))))
    (. .)))

