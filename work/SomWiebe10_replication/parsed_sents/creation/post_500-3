(ROOT
  (S
    (NP (NNP Peter) (NNP Irons))
    (VP (VBZ explains)
      (NP (PRP it))
      (ADVP (RB quite) (RB well))
      (PP (IN in)
        (NP (PRP$ his) (NN paper))))
    (. .)))

nn(Irons-2, Peter-1)
nsubj(explains-3, Irons-2)
root(ROOT-0, explains-3)
dobj(explains-3, it-4)
advmod(well-6, quite-5)
advmod(explains-3, well-6)
poss(paper-9, his-8)
prep_in(explains-3, paper-9)

