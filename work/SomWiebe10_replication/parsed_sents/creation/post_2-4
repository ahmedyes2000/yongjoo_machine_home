(ROOT
  (S
    (NP
      (NP (DT The) (NN development))
      (PP (IN of)
        (NP
          (NP (JJ new) (NN information))
          (PP (IN via)
            (NP (JJ random) (NN mutation))))))
    (VP (VBZ has) (RB not)
      (VP (VBN been)
        (VP (VBN observed))))
    (. .)))

det(development-2, The-1)
nsubjpass(observed-12, development-2)
amod(information-5, new-4)
prep_of(development-2, information-5)
amod(mutation-8, random-7)
prep_via(information-5, mutation-8)
aux(observed-12, has-9)
neg(observed-12, not-10)
auxpass(observed-12, been-11)
root(ROOT-0, observed-12)

