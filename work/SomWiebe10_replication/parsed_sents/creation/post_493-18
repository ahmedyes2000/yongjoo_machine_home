(ROOT
  (NP
    (NP
      (NP (DT A) (JJ fascinating) (NN reading))
      (ADVP (RB indeed))
      (SBAR
        (WHNP (WDT which))
        (S
          (VP (VBZ shows)))))
    (NP
      (NP (DT a)
        (ADJP (RB well) (VBN informed))
        (NN judge))
      (VP (VBG making)
        (NP
          (NP (DT a) (JJ powerful) (NN ruling))
          (SBAR
            (WHNP (WDT which))
            (S
              (VP (VBP ID)
                (NP (NNS proponents))))))))
    (. .)))

det(reading-3, A-1)
amod(reading-3, fascinating-2)
root(ROOT-0, reading-3)
advmod(reading-3, indeed-4)
nsubj(shows-6, which-5)
rcmod(reading-3, shows-6)
det(judge-10, a-7)
advmod(informed-9, well-8)
amod(judge-10, informed-9)
dep(reading-3, judge-10)
partmod(judge-10, making-11)
det(ruling-14, a-12)
amod(ruling-14, powerful-13)
dobj(making-11, ruling-14)
nsubj(ID-16, ruling-14)
rcmod(ruling-14, ID-16)
dobj(ID-16, proponents-17)

