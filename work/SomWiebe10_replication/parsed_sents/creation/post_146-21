(ROOT
  (S
    (NP (PRP I))
    (VP (MD 'd)
      (VP (VB say)
        (SBAR (IN that)
          (S
            (VP (MD would) (RB n't)
              (VP (VB be)
                (ADJP (JJ right))))))))
    (. .) (. .)))

nsubj(say-3, I-1)
aux(say-3, 'd-2)
root(ROOT-0, say-3)
complm(right-8, that-4)
aux(right-8, would-5)
neg(right-8, n't-6)
cop(right-8, be-7)
ccomp(say-3, right-8)

