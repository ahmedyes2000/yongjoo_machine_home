(ROOT
  (S
    (NP (PRP I))
    (VP (VBP 'm)
      (ADJP (VBN surprised)
        (SBAR
          (S
            (NP (PRP you))
            (VP (VBD did) (RB n't)
              (VP (VB mention)
                (NP (NNP Thoreau))))))))
    (. .)))

nsubjpass(surprised-3, I-1)
auxpass(surprised-3, 'm-2)
root(ROOT-0, surprised-3)
nsubj(mention-7, you-4)
aux(mention-7, did-5)
neg(mention-7, n't-6)
dep(surprised-3, mention-7)
dobj(mention-7, Thoreau-8)

