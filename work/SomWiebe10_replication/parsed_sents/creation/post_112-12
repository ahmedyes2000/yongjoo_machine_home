(ROOT
  (S
    (S
      (NP (JJ Many) (NNS things))
      (VP (MD would)
        (VP (VB leave)
          (NP (NNS signs)))))
    (: ...)
    (SINV
      (ADVP (RB often))
      (ADJP (JJ creationist))
      (VP (VBZ 's)
        (NP (NN view)))
      (NP
        (NP (DT the) (JJ big) (NN bang))
        (PP (IN with)
          (NP (VBN mixed) (NNS feelings))))
      (. .))
    (. .)))

amod(things-2, Many-1)
nsubj(leave-4, things-2)
aux(leave-4, would-3)
root(ROOT-0, leave-4)
dobj(leave-4, signs-5)
advmod(view-10, often-7)
dep(view-10, creationist-8)
cop(view-10, 's-9)
parataxis(leave-4, view-10)
det(bang-13, the-11)
amod(bang-13, big-12)
nsubj(view-10, bang-13)
amod(feelings-16, mixed-15)
prep_with(bang-13, feelings-16)

