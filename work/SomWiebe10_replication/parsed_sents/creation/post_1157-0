(ROOT
  (S
    (NP (PRP I))
    (VP (VBP think)
      (SBAR
        (S
          (NP (PRP you))
          (VP (MD will)
            (VP (VB find)
              (SBAR (IN that)
                (S
                  (NP (EX there))
                  (VP (VBZ is)
                    (NP
                      (NP (DT a) (JJ vast) (NN amount))
                      (PP (IN of)
                        (NP
                          (NP (NN evidence))
                          (PP (IN for)
                            (NP (NN evolution))))))
                    (SBAR (IN if)
                      (S
                        (NP (PRP you))
                        (VP (MD would)
                          (VP (VB care)
                            (S
                              (VP (TO to)
                                (VP (VB look))))))))))))))))
    (. .)))

nsubj(think-2, I-1)
root(ROOT-0, think-2)
nsubj(find-5, you-3)
aux(find-5, will-4)
ccomp(think-2, find-5)
complm(is-8, that-6)
expl(is-8, there-7)
ccomp(find-5, is-8)
det(amount-11, a-9)
amod(amount-11, vast-10)
nsubj(is-8, amount-11)
prep_of(amount-11, evidence-13)
prep_for(evidence-13, evolution-15)
mark(care-19, if-16)
