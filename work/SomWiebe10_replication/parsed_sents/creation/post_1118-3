(ROOT
  (S
    (NP (JJ Many) (NNS scientists))
    (VP (VBP have)
      (VP
        (ADVP (RB also))
        (VBD confessed)
        (NP (PRP it))))
    (. .)))

amod(scientists-2, Many-1)
nsubj(confessed-5, scientists-2)
aux(confessed-5, have-3)
advmod(confessed-5, also-4)
root(ROOT-0, confessed-5)
dobj(confessed-5, it-6)

