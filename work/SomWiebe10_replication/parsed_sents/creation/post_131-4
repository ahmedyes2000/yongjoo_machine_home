(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP they))
        (VP (VBP are) (RB not))))
    (, ,) (RB then)
    (NP (NNP ID))
    (ADVP (RB just))
    (VP (VBZ posits)
      (NP
        (NP (NNP God))
        (PP (IN at)
          (NP
            (NP (DT some) (NN degree))
            (PP (IN of)
              (NP (NN separation)))))))
    (. .)))

mark(are-3, If-1)
nsubj(are-3, they-2)
advcl(posits-9, are-3)
neg(are-3, not-4)
advmod(posits-9, then-6)
nsubj(posits-9, ID-7)
advmod(posits-9, just-8)
root(ROOT-0, posits-9)
dobj(posits-9, God-10)
det(degree-13, some-12)
prep_at(God-10, degree-13)
prep_of(degree-13, separation-15)

