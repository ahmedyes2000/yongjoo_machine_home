(ROOT
  (S ('' '')
    (NP (PRP We))
    (VP (VBP focus)
      (ADVP (RB only))
      (PP (IN on)
        (NP (DT the) (JJ constitutional) (NNS aspects)))
      (PP (TO to)
        (NP (DT this) (NN issue))))
    (. .)))

nsubj(focus-3, We-2)
root(ROOT-0, focus-3)
advmod(focus-3, only-4)
det(aspects-8, the-6)
amod(aspects-8, constitutional-7)
prep_on(focus-3, aspects-8)
det(issue-11, this-10)
prep_to(focus-3, issue-11)

