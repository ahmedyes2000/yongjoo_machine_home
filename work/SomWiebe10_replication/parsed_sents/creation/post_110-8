(ROOT
  (S
    (ADVP (RB since))
    (NP (PRP it))
    (VP (VBZ is)
      (ADJP (RB divinely) (JJ inspired)))
    (. .)))

advmod(inspired-5, since-1)
nsubj(inspired-5, it-2)
cop(inspired-5, is-3)
advmod(inspired-5, divinely-4)
root(ROOT-0, inspired-5)

(ROOT
  (S (RB Then)
    (NP (PRP it))
    (VP (MD can)
      (VP (VB be)
        (VP (VBN used)
          (S
            (VP (TO to)
              (VP (VB make)
                (NP
                  (NP
                    (ADJP (JJ accurate))
                    (NNS predictions))
                  (PP (IN of)
                    (NP (JJ physical) (NNS phenomena)))))))
          (, ,)
          (SBAR (IN because)
            (S
              (NP (PRP it))
              (VP (VBZ is)
                (ADJP (JJ correct)
                  (PP (IN in)
                    (NP (DT every) (NN detail))))))))))
    (. .)))

advmod(used-5, Then-1)
nsubjpass(used-5, it-2)
aux(used-5, can-3)
auxpass(used-5, be-4)
root(ROOT-0, used-5)
aux(make-7, to-6)
xcomp(used-5, make-7)
amod(predictions-9, accurate-8)
dobj(make-7, predictions-9)
amod(phenomena-12, physical-11)
prep_of(predictions-9, phenomena-12)
mark(correct-17, because-14)
nsubj(correct-17, it-15)
cop(correct-17, is-16)
advcl(used-5, correct-17)
det(detail-20, every-19)
prep_in(correct-17, detail-20)

