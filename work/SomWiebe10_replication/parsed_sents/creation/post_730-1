(ROOT
  (S
    (ADVP (RB PEOPLE))
    (, ,)
    (NP (NNP Evolution))
    (VP (VBZ is)
      (NP (DT a) (NN fact) (, ,) (NN science) (NNS measures) (NNS facts)))
    (. .)))

advmod(facts-10, PEOPLE-1)
nsubj(facts-10, Evolution-3)
cop(facts-10, is-4)
det(facts-10, a-5)
nn(facts-10, fact-6)
dep(facts-10, science-8)
nn(facts-10, measures-9)
root(ROOT-0, facts-10)

