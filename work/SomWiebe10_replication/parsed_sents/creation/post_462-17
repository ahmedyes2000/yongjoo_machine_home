(ROOT
  (S
    (NP (PRP They))
    (VP (VBD wanted)
      (S
        (VP (TO to)
          (VP (VB make)
            (S
              (NP (PRP him))
              (VP (VB suffer)
                (S
                  (VP (TO to)
                    (VP (VB scare)
                      (NP (NNS others))
                      (PP (IN from)
                        (S
                          (VP (VBG doing)
                            (NP (DT the) (JJ same) (NN thing))))))))))))))
    (. .)))

nsubj(wanted-2, They-1)
xsubj(make-4, They-1)
root(ROOT-0, wanted-2)
aux(make-4, to-3)
xcomp(wanted-2, make-4)
nsubj(suffer-6, him-5)
xsubj(scare-8, him-5)
ccomp(make-4, suffer-6)
aux(scare-8, to-7)
xcomp(suffer-6, scare-8)
dobj(scare-8, others-9)
prepc_from(scare-8, doing-11)
det(thing-14, the-12)
amod(thing-14, same-13)
dobj(doing-11, thing-14)

