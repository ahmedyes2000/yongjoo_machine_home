(ROOT
  (S
    (NP (RB Here))
    (VP (VBZ is)
      (NP
        (NP (DT a) (NN video))
        (PP (IN of)
          (S
            (NP (PRP him))
            (VP (VBG proving)
              (NP (DT the) (NN argument)
                (S
                  (VP (TO to)
                    (VP (VB be)
                      (ADJP (JJ invalid)))))))))))
    (. .)))

