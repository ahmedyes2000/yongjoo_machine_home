(ROOT
  (S
    (NP (NNP THEY))
    (ADVP (RB just))
    (VP (VBP are) (RB not)
      (ADJP (JJ arbitrary)
        (NP
          (NP (NNS issues))
          (ADVP (IN off)
            (NP (DT the) (NN cuff)))))
      (, ,)
      (S (RB so)
        (VP (TO to)
          (VP (VB speak)))))
    (: ...)))

nsubj(arbitrary-5, THEY-1)
xsubj(speak-13, THEY-1)
advmod(arbitrary-5, just-2)
cop(arbitrary-5, are-3)
neg(arbitrary-5, not-4)
root(ROOT-0, arbitrary-5)
dep(arbitrary-5, issues-6)
advmod(issues-6, off-7)
det(cuff-9, the-8)
pobj(off-7, cuff-9)
advmod(speak-13, so-11)
aux(speak-13, to-12)
xcomp(arbitrary-5, speak-13)

