(ROOT
  (S
    (PP (IN because)
      (NP
        (NP (DT all))
        (SBAR
          (WHNP (WP who))
          (S
            (VP (VBP seek))))))
    (NP (PRP him))
    (VP (MD will)
      (VP (VB find)
        (NP (PRP him))
        (SBAR (IN if)
          (S
            (NP (PRP they))
            (VP (VBP do)
              (ADJP (RB so) (JJ earnestly)))))))
    (. .)))

nsubj(seek-4, all-2)
prep_because(find-7, all-2)
rcmod(all-2, seek-4)
nsubj(find-7, him-5)
aux(find-7, will-6)
root(ROOT-0, find-7)
dobj(find-7, him-8)
mark(earnestly-13, if-9)
nsubj(earnestly-13, they-10)
dep(earnestly-13, do-11)
advmod(earnestly-13, so-12)
advcl(find-7, earnestly-13)

