(ROOT
  (S
    (PP (IN In)
      (ADJP (JJ short)))
    (, ,)
    (NP (EX there))
    (VP (VBZ is)
      (NP
        (NP (DT NO) (NN evidence))
        (PP (IN for)
          (NP (NNP Intelligent) (NNP Design)))))
    (. .)))

prep_in(is-5, short-2)
expl(is-5, there-4)
root(ROOT-0, is-5)
det(evidence-7, NO-6)
nsubj(is-5, evidence-7)
nn(Design-10, Intelligent-9)
prep_for(evidence-7, Design-10)

