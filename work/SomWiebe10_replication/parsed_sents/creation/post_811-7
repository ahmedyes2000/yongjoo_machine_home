  (S ('' '')
    (S
      (ADVP (RB Also))
      (NP
        (NP (CD one))
        (PP (IN of)
          (NP (DT the) (NNS consistencies))))
      (VP (MD would)
        (ADVP (RB naturally))
        (VP (VB be))))
    (, ,)
    (VP (VBP inconsistency))
    (. .)))

