(ROOT
  (S
    (NP (DT Some))
    (VP (VBP save)
      (SBAR
        (S
          (NP (PRP it))
          (VP (VBD was)
            (NP (CD 6000) (NNS years))))))
    (. .)))

nsubj(save-2, Some-1)
root(ROOT-0, save-2)
nsubj(years-6, it-3)
cop(years-6, was-4)
num(years-6, 6000-5)
ccomp(save-2, years-6)

