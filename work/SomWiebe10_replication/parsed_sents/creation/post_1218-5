(ROOT
  (INTJ (UH .)))


(ROOT
  (S
    (NP (DT that))
    (VP (VBZ 's)
      (SBAR
        (WHADVP (WRB where))
        (S
          (NP (PRP you))
          (VP (VBP 're)
            (NP
              (NP (NN evidence))
              (SBAR
                (S
                  (VP (MD will)
                    (VP (VB be)
                      (VP (VBN found)))))))))))
    (: ...)))

nsubj('s-2, that-1)
root(ROOT-0, 's-2)
advmod(evidence-6, where-3)
nsubj(evidence-6, you-4)
cop(evidence-6, 're-5)
advcl('s-2, evidence-6)
aux(found-9, will-7)
auxpass(found-9, be-8)
rcmod(evidence-6, found-9)

