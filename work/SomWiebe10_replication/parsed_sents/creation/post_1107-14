(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP (VBP have)
          (NP
            (NP (NN faith))
            (PP (IN in)
              (NP (NN something)))))))
    (, ,)
    (NP (PRP you))
    (VP (VBP do) (RB n't)
      (VP (VB need)
        (NP
          (NP (DT an) (NN explanation))
          (PP (IN of)
            (NP
              (NP (PRP it) (POS 's))
              (NN existence))))))
    (. .)))

mark(have-3, If-1)
nsubj(have-3, you-2)
advcl(need-11, have-3)
dobj(have-3, faith-4)
prep_in(faith-4, something-6)
nsubj(need-11, you-8)
aux(need-11, do-9)
neg(need-11, n't-10)
root(ROOT-0, need-11)
det(explanation-13, an-12)
dobj(need-11, explanation-13)
poss(existence-17, it-15)
prep_of(explanation-13, existence-17)

