(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is)
      (ADJP (RB quite) (JJ true))
      (SBAR (IN that)
        (S
          (NP (EX there))
          (VP (VBZ is)
            (NP (DT no) (JJ scientific) (NN controversy))))))
    (. .)))

nsubj(true-4, It-1)
cop(true-4, is-2)
advmod(true-4, quite-3)
root(ROOT-0, true-4)
complm(is-7, that-5)
expl(is-7, there-6)
ccomp(true-4, is-7)
det(controversy-10, no-8)
amod(controversy-10, scientific-9)
nsubj(is-7, controversy-10)

