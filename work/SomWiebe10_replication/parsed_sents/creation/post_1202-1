      (NP (CD one) (NN thing))
      (SBAR
        (S
          (NP (JJ many) (NN non) (NNS evolutionists))
          (VP (VBP need)
            (S
              (VP (TO to)
                (VP (VB believe))))))))
    (VP (VBZ is) (, ,)
      (SBAR (IN that)
        (S
          (NP (JJS most) (NNS evolutionists))
