(ROOT
  (S
    (ADVP (RB First) (RB off)
      (SBAR (IN if)
        (S
          (NP (EX there))
          (VP (VBD was) (RB n't)
            (VP (VBG evolution)
              (NP (DT the) (NNS diseases)))))))
    (NP (PRP we))
    (VP (VBP have)
      (ADVP (NN today))
      (VP (MD would) (RB n't)
        (VP (VB be)
          (ADJP (RB as) (JJ strong))
          (SBAR (IN as)
            (S
              (NP (PRP they))
              (VP (VBP are)))))))
    (. !)))

advmod(off-2, First-1)
advmod(strong-17, off-2)
mark(evolution-7, if-3)
expl(evolution-7, there-4)
aux(evolution-7, was-5)
neg(evolution-7, n't-6)
dep(off-2, evolution-7)
det(diseases-9, the-8)
dobj(evolution-7, diseases-9)
