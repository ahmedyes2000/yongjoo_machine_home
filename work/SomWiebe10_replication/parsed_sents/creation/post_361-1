(ROOT
  (S
    (ADVP (RB Otherwise))
    (, ,)
    (NP (EX there))
    (VP (MD would) (RB n't)
      (VP (VB be)
        (NP
          (NP (DT any) (NN way))
          (PP (IN of)
            (S
              (VP (VBG telling)
                (NP (DT the) (NN difference))))))))
    (. .)))

advmod(way-8, Otherwise-1)
expl(way-8, there-3)
aux(way-8, would-4)
neg(way-8, n't-5)
