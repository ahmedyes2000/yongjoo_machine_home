(ROOT
  (SBARQ
    (WHADVP (WRB Why))
    (SQ (MD should)
      (NP (PRP I))
      (VP (VB waste)
        (NP
          (NP (DT any) (NN time))
          (VP
            (VP
              (ADVP (RB even))
              (VBG reading)
              (NP (PRP them)))
            (, ,)
            (ADVP (RB much) (RBR less))
            (VP (VBG bothering)
              (S
                (VP (TO to)
                  (VP (VB reply)))))))))
    (. .)))

advmod(waste-4, Why-1)
aux(waste-4, should-2)
nsubj(waste-4, I-3)
root(ROOT-0, waste-4)
det(time-6, any-5)
dobj(waste-4, time-6)
xsubj(reply-15, time-6)
advmod(reading-8, even-7)
dep(time-6, reading-8)
dobj(reading-8, them-9)
advmod(less-12, much-11)
advmod(reading-8, less-12)
dep(reading-8, bothering-13)
aux(reply-15, to-14)
xcomp(bothering-13, reply-15)

