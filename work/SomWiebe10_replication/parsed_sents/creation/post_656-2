(ROOT
  (S
    (NP (DT The) (JJ scientific) (NNS questions))
    (VP (VBD were)
      (VP (VBN settled)
        (ADVP
          (NP (JJ many) (NNS decades))
          (RB ago))))
    (. .)))

det(questions-3, The-1)
amod(questions-3, scientific-2)
nsubjpass(settled-5, questions-3)
auxpass(settled-5, were-4)
root(ROOT-0, settled-5)
amod(decades-7, many-6)
npadvmod(ago-8, decades-7)
advmod(settled-5, ago-8)

