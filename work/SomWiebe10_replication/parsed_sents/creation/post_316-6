(ROOT
  (S ('' '')
    (NP (NNP Sure))
    (VP (VBZ seems)
      (SBAR (IN like)
        (S
          (NP (DT the) (NN cell))
          (VP (VBZ is)
            (VP (VBN designed)
              (PP (TO to)
                (NP (PRP me))))))))
    (. ...)))

nsubj(seems-3, Sure-2)
root(ROOT-0, seems-3)
mark(designed-8, like-4)
det(cell-6, the-5)
nsubjpass(designed-8, cell-6)
auxpass(designed-8, is-7)
advcl(seems-3, designed-8)
prep_to(designed-8, me-10)

