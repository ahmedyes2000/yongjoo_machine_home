(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP (VBP follow)
          (NP (DT that) (NN argument)))))
    (, ,)
    (ADVP (RB then))
    (NP (PRP you))
    (VP (VBP need) (RB not)
      (VP (VB believe)
        (S
          (VP (VBG ANYTHING)
            (PP (IN in)
              (NP (DT the) (NNP Bible)))))
        (SBAR (IN since)
          (S
            (NP (PRP it))
            (VP (MD could)
              (VP (VB be)
                (SBAR (IN that)
                  (S
                    (NP (NNP God))
                    (VP (VBD meant)
                      (NP (NN something))
                      (PP (IN beyond)
                        (NP (PRP$ our) (NN understanding))))))))))))
    (. .)))

mark(follow-3, If-1)
nsubj(follow-3, you-2)
advcl(need-9, follow-3)
det(argument-5, that-4)
dobj(follow-3, argument-5)
advmod(need-9, then-7)
nsubj(need-9, you-8)
root(ROOT-0, need-9)
neg(need-9, not-10)
dep(need-9, believe-11)
xcomp(believe-11, ANYTHING-12)
det(Bible-15, the-14)
prep_in(ANYTHING-12, Bible-15)
mark(be-19, since-16)
nsubj(be-19, it-17)
aux(be-19, could-18)
advcl(believe-11, be-19)
complm(meant-22, that-20)
nsubj(meant-22, God-21)
ccomp(be-19, meant-22)
dobj(meant-22, something-23)
poss(understanding-26, our-25)
prep_beyond(meant-22, understanding-26)

