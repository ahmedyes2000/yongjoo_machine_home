(ROOT
  (S
    (NP (NNS Scientists))
    (VP (VBD gave)
      (NP (JJ intelligent) (NN design))
      (NP (DT a) (JJ fair) (NN chance)))
    (. .)))

nsubj(gave-2, Scientists-1)
root(ROOT-0, gave-2)
amod(design-4, intelligent-3)
iobj(gave-2, design-4)
det(chance-7, a-5)
amod(chance-7, fair-6)
dobj(gave-2, chance-7)

