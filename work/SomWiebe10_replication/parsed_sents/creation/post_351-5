(ROOT
  (S
    (NP (PRP You))
    (VP (VBD referred)
      (PP (TO to)
        (NP (DT the) (NN work)))
      (PP (IN by)
        (NP
          (NP (NNP Ryals))
          (PRN (-LRB- -LRB-)
            (NP (NN aka) (NN island))
            (-RRB- -RRB-))
          (SBAR
            (WHNP (WP who))
            (S
              (VP (VBD made)
                (NP (DT some) (JJ pretty)
                  (ADJP (FW ad) (FW hoc))
                  (NNS claims))))))))
    (. .)))

