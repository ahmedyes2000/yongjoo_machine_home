(ROOT
  (S
    (NP (PRP I))
    (VP (VBD told)
      (NP (PRP you))
      (ADVP
        (ADVP (RBR earlier))
        (SBAR (IN that)
          (S
            (NP (PRP I))
            (VP (VBD did) (RB not)
              (ADVP (RB always))
              (VP (VB believe)
                (PP (IN in)
                  (NP (NNP God)))
                (NP
                  (NP (DT the) (NN way))
                  (SBAR
                    (S
                      (NP (PRP He))
                      (VP (VBZ wants)
                        (S
                          (NP (PRP us))
                          (VP (TO to)
                            (VP (VB believe)
                              (PP (IN in)
                                (NP (PRP Him))))))))))))))))
    (. .)))

nsubj(told-2, I-1)
root(ROOT-0, told-2)
dobj(told-2, you-3)
advmod(told-2, earlier-4)
dep(believe-10, that-5)
nsubj(believe-10, I-6)
aux(believe-10, did-7)
neg(believe-10, not-8)
advmod(believe-10, always-9)
ccomp(earlier-4, believe-10)
prep_in(believe-10, God-12)
det(way-14, the-13)
dobj(believe-10, way-14)
dobj(wants-16, way-14)
nsubj(wants-16, He-15)
rcmod(way-14, wants-16)
nsubj(believe-19, us-17)
aux(believe-19, to-18)
xcomp(wants-16, believe-19)
prep_in(believe-19, Him-21)

