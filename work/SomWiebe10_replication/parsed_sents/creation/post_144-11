(ROOT
  (S
    (NP (PRP I))
    (ADVP (RB still))
    (VP (VBP fail)
      (S
        (VP (TO to)
          (VP (VB grasp)
            (NP
              (NP (DT the) (NN amount))
              (PP (IN of)
                (NP (NN faith)))
              (SBAR
                (S
                  (NP (PRP it))
                  (VP (VBZ takes)
                    (S
                      (VP (TO to)
                        (VP (VB believe)
                          (NP (DT this)))))))))))))
    (. .)))

nsubj(fail-3, I-1)
xsubj(grasp-5, I-1)
advmod(fail-3, still-2)
root(ROOT-0, fail-3)
aux(grasp-5, to-4)
xcomp(fail-3, grasp-5)
det(amount-7, the-6)
dobj(grasp-5, amount-7)
dobj(takes-11, amount-7)
