(ROOT
  (S
    (S
      (NP
        (NP (JJ Numerous) (NNS documents))
        (PP (IN in)
          (NP (NN evidence))))
      (VP (VBP reveal)
        (S
          (NP (NNP FTE))
          (VP (TO to)
            (VP (VB be)
              (NP
                (NP (DT a) (JJ religious) (NN organization))
                (PP (IN with)
                  (NP (JJ religious) (NNS objectives)))))))))
    (, ,) (RB not)
    (S
      (NP (DT a) (JJ scientific) (NN one))
      (VP (VBG pursuing)
        (NP (JJ scientific) (NNS aims))))
    (. .)))

amod(documents-2, Numerous-1)
nsubj(reveal-5, documents-2)
prep_in(documents-2, evidence-4)
root(ROOT-0, reveal-5)
nsubj(organization-11, FTE-6)
aux(organization-11, to-7)
cop(organization-11, be-8)
det(organization-11, a-9)
amod(organization-11, religious-10)
xcomp(reveal-5, organization-11)
amod(objectives-14, religious-13)
prep_with(organization-11, objectives-14)
neg(reveal-5, not-16)
det(one-19, a-17)
amod(one-19, scientific-18)
nsubj(pursuing-20, one-19)
dep(reveal-5, pursuing-20)
amod(aims-22, scientific-21)
dobj(pursuing-20, aims-22)

