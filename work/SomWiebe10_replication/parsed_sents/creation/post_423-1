  (S
    (NP (PRP It))
    (VP (VBZ is)
      (ADJP (JJ true))
      (SBAR
        (SBAR (IN that)
          (S
            (NP (NNP Science))
            (VP (VBZ does)
              (VP (VB refute)
                (NP (JJ certain) (JJ religious) (NNS teachings))))))
        (, ,)
        (CC but)
        (SBAR (IN that)
          (S
            (VP (VBZ is)
              (ADJP (JJ incidental)
                (PP (TO to)
                  (NP (DT the) (NN Science)))))
            (NP (PRP itself))))))
    (. .)))

