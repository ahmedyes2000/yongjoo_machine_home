(ROOT
  (S
    (NP
      (NP (NNS Critics))
      (ADVP (RB however)))
    (, ,)
    (VP (VBP do) (RB n't)
      (VP (VB care)
        (PP (IN about)
          (NP (DT the) (NN truth)))))
    (. .)))

nsubj(care-6, Critics-1)
advmod(Critics-1, however-2)
aux(care-6, do-4)
neg(care-6, n't-5)
root(ROOT-0, care-6)
det(truth-9, the-8)
