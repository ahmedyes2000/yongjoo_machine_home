(ROOT
  (SQ
    (VP (VB cry)
      (S
        (NP (PRP me))
        (NP
          (NP (DT a) (NN river))
          (SBAR
            (S
              (NP (PRP I))
              (VP (VBP 'm)
                (ADJP (JJ sure)
                  (SBAR
                    (S
                      (NP (PRP you) (NNS guys))
                      (VP (VBD were)
                        (ADJP (NN nothing)
                          (CC but)
                          (VBG saints)))))))))))
      (ADVP (RB right)))
    (. ?)))

root(ROOT-0, cry-1)
nsubj(river-4, me-2)
