(ROOT
  (NP (FW .) (. .)))


(ROOT
  (S
    (PP (IN of)
      (NP (NN course)))
    (PRN (, ,)
      (S
        (NP (DT the) (NNP Bible))
        (VP (VBD was) (RB n't)
          (VP (VBN written)
            (S
              (VP (TO to)
                (VP (VB make)
                  (S
                    (NP (NN evolutionist))
                    (ADJP (JJ happy)))))))))
      (, ,))
    (NP (PRP it))
    (ADVP (RB just))
    (VP (VBZ supplies)
      (NP
        (QP (RB about) (CD 2))
        (NNS pages))
      (PP (TO to)
        (NP (NN creation))))
    (. .)))

