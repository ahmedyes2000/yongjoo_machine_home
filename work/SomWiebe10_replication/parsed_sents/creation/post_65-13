(ROOT
  (S
    (S
      (NP (NNP PvMI))
      (VP (VBP understand)
        (SBAR
          (WHADVP (WRB why))
          (S
            (NP (PRP you))
            (VP (VBP want)
              (S
                (VP (TO to)
                  (VP (VB abandon)
                    (S
                      (VP (VBG discussing)
                        (NP (NNP ID))))))))))))
    (, ,)
    (NP (PRP I))
    (VP (MD would)
      (ADVP (RB also))
      (VP (VB hate)
        (S
          (VP (TO to)
            (VP (VB have)
              (S
                (VP (TO to)
                  (VP (VB defend)
                    (NP (PRP it))))))))))
    (. .)))

nsubj(understand-2, PvMI-1)
ccomp(hate-14, understand-2)
advmod(want-5, why-3)
nsubj(want-5, you-4)
xsubj(abandon-7, you-4)
advcl(understand-2, want-5)
aux(abandon-7, to-6)
xcomp(want-5, abandon-7)
xcomp(abandon-7, discussing-8)
dobj(discussing-8, ID-9)
nsubj(hate-14, I-11)
xsubj(have-16, I-11)
aux(hate-14, would-12)
advmod(hate-14, also-13)
root(ROOT-0, hate-14)
aux(have-16, to-15)
xcomp(hate-14, have-16)
aux(defend-18, to-17)
xcomp(have-16, defend-18)
dobj(defend-18, it-19)

