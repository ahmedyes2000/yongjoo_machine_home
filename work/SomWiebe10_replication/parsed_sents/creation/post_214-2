(ROOT
  (S
    (PP (IN Without)
      (NP (DT a) (NN theory)))
    (, ,)
    (NP (PRP it))
    (VP (VBZ s)
      (ADJP (RB very) (JJ hard))
      (S
        (VP (TO to)
          (VP (VB know)
            (SBAR
              (WHADVP (WRB where))
              (S
                (VP (TO to)
                  (VP (VB direct)
                    (NP (PRP$ your) (NN research) (NN focus))))))))))
    (. .)))

det(theory-3, a-2)
