(ROOT
  (S
    (PP (IN In)
      (NP (JJ other) (NNS words)))
    (, ,)
    (NP
      (NP (PRP$ his) (NN lack))
      (PP (IN of)
        (NP
          (NP (NN imagination)
            (CC or)
            (NN familiarity))
          (PP (IN with)
            (NP (NN science))))))
    (VP (VBZ causes)
      (S
        (NP (PRP him))
        (VP (TO to)
          (VP (VB propose)
            (NP
              (NP (DT the) (NN need))
              (PP (IN for)
                (NP (DT a) (NN designer))))))))
    (. .)))

amod(words-3, other-2)
prep_in(causes-13, words-3)
poss(lack-6, his-5)
nsubj(causes-13, lack-6)
prep_of(lack-6, imagination-8)
prep_of(lack-6, familiarity-10)
conj_or(imagination-8, familiarity-10)
prep_with(imagination-8, science-12)
root(ROOT-0, causes-13)
nsubj(propose-16, him-14)
aux(propose-16, to-15)
xcomp(causes-13, propose-16)
det(need-18, the-17)
dobj(propose-16, need-18)
det(designer-21, a-20)
prep_for(need-18, designer-21)

