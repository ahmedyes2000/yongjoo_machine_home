(ROOT
  (S
    (NP
      (NP (DT An) (NN appeal))
      (PP (TO to)
        (NP (CD one) (JJ narrow) (JJ religious) (NN text))))
    (VP (VBZ is) (RB n't)
      (ADJP (RB very) (JJ persuasive)))
    (. .)))

det(appeal-2, An-1)
nsubj(persuasive-11, appeal-2)
num(text-7, one-4)
amod(text-7, narrow-5)
amod(text-7, religious-6)
prep_to(appeal-2, text-7)
cop(persuasive-11, is-8)
neg(persuasive-11, n't-9)
advmod(persuasive-11, very-10)
root(ROOT-0, persuasive-11)

