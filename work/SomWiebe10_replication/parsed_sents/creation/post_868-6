(ROOT
  (S
    (NP
      (NP (DT The) (NN margin))
      (PP (IN of)
        (NP
          (NP (NN error))
          (PP (IN with)
            (NP (DT that) (NN technique))))))
    (VP (VBZ is)
      (NP
        (NP (NNS millions))
        (PP (IN of)
          (NP
            (NP (NNS years))
            (ADVP (JJ wide))))))
    (. .)))

det(margin-2, The-1)
nsubj(millions-9, margin-2)
prep_of(margin-2, error-4)
det(technique-7, that-6)
prep_with(error-4, technique-7)
cop(millions-9, is-8)
root(ROOT-0, millions-9)
prep_of(millions-9, years-11)
