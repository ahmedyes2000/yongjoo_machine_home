(ROOT
  (S
    (PP (IN In)
      (NP
        (NP (DT the) (NN case))
        (PP (IN of)
          (NP (NN evolution)))))
    (, ,)
    (NP (PRP we))
    (VP (VBP have)
      (NP
        (NP (JJ huge) (NNS amounts))
        (PP (IN of)
          (NP
            (NP (NNS data))
            (PP (IN from)
              (NP (JJ diverse) (NNS fields)))))))
    (. .)))

det(case-3, the-2)
prep_in(have-8, case-3)
prep_of(case-3, evolution-5)
nsubj(have-8, we-7)
root(ROOT-0, have-8)
amod(amounts-10, huge-9)
dobj(have-8, amounts-10)
