(ROOT
  (S
    (NP (DT Each))
    (VP
      (VP (VBD was)
        (VP (VBN written)
          (PP (IN for)
            (NP (DT a) (JJ different) (NN audience)))))
      (CC and)
      (VP (VBZ emphasizes)
        (NP
          (NP (JJ different) (NNS aspects))
          (PP (IN of)
            (NP (NNP Jesus))))))
    (. .)))

nsubjpass(written-3, Each-1)
nsubj(emphasizes-9, Each-1)
auxpass(written-3, was-2)
root(ROOT-0, written-3)
det(audience-7, a-5)
amod(audience-7, different-6)
prep_for(written-3, audience-7)
conj_and(written-3, emphasizes-9)
amod(aspects-11, different-10)
dobj(emphasizes-9, aspects-11)
prep_of(aspects-11, Jesus-13)

