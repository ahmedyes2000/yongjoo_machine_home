(ROOT
  (S
    (SBAR
      (WHNP (WP What))
      (S
        (NP (PRP I))
        (VP (VBP am)
          (VP (VBG arguing)))))
    (VP (VBZ is)
      (ADJP
        (ADJP (RB far) (JJR harder))
        (PP (IN for)
          (NP (NNP ID))))
      (S
        (VP (TO to)
          (VP (VB respond)
            (PP (TO to)
              (NP (RB namely) (DT the) (NN fact))))))
      (SBAR (IN that)
        (S
          (NP (PRP it))
          (VP (VBZ is)
            (ADVP (RB scientifically)
              (S
                (VP (VBG speaking)
                  (PP (IN without)
                    (NP (NN content))))))))))
    (. .)))

dobj(arguing-4, What-1)
nsubj(arguing-4, I-2)
aux(arguing-4, am-3)
csubj(harder-7, arguing-4)
cop(harder-7, is-5)
advmod(harder-7, far-6)
root(ROOT-0, harder-7)
prep_for(harder-7, ID-9)
aux(respond-11, to-10)
xcomp(harder-7, respond-11)
advmod(fact-15, namely-13)
det(fact-15, the-14)
prep_to(respond-11, fact-15)
complm(is-18, that-16)
nsubj(is-18, it-17)
ccomp(harder-7, is-18)
advmod(is-18, scientifically-19)
dep(scientifically-19, speaking-20)
prep_without(speaking-20, content-22)

