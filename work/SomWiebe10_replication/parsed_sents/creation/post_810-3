(ROOT
  (S
    (NP (DT This))
    (VP (VBZ is)
      (ADVP (RB entirely))
      (PP (IN within)
        (NP
          (NP (DT the) (NN edict))
          (PP (IN of)
            (NP (NNP Scripture))))))
    (. .)))

nsubj(is-2, This-1)
root(ROOT-0, is-2)
advmod(is-2, entirely-3)
det(edict-6, the-5)
prep_within(is-2, edict-6)
prep_of(edict-6, Scripture-8)

