(ROOT
  (S
    (NP
      (NP (JJ Indian) (NNS texts))
      (VP (VBG dating)
        (ADVP (RB back))
        (PP (TO to)
          (NP (NNP 1000) (NNP BCE)))))
    (VP (MD can)
      (VP (VB be)
        (VP (VBN interpreted)
          (ADVP (RB just) (RB as) (RB easily))
          (S
            (VP (TO to)
              (VP (VB indicate)
                (NP (DT a) (JJ spherical) (NN planet))))))))
    (. .)))

amod(texts-2, Indian-1)
nsubjpass(interpreted-10, texts-2)
partmod(texts-2, dating-3)
advmod(dating-3, back-4)
nn(BCE-7, 1000-6)
prep_to(dating-3, BCE-7)
aux(interpreted-10, can-8)
auxpass(interpreted-10, be-9)
root(ROOT-0, interpreted-10)
advmod(easily-13, just-11)
advmod(easily-13, as-12)
advmod(interpreted-10, easily-13)
aux(indicate-15, to-14)
xcomp(interpreted-10, indicate-15)
det(planet-18, a-16)
amod(planet-18, spherical-17)
dobj(indicate-15, planet-18)

