(ROOT
  (S (CC And)
    (NP (PRP you))
    (VP (VBP want)
      (S
        (NP (NNP ID) (NNS scientists))
        (VP (TO to)
          (VP (VB publish)
            (NP (PRP$ their) (NNS articles))))))
    (. ?)))

cc(want-3, And-1)
nsubj(want-3, you-2)
root(ROOT-0, want-3)
nn(scientists-5, ID-4)
nsubj(publish-7, scientists-5)
aux(publish-7, to-6)
xcomp(want-3, publish-7)
poss(articles-9, their-8)
dobj(publish-7, articles-9)

