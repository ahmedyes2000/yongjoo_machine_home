(ROOT
  (S
    (S
      (NP (NNP Adam))
      (VP (VBD was)
        (NP (DT the) (JJ first) (JJ human) (NN being))))
    (CC and)
    (RB then)
    (S
      (NP (DT the) (JJ human) (NN race))
      (VP (VBZ begins)
        (PP (IN from)
          (NP (RB there)))))
    (. .)))

nsubj(being-6, Adam-1)
cop(being-6, was-2)
det(being-6, the-3)
amod(being-6, first-4)
amod(being-6, human-5)
root(ROOT-0, being-6)
advmod(being-6, then-8)
conj_and(being-6, then-8)
det(race-11, the-9)
amod(race-11, human-10)
nsubj(begins-12, race-11)
ccomp(being-6, begins-12)
prep(begins-12, from-13)
pobj(from-13, there-14)

