(ROOT
  (S
    (NP (NN Evidence))
    (VP (VBZ says)
      (SBAR
        (S
          (NP (DT this))
          (ADVP (RB still))
          (VP (VBZ happens)
            (NP (NN today))))))
    (. .)))

nsubj(says-2, Evidence-1)
root(ROOT-0, says-2)
nsubj(happens-5, this-3)
advmod(happens-5, still-4)
ccomp(says-2, happens-5)
tmod(happens-5, today-6)

