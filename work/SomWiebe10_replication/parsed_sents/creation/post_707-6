(ROOT
  (S (CC but)
    (NP
      (NP (DT the) (JJ poetic) (NN nature))
      (PP (IN of)
        (NP (DT the) (NN statement))))
    (VP (VBZ is)
      (ADJP (JJ unavoidable)))
    (. .)))

cc(unavoidable-9, but-1)
det(nature-4, the-2)
amod(nature-4, poetic-3)
nsubj(unavoidable-9, nature-4)
det(statement-7, the-6)
prep_of(nature-4, statement-7)
cop(unavoidable-9, is-8)
root(ROOT-0, unavoidable-9)

(ROOT
  (S
    (INTJ (RB Sure))
    (, ,)
    (ADVP (CC but)
      (RB objectively))
    (NP
      (NP (JJ many) (NNS religions))
      (PP (IN from)
        (NP (DT that) (NN time) (NN period))))
    (VP (VBD placed)
      (NP (PRP$ their) (NN deity))
      (PP (IN in)
        (NP (DT the) (NNS heavens))))
    (. .)))

dep(placed-11, Sure-1)
cc(objectively-4, but-3)
advmod(placed-11, objectively-4)
amod(religions-6, many-5)
nsubj(placed-11, religions-6)
det(period-10, that-8)
nn(period-10, time-9)
prep_from(religions-6, period-10)
root(ROOT-0, placed-11)
poss(deity-13, their-12)
dobj(placed-11, deity-13)
det(heavens-16, the-15)
