(ROOT
  (S (RB Then)
    (NP (PRP they))
    (VP (VBP claim)
      (SBAR (IN that)
        (S
          (NP (NNP ID) (NNS scientists))
          (VP (VBP have) (RB n't)
            (VP (VBN published)
              (NP (DT any) (NNS articles))
              (S
                (VP (TO to)
                  (VP (VB prove)
                    (NP (PRP$ their) (NN point))
                    (SBAR (IN that)
                      (S
                        (NP (PRP it))
                        (VP (VBZ is) (RB not)
                          (NP (JJ good) (NN science)))))))))))))
    (. .)))

advmod(claim-3, Then-1)
nsubj(claim-3, they-2)
root(ROOT-0, claim-3)
complm(published-9, that-4)
nn(scientists-6, ID-5)
nsubj(published-9, scientists-6)
aux(published-9, have-7)
neg(published-9, n't-8)
ccomp(claim-3, published-9)
det(articles-11, any-10)
dobj(published-9, articles-11)
aux(prove-13, to-12)
xcomp(published-9, prove-13)
poss(point-15, their-14)
dobj(prove-13, point-15)
complm(science-21, that-16)
nsubj(science-21, it-17)
cop(science-21, is-18)
neg(science-21, not-19)
amod(science-21, good-20)
ccomp(prove-13, science-21)

