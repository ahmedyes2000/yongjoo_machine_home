  (S (CC And)
    (NP (PRP I))
    (VP
      (VP (VBP am)
        (VP (VBG trying)
          (S
            (VP (TO to)
              (VP (VB be)
                (ADJP (RB as) (JJ respectable)
                  (PP (TO to)
                    (NP (NNS evolutionists)))))))))
      (CC and)
      (VP (VBP am)
        (ADVP (RB eagerly))
        (VP (VBG awaiting)
          (NP (NNS opinions)))))
    (. .)))

nsubj(trying-4, I-2)
xsubj(respectable-8, I-2)
