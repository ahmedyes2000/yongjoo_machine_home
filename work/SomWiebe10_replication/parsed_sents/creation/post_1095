(ROOT
  (S
    (NP
      (NP (DT The) (NN one))
      (SBAR
        (WHNP (WDT that))
        (S
          (VP (VBZ suits)
            (SBAR
              (S
                (NP (PRP it))
                (VP (VBZ 's)
                  (NP (JJ current) (NN environment)))))))))
    (VP
      (VP (VBZ surveys))
      (CC and)
      (VP (VBP grow)
        (PRT (RP up))
        (S
          (VP (TO to)
            (VP (VB breed))))))
    (. .)))

det(one-2, The-1)
nsubj(suits-4, one-2)
nsubj(surveys-9, one-2)
nsubj(grow-11, one-2)
xsubj(breed-14, one-2)
