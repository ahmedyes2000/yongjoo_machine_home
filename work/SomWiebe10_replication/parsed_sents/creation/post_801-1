(ROOT
  (S
    (S
      (NP (PRP I))
      (VP (VBP have)))
    (CC and)
    (S
      (NP (PRP I))
      (VP (VBP find)
        (S
          (NP (PRP it))
          (VP (TO to)
            (VP (VB be)
              (NP (DT a)
                (ADJP (RB pretty) (RB far))
                (NN stretch)))))))
    (. .)))

nsubj(have-2, I-1)
root(ROOT-0, have-2)
nsubj(find-5, I-4)
conj_and(have-2, find-5)
nsubj(stretch-12, it-6)
aux(stretch-12, to-7)
