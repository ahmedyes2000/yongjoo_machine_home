(ROOT
  (S
    (S
      (NP (PRP I))
      (VP (VBP 'm)
        (ADJP (JJ sure)
          (SBAR
            (S
              (NP (PRP they))
              (VP (VB fool)
                (NP (DT some) (NNS people))))))))
    (, ,)
    (CC but)
    (S
      (NP (JJS most) (NNS people))
      (VP (VBP are) (RB n't)
        (ADJP (RB that) (JJ dumb))))
    (. .)))

nsubj(sure-3, I-1)
cop(sure-3, 'm-2)
root(ROOT-0, sure-3)
nsubj(fool-5, they-4)
dep(sure-3, fool-5)
det(people-7, some-6)
dobj(fool-5, people-7)
amod(people-11, most-10)
nsubj(dumb-15, people-11)
cop(dumb-15, are-12)
neg(dumb-15, n't-13)
advmod(dumb-15, that-14)
conj_but(sure-3, dumb-15)

