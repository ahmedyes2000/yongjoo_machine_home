(ROOT
  (S
    (VP
      (VP
        (ADVP (RB Specifically))
        (VBG having)
        (NP (NN god)))
      (CC or)
      (VP
        (NP (DT a) (JJ supernatural) (NN creator))
        (PP (IN in)
          (NP
            (NP (DT the) (NN role))
            (PP (IN of)
              (NP (DT the) (NN designer)))))))
    (. .)))

advmod(having-2, Specifically-1)
root(ROOT-0, having-2)
dobj(having-2, god-3)
det(creator-7, a-5)
amod(creator-7, supernatural-6)
conj_or(having-2, creator-7)
det(role-10, the-9)
prep_in(creator-7, role-10)
det(designer-13, the-12)
prep_of(role-10, designer-13)

