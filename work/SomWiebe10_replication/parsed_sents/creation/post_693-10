(ROOT
  (S (CC And)
    (NP (DT that) (NNP God))
    (VP
      (VP (VBD created)
        (NP (NN man)))
      (, ,)
      (VP (VBZ has)
        (VP (VBN cared)
          (PP (IN for)
            (NP (PRP him)))))
      (, ,)
      (CC and)
      (VP (VBZ has)
        (VP (VBN given)
          (NP (PRP him))
          (NP
            (NP (DT a) (JJ special) (NN place))
            (PP (IN in)
              (NP (PRP$ His) (NN order)))))))
    (. .)))

det(God-3, that-2)
nsubj(created-4, God-3)
nsubj(cared-8, God-3)
nsubj(given-14, God-3)
root(ROOT-0, created-4)
dobj(created-4, man-5)
aux(cared-8, has-7)
conj_and(created-4, cared-8)
prep_for(cared-8, him-10)
aux(given-14, has-13)
conj_and(created-4, given-14)
iobj(given-14, him-15)
det(place-18, a-16)
amod(place-18, special-17)
dobj(given-14, place-18)
poss(order-21, His-20)
prep_in(place-18, order-21)

