(ROOT
  (S
    (PP (IN On)
      (NP (NNP Day) (CD 6)))
    (, ,)
    (NP (NNP God))
    (VP (VBZ creates)
      (NP
        (NP (NN land) (NNS animals))
        (, ,)
        (SBAR
          (WHNP (WDT which))
          (S
            (VP (VBP are)
              (ADVP (RB basically))
              (NP
                (NP (NNS mammals))
                (, ,)
                (SBAR
                  (WHNP (WDT which))
                  (S
                    (ADVP (RB also))
                    (VP (VBD came)
                      (ADVP (JJ next))
                      (PP (VBG according)
                        (PP (TO to)
                          (NP (NN evolution)))))))))))))
    (. .)))

prep_on(creates-6, Day-2)
num(Day-2, 6-3)
nsubj(creates-6, God-5)
root(ROOT-0, creates-6)
nn(animals-8, land-7)
dobj(creates-6, animals-8)
nsubj(mammals-13, animals-8)
cop(mammals-13, are-11)
advmod(mammals-13, basically-12)
rcmod(animals-8, mammals-13)
nsubj(came-17, mammals-13)
advmod(came-17, also-16)
rcmod(mammals-13, came-17)
advmod(came-17, next-18)
prepc_according_to(came-17, to-20)
pobj(came-17, evolution-21)

(ROOT
  (S
    (ADVP (RB Finally))
    (, ,)
    (NP (NNP God))
    (VP (VBD created)
      (NP
        (NP (NN man))
        (, ,)
        (SBAR
          (WHNP (WDT which))
          (S
            (VP (VBZ fits)
              (PRT (RP in))
              (ADVP (RB perfectly))
              (PP (IN with)
                (NP (JJ evolutionary) (NN theory))))))))
    (. .)))

advmod(created-4, Finally-1)
nsubj(created-4, God-3)
root(ROOT-0, created-4)
dobj(created-4, man-5)
nsubj(fits-8, man-5)
rcmod(man-5, fits-8)
prt(fits-8, in-9)
advmod(fits-8, perfectly-10)
amod(theory-13, evolutionary-12)
prep_with(fits-8, theory-13)

