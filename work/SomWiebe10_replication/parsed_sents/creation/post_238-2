(ROOT
  (S
    (ADVP (RB However))
    (, ,)
    (NP (NNP ID))
    (VP (VBZ seems)
      (S
        (VP (TO to)
          (VP (VB have)
            (VP
              (ADVP (RB mostly))
              (VBN ignored)
              (NP (DT these)
                (ADJP (JJ inconvenient))
                (NNS facts)))))))
    (. .)))

advmod(seems-4, However-1)
nsubj(seems-4, ID-3)
xsubj(ignored-8, ID-3)
root(ROOT-0, seems-4)
aux(ignored-8, to-5)
aux(ignored-8, have-6)
advmod(ignored-8, mostly-7)
xcomp(seems-4, ignored-8)
det(facts-11, these-9)
amod(facts-11, inconvenient-10)
dobj(ignored-8, facts-11)

