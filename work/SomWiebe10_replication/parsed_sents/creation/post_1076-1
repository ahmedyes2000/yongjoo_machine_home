(ROOT
  (S
    (PP (IN For)
      (NP (CD one) (NN thing)))
    (, ,)
    (NP (DT the) (NNS creationists))
    (VP (VBP hold)
      (SBAR (IN that)
        (S
          (NP (DT the) (NN Earth))
          (VP (VBZ is)
            (ADJP
              (NP
                (QP (RB only) (CD 6000))
                (NNS years))
              (JJ old))))))
    (. .)))

num(thing-3, one-2)
prep_for(hold-7, thing-3)
det(creationists-6, the-5)
nsubj(hold-7, creationists-6)
root(ROOT-0, hold-7)
complm(old-15, that-8)
det(Earth-10, the-9)
nsubj(old-15, Earth-10)
cop(old-15, is-11)
quantmod(6000-13, only-12)
num(years-14, 6000-13)
npadvmod(old-15, years-14)
ccomp(hold-7, old-15)

