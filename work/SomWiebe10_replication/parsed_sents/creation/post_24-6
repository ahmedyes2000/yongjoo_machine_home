(ROOT
  (S
    (NP (PRP They))
    (ADVP (RB frequently))
    (VP (VBP call)
      (PP (IN for)
        (NP (NNS individuals) (NNS classes)))
      (S
        (VP (TO to)
          (VP (VB be)
            (VP (VBN created)
              (SBAR (IN that)
                (S
                  (VP (VBP teach)
                    (NP
                      (NP (`` ``) (NN creationism) (NN science) ('' ''))
                      (PP (IN as)
                        (NP (NN truth))))))))))))
    (. .)))

nsubj(call-3, They-1)
xsubj(created-9, They-1)
advmod(call-3, frequently-2)
root(ROOT-0, call-3)
nn(classes-6, individuals-5)
prep_for(call-3, classes-6)
aux(created-9, to-7)
auxpass(created-9, be-8)
xcomp(call-3, created-9)
complm(teach-11, that-10)
ccomp(created-9, teach-11)
nn(science-14, creationism-13)
dobj(teach-11, science-14)
prep_as(science-14, truth-17)

