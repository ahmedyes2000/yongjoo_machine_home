(ROOT
  (S ('' '')
    (NP (PRP I))
    (VP (VBP use)
      (NP (NNS quotes))
      (SBAR (IN because)
        (S
          (NP (NN creationism))
          (VP
            (VP (VBZ offers)
              (NP (DT no) (JJ specific) (NNS mechanisms)))
            (CC and)
            (VP (VBZ makes)
              (NP (DT no) (JJ useful) (NNS predictions)))))))
    (. .)))

nsubj(use-3, I-2)
root(ROOT-0, use-3)
dobj(use-3, quotes-4)
mark(offers-7, because-5)
nsubj(offers-7, creationism-6)
nsubj(makes-12, creationism-6)
advcl(use-3, offers-7)
det(mechanisms-10, no-8)
amod(mechanisms-10, specific-9)
dobj(offers-7, mechanisms-10)
advcl(use-3, makes-12)
conj_and(offers-7, makes-12)
det(predictions-15, no-13)
amod(predictions-15, useful-14)
dobj(makes-12, predictions-15)

