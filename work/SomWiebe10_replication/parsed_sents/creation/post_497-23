(ROOT
  (S ('' '')
    (S
      (SBAR (IN If)
        (S
          (NP (DT these) (NNS prongs))
          (ADVP (RB properly))
          (VP (VB define))))
      (VP (`` ``) (VBZ religion) ('' '')))
    (, ,) (RB then)
    (S
      (NP (NNP Judge) (NNP Jones))
      (VP (VBD erred)
        (PP (IN in)
          (NP (PRP$ his) (NN decision)))))
    (. .)))

mark(define-6, If-2)
det(prongs-4, these-3)
nsubj(define-6, prongs-4)
advmod(define-6, properly-5)
csubj(religion-8, define-6)
root(ROOT-0, religion-8)
advmod(religion-8, then-11)
nn(Jones-13, Judge-12)
nsubj(erred-14, Jones-13)
ccomp(religion-8, erred-14)
