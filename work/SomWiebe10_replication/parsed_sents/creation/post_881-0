(ROOT
  (S
    (NP
      (NP (DT The) (NN argument))
      (SBAR
        (S
          (NP (PRP he))
          (VP (VBZ is)
            (VP (VBG giving))))))
    (VP (VBZ is)
      (SBAR (IN that)
        (S
          (NP (DT the) (NN day) (NN age) (NN theory))
          (VP (VBZ works)
            (PP
              (ADVP (RBR better))
              (RB then)
              (NP (DT the) (JJ 6-day) (NN creation)))))))
    (. .)))

det(argument-2, The-1)
dobj(giving-5, argument-2)
nsubj(is-6, argument-2)
nsubj(giving-5, he-3)
aux(giving-5, is-4)
rcmod(argument-2, giving-5)
root(ROOT-0, is-6)
complm(works-12, that-7)
det(theory-11, the-8)
nn(theory-11, day-9)
nn(theory-11, age-10)
nsubj(works-12, theory-11)
ccomp(is-6, works-12)
prep(works-12, better-13)
advmod(better-13, then-14)
det(creation-17, the-15)
amod(creation-17, 6-day-16)
pobj(better-13, creation-17)

