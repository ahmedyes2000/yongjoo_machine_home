(ROOT
  (S
    (S
      (SBAR (IN Though)
        (S
          (NP (PRP I))
          (VP (VBP do))))
      (VP (VBP think)
        (S
          (NP (PRP I))
          (VP (VB find)
            (NP
              (NP
                (ADJP (RB much) (JJR more))
                (NN animosity))
              (VP (VBN spouted)
                (PP (IN by)
                  (NP (DT the) (NN atheist)))))))))
    (: ...)
    (CC and)
    (S
      (S
        (PP (IN for)
          (NP (NN balance)))
        (NP (PRP I))
        (VP (VBP do)
          (ADVP (RB usually))
          (VP (VB find)
            (NP (JJR more) (NN ignorance))
            (PP (IN from)
              (NP (NNS christians))))))
      (: ...) (RB not)
      (S
        (ADVP (RB all) (RB just))
        (PP (IN on)
          (NP (DT the) (NN average)))))
    (. .)))

