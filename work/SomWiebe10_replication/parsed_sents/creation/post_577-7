(ROOT
  (S ('' '')
    (NP (DT That))
    (VP (VBD was)
      (ADVP
        (NP
          (QP (IN over) (CD ten))
          (NNS years))
        (RB ago))
      (VP (VBG relating)
        (PP (TO to)
          (NP
            (NP (CD one) (NN form))
            (PP (IN of)
              (NP (NNP RNA)))))))
    (. .)))

nsubj(relating-8, That-2)
aux(relating-8, was-3)
quantmod(ten-5, over-4)
num(years-6, ten-5)
npadvmod(ago-7, years-6)
advmod(relating-8, ago-7)
root(ROOT-0, relating-8)
num(form-11, one-10)
prep_to(relating-8, form-11)
prep_of(form-11, RNA-13)

