(ROOT
  (S
    (NP (PRP I))
    (VP (VBP have)
      (VP (VBN exposed)
        (NP
          (NP (PRP$ their) (JJ false) (NNS claims))
          (PP (IN of)
            (NP (NN truth))))))
    (. .)))

nsubj(exposed-3, I-1)
aux(exposed-3, have-2)
root(ROOT-0, exposed-3)
poss(claims-6, their-4)
amod(claims-6, false-5)
dobj(exposed-3, claims-6)
prep_of(claims-6, truth-8)

