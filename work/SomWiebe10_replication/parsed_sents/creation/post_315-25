(ROOT
  (S
    (NP (PRP I))
    (VP (VBP know)
      (NP (PRP it))
      (SBAR
        (WHADVP (WRB when))
        (S
          (NP (PRP I))
          (VP (VBP see)
            (NP (PRP it))))))
    (. ?)))

nsubj(know-2, I-1)
root(ROOT-0, know-2)
dobj(know-2, it-3)
advmod(see-6, when-4)
nsubj(see-6, I-5)
advcl(know-2, see-6)
dobj(see-6, it-7)

