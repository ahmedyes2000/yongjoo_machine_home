(ROOT
  (S ('' '')
    (NP (DT The) (NNS unbelievers))
    (VP (VBP do) (RB n't)
      (VP (VB believe)
        (PP (IN in)
          (NP (NNP God)))
        (SBAR (IN until)
          (S
            (NP (PRP they))
            (VP (VBP find)
              (NP (PRP themselves))
              (PP
                (PP (IN in)
                  (NP (DT a) (NN disaster)))
                (CC and)
                (PP (IN at)
                  (NP
                    (NP (DT the) (NN brink))
                    (PP (IN of)
                      (NP (NN death)))))))))))
    (. .)))

det(unbelievers-3, The-2)
nsubj(believe-6, unbelievers-3)
aux(believe-6, do-4)
neg(believe-6, n't-5)
root(ROOT-0, believe-6)
prep_in(believe-6, God-8)
mark(find-11, until-9)
nsubj(find-11, they-10)
nsubj(find-11', they-10)
advcl(believe-6, find-11)
advcl(believe-6, find-11')
conj_and(find-11, find-11')
dobj(find-11, themselves-12)
det(disaster-15, a-14)
prep_in(find-11, disaster-15)
det(brink-19, the-18)
prep_at(find-11', brink-19)
prep_of(brink-19, death-21)

