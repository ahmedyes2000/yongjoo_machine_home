(ROOT
  (S
    (NP (DT That) (NN way))
    (NP (PRP we))
    (VP (VBP keep)
      (NP
        (NP (DT the) (NN divide))
        (PP (IN between)
          (NP (NN church)
            (CC and)
            (NN state)))))
    (. .)))

det(way-2, That-1)
nsubj(keep-4, way-2)
nsubj(keep-4, we-3)
root(ROOT-0, keep-4)
det(divide-6, the-5)
dobj(keep-4, divide-6)
prep_between(divide-6, church-8)
prep_between(divide-6, state-10)
conj_and(church-8, state-10)

