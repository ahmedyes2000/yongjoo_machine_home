(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (DT the) (NNP Catholic) (NNP Church))
        (VP (VBZ says)
          (NP (PRP it)))))
    (, ,)
    (NP (PRP it))
    (VP (VBZ has)
      (S
        (NP (RB just) (NN gotta))
        (VP (VB be)
          (ADJP (JJ true)))))
    (. !)))

mark(says-5, If-1)
det(Church-4, the-2)
nn(Church-4, Catholic-3)
nsubj(says-5, Church-4)
advcl(has-9, says-5)
dobj(says-5, it-6)
nsubj(has-9, it-8)
root(ROOT-0, has-9)
