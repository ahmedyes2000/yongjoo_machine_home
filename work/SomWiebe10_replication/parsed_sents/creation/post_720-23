(ROOT
  (S
    (NP (DT That))
    (VP (VBZ is)
      (SBAR
        (WHADVP (WRB why))
        (S
          (NP (PRP they))
          (VP (VBD evolved)
            (PP (IN into)
              (NP (JJ nocturnal) (NNS animals)))))))
    (. .)))

nsubj(is-2, That-1)
root(ROOT-0, is-2)
advmod(evolved-5, why-3)
nsubj(evolved-5, they-4)
advcl(is-2, evolved-5)
amod(animals-8, nocturnal-7)
prep_into(evolved-5, animals-8)

