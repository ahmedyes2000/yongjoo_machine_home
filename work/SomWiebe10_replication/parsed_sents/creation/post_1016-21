(ROOT
  (S
    (NP (PRP I))
    (VP (VBP do) (RB not)
      (VP (VB believe)
        (SBAR
          (S
            (NP (DT this) (NN debate))
            (VP (VBZ is)
              (ADJP (JJ fair))
              (, ,)
              (SBAR (IN because)
                (S
                  (NP (NN evolution))
                  (VP (VBZ has)
                    (NP
                      (NP
                        (ADJP (RB so) (JJ many))
                        (NNS gaps))
                      (PP (IN in)
                        (NP (PRP$ its) (NN reasoning))))))))))))
    (. .)))

nsubj(believe-4, I-1)
aux(believe-4, do-2)
neg(believe-4, not-3)
root(ROOT-0, believe-4)
det(debate-6, this-5)
nsubj(fair-8, debate-6)
cop(fair-8, is-7)
ccomp(believe-4, fair-8)
mark(has-12, because-10)
nsubj(has-12, evolution-11)
advcl(fair-8, has-12)
advmod(many-14, so-13)
amod(gaps-15, many-14)
dobj(has-12, gaps-15)
poss(reasoning-18, its-17)
prep_in(gaps-15, reasoning-18)

