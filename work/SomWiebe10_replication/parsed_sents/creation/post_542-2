(ROOT
  (S
    (NP (EX There))
    (VP (VBD was) (RB n't)
      (NP
        (NP (DT a) (NN free-for-all))
        (PP (IN on)
          (NP (NN murder)
            (CC or)
            (NN theft))))
      (SBAR (IN before)
        (S
          (NP (NNP Moses))
          (VP (VBD came)
            (PRT (RP down))
            (PP (IN from)
              (NP (DT the) (NN mountain)))))))
    (. .)))

expl(was-2, There-1)
root(ROOT-0, was-2)
neg(was-2, n't-3)
det(free-for-all-5, a-4)
nsubj(was-2, free-for-all-5)
prep_on(free-for-all-5, murder-7)
prep_on(free-for-all-5, theft-9)
conj_or(murder-7, theft-9)
mark(came-12, before-10)
nsubj(came-12, Moses-11)
advcl(was-2, came-12)
prt(came-12, down-13)
det(mountain-16, the-15)
prep_from(came-12, mountain-16)

