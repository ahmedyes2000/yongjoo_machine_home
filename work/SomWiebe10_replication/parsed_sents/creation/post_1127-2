(ROOT
  (S
    (ADVP (RB Secondly))
    (, ,)
    (SBAR (IN if)
      (S
        (NP (NNP God))
        (VP (VBD did)
          (VP (VB create)
            (NP (NN everything))))))
    (, ,)
    (ADVP (RB then))
    (NP (PRP we))
    (VP (MD would)
      (VP (VB have)
        (VP (VBN EVOLVED)
          (PP (IN from)
            (SBAR
              (WHNP (WP what))
              (S
                (NP (PRP he))
                (VP (VBD made))))))))
    (. .)))

advmod(EVOLVED-13, Secondly-1)
mark(create-6, if-3)
nsubj(create-6, God-4)
aux(create-6, did-5)
advcl(EVOLVED-13, create-6)
dobj(create-6, everything-7)
