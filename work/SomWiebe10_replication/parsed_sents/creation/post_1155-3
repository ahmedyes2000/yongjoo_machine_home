(ROOT
  (S
    (PP (IN By)
      (NP (PRP$ its) (NN definition)))
    (NP (PRP it))
    (VP (VBZ means) (`` ``)
      (S
        (NP
          (NP (DT a) (JJ coherent) (NN group))
          (PP (IN of)
            (NP (JJ general) (NNS propositions))))
        (VP (VBN used)
          (PP (IN as)
            (NP
              (NP (NNS principles))
              (PP (IN of)
                (NP (NN explanation)))))
          (PP (IN for)
            (NP
              (NP (DT a) (NN class))
              (PP (IN of)
                (NP (NNS phenomena)))))))
      ('' ''))
    (. .)))

poss(definition-3, its-2)
prep_by(means-5, definition-3)
nsubj(means-5, it-4)
root(ROOT-0, means-5)
