(ROOT
  (FRAG (CC And)
    (NP
      (NP (DT the) (NN reason))
      (SBAR
        (WHADVP (WRB why))
        (S
          (NP (EX there))
          (VP (VBP are)
            (ADVP (RB so))
            (NP
              (NP (JJ many) (NNS species))
              (SBAR
                (WHNP (WDT that))
                (S
                  (VP (VBP are) (RB n't)
                    (PP (IN on)
                      (NP (DT this) (NN planet)))
                    (ADVP (DT any) (RBR longer))))))))))
    (. .)))

dep(reason-3, And-1)
det(reason-3, the-2)
root(ROOT-0, reason-3)
advmod(are-6, why-4)
expl(are-6, there-5)
rcmod(reason-3, are-6)
advmod(are-6, so-7)
amod(species-9, many-8)
nsubj(are-6, species-9)
nsubj(are-11, species-9)
rcmod(species-9, are-11)
neg(are-11, n't-12)
det(planet-15, this-14)
prep_on(are-11, planet-15)
dep(longer-17, any-16)
advmod(are-11, longer-17)

