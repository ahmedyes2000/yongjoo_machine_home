(ROOT
  (S
    (NP (DT That))
    (VP (VBZ 's)
      (SBAR
        (WHNP (WP what))
        (S
          (NP (NN science))
          (VP (MD would)
            (VP (VB do))))))
    (. .)))

nsubj('s-2, That-1)
root(ROOT-0, 's-2)
dobj(do-6, what-3)
nsubj(do-6, science-4)
aux(do-6, would-5)
ccomp('s-2, do-6)

