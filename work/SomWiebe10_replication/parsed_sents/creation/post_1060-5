(ROOT
  (S
    (PP (IN In)
      (NP (DT a) (NN laboratory)))
    (NP (PRP we))
    (VP (MD can)
      (VP (VB conclude)
        (PP (IN as)
          (NP
            (NP (DT a) (NN consequence))
            (PP (IN of)
              (NP (DT an) (NN experiment)))))
        (SBAR (IN that)
          (S
            (NP (NN plastic))
            (VP (VBZ is)
              (VP (VBN derived)
                (PP (IN from)
                  (NP (NN petroleum)))))))))
    (. .)))

det(laboratory-3, a-2)
prep_in(conclude-6, laboratory-3)
nsubj(conclude-6, we-4)
aux(conclude-6, can-5)
root(ROOT-0, conclude-6)
