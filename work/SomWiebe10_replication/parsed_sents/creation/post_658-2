(ROOT
  (S
    (PP (IN In)
      (NP (NN fact)))
    (, ,)
    (SBAR (IN as)
      (S
        (NP (PRP I))
        (VP (VBP have)
          (VP (VBN shown)))))
    (, ,)
    (NP
      (NP (NNP ID) (POS 's))
      (JJ chosen) (NN approach))
    (VP (VBZ makes)
      (NP
        (ADJP (RB very) (JJ unreliable))
        (NNS inferences)))
    (. .)))

prep_in(makes-13, fact-2)
mark(shown-7, as-4)
nsubj(shown-7, I-5)
aux(shown-7, have-6)
advcl(makes-13, shown-7)
poss(approach-12, ID-9)
amod(approach-12, chosen-11)
nsubj(makes-13, approach-12)
root(ROOT-0, makes-13)
advmod(unreliable-15, very-14)
amod(inferences-16, unreliable-15)
dobj(makes-13, inferences-16)

