(ROOT
  (S
    (NP (EX There))
    (VP (VBZ is)
      (NP
        (NP (DT a) (NN difference))
        (PP (IN of)
          (NP (NN opinion))))
      (ADVP (RB here)))
    (. .)))

expl(is-2, There-1)
root(ROOT-0, is-2)
det(difference-4, a-3)
nsubj(is-2, difference-4)
prep_of(difference-4, opinion-6)
advmod(is-2, here-7)

