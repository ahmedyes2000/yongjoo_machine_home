(ROOT
  (S
    (NP
      (NP (DT The) (NN alteration))
      (PP (IN of)
        (NP (DT an) (NN organism))))
    (VP
      (VP (MD will) (RB not)
        (ADVP (RB always))
        (VP (VB be)
          (ADJP (JJ slow)
            (CC and)
            (JJ gradual))))
      (, ,)
      (CC or)
      (VP (VBZ step-by-step)))
    (. .)))

det(alteration-2, The-1)
nsubj(slow-10, alteration-2)
nsubj(gradual-12, alteration-2)
nsubj(step-by-step-15, alteration-2)
det(organism-5, an-4)
prep_of(alteration-2, organism-5)
aux(slow-10, will-6)
neg(slow-10, not-7)
