(ROOT
  (S (CC And)
    (S
      (NP (NNP Jacob))
      (VP (VBD became)
        (VP (JJ equipped)
          (PP (IN with)
            (NP (CD twelve) (NNS sons))))))
    (, ,) (IN so)
    (S
      (NP (DT the) (CD twelve) (NNS tribes))
      (VP (MD should)
        (VP (VB be)
          (ADJP (JJ equal)))))
    (. .)))

cc(equipped-4, And-1)
nsubj(equipped-4, Jacob-2)
aux(equipped-4, became-3)
root(ROOT-0, equipped-4)
num(sons-7, twelve-6)
