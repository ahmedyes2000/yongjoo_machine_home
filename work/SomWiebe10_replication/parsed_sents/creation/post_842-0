(ROOT
  (S
    (SBAR
      (ADVP (RB Well))
      (IN once)
      (S
        (NP (PRP you))
        (VP (VBP accept)
          (NP
            (NP (DT the) (NN existence))
            (PP (IN of)
              (NP
                (NP (DT an)
                  (ADJP (RB all) (JJ powerful))
                  (NN deity))
                (ADVP (RB then))))))))
    (NP (NN anything))
    (VP (VBZ is)
      (ADJP (JJ possible)))
    (. ...)))

advmod(accept-4, Well-1)
dep(accept-4, once-2)
nsubj(accept-4, you-3)
advcl(possible-15, accept-4)
det(existence-6, the-5)
dobj(accept-4, existence-6)
det(deity-11, an-8)
advmod(powerful-10, all-9)
amod(deity-11, powerful-10)
prep_of(existence-6, deity-11)
advmod(deity-11, then-12)
nsubj(possible-15, anything-13)
cop(possible-15, is-14)
root(ROOT-0, possible-15)

