(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP (VBP 're)
          (VP (VBG looking)
            (PP (IN for)
              (NP (JJ reliable) (NN evidence)))))))
    (, ,)
    (NP (EX there))
    (VP (VBZ 's)
      (NP (DT a) (JJ clear) (NN choice)))
    (. .)))

mark(looking-4, If-1)
nsubj(looking-4, you-2)
aux(looking-4, 're-3)
advcl('s-10, looking-4)
amod(evidence-7, reliable-6)
prep_for(looking-4, evidence-7)
expl('s-10, there-9)
root(ROOT-0, 's-10)
det(choice-13, a-11)
amod(choice-13, clear-12)
nsubj('s-10, choice-13)

