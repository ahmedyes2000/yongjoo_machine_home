(ROOT
  (S
    (NP (PRP I))
    (VP (VBP wish)
      (SBAR
        (S
          (NP (PRP I))
          (VP (MD could)
            (VP (VB express)
              (S
                (NP
                  (NP (PRP$ my) (NN support))
                  (PP (IN for)
                    (NP (PRP$ your) (NN comment))))
                (ADJP (RBR more) (RB fully))))))))
    (. .)))

nsubj(wish-2, I-1)
root(ROOT-0, wish-2)
nsubj(express-5, I-3)
aux(express-5, could-4)
ccomp(wish-2, express-5)
poss(support-7, my-6)
nsubj(fully-12, support-7)
poss(comment-10, your-9)
prep_for(support-7, comment-10)
advmod(fully-12, more-11)
xcomp(express-5, fully-12)

(ROOT
  (S
    (NP (NNP Well))
    (VP (VBD said))
    (. .)))

nsubj(said-2, Well-1)
root(ROOT-0, said-2)

