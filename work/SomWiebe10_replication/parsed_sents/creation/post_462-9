(ROOT
  (S (CC And)
    (NP (PRP you))
    (VP (VBP know)
      (ADVP
        (ADVP (RB very) (RB well))
        (SBAR
          (WHADVP (WRB why))
          (S
            (NP (NNP ID) (NNS scientists))
            (VP (VBP have)
              (NP
                (NP (DT a) (JJ hard) (NN time))
                (VP (VBG getting)
                  (NP
                    (NP (PRP$ their) (NN research))
                    (VP (VBN published)
                      (PP (IN in)
                        (NP (NN peer) (NNS journals))))))))))))
    (. .)))

cc(know-3, And-1)
nsubj(know-3, you-2)
root(ROOT-0, know-3)
advmod(well-5, very-4)
advmod(know-3, well-5)
advmod(have-9, why-6)
nn(scientists-8, ID-7)
nsubj(have-9, scientists-8)
dep(well-5, have-9)
det(time-12, a-10)
amod(time-12, hard-11)
dobj(have-9, time-12)
partmod(time-12, getting-13)
poss(research-15, their-14)
dobj(getting-13, research-15)
partmod(research-15, published-16)
nn(journals-19, peer-18)
