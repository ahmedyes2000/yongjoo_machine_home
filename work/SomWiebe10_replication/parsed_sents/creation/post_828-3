            (, ,))
          (VP (VBZ is)
            (ADJP (JJ ludicrous))))))
    (. .)))

nsubj('s-2, That-1)
root(ROOT-0, 's-2)
advmod(ludicrous-27, why-3)
det(depiction-5, the-4)
