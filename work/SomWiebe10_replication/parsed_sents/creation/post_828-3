(ROOT
  (S
    (NP (DT That))
    (VP (VBZ 's)
      (SBAR
        (WHADVP (WRB why))
        (S
          (NP
            (NP (DT the) (NN depiction))
            (PP (IN of)
              (NP
                (NP (NN life))
                (ADVP (RB prior)
                  (PP (TO to)
                    (NP
                      (NP (DT the) (NN Fall))
                      (PP (IN of)
                        (NP (NN Man))))))))
            (, ,)
            (PP (IN with)
              (NP
                (NP
                  (NP (DT no) (NNS animals))
