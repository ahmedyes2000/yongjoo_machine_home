(ROOT
  (S
    (NP (PRP I))
    (VP (VBP am) (RB not)
      (ADJP (JJ sure))
      (SBAR (IN if)
        (S
          (NP (DT the) (NNP NCSE))
          (VP (VBZ argues)
            (SBAR (IN that)
              (S
                (NP (NNP ID))
                (VP (VBZ is)
                  (ADJP (JJ unscientific))
                  (SBAR (RB just) (IN because)
                    (S
                      (NP (PRP it))
                      (VP (VBZ is)
                        (ADVP (RB religiously))
                        (VP (VBN motivated))))))))))))
    (. .)))

nsubj(sure-4, I-1)
cop(sure-4, am-2)
neg(sure-4, not-3)
root(ROOT-0, sure-4)
mark(argues-8, if-5)
det(NCSE-7, the-6)
nsubj(argues-8, NCSE-7)
advcl(sure-4, argues-8)
complm(unscientific-12, that-9)
nsubj(unscientific-12, ID-10)
cop(unscientific-12, is-11)
ccomp(argues-8, unscientific-12)
advmod(motivated-18, just-13)
dep(motivated-18, because-14)
nsubjpass(motivated-18, it-15)
auxpass(motivated-18, is-16)
advmod(motivated-18, religiously-17)
ccomp(unscientific-12, motivated-18)

