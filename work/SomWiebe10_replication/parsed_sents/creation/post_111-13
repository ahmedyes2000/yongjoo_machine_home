(ROOT
  (S
    (ADVP (RB Thus))
    (NP (DT the) (NNP Bible))
    (VP (MD can)
      (VP (VB be)
        (VP (VBN used)
          (S
            (VP (TO to)
              (VP (VB determine)
                (NP (JJ new) (NNS theories))
                (ADVP (RB as) (RB well))))))))
    (. .)))

advmod(used-6, Thus-1)
det(Bible-3, the-2)
nsubjpass(used-6, Bible-3)
aux(used-6, can-4)
auxpass(used-6, be-5)
root(ROOT-0, used-6)
aux(determine-8, to-7)
xcomp(used-6, determine-8)
amod(theories-10, new-9)
dobj(determine-8, theories-10)
advmod(well-12, as-11)
mwe(well-12, as-11)
advmod(determine-8, well-12)

