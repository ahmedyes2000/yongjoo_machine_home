(ROOT
  (S (CC But)
    (PP (IN as)
      (NP (DT a) (NN science)))
    (NP (NNP ID))
    (VP
      (ADVP (RB simply))
      (VBZ provides)
      (NP (DT no) (NN solution)))
    (. .)))

cc(provides-7, But-1)
det(science-4, a-3)
prep_as(provides-7, science-4)
nsubj(provides-7, ID-5)
advmod(provides-7, simply-6)
root(ROOT-0, provides-7)
det(solution-9, no-8)
dobj(provides-7, solution-9)

