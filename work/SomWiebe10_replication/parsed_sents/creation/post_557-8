(ROOT
  (FRAG
    (ADVP (RB Perhaps))
    (SBAR
      (PP (IN as)
        (NP (DT a) (NN theology) (RB yes)))
      (, ,) (IN although)
      (S
        (NP (PRP it))
        (VP (VBZ raises)
          (NP (DT some) (JJ significant) (NNS questions))
          (ADVP (RB here) (RB too)))))
    (. .)))

root(ROOT-0, Perhaps-1)
dep(raises-9, as-2)
det(theology-4, a-3)
pobj(as-2, theology-4)
advmod(theology-4, yes-5)
dep(raises-9, although-7)
nsubj(raises-9, it-8)
dep(Perhaps-1, raises-9)
det(questions-12, some-10)
amod(questions-12, significant-11)
dobj(raises-9, questions-12)
advmod(too-14, here-13)
advmod(raises-9, too-14)

