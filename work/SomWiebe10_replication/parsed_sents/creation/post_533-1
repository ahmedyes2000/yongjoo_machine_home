(ROOT
  (S
    (PP (IN On)
      (NP (DT the) (NN contrary)))
    (, ,)
    (NP (PRP I))
    (VP (VBP have)
      (VP (VBN been)
        (VP (VBG asking)
          (NP (PRP you))
          (NP (JJ several) (NNS times))
          (SBAR
            (WHADVP
              (ADVP (RB now) (: :))
              (WRB How))
            (S
              (VP (VBZ does)
                (SBAR
                  (S
                    (NP (NNP ID))
                    (VP (VBZ explains)
                      (NP (DT the) (JJ bacterial) (NN flagella)))))))))))
    (. .)))

det(contrary-3, the-2)
prep_on(asking-8, contrary-3)
nsubj(asking-8, I-5)
aux(asking-8, have-6)
aux(asking-8, been-7)
root(ROOT-0, asking-8)
iobj(asking-8, you-9)
amod(times-11, several-10)
dobj(asking-8, times-11)
advmod(How-14, now-12)
advmod(does-15, How-14)
dep(asking-8, does-15)
nsubj(explains-17, ID-16)
ccomp(does-15, explains-17)
det(flagella-20, the-18)
amod(flagella-20, bacterial-19)
dobj(explains-17, flagella-20)

