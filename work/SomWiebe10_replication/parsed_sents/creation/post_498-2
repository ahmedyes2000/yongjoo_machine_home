  (S
    (SBAR (IN Once)
      (S
        (NP (PRP we))
        (VP (VBP agree)
          (PP (IN on)
            (NP (DT this) (JJ simple) (NN fact))))))
    (, ,)
    (NP (PRP we))
    (VP (MD can)
      (VP (VB see)
        (SBAR (IN if)
          (S
            (NP
              (NP (DT the) (NNP Judge) (POS 's))
              (NN ruling))
            (VP (VBD had)
              (NP (DT a) (JJ solid) (NN foundation)))))))
    (. .)))

