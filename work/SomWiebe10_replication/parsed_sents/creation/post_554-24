(ROOT
  (S ('' '')
    (PP (RB rather) (IN than)
      (S
        (VP (VB answer)
          (NP (PRP it)))))
    (VP (VBZ 's)
      (NP (NNS claims))
      (ADVP (RB rationally))
      (, ,)
      (SBAR (IN because)
        (S
          (NP (EX there))
          (VP (VBP are)
            (NP (DT no) (NNS answers))))))
    (. .)))

advmod(claims-7, rather-2)
prepc_than(claims-7, answer-4)
dobj(answer-4, it-5)
cop(claims-7, 's-6)
root(ROOT-0, claims-7)
advmod(claims-7, rationally-8)
mark(are-12, because-10)
expl(are-12, there-11)
advcl(claims-7, are-12)
det(answers-14, no-13)
nsubj(are-12, answers-14)

