(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (NN desire))
        (VP (VBN guided)
          (NP (PRP$ my) (NNS beliefs)))))
    (, ,)
    (NP (PRP I))
    (VP (MD would)
      (VP (VB be)
        (UCP (RB not)
          (ADVP (RB only)
            (NP (DT a) (NN supporter))
            (PP (IN of)
              (NP (NNP Intelligent) (NNP Design))))
          (, ,)
          (CC but)
          (NP
            (NP (DT a) (JJ young) (NNP Earth) (NNP Creationist))
            (CC and)
            (NP (DT a) (JJ fundamentalist) (NNP Christian))))))
    (. .)))

mark(guided-3, If-1)
nsubj(guided-3, desire-2)
advcl(be-9, guided-3)
poss(beliefs-5, my-4)
dobj(guided-3, beliefs-5)
nsubj(be-9, I-7)
aux(be-9, would-8)
root(ROOT-0, be-9)
dep(be-9, not-10)
