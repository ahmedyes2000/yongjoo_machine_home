(ROOT
  (S
    (ADVP (RB Possibly))
    (NP (NNP God))
    (VP (VBD designed)
      (NP (NNS things))
      (S
        (VP (TO to)
          (VP (VB include)
            (NP (DT a) (JJ long) (JJ evolutionary) (NN process))))))
    (. ?)))

advmod(designed-3, Possibly-1)
nsubj(designed-3, God-2)
root(ROOT-0, designed-3)
dobj(designed-3, things-4)
aux(include-6, to-5)
xcomp(designed-3, include-6)
det(process-10, a-7)
amod(process-10, long-8)
amod(process-10, evolutionary-9)
dobj(include-6, process-10)

