(ROOT
  (S
    (NP (PRP You))
    (VP (MD ca) (RB n't)
      (VP (VB argue)
        (NP
          (NP (NN w) (NN \/) (NN science))
          (, ,)
          (ADJP (JJ sorry)))))
    (. .)))

nsubj(argue-4, You-1)
aux(argue-4, ca-2)
neg(argue-4, n't-3)
root(ROOT-0, argue-4)
nn(science-7, w-5)
nn(science-7, \/-6)
dobj(argue-4, science-7)
amod(science-7, sorry-9)

