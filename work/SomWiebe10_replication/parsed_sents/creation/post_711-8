(ROOT
  (S
    (NP (PRP One))
    (VP (MD can)
      (ADVP (RB also))
      (VP (VB ask)
        (SBAR
          (WHADVP (WRB why))
          (S
            (NP (DT the) (NNS levels))
            (VP (VBD cluster)
              (NP (NNS creatures))
              (SBAR (IN as)
                (S
                  (NP (PRP they))
                  (VP (VBP do)))))))))
    (. .)))

nsubj(ask-4, One-1)
aux(ask-4, can-2)
advmod(ask-4, also-3)
root(ROOT-0, ask-4)
advmod(cluster-8, why-5)
det(levels-7, the-6)
nsubj(cluster-8, levels-7)
advcl(ask-4, cluster-8)
dobj(cluster-8, creatures-9)
mark(do-12, as-10)
nsubj(do-12, they-11)
advcl(cluster-8, do-12)

