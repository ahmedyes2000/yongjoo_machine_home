(ROOT
  (S
    (ADVP (RB First))
    (, ,)
    (NP (EX there))
    (VP (VBZ is)
      (NP
        (NP (DT no) (JJ such) (NN thing))
        (PP (IN as)
          (NP (DT an) (`` ``) (NN evolutionist) ('' '')))))
    (. .)))

advmod(is-4, First-1)
expl(is-4, there-3)
root(ROOT-0, is-4)
det(thing-7, no-5)
amod(thing-7, such-6)
nsubj(is-4, thing-7)
det(evolutionist-11, an-9)
prep_as(thing-7, evolutionist-11)

