(ROOT
  (S
    (S
      (NP (NNP Behe))
      (VP (VBZ offers)
        (S
          (NP (DT no) (NNP DATA))
          (VP (TO to)
            (VP (VB support)
              (NP (PRP$ his) (NNS hypothesis)))))))
    (: ;)
    (S
      (NP (WDT which))
      (VP (VBP make)
        (NP (DT no) (NNS predictions))))
    (. .)))

nsubj(offers-2, Behe-1)
root(ROOT-0, offers-2)
det(DATA-4, no-3)
nsubj(support-6, DATA-4)
aux(support-6, to-5)
xcomp(offers-2, support-6)
poss(hypothesis-8, his-7)
dobj(support-6, hypothesis-8)
nsubj(make-11, which-10)
parataxis(offers-2, make-11)
det(predictions-13, no-12)
dobj(make-11, predictions-13)

