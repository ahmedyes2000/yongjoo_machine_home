(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ 's)
      (NP
        (NP (NN kind))
        (PP (IN of)
          (PP (IN like)
            (NP (DT a) (NN pendulum))))))
    (. .)))

nsubj(kind-3, It-1)
cop(kind-3, 's-2)
root(ROOT-0, kind-3)
prep(kind-3, of-4)
pcomp(of-4, like-5)
det(pendulum-7, a-6)
prep_like(of-4, pendulum-7)

