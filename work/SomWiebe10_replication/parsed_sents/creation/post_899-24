(ROOT
  (S
    (NP (EX There))
    (VP (VBP are)
      (NP
        (NP (NNS thousands))
        (PP (IN of)
          (NP (NNS scientists))))
      (PP (DT all) (IN over)
        (NP (DT the) (NN globe)))
      (PP (IN from)
        (NP
          (NP (DT every) (NN creed) (, ,) (NN ethnicity) (, ,)
            (CC and)
            (NN nation))
          (VP (VBG relying)
            (PP (IN upon)
              (NP
                (NP (DT the) (NN theory))
                (PP (IN in)
                  (NP (PRP$ their) (JJ daily) (NN work)))))))))
    (. .)))

expl(are-2, There-1)
root(ROOT-0, are-2)
nsubj(are-2, thousands-3)
prep_of(thousands-3, scientists-5)
dep(are-2, all-6)
det(globe-9, the-8)
prep_over(are-2, globe-9)
det(creed-12, every-11)
prep_from(are-2, creed-12)
prep_from(are-2, ethnicity-14)
conj_and(creed-12, ethnicity-14)
prep_from(are-2, nation-17)
conj_and(creed-12, nation-17)
partmod(creed-12, relying-18)
det(theory-21, the-20)
prep_upon(relying-18, theory-21)
poss(work-25, their-23)
amod(work-25, daily-24)
prep_in(theory-21, work-25)

