(ROOT
  (S
    (VP (VB Remember)
      (SBAR (IN that)
        (S
          (NP (NNP Darwin))
          (VP (VBD published)
            (NP (PRP$ his) (NN theory))
            (PP (IN before)
              (NP
                (NP (DT the) (NN discovery))
                (PP (IN of)
                  (NP (NNP DNA)))))))))
    (. .)))

root(ROOT-0, Remember-1)
complm(published-4, that-2)
nsubj(published-4, Darwin-3)
ccomp(Remember-1, published-4)
poss(theory-6, his-5)
dobj(published-4, theory-6)
det(discovery-9, the-8)
prep_before(published-4, discovery-9)
prep_of(discovery-9, DNA-11)

