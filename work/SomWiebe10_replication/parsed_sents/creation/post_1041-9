(ROOT
  (S
    (SBAR
      (WHADVP (WRB When))
      (S
        (NP (CD two) (JJ continental) (NNS plates))
        (VP (VBP collide))))
    (, ,)
    (NP (NNS mountains))
    (VP (VBP form))
    (. .)))

advmod(collide-5, When-1)
num(plates-4, two-2)
amod(plates-4, continental-3)
nsubj(collide-5, plates-4)
advcl(form-8, collide-5)
nsubj(form-8, mountains-7)
root(ROOT-0, form-8)

