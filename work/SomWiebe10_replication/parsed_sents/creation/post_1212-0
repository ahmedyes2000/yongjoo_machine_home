(ROOT
  (S
    (NP (NNP Darwin))
    (VP
      (VP (VBD observed)
        (NP (NNS species)))
      (CC and)
      (VP (VBD wrote)
        (NP (PRP it))
        (PP (IN down)
          (NP (NN evidence)))))
    (. !)))

nsubj(observed-2, Darwin-1)
nsubj(wrote-5, Darwin-1)
root(ROOT-0, observed-2)
dobj(observed-2, species-3)
conj_and(observed-2, wrote-5)
dobj(wrote-5, it-6)
prep_down(wrote-5, evidence-8)

