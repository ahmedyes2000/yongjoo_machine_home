(ROOT
  (S ('' '')
    (S
      (NP (DT This) (NN passage))
      (VP (VBZ suggests)
        (ADVP (RB otherwise))))
    (, ,)
    (NP (PRP it))
    (VP (VBZ states)
      (SBAR (IN that) (, ,) (`` ``)
        (S
          (NP (DT Some) (NN master) (NN intellect))
          (VP (VBZ is)
            (NP
              (NP (DT the) (NN creator))
              (PP (IN of)
                (NP (NN life))))))))
    (. .)))

det(passage-3, This-2)
nsubj(suggests-4, passage-3)
ccomp(states-8, suggests-4)
advmod(suggests-4, otherwise-5)
nsubj(states-8, it-7)
root(ROOT-0, states-8)
complm(creator-17, that-9)
det(intellect-14, Some-12)
nn(intellect-14, master-13)
nsubj(creator-17, intellect-14)
cop(creator-17, is-15)
det(creator-17, the-16)
ccomp(states-8, creator-17)
prep_of(creator-17, life-19)

