(ROOT
  (S
    (S
      (NP (DT That))
      (VP (VBG being)
        (VP (VBD said))))
    (, ,)
    (NP (PRP I))
    (ADVP (RB certainly))
    (VP (VBP agree)
      (SBAR (IN that)
        (S
          (NP (PRP they))
          (VP (VBP do)
            (VP (VB say)
              (NP
                (NP (DT a) (NN lot))
                (PP (IN about)
                  (NP (PRP$ our) (JJ basic) (JJ human) (NN nature)))))))))
    (. .)))

nsubjpass(said-3, That-1)
auxpass(said-3, being-2)
partmod(agree-7, said-3)
nsubj(agree-7, I-5)
advmod(agree-7, certainly-6)
root(ROOT-0, agree-7)
complm(say-11, that-8)
