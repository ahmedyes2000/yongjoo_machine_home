(ROOT
  (S
    (SBAR (IN If)
      (FRAG
        (ADVP (RB so))))
    (, ,)
    (NP (DT these) (JJ childish) (NNS arguments))
    (VP (VBP are)
      (NP (DT the) (JJ wrong) (NN approach)))
    (. .)))

mark(so-2, If-1)
advcl(approach-10, so-2)
det(arguments-6, these-4)
amod(arguments-6, childish-5)
nsubj(approach-10, arguments-6)
cop(approach-10, are-7)
det(approach-10, the-8)
amod(approach-10, wrong-9)
root(ROOT-0, approach-10)

