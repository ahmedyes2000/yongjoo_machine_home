(ROOT
  (S (CC And)
    (NP (DT no) (JJ real) (NN scientist))
    (VP (VBZ claims)
      (SBAR (IN that)
        (S
          (NP (DT these) (NNS discoveries))
          (VP (VBP are)
            (NP
              (NP (DT a) (NN problem))
              (PP (IN for)
                (NP (NN evolution))))))))
    (. .)))

cc(claims-5, And-1)
det(scientist-4, no-2)
amod(scientist-4, real-3)
nsubj(claims-5, scientist-4)
root(ROOT-0, claims-5)
complm(problem-11, that-6)
det(discoveries-8, these-7)
nsubj(problem-11, discoveries-8)
cop(problem-11, are-9)
det(problem-11, a-10)
ccomp(claims-5, problem-11)
prep_for(problem-11, evolution-13)

