(ROOT
  (S
    (NP (DT The) (RBS most) (JJ significant))
    (VP (VBP are)
      (NP (DT the) (JJ following) (NNS claims))
      (NP (CD .1)))
    (. .)))

det(significant-3, The-1)
advmod(significant-3, most-2)
nsubj(claims-7, significant-3)
cop(claims-7, are-4)
det(claims-7, the-5)
amod(claims-7, following-6)
root(ROOT-0, claims-7)
attr(claims-7, .1-8)

