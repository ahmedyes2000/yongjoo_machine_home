(ROOT
  (S
    (NP
      (NP (PRP$ Our) (JJ main) (NN concern))
      (PP (IN about)
        (NP (NNP Expelled))))
    (VP (VBZ is)
      (SBAR (IN that)
        (S
          (NP (PRP it))
          (VP (VBZ paints)
            (NP (DT a)
              (ADJP (JJ distorted))
              (NN picture))))))
    (. .)))

poss(concern-3, Our-1)
amod(concern-3, main-2)
nsubj(is-6, concern-3)
prep_about(concern-3, Expelled-5)
root(ROOT-0, is-6)
complm(paints-9, that-7)
nsubj(paints-9, it-8)
ccomp(is-6, paints-9)
det(picture-12, a-10)
amod(picture-12, distorted-11)
dobj(paints-9, picture-12)

