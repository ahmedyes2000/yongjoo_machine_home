(ROOT
  (S (CC And)
    (PP (IN that)
      (NP (PRP$ my) (NNS friends)))
    (, ,)
    (VP (VBZ is)
      (NP
        (NP (DT the) (NN rest))
        (PP (IN of)
          (NP (DT the) (NN story)))))
    (. .)))

cc(rest-8, And-1)
poss(friends-4, my-3)
prep_that(rest-8, friends-4)
cop(rest-8, is-6)
det(rest-8, the-7)
root(ROOT-0, rest-8)
