(ROOT
  (S
    (SBAR
      (WHNP (WP What))
      (S
        (NP (PRP I))
        (VP (VBP am)
          (VP (VBG saying)))))
    (VP (VBZ is)
      (SBAR (IN if)
        (S
          (NP (PRP we))
          (VP (VBP teach)
            (NP
              (NP (NNS laws))
              (SBAR
                (S
                  (NP
                    (NP (CD .5) (NNS laws))
                    (PP (IN of)
                      (NP (DT the) (NN land))))
                  (VP (VBP are)
                    (PP (IN from)
                      (NP (DT the) (NNP Bible)))))))))))
    (. .)))

dobj(saying-4, What-1)
nsubj(saying-4, I-2)
aux(saying-4, am-3)
csubj(is-5, saying-4)
root(ROOT-0, is-5)
mark(teach-8, if-6)
nsubj(teach-8, we-7)
advcl(is-5, teach-8)
dobj(teach-8, laws-9)
num(laws-11, .5-10)
nsubj(are-15, laws-11)
det(land-14, the-13)
prep_of(laws-11, land-14)
rcmod(laws-9, are-15)
det(Bible-18, the-17)
prep_from(are-15, Bible-18)

(ROOT
  (S (IN So)
    (NP (PRP you))
    (VP (VBP are)
      (VP (VBG teaching)
        (NP (NN religion))))
    (. .)))

dep(teaching-4, So-1)
nsubj(teaching-4, you-2)
aux(teaching-4, are-3)
root(ROOT-0, teaching-4)
dobj(teaching-4, religion-5)

