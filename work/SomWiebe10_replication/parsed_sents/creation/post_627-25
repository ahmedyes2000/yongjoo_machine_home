(ROOT
  (S
    (NP (NNP Theology)
      (CC and)
      (NNP Science))
    (VP
      (VP (MD should)
        (VP (VB talk)
          (ADVP (RB again))))
      (, ,)
      (CC neither)
      (VP (VBZ fearing)))
    (. .)))

nsubj(talk-5, Theology-1)
conj_and(Theology-1, Science-3)
nsubj(talk-5, Science-3)
aux(talk-5, should-4)
root(ROOT-0, talk-5)
advmod(talk-5, again-6)
dep(talk-5, neither-8)
conj(talk-5, fearing-9)

