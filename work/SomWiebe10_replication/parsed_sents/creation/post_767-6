(ROOT
  (S
    (NP (JJ Such) (NNS discoveries))
    (VP (VBP bring)
      (NP
        (NP (NNS lots))
        (PP (IN of)
          (NP (NN grant) (NN money)))))
    (. .)))

amod(discoveries-2, Such-1)
nsubj(bring-3, discoveries-2)
root(ROOT-0, bring-3)
dobj(bring-3, lots-4)
nn(money-7, grant-6)
prep_of(lots-4, money-7)

