(ROOT
  (S
    (ADVP (RB Furthermore))
    (, ,)
    (NP
      (NP (NN understanding))
      (SBAR
        (WHADVP (WRB how))
        (S
          (NP (NN evolution))
          (VP (VBZ works)
            (SBAR (IN as)
              (S
                (NP (DT a) (NN theory))
                (VP (VBZ helps))))))))
    (VP (VBP teach)
      (NP (NNS students))
      (PP (IN about)
        (NP
          (NP (DT the) (NN nature))
          (PP (IN of)
            (NP (JJ scientific) (NN method)))))
      (ADVP (DT a) (NN matter)
        (SBAR
          (WHPP (IN on)
            (WHNP (WDT which)))
          (S
            (NP (NNS creationists))
            (VP (VBP are)
              (ADJP (RB notoriously) (JJ shaky)))))))
    (. .)))

advmod(teach-11, Furthermore-1)
nsubj(teach-11, understanding-3)
advmod(works-6, how-4)
nsubj(works-6, evolution-5)
dep(understanding-3, works-6)
mark(helps-10, as-7)
det(theory-9, a-8)
nsubj(helps-10, theory-9)
advcl(works-6, helps-10)
root(ROOT-0, teach-11)
dobj(teach-11, students-12)
det(nature-15, the-14)
prep_about(teach-11, nature-15)
amod(method-18, scientific-17)
prep_of(nature-15, method-18)
dep(matter-20, a-19)
advmod(teach-11, matter-20)
dep(shaky-26, on-21)
pobj(on-21, which-22)
nsubj(shaky-26, creationists-23)
cop(shaky-26, are-24)
advmod(shaky-26, notoriously-25)
dep(matter-20, shaky-26)

