(ROOT
  (S
    (SBAR (IN If)
      (FRAG
        (ADVP (RB so))))
    (, ,)
    (NP (NNP AiG))
    (VP (VBZ needs)
      (S
        (VP (TO to)
          (VP (VB pick)
            (NP (PRP$ their) (NNS battles))
            (ADVP (RBR more))
            (ADVP (RB wisely))))))
    (. .)))

mark(so-2, If-1)
advcl(needs-5, so-2)
nsubj(needs-5, AiG-4)
xsubj(pick-7, AiG-4)
root(ROOT-0, needs-5)
aux(pick-7, to-6)
xcomp(needs-5, pick-7)
poss(battles-9, their-8)
dobj(pick-7, battles-9)
advmod(pick-7, more-10)
advmod(pick-7, wisely-11)

