(ROOT
  (S
    (PP (IN Of)
      (NP (NN course)))
    (, ,)
    (SBAR
      (WHADVP (WRB how))
      (S
        (NP (NN life))
        (ADVP (RB first))
        (VP (VBD appeared))))
    (VP (VBZ is)
      (NP (DT a) (JJ separate) (NN issue)))
    (. .)))

prep_of(issue-11, course-2)
advmod(appeared-7, how-4)
nsubj(appeared-7, life-5)
advmod(appeared-7, first-6)
csubj(issue-11, appeared-7)
cop(issue-11, is-8)
det(issue-11, a-9)
amod(issue-11, separate-10)
root(ROOT-0, issue-11)

