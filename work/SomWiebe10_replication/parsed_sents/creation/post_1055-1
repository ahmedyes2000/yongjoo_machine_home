(ROOT
  (S
    (NP (DT No) (JJ other) (NN conclusion))
    (VP (MD can)
      (VP (VB be)
        (ADJP (RB validly) (JJ deduced))))
    (. .)))

det(conclusion-3, No-1)
amod(conclusion-3, other-2)
nsubj(deduced-7, conclusion-3)
aux(deduced-7, can-4)
cop(deduced-7, be-5)
advmod(deduced-7, validly-6)
root(ROOT-0, deduced-7)

