(ROOT
  (S (CC And)
    (NP (RB yes))
    (, ,)
    (NP (PRP I))
    (VP (VBP agree)
      (SBAR (IN that)
        (S
          (NP (NN design))
          (VP (VBZ has)
            (VP (VBN been)
              (ADJP (JJ successful))
              (PP (IN in)
                (NP (DT the) (JJ historical) (NNS sciences))))))))
    (. .)))

cc(agree-5, And-1)
nsubj(agree-5, yes-2)
