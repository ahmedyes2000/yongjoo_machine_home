(ROOT
  (S
    (NP
      (NP (NNP Sorry))
      (PP (CC but)
        (NP
          (NP (PRP$ your) (NN ignorance))
          (PP (IN of)
            (NP (JJ legal) (NNS procedures))))))
    (VP (VBZ has)
      (VP (VBN caused)
        (S
          (NP (PRP you))
          (VP (TO to)
            (VP (VB make)
              (NP (JJ foolish) (NNS arguments)))))))
    (. .)))

nsubj(caused-9, Sorry-1)
prep(Sorry-1, but-2)
poss(ignorance-4, your-3)
dep(but-2, ignorance-4)
amod(procedures-7, legal-6)
prep_of(ignorance-4, procedures-7)
aux(caused-9, has-8)
root(ROOT-0, caused-9)
nsubj(make-12, you-10)
aux(make-12, to-11)
xcomp(caused-9, make-12)
amod(arguments-14, foolish-13)
dobj(make-12, arguments-14)

