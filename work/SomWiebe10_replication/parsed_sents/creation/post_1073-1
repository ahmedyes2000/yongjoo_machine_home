(ROOT
  (S
    (SBAR
      (WHADVP (WRB How))
      (S
        (NP
          (NP (NN anyone))
          (PP (IN with)
            (NP (PDT half) (DT a) (NN brain))))
        (VP (MD can)
          (VP (VB accept)
            (NP (NN creationism))))))
    (VP (VBZ is)
      (PP (IN beyond)
        (NP (PRP me))))
    (. .)))

advmod(accept-8, How-1)
nsubj(accept-8, anyone-2)
predet(brain-6, half-4)
det(brain-6, a-5)
prep_with(anyone-2, brain-6)
aux(accept-8, can-7)
csubj(is-10, accept-8)
dobj(accept-8, creationism-9)
root(ROOT-0, is-10)
prep_beyond(is-10, me-12)

