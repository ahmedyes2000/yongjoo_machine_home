  (S
    (ADVP (RB Essentially))
    (ADVP (RB then))
    (, ,)
    (NP (DT the) (NN e.) (NNS coli))
    (VP (VBD had)
      (NP
        (QP (CD 1.2) (CD million))
        (NNS years))
      (S
        (VP (TO to)
          (VP (VB change)
            (PP (IN into)
              (NP
                (NP (DT a) (JJ new) (NN species))
                (PP (IN via)
                  (NP
                    (NP (JJ natural) (NN selection))
                    (CC and)
                    (NP (JJ random) (NN mutation))))))))))
    (. .)))

