(ROOT
  (S
    (NP (PRP I))
    (ADVP (RB just))
    (VP (MD will)
      (ADVP (RB NEVER))
      (VP (VB allow)
        (S
          (NP (PRP myself))
          (VP (TO to)
            (VP (VB believe)
              (PP (IN in)
                (NP
                  (NP (NN anything))
                  (ADJP (JJ different)
                    (PP (IN than)
                      (NP (DT a) (NN creator)))))))))))
    (. .)))

