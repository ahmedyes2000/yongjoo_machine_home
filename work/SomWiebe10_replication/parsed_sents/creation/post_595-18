(ROOT
  (S
    (NP (PRP You))
    (VP (VBP say)
      (SBAR
        (S
          (NP (PRP it))
          (VP (VBZ s)
            (NP (DT a) (NN fact))
            (SBAR (IN that)
              (S
                (NP (NN regularity)
                  (CC and)
                  (NN chance) (NNS processes))
                (VP (MD can)
                  (VP (VB increase)
                    (NP (NN information)
                      (CC and)
                      (NN complexity))))))))))
    (. .)))

nsubj(say-2, You-1)
root(ROOT-0, say-2)
nsubj(fact-6, it-3)
cop(fact-6, s-4)
