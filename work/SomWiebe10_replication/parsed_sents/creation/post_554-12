(ROOT
  (S ('' '')
    (ADVP (RB HOWEVER))
    (, ,)
    (NP (EX there))
    (VP (VBZ is)
      (NP (DT no) (NN reason))
      (ADVP (RB whatsoever))
      (SBAR (IN that)
        (S
          (NP (EX there))
          (VP (MD can) (RB not)
            (VP (VB be)
              (NP
                (NP (JJ fair) (NN debate))
                (PP (IN about)
                  (NP (PRP it)))))))))
    (. .)))

advmod(is-5, HOWEVER-2)
expl(is-5, there-4)
root(ROOT-0, is-5)
det(reason-7, no-6)
nsubj(is-5, reason-7)
advmod(is-5, whatsoever-8)
complm(debate-15, that-9)
expl(debate-15, there-10)
aux(debate-15, can-11)
neg(debate-15, not-12)
cop(debate-15, be-13)
amod(debate-15, fair-14)
ccomp(is-5, debate-15)
prep_about(debate-15, it-17)

