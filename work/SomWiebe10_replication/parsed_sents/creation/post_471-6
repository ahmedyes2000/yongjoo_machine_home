(ROOT
  (S
    (NP (DT This))
    (VP (VBZ is)
      (ADJP (RB even) (RBR more) (JJ difficult)
        (S
          (VP (TO to)
            (VP (VB understand)
              (SBAR (IN if)
                (S
                  (NP (DT the) (NN person))
                  (VP (VBZ is) (RB not)
                    (NP (DT a) (JJ true) (NN naturalist))))))))))
    (. .)))

nsubj(difficult-5, This-1)
cop(difficult-5, is-2)
advmod(difficult-5, even-3)
advmod(difficult-5, more-4)
root(ROOT-0, difficult-5)
aux(understand-7, to-6)
xcomp(difficult-5, understand-7)
mark(naturalist-15, if-8)
det(person-10, the-9)
nsubj(naturalist-15, person-10)
cop(naturalist-15, is-11)
neg(naturalist-15, not-12)
det(naturalist-15, a-13)
amod(naturalist-15, true-14)
advcl(understand-7, naturalist-15)

