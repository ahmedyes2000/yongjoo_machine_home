(ROOT
  (S
    (ADVP (IN So) (RB far))
    (NP (NNP ID))
    (VP (VBZ has)
      (VP (VBN failed)
        (ADVP (RB miserably)
          (PP (IN in)
            (S
              (VP (VBG presenting)
                (NP (DT a) (JJ scientific) (NN argument))))))))
    (. .)))

advmod(failed-5, So-1)
advmod(So-1, far-2)
nsubj(failed-5, ID-3)
aux(failed-5, has-4)
root(ROOT-0, failed-5)
advmod(failed-5, miserably-6)
prepc_in(miserably-6, presenting-8)
det(argument-11, a-9)
amod(argument-11, scientific-10)
dobj(presenting-8, argument-11)

