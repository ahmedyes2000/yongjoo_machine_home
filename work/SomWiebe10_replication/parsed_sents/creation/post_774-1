(ROOT
  (S
    (NP (PRP I))
    (VP (VBP do) (RB n't)
      (VP (VB think)
        (SBAR
          (S
            (NP (NNP AiG))
            (VP (MD would)
              (VP (VB be)
                (ADJP (JJ happy))))))))
    (. !)))

nsubj(think-4, I-1)
aux(think-4, do-2)
neg(think-4, n't-3)
root(ROOT-0, think-4)
nsubj(happy-8, AiG-5)
aux(happy-8, would-6)
cop(happy-8, be-7)
ccomp(think-4, happy-8)

