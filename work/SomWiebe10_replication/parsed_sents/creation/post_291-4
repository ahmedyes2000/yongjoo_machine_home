(ROOT
  (S
    (NP (PRP He))
    (VP (VBZ offers)
      (NP
        (NP (DT no) (NN evidence))
        (, ,)
        (NP (DT no) (NNS data))
        (, ,)
        (CC and)
        (NP
          (NP (DT no) (NN sembelence))
          (PP (IN of)
            (NP (DT a) (JJ coherent) (NN argument))))))
    (. .)))

nsubj(offers-2, He-1)
root(ROOT-0, offers-2)
det(evidence-4, no-3)
dobj(offers-2, evidence-4)
det(data-7, no-6)
dobj(offers-2, data-7)
conj_and(evidence-4, data-7)
det(sembelence-11, no-10)
dobj(offers-2, sembelence-11)
conj_and(evidence-4, sembelence-11)
det(argument-15, a-13)
