(ROOT
  (S
    (ADVP (RB Firstly))
    (, ,)
    (NP (EX there))
    (VP (VBZ 's)
      (NP (JJ concrete) (NN evidence))
      (SBAR (IN that)
        (S
          (NP (PRP we))
          (VP (VBP have)
            (VP (VBN evolved))))))
    (. .)))

advmod('s-4, Firstly-1)
expl('s-4, there-3)
root(ROOT-0, 's-4)
amod(evidence-6, concrete-5)
nsubj('s-4, evidence-6)
complm(evolved-10, that-7)
nsubj(evolved-10, we-8)
aux(evolved-10, have-9)
ccomp('s-4, evolved-10)

