(ROOT
  (S
    (NP
      (NP (DT Any) (NN way))
      (SBAR
        (S
          (NP (PRP you))
          (VP (VB flip)
            (NP (PRP it))))))
    (, ,)
    (NP (DT the) (JJ hard) (NN result))
    (VP (VBZ is)
      (SBAR (IN that)
        (S
          (NP (PRP I))
          (VP (VBP am)
            (ADVP (RB still))
            (NP (DT a) (JJ human) (NN being))))))
    (. .)))

det(way-2, Any-1)
nsubj(is-10, way-2)
nsubj(flip-4, you-3)
rcmod(way-2, flip-4)
dobj(flip-4, it-5)
det(result-9, the-7)
amod(result-9, hard-8)
nsubj(is-10, result-9)
root(ROOT-0, is-10)
complm(being-17, that-11)
nsubj(being-17, I-12)
cop(being-17, am-13)
advmod(being-17, still-14)
det(being-17, a-15)
amod(being-17, human-16)
ccomp(is-10, being-17)

