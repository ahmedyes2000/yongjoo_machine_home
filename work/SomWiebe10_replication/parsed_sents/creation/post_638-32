(ROOT
  (S ('' '')
    (NP (DT All))
    (ADVP (RB because) (RB there))
    (VP (VBZ exists)
      (NP
        (NP (DT a)
          (ADJP (RB well) (VBN established))
          (NN trail))
        (PP (IN of)
          (NP
            (NP (NN evidence))
            (VP (VBG linking)
              (NP (PRP them))
              (ADVP (DT all) (RB together)))))))
    (. .)))

nsubj(exists-5, All-2)
advmod(there-4, because-3)
advmod(exists-5, there-4)
root(ROOT-0, exists-5)
det(trail-9, a-6)
advmod(established-8, well-7)
amod(trail-9, established-8)
dobj(exists-5, trail-9)
prep_of(trail-9, evidence-11)
partmod(evidence-11, linking-12)
dobj(linking-12, them-13)
dep(together-15, all-14)
advmod(linking-12, together-15)

