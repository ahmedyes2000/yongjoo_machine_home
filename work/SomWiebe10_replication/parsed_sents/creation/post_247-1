(ROOT
  (S ('' '')
    (S (CC And)
      (NP (TO to))
      (VP (VBP think)))
    (IN that)
    (S
      (NP (PRP you))
      (VP (VBP accuse)
        (NP
          (NP (NNP US))
          (PP (IN of)
            (S
              (VP (VBG promoting)
                (NP (DT a) (JJ false) (NN dichotomy))))))))
    (. .)))

cc(think-4, And-2)
nsubj(think-4, to-3)
root(ROOT-0, think-4)
dep(think-4, that-5)
