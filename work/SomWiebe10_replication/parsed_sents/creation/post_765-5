(ROOT
  (S
    (NP (NNS Scientists))
    (VP
      (VP (VBD discarded)
        (NP (JJ such) (NNS ideas)))
      (CC and)
      (VP (VBN incorporated)
        (NP (NNS discoveries))
        (PP (IN from)
          (NP (NN biology)))
        (S
          (VP (TO to)
            (VP (VB create)
              (NP (DT the) (JJ modern) (JJ evolutionary) (NN synthesis)))))))
    (. .)))

nsubj(discarded-2, Scientists-1)
nsubj(incorporated-6, Scientists-1)
xsubj(create-11, Scientists-1)
root(ROOT-0, discarded-2)
amod(ideas-4, such-3)
dobj(discarded-2, ideas-4)
conj_and(discarded-2, incorporated-6)
dobj(incorporated-6, discoveries-7)
prep_from(incorporated-6, biology-9)
aux(create-11, to-10)
xcomp(incorporated-6, create-11)
det(synthesis-15, the-12)
amod(synthesis-15, modern-13)
amod(synthesis-15, evolutionary-14)
dobj(create-11, synthesis-15)

