(ROOT
  (S (`` ``)
    (NP (DT Some) (JJ foolish) (NNS men))
    (VP (VBP declare)
      (SBAR (IN that)
        (S
          (NP (DT a) (NNP Creator))
          (VP (VBD made)
            (NP (DT the) (NN world))))))
    (. .)))

det(men-4, Some-2)
amod(men-4, foolish-3)
nsubj(declare-5, men-4)
root(ROOT-0, declare-5)
complm(made-9, that-6)
det(Creator-8, a-7)
nsubj(made-9, Creator-8)
ccomp(declare-5, made-9)
det(world-11, the-10)
dobj(made-9, world-11)

