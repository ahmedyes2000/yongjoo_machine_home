(ROOT
  (S
    (NP (DT No) (NN favor))
    (VP (MD should)
      (VP (VB be)
        (VP (VBN given)
          (NP
            (NP (CD one) (NN way))
            (CC or)
            (NP (DT another))))))
    (. .)))

det(favor-2, No-1)
nsubjpass(given-5, favor-2)
aux(given-5, should-3)
auxpass(given-5, be-4)
root(ROOT-0, given-5)
num(way-7, one-6)
dobj(given-5, way-7)
dobj(given-5, another-9)
conj_or(way-7, another-9)

