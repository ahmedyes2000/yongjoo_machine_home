(ROOT
  (S
    (NP
      (NP (DT Another) (NN thing))
      (NP (PRP$ my) (NN son)))
    (VP (VBZ is)
      (VP (VBG going)
        (S
          (VP (TO to)
            (VP (VB hold)
              (PRT (RP over))
              (NP (PRP$ my) (NN head)))))))
    (. .)))

det(thing-2, Another-1)
nsubj(going-6, thing-2)
xsubj(hold-8, thing-2)
poss(son-4, my-3)
dep(thing-2, son-4)
aux(going-6, is-5)
root(ROOT-0, going-6)
aux(hold-8, to-7)
xcomp(going-6, hold-8)
prt(hold-8, over-9)
poss(head-11, my-10)
dobj(hold-8, head-11)

