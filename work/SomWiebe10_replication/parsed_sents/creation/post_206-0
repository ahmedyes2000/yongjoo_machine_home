(ROOT
  (S
    (SBAR
      (WHADVP (WRB When))
      (S
        (NP (PRP I))
        (VP (VBD grew)
          (PRT (RP up)))))
    (, ,)
    (NP (NN science))
    (VP (VBD was)
      (NP
        (NP (DT a) (JJ useful) (NN tool))
        (PP (IN for)
          (S
            (VP (VBG discovering)
              (NP (NNS things))
              (PP (IN about)
                (NP
                  (NP (DT the) (NN world))
                  (PP (IN around)
                    (NP (PRP us))))))))))
    (. .)))

advmod(grew-3, When-1)
nsubj(grew-3, I-2)
advcl(tool-10, grew-3)
prt(grew-3, up-4)
nsubj(tool-10, science-6)
cop(tool-10, was-7)
det(tool-10, a-8)
amod(tool-10, useful-9)
root(ROOT-0, tool-10)
prepc_for(tool-10, discovering-12)
dobj(discovering-12, things-13)
det(world-16, the-15)
prep_about(discovering-12, world-16)
prep_around(world-16, us-18)

