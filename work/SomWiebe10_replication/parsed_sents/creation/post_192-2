        (PP (IN for)
          (NP (NNP Luskin)))
        (S
          (VP (TO to)
            (VP (VB explain)
              (SBAR
                (WHADVP (WRB why))
                (S
                  (NP (PRP he))
                  (VP (VBZ thinks)
                    (SBAR (IN that)
                      (S
                        (NP (NNP ID))
                        (VP (MD should)
                          (VP (VB predict)
                            (NP (DT an) (JJ abrupt))
                            (PP (RB rather) (IN than)
                              (NP
                                (NP (DT a) (JJ non-abrupt) (NN appearance))
                                (PP (IN of)
                                  (NP (NNS species)))))))))))))))))
    (. .)))

nsubj(like-4, I-2)
xsubj(explain-8, I-2)
aux(like-4, would-3)
root(ROOT-0, like-4)
