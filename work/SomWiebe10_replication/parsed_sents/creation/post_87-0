(ROOT
  (S
    (NP (DT This))
    (VP
      (VP (MD will))
      (CC and)
      (VP (VBZ is)
        (ADVP (RB already))
        (VP (VBG descending)
          (PP (IN into)
            (NP
              (NP (DT a) (NN debate))
              (PP (IN about)
                (NP
                  (NP (DT the) (NN philosophy))
                  (PP (IN of)
                    (NP (NN science))))))))))
    (. .)))

nsubj(will-2, This-1)
nsubj(descending-6, This-1)
root(ROOT-0, will-2)
aux(descending-6, is-4)
advmod(descending-6, already-5)
conj_and(will-2, descending-6)
det(debate-9, a-8)
prep_into(descending-6, debate-9)
det(philosophy-12, the-11)
prep_about(debate-9, philosophy-12)
prep_of(philosophy-12, science-14)

