  (S (: --)
    (NP
      (NP (DT the) (JJ Eonic) (NN website) (NNS claims))
      (SBAR
        (S
          (NP (NN erroneouslyThe) (NN one) (NN thing) (NNP Darwinists) (NNP don) (NNP t))
          (VP (VBP want)
            (S
              (VP (TO to)
                (VP (VB find))))))))
    (VP (VBZ is)
      (NP (PDT such) (DT a) (JJ non-random) (NN pattern))
      (, ,)
      (ADVP (RB anywhere)))
    (. .)))

