(ROOT
  (S
    (ADVP (RB Thus))
    (NP (NNP ID))
    (VP (VBZ is)
      (NP (RB not)
        (NP (NN science))
        ('' '')
        (NP
          (NP (NNP Completely) (NNP illogical))
          (PP (IN of)
            (NP
              (NP (NN course))
              (SBAR
                (S
                  (NP (PRP you))
                  (VP (VBP idiot)))))))))
    (. .)))

advmod(science-5, Thus-1)
nsubj(science-5, ID-2)
cop(science-5, is-3)
neg(science-5, not-4)
root(ROOT-0, science-5)
nn(illogical-8, Completely-7)
dep(science-5, illogical-8)
prep_of(illogical-8, course-10)
dobj(idiot-12, course-10)
nsubj(idiot-12, you-11)
rcmod(course-10, idiot-12)

