(ROOT
  (S
    (NP (NNP New) (NNS species))
    (ADVP (RB usually))
    (VP (VB evolve)
      (PP (IN from)
        (NP (VBG existing) (NNS populations)))
      (PP (RB due)
        (PP (TO to)
          (NP
            (NP (JJ genetic) (NNS changes))
            (PP (IN over)
              (NP (JJ vast) (NN time) (NNS scales)))))
        (, ,) (RB not)
        (PP (IN from)
          (NP
            (NP (DT the) (NN mating))
            (PP (IN of)
              (NP (CD two) (JJ separate) (NNS species)))))))
    (. .)))

nn(species-2, New-1)
nsubj(evolve-4, species-2)
advmod(evolve-4, usually-3)
root(ROOT-0, evolve-4)
amod(populations-7, existing-6)
prep_from(evolve-4, populations-7)
amod(changes-11, genetic-10)
prep_due_to(evolve-4, changes-11)
amod(scales-15, vast-13)
nn(scales-15, time-14)
prep_over(changes-11, scales-15)
neg(evolve-4, not-17)
det(mating-20, the-19)
prep_from(evolve-4, mating-20)
num(species-24, two-22)
amod(species-24, separate-23)
prep_of(mating-20, species-24)

