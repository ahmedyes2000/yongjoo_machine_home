(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP it))
        (VP (MD can))))
    (, ,)
    (ADVP (RB then))
    (NP (DT the) (NN evidence))
    (VP (VBZ is)
      (ADJP (JJ strong)))
    (. .)))

mark(can-3, If-1)
nsubj(can-3, it-2)
advcl(strong-9, can-3)
advmod(strong-9, then-5)
det(evidence-7, the-6)
nsubj(strong-9, evidence-7)
cop(strong-9, is-8)
root(ROOT-0, strong-9)

