(ROOT
  (S ('' '')
    (PP (RB Not)
      (S
        (VP (TO to)
          (VP (VB mention)
            (NP
              (NP (DT the) (NN earth))
              (SBAR
                (WHNP (WDT which))))))))
    (VP (VBZ sustains)
      (NP (PRP them)))
    (. .)))

