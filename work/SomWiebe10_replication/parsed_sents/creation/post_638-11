(ROOT
  (S
    (VP (VBG Using)
      (S
        (NP
          (NP (DT the) (NN bible))
          (CC or)
          (NP (DT any) (JJ sacred) (NN text)))
        (VP (TO to)
          (VP (VB form)
            (NP (PRP$ their) (NNS conclusions))
            ('' '')
            (S
              (NP
                (NP (DT The) (NN content))
                (PP (IN of)
                  (NP
                    (NP (NNP Pandas))
                    (, ,)
                    (VP (VBN published)
                      (PP (IN by)
                        (NP (DT a) (JJ religious) (NN organization))))))))))))
    (: ...)))

root(ROOT-0, Using-1)
det(bible-3, the-2)
nsubj(form-9, bible-3)
det(text-7, any-5)
amod(text-7, sacred-6)
conj_or(bible-3, text-7)
nsubj(form-9, text-7)
aux(form-9, to-8)
xcomp(Using-1, form-9)
poss(conclusions-11, their-10)
dobj(form-9, conclusions-11)
det(content-14, The-13)
xcomp(form-9, content-14)
prep_of(content-14, Pandas-16)
partmod(Pandas-16, published-18)
det(organization-22, a-20)
amod(organization-22, religious-21)
agent(published-18, organization-22)

