(ROOT
  (S
    (NP (DT this))
    (VP (VBZ is)
      (SBAR (IN because)
        (S
          (PP (IN in)
            (NP (PRP$ my) (NN opinion)))
          (NP
            (NP (DT every) (NN religion) (POS 's))
            (NN creation) (NN story))
          (VP (VBZ is)
            (NP
              (NP (DT the) (NN creation) (NN story))
              (PP (IN of)
                (NP (JJ THEIR) (NNS people))))))))
    (. .)))

nsubj(is-2, this-1)
root(ROOT-0, is-2)
mark(story-15, because-3)
poss(opinion-6, my-5)
prep_in(story-15, opinion-6)
det(religion-8, every-7)
poss(story-11, religion-8)
nn(story-11, creation-10)
nsubj(story-15, story-11)
cop(story-15, is-12)
det(story-15, the-13)
nn(story-15, creation-14)
advcl(is-2, story-15)
amod(people-18, THEIR-17)
prep_of(story-15, people-18)

