(ROOT
  (S
    (SBAR (IN As)
      (S
        (NP (PRP I))
        (VP (VBD explained)
          (ADVP (RB before)))))
    (, ,)
    (NP (JJ other) (NN evidence))
    (ADVP (RB also))
    (VP (VBZ points)
      (PP (IN in)
        (NP (DT this) (NN direction))))
    (. .)))

mark(explained-3, As-1)
nsubj(explained-3, I-2)
advcl(points-9, explained-3)
advmod(explained-3, before-4)
amod(evidence-7, other-6)
nsubj(points-9, evidence-7)
advmod(points-9, also-8)
root(ROOT-0, points-9)
det(direction-12, this-11)
prep_in(points-9, direction-12)

