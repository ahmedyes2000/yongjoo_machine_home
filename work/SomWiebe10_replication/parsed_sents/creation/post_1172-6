(ROOT
  (S
    (NP (PRP One))
    (VP (MD can) (RB not)
      (VP (VB carry)
        (PP (IN on)
          (NP (NN life)))
        (PP (IN without)
          (NP (DT the) (JJ other)))))
    (. .)))

nsubj(carry-4, One-1)
aux(carry-4, can-2)
neg(carry-4, not-3)
root(ROOT-0, carry-4)
prep_on(carry-4, life-6)
det(other-9, the-8)
prep_without(carry-4, other-9)

