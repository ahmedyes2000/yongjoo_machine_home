(ROOT
  (S
    (NP (DT Every) (NN primate))
    (VP (VBZ has)
      (VP (VBN evolved)
        (PP (IN over)
          (NP (DT the) (NNS years)))))
    (. .)))

det(primate-2, Every-1)
nsubj(evolved-4, primate-2)
aux(evolved-4, has-3)
root(ROOT-0, evolved-4)
det(years-7, the-6)
prep_over(evolved-4, years-7)

