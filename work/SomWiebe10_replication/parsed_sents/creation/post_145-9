(ROOT
  (S
    (S
      (NP (NNP Meaning))
      (VP (VBZ does) (RB n't)
        (VP (VB need)
          (S
            (VP (TO to)
              (VP (VB be)
                (VP (VBN handed)
                  (PRT (RP down))
                  (PP (IN from)
                    (ADJP (JJ above))))))))))
    (: ;)
    (S
      (NP (PRP it))
      (VP (VBZ comes)
        (PP (IN from)
          (PP (IN within)
            (ADVP (RB too))))))
    (. .)))

nsubj(need-4, Meaning-1)
xsubj(handed-7, Meaning-1)
aux(need-4, does-2)
neg(need-4, n't-3)
root(ROOT-0, need-4)
aux(handed-7, to-5)
auxpass(handed-7, be-6)
xcomp(need-4, handed-7)
prt(handed-7, down-8)
prep_from(handed-7, above-10)
nsubj(comes-13, it-12)
parataxis(need-4, comes-13)
prep(comes-13, from-14)
pcomp(from-14, within-15)
prep(from-14, within-15)
pcomp(within-15, too-16)

