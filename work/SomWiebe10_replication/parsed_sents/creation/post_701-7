(ROOT
  (S (RB Even)
    (ADVP (RB now))
    (NP (DT the) (NN curiosty))
    (ADVP (RB still))
    (VP (VBZ beckons)
      (PP (IN like)
        (NP (DT an) (JJ unopened) (NN door))))
    (. .)))

advmod(beckons-6, Even-1)
advmod(beckons-6, now-2)
det(curiosty-4, the-3)
nsubj(beckons-6, curiosty-4)
advmod(beckons-6, still-5)
root(ROOT-0, beckons-6)
det(door-10, an-8)
amod(door-10, unopened-9)
prep_like(beckons-6, door-10)

