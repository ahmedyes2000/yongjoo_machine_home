    (NP (DT The) (NNS media))
    (VP (VBZ touts)
      (NP
        (NP (NNP ID))
        (PP (IN as)
          (NP (NN creationism))))
      (, ,)
      (SBAR (IN while)
        (S
          (NP (JJS most) (NNS universities))
          (VP (VBP do) (RB not)
            (VP (VB support)
              (NP
                (NP (NN curriculum))
                (VP (VBG questioning)
                  (NP
                    (NP (DT the) (NN theory))
                    (PP (IN of)
                      (NP (NN evolution)))))))))))
    (. .)))

