(ROOT
  (S
    (S
      (ADVP (RB Now))
      (, ,)
      (NP (PRP we))
      (ADVP (DT all))
      (VP (VBP know)
        (SBAR (IN that)
          (S
            (NP (DT some) (NNS articles))
            (VP (VBP have)
              (VP (VBN been)
                (VP (VBN published))))))))
    (, ,)
    (CC but)
    (S
      (NP
        (NP (RB very) (JJ few))
        (CC and)
        (NP (DT the) (NNP Sternberg) (NN controversy)))
      (VP (VBZ shows)
        (SBAR
          (WHADVP (WRB why))
          (S
            (NP
              (NP (DT the) (NNS numbers))
              (PP (IN of)
                (NP
                  (NP (NNS articles))
                  (SBAR
                    (WHNP (WDT that))
                    (S
                      (VP (VBP have)
                        (VP (VBN been)
                          (VP (VBN published)))))))))
            (VP (VBZ is)
              (ADJP (RB so) (JJ few)))))))
    (. .)))

