(ROOT
  (S
    (NP (NNP Let) (POS 's))
    (VP (VBP see)
      (SBAR (IN if)
        (S
          (NP (PRP$ my) (NNS predictions))
          (VP (VBP are)
            (VP (VBN supported)
              (PP (IN by)
                (NP (NN evidence))))))))
    (. .)))

nsubj(see-3, Let-1)
possessive(Let-1, 's-2)
root(ROOT-0, see-3)
mark(supported-8, if-4)
poss(predictions-6, my-5)
nsubjpass(supported-8, predictions-6)
auxpass(supported-8, are-7)
advcl(see-3, supported-8)
agent(supported-8, evidence-10)

