  (S
    (NP (PRP It))
    (VP (VBZ makes)
      (S
        (NP (PRP it))
        (ADJP (JJ hard)
          (S
            (VP
              (VP (TO to)
                (VP (VB argue)
                  (SBAR (IN because)
                    (S
                      (NP (PRP I))
                      (VP (VBP believe)
                        (PP (IN in)
                          (NP (NNP Creationism))))))))
              (PRN (-LRB- -LRB-)
                (FRAG
                  (NP (RB not))
