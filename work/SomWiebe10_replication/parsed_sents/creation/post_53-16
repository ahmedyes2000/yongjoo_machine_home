(ROOT
  (S
    (NP (PRP I))
    (VP (VBP have) (RB not)
      (VP (VBN focused)
        (PP (IN on)
          (NP (JJ pure) (NN chance)))
        (, ,)
        (ADVP (RB either))))
    (. .)))

nsubj(focused-4, I-1)
aux(focused-4, have-2)
neg(focused-4, not-3)
root(ROOT-0, focused-4)
amod(chance-7, pure-6)
prep_on(focused-4, chance-7)
advmod(focused-4, either-9)

