(ROOT
  (S
    (PP (IN because)
      (NP (NN language)))
    (NP (PRP itself))
    (VP (VBZ is)
      (ADVP (RB always))
      (ADJP (JJ open)
        (PP (TO to)
          (NP (NN interpretation)))))
    (. .)))

prep_because(open-6, language-2)
nsubj(open-6, itself-3)
cop(open-6, is-4)
advmod(open-6, always-5)
root(ROOT-0, open-6)
prep_to(open-6, interpretation-8)

(ROOT
  (S
    (ADVP (JJ True))
    (, ,)
    (S
      (ADVP (RB however))
      (NP (PRP you))
      (VP (VBP have)
        (VP (VBN stated)
          (SBAR (IN that)
            (S
              (NP
                (NP (DT some) (NNS verses))
                (PP (IN in)
                  (NP (DT the) (NNP Bible))))
              (VP (VBP are)
                (NP (JJ literal) (NN truth))))))))
    (CC and)
    (S
      (NP (NNS others))
      (VP (VBP are)
        (ADJP (JJ symbolic))))
    (. .)))

advmod(stated-6, True-1)
advmod(stated-6, however-3)
nsubj(stated-6, you-4)
aux(stated-6, have-5)
root(ROOT-0, stated-6)
complm(truth-15, that-7)
det(verses-9, some-8)
nsubj(truth-15, verses-9)
det(Bible-12, the-11)
prep_in(verses-9, Bible-12)
cop(truth-15, are-13)
amod(truth-15, literal-14)
ccomp(stated-6, truth-15)
nsubj(symbolic-19, others-17)
cop(symbolic-19, are-18)
conj_and(stated-6, symbolic-19)

