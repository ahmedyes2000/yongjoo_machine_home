(ROOT
  (S
    (S
      (NP (PRP You))
      (VP (VBP are)
        (VP (VBN entitled)
          (PP (TO to)
            (NP (PRP$ your) (JJ religious) (NNS beliefs))))))
    (CC but)
    (S
      (NP (NN religion))
      (VP (VBZ is)
        (VP (VBN based)
          (PP (IN on)
            (NP (NN faith))))))
    (CC and)
    (S
      (S
        (NP (NN science))
        (VP (VBZ is)
          (VP (VBN based)
            (PP (IN on)
              (NP (NN fact))))))
      (, ,)
      (CC and)
      (S
        (NP (DT the) (NNS facts))
        (VP (VBP are)
          (ADVP (RB definitely))
          (PP (RB not) (IN on)
            (NP (PRP$ your) (NN side))))))
    (. .)))

nsubjpass(entitled-3, You-1)
auxpass(entitled-3, are-2)
root(ROOT-0, entitled-3)
poss(beliefs-7, your-5)
amod(beliefs-7, religious-6)
prep_to(entitled-3, beliefs-7)
nsubjpass(based-11, religion-9)
auxpass(based-11, is-10)
conj_but(entitled-3, based-11)
prep_on(based-11, faith-13)
nsubjpass(based-17, science-15)
auxpass(based-17, is-16)
conj_and(entitled-3, based-17)
prep_on(based-17, fact-19)
det(facts-23, the-22)
nsubj(are-24, facts-23)
conj_and(entitled-3, are-24)
conj_and(based-17, are-24)
advmod(are-24, definitely-25)
neg(are-24, not-26)
poss(side-29, your-28)
prep_on(are-24, side-29)

