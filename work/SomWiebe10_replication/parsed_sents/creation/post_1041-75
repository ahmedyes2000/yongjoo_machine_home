(ROOT
  (S
    (PP (IN In)
      (NP (DT this) (NN case)))
    (, ,)
    (NP
      (NP (PRP$ our) (NN knowledge))
      (PP (IN of)
        (NP (NN evolution))))
    (ADVP (RB actually))
    (VP (VBZ saves)
      (NP
        (NP (NNS people) (POS 's))
        (NNS lives)))
    (. .)))

det(case-3, this-2)
prep_in(saves-10, case-3)
poss(knowledge-6, our-5)
nsubj(saves-10, knowledge-6)
prep_of(knowledge-6, evolution-8)
advmod(saves-10, actually-9)
root(ROOT-0, saves-10)
poss(lives-13, people-11)
dobj(saves-10, lives-13)

