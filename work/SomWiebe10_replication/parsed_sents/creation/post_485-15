(ROOT
  (S
    (NP (PRP They))
    (VP (VBP are) (RB not)
      (NP
        (NP (DT the) (NNS determiners))
        (PP (IN of)
          (NP (NN truth))))
      (SBAR (IN as)
        (S
          (NP (PRP I))
          (VP (VBP 'm)
            (ADJP (JJ sure)
              (SBAR
                (S
                  (NP (PRP you))
                  (VP (MD would)
                    (VP (VB agree))))))))))
    (. .)))

nsubj(determiners-5, They-1)
cop(determiners-5, are-2)
neg(determiners-5, not-3)
det(determiners-5, the-4)
root(ROOT-0, determiners-5)
prep_of(determiners-5, truth-7)
mark(sure-11, as-8)
nsubj(sure-11, I-9)
cop(sure-11, 'm-10)
advcl(determiners-5, sure-11)
nsubj(agree-14, you-12)
aux(agree-14, would-13)
dep(sure-11, agree-14)

