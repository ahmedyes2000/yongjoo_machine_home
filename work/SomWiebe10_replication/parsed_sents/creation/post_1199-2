(ROOT
  (S
    (NP (DT That))
    (VP (VBZ 's)
      (NP
        (NP (DT all))
        (SBAR
          (S
            (NP (PRP I))
            (VP (VBP am)
              (VP (VBG going)
                (S
                  (VP (TO to)
                    (VP (VB say))))))))))
    (. .)))

nsubj(all-3, That-1)
cop(all-3, 's-2)
root(ROOT-0, all-3)
dobj(going-6, all-3)
nsubj(going-6, I-4)
xsubj(say-8, I-4)
aux(going-6, am-5)
rcmod(all-3, going-6)
aux(say-8, to-7)
xcomp(going-6, say-8)

