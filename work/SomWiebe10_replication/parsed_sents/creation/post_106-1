(ROOT
  (S ('' '')
    (ADVP (JJ First)
      (PP (IN of)
        (NP (DT all))))
    (, ,)
    (NP
      (NP (DT the) (NN expansion))
      (PP (IN of)
        (NP (PRP$ our) (NN universe))))
    (VP (VBZ is)
      (NP (DT an)
        (ADJP (JJ observed))
        (NN fact)))
    (. .)))

advmod(fact-14, First-2)
prep_of(First-2, all-4)
det(expansion-7, the-6)
nsubj(fact-14, expansion-7)
poss(universe-10, our-9)
prep_of(expansion-7, universe-10)
cop(fact-14, is-11)
det(fact-14, an-12)
amod(fact-14, observed-13)
root(ROOT-0, fact-14)

