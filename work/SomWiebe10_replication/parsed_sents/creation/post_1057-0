(ROOT
  (S
    (NP (NNP Evolution))
    (VP (VBZ has)
      (ADVP (RB actually))
      (ADVP (RB never))
      (VP (VBN been)
        (VP (VBN observed))))
    (. .)))

nsubjpass(observed-6, Evolution-1)
aux(observed-6, has-2)
advmod(observed-6, actually-3)
neg(observed-6, never-4)
auxpass(observed-6, been-5)
root(ROOT-0, observed-6)

