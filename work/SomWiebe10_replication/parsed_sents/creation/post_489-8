(ROOT
  (S
    (NP (PRP You))
    (VP (VBP have)
      (PRN (, ,)
        (PP (IN by)
          (S
            (VP (VBG making)
              (NP (PRP$ his) (NN claim)))))
        (, ,))
      (VP (VBN accepted)
        (NP (DT the) (NN responsibility)
          (S
            (VP (TO to)
              (VP
                (VP (VB find)
                  (NP
                    (NP (DT every) (RB possibly) (JJ secular) (NN purpose))
                    (PP (IN of)
                      (NP (NNP ID)))))
                (CC and)
                (VP (VB demostrate)
                  (PP (IN with)
                    (NP (JJ little) (NN doubt)))
                  (SBAR
                    (WHADVP (WRB why))
                    (S
                      (NP (PRP it))
                      (VP (VBZ is)
                        (NP (DT a) (NN sham))))))))))))
    (. .)))

