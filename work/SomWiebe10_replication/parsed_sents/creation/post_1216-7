(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP (MD can)
          (VP (VB answer)
            (NP (DT this) (NN question))
            (ADVP (RB correctly))))))
    (, ,)
    (NP (PRP I))
    (VP (MD 'll)
      (VP (VB go)
        (PRT (RP on))
        (S
          (VP (TO to)
            (VP (VB show)
              (S
                (NP (PRP you))
                (NP
                  (NP (DT some) (NN evidence))
                  (PP (IN for)
                    (NP (DT the) (NN theory))))))))))
    (. .)))

mark(answer-4, If-1)
nsubj(answer-4, you-2)
aux(answer-4, can-3)
advcl(go-11, answer-4)
det(question-6, this-5)
dobj(answer-4, question-6)
advmod(answer-4, correctly-7)
nsubj(go-11, I-9)
xsubj(show-14, I-9)
aux(go-11, 'll-10)
root(ROOT-0, go-11)
prt(go-11, on-12)
aux(show-14, to-13)
xcomp(go-11, show-14)
nsubj(evidence-17, you-15)
det(evidence-17, some-16)
xcomp(show-14, evidence-17)
det(theory-20, the-19)
prep_for(evidence-17, theory-20)

