    (PP (IN With)
      (NP
        (NP (DT that))
        (VP (VBG being)
          (VP (VBD said)))))
    (, ,)
    (NP (PRP I))
    (VP (MD 'll)
      (VP (VB ask)
        (SBAR
          (S
            (NP (PRP you) (DT a))
