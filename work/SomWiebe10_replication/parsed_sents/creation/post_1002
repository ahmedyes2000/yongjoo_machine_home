(ROOT
  (S
    (S (CC But)
      (SBAR (RB just) (IN because)
        (S
          (NP (NN something))
          (VP (VBZ is)
            (NP
              (NP (DT a) (NN theory))
              (SBAR
                (S
                  (VP (VBZ does) (RB not)
                    (VP (VB mean)
                      (NP (NNS people))))))))))
      (VP (MD can) (RB not)
        (VP (VB believe)
          (PP (IN in)
            (NP (PRP it))))))
    (: ;)
    (S
      (NP (PRP I))
      (ADVP (RB personally))
      (VP (VBP believe)
        (PP (IN in)
          (NP
            (NP (DT both) (NN evolution))
            (CC and)
            (NP (DT the) (NNP Big) (NNP Bang))))))
    (. .)))

