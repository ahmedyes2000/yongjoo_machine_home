(ROOT
  (S
    (NP (PRP It))
    (VP (VBD had)
      (VP (VBN been)
        (VP (VBN known)
          (ADVP (RB long))
          (PP (IN into)
            (NP
              (NP (NN antiquity))
              (SBAR
                (WHNP (WDT that) (NNS species))
                (S
                  (VP (VBP inherit)
                    (NP (NNS traits)
                      (CC and)
                      (NN change))
                    (PP (IN over)
                      (NP (NN time)))))))))))
    (. .)))

nsubjpass(known-4, It-1)
aux(known-4, had-2)
auxpass(known-4, been-3)
root(ROOT-0, known-4)
advmod(known-4, long-5)
prep_into(known-4, antiquity-7)
det(species-9, antiquity-7)
nsubj(inherit-10, species-9)
rcmod(antiquity-7, inherit-10)
dobj(inherit-10, traits-11)
dobj(inherit-10, change-13)
conj_and(traits-11, change-13)
prep_over(inherit-10, time-15)

