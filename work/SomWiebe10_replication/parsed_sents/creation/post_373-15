(ROOT
  (S ('' '')
    (NP (DT That))
    (VP (VBZ 's) (RB not)
      (NP
        (NP (DT a) (`` ``) (NN method) ('' ''))
        (SBAR
          (WHNP (WDT that))
          (S
            (VP (VBZ 's)
              (NP
                (NP (JJ wishful) (NN thinking))
                (PP (VBN based)
                  (PP (IN on)
                    (NP (PRP$ their) (JJ religious) (NN motivation))))))))))
    (. .)))

nsubj(method-7, That-2)
cop(method-7, 's-3)
neg(method-7, not-4)
det(method-7, a-5)
root(ROOT-0, method-7)
nsubj(thinking-12, method-7)
cop(thinking-12, 's-10)
amod(thinking-12, wishful-11)
