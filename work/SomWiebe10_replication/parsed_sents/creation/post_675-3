(ROOT
  (S
    (NP (NNS Cattle))
    (VP (VBP follow)
      (NP (DT the) (NN leader))
      (PP (IN without)
        (S
          (VP (VBG questioning)
            (NP (DT the) (NN direction))))))
    (. .)))

nsubj(follow-2, Cattle-1)
root(ROOT-0, follow-2)
det(leader-4, the-3)
dobj(follow-2, leader-4)
prepc_without(follow-2, questioning-6)
det(direction-8, the-7)
dobj(questioning-6, direction-8)

