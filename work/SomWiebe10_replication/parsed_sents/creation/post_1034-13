(ROOT
  (S (CC And)
    (PP (IN on)
      (SBAR
        (WHADVP (WRB how))
        (S
          (NP (DT the) (NN atheist))
          (VP (MD would)
            (VP (VB consider))))))
    (NP (PRP him))
    (VP
      (VP (VBZ religious))
      (PRN (-LRB- -LRB-)
        (ADVP (RB sort)
          (PP (IN of)
            (SBAR
              (WHADVP (WRB how))
              (S
                (NP (NNS people))
                (ADVP (RB constantly))
                (VP (VBP view)
                  (NP (PRP me)))))))
        (-RRB- -RRB-)))
    (. .)))

cc(religious-9, And-1)
dep(religious-9, on-2)
advmod(consider-7, how-3)
det(atheist-5, the-4)
