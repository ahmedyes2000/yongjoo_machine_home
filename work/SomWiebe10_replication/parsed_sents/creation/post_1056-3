(ROOT
  (S
    (PP (IN From)
      (NP (DT the) (JJ fossil) (NN record)))
    (S
      (VP (TO to)
        (VP (VB plant))))
    (VP (VBZ homologies)
      (S
        (VP (TO to)
          (VP (VB speciation)))))
    (. .)))

det(record-4, the-2)
amod(record-4, fossil-3)
prep_from(homologies-7, record-4)
aux(plant-6, to-5)
csubj(homologies-7, plant-6)
root(ROOT-0, homologies-7)
aux(speciation-9, to-8)
xcomp(homologies-7, speciation-9)

