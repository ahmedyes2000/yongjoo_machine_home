(ROOT
  (S
    (NP (DT The) (NN question))
    (VP (VBZ is)
      (SBAR (IN whether)
        (S
          (NP (PRP you))
          (VP (VBP agree)
            (PP (IN in)
              (NP
                (NP (DT the) (NN interpretation))
                (PP (IN of)
                  (NP (DT the) (NN evidence)))))))))
    (. .)))

det(question-2, The-1)
nsubj(is-3, question-2)
root(ROOT-0, is-3)
complm(agree-6, whether-4)
nsubj(agree-6, you-5)
ccomp(is-3, agree-6)
det(interpretation-9, the-8)
prep_in(agree-6, interpretation-9)
det(evidence-12, the-11)
prep_of(interpretation-9, evidence-12)

