(ROOT
  (S
    (NP (EX There))
    (VP (VBZ is)
      (NP (DT no) (NN evidence)
        (S
          (VP (TO to)
            (VP
              (ADVP (RB outright))
              (VB deny)
              (NP
                (NP (DT the) (NNP Virgin) (NNP Birth))
                (PP (IN of)
                  (NP
                    (NP (NNP Christ))
                    (CC or)
                    (NP (PRP$ His) (NN Resurrection))))))))))
    (. .)))

expl(is-2, There-1)
root(ROOT-0, is-2)
det(evidence-4, no-3)
nsubj(is-2, evidence-4)
aux(deny-7, to-5)
advmod(deny-7, outright-6)
infmod(evidence-4, deny-7)
det(Birth-10, the-8)
nn(Birth-10, Virgin-9)
dobj(deny-7, Birth-10)
prep_of(Birth-10, Christ-12)
poss(Resurrection-15, His-14)
prep_of(Birth-10, Resurrection-15)
conj_or(Christ-12, Resurrection-15)

