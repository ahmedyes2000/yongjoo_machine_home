(ROOT
  (S
    (NP (EX There))
    (VP (VBZ is)
      (NP
        (NP (DT no) (NN theory))
        (PP (IN of)
          (NP (NN creation)))
        (SBAR
          (S
            (NP (PRP it))
            (VP (VBZ is) (RB not)
              (NP
                (NP (RB even) (DT a) (NNS hypothesis))
                (SBAR
                  (S
                    (NP (PRP it))
                    (VP (VBZ is)
                      (NP (DT a) (NN belief)
                        (CC or)
                        (NN faith)))))))))))
    (. .)))

expl(is-2, There-1)
root(ROOT-0, is-2)
det(theory-4, no-3)
nsubj(is-2, theory-4)
prep_of(theory-4, creation-6)
nsubj(hypothesis-12, it-7)
cop(hypothesis-12, is-8)
neg(hypothesis-12, not-9)
advmod(hypothesis-12, even-10)
det(hypothesis-12, a-11)
rcmod(theory-4, hypothesis-12)
nsubj(belief-16, it-13)
cop(belief-16, is-14)
det(belief-16, a-15)
rcmod(hypothesis-12, belief-16)
rcmod(hypothesis-12, faith-18)
conj_or(belief-16, faith-18)

