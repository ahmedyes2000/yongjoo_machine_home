(ROOT
  (S
    (NP (PRP We))
    (VP (MD can) (RB not)
      (VP (VB detect)
        (NP (NN design))
        (ADVP (RB reliably))
        (SBAR
          (WHADVP (WRB when))
          (S
            (NP (PRP we))
            (VP (VBP lack)
              (S
                (NP (DT any) (JJ positive) (NNS data))
                (VP (TO to)
                  (VP (VB constrain)
                    (NP (DT the) (NN designer))))))))))
    (. .)))

nsubj(detect-4, We-1)
aux(detect-4, can-2)
neg(detect-4, not-3)
root(ROOT-0, detect-4)
dobj(detect-4, design-5)
advmod(detect-4, reliably-6)
advmod(lack-9, when-7)
nsubj(lack-9, we-8)
advcl(detect-4, lack-9)
det(data-12, any-10)
amod(data-12, positive-11)
nsubj(constrain-14, data-12)
aux(constrain-14, to-13)
xcomp(lack-9, constrain-14)
det(designer-16, the-15)
dobj(constrain-14, designer-16)

