(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is)
      (ADVP (RB also))
      (NP (DT a) (NN theory))
      (SBAR (IN that)
        (S
          (NP (PRP we))
          (VP (MD can)
            (VP (VB observe)
              (PP (IN in)
                (NP (NN action)))
              (PP (RB then)
                (NP (NN test))))))))
    (. .)))

nsubj(theory-5, It-1)
cop(theory-5, is-2)
advmod(theory-5, also-3)
det(theory-5, a-4)
root(ROOT-0, theory-5)
complm(observe-9, that-6)
nsubj(observe-9, we-7)
aux(observe-9, can-8)
ccomp(theory-5, observe-9)
prep_in(observe-9, action-11)
prep(observe-9, then-12)
pobj(then-12, test-13)

