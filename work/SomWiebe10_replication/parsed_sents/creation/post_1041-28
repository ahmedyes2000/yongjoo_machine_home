(ROOT
  (S
    (S
      (NP
        (NP (DT The) (NNS problems))
        (PP (IN with)
          (NP (PRP$ his) (NN research))))
      (VP (VBP are)
        (NP (JJ numerous))))
    (, ,)
    (CC but)
    (S
      (PP (IN for)
        (NP
          (NP (DT the) (NN sake))
          (PP (IN of)
            (NP (NN brevity)))))
      (NP (PRP I))
      (VP (MD will)
        (VP (VB give)
          (NP (PRP you))
          (NP
            (NP (DT the) (JJ basic) (NNS reasons))
            (SBAR
              (WHADVP (WRB why))
              (S
                (NP (PRP he))
                (VP (VBZ 's)
                  (ADJP (JJ wrong)))))))))
    (. .)))

