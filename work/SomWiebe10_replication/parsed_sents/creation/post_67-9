(ROOT
  (S
    (PP (IN After)
      (NP (DT all)))
    (, ,)
    (VP (VBD was)
      (NP (PRP it)))
    (SBAR (RB not)
      (S
        (VP (VBD claimed)
          (S
            (VP (TO to)
              (VP (VB be)
                (NP
                  (NP (DT an) (NN inference))
                  (PP (TO to)
                    (NP (DT the) (JJS best) (NN explanation))))))))))
    (. .)))

prep_after(it-5, all-2)
cop(it-5, was-4)
root(ROOT-0, it-5)
dep(claimed-7, not-6)
dep(it-5, claimed-7)
aux(inference-11, to-8)
