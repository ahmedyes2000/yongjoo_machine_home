(ROOT
  (FRAG
    (ADJP (RB Especially)
      (S
        (VP (VBG considering)
          (NP
            (NP (DT the) (NN existence))
            (PP (IN of)
              (NP (JJ other) (JJ religious) (NNS texts)))))))
    (, ,)
    (SBAR
      (WHNP (JJS most)
        (WHPP (IN of)
          (WHNP (WP whom))))
      (S
        (NP (PRP you))
        (VP (MD would)
          (VP (VB DENY)
            (PP (RB because) (IN of)
              (NP
                (NP (PRP$ your) (NN faith))
                (PP (IN in)
                  (NP (DT the) (NNP Bible)))))))))
    (. .)))

root(ROOT-0, Especially-1)
dep(Especially-1, considering-2)
det(existence-4, the-3)
dobj(considering-2, existence-4)
amod(texts-8, other-6)
amod(texts-8, religious-7)
prep_of(existence-4, texts-8)
dobj(DENY-15, most-10)
prep_of(most-10, whom-12)
nsubj(DENY-15, you-13)
aux(DENY-15, would-14)
dep(Especially-1, DENY-15)
poss(faith-19, your-18)
prep_because_of(DENY-15, faith-19)
det(Bible-22, the-21)
prep_in(faith-19, Bible-22)

