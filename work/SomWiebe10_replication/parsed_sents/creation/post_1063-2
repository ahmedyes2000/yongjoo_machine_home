(ROOT
  (S
    (S
      (NP (PRP I))
      (VP (MD could)
        (VP (VB go)
          (PRT (RP on)
            (CC and)
            (RP on)))))
    (: ...)
    (CC but)
    (S
      (NP (PRP I))
      (VP (VBP think)
        (S
          (NP (PRP you))
          (VP (VB get)
            (NP (DT the) (NN picture))))))
    (. .)))

nsubj(go-3, I-1)
aux(go-3, could-2)
root(ROOT-0, go-3)
prt(go-3, on-4)
dep(on-4, and-5)
dep(on-4, on-6)
nsubj(think-10, I-9)
