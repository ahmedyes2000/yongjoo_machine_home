(ROOT
  (S
    (ADVP (RB ''))
    (: ...)
    (NP (DT the) (NN universe))
    (VP (VBZ is)
      (NP (DT a)
        (ADJP (RB stunningly) (JJ complex))
        (NN place)))
    (. .)))

det(universe-4, the-3)
nsubj(place-9, universe-4)
cop(place-9, is-5)
det(place-9, a-6)
advmod(complex-8, stunningly-7)
amod(place-9, complex-8)
root(ROOT-0, place-9)

