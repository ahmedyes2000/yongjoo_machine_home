(ROOT
  (S
    (NP (PRP I))
    (VP (VBP appreciate)
      (NP (PRP he))
      (ADVP (NN way))
      (SBAR
        (S
          (NP (PRP you))
          (VP (VBP have)
            (VP (VBN taken)
              (NP (NN time))
              (S
                (VP (TO to)
                  (VP
                    (ADVP (RB methodically))
                    (VB object)))))))))
    (. .)))

nsubj(appreciate-2, I-1)
root(ROOT-0, appreciate-2)
dobj(appreciate-2, he-3)
advmod(appreciate-2, way-4)
nsubj(taken-7, you-5)
aux(taken-7, have-6)
dep(appreciate-2, taken-7)
dobj(taken-7, time-8)
aux(object-11, to-9)
advmod(object-11, methodically-10)
xcomp(taken-7, object-11)

