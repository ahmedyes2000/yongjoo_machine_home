(ROOT
  (S
    (S
      (NP (NNP God))
      (VP (MD may)
        (VP (VB have)
          (VP (VBN created)
            (NP (DT the) (JJ first) (NN life))))))
    (, ,)
    (CC and)
    (S
      (NP
        (NP (DT the) (NN theory))
        (PP (IN of)
          (NP (NN evolution))))
      (VP (MD would)
        (ADVP (RB still))
        (VP (VB be)
          (ADJP (JJ correct)))))
    (. .)))

nsubj(created-4, God-1)
aux(created-4, may-2)
aux(created-4, have-3)
root(ROOT-0, created-4)
det(life-7, the-5)
amod(life-7, first-6)
dobj(created-4, life-7)
det(theory-11, the-10)
nsubj(correct-17, theory-11)
prep_of(theory-11, evolution-13)
aux(correct-17, would-14)
advmod(correct-17, still-15)
cop(correct-17, be-16)
conj_and(created-4, correct-17)

