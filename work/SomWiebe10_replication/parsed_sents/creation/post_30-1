(ROOT
  (S
    (NP (NNP Evolutionism))
    (VP
      (VP (VBZ is) (RB not)
        (NP (DT a) (NN religion)))
      (, ,)
      (CC and)
      (VP
        (ADVP (RB therefore))
        (VBZ renders)
        (NP (DT this) (NN argument) (NN void))))
    (. .)))

