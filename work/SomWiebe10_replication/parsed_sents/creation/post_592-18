(ROOT
  (S
    (PP (VBG According)
      (PP (TO to)
        (NP (NN evolution))))
    (NP (PRP it))
    (ADVP (RB only))
    (VP (VBZ requires)
      (NP (JJ random) (NN chemical) (NNS reactions)))
    (. .)))

prepc_according_to(requires-6, to-2)
pobj(requires-6, evolution-3)
nsubj(requires-6, it-4)
advmod(requires-6, only-5)
root(ROOT-0, requires-6)
amod(reactions-9, random-7)
nn(reactions-9, chemical-8)
dobj(requires-6, reactions-9)

