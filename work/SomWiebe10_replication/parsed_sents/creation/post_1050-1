(ROOT
  (S
    (PP (IN Before)
      (NP (NNS people) (NN post) (NNS things)))
    (, ,)
    (NP (PRP they))
    (VP (MD should)
      (ADVP (RB really))
      (VP (VB do)
        (NP (PRP$ their) (NN research))
        (ADVP (RB first))))
    (. .)))

nn(things-4, people-2)
nn(things-4, post-3)
prep_before(do-9, things-4)
nsubj(do-9, they-6)
aux(do-9, should-7)
advmod(do-9, really-8)
root(ROOT-0, do-9)
poss(research-11, their-10)
dobj(do-9, research-11)
advmod(do-9, first-12)

