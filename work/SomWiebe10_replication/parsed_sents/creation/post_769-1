(ROOT
  (S (`` ``)
    (NP (PRP They))
    (VP (VBP are) (RB not)
      (NP (NNS scientists))
      (SBAR (IN because)
        (S
          (NP (PRP$ their) (`` ``) (NN evidence) ('' ''))
          (VP (VBZ is)
            (ADJP (JJ false))))))
    (. .)))

nsubj(scientists-5, They-2)
cop(scientists-5, are-3)
neg(scientists-5, not-4)
root(ROOT-0, scientists-5)
mark(false-12, because-6)
poss(evidence-9, their-7)
nsubj(false-12, evidence-9)
cop(false-12, is-11)
advcl(scientists-5, false-12)

