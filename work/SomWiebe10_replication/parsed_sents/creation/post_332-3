(ROOT
  (S ('' '')
    (NP (DT The) (NN process) (NN I))
    (VP (MD will)
      (VP (VB follow)
        (SBAR
          (S
            (VP (VBZ is)
              (NP
                (NP (DT the) (JJ scientific) (NN method))
                (PP (IN of)
                  (NP (JJ hypothesis) (NN testing)))))))))
    (. .)))

det(I-4, The-2)
nn(I-4, process-3)
