(ROOT
  (S
    (NP (NNS Motives)
      (CC and)
      (NNS opportunities))
    (ADVP (RB also))
    (VP (VBP come)
      (NP
        (NP (NNP AFTER))
        (SBAR
          (S
            (NP (DT the) (NN design) (NN inference))
            (VP (VBZ has)
              (VP (VBN been)
                (VP (VBN made))))))))
    (. .)))

nsubj(come-5, Motives-1)
conj_and(Motives-1, opportunities-3)
nsubj(come-5, opportunities-3)
advmod(come-5, also-4)
root(ROOT-0, come-5)
dobj(come-5, AFTER-6)
det(inference-9, the-7)
nn(inference-9, design-8)
nsubjpass(made-12, inference-9)
aux(made-12, has-10)
auxpass(made-12, been-11)
rcmod(AFTER-6, made-12)

