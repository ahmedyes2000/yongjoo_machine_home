(ROOT
  (NP
    (NP
      (LST (LS 3) (-RRB- -RRB-))
      (NP (DT The) (NN evidence))
      (PP (IN for)
        (NP (NN design))))
    (SBAR
      (S
        (VP (VBZ is)
          (NP (DT the) (JJ same))
          (ADVP (DT either) (NN way)
            (SBAR (IN whether)
              (S
                (NP (DT the) (NN designer))
                (VP (VBZ is)
                  (ADJP (JJ supernatural)
                    (CC or)
                    (JJ human)))))))))
    (. .)))

dep(evidence-4, 3-1)
det(evidence-4, The-3)
root(ROOT-0, evidence-4)
