  (S ('' '')
    (NP (WP What))
    (VP (VBZ 's)
      (ADJP
        (ADJP (RB even) (JJR worse))
        (PP (IN about)
          (NP (NNP ID))))
      (S
        (VP (VBG being)
          (NP (JJ scientifically) (NNS vacuous)))))
    (. ?)))

