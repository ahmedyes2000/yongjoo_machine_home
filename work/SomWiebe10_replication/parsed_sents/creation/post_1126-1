(ROOT
  (S
    (S
      (NP
        (NP (DT The) (NNS individuals))
        (PP (IN with)
          (NP (JJR better) (NNS characteristics))))
      (VP (VBP survive)
        (ADVP (RBR more) (RB often))))
    (, ,) (IN so)
    (S
      (NP (PRP they))
      (VP (VBP are)
        (VP (VBN passed)
          (PRT (RP on)))))
    (. .)))

det(individuals-2, The-1)
nsubj(survive-6, individuals-2)
amod(characteristics-5, better-4)
prep_with(individuals-2, characteristics-5)
root(ROOT-0, survive-6)
advmod(often-8, more-7)
advmod(survive-6, often-8)
dep(survive-6, so-10)
nsubjpass(passed-13, they-11)
auxpass(passed-13, are-12)
ccomp(survive-6, passed-13)
prt(passed-13, on-14)

