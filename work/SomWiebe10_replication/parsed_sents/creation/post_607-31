(ROOT
  (S
    (NP (NNP Darwinism))
    (VP (VBZ is)
      (ADVP (RB often))
      (VP (VBN used)
        (PP (IN as)
          (NP
            (NP (JJ scientific) (NN proof))
            (PP (IN of)
              (NP (DT no) (NNP God)))))))
    (. .)))

nsubjpass(used-4, Darwinism-1)
auxpass(used-4, is-2)
advmod(used-4, often-3)
root(ROOT-0, used-4)
amod(proof-7, scientific-6)
prep_as(used-4, proof-7)
det(God-10, no-9)
prep_of(proof-7, God-10)

