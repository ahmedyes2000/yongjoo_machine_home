(ROOT
  (S
    (NP (DT That))
    (VP (VBZ is)
      (NP
        (NP (DT a) (JJ distinct) (NN possibility))
        (SBAR
          (S
            (NP (PRP you))
            (VP (VBP know))))))
    (. .)))

nsubj(possibility-5, That-1)
cop(possibility-5, is-2)
det(possibility-5, a-3)
amod(possibility-5, distinct-4)
root(ROOT-0, possibility-5)
dobj(know-7, possibility-5)
nsubj(know-7, you-6)
rcmod(possibility-5, know-7)

