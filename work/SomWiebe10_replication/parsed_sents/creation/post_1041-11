(ROOT
  (S
    (S
      (PP (IN In)
        (NP (NN fact)))
      (NP (DT this) (NN collision))
      (VP (VBZ is)
        (ADVP (RB still))
        (VP (VBG occurring))))
    (CC and)
    (S
      (NP (NNP Mt.) (NNP Everest))
      (VP (VBZ is)
        (VP (VBG rising)
          (NP (DT a) (JJ few) (NNS inches))
          (NP (DT each) (NN year)))))
    (. .)))

prep_in(occurring-7, fact-2)
det(collision-4, this-3)
nsubj(occurring-7, collision-4)
aux(occurring-7, is-5)
advmod(occurring-7, still-6)
root(ROOT-0, occurring-7)
nn(Everest-10, Mt.-9)
nsubj(rising-12, Everest-10)
aux(rising-12, is-11)
conj_and(occurring-7, rising-12)
det(inches-15, a-13)
amod(inches-15, few-14)
dobj(rising-12, inches-15)
det(year-17, each-16)
tmod(rising-12, year-17)

