(ROOT
  (S (CC But)
    (NP (PRP we))
    (VP (VBP have)
      (VP (VBN found)
        (NP (DT no) (JJ such) (NN thing)))
      (, ,)
      (ADVP (RB ever)))
    (. .)))

cc(found-4, But-1)
nsubj(found-4, we-2)
aux(found-4, have-3)
root(ROOT-0, found-4)
det(thing-7, no-5)
amod(thing-7, such-6)
dobj(found-4, thing-7)
advmod(found-4, ever-9)

