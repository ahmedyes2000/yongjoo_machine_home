(ROOT
  (S
    (NP (NNP Religion))
    (ADVP (RB simply))
    (VP (VBZ is) (RB not)
      (VP (VBN concerned)
        (PP (IN with)
          (NP
            (NP (DT the) (NN truth))
            (PP (IN of)
              (NP (NN anything)))))))
    (. .)))

nsubjpass(concerned-5, Religion-1)
advmod(concerned-5, simply-2)
auxpass(concerned-5, is-3)
neg(concerned-5, not-4)
root(ROOT-0, concerned-5)
det(truth-8, the-7)
prep_with(concerned-5, truth-8)
prep_of(truth-8, anything-10)

