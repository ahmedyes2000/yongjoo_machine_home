(ROOT
  (S (CC And)
    (NP (NNP thou))
    (VP (VBP shalt) (RB not)
      (VP (VB kill) (: ...)
        (NP (JJ thou) (NN shalt))
        (PP (RB not)
          (NP (NN murder)))))
    (. .)))

cc(shalt-3, And-1)
nsubj(shalt-3, thou-2)
root(ROOT-0, shalt-3)
neg(shalt-3, not-4)
dep(shalt-3, kill-5)
amod(shalt-8, thou-7)
dobj(kill-5, shalt-8)
prep(kill-5, not-9)
pobj(not-9, murder-10)

(ROOT
  (FRAG
    (ADVP (RB right) (. .))
    (. .)))

root(ROOT-0, right-1)

