(ROOT
  (S (CC And)
    (PP (IN for)
      (NP (JJ good) (NNS reasons)))
    (PP (IN because)
      (NP (NNP ID)))
    (ADVP (RB really))
    (VP (MD can) (RB not)
      (VP (VB say)
        (ADVP (RBR more) (IN than))
        (SBAR (IN that) (IN as)
          (S
            (NP (PRP it))
            (VP (VBZ is)
              (VP (VBN based)
                (PP (IN on)
                  (NP (DT an) (JJ eliminative\/negative) (NN argument)))))))))
    (. .)))

cc(say-10, And-1)
amod(reasons-4, good-3)
prep_for(say-10, reasons-4)
prep_because(say-10, ID-6)
advmod(say-10, really-7)
aux(say-10, can-8)
neg(say-10, not-9)
root(ROOT-0, say-10)
advmod(than-12, more-11)
advmod(say-10, than-12)
complm(based-17, that-13)
dep(based-17, as-14)
nsubjpass(based-17, it-15)
auxpass(based-17, is-16)
ccomp(say-10, based-17)
det(argument-21, an-19)
amod(argument-21, eliminative\/negative-20)
prep_on(based-17, argument-21)

