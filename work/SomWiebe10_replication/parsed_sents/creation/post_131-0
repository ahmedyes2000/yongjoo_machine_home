(ROOT
  (S
    (NP (NNP ID))
    (VP (VBZ has)
      (NP (DT no) (NN merit))
      (PP (IN except)
        (PP (IN as)
          (NP (DT a) (JJ religious) (NN idea)))))
    (. .)))

nsubj(has-2, ID-1)
root(ROOT-0, has-2)
det(merit-4, no-3)
dobj(has-2, merit-4)
prep(has-2, except-5)
pcomp(except-5, as-6)
det(idea-9, a-7)
amod(idea-9, religious-8)
prep_as(except-5, idea-9)

