(ROOT
  (S ('' '')
    (NP (DT This) (JJ absolute) (NN statement))
    (VP (VBZ is)
      (NP
        (NP (DT an) (JJ excellent) (NN illustration))
        (PP (IN of)
          (NP
            (NP (DT the) (NN mistake))
            (SBAR
              (S
                (NP (DT some) (NNS naturalists))
                (VP (VBP make))))))))
    (. .)))

det(statement-4, This-2)
amod(statement-4, absolute-3)
nsubj(illustration-8, statement-4)
cop(illustration-8, is-5)
det(illustration-8, an-6)
amod(illustration-8, excellent-7)
root(ROOT-0, illustration-8)
det(mistake-11, the-10)
prep_of(illustration-8, mistake-11)
dobj(make-14, mistake-11)
det(naturalists-13, some-12)
nsubj(make-14, naturalists-13)
rcmod(mistake-11, make-14)

