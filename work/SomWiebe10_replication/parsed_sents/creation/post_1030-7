(ROOT
  (S
    (S
      (NP (PRP I))
      (VP (VBP 've)
        (VP (VBN posted)
          (S
            (NP (DT this))
            (ADVP (RB before))))))
    (, ,)
    (CC but)
    (S
      (ADVP (RB here))
      (NP (PRP you))
      (VP (VBP go)
        (ADVP (RB again))))
    (. .)))

nsubj(posted-3, I-1)
aux(posted-3, 've-2)
root(ROOT-0, posted-3)
dep(posted-3, this-4)
advmod(this-4, before-5)
advmod(go-10, here-8)
nsubj(go-10, you-9)
conj_but(posted-3, go-10)
advmod(go-10, again-11)

