(ROOT
  (FRAG
    (ADVP (RB Again))
    (SBAR (IN if)
      (S
        (NP (PRP you))
        (VP (VBP want)
          (S
            (VP (TO to)
              (VP (VB say)
                (NP
                  (NP
                    (NP (DT this))
                    (PP (IN as)
                      (NP (DT a) (JJ personal) (NN belief) (NN GREAT))))
                  (X (SYM \*)))))))))
    (. .)))

root(ROOT-0, Again-1)
mark(want-4, if-2)
nsubj(want-4, you-3)
xsubj(say-6, you-3)
dep(Again-1, want-4)
aux(say-6, to-5)
