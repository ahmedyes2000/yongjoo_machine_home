(ROOT
  (S
    (VP (VB Do) (RB not)
      (VP (VB mistake)
        (NP
          (NP (DT the) (NN finger))
          (PP (IN for)
            (NP (DT the) (NN moon))))))
    (. .)))

aux(mistake-3, Do-1)
neg(mistake-3, not-2)
root(ROOT-0, mistake-3)
det(finger-5, the-4)
dobj(mistake-3, finger-5)
det(moon-8, the-7)
prep_for(finger-5, moon-8)

