(ROOT
  (S
    (SBAR
      (NP (DT The) (JJ very) (NN fact))
      (IN that)
      (S
        (NP (PRP$ our) (NNS brains))
        (VP (VBP are)
          (NP (DT the) (JJ rational) (NN animal) (NN kind)))))
    (, ,)
    (NP (PRP we))
    (VP (VBP see)
      (NP (NNS things))
      (, ,)
      (ADVP (RB often))
      (, ,)
      (PP (IN in)
        (NP (JJ irrational) (NNS ways))))
    (. .)))

det(fact-3, The-1)
amod(fact-3, very-2)
dep(kind-11, fact-3)
dep(kind-11, that-4)
poss(brains-6, our-5)
nsubj(kind-11, brains-6)
cop(kind-11, are-7)
det(kind-11, the-8)
amod(kind-11, rational-9)
nn(kind-11, animal-10)
dep(see-14, kind-11)
nsubj(see-14, we-13)
root(ROOT-0, see-14)
dobj(see-14, things-15)
advmod(see-14, often-17)
amod(ways-21, irrational-20)
prep_in(see-14, ways-21)

