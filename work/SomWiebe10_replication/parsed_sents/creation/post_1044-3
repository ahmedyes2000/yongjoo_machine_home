(ROOT
  (S
    (ADVP (RB However))
    (NP (PRP it))
    (VP (VBZ is) (RB n't)
      (VP (VBG evolution)
        (NP (PRP itself))))
    (. .)))

advmod(evolution-5, However-1)
nsubj(evolution-5, it-2)
aux(evolution-5, is-3)
neg(evolution-5, n't-4)
root(ROOT-0, evolution-5)
dobj(evolution-5, itself-6)

