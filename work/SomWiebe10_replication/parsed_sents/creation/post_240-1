  (S
    (NP (DT Both) (NNS books))
    (VP (VBP attempt)
      (S
        (VP (TO to)
          (VP (VB show)
            (SBAR (IN that)
              (S
                (NP (EX there))
                (VP (VBZ limits)
                  (PRN (-LRB- -LRB-) (VB edges) (-RRB- -RRB-))
                  (PP (TO to)
                    (NP
                      (NP (JJ evolutionary) (NN theory))
                      (SBAR
                        (WHNP (WDT which))
                        (S
                          (VP (MD can) (RB not)
                            (VP (VB be)
                              (VP (VBN crossed)))))))))))))))
    (. .)))

