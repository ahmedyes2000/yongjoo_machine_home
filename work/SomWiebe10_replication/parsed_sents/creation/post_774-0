(ROOT
  (S
    (NP (PRP You))
    (VP (VBP are)
      (VP (VBG opening)
        (NP (DT the) (NN door))
        (PP (TO to)
          (NP
            (NP (DT a) (JJ non-literal) (NN explanation))
            (PP (IN of)
              (NP (DT the) (NN text)))))))
    (. .)))

nsubj(opening-3, You-1)
aux(opening-3, are-2)
root(ROOT-0, opening-3)
det(door-5, the-4)
dobj(opening-3, door-5)
det(explanation-9, a-7)
amod(explanation-9, non-literal-8)
prep_to(opening-3, explanation-9)
det(text-12, the-11)
prep_of(explanation-9, text-12)

