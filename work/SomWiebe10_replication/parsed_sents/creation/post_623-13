(ROOT
  (S ('' '')
    (NP (PRP I))
    (VP (VBP do) (RB n't)
      (VP (VB believe)
        (SBAR (IN that)
          (S
            (NP (EX there))
            (VP (VBZ is)
              (NP
                (NP (DT a) (JJ significant) (NN controversy))
                (PP (IN about)
                  (NP
                    (NP (DT the) (NN concept))
                    (PP (IN of)
                      (NP (JJ natural) (NN selection)))))))))))
    (. .)))

nsubj(believe-5, I-2)
aux(believe-5, do-3)
neg(believe-5, n't-4)
root(ROOT-0, believe-5)
complm(is-8, that-6)
expl(is-8, there-7)
ccomp(believe-5, is-8)
det(controversy-11, a-9)
amod(controversy-11, significant-10)
nsubj(is-8, controversy-11)
det(concept-14, the-13)
prep_about(controversy-11, concept-14)
amod(selection-17, natural-16)
prep_of(concept-14, selection-17)

