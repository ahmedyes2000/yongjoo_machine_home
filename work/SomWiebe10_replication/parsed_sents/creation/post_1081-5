(ROOT
  (S
    (PP (IN Of)
      (NP (NN course)))
    (NP (PRP they))
    (VP (VBP 're) (RB not)
      (VP (VBG going)
        (S
          (VP (TO to)
            (VP (VB want)
              (S
                (VP (TO to)
                  (VP (VB abandon)
                    (NP (PRP it))))))))))
    (. .)))

prep_of(going-6, course-2)
nsubj(going-6, they-3)
xsubj(want-8, they-3)
aux(going-6, 're-4)
neg(going-6, not-5)
root(ROOT-0, going-6)
aux(want-8, to-7)
xcomp(going-6, want-8)
aux(abandon-10, to-9)
xcomp(want-8, abandon-10)
dobj(abandon-10, it-11)

