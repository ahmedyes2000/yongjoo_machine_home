(ROOT
  (S
    (NP (DT That))
    (VP (VBZ is)
      (NP
        (NP (JJ critical) (NN analysis))
        (PRN (: --)
          (SBAR
            (WHNP (WDT which))
            (PRN (, ,)
              (ADVP
                (ADVP (JJ contrary))
                (SBAR
                  (WHPP (TO to)
                    (WHNP (WP what)))
                  (S
                    (NP (NNS evolutionists))
                    (VP (MD may)
                      (VP (VB say))))))
              (, ,))
            (S
              (VP (VBZ is)
                (ADJP (RB quite) (JJ different))))))
        (SBAR
          (WHNP (WDT that))
          (S
            (VP (VBG teaching)
              (NP (NN creationism)))))))
    (. .)))

nsubj(analysis-4, That-1)
cop(analysis-4, is-2)
amod(analysis-4, critical-3)
root(ROOT-0, analysis-4)
nsubj(teaching-19, analysis-4)
