  (S
    (NP (PRP He))
    (VP
      (VP (MD could)
        (VP (VB have)
          (VP (VBN handed)
            (NP (DT the) (NN judge))
            (NP
              (NP (DT a) (NN pile))
              (PP (IN of)
                (NP
                  (NP (NNS recipes))
                  (PP (IN for)
                    (NP (JJ sponge) (NN cake)))))))))
      (CC and)
      (VP (VBD called)
        (S
          (NP (PRP it))
          (NP (DT a) (NN refutation)))))
    (. .)))

