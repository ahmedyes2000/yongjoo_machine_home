(ROOT
  (S
    (PP (IN At)
      (NP (DT this) (NN point)))
    (NP (DT the) (NNP ID) (NN proponent))
    (VP (VBZ has)
      (VP
        (VP (VBN abandoned)
          (NP (DT the) (JJ original) (NN argument)))
        (CC and)
        (VP
          (ADVP (RB therefore))
          (VBN lost) (, ,)
          (SBAR (RB even) (IN though)
            (S
              (NP (PRP he))
              (VP (VBD won)
                (SBAR
                  (S
                    (NP (NN t))
                    (VP (VBP admit)
                      (NP (PRP it)))))))))))
    (. .)))

det(point-3, this-2)
prep_at(abandoned-8, point-3)
det(proponent-6, the-4)
