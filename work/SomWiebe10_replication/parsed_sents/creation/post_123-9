(ROOT
  (S
    (NP (PRP I))
    (VP (VBP need)
      (NP
        (NP (DT every) (JJ single) (NN mutation))
        (CC &)
        (NP (NN selection) (NN pressure)))
      (SBAR (IN before)
        (S
          (NP (PRP I))
          (VP (VBP admit)
            (SBAR (IN that)
              (S
                (NP (NN evolution))
                (VP (VBD produced)
                  (NP
                    (NP (DT this) (NNP IC) (NN system))
                    (CONJP (RB rather) (IN than))
                    (NP (NNP ID))))))))))
    (. .)))

