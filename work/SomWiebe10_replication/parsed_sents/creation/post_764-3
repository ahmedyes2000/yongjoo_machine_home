(ROOT
  (S
    (S
      (NP (PRP I))
      (VP (VBP 've)
        (VP (VBD listened)
          (PP (TO to)
            (NP (PRP$ their) (NN evidence))))))
    (, ,)
    (CC and)
    (S
      (NP (PRP it))
      (VP (VBZ is)
        (ADJP
          (ADVP (RBR less) (IN than))
          (JJ convincing))))
    (. .)))

nsubj(listened-3, I-1)
aux(listened-3, 've-2)
root(ROOT-0, listened-3)
poss(evidence-6, their-5)
prep_to(listened-3, evidence-6)
nsubj(convincing-13, it-9)
cop(convincing-13, is-10)
advmod(than-12, less-11)
advmod(convincing-13, than-12)
conj_and(listened-3, convincing-13)

