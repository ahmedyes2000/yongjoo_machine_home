(ROOT
  (S
    (NP (PRP It))
    (, ,)
    (ADVP (RB therefore))
    (, ,)
    (VP (VBZ seems)
      (ADJP (JJ illogical)
        (S
          (VP (TO to)
            (VP (VB claim)
              (SBAR (IN that)
                (S
                  (NP (EX there))
                  (VP (VBZ exists)
                    (SBAR
                      (S
                        (NP (NN evidence))
                        (VP (VBP AGAINST)
                          (NP (PRP it)))))))))))))
    (. .)))

nsubj(illogical-6, It-1)
advmod(illogical-6, therefore-3)
cop(illogical-6, seems-5)
root(ROOT-0, illogical-6)
aux(claim-8, to-7)
xcomp(illogical-6, claim-8)
complm(exists-11, that-9)
expl(exists-11, there-10)
ccomp(claim-8, exists-11)
nsubj(AGAINST-13, evidence-12)
ccomp(exists-11, AGAINST-13)
dobj(AGAINST-13, it-14)

