(ROOT
  (S (CC So)
    (NP (CD seven) (NNS days) (RB very))
    (ADVP (RB well))
    (VP (MD may)
      (VP (VB have)
        (VP (VBN been)
          (NP
            (NP (NNS billions))
            (PP (IN of)
              (NP (NNS years)))))))
    (. .)))

cc(billions-9, So-1)
num(days-3, seven-2)
nsubj(billions-9, days-3)
advmod(days-3, very-4)
advmod(billions-9, well-5)
aux(billions-9, may-6)
aux(billions-9, have-7)
cop(billions-9, been-8)
root(ROOT-0, billions-9)
prep_of(billions-9, years-11)

