  (S (CC but)
    (NP
      (NP (DT the) (NN weight))
      (PP (IN of)
        (NP (DT the) (NN argument))))
    (VP (MD would)
      (VP (VB be)
        (ADJP (RB dramatically) (JJ lessened))))
    (. .)))

cc(lessened-10, but-1)
det(weight-3, the-2)
nsubj(lessened-10, weight-3)
det(argument-6, the-5)
prep_of(weight-3, argument-6)
aux(lessened-10, would-7)
