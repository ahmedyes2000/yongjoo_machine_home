(ROOT
  (S
    (NP (JJ Many) (NNS fundamentalists))
    (VP (VBP take)
      (NP (NNS Revelations))
      (PP (IN as)
        (S
          (VP (VBG being)
            (NP (DT the) (JJ literal) (NN truth)))))
      (, ,)
      (SBAR
        (WHADVP (WRB how))
        (S
          (VP (VBP do)
            (SBAR
              (S
                (NP (PRP you))
                (VP (VBP demonstrate)
                  (SBAR (IN that)
                    (S
                      (NP (PRP they))
                      (VP (VBP are)
                        (ADJP (JJ wrong))))))))))))
    (. .)))

amod(fundamentalists-2, Many-1)
nsubj(take-3, fundamentalists-2)
root(ROOT-0, take-3)
dobj(take-3, Revelations-4)
cop(truth-9, being-6)
det(truth-9, the-7)
amod(truth-9, literal-8)
prepc_as(take-3, truth-9)
advmod(do-12, how-11)
dep(take-3, do-12)
