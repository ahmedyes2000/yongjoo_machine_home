(ROOT
  (S (CC And)
    (NP (DT neither) (NN side))
    (ADVP (RB really))
    (VP (VBZ has)
      (NP
        (NP (JJ compelling) (JJ scientific) (NNS arguments))
        (PP (IN as) (TO to)
          (SBAR
            (WHADVP (WRB why))
            (S
              (NP (CD one) (NN position))
              (VP (MD should)
                (VP (VB be)
                  (ADJP (JJR better))
                  (PP (IN than)
                    (NP (NNS others))))))))))
    (. .)))

cc(has-5, And-1)
det(side-3, neither-2)
nsubj(has-5, side-3)
advmod(has-5, really-4)
root(ROOT-0, has-5)
amod(arguments-8, compelling-6)
amod(arguments-8, scientific-7)
dobj(has-5, arguments-8)
advmod(better-16, why-11)
num(position-13, one-12)
nsubj(better-16, position-13)
aux(better-16, should-14)
cop(better-16, be-15)
prepc_as_to(arguments-8, better-16)
prep_than(better-16, others-18)

