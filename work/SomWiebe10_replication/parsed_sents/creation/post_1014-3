(ROOT
  (S
    (NP (PRP I))
    (VP (VBP believe)
      (S
        (NP (PRP they))
        (VP (VB study)
          (NP
            (NP (DT the) (NN ocean)
              (CC and)
              (NN sea))
            (CC and)
            (NP
              (NP (PDT all) (DT that) (NN reside))
              (PP (IN in)
                (NP (PRP them))))))))
    (. .)))

nsubj(believe-2, I-1)
root(ROOT-0, believe-2)
nsubj(study-4, they-3)
ccomp(believe-2, study-4)
det(ocean-6, the-5)
dobj(study-4, ocean-6)
dobj(study-4, sea-8)
conj_and(ocean-6, sea-8)
predet(reside-12, all-10)
det(reside-12, that-11)
dobj(study-4, reside-12)
conj_and(ocean-6, reside-12)
prep_in(reside-12, them-14)

