(ROOT
  (S
    (NP (DT That))
    (VP (VBZ is)
      (SBAR
        (WHADVP (WRB how))
        (S
          (NP
            (NP (NNS critics))
            (PP (IN like)
              (NP (DT the) (NNP NCSE)
                (CC or)
                (NNP ACLU))))
          (VP (VBP describe)
            (NP (NN theory))))))
    (. .)))

nsubj(is-2, That-1)
root(ROOT-0, is-2)
advmod(describe-10, how-3)
nsubj(describe-10, critics-4)
det(NCSE-7, the-6)
prep_like(critics-4, NCSE-7)
prep_like(critics-4, ACLU-9)
conj_or(NCSE-7, ACLU-9)
ccomp(is-2, describe-10)
dobj(describe-10, theory-11)

