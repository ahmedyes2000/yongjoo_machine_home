(ROOT
  (SQ (VBZ Is)
    (NP (PRP it))
    (NP
      (NP (PRP me))
      (SBAR (CC or)
        (S
          (VP (VBZ is)
            (S
              (NP (PRP it))
              (NP (PRP you)))))))
    (. ?)))

cop(me-3, Is-1)
nsubj(me-3, it-2)
root(ROOT-0, me-3)
cc(is-5, or-4)
dep(me-3, is-5)
nsubj(you-7, it-6)
xcomp(is-5, you-7)

(ROOT
  (S
    (NP (RB Only) (NN time))
    (VP (MD will)
      (VP (VB tell)))
    (. .)))

advmod(time-2, Only-1)
nsubj(tell-4, time-2)
aux(tell-4, will-3)
root(ROOT-0, tell-4)

