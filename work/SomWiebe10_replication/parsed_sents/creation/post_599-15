(ROOT
  (S
    (VP (VB Keep)
      (S
        (VP (VBG repeating)
          (SBAR (IN until)
            (S
              (NP (PRP you))
              (VP (VBP 've)
                (VP (VBN made)
                  (S
                    (NP (PRP yourself))
                    (ADJP (JJ happy)))))))))
      (SBAR (IN that)
        (S
          (NP (EX there))
          (VP (VBZ is)
            (NP (DT no) (JJ other) (NN outcome))))))
    (. .)))

root(ROOT-0, Keep-1)
xcomp(Keep-1, repeating-2)
mark(made-6, until-3)
nsubj(made-6, you-4)
aux(made-6, 've-5)
advcl(repeating-2, made-6)
nsubj(happy-8, yourself-7)
xcomp(made-6, happy-8)
complm(is-11, that-9)
expl(is-11, there-10)
ccomp(Keep-1, is-11)
det(outcome-14, no-12)
amod(outcome-14, other-13)
nsubj(is-11, outcome-14)

