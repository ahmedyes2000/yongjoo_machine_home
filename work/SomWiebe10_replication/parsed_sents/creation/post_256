(ROOT
  (S (`` ``)
    (CC But)
    (NP
      (NP (WP what))
      (PP (IN about)
        (NP (PRP$ your) (NN opinion))))
    (ADVP (RB on))
    (VP (VBP ID))
    (. ?)))

cc(ID-8, But-2)
nsubj(ID-8, what-3)
poss(opinion-6, your-5)
prep_about(what-3, opinion-6)
advmod(ID-8, on-7)
root(ROOT-0, ID-8)

(ROOT
  (S (`` ``)
    (NP (NNP ID))
    (VP (VBZ is)
      (UCP
        (ADVP (RB ridiculously))
        (CC and)
        (PP
