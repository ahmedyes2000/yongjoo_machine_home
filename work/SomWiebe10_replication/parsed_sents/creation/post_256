neg(obviously-6, not-7)
pobj(obviously-6, science-8)

(ROOT
  (PRN (-LRB- -LRB-)
    (FRAG
      (NP (NNP Hint))
      (: :)
      (S
        (NP (PRP we))
        (VP (VBP 've)
          (VP (VBN been)
