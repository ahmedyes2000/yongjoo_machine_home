(ROOT
  (S
    (NP (DT some) (NNS things))
    (VP (VBD were)
      (VP (VBN meant)
        (S
          (VP (TO to)
            (VP (VB be)
              (NP (JJ literal) (NNS others)))))))
    (. .) (. .)))

det(things-2, some-1)
nsubjpass(meant-4, things-2)
xsubj(others-8, things-2)
auxpass(meant-4, were-3)
root(ROOT-0, meant-4)
aux(others-8, to-5)
cop(others-8, be-6)
amod(others-8, literal-7)
xcomp(meant-4, others-8)

