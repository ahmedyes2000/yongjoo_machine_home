(ROOT
  (S
    (NP (PRP I))
    (VP (VBP do)
      (SBAR
        (S
          (VP (VBP believe)
            (SBAR
              (S
                (NP (DT the) (NN world))
                (VP (VBD was)
                  (VP (VBN created)
                    (PP (IN in)
                      (NP (CD 6) (NNS days)))))))))))
    (. .)))

nsubj(do-2, I-1)
root(ROOT-0, do-2)
ccomp(do-2, believe-3)
det(world-5, the-4)
nsubjpass(created-7, world-5)
auxpass(created-7, was-6)
ccomp(believe-3, created-7)
num(days-10, 6-9)
prep_in(created-7, days-10)

