(ROOT
  (S
    (S
      (NP (DT The) (JJ bottom) (NN line))
      (VP (VBZ is)
        (SBAR
          (S
            (NP (PRP we))
            (VP (VBP do)
              (VP (VB exist)))))))
    (CC and)
    (S
      (NP (EX there))
      (VP (VBZ is)
        (NP
          (NP (RB only) (CD ONE) (NN reality))
          (PP (IN behind)
            (NP (PRP$ our) (NN existence))))))
    (. .)))

det(line-3, The-1)
amod(line-3, bottom-2)
nsubj(is-4, line-3)
root(ROOT-0, is-4)
nsubj(exist-7, we-5)
aux(exist-7, do-6)
