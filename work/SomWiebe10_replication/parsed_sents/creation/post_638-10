  (NP
    (NP
      (NP (DT A) (JJ global) (JJ catastrophic) (NN flood))
      (PRN (-LRB- -LRB-)
        (CC and)
        (NP
          (NP (NN flood))
          (NP (NN geology)))
        (-RRB- -RRB-)))
    (ADJP (CD 5))
    (. .)))

