(ROOT
  (S
    (NP (DT The) (NNS AUSCS))
    (VP (VBD began)
      (CC and)
      (VBD ended)
      (NP
        (NP (PRP$ their) (NN piece))
        (VP (VBG talking)
          (PP (IN about)
            (NP (NNP ID)))
          (PP (IN on)
            (NP (JJ shaky) (JJ scientific) (NNS grounds))))))
    (. .)))

det(AUSCS-2, The-1)
nsubj(began-3, AUSCS-2)
nsubj(ended-5, AUSCS-2)
root(ROOT-0, began-3)
conj_and(began-3, ended-5)
poss(piece-7, their-6)
dobj(began-3, piece-7)
partmod(piece-7, talking-8)
prep_about(talking-8, ID-10)
amod(grounds-14, shaky-12)
amod(grounds-14, scientific-13)
prep_on(talking-8, grounds-14)

