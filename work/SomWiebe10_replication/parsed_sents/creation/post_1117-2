(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP we))
        (VP (VBP see)
          (NP
            (NP (NN creation))
            (PP (IN of)
              (NP (DT this) (NN universe)))))))
    (NP (RB then) (DT every) (JJ single) (NN thing))
    (VP (VBZ gives)
      (NP (PRP us))
      (NP
        (NP (DT the) (NN idea))
        (PP (IN of)
          (NP
            (NP (DT the) (NN greatness))
            (PP (IN of)
              (NP (NNP God)))))))
    (. .)))

mark(see-3, If-1)
nsubj(see-3, we-2)
advcl(gives-12, see-3)
dobj(see-3, creation-4)
det(universe-7, this-6)
prep_of(creation-4, universe-7)
advmod(thing-11, then-8)
det(thing-11, every-9)
amod(thing-11, single-10)
nsubj(gives-12, thing-11)
root(ROOT-0, gives-12)
iobj(gives-12, us-13)
det(idea-15, the-14)
dobj(gives-12, idea-15)
det(greatness-18, the-17)
prep_of(idea-15, greatness-18)
prep_of(greatness-18, God-20)

