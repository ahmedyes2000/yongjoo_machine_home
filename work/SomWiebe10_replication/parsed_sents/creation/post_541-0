(ROOT
  (S
    (NP (JJ Poblic) (NNS schools))
    (VP (VBP are)
      (VP (RB not)
        (S
          (VP (TO to)
            (VP (VB promote)
              (NP (NN religion)))))))
    (. .)))

amod(schools-2, Poblic-1)
nsubj(not-4, schools-2)
xsubj(promote-6, schools-2)
aux(not-4, are-3)
root(ROOT-0, not-4)
aux(promote-6, to-5)
xcomp(not-4, promote-6)
dobj(promote-6, religion-7)

(ROOT
  (SQ (VBZ Is)
    (NP (DT that))
    (NP (DT a) (NN fact))
    (. ?)))

cop(fact-4, Is-1)
nsubj(fact-4, that-2)
det(fact-4, a-3)
root(ROOT-0, fact-4)

