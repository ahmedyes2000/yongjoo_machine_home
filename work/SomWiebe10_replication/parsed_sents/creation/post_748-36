  (S (CC and)
    (NP (PRP I))
    (VP (VBP 'm)
      (ADJP (JJ sure)
        (SBAR
          (S
            (NP (PRP you))
            (VP (MD could)
              (VP (VB continue)
                (PP (IN for)
                  (NP
                    (NP (NNS pages))
                    (PP (IN with)
                      (NP (NNS examples)))))))
            (. .)))))
    (. .)))

cc(sure-4, and-1)
