(ROOT
  (S ('' '')
    (S
      (NP (NNP Marks))
      (VP (VBD had)
        (VP (VBN started)
          (NP (JJ numerous) (JJ other) (NNS labs))
          (ADVP (RB prior)
            (PP (TO to)
              (NP (DT that)))))))
    (CC and)
    (S
      (NP (PRP he))
      (ADVP (RB never))
      (VP (VBD had)
        (S
          (VP (TO to)
            (VP (VB ask)
              (NP (NN permission)))))
        (S
          (VP (TO to)
            (VP (VB start)
              (NP
                (NP (DT any))
                (PP (IN of)
                  (NP (PRP them)))))))))
    (. .)))

nsubj(started-4, Marks-2)
aux(started-4, had-3)
root(ROOT-0, started-4)
amod(labs-7, numerous-5)
