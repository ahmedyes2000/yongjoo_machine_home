  (S
    (PP (IN In)
      (NP
        (NP (PRP$ his) (NN response))
        (PP (TO to)
          (NP
            (NP
              (NP (NNP Grayling) (POS 's))
              (NN review))
            (PP (IN of)
              (NP (PRP$ his) (NN book)))))))
    (, ,)
    (NP
      (NP (NNP Fuller))
      (, ,)
      (NP (NN foolishly))
      (, ,))
    (VP (VBD argued)
      (SBAR (IN that)
        (S
          (NP (NNP ID))
          (VP (VBZ is)
            (NP
              (NP (DT an) (NN argument))
              (PP (TO to)
                (NP (DT the) (JJS best) (NN explanation))))))))
    (. .)))

poss(response-3, his-2)
prep_in(argued-16, response-3)
