(ROOT
  (S
    (ADVP (RB well))
    (VP (VB obviously)
      (ADJP (RB little) (JJ else)
        (PRN
          (S
            (NP (PRP they))
            (VP (MD would)
              (VP (VB be)
                (ADJP (RB Christians) (JJ =)))))
          (-RRB- -RRB-))
        (SBAR
          (WHNP (WDT Which))
          (S
            (VP (VBP represent)
              (SBAR
                (S
                  (NP
                    (NP (DT a) (JJ significant) (NN portion))
                    (PP (IN of)
                      (NP (DT the) (NN world))))
                  (VP (VBZ s)
                    (NP (NN population))))))))))
    (. .)))

advmod(obviously-2, well-1)
root(ROOT-0, obviously-2)
advmod(else-4, little-3)
acomp(obviously-2, else-4)
nsubj(=-9, they-5)
aux(=-9, would-6)
cop(=-9, be-7)
advmod(=-9, Christians-8)
dep(else-4, =-9)
nsubj(represent-12, Which-11)
dep(else-4, represent-12)
det(portion-15, a-13)
amod(portion-15, significant-14)
nsubj(population-20, portion-15)
det(world-18, the-17)
prep_of(portion-15, world-18)
cop(population-20, s-19)
ccomp(represent-12, population-20)

