(ROOT
  (S
    (ADVP (RB Now))
    (NP
      (ADVP (RB suddenly))
      (DT the) (NNS police))
    (VP (VBD became)
      (ADJP (JJ suspicious)))
    (. .)))

advmod(suspicious-6, Now-1)
advmod(police-4, suddenly-2)
det(police-4, the-3)
nsubj(suspicious-6, police-4)
cop(suspicious-6, became-5)
root(ROOT-0, suspicious-6)

