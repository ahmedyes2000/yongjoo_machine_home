(ROOT
  (S
    (NP (PRP We))
    (VP (MD can) (RB not)
      (VP (VB say)
        (SBAR (IN that)
          (S
            (NP (NN petroleum))
            (VP (MD will)
              (VP (VB become)
                (NP (JJ plastic) (CD one) (NN day))))))))
    (. .)))

nsubj(say-4, We-1)
aux(say-4, can-2)
neg(say-4, not-3)
root(ROOT-0, say-4)
complm(day-11, that-5)
nsubj(day-11, petroleum-6)
aux(day-11, will-7)
cop(day-11, become-8)
amod(day-11, plastic-9)
num(day-11, one-10)
ccomp(say-4, day-11)

