(ROOT
  (S (CC but)
    (NP (PRP$ our) (NNS helicopters))
    (VP (VBP suck)
      (PP (IN in)
        (NP (NN comparison))))
    (. .)))

cc(suck-4, but-1)
poss(helicopters-3, our-2)
nsubj(suck-4, helicopters-3)
root(ROOT-0, suck-4)
prep_in(suck-4, comparison-6)

