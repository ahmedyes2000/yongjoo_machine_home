(ROOT
  (S
    (ADVP (RB Meanwhile))
    (, ,)
    (NP
      (NP (NN hypocritically))
      (VP (VBG saying)
        (NP (PRP they))))
    (VP (MD can)
      (VP (VB know)
        (NP (DT the) (NNS origins))
        (, ,)
        (PP (IN without)
          (S
            (VP (VBG adhering)
              (PP (TO to)
                (NP (DT the) (JJ scientific) (NN method))))))))
    (. .)))

advmod(know-7, Meanwhile-1)
nsubj(know-7, hypocritically-3)
partmod(hypocritically-3, saying-4)
dobj(saying-4, they-5)
aux(know-7, can-6)
root(ROOT-0, know-7)
det(origins-9, the-8)
dobj(know-7, origins-9)
prepc_without(know-7, adhering-12)
det(method-16, the-14)
amod(method-16, scientific-15)
prep_to(adhering-12, method-16)

