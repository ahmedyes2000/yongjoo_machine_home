(ROOT
  (S ('' '')
    (NP (NNP Explanations))
    (VP
      (VP
        (ADVP (IN that))
        (VBP call)
        (PP (IN on)
          (NP (JJ intelligent) (NNS causes)))
        (S
          (VP (VB require)
            (NP (DT no) (NNS miracles)))))
      (CC but)
      (VP (MD can) (RB not)
        (VP (VB be)
          (VP (VBN reduced)
            (PP (TO to)
              (NP (JJ materialistic) (NNS explanations)))))))
    (. .)))

nsubj(call-4, Explanations-2)
nsubjpass(reduced-15, Explanations-2)
advmod(call-4, that-3)
root(ROOT-0, call-4)
amod(causes-7, intelligent-6)
prep_on(call-4, causes-7)
ccomp(call-4, require-8)
det(miracles-10, no-9)
dobj(require-8, miracles-10)
aux(reduced-15, can-12)
neg(reduced-15, not-13)
auxpass(reduced-15, be-14)
conj_but(call-4, reduced-15)
amod(explanations-18, materialistic-17)
prep_to(reduced-15, explanations-18)

