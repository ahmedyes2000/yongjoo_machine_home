(ROOT
  (NP
    (NP
      (NP (NNP Island))
      (: --)
      (SBAR
        (WHNP (WDT Which))
        (S
          (VP (VBZ makes)
            (S
              (NP (DT the) (NN debate))
              (ADJP
                (ADJP (JJ political))
                (, ,)
                (CONJP (RB rather) (IN than))
                (ADJP (JJ scientific))
                (, ,)
                (SBAR
                  (S
                    (VP
                      (ADVP (RB much)
                        (PP (TO to)
                          (NP
                            (NP (DT the)
                              (ADJP (RB highly) (JJ vocal))
                              (NN chagrin))
                            (PP (IN of)
                              (NP (NN theoretically))))))
                      (VBZ righteous)
                      (NP (NNS scientists)))))))))))
    (: ...)))

root(ROOT-0, Island-1)
nsubj(makes-4, Which-3)
rcmod(Island-1, makes-4)
det(debate-6, the-5)
nsubj(political-7, debate-6)
nsubj(scientific-11, debate-6)
xcomp(makes-4, political-7)
xcomp(makes-4, scientific-11)
conj_negcc(political-7, scientific-11)
advmod(righteous-21, much-13)
det(chagrin-18, the-15)
advmod(vocal-17, highly-16)
amod(chagrin-18, vocal-17)
prep_to(much-13, chagrin-18)
prep_of(chagrin-18, theoretically-20)
dep(political-7, righteous-21)
dobj(righteous-21, scientists-22)

