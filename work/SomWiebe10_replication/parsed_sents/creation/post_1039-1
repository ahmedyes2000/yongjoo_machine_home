(ROOT
  (S
    (PP (IN Out)
      (PP (IN of)
        (NP (PDT all) (DT the) (NNS claims))))
    (, ,)
    (NP (PRP I))
    (VP (VBP 've)
      (ADVP (RB never))
      (VP (VBN heard)
        (NP (DT any) (JJ solid) (NN evidence))
        (S
          (VP (TO to)
            (VP (VB support)
              (NP (PRP them)))))))
    (. .)))

prepc_out_of(heard-10, of-2)
predet(claims-5, all-3)
det(claims-5, the-4)
pobj(heard-10, claims-5)
