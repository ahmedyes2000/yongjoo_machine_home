  (S
    (ADVP (DT that) (JJ even)
      (PP (IN in)
        (NP (DT the) (`` ``) (JJ inferior) ('' '') (NNS translations))))
    (NP (DT the) (NN meaning))
    (ADVP (RB still))
    (VP (VBZ is)
      (NP (DT the) (JJ same)))
    (. .)))

dep(even-2, that-1)
advmod(same-14, even-2)
det(translations-8, the-4)
amod(translations-8, inferior-6)
