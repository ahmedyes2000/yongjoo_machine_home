(ROOT
  (S
    (NP (DT Some))
    (VP (VBP have)
      (ADVP (RB even) (RB seriously))
      (VP (VBN nominated)
        (NP (JJ advanced) (NN space) (NNS aliens))
        (PP (IN for)
          (NP (DT the) (NN role)))))
    (. .)))

nsubj(nominated-5, Some-1)
aux(nominated-5, have-2)
advmod(seriously-4, even-3)
advmod(nominated-5, seriously-4)
root(ROOT-0, nominated-5)
amod(aliens-8, advanced-6)
nn(aliens-8, space-7)
dobj(nominated-5, aliens-8)
det(role-11, the-10)
prep_for(nominated-5, role-11)

