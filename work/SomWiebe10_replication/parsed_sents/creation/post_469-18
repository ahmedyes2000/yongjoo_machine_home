(ROOT
  (S
    (NP (PRP$ His) (NN science))
    (VP (MD should)
      (VP (VB stand)
        (CC or)
        (VB fall)
        (PP (IN on)
          (NP (PRP$ its) (JJ own)))))
    (. .)))

poss(science-2, His-1)
nsubj(stand-4, science-2)
nsubj(fall-6, science-2)
aux(stand-4, should-3)
root(ROOT-0, stand-4)
conj_or(stand-4, fall-6)
poss(own-9, its-8)
prep_on(stand-4, own-9)

