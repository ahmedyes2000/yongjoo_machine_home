(ROOT
  (S
    (NP
      (NP (DT The) (NN youth))
      (PP (IN of)
        (NP
          (NP (DT the) (NN nation))
          (SBAR
            (S
              (VP (VBP are)
                (VP (VBG reasearching)
                  (ADVP (RB now)))))))))
    (VP (VBZ thinks)
      (PP (TO to)
        (NP (JJ alternative) (NNP Media))))
    (. .)))

det(youth-2, The-1)
nsubj(thinks-9, youth-2)
det(nation-5, the-4)
prep_of(youth-2, nation-5)
aux(reasearching-7, are-6)
