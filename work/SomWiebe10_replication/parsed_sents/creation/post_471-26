(ROOT
  (S
    (NP (DT Many))
    (VP (VBP have)
      (VP (VBN taken)
        (NP (DT that) (NN step))
        (SBAR
          (WHADVP (WRB when))
          (S
            (NP (PRP they))
            (VP
              (VP (VBD started)
                (S
                  (VP (TO to)
                    (VP (VB believe)
                      (PP (IN in)
                        (NP (NNP Darwinism)))))))
              (CC and)
              (VP (VBD understood)
                (NP
                  (NP (DT the) (NNS implications))
                  (PP (IN of)
                    (NP (DT that) (NN view))))
                (PP (IN on)
                  (NP
                    (NP (DT the) (NNP Bible))
                    (CC and)
                    (NP (NN religion))))))))))
    (. .)))

nsubj(taken-3, Many-1)
aux(taken-3, have-2)
root(ROOT-0, taken-3)
det(step-5, that-4)
dobj(taken-3, step-5)
advmod(started-8, when-6)
nsubj(started-8, they-7)
xsubj(believe-10, they-7)
nsubj(understood-14, they-7)
advcl(taken-3, started-8)
aux(believe-10, to-9)
xcomp(started-8, believe-10)
prep_in(believe-10, Darwinism-12)
advcl(taken-3, understood-14)
conj_and(started-8, understood-14)
det(implications-16, the-15)
dobj(understood-14, implications-16)
det(view-19, that-18)
prep_of(implications-16, view-19)
det(Bible-22, the-21)
prep_on(understood-14, Bible-22)
prep_on(understood-14, religion-24)
conj_and(Bible-22, religion-24)

