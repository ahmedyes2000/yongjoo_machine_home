(ROOT
  (S ('' '')
    (NP (DT This) (NN argument))
    (VP (VBZ has)
      (ADVP (RB already))
      (VP (VBN been)
        (VP (VBN debunked)
          (PP (IN by)
            (NP
              (NP (JJ numerous) (NNS scientists))
              (PP (VBG including)
                (NP (NNP Ken) (NNP Miller))))))))
    (. .)))

det(argument-3, This-2)
nsubjpass(debunked-7, argument-3)
aux(debunked-7, has-4)
advmod(debunked-7, already-5)
auxpass(debunked-7, been-6)
root(ROOT-0, debunked-7)
amod(scientists-10, numerous-9)
agent(debunked-7, scientists-10)
nn(Miller-13, Ken-12)
prep_including(scientists-10, Miller-13)

