(ROOT
  (S
    (NP (PRP I))
    (VP (MD 'll)
      (VP (VB respond)
        (SBAR
          (WHADVP (WRB when))
          (S
            (NP (PRP I))
            (VP (VBP feel)
              (SBAR
                (S
                  (NP (PRP I))
                  (VP (VBP understand)
                    (SBAR
                      (WHNP (WP what))
                      (S
                        (NP (PRP you))
                        (VP (VBP are)
                          (VP (VBG saying)))))))))))))
    (. .)))

nsubj(respond-3, I-1)
aux(respond-3, 'll-2)
root(ROOT-0, respond-3)
advmod(feel-6, when-4)
nsubj(feel-6, I-5)
advcl(respond-3, feel-6)
nsubj(understand-8, I-7)
ccomp(feel-6, understand-8)
dobj(saying-12, what-9)
nsubj(saying-12, you-10)
aux(saying-12, are-11)
ccomp(understand-8, saying-12)

