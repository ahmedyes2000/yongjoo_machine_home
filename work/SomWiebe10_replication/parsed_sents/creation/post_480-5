(ROOT
  (S
    (PP (VBN Come)
      (PP (IN on)))
    (, ,)
    (VP (VB let)
      (SBAR
        (S
          (NP (PRP me))
          (VP (VBP hear)
            (SBAR
              (S
                (NP (PRP you))
                (VP (VBP say)
                  (S
                    (NP (PRP it))
                    (ADJP (RB straight))
                    (ADVP (RB out))))))))))
    (. .)))

prep(let-4, Come-1)
pcomp(Come-1, on-2)
prep(Come-1, on-2)
root(ROOT-0, let-4)
nsubj(hear-6, me-5)
ccomp(let-4, hear-6)
nsubj(say-8, you-7)
ccomp(hear-6, say-8)
nsubj(straight-10, it-9)
xcomp(say-8, straight-10)
advmod(straight-10, out-11)

