(ROOT
  (SINV (RB Then)
    (VP (VBZ comes))
    (NP
      (NP
        (NP (DT the) (NN definition))
        (PP (IN of)
          (NP (NNS units))))
      (PP (IN of)
        (NP (NN time)))
      (, ,)
      (PP (JJ such) (IN as)
        (NP (DT a) (NN day))))
    (. .)))

advmod(comes-2, Then-1)
root(ROOT-0, comes-2)
det(definition-4, the-3)
nsubj(comes-2, definition-4)
prep_of(definition-4, units-6)
prep_of(definition-4, time-8)
det(day-13, a-12)
prep_such_as(definition-4, day-13)

