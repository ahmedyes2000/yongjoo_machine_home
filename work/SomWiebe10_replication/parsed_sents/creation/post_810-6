(ROOT
  (FRAG
    (SBAR
      (WHADJP (WRB How)
        (ADJP (IN about)))
      (S
        (VP (VBG making)
          (NP (DT the) (JJ next) (`` ``) (NN mile)))))
    (. .)))

advmod(about-2, How-1)
dep(making-3, about-2)
root(ROOT-0, making-3)
det(mile-7, the-4)
amod(mile-7, next-5)
dobj(making-3, mile-7)

