  (S ('' '')
    (NP (EX There))
    (VP (VBZ is)
      (NP
        (NP (DT no) (NN explanation))
        (VP (VBG invoking)
          (NP
            (NP (NN randomness)
              (CC or)
              (NN chance))
            (SBAR
              (WHNP (WDT that))
              (S
                (VP (MD can)
                  (VP (VB explain)
                    (NP (VBN specified) (NN complexity))))))))))
    (. .)))

expl(is-3, There-2)
root(ROOT-0, is-3)
det(explanation-5, no-4)
nsubj(is-3, explanation-5)
partmod(explanation-5, invoking-6)
dobj(invoking-6, randomness-7)
nsubj(explain-12, randomness-7)
