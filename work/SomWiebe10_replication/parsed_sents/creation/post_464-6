(ROOT
  (S
    (NP (JJ Few) (JJ regular) (NNS citizens))
    (ADVP (RB even))
    (VP (VBP know)
      (SBAR (IN that)
        (S
          (S
            (NP (DT this) (NN report))
            (VP (VBZ exists)
              (ADVP (RB though))))
          (CC and)
          (S
            (NP (DT no) (NN action)
              (CC or)
              (NN fine))
            (VP (VBD was)
              (VP (VBN put)
                (PP (IN on)
                  (NP
                    (NP (DT the) (NNP Institution))
                    (CC or)
                    (NP (DT the) (NN NCSE))))
                (PP (IN for)
                  (NP
                    (NP (PRP$ their) (NN role))
                    (PP (IN in)
                      (NP (DT this) (NN harrassment)))))))))))
    (. .)))

amod(citizens-3, Few-1)
amod(citizens-3, regular-2)
