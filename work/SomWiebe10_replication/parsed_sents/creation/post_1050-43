(ROOT
  (S (CC But)
    (SBAR (IN if)
      (S
        (NP (PRP I))
        (VP (VBD had)
          (S
            (VP (TO to)
              (VP (VB pick)
                (NP (CD one))))))))
    (NP (PRP I))
    (VP (MD would)
      (VP (VB say)
        (NP (NN creation))))
    (. .)))

cc(say-10, But-1)
mark(had-4, if-2)
nsubj(had-4, I-3)
xsubj(pick-6, I-3)
dep(say-10, had-4)
aux(pick-6, to-5)
xcomp(had-4, pick-6)
dobj(pick-6, one-7)
