(ROOT
  (S
    (S
      (NP (PRP We))
      (VP (MD must)
        (VP (VB commit)
          (PP (TO to)
            (NP
              (NP (PRP$ his) (NN existence))
              (PP (IN on)
                (NP (PRP$ our) (JJ own))))))))
    (: ...)
    (S
      (NP (NN science))
      (VP (VBZ is) (RB n't)
        (VP (VBG going)
          (S
            (VP (TO to)
              (VP (VB give)
                (NP (PRP us))
                (NP
                  (NP (DT that) (JJ holy) (NN grail))
                  (SBAR
                    (S
                      (NP (PRP we))
                      (VP (VBP are)
                        (VP (VBG looking)
                          (PP (IN for)))))))))))))
    (. .)))

nsubj(commit-3, We-1)
aux(commit-3, must-2)
root(ROOT-0, commit-3)
poss(existence-6, his-5)
prep_to(commit-3, existence-6)
poss(own-9, our-8)
prep_on(existence-6, own-9)
nsubj(going-14, science-11)
xsubj(give-16, science-11)
aux(going-14, is-12)
neg(going-14, n't-13)
