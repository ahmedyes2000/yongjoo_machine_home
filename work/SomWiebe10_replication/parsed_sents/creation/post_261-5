(ROOT
  (S ('' '')
    (PP (IN Like)
      (NP
        (NP (PRP it))
        (CC or)
        (NP (RB not))))
    (, ,)
    (NP (DT that))
    (VP (VBZ 's)
      (NP
        (NP (DT the) (NN way))
        (SBAR
          (S
            (NP (NNP ID))
            (VP (VBZ works))))))
    (. .)))

prep_like(way-10, it-3)
conj_or(it-3, not-5)
prep_like(way-10, not-5)
nsubj(way-10, that-7)
cop(way-10, 's-8)
det(way-10, the-9)
root(ROOT-0, way-10)
dobj(works-12, way-10)
nsubj(works-12, ID-11)
rcmod(way-10, works-12)

