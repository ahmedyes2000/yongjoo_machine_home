(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ does) (RB not)
      (VP (VB offer)
        (NP
          (NP (NNS predictions))
          (SBAR
            (WHNP (WDT that))
            (S
              (VP (MD will)
                (VP (VB validate)
                  (NP
                    (NP (PRP it) (POS 's))
                    (NNS claims)))))))))
    (. .)))

nsubj(offer-4, It-1)
aux(offer-4, does-2)
neg(offer-4, not-3)
root(ROOT-0, offer-4)
dobj(offer-4, predictions-5)
nsubj(validate-8, predictions-5)
aux(validate-8, will-7)
rcmod(predictions-5, validate-8)
poss(claims-11, it-9)
dobj(validate-8, claims-11)

