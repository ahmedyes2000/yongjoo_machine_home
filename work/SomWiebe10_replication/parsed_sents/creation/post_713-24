(ROOT
  (S
    (NP (DT The) (NN lord))
    (VP (VBZ does)
      (NP
        (NP (NN work))
        (PP (IN in)
          (NP (JJ mysterious) (NNS ways))))
      (ADVP (RB indeed)))
    (. .)))

det(lord-2, The-1)
nsubj(does-3, lord-2)
root(ROOT-0, does-3)
dobj(does-3, work-4)
amod(ways-7, mysterious-6)
prep_in(work-4, ways-7)
advmod(does-3, indeed-8)

