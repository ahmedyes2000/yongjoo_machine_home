(ROOT
  (S
    (NP (NNP Science))
    (VP (VBZ is) (RB not)
      (NP
        (NP (DT a) (NN collection))
        (PP (IN of)
          (PRN (, ,)
            (S
              (VP (VBN cast)
                (PP (IN in)
                  (NP (NN stone)))))
            (, ,))
          (NP (NNS answers)))))
    (. .)))

nsubj(collection-5, Science-1)
cop(collection-5, is-2)
neg(collection-5, not-3)
det(collection-5, a-4)
root(ROOT-0, collection-5)
dep(collection-5, cast-8)
prep_in(cast-8, stone-10)
prep_of(collection-5, answers-12)

