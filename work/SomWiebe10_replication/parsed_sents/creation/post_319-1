(ROOT
  (S (IN So)
    (NP (PRP$ your) (NN sentence))
    (VP (VBZ is)
      (ADVP (RB backwards)
        (PP (IN of)
          (NP (NN course)))))
    (. .)))

dep(is-4, So-1)
poss(sentence-3, your-2)
nsubj(is-4, sentence-3)
root(ROOT-0, is-4)
advmod(is-4, backwards-5)
prep_of(backwards-5, course-7)

