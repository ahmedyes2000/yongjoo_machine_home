(ROOT
  (S
    (NP (DT That))
    (VP (VBZ makes)
      (NP
        (NP (DT the) (NN question))
        (PP (IN of)
          (NP (NNP HOW)))
        (SBAR
          (S
            (NP (PRP you))
            (VP (VBD got)
              (PP (TO to)
                (NP (DT those) (JJ valid)))
              (PP (IN in)
                (NP (PRP$ my) (NN book))))))))
    (. .)))

nsubj(makes-2, That-1)
root(ROOT-0, makes-2)
det(question-4, the-3)
dobj(makes-2, question-4)
dobj(got-8, question-4)
prep_of(question-4, HOW-6)
nsubj(got-8, you-7)
rcmod(question-4, got-8)
det(valid-11, those-10)
prep_to(got-8, valid-11)
