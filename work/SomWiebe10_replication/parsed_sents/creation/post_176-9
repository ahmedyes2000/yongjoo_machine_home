(ROOT
  (S
    (S
      (ADVP (RB Unsurprisingly))
      (NP (DT the)
        (ADJP (RB very) (JJ next))
        (NN sentence))
      (VP (VBZ is)
        (ADJP (NN contradictory))))
    (: :)
    (S
      (NP (DT Those) (JJ alternate) (NNS ideas))
      (, ,)
      (PP (IN unlike)
        (NP
          (NP (DT the) (NNS hypotheses))
          (VP (VBN investigated)
            (PP (IN in)
              (NP (DT these) (NNS papers))))))
      (, ,)
      (VP (VBP remain)
        (ADJP (RB thoroughly) (JJ untested))))
    (. .)))

advmod(contradictory-7, Unsurprisingly-1)
det(sentence-5, the-2)
advmod(next-4, very-3)
amod(sentence-5, next-4)
nsubj(contradictory-7, sentence-5)
cop(contradictory-7, is-6)
root(ROOT-0, contradictory-7)
det(ideas-11, Those-9)
