(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP$ his) (JJ non-creationist) (NN work))
        (VP (VBZ is)
          (ADJP (JJ good)))))
    (, ,)
    (ADVP (RB then))
    (NP (PRP I))
    (VP (VBP think)
      (SBAR
        (S
          (NP (PRP he))
          (VP (VBZ deserves)
            (NP (DT the) (NN job))))))
    (. .)))

mark(good-6, If-1)
poss(work-4, his-2)
amod(work-4, non-creationist-3)
nsubj(good-6, work-4)
cop(good-6, is-5)
advcl(think-10, good-6)
advmod(think-10, then-8)
nsubj(think-10, I-9)
root(ROOT-0, think-10)
nsubj(deserves-12, he-11)
ccomp(think-10, deserves-12)
det(job-14, the-13)
dobj(deserves-12, job-14)

