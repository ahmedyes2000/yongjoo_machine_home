  (S
    (NP
      (NP (NNP \/) (NNP Scientific) (NNP dating))
      (, ,)
      (NP (NN geology))
      (, ,)
      (NP (NN paleontology))
      (CC and)
      (NP (JJ other) (NNS sciences)))
    (VP (VBP have)
      (VP (VBN proved)
        (PP (IN beyond)
          (NP (NN doubt)))
        (SBAR (IN that)
          (S
            (NP
              (NP (DT the) (NN evolution))
              (PP (IN of)
                (NP (DT the) (NN earth)
                  (CC and)
                  (NN life) (NNS forms))))
            (VP (VBZ has)
              (VP (VBN taken)
                (NP (NN place))
                (PP (IN over)
                  (NP
                    (NP (NNS billions))
                    (PP (IN of)
                      (NP (NNS years)))))))))))
    (. .)))

