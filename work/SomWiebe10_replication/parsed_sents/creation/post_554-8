(ROOT
  (S
    (NP (DT Neither) (NN side))
    (VP (VBZ is)
      (VP (VBN proven)
        (SBAR (IN so)
          (S
            (NP (DT both) (NNS sides))
            (VP (VBP deserve)
              (NP (DT a) (NN hearing)))))))
    (. .)))

det(side-2, Neither-1)
nsubjpass(proven-4, side-2)
auxpass(proven-4, is-3)
root(ROOT-0, proven-4)
mark(deserve-8, so-5)
det(sides-7, both-6)
nsubj(deserve-8, sides-7)
advcl(proven-4, deserve-8)
det(hearing-10, a-9)
dobj(deserve-8, hearing-10)

