(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (NNP God))
        (VP (VBZ is)
          (ADJP (JJ perfect)))))
    (, ,)
    (ADVP (RB then))
    (NP (DT the) (NN world))
    (VP (MD should)
      (VP (VB be)
        (ADJP (JJ perfect))))
    (. .)))

mark(perfect-4, If-1)
nsubj(perfect-4, God-2)
cop(perfect-4, is-3)
advcl(perfect-11, perfect-4)
advmod(perfect-11, then-6)
det(world-8, the-7)
nsubj(perfect-11, world-8)
aux(perfect-11, should-9)
cop(perfect-11, be-10)
root(ROOT-0, perfect-11)

