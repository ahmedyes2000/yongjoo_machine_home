(ROOT
  (S
    (NP (NN Reasoning))
    (ADVP (RB otherwise))
    (VP (VBZ puts)
      (NP (DT an) (NN end))
      (PP (TO to)
        (NP (JJ scientific) (NN inquiry))))
    (. .)))

nsubj(puts-3, Reasoning-1)
advmod(puts-3, otherwise-2)
root(ROOT-0, puts-3)
det(end-5, an-4)
dobj(puts-3, end-5)
amod(inquiry-8, scientific-7)
prep_to(puts-3, inquiry-8)

