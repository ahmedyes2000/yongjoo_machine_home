(ROOT
  (S
    (NP (NNS Evolutionists))
    (VP (VBP extrapolate)
      (S
        (NP
          (NP (NN evidence))
          (PP (IN for)
            (NP (JJ observed) (NN evolution))))
        (VP (TO to)
          (VP (VB be)
            (NP
              (NP (NN evidence))
              (PP (IN for)
                (NP (JJ unobserved) (NN evolution))))))))
    (. .)))

nsubj(extrapolate-2, Evolutionists-1)
root(ROOT-0, extrapolate-2)
nsubj(evidence-9, evidence-3)
amod(evolution-6, observed-5)
prep_for(evidence-3, evolution-6)
aux(evidence-9, to-7)
cop(evidence-9, be-8)
