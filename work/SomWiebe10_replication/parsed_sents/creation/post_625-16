(ROOT
  (S ('' '')
    (PP (VBG Only)
      (PP (IN in)
        (NP (DT the) (JJ same))))
    (VP (VBP sense)
      (SBAR (IN that)
        (S
          (NP (EX there))
          (VP (VBZ is)
            (NP
              (NP (DT no) (NN evidence))
              (PP (TO to)
                (NP (DT the) (NN contrary))))))))
    (. .)))

prep(sense-6, Only-2)
pcomp(Only-2, in-3)
det(same-5, the-4)
prep_in(Only-2, same-5)
root(ROOT-0, sense-6)
complm(is-9, that-7)
expl(is-9, there-8)
ccomp(sense-6, is-9)
det(evidence-11, no-10)
nsubj(is-9, evidence-11)
det(contrary-14, the-13)
prep_to(evidence-11, contrary-14)

