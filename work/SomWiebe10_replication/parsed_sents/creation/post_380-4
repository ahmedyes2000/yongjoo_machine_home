(ROOT
  (S
    (PP (IN In)
      (NP (CD 1972)))
    (NP (NN scientist))
    (VP (VBD began)
      (S
        (VP (VBG calling)
          (NP (DT this) (NNP Punctuated) (NNP Equilibrium)))))
    (. .)))

prep_in(began-4, 1972-2)
nsubj(began-4, scientist-3)
root(ROOT-0, began-4)
xcomp(began-4, calling-5)
det(Equilibrium-8, this-6)
nn(Equilibrium-8, Punctuated-7)
