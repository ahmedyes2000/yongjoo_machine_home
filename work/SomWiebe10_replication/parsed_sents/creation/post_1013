  (S
    (PP
      (PP (TO To)
        (NP (DT each)))
      (PP
        (NP (PRP$ their) (JJ own))
        (IN as)
        (NP (DT a) (JJ personal) (NN belief))))
    (NP (PRP I))
    (VP (VBP have)
      (NP
        (NP (NN nothing))
        (PP (IN against)
          (NP
            (NP (DT those))
            (SBAR
              (WHNP (WDT that))
              (S
                (VP
                  (VP (VBP subscribe)
                    (S
                      (VP (TO to)
                        (VP (VB creationism)))))
                  (CC but)
                  (VP (VBP feel)
                    (ADVP (RB strongly)))))))))
      (SBAR (IN that)
        (S
          (NP (PRP it))
          (VP (VBZ does) (RB not)
            (VP (VB belong)
              (PP (IN in)
                (NP (NNS schools))))))))
    (. .)))

