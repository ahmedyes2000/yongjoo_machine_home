(ROOT
  (S
    (VP (VBD \/) (-RRB- -RRB-)
      (NP
        (NP
          (ADJP
            (ADVP (RB As)
              (PP (IN for)
                (NP (DT the) (JJ other) (NN simian) (NN species))))
            (NP (PRP they))
            (JJ have))
          (NNS evolved))
        (ADVP (RB just)))
      (ADVP
        (ADVP (RB as) (RB much))
        (SBAR (IN as)
          (S
            (NP (PRP we))
            (VP (VBP have)
              (NP
                (NP (RB just) (RB not))
                (PP (IN in)
                  (NP (DT the) (JJ same) (NNS ways)))))))))
    (. .)))

root(ROOT-0, \/-1)
det(species-8, the-5)
amod(species-8, other-6)
nn(species-8, simian-7)
prep_as_for(have-10, species-8)
dep(have-10, they-9)
amod(evolved-11, have-10)
dobj(\/-1, evolved-11)
advmod(evolved-11, just-12)
advmod(much-14, as-13)
advmod(\/-1, much-14)
mark(have-17, as-15)
nsubj(have-17, we-16)
ccomp(much-14, have-17)
advmod(not-19, just-18)
dobj(have-17, not-19)
det(ways-23, the-21)
amod(ways-23, same-22)
prep_in(not-19, ways-23)

