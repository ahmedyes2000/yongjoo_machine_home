(ROOT
  (NP
    (NP (NNP Deny))
    (NP
      (NP (DT the) (NN relevance))
      (PP (IN of)
        (NP
          (NP (DT the) (NN work))
          (PP (IN of)
            (NP (DT these) (NNS scientists))))))
    (. .)))

root(ROOT-0, Deny-1)
det(relevance-3, the-2)
dep(Deny-1, relevance-3)
det(work-6, the-5)
