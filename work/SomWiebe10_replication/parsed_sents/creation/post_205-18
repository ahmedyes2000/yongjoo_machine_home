    (SBAR (IN If)
      (S
        (NP (NNP ID))
        (VP (VBZ thinks)
          (SBAR (IN that)
            (S
              (NP (EX there))
              (ADVP (RB really))
              (VP (MD could)
                (VP (VB be)
                  (NP
                    (NP (DT a) (JJ natural) (NN cause))
                    (SBAR
                      (WHNP (WDT that))
                      (S
                        (VP (VBZ explains)
                          (NP (PRP$ their) (VBN supposed) (NN design) (NN effect)))))))))))))
    (, ,)
    (NP (PRP they))
    (VP (MD should)
      (VP
        (VP (VB tell)
          (NP (PRP$ their) (JJ religious) (NNS supporters)))
        (CC and)
        (VP (VB stop)
          (S
            (VP (VBG taking)
              (NP (NN money))
              (PP (IN from)
                (NP (PRP them))))))))
    (. .)))

