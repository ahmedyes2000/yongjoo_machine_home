(ROOT
  (S
    (NP (NNP Synergy))
    (VP (VBZ sounds)
      (PP (IN like)
        (NP (DT a) (JJ pretty) (JJ universal) (NN sign)))
      (PP (TO to)
        (NP (PRP me))))
    (. .) ('' '')))

nsubj(sounds-2, Synergy-1)
root(ROOT-0, sounds-2)
det(sign-7, a-4)
amod(sign-7, pretty-5)
amod(sign-7, universal-6)
prep_like(sounds-2, sign-7)
prep_to(sounds-2, me-9)

(ROOT
  (X
    (ADVP (RB So))
    (NP (WP what))
    (. ?)))

dep(what-2, So-1)
root(ROOT-0, what-2)

