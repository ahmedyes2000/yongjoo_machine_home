  (S
    (NP (PRP It))
    (VP (VBZ is)
      (NP
        (NP (JJR more))
        (PP (IN of)
          (NP
            (NP (DT a) (JJ deterministic) (NN stance))
            (, ,)
            (NP (JJ i.e.) (NN arrogance))))))
    (. .)))

