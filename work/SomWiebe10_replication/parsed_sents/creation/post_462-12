(ROOT
  (S
    (NP (PRP They))
    (VP (VBP want)
      (S
        (NP (PRP us))
        (VP (TO to)
          (VP
            (VP (VB bow)
              (PP (IN at)
                (NP (PRP$ their) (NNS feet))))
            (CC and)
            (VP (VB take)
              (NP (PRP$ their) (NNS opinions))
              (PP (IN as)
                (NP (JJ absolute) (NN truth))))))))
    (. .)))

nsubj(want-2, They-1)
root(ROOT-0, want-2)
nsubj(bow-5, us-3)
nsubj(take-10, us-3)
aux(bow-5, to-4)
xcomp(want-2, bow-5)
poss(feet-8, their-7)
prep_at(bow-5, feet-8)
xcomp(want-2, take-10)
conj_and(bow-5, take-10)
poss(opinions-12, their-11)
dobj(take-10, opinions-12)
amod(truth-15, absolute-14)
prep_as(take-10, truth-15)

