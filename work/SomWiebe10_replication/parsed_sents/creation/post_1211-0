(ROOT
  (S
    (ADVP (RB Sorry))
    (, ,)
    (NP (IN but) (DT this))
    (VP (VBZ is) (RB not)
      (NP (DT an) (NN opinion)))
    (. .)))

advmod(opinion-8, Sorry-1)
amod(this-4, but-3)
nsubj(opinion-8, this-4)
cop(opinion-8, is-5)
neg(opinion-8, not-6)
det(opinion-8, an-7)
root(ROOT-0, opinion-8)

