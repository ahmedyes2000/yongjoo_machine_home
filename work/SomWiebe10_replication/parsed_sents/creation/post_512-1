(ROOT
  (S
    (VP (VBZ Seems)
      (SBAR (IN that)
        (S
          (NP (DT the) (JJ sham) (NN secular) (NN purpose) (NN determination))
          (VP (VBD was)
            (PP (IN in)
              (NP (NN fact)))
            (ADJP (JJ relevant)
              (PP (IN after)
                (NP (DT all))))))))
    (. .)))

root(ROOT-0, Seems-1)
complm(relevant-11, that-2)
det(determination-7, the-3)
amod(determination-7, sham-4)
nn(determination-7, secular-5)
nn(determination-7, purpose-6)
