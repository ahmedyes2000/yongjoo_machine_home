(ROOT
  (S
    (S
      (NP (JJ Transitional) (NNS forms))
      (VP (VBP are)
        (ADVP (RB generally))
        (VP (VBG lacking)
          (PP (IN at)
            (NP (DT the) (NN species) (NN level))))))
    (, ,)
    (CC but)
    (S
      (NP (PRP they))
      (VP (VBP are)
        (ADJP (JJ abundant)
          (PP (IN between)
            (NP (JJR larger) (NNS groups))))))
    (. .)))

amod(forms-2, Transitional-1)
nsubj(lacking-5, forms-2)
aux(lacking-5, are-3)
advmod(lacking-5, generally-4)
root(ROOT-0, lacking-5)
det(level-9, the-7)
nn(level-9, species-8)
prep_at(lacking-5, level-9)
nsubj(abundant-14, they-12)
cop(abundant-14, are-13)
conj_but(lacking-5, abundant-14)
amod(groups-17, larger-16)
prep_between(abundant-14, groups-17)

