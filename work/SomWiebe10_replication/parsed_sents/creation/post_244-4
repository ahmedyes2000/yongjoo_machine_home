(ROOT
  (S
    (VP
      (VP (VB Be)
        (VP (VBN prepared)
          (SBAR
            (WHNP (WDT which))
            (S
              (VP (MD might)
                (VP (VB advise)
                  (VP (VB be)
                    (ADJP (JJ patient)))))))))
      (CC and)
      (VP (VB have)
        (NP (NN pity))))
    (. .)))

auxpass(prepared-2, Be-1)
root(ROOT-0, prepared-2)
nsubj(advise-5, which-3)
aux(advise-5, might-4)
ccomp(prepared-2, advise-5)
cop(patient-7, be-6)
dep(advise-5, patient-7)
conj_and(prepared-2, have-9)
dobj(have-9, pity-10)

