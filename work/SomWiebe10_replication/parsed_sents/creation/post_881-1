(ROOT
  (S ('' '')
    (NP (DT A) (NN day))
    (VP (VBZ is)
      (NP
        (NP (DT a) (CD thousand) (NNS years))
        ('' '')
        (SBAR
          (S
            (VP (VBZ is)
              (ADJP
                (ADVP (RB no) (RBR more))
                (JJ literal))
              (ADVP (RB then))
              (SBAR
                (WHADVP (WRB when))
                (S
                  (NP (NNP Jesus))
                  (VP (VBD said)
                    (SBAR
                      (S
                        (NP (PRP you))
                        (VP (VBP have)
                          (S
                            (VP (TO to)
                              (VP (VB forgive)
                                (NP
                                  (NP (DT a) (NN person)
                                    (QP (CD 7) (NNS times)))
                                  (ADJP (CD 7)))))))))))))))))
    (. .)))

det(day-3, A-2)
nsubj(years-7, day-3)
