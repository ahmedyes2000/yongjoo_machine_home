(ROOT
  (S
    (S
      (ADVP (RB Nobody))
      (, ,)
      (NP (PRP you))
      (VP (MD can)
        (VP (VB say)
          (S
            (NP (PRP you))
            (VP (VB believe)
              (PP (IN in)
                (NP (DT the) (JJ literal) (NNS words))))))))
    (, ,)
    (CC but)
    (S
      (NP (PRP you))
      (VP (VBD did) (RB n't)
        (VP (VB answer)
          (NP
            (NP (DT the) (NN question))
            (WHPP (IN of)
              (WHNP (WDT which) (NNS words)))))))
    (. .)))

advmod(say-5, Nobody-1)
nsubj(say-5, you-3)
aux(say-5, can-4)
root(ROOT-0, say-5)
nsubj(believe-7, you-6)
ccomp(say-5, believe-7)
det(words-11, the-9)
amod(words-11, literal-10)
prep_in(believe-7, words-11)
nsubj(answer-17, you-14)
aux(answer-17, did-15)
neg(answer-17, n't-16)
