(ROOT
  (S
    (NP (EX There))
    (VP (VBD were)
      (NP
        (NP (NNS pockets))
        (PP (IN of)
          (NP
            (NP (NNS scholars))
            (PP (IN at)
              (NP
                (NP (DT the) (NN time))
                (SBAR
                  (WHNP (WDT that))
                  (S
                    (VP (VBD had)
                      (NP
                        (NP (NNS views))
                        (PP (IN on)
                          (NP (DT these) (NNS things)))))))))))))
    (. .) (. .)))

expl(were-2, There-1)
root(ROOT-0, were-2)
nsubj(were-2, pockets-3)
prep_of(pockets-3, scholars-5)
det(time-8, the-7)
prep_at(scholars-5, time-8)
nsubj(had-10, time-8)
rcmod(time-8, had-10)
dobj(had-10, views-11)
det(things-14, these-13)
prep_on(views-11, things-14)

