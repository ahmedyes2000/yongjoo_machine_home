(ROOT
  (S
    (NP
      (NP (DT The) (JJS worst) (NN case) (NN scenario))
      (SBAR
        (WHNP (WDT which))
        (S
          (VP (VBZ does) (RB not)
            (VP (VB seem)
              (ADJP (RB altogether) (JJ unlikely)))))))
    (VP (VBZ is)
      (SBAR (IN that)
        (S
          (NP (NNP Sternberg))
          (ADVP (RB indeed))
          (VP (VBD experienced)
            (NP (DT a) (JJ hostile) (NN work) (NN environment))))))
    (. .)))

det(scenario-4, The-1)
amod(scenario-4, worst-2)
nn(scenario-4, case-3)
nsubj(unlikely-10, scenario-4)
nsubj(is-11, scenario-4)
aux(unlikely-10, does-6)
neg(unlikely-10, not-7)
cop(unlikely-10, seem-8)
advmod(unlikely-10, altogether-9)
rcmod(scenario-4, unlikely-10)
root(ROOT-0, is-11)
complm(experienced-15, that-12)
nsubj(experienced-15, Sternberg-13)
advmod(experienced-15, indeed-14)
ccomp(is-11, experienced-15)
det(environment-19, a-16)
amod(environment-19, hostile-17)
nn(environment-19, work-18)
dobj(experienced-15, environment-19)

