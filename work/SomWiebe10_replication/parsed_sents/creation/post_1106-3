(ROOT
  (S
    (NP
      (NP (NNP Apart))
      (PP (IN from)
        (NP (DT this) (NN evolution) (NN theory))))
    (VP (VBZ is) (RB not)
      (ADJP (RB fully) (JJ truthful)))
    (. .)))

nsubj(truthful-9, Apart-1)
det(theory-5, this-3)
nn(theory-5, evolution-4)
prep_from(Apart-1, theory-5)
cop(truthful-9, is-6)
neg(truthful-9, not-7)
advmod(truthful-9, fully-8)
root(ROOT-0, truthful-9)

