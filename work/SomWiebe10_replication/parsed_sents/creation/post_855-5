  (S
    (NP (DT The) (NN reality))
    (VP (VBZ is)
      (SBAR (IN that)
        (S
          (NP (PRP we))
          (VP (VBP do) (RB not)
            (VP (VB need)
              (NP (DT a) (NN god)
                (S
                  (VP (TO to)
                    (VP (VB explain)
                      (NP (NN anything))
                      (ADVP (RB all)
                        (NP (DT the) (NN way))
                        (PP (TO to)
                          (NP (DT the) (JJ big) (NN bang)))))))))))))
    (. .)))

det(reality-2, The-1)
nsubj(is-3, reality-2)
root(ROOT-0, is-3)
