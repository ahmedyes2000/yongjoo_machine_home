(ROOT
  (NP ('' '')
    (CC And)
    (NP
      (NP (JJ odd) (NN plus))
      (CC and)
      (NP
        (NP (JJ odd))
        (VP (VBZ equals)
          (NP (DT an) (RB even)))))
    (. .)))

amod(plus-4, odd-3)
root(ROOT-0, plus-4)
conj_and(plus-4, odd-6)
dep(odd-6, equals-7)
det(even-9, an-8)
dobj(equals-7, even-9)

