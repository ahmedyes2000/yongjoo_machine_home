(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP we))
        (VP (VBP move)
          (PP (TO to)
            (NP
              (NP (NNP Behe) (POS 's))
              (JJ likely) (NN defense) (NN position))))))
    (, ,)
    (NP (DT the) (NN situation))
    (VP (VBZ does) (RB n't)
      (VP (VB improve)))
    (. .)))

mark(move-3, If-1)
nsubj(move-3, we-2)
advcl(improve-15, move-3)
poss(position-9, Behe-5)
amod(position-9, likely-7)
nn(position-9, defense-8)
prep_to(move-3, position-9)
det(situation-12, the-11)
nsubj(improve-15, situation-12)
aux(improve-15, does-13)
neg(improve-15, n't-14)
root(ROOT-0, improve-15)

