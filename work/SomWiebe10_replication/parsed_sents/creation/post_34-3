(ROOT
  (S
    (ADVP (WRB Why))
    (NP (NNP Creationism)
      (CC and)
      (NNP Intelligent) (NNP Design))
    (VP (VBP Do) (RB n't)
      (VP (VB Belong) ('' '')))
    (. .)))

advmod(Belong-8, Why-1)
nn(Design-5, Creationism-2)
conj_and(Creationism-2, Intelligent-4)
nn(Design-5, Intelligent-4)
nsubj(Belong-8, Design-5)
aux(Belong-8, Do-6)
neg(Belong-8, n't-7)
root(ROOT-0, Belong-8)

