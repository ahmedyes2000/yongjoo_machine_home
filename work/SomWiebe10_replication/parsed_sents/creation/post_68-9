(ROOT
  (S
    (NP (NNP IOW))
    (ADVP (RB once))
    (ADVP (RB again))
    (NP (PRP you))
    (VP (VBP do) (RB n't)
      (VP (VB have)
        (NP (DT an) (NN explanation))))
    (. .)))

nsubj(have-7, IOW-1)
advmod(have-7, once-2)
advmod(have-7, again-3)
nsubj(have-7, you-4)
aux(have-7, do-5)
neg(have-7, n't-6)
root(ROOT-0, have-7)
det(explanation-9, an-8)
dobj(have-7, explanation-9)

