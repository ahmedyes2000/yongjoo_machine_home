  (S
    (PP (IN In)
      (NP (NN fact)))
    (PRN (, ,)
      (S
        (NP (NNP ID))
        (VP (VBZ does) (RB not)
          (VP (VB infer)
            (NP
              (NP (NN purpose))
              (CC or)
              (NP (DT a) (JJ purposeful) (NN arrangement))))))
      (, ,))
    (NP (PRP it))
    (ADVP (RB merely))
    (VP (VBZ claims)
      (SBAR (IN that)
        (S
          (NP (NN design))
          (VP (VBZ is)
            (NP
              (NP (DT that))
              (SBAR
                (WHNP (WDT which))
                (S
                  (VP (VBZ remains)
                    (SBAR
                      (WHADVP (WRB when))
                      (S
                        (NP (NN science))
                        (VP (VBZ has)
                          (VP (VBN failed)
                            (PRN (-LRB- -LRB-)
                              (ADVP (RB so) (RB far))
                              (-RRB- -RRB-))))))))))
            (S
              (VP (TO to)
                (VP (VB explain)
                  (NP (DT a) (JJ particular) (NN feature)
                    (CC or)
                    (NN system)))))))))
    (. .)))

