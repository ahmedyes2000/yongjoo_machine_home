(ROOT
  (S (`` ``)
    (ADVP (WRB Why))
    (VP (MD would)
      (ADVP (RB God))
      (VP (VB take)
        (NP
          (QP (CD 4.5) (CD billion))
          (NNS years))))
    (: ...)))

advmod(take-5, Why-2)
aux(take-5, would-3)
advmod(take-5, God-4)
root(ROOT-0, take-5)
number(billion-7, 4.5-6)
num(years-8, billion-7)
tmod(take-5, years-8)

