(ROOT
  (S
    (NP (PRP I))
    (VP (VBP guess)
      (S
        (NP (DT both) (NNS sides))
        (VP (VB have)
          (NP (NN faith))
          (, ,)
          (ADVP (RB huh)))))
    (. !)))

nsubj(guess-2, I-1)
root(ROOT-0, guess-2)
det(sides-4, both-3)
nsubj(have-5, sides-4)
ccomp(guess-2, have-5)
dobj(have-5, faith-6)
advmod(have-5, huh-8)

