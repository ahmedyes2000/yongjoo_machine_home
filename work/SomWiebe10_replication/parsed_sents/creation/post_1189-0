(ROOT
  (S
    (ADVP (RB Yes))
    (, ,)
    (SBAR (IN if)
      (S
        (SBAR
          (WHNP (WP what))
          (S
            (NP (PRP you))
            (VP (VBP are)
              (VP (VBG saying)))))
        (VP (VBZ is)
          (ADJP (JJ true)))))
    (, ,)
    (ADVP (RB then))
    (NP (PRP I))
    (VP (VBP 'm)
      (PP (IN for)
        (NP (PRP it))))
    (. .)))

advmod('m-13, Yes-1)
mark(true-9, if-3)
dobj(saying-7, what-4)
nsubj(saying-7, you-5)
aux(saying-7, are-6)
csubj(true-9, saying-7)
cop(true-9, is-8)
advcl('m-13, true-9)
advmod('m-13, then-11)
nsubj('m-13, I-12)
root(ROOT-0, 'm-13)
prep_for('m-13, it-15)

