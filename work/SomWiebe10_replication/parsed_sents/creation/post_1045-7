    (NP (PRP you))
    (ADVP (RB instead))
    (VP (VB settle)
      (PP (IN for)
        (NP
          (NP (DT the) (NN evolution))
          (PP (IN of)
            (NP (DT a) (NN mechanism)))))
      (PP (IN in)
        (NP
          (NP (DT an) (NN organism))
          (SBAR
            (WHNP (WDT that))
            (S
              (VP (MD could)
