(ROOT
  (S
    (NP
      (NP (NNP Exactly))
      (CC and)
      (NP (DT the) (NN evidence)))
    (VP (VBZ supports)
      (NP (DT a)
        (ADJP (RB much) (JJR better))
        (NN explanation))
      (SBAR (IN as)
        (S
          (NP (NNP Nick) (NNP Matzke))
          (VP (VBZ has)
            (VP (VBN shown))))))
    (. .)))

nsubj(supports-5, Exactly-1)
det(evidence-4, the-3)
conj_and(Exactly-1, evidence-4)
nsubj(supports-5, evidence-4)
root(ROOT-0, supports-5)
det(explanation-9, a-6)
advmod(better-8, much-7)
amod(explanation-9, better-8)
dobj(supports-5, explanation-9)
mark(shown-14, as-10)
nn(Matzke-12, Nick-11)
