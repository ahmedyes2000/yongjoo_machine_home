(ROOT
  (S
    (NP (PRP I))
    (VP (VBD had)
      (ADVP (RB always))
      (VP (VBN been)
        (VP (VBN taught)
          (SBAR
            (S
              (NP (PRP it))
              (VP (VBD was)
                (NP (DT the) (NNS plants))))))))
    (. .)))

