(ROOT
  (S
    (PP (IN In)
      (NP (NN fact)))
    (, ,)
    (S
      (NP
        (NP (NNP Behe) (POS 's))
        (NN claim))
      (VP (VBZ is) (RB either)
        (ADJP (JJ tautological))))
    (CC or)
    (S
      (NP (PRP it))
      (VP (VBZ is)
        (ADJP (JJ wrong))))
    (. .)))

prep_in(tautological-9, fact-2)
poss(claim-6, Behe-4)
nsubj(tautological-9, claim-6)
cop(tautological-9, is-7)
advmod(tautological-9, either-8)
root(ROOT-0, tautological-9)
nsubj(wrong-13, it-11)
cop(wrong-13, is-12)
