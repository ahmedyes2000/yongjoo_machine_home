(ROOT
  (FRAG (RB Not)
    (PP (IN in)
      (NP
        (NP (DT the) (NN eye))
        (PP (IN of)
          (NP
            (NP (DT this) (NN beholder))
            (ADVP (RB anyway))))))
    (. .)))

root(ROOT-0, Not-1)
dep(Not-1, in-2)
det(eye-4, the-3)
pobj(in-2, eye-4)
det(beholder-7, this-6)
prep_of(eye-4, beholder-7)
advmod(beholder-7, anyway-8)

