(ROOT
  (S ('' '')
    (PP (IN Less)
      (NP (NN illustratively)))
    (, ,)
    (NP (NNS teachers))
    (VP (VBP do) (RB not)
      (VP (VB have)
        (NP
          (NP (NN freedom))
          (PP (IN of)
            (NP (NN speech))))
        (SBAR
          (WHADVP (WRB when))
          (S
            (VP (VBG teaching)
              (NP (NNS children)))))))
    (. .)))

prep_less(have-8, illustratively-3)
nsubj(have-8, teachers-5)
aux(have-8, do-6)
neg(have-8, not-7)
root(ROOT-0, have-8)
dobj(have-8, freedom-9)
prep_of(freedom-9, speech-11)
advmod(teaching-13, when-12)
advcl(have-8, teaching-13)
dobj(teaching-13, children-14)

