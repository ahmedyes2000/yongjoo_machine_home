(ROOT
  (S
    (S
      (VP (VBG Using)
        (NP (JJ biblical) (NNS myths))
        (PP (IN as)
          (NP
            (NP (NN anything) (JJ other))
            (PP (IN than)
              (NP (NNS analogies)))))))
    (VP
      (VP (VBZ is)
        (NP
          (NP (NN disrespectful))
          (PP (IN of)
            (NP (NN religion)))))
      (CC and)
      (VP (VBZ reduces)
        (NP (PRP$ its) (JJ potential) (NN value))
        (PP (TO to)
          (NP (NN society)))))
    (. .)))

csubj(disrespectful-10, Using-1)
csubj(reduces-14, Using-1)
amod(myths-3, biblical-2)
dobj(Using-1, myths-3)
prep_as(Using-1, anything-5)
amod(anything-5, other-6)
prep_than(anything-5, analogies-8)
cop(disrespectful-10, is-9)
root(ROOT-0, disrespectful-10)
prep_of(disrespectful-10, religion-12)
conj_and(disrespectful-10, reduces-14)
poss(value-17, its-15)
amod(value-17, potential-16)
dobj(reduces-14, value-17)
prep_to(reduces-14, society-19)

