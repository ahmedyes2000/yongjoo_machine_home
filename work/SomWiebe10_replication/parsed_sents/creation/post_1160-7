(ROOT
  (S
    (NP (DT That))
    (VP (VBZ is)
      (SBAR
        (WHADVP (WRB why))
        (S
          (NP (PRP I))
          (VP (VBP accept)
            (NP (PRP it))))))
    (. .)))

nsubj(is-2, That-1)
root(ROOT-0, is-2)
advmod(accept-5, why-3)
nsubj(accept-5, I-4)
advcl(is-2, accept-5)
dobj(accept-5, it-6)

