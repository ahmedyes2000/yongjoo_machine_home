(ROOT
  (S
    (NP (NNS ID-proponents))
    (VP (VBP believe)
      (SBAR (IN that)
        (S
          (NP (NN evolution))
          (VP (VBZ needs)
            (S
              (VP (TO to)
                (VP (VB be)
                  (VP (VBN taught)))))))))
    (. .)))

