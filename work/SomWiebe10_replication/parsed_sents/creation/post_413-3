(ROOT
  (S
    (NP (NNP ID))
    (VP
      (VP (VBZ is) (RB not)
        (NP (DT a) (JJ scientific) (NN theory)))
      (PRN (, ,)
        (INTJ (VB please)))
      (VP (VB read)
        (NP
          (NP (DT the) (NN definition))
          (PP (IN of)
            (SBAR
              (WHNP (WP what))
              (S
                (VP (VBZ constitutes)
                  (NP
                    (NP (DT a) (JJ scientific) (NN theory))
                    (PRN (-LRB- -LRB-)
                      (CC and)
                      (S
                        (NP (RB not) (DT the) (NN layman))
                        (VP (VBZ 's)
                          (NP (NN version))))
                      (-RRB- -RRB-))))))))))
    (. .)))

