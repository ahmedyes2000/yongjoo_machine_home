(ROOT
  (S
    (ADVP (RB However))
    (, ,)
    (NP
      (NP (JJ recent) (NNS findings))
      (PP (IN of)
        (NP (NNS paleoanthropologists))))
    (VP (VBP have)
      (VP (VBN revealed)
        (SBAR (IN that)
          (S
            (NP
              (NP (NNP Australopithecus))
              (, ,)
              (NP (NN Homo) (NNS habilis))
              (, ,)
              (CC and)
              (NP (NN Homo) (NNS erectus)))
            (VP (VBD lived)
              (PP (IN at)
                (NP
                  (NP (JJ different) (NNS parts))
                  (PP (IN of)
                    (NP (DT the) (NN world)))))
              (PP (IN at)
                (NP (DT the) (JJ same) (NN time))))))))
    (. .)))

