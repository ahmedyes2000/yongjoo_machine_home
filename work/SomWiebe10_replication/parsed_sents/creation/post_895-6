(ROOT
  (S
    (VP (VB Do) (RB n't)
      (VP (VB use)
        (NP (DT a) (JJ straw-man) (NN argument))
        (S
          (VP (TO to)
            (VP (VB say)
              (SBAR
                (S
                  (NP (PRP we))
                  (VP (MD should) (RB n't)
                    (VP (VB believe)
                      (SBAR
                        (S
                          (NP (NNP God))
                          (VP (VBD was)
                            (VP (VBG being)
                              (VP (VBN literal)))))))))))))))
    (. .)))

aux(use-3, Do-1)
neg(use-3, n't-2)
root(ROOT-0, use-3)
det(argument-6, a-4)
amod(argument-6, straw-man-5)
dobj(use-3, argument-6)
aux(say-8, to-7)
xcomp(use-3, say-8)
nsubj(believe-12, we-9)
aux(believe-12, should-10)
neg(believe-12, n't-11)
ccomp(say-8, believe-12)
nsubjpass(literal-16, God-13)
aux(literal-16, was-14)
auxpass(literal-16, being-15)
ccomp(believe-12, literal-16)

