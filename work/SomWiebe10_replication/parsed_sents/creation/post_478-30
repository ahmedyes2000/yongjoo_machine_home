(ROOT
  (S
    (NP (PRP I))
    (VP (MD 'll)
      (VP (VB respond)
        (PP (TO to)
          (NP (PRP$ your) (NNS thoughts)))
        (PP (IN on)
          (NP
            (NP (NNP Augustine))
            (PP (IN in)
              (NP (DT another) (NN reply)))))))
    (. .)))

nsubj(respond-3, I-1)
aux(respond-3, 'll-2)
root(ROOT-0, respond-3)
poss(thoughts-6, your-5)
prep_to(respond-3, thoughts-6)
prep_on(respond-3, Augustine-8)
det(reply-11, another-10)
prep_in(Augustine-8, reply-11)

(ROOT
  (NP (NN tj)))

root(ROOT-0, tj-1)

