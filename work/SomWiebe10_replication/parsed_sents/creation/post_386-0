(ROOT
  (S
    (NP (PRP you))
    (VP (VBP have)
      (NP
        (NP
          (NP (JJ such) (NNS books))
          (VP (VBN written)
            (PP (IN as))))
        (, ,)
        (NP
          (NP (DT THE) (NN CONSIPIRACY))
          (PP (IN OF)
            (NP (NNP IGNORANCE))))
        (, ,)
        (PP (IN by)
          (NP (NNP Martin) (NNP Gross)))))
    (. .)))

nsubj(have-2, you-1)
root(ROOT-0, have-2)
amod(books-4, such-3)
dobj(have-2, books-4)
partmod(books-4, written-5)
prep(written-5, as-6)
det(CONSIPIRACY-9, THE-8)
appos(books-4, CONSIPIRACY-9)
prep_of(CONSIPIRACY-9, IGNORANCE-11)
nn(Gross-15, Martin-14)
prep_by(books-4, Gross-15)

