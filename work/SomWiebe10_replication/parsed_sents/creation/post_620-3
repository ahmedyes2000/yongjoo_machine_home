(ROOT
  (S
    (NP (DT The) (NNP DI))
    (VP (VBZ is)
      (VP (VBG attempting)
        (S
          (VP (TO to)
            (VP (VB create)
              (NP (DT a) (NN controversy))
              (SBAR
                (WHADVP (WRB where))
                (S
                  (NP (EX there))
                  (VP (VBZ is)
                    (ADVP (RB really))
                    (NP (NN none))))))))))
    (. .)))

det(DI-2, The-1)
nsubj(attempting-4, DI-2)
xsubj(create-6, DI-2)
