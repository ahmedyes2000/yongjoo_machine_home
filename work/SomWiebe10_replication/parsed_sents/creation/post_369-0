(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is)
      (NP (DT a) (NN fact))
      (SBAR (IN that)
        (S
          (NP (NN design))
          (VP (MD can)
            (VP (VB be)
              (VP (VBN determined)
                (PP (IN in)
                  (NP
                    (NP (DT the) (NN absence))
                    (PP (IN of)
                      (NP (DT a) (NN designer)))))))))))
    (. .)))

nsubj(fact-4, It-1)
cop(fact-4, is-2)
det(fact-4, a-3)
root(ROOT-0, fact-4)
complm(determined-9, that-5)
nsubjpass(determined-9, design-6)
aux(determined-9, can-7)
auxpass(determined-9, be-8)
ccomp(fact-4, determined-9)
det(absence-12, the-11)
