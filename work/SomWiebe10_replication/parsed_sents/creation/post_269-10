(ROOT
  (S
    (NP (DT That))
    (VP (VBZ s)
      (ADVP (RBR more) (IN than))
      (VP (VBG throwing)
        (NP (DT a) (NN spanner))
        (PP (IN into)
          (NP (DT the) (NN machine)))
        (, ,)
        (S
          (NP (PRP$ its))
          (VP (VBG targeting)
            (NP (DT a) (NN promoter) (NN sequence))
            (PP (IN with)
              (NP (DT a)
                (ADJP (RB rather) (JJ subtle))
                (NN change)))))))
    (. .)))

nsubj(throwing-5, That-1)
aux(throwing-5, s-2)
advmod(than-4, more-3)
advmod(throwing-5, than-4)
root(ROOT-0, throwing-5)
det(spanner-7, a-6)
dobj(throwing-5, spanner-7)
det(machine-10, the-9)
prep_into(throwing-5, machine-10)
nsubj(targeting-13, its-12)
partmod(throwing-5, targeting-13)
det(sequence-16, a-14)
nn(sequence-16, promoter-15)
dobj(targeting-13, sequence-16)
det(change-21, a-18)
advmod(subtle-20, rather-19)
amod(change-21, subtle-20)
prep_with(targeting-13, change-21)

