(ROOT
  (S (CC But)
    (NP (PRP I))
    (VP (VBP understand)
      (SBAR (IN that)
        (S
          (S
            (VP (VBG calling)
              (NP (PRP him))
              (NP (DT a) (NN racist))))
          (VP (VBZ is)
            (ADJP
              (ADJP (RB much) (JJR easier))
              (SBAR (IN than)
                (S
                  (VP (TO to)
                    (VP (VB refute)
                      (NP (PRP$ his) (NN theory)))))))))))
    (. .)))

cc(understand-3, But-1)
nsubj(understand-3, I-2)
root(ROOT-0, understand-3)
complm(easier-11, that-4)
csubj(easier-11, calling-5)
iobj(calling-5, him-6)
det(racist-8, a-7)
dobj(calling-5, racist-8)
cop(easier-11, is-9)
advmod(easier-11, much-10)
ccomp(understand-3, easier-11)
mark(refute-14, than-12)
aux(refute-14, to-13)
dep(easier-11, refute-14)
poss(theory-16, his-15)
dobj(refute-14, theory-16)

