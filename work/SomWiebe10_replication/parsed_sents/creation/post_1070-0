(ROOT
  (S
    (SBAR
      (WHNP (WP What))
      (S
        (NP (PRP I))
        (VP (VBP feel)
          (PP (IN in)
            (NP (DT this) (NN context))))))
    (VP (VBZ is)
      (NP
        (NP (NN creationism))
        (SBAR
          (S
            (VP (VBZ is)
              (NP
                (NP (NN nothing))
                (PP (CC but)
                  (NP (DT a) (JJ wrong) (NN assumption)))))))))
    (. .)))

dobj(feel-3, What-1)
nsubj(feel-3, I-2)
csubj(creationism-8, feel-3)
det(context-6, this-5)
prep_in(feel-3, context-6)
cop(creationism-8, is-7)
root(ROOT-0, creationism-8)
cop(nothing-10, is-9)
rcmod(creationism-8, nothing-10)
prep(nothing-10, but-11)
det(assumption-14, a-12)
amod(assumption-14, wrong-13)
dep(but-11, assumption-14)

