(ROOT
  (S
    (NP
      (NP (DT The) (NN purpose))
      (PP (IN of)
        (NP (DT the) (NNP Bible))))
    (VP (VBZ is)
      (NP
        (NP (NN man) (POS 's))
        (NN salvation))
      (PP (RB not)
        (NP (DT a) (NN science) (NN text) (NN book))))
    (. .)))

det(purpose-2, The-1)
nsubj(salvation-9, purpose-2)
det(Bible-5, the-4)
prep_of(purpose-2, Bible-5)
cop(salvation-9, is-6)
poss(salvation-9, man-7)
root(ROOT-0, salvation-9)
prep(salvation-9, not-10)
det(book-14, a-11)
nn(book-14, science-12)
nn(book-14, text-13)
pobj(not-10, book-14)

