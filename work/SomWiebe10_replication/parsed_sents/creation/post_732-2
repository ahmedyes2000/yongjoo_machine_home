(ROOT
  (S
    (NP (NNP Creationism))
    (VP (VBZ does) (RB n't)
      (VP (VB stand)
        (CC or)
        (VB fall)
        (PP (IN on)
          (NP (DT that) (NN statement)))))
    (. .)))

nsubj(stand-4, Creationism-1)
nsubj(fall-6, Creationism-1)
aux(stand-4, does-2)
neg(stand-4, n't-3)
root(ROOT-0, stand-4)
conj_or(stand-4, fall-6)
det(statement-9, that-8)
prep_on(stand-4, statement-9)

