(ROOT
  (S
    (NP (PRP She))
    (VP (VBD urged)
      (S
        (NP (PRP them))
        (RB not)
        (VP (TO to)
          (VP (VB attack)
            (NP (PRP$ his) (JJ religious) (NNS views))
            (SBAR (RB so) (RB as) (RB not)
              (S
                (VP (TO to)
                  (VP (VB make)
                    (S
                      (NP (PRP him))
                      (NP (DT a) (NN martyr)))))))))))
    (. .)))

nsubj(urged-2, She-1)
root(ROOT-0, urged-2)
nsubj(attack-6, them-3)
neg(attack-6, not-4)
aux(attack-6, to-5)
xcomp(urged-2, attack-6)
