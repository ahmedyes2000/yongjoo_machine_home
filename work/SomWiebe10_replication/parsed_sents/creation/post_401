root(ROOT-0, testable-3)

(ROOT
  (S
    (VP (VB Suggest)
      (NP
        (NP (DT an) (NN experiment))
        (SBAR
          (WHNP (WDT that))
          (S
            (VP (MD would)
              (VP (VB prove)
                (NP (NNP Intelligent) (NNP Design))))))))
    (. .)))

root(ROOT-0, Suggest-1)
