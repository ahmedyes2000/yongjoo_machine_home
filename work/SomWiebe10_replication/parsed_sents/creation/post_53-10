(ROOT
  (S
    (NP
      (NP (DT A) (NN deck))
      (PP (IN of)
        (NP (NNS cards))))
    (VP (VBZ is)
      (VP (VBN cut)
        (PP (IN before)
          (S
            (VP (VBG dealing)
              (PP (IN as)
                (NP
                  (NP (DT a) (NN check))
                  (PP (IN on)
                    (NP (JJ intentional) (VBG sorting))))))))))
    (. .)))

det(deck-2, A-1)
nsubjpass(cut-6, deck-2)
prep_of(deck-2, cards-4)
auxpass(cut-6, is-5)
root(ROOT-0, cut-6)
