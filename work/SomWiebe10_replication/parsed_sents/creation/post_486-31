(ROOT
  (S
    (NP (DT The) (NN case))
    (VP (VBD was)
      (ADVP (RB not) ('' ''))
      (NP
        (NP (NNP Intelligent) (NNP Design)
          (CC v.)
          (NNP US) (NNP Constitution))
        ('' '')
        (VP (VBN decided)
          (PP (IN for)
            (NP (DT all) (NN time))))))
    (. .)))

det(case-2, The-1)
nsubj(Design-7, case-2)
cop(Design-7, was-3)
neg(Design-7, not-4)
nn(Design-7, Intelligent-6)
root(ROOT-0, Design-7)
nn(Constitution-10, US-9)
conj_v.(Design-7, Constitution-10)
partmod(Design-7, decided-12)
det(time-15, all-14)
prep_for(decided-12, time-15)

