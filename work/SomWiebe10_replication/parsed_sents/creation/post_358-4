(ROOT
  (S
    (ADVP (RB IOW))
    (, ,)
    (NP (PRP you))
    (ADVP (RB just))
    (VP (VBD posted)
      (NP (DT a) (JJ classic) (NN argument))
      (PP (IN from)
        (NP (NN ignorance))))
    (. .)))

advmod(posted-5, IOW-1)
nsubj(posted-5, you-3)
advmod(posted-5, just-4)
root(ROOT-0, posted-5)
det(argument-8, a-6)
amod(argument-8, classic-7)
dobj(posted-5, argument-8)
prep_from(posted-5, ignorance-10)

