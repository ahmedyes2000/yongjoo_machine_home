(ROOT
  (S (CC And)
    (NP (EX there))
    (VP (VBP are)
      (NP
        (NP (NNS masses))
        (PP (IN of)
          (NP
            (NP (NN evidence))
            (PP (IN for)
              (NP
                (NP (DT the) (NN theory))
                (PP (IN of)
                  (NP (NN evolution)))))))))
    (. .)))

cc(are-3, And-1)
expl(are-3, there-2)
root(ROOT-0, are-3)
nsubj(are-3, masses-4)
prep_of(masses-4, evidence-6)
det(theory-9, the-8)
