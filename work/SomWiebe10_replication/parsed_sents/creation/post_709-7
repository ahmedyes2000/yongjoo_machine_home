(ROOT
  (S
    (ADVP (RB Personally))
    (, ,)
    (NP (PRP I))
    (VP (VBP think)
      (SBAR
        (S
          (NP (PRP you))
          (VP (MD can)
            (VP (VB believe)
              (SBAR
                (WHNP (WP what))
                (S
                  (NP (PRP you))
                  (VP (VBP want)
                    (SBAR (CC but)
                      (S
                        (NP (PRP it))
                        (VP (VBZ is)
                          (ADJP (RB very) (JJ foolish))
                          (S
                            (VP (TO to)
                              (VP (VB try)
                                (CC and)
                                (VB find)
                                (NP
                                  (NP (DT some) (NN kind))
                                  (PP (IN of)
                                    (NP (JJ scientific) (NN backing))))
                                (PP (IN for)
                                  (NP
                                    (NP (DT these) (JJ religious) (NN belief))
                                    (CC and)
                                    (NP (NNS mysteries))))))))))))))))))
    (. .)))

advmod(think-4, Personally-1)
nsubj(think-4, I-3)
root(ROOT-0, think-4)
nsubj(believe-7, you-5)
aux(believe-7, can-6)
ccomp(think-4, believe-7)
dep(want-10, what-8)
dobj(foolish-15, what-8)
nsubj(want-10, you-9)
ccomp(believe-7, want-10)
cc(foolish-15, but-11)
nsubj(foolish-15, it-12)
xsubj(try-17, it-12)
cop(foolish-15, is-13)
advmod(foolish-15, very-14)
ccomp(want-10, foolish-15)
aux(try-17, to-16)
xcomp(foolish-15, try-17)
xcomp(foolish-15, find-19)
conj_and(try-17, find-19)
det(kind-21, some-20)
dobj(try-17, kind-21)
amod(backing-24, scientific-23)
prep_of(kind-21, backing-24)
det(belief-28, these-26)
amod(belief-28, religious-27)
prep_for(try-17, belief-28)
prep_for(try-17, mysteries-30)
conj_and(belief-28, mysteries-30)

