(ROOT
  (INTJ (UH OK) (. .)))

root(ROOT-0, OK-1)

(ROOT
  (S
    (NP (RB Here))
    (VP (VBZ 's)
      (NP
        (NP (DT the) (NN way))
        (SBAR
          (S
            (NP (PRP I))
            (VP (VBP think)
              (PP (IN about)
                (NP (PRP it))))))))
    (. .)))

