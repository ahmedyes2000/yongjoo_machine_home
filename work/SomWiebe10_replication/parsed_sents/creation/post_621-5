(ROOT
  (S
    (NP (PRP I))
    (ADVP (RB also))
    (VP (VBP have)
      (VP (VBN seen)
        (NP
          (NP (DT a) (NN lot))
          (PP (IN of)
            (NP (NN controversy)))
          (PRN (-LRB- -LRB-)
            (SBAR
              (WHADVP (WRB how))
              (S
                (NP (JJ meaningful) (, ,) (JJ only) (NN time))
                (VP (MD will)
                  (VP (VB tell)))))
            (-RRB- -RRB-))
          (ADJP (RB even)
            (PP (IN within)
              (NP (DT the) (JJ Neo-Darwinist) (NN camp)))))))
    (. .)))

nsubj(seen-4, I-1)
advmod(seen-4, also-2)
aux(seen-4, have-3)
root(ROOT-0, seen-4)
det(lot-6, a-5)
dobj(seen-4, lot-6)
prep_of(lot-6, controversy-8)
advmod(tell-16, how-10)
amod(time-14, meaningful-11)
amod(time-14, only-13)
