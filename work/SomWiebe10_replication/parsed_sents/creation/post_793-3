(ROOT
  (S
    (S
      (NP (PRP I))
      (VP (VBP 've)
        (VP (VBP do) (RB not)
          (VP (VB have)
            (NP
              (NP (DT any) (NNS issues))
              (PP (IN with)
                (NP (PRP$ your) (NNS beliefs))))))))
    (: ...)
    (CC but)
    (S
      (NP (PRP I))
      (VP (VBP do)
        (VP (VB question)
          (NP (PRP$ their) (NN use))
          (PP (IN in)
            (NP (DT a) (JJ scientific) (NN context))))))
    (. .)))

nsubj(have-5, I-1)
aux(have-5, 've-2)
aux(have-5, do-3)
neg(have-5, not-4)
root(ROOT-0, have-5)
det(issues-7, any-6)
dobj(have-5, issues-7)
poss(beliefs-10, your-9)
prep_with(issues-7, beliefs-10)
nsubj(question-15, I-13)
aux(question-15, do-14)
conj_but(have-5, question-15)
poss(use-17, their-16)
dobj(question-15, use-17)
det(context-21, a-19)
amod(context-21, scientific-20)
prep_in(question-15, context-21)

