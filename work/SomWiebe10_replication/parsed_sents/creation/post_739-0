(ROOT
  (S
    (S
      (SBAR (IN While)
        (S
          (NP (PRP I))
          (VP (VBP have)
            (NP (PRP$ my) (JJ own) (NNS reasons))
            (PP (IN for)
              (S
                (VP (VBG believing)
                  (NP (PRP he))))))))
      (VP (VBD did)
        (SBAR (IN that) (, ,)
          (S
            (NP
              (NP (PRP$ my) (JJ personal) (NNS opinions))
              (PP (VBG regarding)
                (NP (NN religion))))
            (VP (VBP are)
              (NP
                (NP (DT a) (NN bit) (NN outside))
                (PP (IN of)
                  (NP (DT the) (NN mainstream)))))))))
    (, ,) (IN so)
    (S
      (NP (PRP I))
      (VP (VBP 'm) (RB not)
        (VP (VBG going)
          (S
            (VP (TO to)
              (VP (VB discuss)
                (NP (PRP them))))))))
    (. .)))

