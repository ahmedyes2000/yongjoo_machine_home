(ROOT
  (S
    (NP (DT The) (NN religion))
    (VP (VBZ \/)
      (NP
        (NP (JJ ethnic) (NN group))
        (SBAR
          (WHNP (WDT that))
          (S
            (VP (VBD was)
              (ADVP (RB around))
              (PP (IN for)
                (NP (NNS centuries)))
              (SBAR (IN before)
                (S
                  (NP (NNP Jesus))
                  (VP (VBD was)
                    (VP (VBN born))))))))))
    (. ?)))

det(religion-2, The-1)
nsubj(\/-3, religion-2)
root(ROOT-0, \/-3)
amod(group-5, ethnic-4)
dobj(\/-3, group-5)
nsubj(was-7, group-5)
rcmod(group-5, was-7)
