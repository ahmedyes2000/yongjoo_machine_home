(ROOT
  (S
    (NP (PRP I))
    (VP (VBP hope)
      (SBAR
        (S
          (NP (PRP you))
          (VP (VBP understand)
            (NP (DT this) (JJ major) (NN distinction))))))
    (. .)))

nsubj(hope-2, I-1)
root(ROOT-0, hope-2)
nsubj(understand-4, you-3)
ccomp(hope-2, understand-4)
det(distinction-7, this-5)
amod(distinction-7, major-6)
dobj(understand-4, distinction-7)

