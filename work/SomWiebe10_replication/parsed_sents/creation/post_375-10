(ROOT
  (S ('' '')
    (S
      (NP (PRP You))
      (VP (MD should) (RB n't)
        (VP (VB talk)
          (PP (IN about)
            (NP (NNP God)))
          (PP (IN in)
            (NP (DT a) (NN science) (NN classroom))))))
    (, ,) ('' '')
    (NP (PRP he))
    (VP (VBD said))
    (. .)))

nsubj(talk-5, You-2)
aux(talk-5, should-3)
neg(talk-5, n't-4)
ccomp(said-15, talk-5)
prep_about(talk-5, God-7)
det(classroom-11, a-9)
nn(classroom-11, science-10)
prep_in(talk-5, classroom-11)
nsubj(said-15, he-14)
root(ROOT-0, said-15)

