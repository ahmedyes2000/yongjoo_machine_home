(ROOT
  (S
    (SBAR
      (WHNP (WP What))
      (S
        (NP (PRP I))
        (VP (VBP get)
          (ADJP (JJ angry) (IN about)))))
    (VP (VBZ is)
      (SBAR
        (WHADVP (WRB when))
        (S
          (NP (NNS people))
          (VP (VBP attempt)
            (S
              (VP (TO to)
                (VP (VB teach)
                  (NP
                    (NP (NNP ID))
                    (PP (IN in)
                      (NP (NN school)))))))))))
    (. .)))

dobj(angry-4, What-1)
nsubj(angry-4, I-2)
dep(angry-4, get-3)
csubj(is-6, angry-4)
dep(angry-4, about-5)
root(ROOT-0, is-6)
advmod(attempt-9, when-7)
nsubj(attempt-9, people-8)
xsubj(teach-11, people-8)
advcl(is-6, attempt-9)
aux(teach-11, to-10)
xcomp(attempt-9, teach-11)
dobj(teach-11, ID-12)
prep_in(ID-12, school-14)

