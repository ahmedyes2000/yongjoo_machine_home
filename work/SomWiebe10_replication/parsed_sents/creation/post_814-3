(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ contradicts)
      (NP (DT the) (NN concept))
      (SBAR (IN that)
        (S
          (NP (NN creation))
          (VP (VBD was)
            (ADJP (JJ Good))
            (SBAR
              (WHADVP (WRB when))
              (S
                (NP (NNP Adam)
                  (CC and)
                  (NNP Eve))
                (VP (VBD were)
                  (VP (VBN created)))))))))
    (. .)))

nsubj(contradicts-2, It-1)
root(ROOT-0, contradicts-2)
det(concept-4, the-3)
dobj(contradicts-2, concept-4)
complm(Good-8, that-5)
nsubj(Good-8, creation-6)
cop(Good-8, was-7)
ccomp(contradicts-2, Good-8)
advmod(created-14, when-9)
nsubjpass(created-14, Adam-10)
conj_and(Adam-10, Eve-12)
nsubjpass(created-14, Eve-12)
auxpass(created-14, were-13)
advcl(Good-8, created-14)

