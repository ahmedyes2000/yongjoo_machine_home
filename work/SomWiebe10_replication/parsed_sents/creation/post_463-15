  (S
    (NP (PRP He))
    (VP (VBD was) (RB not)
      (VP (VBN fired)
        (CC or)
        (VBN asked)
        (S
          (VP (TO to)
            (VP (VB resign)
              (PP (IN at)
                (NP (DT the) (NNP Smithsonian))))))))
    (. .)))

