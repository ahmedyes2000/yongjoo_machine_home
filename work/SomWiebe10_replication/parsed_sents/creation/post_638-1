(ROOT
  (S ('' '')
    (S
      (PP (IN Because))
      (NP (PRP it))
      (VP (VBZ is) (RB n't)))
    (, ,)
    (NP (DT that))
    (VP (VBZ 's)
      (FRAG
        (WHADVP (WRB why))))
    (. .)))

prep(is-4, Because-2)
nsubj(is-4, it-3)
ccomp('s-8, is-4)
neg(is-4, n't-5)
nsubj('s-8, that-7)
root(ROOT-0, 's-8)
dep('s-8, why-9)

