(ROOT
  (S
    (NP (DT Every) (NN time))
    (NP (PRP I))
    (VP (VBP hear)
      (SBAR
        (SBAR
          (S
            (NP (DT a) (JJ single) (NN word))
            (VP (VBD mentioned)
              (PP (IN about)
                (NP (DT the) (NNS TTSS))))))
        (CC and)
        (SBAR (IN that)
          (S
            (NP (PRP it))
            (VP
              (ADVP (RB somehow))
              (VBZ gives)
              (NP (NN flagellar) (NN evolution))
              (NP
                (NP (DT some) (NN credibility))
                (SBAR
                  (S
                    (NP (PRP I))
                    (VP (VBP lose)
                      (NP (DT a) (JJ few) (NNS months))
                      (PP (IN off)
                        (NP (PRP$ my) (NN life) (NN span))))))))))))
    (. .)))

det(time-2, Every-1)
nsubj(hear-4, time-2)
