(ROOT
  (S
    (NP (PRP I))
    (VP (VBD asked)
      (NP (DT the) (NNS questions))
      (S
        (VP (TO to)
          (VP (VB give)
            (NP (NNP Mr) (NNP Behe))
            (NP
              (NP (DT a) (NN hint))
              (PP (IN about)
                (SBAR
                  (WHADVP (WRB how))
                  (S
                    (VP (TO to)
                      (VP (VB formulate)
                        (NP (DT a) (JJ proper) (NN argument))))))))))))
    (. .)))

nsubj(asked-2, I-1)
root(ROOT-0, asked-2)
det(questions-4, the-3)
dobj(asked-2, questions-4)
aux(give-6, to-5)
