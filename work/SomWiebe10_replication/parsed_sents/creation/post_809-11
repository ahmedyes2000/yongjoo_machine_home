  (S
    (S
      (NP
        (NP (DT The) (JJ vast) (NN majority))
        (PP (IN of)
          (NP (JJ scientific) (NNS ideas))))
      (VP (VBP die)
        (NP (DT this) (NN way))))
    (: ;)
    (S
      (NP (PRP it))
      (VP (VBZ 's)
        (ADJP (JJ okay))))
    (. .)))

