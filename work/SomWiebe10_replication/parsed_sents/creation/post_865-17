(ROOT
  (S
    (PP (IN For)
      (NP (PRP them)))
    (, ,)
    (NP (PRP it))
    (VP (VBD was)
      (NP (JJ real) (NN history))
      (SBAR (IN that)
        (S
          (NP
            (NP (NN earth))
            (CC and)
            (NP (NNS heavens) (, ,) (NN man)
              (CC and)
              (NNS animals)))
          (ADVP (RB suddenly))
          (VP (VBD were)
            (VP (VBN created))))))
    (. .)))

prep_for(history-7, them-2)
nsubj(history-7, it-4)
cop(history-7, was-5)
amod(history-7, real-6)
root(ROOT-0, history-7)
complm(created-18, that-8)
nsubjpass(created-18, earth-9)
conj_and(earth-9, heavens-11)
nsubjpass(created-18, heavens-11)
conj_and(earth-9, man-13)
conj_and(heavens-11, man-13)
conj_and(earth-9, animals-15)
conj_and(heavens-11, animals-15)
advmod(created-18, suddenly-16)
auxpass(created-18, were-17)
ccomp(history-7, created-18)

