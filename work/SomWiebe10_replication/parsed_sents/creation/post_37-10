  (S
    (PP (IN In)
      (NP (JJ other) (NNS words)))
    (, ,)
    (NP (NNP ID))
    (VP
      (VP (VBZ uses)
        (NP
          (NP (JJ misleading) (NN terminology))
          (CONJP (RB as) (RB well) (IN as))
          (NP (NN bait))))
      (CC and)
      (VP (VBP switch)
        (S
          (VP (TO to)
            (VP (VB make)
              (NP (PRP$ its) (NNS claims)))))))
    (. .)))

