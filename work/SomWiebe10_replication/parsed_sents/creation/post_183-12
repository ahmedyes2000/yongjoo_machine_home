(ROOT
  (S
    (NP (DT The) (NNP Lenski) (NN study))
    (VP (VBD did) (RB not)
      (VP (VB show)
        (NP
          (NP (NN e.) (NNS coli))
          (VP (VBG changing)
            (PP (IN into)
              (NP
                (NP (DT a) (JJ different) (NN species))
                (PP (IN of)
                  (NP (NNS bacteria)))))))))
    (. .)))

det(study-3, The-1)
nn(study-3, Lenski-2)
nsubj(show-6, study-3)
aux(show-6, did-4)
neg(show-6, not-5)
root(ROOT-0, show-6)
nn(coli-8, e.-7)
dobj(show-6, coli-8)
partmod(coli-8, changing-9)
det(species-13, a-11)
