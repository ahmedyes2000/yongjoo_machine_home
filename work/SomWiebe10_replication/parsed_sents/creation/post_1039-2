(ROOT
  (S
    (NP (EX There))
    (VP (VBZ 's)
      (NP
        (NP (DT no) (NN evidence))
        (PP (IN for)
          (NP (DT the) (JJ biblical) (NN story)))))
    (. ?)))

expl('s-2, There-1)
root(ROOT-0, 's-2)
det(evidence-4, no-3)
nsubj('s-2, evidence-4)
det(story-8, the-6)
amod(story-8, biblical-7)
prep_for(evidence-4, story-8)

