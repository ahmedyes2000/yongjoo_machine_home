(ROOT
  (S
    (NP (NNP Hard) (NN objective) (NN science))
    (VP (MD will)
      (VP (VB provide)
        (NP (DT the) (NNS answers))
        (PP (TO to)
          (NP (PRP$ our) (NNS questions)))))
    (. .)))

nn(science-3, Hard-1)
nn(science-3, objective-2)
nsubj(provide-5, science-3)
aux(provide-5, will-4)
root(ROOT-0, provide-5)
det(answers-7, the-6)
dobj(provide-5, answers-7)
poss(questions-10, our-9)
prep_to(provide-5, questions-10)

