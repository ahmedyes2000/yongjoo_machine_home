(ROOT
  (S ('' '')
    (NP (RB So) (PRP it))
    (VP (MD can)
      (VP (VB be)
        (NP (DT a) (JJ good) (NN discussion) (NN topic))))
    (. .)))

advmod(it-3, So-2)
nsubj(topic-9, it-3)
aux(topic-9, can-4)
cop(topic-9, be-5)
det(topic-9, a-6)
amod(topic-9, good-7)
nn(topic-9, discussion-8)
root(ROOT-0, topic-9)

