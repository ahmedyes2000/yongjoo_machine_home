(ROOT
  (SBARQ
    (SBAR (IN If)
      (S
        (NP (NN man))
        (VP (VBD evolved)
          (PP (IN from)
            (NP (NNS apes))))))
    (, ,)
    (WHADVP
      (ADVP (RB then))
      (WRB why))
    (SQ (VBZ is)
      (NP (EX there))
      (ADVP (RB still))
      (ADJP (JJ apes)))
    (. ?)))

mark(evolved-3, If-1)
nsubj(evolved-3, man-2)
advcl(apes-12, evolved-3)
prep_from(evolved-3, apes-5)
advmod(why-8, then-7)
advmod(apes-12, why-8)
cop(apes-12, is-9)
expl(apes-12, there-10)
advmod(apes-12, still-11)
root(ROOT-0, apes-12)

