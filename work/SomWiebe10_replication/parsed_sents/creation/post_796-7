(ROOT
  (S
    (NP (EX There))
    (ADVP (RB supposedly))
    (VP (VBZ is)
      (NP
        (NP (JJ scientific) (NN evidence))
        (PP (IN for)
          (NP (DT both) (NNS beliefs)))))
    (: ...)))

expl(is-3, There-1)
advmod(is-3, supposedly-2)
root(ROOT-0, is-3)
amod(evidence-5, scientific-4)
nsubj(is-3, evidence-5)
det(beliefs-8, both-7)
prep_for(evidence-5, beliefs-8)

