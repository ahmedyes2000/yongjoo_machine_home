(ROOT
  (S ('' '')
    (S
      (ADVP (RB Fortunately))
      (, ,)
      (NP (DT the) (NNS courts))
      (VP (VBP have)
        (VP (VBN let)
          (S
            (NP (NNS scientists))
            (VP (VB decide)
              (SBAR
                (WHNP (WP what))
                (S
                  (VP (VBZ is)
                    (NP (NN science))))))))))
    (CC and)
    (S
      (NP (NNS theologists))
      (VP (VBP decide)
        (SBAR
          (WHNP (WP what))
          (S
            (VP (VBZ is)
              (NP (NN religion)))))))
    (. .)))

advmod(let-7, Fortunately-2)
det(courts-5, the-4)
nsubj(let-7, courts-5)
aux(let-7, have-6)
root(ROOT-0, let-7)
nsubj(decide-9, scientists-8)
ccomp(let-7, decide-9)
nsubj(science-12, what-10)
cop(science-12, is-11)
ccomp(decide-9, science-12)
nsubj(decide-15, theologists-14)
conj_and(let-7, decide-15)
nsubj(religion-18, what-16)
cop(religion-18, is-17)
ccomp(decide-15, religion-18)

