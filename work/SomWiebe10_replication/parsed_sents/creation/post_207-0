(ROOT
  (S (CC And)
    (NP (DT this) (NN site))
    (VP (VBZ says)
      (S
        (NP (PRP it))
        (ADJP (JJR better)
          (SBAR (IN than)
            (S
              (NP (PRP I))
              (VP (MD could)))))))
    (. .)))

cc(says-4, And-1)
det(site-3, this-2)
nsubj(says-4, site-3)
root(ROOT-0, says-4)
nsubj(better-6, it-5)
xcomp(says-4, better-6)
mark(could-9, than-7)
nsubj(could-9, I-8)
dep(better-6, could-9)

