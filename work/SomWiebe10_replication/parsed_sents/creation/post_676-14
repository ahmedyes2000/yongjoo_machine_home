(ROOT
  (NP
    (NP (DT All) (JJ clear) (NN evidence))
    (PP (IN of)
      (NP (DT a) (NN lack)))
    (PP (IN of)
      (NP (NN argument)))
    (. .)))

det(evidence-3, All-1)
amod(evidence-3, clear-2)
root(ROOT-0, evidence-3)
det(lack-6, a-5)
prep_of(evidence-3, lack-6)
prep_of(evidence-3, argument-8)

