  (S
    (SBAR (IN If)
      (S
        (NP (DT this))
        (VP (VBD were)
          (NP (DT an) (NN example)
            (S
              (VP (TO to)
                (VP (VB follow))))))))
    (, ,)
    (NP (PRP we))
    (VP (MD should)
      (VP (VB enact)
        (NP (DT NO) (NN judgement))
        (PP (IN on)
          (NP (NN anyone)))))
    (. .)))

mark(example-5, If-1)
nsubj(example-5, this-2)
cop(example-5, were-3)
det(example-5, an-4)
advcl(enact-11, example-5)
aux(follow-7, to-6)
