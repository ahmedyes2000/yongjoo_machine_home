(ROOT
  (S
    (PP (RB because) (IN in)
      (NP (CD one) (NN hand)))
    (NP (PRP it))
    (VP (VBZ seems)
      (S
        (VP (TO to)
          (VP (VB confirm)
            (NP (NN creation))))))
    (. .) (. .)))

advmod(seems-6, because-1)
advcl(seems-6, in-2)
num(hand-4, one-3)
prep_in(seems-6, hand-4)
nsubj(seems-6, it-5)
xsubj(confirm-8, it-5)
root(ROOT-0, seems-6)
aux(confirm-8, to-7)
xcomp(seems-6, confirm-8)
dobj(confirm-8, creation-9)

