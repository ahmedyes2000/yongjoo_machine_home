(ROOT
  (S
    (SBAR (IN Because) (IN if)
      (S
        (NP (DT this) (NN person))
        (VP (VBD was) (RB n't)
          (ADVP (RB then)))))
    (NP (PRP they))
    (ADVP (RB just))
    (VP (VBP are) (RB n't)
      (ADJP (JJ wonderful))
      (ADVP (RB anymore)))
    (. .)))

mark(was-5, Because-1)
dep(was-5, if-2)
det(person-4, this-3)
nsubj(was-5, person-4)
advcl(wonderful-12, was-5)
neg(was-5, n't-6)
advmod(was-5, then-7)
nsubj(wonderful-12, they-8)
advmod(wonderful-12, just-9)
cop(wonderful-12, are-10)
neg(wonderful-12, n't-11)
root(ROOT-0, wonderful-12)
advmod(wonderful-12, anymore-13)

