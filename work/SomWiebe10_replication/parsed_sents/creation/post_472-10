(ROOT
  (S
    (VP (VB Imagine)
      (NP
        (NP (DT a) (NNS scientists))
        (VP (VBG looking)
          (PP (IN at)
            (SBAR
              (WHADVP (WRB how))
              (S
                (NP (DT a) (JJ particular) (NN system))
                (VP (VBZ works))))))))
    (. .)))

root(ROOT-0, Imagine-1)
det(scientists-3, a-2)
dobj(Imagine-1, scientists-3)
partmod(scientists-3, looking-4)
advmod(works-10, how-6)
det(system-9, a-7)
amod(system-9, particular-8)
nsubj(works-10, system-9)
