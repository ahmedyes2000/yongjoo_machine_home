(ROOT
  (S
    (NP
      (NP (DT The) (NNS chances))
      (PP (IN of)
        (NP (DT that) (NN occuring))))
    (ADVP (RB randomly))
    (VP (VBP are)
      (ADJP (RB absurdly) (JJ small)))
    (. .)))

det(chances-2, The-1)
nsubj(small-9, chances-2)
det(occuring-5, that-4)
prep_of(chances-2, occuring-5)
advmod(small-9, randomly-6)
cop(small-9, are-7)
advmod(small-9, absurdly-8)
root(ROOT-0, small-9)

