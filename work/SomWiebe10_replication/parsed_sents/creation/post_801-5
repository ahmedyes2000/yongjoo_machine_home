(ROOT
  (S ('' '')
    (PP (IN For)
      (NP (NN something)
        (S
          (VP (TO to)
            (VP (VB go)
              (PP (IN beyond)
                (NP
                  (NP (DT a) (NN theory))
                  (, ,)
                  (NP (PRP it)))))))))
    (VP (VBZ has)
      (S
        (VP (TO to)
          (VP
            (VP (VB be)
              (VP (VBN proven)
                (PP (IN beyond))))
            (CC and)
            (VP (VB shadow)
              (PP (IN of)
                (NP (DT a) (NN doubt))))))))
    (. .)))

prep_for(has-11, something-3)
aux(go-5, to-4)
infmod(something-3, go-5)
det(theory-8, a-7)
prep_beyond(go-5, theory-8)
appos(theory-8, it-10)
root(ROOT-0, has-11)
aux(proven-14, to-12)
auxpass(proven-14, be-13)
xcomp(has-11, proven-14)
prep(proven-14, beyond-15)
xcomp(has-11, shadow-17)
conj_and(proven-14, shadow-17)
det(doubt-20, a-19)
prep_of(shadow-17, doubt-20)

