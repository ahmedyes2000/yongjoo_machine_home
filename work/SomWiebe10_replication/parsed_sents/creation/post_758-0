(ROOT
  (S
    (NP (EX There))
    (VP (VBZ is)
      (NP (DT no) (NN proof))
      (SBAR
        (SBAR (IN that)
          (S
            (NP (EX there))
            (ADVP (RB even))
            (VP (VBZ exists)
              (S
                (NP (DT a) (NNP God))
                (VP (TO to)
                  (VP (VB begin)
                    (PP (IN with))))))))
        (, ,)
        (CC so)
        (SBAR
          (WHPP (IN in)
            (WHNP (WDT that) (NN case)))
          (S
            (NP (PRP you))
            (VP (MD ca) (RB n't)
              (ADVP (RB really))
              (VP (VB use)
                (ADVP (DT that)
                  (CC either))))))))
    (. .)))

