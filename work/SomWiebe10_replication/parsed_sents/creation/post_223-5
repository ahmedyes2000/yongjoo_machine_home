(ROOT
  (S ('' '')
    (NP (PRP I))
    (VP (VBP think)
      (S
        (NP (JJ genetic) (NN information))
        (ADJP (JJR more)
          (CC or)
          (JJR less)
          (SBAR
            (S
              (VP (VBD came)
                (PRT (RP out))
                (PP (IN of)
                  (NP (RB nowhere)))
                (PP (IN by)
                  (NP
                    (NP (NN chance) (NNS assemblages))
                    (PP (IN of)
                      (NP (JJ short) (NNS polymers)))))))))))
    (. .)))

nsubj(think-3, I-2)
root(ROOT-0, think-3)
amod(information-5, genetic-4)
nsubj(more-6, information-5)
nsubj(less-8, information-5)
xcomp(think-3, more-6)
xcomp(think-3, less-8)
conj_or(more-6, less-8)
dep(more-6, came-9)
prep_out_of(came-9, nowhere-12)
nn(assemblages-15, chance-14)
prep_by(came-9, assemblages-15)
amod(polymers-18, short-17)
prep_of(assemblages-15, polymers-18)

