(ROOT
  (S
    (S
      (NP (DT This))
      (VP (VBZ is)
        (NP
          (NP (NN science))
          (SBAR
            (S
              (NP (PRP we))
              (VP (VBP 're)
                (VP (VBG talking) (IN about))))))))
    (, ,) (RB not)
    (S
      (NP (JJ political) (NNS controversies))
      (VP (VBG concerning)
        (NP (JJ presidential) (NNS candidates))))
    (. .)))

nsubj(science-3, This-1)
cop(science-3, is-2)
root(ROOT-0, science-3)
dobj(talking-6, science-3)
nsubj(talking-6, we-4)
aux(talking-6, 're-5)
rcmod(science-3, talking-6)
dep(talking-6, about-7)
neg(science-3, not-9)
amod(controversies-11, political-10)
nsubj(concerning-12, controversies-11)
dep(science-3, concerning-12)
amod(candidates-14, presidential-13)
dobj(concerning-12, candidates-14)

