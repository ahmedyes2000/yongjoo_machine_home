aux(those-13, would-11)
cop(those-13, be-12)
root(ROOT-0, those-13)
nsubj(mutations-16, those-13)
aux(mutations-16, had-15)
rcmod(those-13, mutations-16)
xcomp(mutations-16, protecting-17)
dobj(protecting-17, them-18)
amod(forms-22, other-20)
nn(forms-22, life-21)
prep_from(protecting-17, forms-22)

