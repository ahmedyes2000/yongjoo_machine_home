(ROOT
  (S
    (PP (IN As)
      (NP (PDT such) (DT the) (JJ only) (NNS multi)))
    (NP
      (NP (JJ cellular) (NN life))
      (SBAR
        (WHNP (WDT that))
        (S
          (VP (MD would)
            (VP (VB endure))))))
    (VP (MD would)
      (VP (VB be)
        (NP
          (NP (DT those))
          (SBAR
            (WHNP (WDT that))
            (S
              (VP (VBD had)
                (VP (VBN mutations)
                  (S
                    (VP (VBG protecting)
                      (NP (PRP them))
                      (PP (IN from)
                        (NP (JJ other) (NN life) (NNS forms))))))))))))
    (. .)))

predet(multi-5, such-2)
det(multi-5, the-3)
amod(multi-5, only-4)
prep_as(those-13, multi-5)
amod(life-7, cellular-6)
nsubj(endure-10, life-7)
