(ROOT
  (S
    (NP (PRP I))
    (VP (VBP think)
      (SBAR
        (S
          (NP
            (NP (DT the) (NN case))
            (PP (IN against)
              (NP (NN evolution))))
          (VP (VBZ is)
            (ADJP (JJ wrong)
              (CC and)
              (JJ misinformed))))))
    (. .)))

nsubj(think-2, I-1)
root(ROOT-0, think-2)
det(case-4, the-3)
nsubj(wrong-8, case-4)
nsubj(misinformed-10, case-4)
prep_against(case-4, evolution-6)
cop(wrong-8, is-7)
ccomp(think-2, wrong-8)
ccomp(think-2, misinformed-10)
conj_and(wrong-8, misinformed-10)

