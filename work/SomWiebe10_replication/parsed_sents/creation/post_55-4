  (S
    (NP
      (NP (DT The) (NNP IC) (NN claim))
      (PP (IN of)
        (NP (DT the) (NN flagellum))))
    (VP (VBZ is)
      (NP
        (NP (DT a) (JJ good) (NN example))
        (PP (IN of)
          (NP (JJ flawed) (NN reasoning)))))
    (. .)))

