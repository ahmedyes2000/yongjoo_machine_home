(ROOT
  (S
    (NP (NNP God))
    (VP (MD could)
      (VP (VB have)
        (VP
          (VP (VBN created)
            (NP (DT the) (NN universe)))
          (CC and)
          (VP (VBN used)
            (NP (NN evolution))
            (PP (IN as)
              (NP
                (NP (NNS means))
                (PP (IN of)
                  (NP (NN creation)))))))))
    (. .)))

nsubj(created-4, God-1)
nsubj(used-8, God-1)
aux(created-4, could-2)
aux(created-4, have-3)
root(ROOT-0, created-4)
det(universe-6, the-5)
dobj(created-4, universe-6)
conj_and(created-4, used-8)
dobj(used-8, evolution-9)
prep_as(used-8, means-11)
prep_of(means-11, creation-13)

