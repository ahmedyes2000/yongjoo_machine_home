(ROOT
  (S ('' '')
    (ADVP (RB Never) (RB mind))
    (, ,)
    (NP (PRP I))
    (VP (MD 'll)
      (ADVP (RB just))
      (VP (VB let)
        (S
          (NP
            (NP (DT the) (JJ explicit) (NN irony))
            (PP (IN of)
              (NP (DT that) (NN statement))))
          (VP (VB speak)
            (PP (IN for)
              (NP (PRP itself)))))))
    (. .)))

dep(mind-3, Never-2)
advmod(let-8, mind-3)
nsubj(let-8, I-5)
aux(let-8, 'll-6)
advmod(let-8, just-7)
root(ROOT-0, let-8)
det(irony-11, the-9)
amod(irony-11, explicit-10)
nsubj(speak-15, irony-11)
det(statement-14, that-13)
prep_of(irony-11, statement-14)
ccomp(let-8, speak-15)
prep_for(speak-15, itself-17)

