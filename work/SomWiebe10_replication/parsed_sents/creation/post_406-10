(ROOT
  (S
    (NP (NNP ID))
    (VP (VBZ postulates)
      (PP (IN on)
        (NP
          (NP (NN evidence))
          (, ,) (RB not)
          (NP (DT a) (JJ religious) (NN position)))))
    (. .)))

nsubj(postulates-2, ID-1)
root(ROOT-0, postulates-2)
prep_on(postulates-2, evidence-4)
neg(evidence-4, not-6)
det(position-9, a-7)
amod(position-9, religious-8)
dep(evidence-4, position-9)

