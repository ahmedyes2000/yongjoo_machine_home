(ROOT
  (S
    (VP (VB Read)
      (NP (DT the) (JJ whole) (NN comment))
      (SBAR (IN that)
        (S
          (NP (PRP he))
          (VP (VBZ makes)
            (ADVP (RB first))))))))

root(ROOT-0, Read-1)
det(comment-4, the-2)
amod(comment-4, whole-3)
dobj(Read-1, comment-4)
complm(makes-7, that-5)
nsubj(makes-7, he-6)
ccomp(Read-1, makes-7)
advmod(makes-7, first-8)

