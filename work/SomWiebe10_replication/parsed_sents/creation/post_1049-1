(ROOT
  (SQ (VBD Were)
    (NP (PRP you))
    (NP (EX there))
    (SBAR
      (WHADVP (WRB when))
      (S
        (NP (NN evolution))
        (VP (VBD was)
          (VP (VBG happening)))))
    (. ?)))

root(ROOT-0, Were-1)
nsubj(Were-1, you-2)
attr(Were-1, there-3)
expl(Were-1, there-3)
advmod(happening-7, when-4)
nsubj(happening-7, evolution-5)
aux(happening-7, was-6)
advcl(Were-1, happening-7)

