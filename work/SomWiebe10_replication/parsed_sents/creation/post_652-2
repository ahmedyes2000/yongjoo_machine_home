(ROOT
  (S
    (VP (VB Let)
      (S
        (NP (PRP me))
        (VP (VB present)
          (S
            (NP (PRP you))
            (NP (PRP$ my) (NNS position1))))))
    (. .)))

root(ROOT-0, Let-1)
nsubj(present-3, me-2)
ccomp(Let-1, present-3)
nsubj(position1-6, you-4)
poss(position1-6, my-5)
xcomp(present-3, position1-6)

