(ROOT
  (S
    (PP (IN like)
      (NP
        (NP (DT a) (NN child))
        (VP (VBG pretending)
          (S
            (VP (TO to)
              (VP (RB not) (VB understand)))))))
    (NP (DT a) (NN parent))
    (VP (VBZ 's)
      (VP (VBG meaning)
        (ADVP (RB simply))
        (SBAR (IN so)
          (S
            (NP (PRP they))
            (VP (MD can)
              (VP (VB ignore)
                (NP (DT the) (NNS implications)))))
          (. .))))
    (. .)))

det(child-3, a-2)
xsubj(understand-7, child-3)
prep_like(meaning-11, child-3)
partmod(child-3, pretending-4)
aux(understand-7, to-5)
neg(understand-7, not-6)
xcomp(pretending-4, understand-7)
det(parent-9, a-8)
nsubj(meaning-11, parent-9)
aux(meaning-11, 's-10)
root(ROOT-0, meaning-11)
advmod(meaning-11, simply-12)
mark(ignore-16, so-13)
nsubj(ignore-16, they-14)
aux(ignore-16, can-15)
advcl(meaning-11, ignore-16)
det(implications-18, the-17)
dobj(ignore-16, implications-18)

