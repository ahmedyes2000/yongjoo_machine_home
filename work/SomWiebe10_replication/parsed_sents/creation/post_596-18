(ROOT
  (S (IN So)
    (ADVP (RB even)
      (PP (IN in)
        (NP (DT this) (NN aspect))))
    (NP (PRP I))
    (VP (VBP have)
      (VP (VBN found)
        (S
          (NP (NNP ID))
          (VP (TO to)
            (VP (VB be)
              (VP (VBG lacking)
                (NP (JJ scientific) (NN relevance))))))))
    (. .)))

dep(found-8, So-1)
advmod(found-8, even-2)
det(aspect-5, this-4)
prep_in(even-2, aspect-5)
nsubj(found-8, I-6)
aux(found-8, have-7)
root(ROOT-0, found-8)
nsubj(lacking-12, ID-9)
aux(lacking-12, to-10)
aux(lacking-12, be-11)
xcomp(found-8, lacking-12)
amod(relevance-14, scientific-13)
dobj(lacking-12, relevance-14)

