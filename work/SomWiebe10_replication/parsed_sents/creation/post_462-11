(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is)
      (ADJP (RB downright) (JJ insulting))
      (NP
        (NP (DT the) (NN way))
        (SBAR
          (S
            (NP (DT the) (NN intelligentsia))
            (VP (VBZ views)
              (NP
                (NP (DT the) (NN public))
                (PP (IN at)
                  (NP (NNS times)))))))))
    (. .)))

nsubj(insulting-4, It-1)
cop(insulting-4, is-2)
advmod(insulting-4, downright-3)
root(ROOT-0, insulting-4)
det(way-6, the-5)
attr(insulting-4, way-6)
det(intelligentsia-8, the-7)
nsubj(views-9, intelligentsia-8)
rcmod(way-6, views-9)
det(public-11, the-10)
dobj(views-9, public-11)
prep_at(public-11, times-13)

