(ROOT
  (SBARQ (RB So)
    (WHADVP (WRB how))
    (SQ (MD can)
      (NP (PRP you))
      (VP (VB believe)
        (SBAR
          (WHNP (WP what))
          (S
            (NP (DT any) (NN man))
            (VP (VBZ says))))))
    (. .)))

advmod(believe-5, So-1)
advmod(believe-5, how-2)
aux(believe-5, can-3)
nsubj(believe-5, you-4)
root(ROOT-0, believe-5)
dobj(says-9, what-6)
det(man-8, any-7)
nsubj(says-9, man-8)
ccomp(believe-5, says-9)

