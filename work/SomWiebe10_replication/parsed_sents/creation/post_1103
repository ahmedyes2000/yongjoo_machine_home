(ROOT
  (S
    (NP
      (NP (DT The) (NN evolution))
      (SBAR (IN that)
        (S
          (NP (PRP I))
          (VP (VBP believe)
            (PRT (RP in))))))
    (VP (VBZ is) (RB not)
      (NP
        (NP (DT the) (NN kind))
        (SBAR
          (S
            (NP (PRP you))
            (VP (VBP think))))))
    (. .)))

