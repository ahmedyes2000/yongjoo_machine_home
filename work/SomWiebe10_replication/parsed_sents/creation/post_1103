(ROOT
  (S
    (NP (PRP I))
    (VP (VBP am)
      (NP
        (NP (DT a) (JJ Christian) (DT an))
        (SBAR
          (S
            (NP
              (NP (DT the) (NN evolution))
              (SBAR (IN that)
                (S
                  (NP (PRP I))
                  (VP (VBP believe)
                    (PRT (RP in))))))
            (VP (VBZ is)
              (NP
                (NP (DT the) (NN kind))
                (PP (IN like)
                  (NP (NNS metamorphosis)
                    (CC and)
                    (NNS things)))
                (PP (IN like)
                  (SBAR (IN that)
                    (S
                      (NP
                        (NP (RB not) (DT the) (NN kind))
                        (SBAR (IN that)
                          (S
                            (NP (PRP we))
                            (VP (VBD evolved)
                              (PP (IN from)
                                (NP (NNS apes)
                                  (CC and)
                                  (NN space)))))))
                      (VP
                        (VP (VBD exploded))
                        (CC and)
                        (VP (VBD created)
                          (NP (DT the) (NN earth)))))))))))))
    (. .)))

