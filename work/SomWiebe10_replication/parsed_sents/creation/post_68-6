  (S (CC Yet)
    (ADVP (RB instead))
    (NP (PRP we))
    (VP (VBP find)
      (SBAR (IN that)
        (S
          (NP (NNS things))
          (`` ``)
          (VP (VBD evolved))
          ('' '')))
      (PP (IN without)
        (S
          (ADVP (RB even))
          (VP (VBG knowing)
            (SBAR (IN whether)
              (CC or)
              (RB not)
              (S
                (NP (DT the) (NN transformation))
                (VP (VBZ is)
                  (ADJP (RB even) (JJ possible)))))))))
    (. .)))

cc(find-4, Yet-1)
advmod(find-4, instead-2)
nsubj(find-4, we-3)
root(ROOT-0, find-4)
complm(evolved-8, that-5)
