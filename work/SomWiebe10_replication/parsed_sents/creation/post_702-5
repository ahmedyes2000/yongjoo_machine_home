(ROOT
  (S
    (VP (VB Think)
      (PP (IN of)
        (NP
          (NP (DT the) (JJ enormous) (NNS benefits))
          (CC and)
          (NP (JJ psychological) (NNS improvements))))
      (PP
        (PP (TO to)
          (NP
            (NP
              (NP (NNS people))
              (SBAR
                (WHNP (WP$ whose) (NNS faces))
                (S
                  (VP (VBP have)
                    (VP (VBN been)
                      (VP (VBN disfigured)))))))
            (, ,)
            (NP
              (NP (NNS workers))
              (SBAR
                (WHNP (WP who))
                (S
                  (VP (VBP have)
                    (VP (VBN lost)
                      (NP
                        (NP (PRP$ their) (NNS fingers))
                        (, ,)
                        (NP (NNS feet))
                        (, ,)
                        (CC or)
                        (NP (JJ other) (NNS limbs)))
                      (PP (IN in)
                        (NP (JJ serious) (NNS accidents))))))))
            (, ,)
            (CC or)
            (NP
              (NP (NNS children))
              (SBAR
                (WHNP (WP who))
                (S
                  (VP (VBD were)
                    (VP (VBN born)
                      (S
                        (ADJP (JJ blind))))))))))
        (CC or)
        (PP (IN with)
          (NP (DT some) (JJ other) (NN birth) (NN defect)))))
    (. .)))

root(ROOT-0, Think-1)
conj_or(Think-1, Think-1')
det(benefits-5, the-3)
