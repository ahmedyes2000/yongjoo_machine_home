(ROOT
  (S
    (NP (NNP ID))
    (VP (VBZ is)
      (VP (VBN excluded)
        (PP (IN from)
          (NP (JJ scientific) (NN text) (NNS books)))
        (PP
          (ADVP (RB simply))
          (IN because) (IN of)
          (NP
            (NP (PRP$ its) (NN lack))
            (PP (IN of)
              (NP (JJ scientific) (NN standing)))))))
    (. .)))

nsubjpass(excluded-3, ID-1)
auxpass(excluded-3, is-2)
root(ROOT-0, excluded-3)
amod(books-7, scientific-5)
nn(books-7, text-6)
prep_from(excluded-3, books-7)
advmod(excluded-3, simply-8)
poss(lack-12, its-11)
prep_because_of(excluded-3, lack-12)
amod(standing-15, scientific-14)
prep_of(lack-12, standing-15)

