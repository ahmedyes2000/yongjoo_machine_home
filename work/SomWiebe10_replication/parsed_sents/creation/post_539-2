(ROOT
  (S
    (NP (EX There))
    (VP (VBD was)
      (NP
        (NP (DT no) (NN official))
        (CC or)
        (NP
          (NP (VBN written) (NN policy)
            (CC or)
            (NN understanding))
          (VP (VBN related)
            (PP (TO to)
              (NP (NN abortion)
                (CC or)
                (NN sterilization)))))))
    (. .)))

expl(was-2, There-1)
root(ROOT-0, was-2)
det(official-4, no-3)
nsubj(was-2, official-4)
amod(policy-7, written-6)
nsubj(was-2, policy-7)
conj_or(official-4, policy-7)
conj_or(official-4, understanding-9)
conj_or(policy-7, understanding-9)
partmod(policy-7, related-10)
prep_to(related-10, abortion-12)
prep_to(related-10, sterilization-14)
conj_or(abortion-12, sterilization-14)

