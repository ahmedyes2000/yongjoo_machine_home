(ROOT
  (S
    (NP (DT Both))
    (VP (VBP are)
      (NP
        (NP (NN speculation))
        (PP (IN on)
          (NP
            (NP (DT the) (NN part))
            (PP (IN of)
              (NP
                (NP (DT the) (NN person))
                (SBAR
                  (WHNP (WDT that))
                  (S
                    (VP (VBZ believes)
                      (S
                        (NP (PRP them))
                        (VP (TO to)
                          (VP (VB be)
                            (ADJP (JJ true))))))))))))))
    (. .)))

nsubj(speculation-3, Both-1)
cop(speculation-3, are-2)
root(ROOT-0, speculation-3)
det(part-6, the-5)
prep_on(speculation-3, part-6)
det(person-9, the-8)
prep_of(part-6, person-9)
nsubj(believes-11, person-9)
rcmod(person-9, believes-11)
nsubj(true-15, them-12)
aux(true-15, to-13)
cop(true-15, be-14)
xcomp(believes-11, true-15)

