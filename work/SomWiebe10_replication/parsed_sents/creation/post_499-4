(ROOT
  (S
    (S
      (NP (DT That))
      (VP (VBD said)))
    (, ,)
    (NP
      (NP (NN none))
      (PP (IN of)
        (NP (DT this))))
    (VP (VBZ relates)
      (PP (TO to)
        (NP (DT the) (NNP ID) (NN ruling)))
      (PRN (, ,)
        (SBAR (IN since)
          (S
            (NP (DT the) (NN board) (NNS members))
            (VP (VBD were) (RB not)
              (NP (NNP ID) (NNS advocates)))))
        (, ,))
      (PP (IN except)
        (NP (RB perhaps) (NN superficially))))
    (. .)))

nsubj(said-2, That-1)
ccomp(relates-7, said-2)
nsubj(relates-7, none-4)
prep_of(none-4, this-6)
root(ROOT-0, relates-7)
det(ruling-11, the-9)
nn(ruling-11, ID-10)
prep_to(relates-7, ruling-11)
mark(advocates-20, since-13)
det(members-16, the-14)
nn(members-16, board-15)
nsubj(advocates-20, members-16)
cop(advocates-20, were-17)
neg(advocates-20, not-18)
nn(advocates-20, ID-19)
parataxis(relates-7, advocates-20)
advmod(superficially-24, perhaps-23)
prep_except(relates-7, superficially-24)

