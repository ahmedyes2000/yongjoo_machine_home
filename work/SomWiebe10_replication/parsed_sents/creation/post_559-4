(ROOT
  (SQ (MD Would) (RB n't)
    (NP (DT that))
    (VP (VB be)
      (NP (DT the) (NN case)))
    (. ?)))

aux(case-6, Would-1)
neg(case-6, n't-2)
nsubj(case-6, that-3)
cop(case-6, be-4)
det(case-6, the-5)
root(ROOT-0, case-6)

