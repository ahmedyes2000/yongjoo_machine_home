(ROOT
  (S
    (NP (NNP Schools))
    (VP (MD should) (RB not)
      (VP (VB teach)
        (NP
          (NP (NNS theories))
          (SBAR
            (WHNP (WDT that))
            (S
              (VP (VBP are)
                (ADVP (RB completely))
                (PP (IN at)
                  (NP
                    (NP (NNS odds))
                    (PP (IN with)
                      (NP (DT each) (JJ other)))))))))))
    (. .)))

nsubj(teach-4, Schools-1)
aux(teach-4, should-2)
neg(teach-4, not-3)
root(ROOT-0, teach-4)
dobj(teach-4, theories-5)
nsubj(are-7, theories-5)
rcmod(theories-5, are-7)
advmod(are-7, completely-8)
prep_at(are-7, odds-10)
det(other-13, each-12)
prep_with(odds-10, other-13)

