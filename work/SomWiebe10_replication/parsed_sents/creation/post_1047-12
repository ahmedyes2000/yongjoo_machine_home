(ROOT
  (S
    (NP
      (NP (JJ Dont) (NN worry))
      (, ,)
      (NP
        (NP (NN friend))
        (, ,)
        (NP (DT the) (NNS answers))))
    (VP (VBP are)
      (VP (VBG coming)
        (ADVP (RBR faster))
        (NP (DT every) (NN day))))
    (. .)))

amod(worry-2, Dont-1)
nsubj(coming-9, worry-2)
appos(worry-2, friend-4)
det(answers-7, the-6)
appos(friend-4, answers-7)
aux(coming-9, are-8)
root(ROOT-0, coming-9)
advmod(coming-9, faster-10)
det(day-12, every-11)
tmod(coming-9, day-12)

