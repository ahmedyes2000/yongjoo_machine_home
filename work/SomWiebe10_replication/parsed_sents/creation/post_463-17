  (S
    (NP (PRP$ His))
    (VP (VBD was)
      (NP
        (NP (DT a) (NN courtesy) (NN appointment))
        (PP (IN as)
          (NP
            (NP (DT a) (NN researcher))
            (, ,)
            (SBAR
              (WHNP (WDT which))
              (S
                (VP (VBD was) (RB not)
                  (VP (VBD rescinded)))))))))
    (. .)))

nsubj(appointment-5, His-1)
cop(appointment-5, was-2)
det(appointment-5, a-3)
