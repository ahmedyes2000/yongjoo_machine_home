(ROOT
  (S
    (NP (DT Every) (NN atheist)
      (CC and)
      (NN evolutionist))
    (VP (VBZ has)
      (NP
        (NP (DT a) (NN problem))
        (PP (IN with)
          (NP (DT those) (CD 3) (NNS words)))))
    (. .)))

det(atheist-2, Every-1)
nsubj(has-5, atheist-2)
conj_and(atheist-2, evolutionist-4)
nsubj(has-5, evolutionist-4)
root(ROOT-0, has-5)
det(problem-7, a-6)
dobj(has-5, problem-7)
det(words-11, those-9)
