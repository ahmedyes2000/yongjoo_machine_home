  (S
    (VP (VBZ Is) (RB n't)
      (NP
        (NP
          (NP (NN evolution) (POS 's))
          (JJ bible)
          (NX (NNP Darwin) (POS 's)))
        ('' '')
        (PP (NNP On)
          (NP (DT the) (NNP Origin)))
        (PP (IN of)
          (NP (NNP Species)))))
    (. ...)))

cop('s-7, Is-1)
neg('s-7, n't-2)
poss('s-7, evolution-3)
