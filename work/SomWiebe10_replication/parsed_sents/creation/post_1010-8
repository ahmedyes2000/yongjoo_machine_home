  (S
    (VP (VB Tell)
      (NP
        (NP (PRP me))
        (SBAR
          (WHNP (WP what))
          (S
            (S
              (NP (NNS 0\/infinity))
              (VP (MD could)
                (VP (VB mean))))
            (, ,)
            (CC and)
            (S
              (NP (PRP$ my) (NN answer))
              (VP (VBZ is)))))
        (: :)
        (NP (DT Both))))
    (. .)))

root(ROOT-0, Tell-1)
dobj(Tell-1, me-2)
dep(mean-6, me-2)
