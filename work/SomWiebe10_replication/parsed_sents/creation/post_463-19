(ROOT
  (S
    (PP (IN From)
      (NP (RB here)))
    (NP (DT the) (NNS claims))
    (VP (VBP seem)
      (S
        (VP (TO to)
          (VP (VB get)
            (ADJP
              (ADJP (RB considerably) (RBR more) (JJ modest))
              (CC and)
              (ADVP (RB also))
              (ADJP
                (NP (DT a) (RB bit))
                (RBR more) (JJ difficult)))
            (S
              (VP (TO to)
                (VP (VB adjudicate))))))))
    (. .)))

prep(seem-5, From-1)
pobj(From-1, here-2)
det(claims-4, the-3)
nsubj(seem-5, claims-4)
xsubj(modest-10, claims-4)
root(ROOT-0, seem-5)
aux(modest-10, to-6)
dep(modest-10, get-7)
advmod(modest-10, considerably-8)
advmod(modest-10, more-9)
xcomp(seem-5, modest-10)
xcomp(seem-5, also-12)
advmod(modest-10, also-12)
conj_and(modest-10, also-12)
det(bit-14, a-13)
dep(difficult-16, bit-14)
advmod(difficult-16, more-15)
xcomp(seem-5, difficult-16)
conj_and(modest-10, difficult-16)
aux(adjudicate-18, to-17)
xcomp(modest-10, adjudicate-18)

