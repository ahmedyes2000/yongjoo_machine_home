(ROOT
  (S ('' '')
    (NP (DT The) (JJ whole) (NN point))
    (VP (VBD was)
      (VP (VBN tor)
        (S
          (VP
            (VP (VB read)
              (NP (PRP$ my) (NN comment))
              (PP (IN on)
                (NP (DT that) (NN site))))
            (CC and)
            (VP (VB give)
              (NP (NNS criticisms))
              (ADVP (RB here)))))))
    (. .)))

det(point-4, The-2)
amod(point-4, whole-3)
nsubjpass(tor-6, point-4)
auxpass(tor-6, was-5)
root(ROOT-0, tor-6)
ccomp(tor-6, read-7)
poss(comment-9, my-8)
dobj(read-7, comment-9)
det(site-12, that-11)
prep_on(read-7, site-12)
ccomp(tor-6, give-14)
conj_and(read-7, give-14)
dobj(give-14, criticisms-15)
advmod(give-14, here-16)

