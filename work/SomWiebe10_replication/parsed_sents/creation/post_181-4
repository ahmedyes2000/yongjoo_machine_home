(ROOT
  (S ('' '')
    (NP (EX There))
    (VP (VBZ is)
      (NP (DT no) (NN evidence))
      (SBAR (IN that)
        (S
          (NP (PDT such) (DT a) (NN being))
          (VP (VBD existed)))))
    (. .)))

expl(is-3, There-2)
root(ROOT-0, is-3)
det(evidence-5, no-4)
nsubj(is-3, evidence-5)
complm(existed-10, that-6)
predet(being-9, such-7)
det(being-9, a-8)
nsubj(existed-10, being-9)
ccomp(is-3, existed-10)

