(ROOT
  (S
    (PP (IN By)
      (S
        (ADVP (RB just))
        (VP (VBG using)
          (NP (PRP$ your) (JJ common) (NN sense)))))
    (NP (PRP you))
    (VP (MD can)
      (VP (VB see)
        (SBAR (IN that)
          (S
            (NP (NN evolution))
            (VP (VBZ is)
              (NP (DT a) (JJ fake) (NN theory)))))))
    (. .)))

advmod(using-3, just-2)
prepc_by(see-9, using-3)
poss(sense-6, your-4)
amod(sense-6, common-5)
dobj(using-3, sense-6)
nsubj(see-9, you-7)
aux(see-9, can-8)
root(ROOT-0, see-9)
complm(theory-15, that-10)
nsubj(theory-15, evolution-11)
cop(theory-15, is-12)
det(theory-15, a-13)
amod(theory-15, fake-14)
ccomp(see-9, theory-15)

