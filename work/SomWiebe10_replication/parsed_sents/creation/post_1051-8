(ROOT
  (S
    (ADVP (RB First))
    (, ,)
    (NP
      (NP (PRP$ your) (NN understanding))
      (PP (IN of)
        (NP
          (NP (DT the) (NN field))
          (, ,)
          (SBAR
            (WHNP (WDT which))
            (S
              (NP (PRP you))
              (VP (VBP are)
                (VP (VBG discussing))))))))
    (VP (VBZ is)
      (ADVP (RB woefully))
      (VP (VBG lacking)))
    (. .)))

advmod(lacking-15, First-1)
poss(understanding-4, your-3)
nsubj(lacking-15, understanding-4)
det(field-7, the-6)
prep_of(understanding-4, field-7)
dobj(discussing-12, field-7)
rel(discussing-12, which-9)
nsubj(discussing-12, you-10)
aux(discussing-12, are-11)
rcmod(field-7, discussing-12)
aux(lacking-15, is-13)
advmod(lacking-15, woefully-14)
root(ROOT-0, lacking-15)

