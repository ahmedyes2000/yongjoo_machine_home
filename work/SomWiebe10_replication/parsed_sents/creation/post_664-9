(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ 's)
      (ADJP (JJS best))
      (S (RB not)
        (VP (TO to)
          (VP (VB inform)
            (NP
              (NP (NNP ID) (NNS supporters))
              (PP (IN of)
                (NP
                  (NP (DT these)
                    (ADJP (JJ inconvenient))
                    (NNS facts))
                  (PP (IN of)
                    (NP (NN science))))))))))
    (. .)))

nsubj(best-3, It-1)
xsubj(inform-6, It-1)
cop(best-3, 's-2)
root(ROOT-0, best-3)
neg(inform-6, not-4)
aux(inform-6, to-5)
xcomp(best-3, inform-6)
nn(supporters-8, ID-7)
dobj(inform-6, supporters-8)
det(facts-12, these-10)
amod(facts-12, inconvenient-11)
prep_of(supporters-8, facts-12)
