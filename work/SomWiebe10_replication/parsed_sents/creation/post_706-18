(ROOT
  (S
    (NP (DT No) (NN one))
    (VP (VBZ has)
      (NP
        (NP (DT a) (JJ full) (NN understanding))
        (PP (IN of)
          (NP (NN chemistry)))))
    (. .) (. .)))

det(one-2, No-1)
nsubj(has-3, one-2)
root(ROOT-0, has-3)
det(understanding-6, a-4)
amod(understanding-6, full-5)
dobj(has-3, understanding-6)
prep_of(understanding-6, chemistry-8)

