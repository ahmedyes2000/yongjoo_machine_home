(ROOT
  (S (: --)
    (NP
      (NP (JJ TJ10The) (NN government) (NN report))
      (PP (IN on)
        (NP (DT the) (NN issue))))
    (VP (VBZ validates)
      (NP
        (NP (DT the) (NNS charges))
        (PP (IN against)
          (NP (DT the) (NNP Smithsonian) (NNP Institute)))))
    (. .)))

amod(report-4, TJ10The-2)
nn(report-4, government-3)
nsubj(validates-8, report-4)
det(issue-7, the-6)
prep_on(report-4, issue-7)
root(ROOT-0, validates-8)
det(charges-10, the-9)
dobj(validates-8, charges-10)
det(Institute-14, the-12)
nn(Institute-14, Smithsonian-13)
prep_against(charges-10, Institute-14)

