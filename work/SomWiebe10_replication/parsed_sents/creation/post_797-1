(ROOT
  (S
    (ADVP (RB Logically))
    (NP (DT the) (NN evidence))
    (VP (VBZ points)
      (PP (TO to)
        (NP
          (NP (DT the)
            (ADJP
              (QP (CD 4.5) (CD billion)))
            (NN year) (NN creation))
          (SBAR
            (WHNP (WDT that))
            (S
              (VP (VBZ is)
                (NP (DT the) (JJ common) (NN consensus))))))))
    (. .)))

advmod(points-4, Logically-1)
det(evidence-3, the-2)
nsubj(points-4, evidence-3)
root(ROOT-0, points-4)
det(creation-10, the-6)
number(billion-8, 4.5-7)
num(creation-10, billion-8)
nn(creation-10, year-9)
prep_to(points-4, creation-10)
nsubj(consensus-15, creation-10)
cop(consensus-15, is-12)
