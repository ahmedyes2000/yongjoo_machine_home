(ROOT
  (S ('' '')
    (PP (IN If)
      (NP (DT this) (NN happens)))
    (, ,)
    (NP (PRP we))
    (VP (VBP are)
      (PP (IN in)
        (NP (DT the) (JJ same) (NN boat))))
    (. .)))

det(happens-4, this-3)
prep_if(are-7, happens-4)
nsubj(are-7, we-6)
root(ROOT-0, are-7)
det(boat-11, the-9)
amod(boat-11, same-10)
prep_in(are-7, boat-11)

