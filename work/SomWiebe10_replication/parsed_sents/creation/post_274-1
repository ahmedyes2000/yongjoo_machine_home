(ROOT
  (S
    (NP (EX There))
    (VP (VBZ is)
      (NP
        (NP (DT no) (NN theory))
        (PP (IN of)
          (NP
            (NP (NNP ID))
            (, ,)
            (SBAR
              (WHNP (WP what))
              (S
                (VP (VBZ 's)
                  (ADJP (RB so) (JJ hard))
                  (S
                    (VP (TO to)
                      (VP (VB understand)
                        (ADVP (RB here))))))))))))
    (. .)))

expl(is-2, There-1)
root(ROOT-0, is-2)
det(theory-4, no-3)
nsubj(is-2, theory-4)
prep_of(theory-4, ID-6)
nsubj(hard-11, ID-6)
