(ROOT
  (S
    (PP (IN After)
      (NP (DT all)))
    (, ,)
    (NP (NN design))
    (VP (VBZ is) (RB not)
      (NP
        (NP (NN something))
        (SBAR
          (WHNP (WDT that))
          (S
            (VP (VBZ seems)
              (ADJP (RB directly) (JJ relevant)
                (PP (TO to)
                  (NP (NN science)))))))))
    (. .)))

prep_after(something-7, all-2)
nsubj(something-7, design-4)
cop(something-7, is-5)
neg(something-7, not-6)
root(ROOT-0, something-7)
nsubj(relevant-11, something-7)
cop(relevant-11, seems-9)
advmod(relevant-11, directly-10)
rcmod(something-7, relevant-11)
prep_to(relevant-11, science-13)

