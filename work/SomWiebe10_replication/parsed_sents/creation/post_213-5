(ROOT
  (S
    (NP (PRP$ Our) (NNS helicopters))
    (VP (VBP are)
      (ADJP (RB far) (RBR more) (JJ useful))
      (ADVP (RB then)
        (NP (DT the) (NN dragonfly))))
    (. .)))

poss(helicopters-2, Our-1)
nsubj(useful-6, helicopters-2)
cop(useful-6, are-3)
advmod(useful-6, far-4)
advmod(useful-6, more-5)
root(ROOT-0, useful-6)
advmod(useful-6, then-7)
det(dragonfly-9, the-8)
pobj(then-7, dragonfly-9)

