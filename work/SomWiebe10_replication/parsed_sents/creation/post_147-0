(ROOT
  (S
    (S
      (NP (PRP$ My) (JJ other) (NN comment))
      (VP (VBD focused)
        (NP (JJR more))
        (PP (IN on)
          (NP (PRP$ my) (JJ atheist) (NN background)))))
    (, ,)
    (CC but)
    (S
      (NP (PRP I))
      (VP (VBD wanted)
        (S
          (VP (TO to)
            (VP (VB respond)
              (PP (TO to)
                (NP (PRP$ your) (JJ scientific) (NNS misconceptions))))))))
    (. .)))

poss(comment-3, My-1)
amod(comment-3, other-2)
nsubj(focused-4, comment-3)
root(ROOT-0, focused-4)
dobj(focused-4, more-5)
poss(background-9, my-7)
amod(background-9, atheist-8)
prep_on(focused-4, background-9)
nsubj(wanted-13, I-12)
xsubj(respond-15, I-12)
conj_but(focused-4, wanted-13)
aux(respond-15, to-14)
xcomp(wanted-13, respond-15)
poss(misconceptions-19, your-17)
amod(misconceptions-19, scientific-18)
prep_to(respond-15, misconceptions-19)

