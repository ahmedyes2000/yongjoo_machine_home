(ROOT
  (S
    (NP
      (NP (DT The) (NNS stories) (, ,) (NNS myths)
        (CC and)
        (NNS legends))
      (PP (IN of)
        (NP (PRP$ our) (NNS origins))))
    (VP (VBP are) (RB n't)
      (VP (IN about)
        (S
          (VP (VBG explaining)
            (SBAR
              (WHADVP (WRB how))
              (S
                (NP (NN creation))
                (VP (VBD was)
                  (VP (VBN accomplished)))))))))
    (. .)))

det(stories-2, The-1)
nsubj(about-12, stories-2)
conj_and(stories-2, myths-4)
nsubj(about-12, myths-4)
conj_and(stories-2, legends-6)
nsubj(about-12, legends-6)
poss(origins-9, our-8)
prep_of(stories-2, origins-9)
aux(about-12, are-10)
neg(about-12, n't-11)
root(ROOT-0, about-12)
xcomp(about-12, explaining-13)
advmod(accomplished-17, how-14)
nsubjpass(accomplished-17, creation-15)
auxpass(accomplished-17, was-16)
ccomp(explaining-13, accomplished-17)

