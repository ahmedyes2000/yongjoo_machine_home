(ROOT
  (S
    (NP (PRP It))
    (VP (MD will)
      (VP (VB have)
        (S
          (VP (TO to)
            (VP (VB be)
              (VP (VBN abandoned)))))))
    (. .)))

nsubj(have-3, It-1)
xsubj(abandoned-6, It-1)
aux(have-3, will-2)
root(ROOT-0, have-3)
aux(abandoned-6, to-4)
auxpass(abandoned-6, be-5)
xcomp(have-3, abandoned-6)

