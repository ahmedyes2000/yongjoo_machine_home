(ROOT
  (S (CC But)
    (S
      (ADJP (RBR more) (JJ important)
        (SBAR
          (S
            (VP (VBP are)
              (NP
                (NP (DT the) (JJ recent) (NNS discoveries))
                (PP (IN in)
                  (NP (NNP Akkadian) (, ,) (NNP Ugaritic) (, ,)
                    (CC and)
                    (NNP Phoenician)))))))))
    (VP (VBZ literatures))
    (. .)))

cc(literatures-15, But-1)
advmod(important-3, more-2)
csubj(literatures-15, important-3)
cop(discoveries-7, are-4)
det(discoveries-7, the-5)
amod(discoveries-7, recent-6)
dep(important-3, discoveries-7)
prep_in(discoveries-7, Akkadian-9)
prep_in(discoveries-7, Ugaritic-11)
conj_and(Akkadian-9, Ugaritic-11)
prep_in(discoveries-7, Phoenician-14)
conj_and(Akkadian-9, Phoenician-14)
root(ROOT-0, literatures-15)

