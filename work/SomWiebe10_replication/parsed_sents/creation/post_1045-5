(ROOT
  (S
    (PP (IN In)
      (NP (NN addition)))
    (NP (PRP you))
    (VP (VBP claim)
      (SBAR (IN that)
        (S
          (NP (PRP you))
          (VP (MD would)
            (ADVP (RB only)
              (PP (IN except)
                (SBAR (IN that)
                  (S
                    (NP (NNS scientists))
                    (VP (VBP have)
                      (VP (VBN observed)
                        (NP (NN evolution))
                        (SBAR (IN if)
                          (S
                            (NP (PRP they))
                            (VP (VBD saw)
                              (NP (NN something))
                              (PP (IN without)
                                (NP (NNS wings))))))))))))
            (VP (VB evolve)
              (PP (IN into)
                (NP (NN something)))
              (PP (IN with)
                (NP (NNS wings))))))))
    (. .)))

prep_in(claim-4, addition-2)
nsubj(claim-4, you-3)
root(ROOT-0, claim-4)
complm(evolve-21, that-5)
