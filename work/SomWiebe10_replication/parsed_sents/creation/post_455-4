(ROOT
  (S
    (ADVP (RB Nevertheless))
    (, ,)
    (NP
      (NP (DT every) (NN atheist))
      (SBAR
        (WHNP (WP who))
        (S
          (ADVP (RB passionately))
          (VP (VBZ opposes)
            (NP (NN theism) (, ,) (NN religion) (, ,)
              (CC and)
              (NN ID))))))
    (VP (VBZ has)
      (VP (VBN made)
        (NP (DT these) (NNS arguments))))
    (. .)))

advmod(made-15, Nevertheless-1)
det(atheist-4, every-3)
nsubj(opposes-7, atheist-4)
nsubj(made-15, atheist-4)
advmod(opposes-7, passionately-6)
rcmod(atheist-4, opposes-7)
dobj(opposes-7, theism-8)
dobj(opposes-7, religion-10)
conj_and(theism-8, religion-10)
dobj(opposes-7, ID-13)
conj_and(theism-8, ID-13)
aux(made-15, has-14)
root(ROOT-0, made-15)
det(arguments-17, these-16)
