(ROOT
  (S
    (NP
      (NP (CD 30) (NNS years))
      (PP (IN from)
        (NP (NN nowit))))
    (VP (MD will)
      (VP (VB be)
        (ADJP (JJ embarrassing)
          (PP (IN for)
            (NP (NN anyone))))
        (S
          (VP (TO to)
            (VP (VB admit)
              (PP (TO to)
                (S
                  (VP (VBG being)
                    (NP (DT an) (NN evolutionist))))))))))
    (. .)))

num(years-2, 30-1)
nsubj(embarrassing-7, years-2)
prep_from(years-2, nowit-4)
aux(embarrassing-7, will-5)
cop(embarrassing-7, be-6)
root(ROOT-0, embarrassing-7)
prep_for(embarrassing-7, anyone-9)
aux(admit-11, to-10)
xcomp(embarrassing-7, admit-11)
cop(evolutionist-15, being-13)
det(evolutionist-15, an-14)
prepc_to(admit-11, evolutionist-15)

