(ROOT
  (S
    (NP (DT The) (JJ whole) (NN thing))
    (VP (VBZ starts)
      (S
        (VP (TO to)
          (VP (VB smell)
            (ADJP (RB real) (JJ funny))
            (SBAR
              (WHADVP (WRB when))
              (S
                (NP (PRP you))
                (VP (VBP look)
                  (PP (IN at)
                    (NP (PRP it)))
                  (PP (IN from)
                    (NP (DT the) (NN side))))))))))
    (. .)))

det(thing-3, The-1)
amod(thing-3, whole-2)
nsubj(starts-4, thing-3)
xsubj(smell-6, thing-3)
root(ROOT-0, starts-4)
aux(smell-6, to-5)
xcomp(starts-4, smell-6)
advmod(funny-8, real-7)
acomp(smell-6, funny-8)
advmod(look-11, when-9)
nsubj(look-11, you-10)
advcl(smell-6, look-11)
prep_at(look-11, it-13)
det(side-16, the-15)
prep_from(look-11, side-16)

