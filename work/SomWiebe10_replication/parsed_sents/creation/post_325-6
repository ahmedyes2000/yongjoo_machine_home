          (PP (TO to)
            (NP (DT the) (JJ natural))))))
    (, ,) (RB then)
    (S
      (NP (EX there))
      (VP (MD could)
        (ADVP (RB never))
        (VP (VB be)
          (NP
            (NP (DT any) (JJ scientific) (NN evidence))
            (PP (IN against)
              (NP (DT the) (JJ supernatural) (NN designer)))))))
    (. .)))

