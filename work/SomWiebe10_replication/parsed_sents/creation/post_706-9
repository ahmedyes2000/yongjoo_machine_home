(ROOT
  (S
    (SBAR
      (WHADVP (WRB When))
      (S
        (NP (DT the) (NNP Bible))
        (VP (VBZ says)
          (SBAR
            (S
              (NP
                (NP (DT the) (NN inhabitants))
                (PP (IN of)
                  (NP (DT the) (NN earth))))
              (VP (VBP are)
                (PP (TO to)
                  (NP
                    (NP (NNP God))
                    (PP (IN like)
                      (NP (NNS grasshoppers)))))))))))
    (: ...)
    (ADVP (RB well))
    (, ,)
    (S
      (NP (DT that))
      (VP (VBZ is)
        (NP (DT a) (NN metaphor))))
    (CC and)
    (S
      (INTJ (UH yes))
      (, ,)
      (NP (DT the) (NNP Bible))
      (VP (VBZ is)
        (ADJP (JJ full)
          (PP (IN of)
            (NP (PRP them))))))
    (. .)))

advmod(says-4, When-1)
det(Bible-3, the-2)
nsubj(says-4, Bible-3)
advcl(metaphor-21, says-4)
det(inhabitants-6, the-5)
nsubj(are-10, inhabitants-6)
det(earth-9, the-8)
prep_of(inhabitants-6, earth-9)
ccomp(says-4, are-10)
prep_to(are-10, God-12)
prep_like(God-12, grasshoppers-14)
advmod(metaphor-21, well-16)
conj_and(metaphor-21, well-16)
nsubj(metaphor-21, that-18)
cop(metaphor-21, is-19)
det(metaphor-21, a-20)
root(ROOT-0, metaphor-21)
dep(full-28, yes-23)
det(Bible-26, the-25)
nsubj(full-28, Bible-26)
cop(full-28, is-27)
conj_and(metaphor-21, full-28)
prep_of(full-28, them-30)

