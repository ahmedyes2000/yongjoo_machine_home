(ROOT
  (S
    (S
      (VP (VBG Quoting)
        (NP (JJ other) (NNS researchers))))
    (VP (VBZ is) (RB not)
      (NP (NN science)))
    (. .)))

csubj(science-6, Quoting-1)
amod(researchers-3, other-2)
dobj(Quoting-1, researchers-3)
cop(science-6, is-4)
neg(science-6, not-5)
root(ROOT-0, science-6)

