(ROOT
  (S
    (PP (IN Because)
      (PP (IN of)
        (NP (DT this))))
    (NP (PRP he))
    (VP (VBZ sees)
      (NP
        (NP (DT no) (NN evidence))
        (PP (IN for)
          (NP
            (NP (JJ natural) (NN selection) (POS 's))
            (NN ability)
            (S
              (VP (TO to)
                (VP (VB improve)
                  (NP (JJ organismal) (NN function)))))))))
    (. .)))

prepc_because_of(sees-5, of-2)
pobj(sees-5, this-3)
nsubj(sees-5, he-4)
root(ROOT-0, sees-5)
det(evidence-7, no-6)
dobj(sees-5, evidence-7)
amod(selection-10, natural-9)
poss(ability-12, selection-10)
prep_for(evidence-7, ability-12)
aux(improve-14, to-13)
infmod(ability-12, improve-14)
amod(function-16, organismal-15)
dobj(improve-14, function-16)

