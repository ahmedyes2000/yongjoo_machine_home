(ROOT
  (SBARQ
    (WHADVP (WRB How))
    (SQ (VBP do)
      (NP (PRP you))
      (VP (VB explain)
        (SBAR (IN that)
          (S
            (NP (DT the) (JJ fossil) (NN record))
            (VP
              (VP (VBZ lacks)
                (NP (JJ intermediate) (NNS lifeforms)))
              (CC and)
              (VP
                (ADVP (RB only))
                (VBZ shows)
                (NP
                  (NP (JJ major) (NNS transitions))
                  (PP (IN of)
                    (NP (NNS organisms))))))))))
    (. .)))

advmod(explain-4, How-1)
aux(explain-4, do-2)
nsubj(explain-4, you-3)
root(ROOT-0, explain-4)
complm(lacks-9, that-5)
det(record-8, the-6)
amod(record-8, fossil-7)
nsubj(lacks-9, record-8)
nsubj(shows-14, record-8)
ccomp(explain-4, lacks-9)
amod(lifeforms-11, intermediate-10)
dobj(lacks-9, lifeforms-11)
advmod(shows-14, only-13)
ccomp(explain-4, shows-14)
conj_and(lacks-9, shows-14)
amod(transitions-16, major-15)
dobj(shows-14, transitions-16)
prep_of(transitions-16, organisms-18)

