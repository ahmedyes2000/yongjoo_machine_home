(ROOT
  (S
    (VP (VBG Fish)
      (PP (IN with)
        (NP
          (NP
            (NP (NNS fins)
              (CC and)
              (NNS scales))
            (, ,)
            (NP
              (NP (NNS birds))
              (PP (IN with)
                (NP
                  (NP (NNS feathers))
                  (, ,)
                  (NP (NNS beaks))
                  (, ,)
                  (CC and)
                  (NP (NNS wings))
                  (, ,) (FW etc)))))
          (: --)
          (NP
            (ADJP (RB Exactly) (VBN mirroring))
            (JJ Christian) (NNS apologetics)))))
    (: ...)))

root(ROOT-0, Fish-1)
prep_with(Fish-1, fins-3)
prep_with(Fish-1, scales-5)
conj_and(fins-3, scales-5)
appos(fins-3, birds-7)
prep_with(birds-7, feathers-9)
prep_with(birds-7, beaks-11)
conj_and(feathers-9, beaks-11)
prep_with(birds-7, wings-14)
conj_and(feathers-9, wings-14)
nn(feathers-9, wings-14)
dep(feathers-9, etc-16)
advmod(mirroring-19, Exactly-18)
amod(apologetics-21, mirroring-19)
amod(apologetics-21, Christian-20)
dep(fins-3, apologetics-21)

