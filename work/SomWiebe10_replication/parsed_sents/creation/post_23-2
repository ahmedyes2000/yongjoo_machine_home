  (FRAG
    (NP (DT Any) (NN reason)
      (SBAR
        (S
          (NP (PRP he))
          (VP (VBZ believes)
            (S
              (NP (DT this))
              (VP (TO to)
                (VP (VB be)
                  (ADVP (RB so)))))))))
    (. ?)))

det(reason-2, Any-1)
