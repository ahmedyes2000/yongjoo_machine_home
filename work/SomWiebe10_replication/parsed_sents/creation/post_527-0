(ROOT
  (S
    (LST (: --))
    (ADVP (RB As)
      (PP (IN for)
        (NP
          (NP (JJ scientific) (NN evidence))
          (PP (IN for)
            (NP (NNP ID))))))
    (VP (VB start)
      (PP (IN with)
        (NP (DT the) (JJ genetic) (NN code))))
    (. .)))

amod(evidence-5, scientific-4)
prep_as_for(start-8, evidence-5)
prep_for(evidence-5, ID-7)
root(ROOT-0, start-8)
det(code-12, the-10)
amod(code-12, genetic-11)
prep_with(start-8, code-12)

