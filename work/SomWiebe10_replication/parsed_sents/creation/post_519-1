(ROOT
  (S
    (NP (EX There))
    (VP (VBZ is)
      (NP (DT a) (JJ wonderful) (NN world) (JJ wide) (NN web))
      (PP (IN out)
        (NP (RB there)))
      (PP (IN with)
        (NP
          (NP (DT a) (NN lot))
          (PP (IN of)
            (NP (NN substance))))))
    (. .)))

expl(is-2, There-1)
root(ROOT-0, is-2)
det(web-7, a-3)
amod(web-7, wonderful-4)
nn(web-7, world-5)
amod(web-7, wide-6)
nsubj(is-2, web-7)
prep(is-2, out-8)
pobj(out-8, there-9)
det(lot-12, a-11)
prep_with(is-2, lot-12)
prep_of(lot-12, substance-14)

