    (, ,)
    (NP (JJ other) (JJ human) (NNS beings))
    (VP (VBP are)
      (NP
        (NP (DT the) (JJ only) (NNS creatures))
        (SBAR
          (S
            (NP (PRP we))
            (VP (VBP have)
              (ADVP (RB ever))
              (VP
                (ADVP (RB demonstrably))
                (VBN seen)
                (NP (NN design) (NN anything))))))))
    (. .)))

