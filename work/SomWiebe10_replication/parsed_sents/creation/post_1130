  (S
    (NP (PRP You))
    (VP (MD should) (RB n't)
      (ADVP (RB just) (RB blindly))
      (VP (VB repeat)
        (NP
          (NP (NNS things))
          (SBAR
            (S
              (NP (PRP you))
              (VP (VBP hear)))))))
    (. .)))

