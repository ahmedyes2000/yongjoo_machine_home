(ROOT
  (S
    (NP (PRP I))
    (ADVP (RB only))
    (VP (VBP have)
      (NP
        (NP (CD one) (NN life))
        (PP (IN on)
          (NP (NNP Earth))))
      (PP (IN in)
        (SBAR
          (WHNP (WDT which))
          (S
            (VP (TO to)
              (VP (VB learn)))))))
    (. .)))

nsubj(have-3, I-1)
advmod(have-3, only-2)
root(ROOT-0, have-3)
num(life-5, one-4)
dobj(have-3, life-5)
prep_on(life-5, Earth-7)
nsubj(learn-11, which-9)
aux(learn-11, to-10)
prepc_in(have-3, learn-11)

