(ROOT
  (S
    (PP (IN After)
      (NP (DT all)))
    (, ,)
    (NP (PRP they))
    (VP (MD would)
      (VP (VB be)
        (NP
          (NP (NNP God) (POS 's))
          (UCP
            (NP (NN creation))
            (CC and)
            (ADVP (RB therefore)))
          (NNS artifacts))))
    (. .)))

prep_after(artifacts-12, all-2)
nsubj(artifacts-12, they-4)
aux(artifacts-12, would-5)
cop(artifacts-12, be-6)
poss(artifacts-12, God-7)
