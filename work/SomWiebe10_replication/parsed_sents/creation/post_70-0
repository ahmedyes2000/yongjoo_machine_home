(ROOT
  (UCP (: --)
    (S (CC And)
      (ADVP (RB as)
        (PP (IN with)
          (NP (NNP ALL) (JJ scientific) (NNS inferences))))
      (NP (JJ further) (NN research))
      (VP (MD can) (RB either)
        (VP (VB confirm)
          (CC or)
          (VB refute)
          (NP (DT that) (NN inference)))))
    (. .)))

advmod(confirm-12, as-3)
nn(inferences-7, ALL-5)
amod(inferences-7, scientific-6)
prep_with(as-3, inferences-7)
amod(research-9, further-8)
nsubj(confirm-12, research-9)
nsubj(refute-14, research-9)
aux(confirm-12, can-10)
advmod(confirm-12, either-11)
root(ROOT-0, confirm-12)
conj_or(confirm-12, refute-14)
det(inference-16, that-15)
dobj(confirm-12, inference-16)

