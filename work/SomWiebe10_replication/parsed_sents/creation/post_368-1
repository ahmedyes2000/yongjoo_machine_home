(ROOT
  (S
    (S
      (NP (DT The) (NN evidence))
      (VP (VBN given)
        (NP (NNS amounts))
        (PP (TO to)
          (NP (DT this)))))
    (: :)
    (S
      (LST (LS 1) (-RRB- -RRB-))
      (NP (NNP Life))
      (VP (VBZ is)
        (ADJP (JJ complex))))
    (. .)))

det(evidence-2, The-1)
nsubj(given-3, evidence-2)
root(ROOT-0, given-3)
dobj(given-3, amounts-4)
prep_to(given-3, this-6)
dep(complex-12, 1-8)
