(ROOT
  (S
    (NP (DT This))
    (VP (VBZ is)
      (SBAR (RB so) (IN because)
        (S
          (NP (PRP it))
          (VP (VBZ has)
            (VP (VBN been)
              (VP (VBN proven)))))))
    (. .)))

nsubj(is-2, This-1)
root(ROOT-0, is-2)
advmod(proven-8, so-3)
dep(proven-8, because-4)
nsubjpass(proven-8, it-5)
aux(proven-8, has-6)
auxpass(proven-8, been-7)
ccomp(is-2, proven-8)

