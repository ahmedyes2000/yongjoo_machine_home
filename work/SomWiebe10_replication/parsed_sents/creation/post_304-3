(ROOT
  (S (`` ``)
    (CC And)
    (ADVP (RB apparently))
    (NP (`` ``) (NN tell-tale) (NNS signs) ('' ''))
    (VP (VBP are)
      (ADVP (RB also))
      (PP (IN in)
        (NP
          (NP (DT the) (NN eye))
          (PP (IN of)
            (NP (DT the) (NN beholder))))))
    (. .)))

cc(are-8, And-2)
advmod(are-8, apparently-3)
nn(signs-6, tell-tale-5)
nsubj(are-8, signs-6)
root(ROOT-0, are-8)
advmod(are-8, also-9)
det(eye-12, the-11)
