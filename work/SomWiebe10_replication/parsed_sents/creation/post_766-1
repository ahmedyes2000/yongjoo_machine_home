(ROOT
  (S
    (NP (PRP They))
    (VP (VBP are)
      (PP (IN after)
        (NP (PDT all) (DT both)))
      (PP (VBN based)
        (PP (IN on)
          (NP
            (NP (NNS books))
            (VP (VBN written)
              (PP (IN by)
                (NP (NNS men))))))))
    (. .)))

nsubj(are-2, They-1)
root(ROOT-0, are-2)
predet(both-5, all-4)
prep_after(are-2, both-5)
prepc_based_on(are-2, on-7)
pobj(are-2, books-8)
partmod(books-8, written-9)
agent(written-9, men-11)

