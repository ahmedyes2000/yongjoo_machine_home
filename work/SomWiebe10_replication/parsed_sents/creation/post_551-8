(ROOT
  (S
    (PP (IN In)
      (NP (JJ other) (NNS words)))
    (, ,)
    (NP (DT this) (NN issue))
    (VP (VBZ is)
      (NP (DT a) (JJ red) (NN herring)))
    (. .)))

amod(words-3, other-2)
prep_in(herring-10, words-3)
det(issue-6, this-5)
nsubj(herring-10, issue-6)
cop(herring-10, is-7)
det(herring-10, a-8)
amod(herring-10, red-9)
root(ROOT-0, herring-10)

