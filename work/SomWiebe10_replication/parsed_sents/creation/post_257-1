(ROOT
  (PRN (-LRB- -LRB-)
    (FRAG
      (NP (NNP Hint))
      (: :)
      (S
        (NP (PRP we))
        (VP (VBP 've)
          (VP (VBN been)
            (ADVP (RB here))
            (ADVP (RB before)))))
      (. .))
    (-RRB- -RRB-)))

root(ROOT-0, Hint-2)
nsubj(been-6, we-4)
aux(been-6, 've-5)
dep(Hint-2, been-6)
advmod(been-6, here-7)
advmod(been-6, before-8)

