(ROOT
  (S
    (NP (PRP they))
    (VP (MD would) (RB all)
      (VP (VB need)
        (S
          (VP (TO to)
            (VP (VB happen)
              (NP
                (NP (NNP simultaneously))
                (PRN (-LRB- -LRB-)
                  (CC or)
                  (ADVP (IN as) (DT a))
                  (NP (NN stretch) (. .)))))))))
    (. .)))

nsubj(need-4, they-1)
xsubj(happen-6, they-1)
aux(need-4, would-2)
advmod(need-4, all-3)
root(ROOT-0, need-4)
aux(happen-6, to-5)
xcomp(need-4, happen-6)
dobj(happen-6, simultaneously-7)
dep(stretch-12, or-9)
dep(stretch-12, as-10)
dep(as-10, a-11)
dep(simultaneously-7, stretch-12)

