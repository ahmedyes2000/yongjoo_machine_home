(ROOT
  (S
    (NP (DT This))
    (VP (VBZ is)
      (SBAR
        (WHADVP (WRB why))
        (S
          (NP (JJ intelligent) (NN design))
          (VP (VBD was)
            (VP (VBN rejected))))))
    (. .)))

nsubj(is-2, This-1)
root(ROOT-0, is-2)
advmod(rejected-7, why-3)
amod(design-5, intelligent-4)
nsubjpass(rejected-7, design-5)
auxpass(rejected-7, was-6)
advcl(is-2, rejected-7)

