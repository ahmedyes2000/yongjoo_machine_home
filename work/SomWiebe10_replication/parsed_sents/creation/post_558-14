(ROOT
  (S
    (NP (DT This) (NN argument))
    (VP (VBZ offers)
      (NP
        (NP
          (NP (DT no) (NNS predictions))
          (PP (IN about)
            (NP (DT the) (NN development))))
        (CC or)
        (NP
          (NP (NNS characteristics))
          (PP (IN of)
            (NP (NN life))))))
    (. .)))

det(argument-2, This-1)
nsubj(offers-3, argument-2)
root(ROOT-0, offers-3)
det(predictions-5, no-4)
dobj(offers-3, predictions-5)
det(development-8, the-7)
prep_about(predictions-5, development-8)
dobj(offers-3, characteristics-10)
conj_or(predictions-5, characteristics-10)
prep_of(characteristics-10, life-12)

