(ROOT
  (S
    (NP (PRP You))
    (VP (VBP 're)
      (VP (VBG missing)
        (NP
          (NP (DT the) (NN point))
          (PP (IN about)
            (NP (DT the) (CD four) (NNS rivers))))))
    (. .)))

nsubj(missing-3, You-1)
aux(missing-3, 're-2)
root(ROOT-0, missing-3)
det(point-5, the-4)
dobj(missing-3, point-5)
det(rivers-9, the-7)
num(rivers-9, four-8)
prep_about(point-5, rivers-9)

