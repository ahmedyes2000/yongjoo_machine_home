(ROOT
  (S
    (NP (EX There))
    (VP (VBZ is)
      (NP
        (NP (DT no) (JJ such) (NN thing))
        (PP (IN as)
          (NP (NN evolution)))
        (SBAR
          (S
            (NP (PRP it))
            (VP (VBZ 's)
              (NP (DT a) (NN myth)))))))
    (. .)))

expl(is-2, There-1)
root(ROOT-0, is-2)
det(thing-5, no-3)
amod(thing-5, such-4)
nsubj(is-2, thing-5)
prep_as(thing-5, evolution-7)
nsubj(myth-11, it-8)
cop(myth-11, 's-9)
det(myth-11, a-10)
rcmod(thing-5, myth-11)

