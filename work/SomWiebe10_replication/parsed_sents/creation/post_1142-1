  (S
    (NP
      (NP (DT The) (JJ next) (NN time))
      (SBAR
        (S
          (NP (PRP you))
          (VP (VBP get)
            (SBAR
              (S
                (NP
                  (NP (DT an) (NN infection))
                  (CC and)
                  (NP (DT the) (NN doctor)))
                (VP (VBZ gives)
                  (NP (PRP you))
                  (NP
                    (NP (CD 10) (NNS days))
                    (PP (IN of)
                      (NP (NNS antibiotics)))))))))))
    (, ,)
    (VP
      (VP (VB take)
        (NP (PRP them))
        (PP (IN for)
          (NP (CD 5) (NNS days))))
      (, ,) (RB then)
      (VP (VB stop)))
    (. .)))

