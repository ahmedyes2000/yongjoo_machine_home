(ROOT
  (S
    (NP
      (NP
        (NP (DT The) (NNP NCSE) (POS 's))
        (NN response))
      (PP (TO to)
        (NP (NNP ID))))
    (VP (VBZ is)
      (SBAR (IN that)
        (S
          (S
            (NP (RB about) (CD 90,000) (NN scientist))
            (VP (VBP do) (RB n't)
              (VP (VB believe)
                (PP (IN in)
                  (NP (NNP ID))))))
          (CC and)
          (S
            (NP (PRP you))
            (VP (MD should) (RB n't)))
          (ADVP (RB either)))))
    (. .)))

det(NCSE-2, The-1)
poss(response-4, NCSE-2)
nsubj(is-7, response-4)
prep_to(response-4, ID-6)
root(ROOT-0, is-7)
complm(believe-14, that-8)
advmod(scientist-11, about-9)
num(scientist-11, 90,000-10)
nsubj(believe-14, scientist-11)
aux(believe-14, do-12)
neg(believe-14, n't-13)
ccomp(is-7, believe-14)
prep_in(believe-14, ID-16)
nsubj(should-19, you-18)
ccomp(is-7, should-19)
conj_and(believe-14, should-19)
neg(should-19, n't-20)
advmod(believe-14, either-21)

