(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP (VBP do) (RB n't))))
    (NP (DT that))
    (VP (VBZ 's)
      (NP (PRP$ your) (NN loss) (NN man)))
    (. .)))

mark(do-3, If-1)
nsubj(do-3, you-2)
advcl(man-9, do-3)
neg(do-3, n't-4)
nsubj(man-9, that-5)
cop(man-9, 's-6)
poss(man-9, your-7)
nn(man-9, loss-8)
root(ROOT-0, man-9)

(ROOT
  (SINV
    (PP (IN As)
      (NP (PRP I)))
    (VP (VBD said))
    (NP
      (NP (NNS b4))
      (, ,)
      (NP
        (NP (JJ good) (NN luck))
        (PP (IN on)
          (NP (PRP$ your) (NN journey)
            (CC and)
            (NN goodbye)))))
    (. .)))

