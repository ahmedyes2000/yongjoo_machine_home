(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is)
      (NP
        (NP (DT the) (JJ scientific) (NN evidence))
        (SBAR
          (WHNP (WDT that))
          (S
            (VP (VBZ is)
              (ADJP (JJ important)))))))
    (. .)))

nsubj(evidence-5, It-1)
cop(evidence-5, is-2)
det(evidence-5, the-3)
amod(evidence-5, scientific-4)
root(ROOT-0, evidence-5)
nsubj(important-8, evidence-5)
cop(important-8, is-7)
rcmod(evidence-5, important-8)

