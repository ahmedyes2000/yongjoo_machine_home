(ROOT
  (S
    (SBAR
      (WHADVP (WRB When))
      (S
        (NP (DT the) (NNS plants))
        (VP (VBD formed))))
    (NP (PRP they))
    (VP (VBD filtered)
      (NP (DT the) (NN atmosphere)))
    (. .)))

advmod(formed-4, When-1)
det(plants-3, the-2)
nsubj(formed-4, plants-3)
advcl(filtered-6, formed-4)
nsubj(filtered-6, they-5)
root(ROOT-0, filtered-6)
det(atmosphere-8, the-7)
dobj(filtered-6, atmosphere-8)

