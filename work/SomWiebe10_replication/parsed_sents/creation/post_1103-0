(ROOT
  (S
    (NP
      (NP (DT The) (NN evolution))
      (SBAR (IN that)
        (S
          (NP (PRP I))
          (VP (VBP believe)
            (PRT (RP in))))))
    (VP (VBZ is) (RB not)
      (NP
        (NP (DT the) (NN kind))
        (SBAR
          (S
            (NP (PRP you))
            (VP (VBP think))))))
    (. .)))

det(evolution-2, The-1)
dobj(believe-5, evolution-2)
nsubj(kind-10, evolution-2)
complm(believe-5, that-3)
nsubj(believe-5, I-4)
dep(evolution-2, believe-5)
prt(believe-5, in-6)
cop(kind-10, is-7)
neg(kind-10, not-8)
det(kind-10, the-9)
root(ROOT-0, kind-10)
dobj(think-12, kind-10)
nsubj(think-12, you-11)
rcmod(kind-10, think-12)

