(ROOT
  (S
    (SBAR
      (WHADVP (WRB When))
      (S
        (VP (VBD asked)
          (S
            (VP (TO to)
              (VP (VB defend)
                (NP (NNP ID))))))))
    (, ,)
    (NP (PRP you))
    (VP (VBP expose)
      (NP
        (NP (PRP$ your) (NN ignorance))
        (PP (IN of)
          (NP (JJ real) (NN science)))))
    (. .)))

advmod(asked-2, When-1)
advcl(expose-8, asked-2)
aux(defend-4, to-3)
xcomp(asked-2, defend-4)
dobj(defend-4, ID-5)
nsubj(expose-8, you-7)
root(ROOT-0, expose-8)
poss(ignorance-10, your-9)
dobj(expose-8, ignorance-10)
amod(science-13, real-12)
