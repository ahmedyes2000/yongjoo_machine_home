(ROOT
  (S ('' '')
    (NP (PRP We))
    (VP (VBP re)
      (S
        (VP (VBG talking)
          (PP (IN about)
            (NP
              (NP (DT the)
                (ADJP (RB publicly) (JJ available))
                (NN evidence))
              (PP (IN of)
                (NP (NN nature)))))
          (, ,) (`` ``)
          (NP (NNS Nope)))))
    (. .)))

nsubj(re-3, We-2)
root(ROOT-0, re-3)
xcomp(re-3, talking-4)
det(evidence-9, the-6)
advmod(available-8, publicly-7)
amod(evidence-9, available-8)
prep_about(talking-4, evidence-9)
prep_of(evidence-9, nature-11)
dobj(talking-4, Nope-14)

