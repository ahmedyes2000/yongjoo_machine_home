(ROOT
  (NP (CD '') (RB No) (. !)))


(ROOT
  (S
    (NP (DT That))
    (VP (VBZ is)
      (NP
        (NP (DT an) (NN opinion))
        (, ,) (RB not)
        (NP (JJ scientific) (NN fact))))
    (. .)))

nsubj(opinion-4, That-1)
cop(opinion-4, is-2)
det(opinion-4, an-3)
root(ROOT-0, opinion-4)
neg(opinion-4, not-6)
amod(fact-8, scientific-7)
dep(opinion-4, fact-8)

