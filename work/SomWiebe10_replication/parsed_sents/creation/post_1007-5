(ROOT
  (S
    (S
      (VP (TO to)
        (VP (VB think)
          (NP (PRP they)))))
    (VP (VBD discovered)
      (NP (DT a) (NN planet)))
    (. !)))

aux(think-2, to-1)
csubj(discovered-4, think-2)
dobj(think-2, they-3)
root(ROOT-0, discovered-4)
det(planet-6, a-5)
dobj(discovered-4, planet-6)

