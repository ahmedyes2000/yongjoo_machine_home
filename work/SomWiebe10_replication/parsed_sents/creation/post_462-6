(ROOT
  (S
    (NP (PRP I))
    (VP (VBP think)
      (S
        (NP (PRP we))
        (RB all)
        (VP (VB know)
          (NP (DT the) (NN reason)))))
    (. .)))

nsubj(think-2, I-1)
root(ROOT-0, think-2)
nsubj(know-5, we-3)
advmod(know-5, all-4)
ccomp(think-2, know-5)
det(reason-7, the-6)
dobj(know-5, reason-7)

