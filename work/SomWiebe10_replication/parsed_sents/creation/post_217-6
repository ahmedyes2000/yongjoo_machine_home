(ROOT
  (S (IN So)
    (SBAR (IN if)
      (S
        (NP (DT those) (NNS words))
        (ADVP (RB actually))
        (VP (VBD had)
          (NP (DT any) (NN meaning)))))
    (, ,)
    (NP (NN everyone))
    (VP (MD would)
      (VP (VB assume)
        (SBAR
          (S
            (NP (PRP it))
            (VP (VBD was)
              (NP (DT a) (JJ religious) (NN meaning)))))))
    (. .)))

dep(assume-12, So-1)
mark(had-6, if-2)
det(words-4, those-3)
nsubj(had-6, words-4)
advmod(had-6, actually-5)
advcl(assume-12, had-6)
det(meaning-8, any-7)
dobj(had-6, meaning-8)
nsubj(assume-12, everyone-10)
aux(assume-12, would-11)
root(ROOT-0, assume-12)
nsubj(meaning-17, it-13)
cop(meaning-17, was-14)
det(meaning-17, a-15)
amod(meaning-17, religious-16)
ccomp(assume-12, meaning-17)

