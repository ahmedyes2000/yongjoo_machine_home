(ROOT
  (S
    (NP
      (NP (DT The) (JJ current) (NN estimate))
      (PP (IN of)
        (NP
          (NP (DT the) (NN age))
          (PP (IN of)
            (NP (DT the) (NN universe))))))
    (VP (VBZ is)
      (ADVP (RB totally))
      (PP (IN in)
        (NP
          (NP (NN accord))
          (PP (IN with)
            (NP (DT this) (JJ minimal) (NN assuption))))))
    (. .)))

det(estimate-3, The-1)
amod(estimate-3, current-2)
nsubj(is-10, estimate-3)
det(age-6, the-5)
prep_of(estimate-3, age-6)
det(universe-9, the-8)
prep_of(age-6, universe-9)
root(ROOT-0, is-10)
advmod(is-10, totally-11)
prep_in(is-10, accord-13)
det(assuption-17, this-15)
amod(assuption-17, minimal-16)
prep_with(accord-13, assuption-17)

