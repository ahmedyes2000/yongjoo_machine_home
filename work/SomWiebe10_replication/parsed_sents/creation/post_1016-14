(ROOT
  (S ('' '')
    (S
      (NP
        (NP (: ...)
          (NP (NNP and))
          (PP (VBN investigating)
            (SBAR
              (WHADVP (WRB how))
              (S
                (NP (DT this))
                (VP (VBZ happens)
                  (SBAR
                    (S
                      (VP (VBZ does)))))))))
        (ADVP (RB not)))
      (VP (VBP depend)))
    (IN on)
    (S
      (NP (NN understanding))
      (VP (VBZ exactly)
        (SBAR
          (WHADVP (WRB how))
          (S
            (NP (NN life))
            (VP (VBD began))))))
    (. .)))

nsubj(depend-10, and-3)
prep(and-3, investigating-4)
advmod(happens-7, how-5)
nsubj(happens-7, this-6)
pcomp(investigating-4, happens-7)
ccomp(happens-7, does-8)
advmod(and-3, not-9)
root(ROOT-0, depend-10)
dep(depend-10, on-11)
nsubj(exactly-13, understanding-12)
ccomp(depend-10, exactly-13)
advmod(began-16, how-14)
nsubj(began-16, life-15)
ccomp(exactly-13, began-16)

