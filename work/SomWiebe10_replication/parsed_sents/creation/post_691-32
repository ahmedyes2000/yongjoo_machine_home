(ROOT
  (S
    (NP (PRP I))
    (VP (VBP am)
      (ADVP (RB always))
      (ADJP (JJ willing)
        (S
          (VP (TO to)
            (VP (VB learn)
              (NP (JJR more)))))))
    (. .)))

nsubj(willing-4, I-1)
cop(willing-4, am-2)
advmod(willing-4, always-3)
root(ROOT-0, willing-4)
aux(learn-6, to-5)
xcomp(willing-4, learn-6)
dobj(learn-6, more-7)

