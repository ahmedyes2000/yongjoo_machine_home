(ROOT
  (S
    (NP (PRP I))
    (VP (VBP 'm)
      (ADJP (RB pretty) (JJ sure)
        (SBAR
          (S
            (NP (PRP YOU))
            (VP (MD can)
              (ADVP (RB even))
              (VP (VB create)
                (NP (NN light))
                (PP (IN without)
                  (NP
                    (NP (DT the) (NNP Sun))
                    (PP (IN in)
                      (NP (JJ many) (NNS ways)))))
                (ADVP (RB even))))))))
    (. .)))

nsubj(sure-4, I-1)
cop(sure-4, 'm-2)
advmod(sure-4, pretty-3)
root(ROOT-0, sure-4)
nsubj(create-8, YOU-5)
aux(create-8, can-6)
advmod(create-8, even-7)
dep(sure-4, create-8)
dobj(create-8, light-9)
det(Sun-12, the-11)
prep_without(create-8, Sun-12)
amod(ways-15, many-14)
prep_in(Sun-12, ways-15)
advmod(create-8, even-16)

