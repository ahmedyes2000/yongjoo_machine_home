      (NP
        (NP (JJ large) (NNS amounts))
        (PP (IN of)
          (NP
            (NP (NN material) (NN word))
            (PP (IN for)
              (NP (NN word))))))
      (PP (IN from)
        (NP
          (NP (DT the) (NN material))
          (SBAR
            (S
              (NP (PRP he))
              (VP (VBD received)
                (PP (IN from)
                  (NP (DT the) (NNP ACLU)))))))))
    (. .)))

