(ROOT
  (S
    (NP (EX There))
    (VP (VBZ is)
      (NP
        (NP (DT no) (`` ``) (JJ if-then) ('' '') (NN argument))
        (VP (VBN required)
          (PP (IN for)
            (NP
              (NP (NN something))
              (SBAR
                (WHNP (WDT which))
                (S
                  (VP (VBZ has)
                    (NP
                      (ADJP (RB absolutely) (DT no))
                      (NN evidence))))))))))
    (. .)))

expl(is-2, There-1)
root(ROOT-0, is-2)
det(argument-7, no-3)
amod(argument-7, if-then-5)
nsubj(is-2, argument-7)
partmod(argument-7, required-8)
prep_for(required-8, something-10)
nsubj(has-12, something-10)
rcmod(something-10, has-12)
advmod(no-14, absolutely-13)
amod(evidence-15, no-14)
dobj(has-12, evidence-15)

