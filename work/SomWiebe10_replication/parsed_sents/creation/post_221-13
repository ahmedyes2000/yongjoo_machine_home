(ROOT
  (S
    (NP
      (NP (DT The) (NN universe))
      (SBAR (IN that)
        (S
          (NP (PRP we))
          (VP (MD can)
            (VP (VB see)
              (ADVP (RB so) (RB far)))))))
    (VP (VBZ is)
      (ADJP (RB quite) (JJ simple)))
    (. .)))

det(universe-2, The-1)
dobj(see-6, universe-2)
nsubj(simple-11, universe-2)
complm(see-6, that-3)
nsubj(see-6, we-4)
