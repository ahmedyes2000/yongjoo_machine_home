(ROOT
  (S
    (NP (PRP They))
    (VP (VBP get)
      (ADJP (VBN upset)
        (SBAR
          (WHADVP (WRB when))
          (S
            (NP (PRP$ their) (NN world) (NN view))
            (VP (VBZ is)
              (VP (VBN challenged)))))))
    (. .)))

