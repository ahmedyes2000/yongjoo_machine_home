(ROOT
  (S
    (NP (PRP You))
    (VP (VBP are)
      (ADJP (JJ justified)
        (PP (IN for)
          (S
            (VP (VBG inventing)
              (NP (NNS ideas)))))))
    (. .)))

nsubj(justified-3, You-1)
cop(justified-3, are-2)
root(ROOT-0, justified-3)
prepc_for(justified-3, inventing-5)
dobj(inventing-5, ideas-6)

