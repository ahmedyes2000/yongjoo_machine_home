(ROOT
  (S
    (NP (NNP ID))
    (VP (VBZ is)
      (NP (DT an) (JJ interesting)
        (ADJP (JJ subject)
          (PP (IN for)
            (NP (JJ late-night) (NNS discussions)))
          (PP (IN after)
            (NP
              (NP (DT a) (NN party))
              (PP (IN like)
                (NP
                  (NP (NN philosophy))
                  (, ,)
                  (NP (JJ comparative) (NN religion))
                  (, ,)
                  (NP (NNS politics))
                  (, ,)
                  (CC and)
                  (NP (NNS sports)))))))
        (NN history)))
    (. .)))

nsubj(history-22, ID-1)
cop(history-22, is-2)
det(history-22, an-3)
amod(history-22, interesting-4)
amod(history-22, subject-5)
amod(discussions-8, late-night-7)
prep_for(subject-5, discussions-8)
det(party-11, a-10)
prep_after(subject-5, party-11)
