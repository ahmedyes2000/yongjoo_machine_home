(ROOT
  (S
    (VP (VBD \/) (-RRB- -RRB-)
      (NP
        (NP (NNP Also))
        (, ,)
        (NP (JJS most) (NNS scientists)))
      (ADVP (DT no) (RB longer)
        (SBAR
          (S
            (VP (VBP believe)
              (SBAR
                (SBAR (IN that)
                  (S
                    (NP (PRP we))
                    (VP (VBD evolved)
                      (PP (IN from)
                        (NP (NN cavemen))))))
                (CC or)
                (SBAR
                  (WHADVP (WDT whatever)))))))))
    (. .)))

root(ROOT-0, \/-1)
dobj(\/-1, Also-3)
amod(scientists-6, most-5)
appos(Also-3, scientists-6)
dep(longer-8, no-7)
advmod(\/-1, longer-8)
dep(longer-8, believe-9)
complm(evolved-12, that-10)
nsubj(evolved-12, we-11)
ccomp(believe-9, evolved-12)
prep_from(evolved-12, cavemen-14)
ccomp(believe-9, whatever-16)
conj_or(evolved-12, whatever-16)

