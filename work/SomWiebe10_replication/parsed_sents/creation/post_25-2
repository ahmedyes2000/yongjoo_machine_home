(ROOT
  (S ('' '')
    (NP (NNP Flat))
    (VP
      (ADVP (RB earthism))
      ('' '') (MD would)
      (VP
        (ADVP (RB never))
        (VB be)
        (VP (VBN taught)
          (NP (NN today))
          (PP (IN as)
            (NP (DT a) (JJ legitimate) (NN theory))))))
    (. .)))

nsubjpass(taught-8, Flat-2)
advmod(taught-8, earthism-3)
aux(taught-8, would-5)
neg(taught-8, never-6)
auxpass(taught-8, be-7)
root(ROOT-0, taught-8)
tmod(taught-8, today-9)
det(theory-13, a-11)
amod(theory-13, legitimate-12)
prep_as(taught-8, theory-13)

