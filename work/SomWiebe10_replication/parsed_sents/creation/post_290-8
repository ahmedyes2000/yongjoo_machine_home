  (S
    (ADVP (RB Surely))
    (NP (PRP you))
    (VP (MD would) (RB not)
      (VP (VB want)
        (S
          (VP (TO to)
            (VP (VB portray)
              (NP (PDT such) (DT a) (JJ foolish) (NN notion)))))))
    (. ?)))

