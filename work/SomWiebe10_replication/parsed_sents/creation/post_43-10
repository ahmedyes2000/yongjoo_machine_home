(ROOT
  (S
    (S
      (NP (PRP It))
      (VP (VBD was)
        (ADVP (RB not) ('' ''))))
    (NP (PRP I))
    (VP (VBP refuse)
      (S
        (VP (TO to)
          (VP (VB believe)
            (SBAR
              (S
                (NP (NNP GOD))
                (VP (MD would)
                  (VP (VB play)
                    (NP (NN chance))
                    (PP (IN with)
                      (NP (DT the) (NNP Universe))))))))))
      ('' '')
      (ADVP
        (ADVP (RB but) (RB rather))
        ('' '')
        (SBAR
          (WHNP
            (NP (PRP I))
            (PRN (, ,)
              (PP (IN at)
                (NP (DT any) (NN rate)))
              (, ,)))
          (S
            (VP (VBP am)
              (ADJP (VBN convinced)
                (SBAR (IN that)
                  (S
                    (NP (PRP He)
                      (PRN (-LRB- -LRB-)
                        (NP (NNP God))
                        (-RRB- -RRB-)))
                    (VP (VBZ does) (RB not)
                      (VP (VB throw)
                        (NP (NNS dice))))))))))))
    (. .)))

