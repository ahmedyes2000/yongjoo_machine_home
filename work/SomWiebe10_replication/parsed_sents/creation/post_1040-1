(ROOT
  (S (CC But)
    (NP (PRP I))
    (VP (VBP do) (RB n't)
      (VP (VB understand)
        (SBAR
          (WHADVP (WRB why))
          (S
            (NP (PRP you))
            (VP (VBD opposed)
              (NP (PRP$ my) (NN argument)))))))
    (: ...)))

cc(understand-5, But-1)
nsubj(understand-5, I-2)
aux(understand-5, do-3)
neg(understand-5, n't-4)
root(ROOT-0, understand-5)
advmod(opposed-8, why-6)
nsubj(opposed-8, you-7)
advcl(understand-5, opposed-8)
poss(argument-10, my-9)
dobj(opposed-8, argument-10)

