(ROOT
  (S
    (NP (PRP You))
    (VP (MD should) (RB n't)
      (ADVP (RB just) (RB blindly))
      (VP (VB repeat)
        (NP
          (NP (NNS things))
          (SBAR
            (S
              (NP (PRP you))
              (VP (VBP hear)))))))
    (. .)))

nsubj(repeat-6, You-1)
aux(repeat-6, should-2)
neg(repeat-6, n't-3)
advmod(blindly-5, just-4)
advmod(repeat-6, blindly-5)
root(ROOT-0, repeat-6)
dobj(repeat-6, things-7)
dobj(hear-9, things-7)
nsubj(hear-9, you-8)
rcmod(things-7, hear-9)

