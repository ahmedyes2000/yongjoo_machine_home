(ROOT
  (SQ (VBP Are)
    (NP (PRP they))
    (VP (VBN born)
      (PP (TO to)
        (NP
          (NP (NNS mothers))
          (PP (IN of)
            (NP (DT a)
              (ADJP (RB completely) (JJ different))
              (NN species))))))
    (. ?)))

auxpass(born-3, Are-1)
nsubjpass(born-3, they-2)
root(ROOT-0, born-3)
prep_to(born-3, mothers-5)
det(species-10, a-7)
advmod(different-9, completely-8)
amod(species-10, different-9)
