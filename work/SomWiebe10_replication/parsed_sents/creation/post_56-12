(ROOT
  (S
    (NP (NNP Intermediate) (NN probability))
    (RB then)
    (NP (PRP it))
    (VP (VBD was)
      (VP (VBN caused)
        (PP (IN by)
          (NP (CD chance3)))))
    (. .)))

