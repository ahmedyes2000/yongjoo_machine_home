(ROOT
  (S
    (VP (VBG Using)
      (S
        (NP
          (NP (DT the) (NN bible))
          (CC or)
          (NP (DT any) (JJ sacred) (NN text)))
        (VP (TO to)
          (VP (VB form)
            (NP (CD conclusions) (CD 6))))))
    (. .)))

root(ROOT-0, Using-1)
det(bible-3, the-2)
nsubj(form-9, bible-3)
det(text-7, any-5)
amod(text-7, sacred-6)
conj_or(bible-3, text-7)
nsubj(form-9, text-7)
aux(form-9, to-8)
xcomp(Using-1, form-9)
num(6-11, conclusions-10)
dobj(form-9, 6-11)

