(ROOT
  (S
    (VP (VBG Going)
      (PRT (RP back))
      (PP (TO to)
        (NP
          (NP (DT the) (NN idea))
          (PP (IN of)
            (NP
              (NP (DT a) (NN storm))
              (CC and)
              (NP (NNS engines)))))))
    (. .)))

root(ROOT-0, Going-1)
prt(Going-1, back-2)
det(idea-5, the-4)
prep_to(Going-1, idea-5)
det(storm-8, a-7)
