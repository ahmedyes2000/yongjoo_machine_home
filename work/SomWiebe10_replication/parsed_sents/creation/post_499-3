(ROOT
  (S
    (ADVP (RB None-the-less))
    (, ,)
    (NP (PRP I))
    (VP (VBP do) (RB n't)
      (VP (VB disagree)
        (PP (IN with)
          (NP
            (NP (PRP$ his) (NN ruling))
            (PP (VBG regarding)
              (NP (DT the) (NN school) (NN board)))))))
    (. .)))

advmod(disagree-6, None-the-less-1)
nsubj(disagree-6, I-3)
aux(disagree-6, do-4)
neg(disagree-6, n't-5)
root(ROOT-0, disagree-6)
poss(ruling-9, his-8)
prep_with(disagree-6, ruling-9)
det(board-13, the-11)
nn(board-13, school-12)
prep_regarding(ruling-9, board-13)

