(ROOT
  (S ('' '')
    (NP (DT The) (NN responder))
    (VP (VBZ is)
      (VP (VBG dealing)
        (PP (IN in)
          (NP
            (NP (NN word))
            (S
              (S
                (VP (VBZ salads)))
              (, ,) (FW i.e.) (, ,)
              (S
                (VP (VBG formulating)
                  (NP
                    (NP (NNS arguments))
                    (SBAR
                      (WHNP (WDT that))
                      (S
                        (VP (VBP float)
                          (PP (IN without)
                            (NP (NN reference)))
                          (PP (TO to)
                            (NP (NN reality))))))))))))))
    (. .)))

det(responder-3, The-2)
nsubj(dealing-5, responder-3)
aux(dealing-5, is-4)
root(ROOT-0, dealing-5)
prep_in(dealing-5, word-7)
dep(word-7, salads-8)
dep(salads-8, i.e.-10)
dep(salads-8, formulating-12)
dobj(formulating-12, arguments-13)
nsubj(float-15, arguments-13)
