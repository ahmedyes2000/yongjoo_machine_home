(ROOT
  (S
    (S
      (NP (PRP It))
      (VP (VBZ 's)
        (NP (DT a) (NN shame))))
    (, ,)
    (NP (PRP I))
    (VP (VBD came)
      (PP (TO to)
        (NP (DT this) (NN site)))
      (S
        (VP (TO to)
          (VP
            (VP (VB express)
              (NP (PRP$ my) (NN opinion)))
            (CC and)
            (VP (VB hear)
              (NP (NNS others)))
            (NP (JJ GOOD) (NNS opinions))))))
    (. .)))

nsubj(shame-4, It-1)
cop(shame-4, 's-2)
det(shame-4, a-3)
ccomp(came-7, shame-4)
nsubj(came-7, I-6)
xsubj(express-12, I-6)
root(ROOT-0, came-7)
det(site-10, this-9)
prep_to(came-7, site-10)
aux(express-12, to-11)
xcomp(came-7, express-12)
poss(opinion-14, my-13)
dobj(express-12, opinion-14)
xcomp(came-7, hear-16)
conj_and(express-12, hear-16)
dobj(hear-16, others-17)
amod(opinions-19, GOOD-18)
dobj(express-12, opinions-19)

