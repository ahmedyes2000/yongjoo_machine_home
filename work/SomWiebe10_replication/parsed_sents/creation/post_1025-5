  (S
    (NP
      (NP (JJ Intelligent) (NN design))
      (, ,)
      (PP (IN on)
        (NP (DT the) (JJ other) (NN hand))))
    (VP (VBD began)
      (PP (IN as)
        (NP (DT an) (NN attempt)
          (S
            (VP (TO to)
              (VP (VB discredit)
                (NP (NN evolution)))))))
      (, ,)
      (PP (IN for)
        (NP (JJ religious) (NNS reasons))))
    (. .)))

amod(design-2, Intelligent-1)
nsubj(began-8, design-2)
det(hand-7, the-5)
amod(hand-7, other-6)
