(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ 's)
      (ADJP
        (ADVP (NN kind) (IN of))
        (JJ silly))
      (SBAR
        (WHADVP (WRB when))
        (S
          (NP (PRP you))
          (VP (VBP think)
            (PP (IN about)
              (NP (PRP it)))))))
    (. .)))

nsubj(silly-5, It-1)
cop(silly-5, 's-2)
dep(of-4, kind-3)
advmod(silly-5, of-4)
root(ROOT-0, silly-5)
advmod(think-8, when-6)
nsubj(think-8, you-7)
advcl(silly-5, think-8)
prep_about(think-8, it-10)

