(ROOT
  (S
    (NP
      (NP (DT The) (NN notion))
      (PP (IN of)
        (NP (JJ intelligent) (NN design))))
    (ADVP (RB directly))
    (VP (VBZ impacts)
      (NP
        (NP (PRP$ my) (NNS areas))
        (PP (IN of)
          (NP (NN study)))))
    (. .)))

det(notion-2, The-1)
nsubj(impacts-7, notion-2)
amod(design-5, intelligent-4)
prep_of(notion-2, design-5)
advmod(impacts-7, directly-6)
root(ROOT-0, impacts-7)
poss(areas-9, my-8)
