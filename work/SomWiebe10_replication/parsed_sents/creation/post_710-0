(ROOT
  (S
    (NP (NNP Genesis))
    (VP (VBZ is)
      (ADJP (RB very) (JJ clear))
      (SBAR (IN that)
        (S
          (NP
            (NP (DT the) (NN evening))
            (CC and)
            (NP (DT the) (NN morning)))
          (VP (VBD were)
            (NP (DT the) (JJ first) (NN day))))))
    (. .)))

nsubj(clear-4, Genesis-1)
cop(clear-4, is-2)
advmod(clear-4, very-3)
root(ROOT-0, clear-4)
complm(day-14, that-5)
det(evening-7, the-6)
nsubj(day-14, evening-7)
det(morning-10, the-9)
conj_and(evening-7, morning-10)
nsubj(day-14, morning-10)
cop(day-14, were-11)
det(day-14, the-12)
amod(day-14, first-13)
ccomp(clear-4, day-14)

