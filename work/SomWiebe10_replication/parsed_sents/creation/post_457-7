(ROOT
  (S
    (S
      (NP (PRP We))
      (VP (VBP use)
        (NP (PRP$ our) (NNS imaginations))
        (S
          (VP (TO to)
            (VP (VB try)
              (S
                (VP (TO to)
                  (VP (VB figure)
                    (NP (NN stuff))
                    (PRT (RP out))))))))))
    (, ,)
    (CC and)
    (S
      (NP (PRP we))
      (VP (VBP invent)
        (S
          (NP (NNS explanations)
            (CC or)
            (NNS concepts))
          (VP (TO to)
            (VP (VB help)
              (S
                (NP (PRP us))
                (VP (VB deal)
                  (PP (IN with)
                    (NP (NNS problems))))))))))
    (. .)))

nsubj(use-2, We-1)
root(ROOT-0, use-2)
poss(imaginations-4, our-3)
dobj(use-2, imaginations-4)
aux(try-6, to-5)
xcomp(use-2, try-6)
aux(figure-8, to-7)
xcomp(try-6, figure-8)
dobj(figure-8, stuff-9)
prt(figure-8, out-10)
nsubj(invent-14, we-13)
conj_and(use-2, invent-14)
nsubj(help-19, explanations-15)
conj_or(explanations-15, concepts-17)
nsubj(help-19, concepts-17)
aux(help-19, to-18)
xcomp(invent-14, help-19)
nsubj(deal-21, us-20)
ccomp(help-19, deal-21)
prep_with(deal-21, problems-23)

