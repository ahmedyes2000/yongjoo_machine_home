(ROOT
  (S
    (NP (NNP DNA))
    (VP (VBZ is) (RB not)
      (NP
        (NP (DT a) (NN molecule))
        (PP (IN with)
          (NP (DT a) (NN pattern)))))
    (. .)))

nsubj(molecule-5, DNA-1)
cop(molecule-5, is-2)
neg(molecule-5, not-3)
det(molecule-5, a-4)
root(ROOT-0, molecule-5)
det(pattern-8, a-7)
prep_with(molecule-5, pattern-8)

