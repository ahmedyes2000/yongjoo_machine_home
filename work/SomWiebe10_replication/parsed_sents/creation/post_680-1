(ROOT
  (S
    (VP (VB Email)
      (NP (PRP me))
      (SBAR (IN if)
        (S
          (NP (PRP you))
          (VP (VBP want)
            (ADVP (RBR more))))))
    (. .)))

root(ROOT-0, Email-1)
dobj(Email-1, me-2)
mark(want-5, if-3)
nsubj(want-5, you-4)
advcl(Email-1, want-5)
advmod(want-5, more-6)

