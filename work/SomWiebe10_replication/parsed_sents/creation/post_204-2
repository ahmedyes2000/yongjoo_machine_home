(ROOT
  (S (IN So)
    (NP (JJ intelligent) (NN design))
    (VP (VBZ fails)
      (PP (IN in)
        (NP (PRP$ its) (JJ anti) (NN evolution) (NN purpose))))
    (. .)))

dep(fails-4, So-1)
amod(design-3, intelligent-2)
nsubj(fails-4, design-3)
root(ROOT-0, fails-4)
poss(purpose-9, its-6)
amod(purpose-9, anti-7)
nn(purpose-9, evolution-8)
prep_in(fails-4, purpose-9)

