(ROOT
  (S
    (NP (NNP ID))
    (VP (VBZ excludes)
      (NP (DT the) (NN designer))
      (PP (IN from)
        (NP
          (NP (DT the) (NN explanation))
          (PP (IN for)
            (NP
              (NP (NN life))
              (PP (IN on)
                (NP (NN earth))))))))
    (. .)))

nsubj(excludes-2, ID-1)
root(ROOT-0, excludes-2)
det(designer-4, the-3)
dobj(excludes-2, designer-4)
det(explanation-7, the-6)
prep_from(excludes-2, explanation-7)
prep_for(explanation-7, life-9)
prep_on(life-9, earth-11)

