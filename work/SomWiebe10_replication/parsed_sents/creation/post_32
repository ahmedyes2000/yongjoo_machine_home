  (S
    (NP (NNP Public) (NN opinion))
    (VP (VBZ is)
      (ADJP (JJ unimportant)
        (PP (TO to)
          (SBAR (IN whether)
            (S
              (NP (NN creationism))
              (VP (VBZ belongs)
                (PP (IN in)
                  (NP
                    (NP (NN science) (NNS classes))
                    (SBAR
                      (S
                        (NP (NNP Public) (NN opinion))
                        (VP (VBZ does) (RB not)
                          (VP (VB determine)
                            (SBAR (IN whether)
                              (S
                                (NP (NN creationism))
                                (VP (VBZ qualifies)
                                  (PP (IN as)
                                    (NP (NN science))))))))))))))))))
    (. .)))

