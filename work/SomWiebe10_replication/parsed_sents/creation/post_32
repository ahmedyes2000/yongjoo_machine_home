(ROOT
  (S
    (S
      (NP (DT This))
      (VP (VBZ is)
        (NP
          (NP (DT the) (JJ primary) (NN question))
          (VP (VBG surrounding)
            (SBAR (IN whether)
              (S
                (NP (PRP it))
                (VP (VBZ belongs)
                  (PP (IN in)
                    (NP (DT the) (NN classroom))))))))))
    (, ,) (IN so)
    (S
      (NP (JJ public) (NN opinion))
      (VP (MD should) (RB not)
        (VP (VB have)
          (NP
            (NP (DT any) (NN place))
            (PP (IN in)
              (NP (DT this) (NN debate)))))))
    (. .)))

