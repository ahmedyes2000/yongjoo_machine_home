(ROOT
  (S
    (NP (NNP ID))
    (VP (VBZ is)
      (ADVP (RB simply))
      (PP (IN about)
        (NP
          (NP (DT the) (NN evidence))
          (PP (IN for)
            (NP (NN design))))))
    (. .)))

nsubj(is-2, ID-1)
root(ROOT-0, is-2)
advmod(is-2, simply-3)
det(evidence-6, the-5)
prep_about(is-2, evidence-6)
prep_for(evidence-6, design-8)

