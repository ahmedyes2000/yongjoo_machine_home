  (S
    (S
      (NP (JJ Forgot) (DT that) (NN atheism))
      (VP (VBZ IS) (RB N'T)
        (NP (DT a) (NN religion))))
    (, ,)
    (CC and)
    (S
      (SBAR (IN if)
        (S
          (NP (PRP I))
          (VP (VBP say)
            (ADVP (RB otherwise)))))
      (, ,)
      (NP (NNS people))
      (VP (MD will)
        (ADVP (RB apologetically))
        (VP (VB insist)
          (SBAR
            (S
              (NP (PRP I))
              (VP (VBP 'm)
                (ADJP (JJ wrong))))))))
    (. .)))

