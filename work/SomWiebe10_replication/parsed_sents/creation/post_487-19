(ROOT
  (S
    (PP (IN For)
      (NP (NN example)))
    (, ,)
    (NP (NNP discussing) (NNP ID))
    (VP (MD can)
      (VP (VB help)
        (S
          (NP (NNS students))
          (VP (VB expand)
            (NP (NNS horizons))))))
    (. .)))

prep_for(help-7, example-2)
nn(ID-5, discussing-4)
nsubj(help-7, ID-5)
aux(help-7, can-6)
root(ROOT-0, help-7)
