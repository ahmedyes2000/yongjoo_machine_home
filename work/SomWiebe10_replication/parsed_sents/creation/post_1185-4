(ROOT
  (S
    (SBAR (IN Because)
      (S
        (NP (EX there))
        (VP (VBZ is)
          (NP (DT no) (NN ability)
            (S
              (VP (TO to)
                (VP (VB answer)
                  (SBARQ
                    (WHADVP (WRB why))))))))))
    (, ,)
    (NP (PRP they))
    (VP (VBP say)
      (SBAR
        (S
          (NP (EX there))
          (VP (VBZ is)
            (ADJP (RB no) (JJ why))))))
    (. .)))

mark(is-3, Because-1)
expl(is-3, there-2)
advcl(say-11, is-3)
det(ability-5, no-4)
nsubj(is-3, ability-5)
aux(answer-7, to-6)
infmod(ability-5, answer-7)
dep(answer-7, why-8)
nsubj(say-11, they-10)
root(ROOT-0, say-11)
expl(is-13, there-12)
ccomp(say-11, is-13)
advmod(why-15, no-14)
acomp(is-13, why-15)

