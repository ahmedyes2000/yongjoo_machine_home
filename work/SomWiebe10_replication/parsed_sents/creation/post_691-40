(ROOT
  (S
    (NP (DT The) (NNP Bible))
    (VP (VBZ does)
      (VP (VB have)
        (NP
          (NP (NNP Truth))
          (PP (IN in)
            (NP (PRP it))))))
    (. .)))

det(Bible-2, The-1)
nsubj(have-4, Bible-2)
aux(have-4, does-3)
root(ROOT-0, have-4)
dobj(have-4, Truth-5)
prep_in(Truth-5, it-7)

