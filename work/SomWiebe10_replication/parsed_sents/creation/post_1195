(ROOT
  (S
    (INTJ (UH No))
    (, ,)
    (NP (PRP it))
    (VP (VBZ is) (RB n't))
    (. .)))

dep(is-4, No-1)
nsubj(is-4, it-3)
root(ROOT-0, is-4)
neg(is-4, n't-5)

(ROOT
  (S
    (ADVP (RB As) (RB far)
      (SBAR (IN as)
        (S
          (NP (PRP I))
          (VP (MD can)
            (VP (VB tell))))))
    (NP (DT the) (NN evidence))
    (VP (VBZ is)
      (VP (VBG getting)
        (ADJP (JJR stronger))))
    (. .)))

advmod(far-2, As-1)
advmod(stronger-11, far-2)
mark(tell-6, as-3)
nsubj(tell-6, I-4)
aux(tell-6, can-5)
ccomp(far-2, tell-6)
det(evidence-8, the-7)
nsubj(stronger-11, evidence-8)
aux(stronger-11, is-9)
dep(stronger-11, getting-10)
root(ROOT-0, stronger-11)

