(ROOT
  (S
    (S
      (NP (PRP I))
      (ADVP (RB always))
      (VP (VBD thought)
        (SBAR
          (S
            (NP
              (NP (DT the) (NN light))
              (PP (IN on)
                (NP (DT the) (JJ first) (NN day))))
            (VP (VBD was)
              (NP (DT the) (NN sun)))))))
    (, ,)
    (CC but)
    (S
      (NP (DT that))
      (VP (VBZ is)
        (NP (DT another) (NN argument))))
    (. .)))

nsubj(thought-3, I-1)
advmod(thought-3, always-2)
root(ROOT-0, thought-3)
det(light-5, the-4)
nsubj(sun-12, light-5)
det(day-9, the-7)
amod(day-9, first-8)
prep_on(light-5, day-9)
cop(sun-12, was-10)
det(sun-12, the-11)
ccomp(thought-3, sun-12)
nsubj(argument-18, that-15)
cop(argument-18, is-16)
det(argument-18, another-17)
conj_but(thought-3, argument-18)

