(ROOT
  (S
    (S
      (VP (VBG Silly)
        (NP (PRP me))))
    (, ,)
    (NP (PRP I))
    (VP (VBD thought)
      (SBAR
        (S
          (NP (DT those) (CD two) (NNS categories))
          (VP (VBD were)
            (ADJP (RB rather) (JJ all-encompassing))))))
    (. .)))

partmod(thought-5, Silly-1)
dobj(Silly-1, me-2)
