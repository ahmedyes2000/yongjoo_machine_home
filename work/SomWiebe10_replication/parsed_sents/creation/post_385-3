(ROOT
  (S ('' '')
    (PP (IN If)
      (NP
        (NP (PRP$ your) (NN science))
        (SBAR
          (S
            (VP (VBZ has)
              (NP
                (NP (DT no) (NN room))
                (PP (IN for)
                  (NP (NNP God))))
              (PP (RB then)
                (NP (PRP$ your) (NN science))))))))
    (VP (VBZ is)
      (ADJP (RB too) (JJ small)))
    (. .)))

poss(science-4, your-3)
prep_if(small-15, science-4)
rcmod(science-4, has-5)
det(room-7, no-6)
dobj(has-5, room-7)
prep_for(room-7, God-9)
prep(has-5, then-10)
poss(science-12, your-11)
pobj(then-10, science-12)
cop(small-15, is-13)
advmod(small-15, too-14)
root(ROOT-0, small-15)

