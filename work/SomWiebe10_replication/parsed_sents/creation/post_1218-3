(ROOT
  (S (CC But)
    (NP (DT the) (NNP Bible))
    (VP (VBD was)
      (VP (VBN written)
        (S
          (VP (TO to)
            (VP (VB tell)
              (NP (NNS people))
              (SBAR
                (SBAR
                  (WHADVP (WRB how))
                  (S
                    (VP (TO to)
                      (VP (VB live)
                        (NP (DT a) (JJR better) (NN life))
                        (, ,)
                        (ADVP (RB here))))))
                (CC and)
                (SBAR
                  (WHADVP (WRB how))
                  (S
                    (VP (TO to)
                      (VP (VB fit)
                        (PRT (RP in))
                        (, ,)
                        (PP (IN in)
                          (NP (NN heaven)))))))))))))
    (. .)))

cc(written-5, But-1)
det(Bible-3, the-2)
nsubjpass(written-5, Bible-3)
xsubj(tell-7, Bible-3)
auxpass(written-5, was-4)
root(ROOT-0, written-5)
aux(tell-7, to-6)
xcomp(written-5, tell-7)
