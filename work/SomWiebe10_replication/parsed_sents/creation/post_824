(ROOT
  (S
    (NP (CD One) (NN day))
    (NP (PRP we) (DT all))
    (VP (MD will)
      (VP (VB know)
        (NP (DT the) (NN truth))))
    (. !)))

num(day-2, One-1)
tmod(know-6, day-2)
nsubj(know-6, we-3)
det(we-3, all-4)
aux(know-6, will-5)
root(ROOT-0, know-6)
det(truth-8, the-7)
dobj(know-6, truth-8)

