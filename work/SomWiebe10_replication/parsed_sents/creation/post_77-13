(ROOT
  (S
    (NP (NNP ID) (NNS scientists))
    (VP (VBP think)
      (SBAR
        (S
          (NP (PRP we))
          (VP (MD should) (RB not)))))
    (. .)))

nn(scientists-2, ID-1)
nsubj(think-3, scientists-2)
root(ROOT-0, think-3)
nsubj(should-5, we-4)
ccomp(think-3, should-5)
neg(should-5, not-6)

