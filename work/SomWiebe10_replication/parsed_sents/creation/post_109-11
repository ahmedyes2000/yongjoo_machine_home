    (S
      (SBAR (IN though)
        (S
          (NP (PRP I))
          (VP (VBP think)
            (NP (DT the) (JJ dead) (NN sea)))))
      (NP (NNS scrolls))
      (VP (VBP show)
        (ADVP (RB otherwise))))
    (CC and)
    (S
      (NP (PRP I))
      (VP (VBP believe)
        (SBAR
          (S
            (NP
              (NP (DT the) (NNP Bible) (POS 's))
