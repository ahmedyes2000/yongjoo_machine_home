(ROOT
  (SQ (VBZ Has)
    (NP (PRP he))
    (VP (VBN admitted)
      (NP
        (NP (DT the) (NN impact))
        (PP (IN of)
          (NP (JJ such) (NN falsification))))
      (PP (IN on)
        (NP (NNP ID))))
    (. ?)))

aux(admitted-3, Has-1)
