(ROOT
  (ADJP (JJ Free)
    (S
      (VP (TO to) (-LRB- -LRB-)
        (VP (VB follow)
          (NP
            (NP (DT the) (NN evidence))
            (, ,)
            (SBAR
              (WHADVP (WRB where))
              (S
                (ADVP (RB ever))
                (NP (PRP it))
                (VP (VBZ leads))))
            (, ,)
            (CC and)
            (SBAR
              (WHPP (TO to)
                (WHNP (WP what)))
              (FRAG
                (ADVP (RB ever))
                (NP
                  (NP (NNS conclusions))
                  (, ,)
                  (VP (VBN bringsmore)
                    (NP (NN enlightenment))))
                (. .)))))
        (-RRB- -RRB-)))))

root(ROOT-0, Free-1)
aux(follow-4, to-2)
xcomp(Free-1, follow-4)
det(evidence-6, the-5)
