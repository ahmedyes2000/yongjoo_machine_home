(ROOT
  (S
    (NP (PRP They))
    (VP (MD would)
      (VP (VB have)
        (S
          (VP (TO to)
            (VP (VB have)
              (VP (VBN been)
                (VP (VBN covered)
                  (PP (IN for)
                    (NP
                      (NP (NNS thousands))
                      (PP (IN of)
                        (NP (NNS years))))))))))))
    (. .)))

nsubj(have-3, They-1)
xsubj(covered-7, They-1)
aux(have-3, would-2)
root(ROOT-0, have-3)
aux(covered-7, to-4)
aux(covered-7, have-5)
auxpass(covered-7, been-6)
xcomp(have-3, covered-7)
prep_for(covered-7, thousands-9)
prep_of(thousands-9, years-11)

