(ROOT
  (S
    (SBAR (RB Even) (IN if)
      (S
        (NP (PRP you))
        (VP (VBP are)
          (ADJP (JJ correct)))))
    (, ,)
    (NP (NNS men))
    (VP (MD can)
      (VP (VB lie)))
    (. .)))

advmod(correct-5, Even-1)
dep(correct-5, if-2)
nsubj(correct-5, you-3)
cop(correct-5, are-4)
advcl(lie-9, correct-5)
nsubj(lie-9, men-7)
aux(lie-9, can-8)
root(ROOT-0, lie-9)

