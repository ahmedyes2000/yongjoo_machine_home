(ROOT
  (S
    (SBAR
      (WHADVP (WRB When))
      (S
        (NP (PRP you))
        (VP
          (VP (VB get)
            (PP (IN in)
              (NP (DT a) (NN car))))
          (CC and)
          (VP (VB turn)
            (NP (DT the) (NN key))))))
    (NP (PRP you))
    (VP (VBP have)
      (S
        (NP (DT a) (JJ spontaneous) (JJ violent) (NN storm))
        (VP (VB erupt)
          (PP (IN inside)
            (NP (DT the) (NN engine))))))
    (. .)))

advmod(get-3, When-1)
nsubj(get-3, you-2)
nsubj(turn-8, you-2)
advcl(have-12, get-3)
det(car-6, a-5)
prep_in(get-3, car-6)
conj_and(get-3, turn-8)
advcl(have-12, turn-8)
det(key-10, the-9)
dobj(turn-8, key-10)
nsubj(have-12, you-11)
root(ROOT-0, have-12)
det(storm-16, a-13)
amod(storm-16, spontaneous-14)
amod(storm-16, violent-15)
nsubj(erupt-17, storm-16)
ccomp(have-12, erupt-17)
det(engine-20, the-19)
prep_inside(erupt-17, engine-20)

