(ROOT
  (S
    (INTJ (UH No))
    (, ,)
    (NP (IN but) (DT the) (NN point))
    (VP (VBZ remains)
      (NP (DT the) (JJ same)))
    (. .)))

dep(same-8, No-1)
amod(point-5, but-3)
det(point-5, the-4)
nsubj(same-8, point-5)
cop(same-8, remains-6)
det(same-8, the-7)
root(ROOT-0, same-8)

(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ 's)
      (NP
        (NP (NN denial))
        (PP (IN of)
          (NP (NN something)))
        (SBAR
          (WHNP (WDT that))
          (S
            (VP (VBZ 's)
              (VP (VBG staring)
                (NP (PRP you))
                (PP (IN in)
                  (NP (DT the) (NN face)))))))))
    (. .)))

nsubj(denial-3, It-1)
cop(denial-3, 's-2)
root(ROOT-0, denial-3)
nsubj(staring-8, denial-3)
prep_of(denial-3, something-5)
aux(staring-8, 's-7)
rcmod(denial-3, staring-8)
dobj(staring-8, you-9)
det(face-12, the-11)
prep_in(staring-8, face-12)

