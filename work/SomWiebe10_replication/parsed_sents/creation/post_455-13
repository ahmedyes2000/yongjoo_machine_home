(ROOT
  (S
    (NP (DT These))
    (VP (VBP are)
      (NP
        (NP (PRP$ my) (NNS predictions))
        (VP (VBG using)
          (NP (JJ self-verification) (NN theory)))))
    (. .)))

nsubj(predictions-4, These-1)
cop(predictions-4, are-2)
poss(predictions-4, my-3)
root(ROOT-0, predictions-4)
partmod(predictions-4, using-5)
amod(theory-7, self-verification-6)
dobj(using-5, theory-7)

