(ROOT
  (S ('' '')
    (NP (WP What))
    (ADVP (JJ other))
    (VP (VBZ records)
      (SBAR
        (S
          (VP (VBP are)
            (NP
              (NP (RB there))
              (SBAR
                (WHNP (WDT that))
                (S
                  (VP (VBP are)
                    (ADJP
                      (NP
                        (QP (CD six) (CD thousand))
                        (NNS years))
                      (JJ old))
                    (PP (IN besides)
                      (NP (DT the) (NNP Bible)))))))))))
    (. .)))

nsubj(records-4, What-2)
advmod(records-4, other-3)
root(ROOT-0, records-4)
cop(there-6, are-5)
ccomp(records-4, there-6)
nsubj(old-12, there-6)
cop(old-12, are-8)
number(thousand-10, six-9)
num(years-11, thousand-10)
npadvmod(old-12, years-11)
rcmod(there-6, old-12)
det(Bible-15, the-14)
prep_besides(old-12, Bible-15)

