(ROOT
  (S
    (NP (NNP Science))
    (VP (VBZ does) (RB not)
      (VP (VB end)
        (PP (IN with)
          (NP
            (NP (DT an) (NN inference))
            (PP (IN of)
              (NP (NN design)))))
        (, ,)
        (ADVP (RB ever))))
    (. .)))

nsubj(end-4, Science-1)
aux(end-4, does-2)
neg(end-4, not-3)
root(ROOT-0, end-4)
det(inference-7, an-6)
prep_with(end-4, inference-7)
prep_of(inference-7, design-9)
advmod(end-4, ever-11)

