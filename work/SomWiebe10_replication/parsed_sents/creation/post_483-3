(ROOT
  (S
    (NP (PRP He))
    (VP (VBD was)
      (ADJP
        (NP (DT a) (RB bit))
        (JJ allegorical)
        (PP (IN in)
          (NP
            (NP (PRP$ his) (NN interpretation))
            (PP (IN of)
              (NP (NNP Scripture)))))))
    (. .)))

nsubj(allegorical-5, He-1)
cop(allegorical-5, was-2)
det(bit-4, a-3)
npadvmod(allegorical-5, bit-4)
root(ROOT-0, allegorical-5)
poss(interpretation-8, his-7)
prep_in(allegorical-5, interpretation-8)
prep_of(interpretation-8, Scripture-10)

