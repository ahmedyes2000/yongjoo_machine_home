(ROOT
  (S
    (PP (IN In)
      (NP (JJ other) (NNS words)))
    (, ,)
    (NP (NNP IC))
    (VP (VBZ is)
      (ADVP (RB basically))
      (NP (DT the)
        (ADJP (RB much) (JJR narrower))
        (NN claim))
      (SBAR (IN that)
        (S
          (NP (JJ certain) (NNS systems))
          (VP (MD could) (RB not)
            (ADVP (RB possibly))
            (VP (VB have)
              (VP (VBN evolved)
                (PP (IN by)
                  (NP (JJ Darwinian) (NNS processes)))
                (PP (IN while)
                  (S
                    (VP (VBG maintaining)
                      (NP (PRP$ their) (JJ original) (NN function)))))))))))
    (. .)))

