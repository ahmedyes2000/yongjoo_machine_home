(ROOT
  (S
    (NP (DT The) (NN statute))
    (VP (MD must)
      (VP (VB have)
        (NP (DT a) (NN secular) (NN purpose) (CD .2))))
    (. .)))

det(statute-2, The-1)
nsubj(have-4, statute-2)
aux(have-4, must-3)
root(ROOT-0, have-4)
det(purpose-7, a-5)
nn(purpose-7, secular-6)
dobj(have-4, purpose-7)
num(purpose-7, .2-8)

