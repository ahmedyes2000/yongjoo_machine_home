(ROOT
  (S
    (NP (PRP You))
    (VP (VBP are)
      (PP (IN under)
        (NP (DT the) (JJ mistaken) (NN impression)))
      (SBAR (IN that)
        (S
          (NP (NN complexity)
            (CC and)
            (NN ability))
          (VP (TO to)
            (VP (VB buy)
              (S
                (VP (VBG washing)
                  (SBAR
                    (S
                      (NP (NNS machines))
                      (VP (VBZ equals)
                        (ADJP (RBR more) (JJ evolved))))))))))))
    (. .)))

nsubj(are-2, You-1)
root(ROOT-0, are-2)
det(impression-6, the-4)
amod(impression-6, mistaken-5)
prep_under(are-2, impression-6)
complm(buy-12, that-7)
nsubj(buy-12, complexity-8)
