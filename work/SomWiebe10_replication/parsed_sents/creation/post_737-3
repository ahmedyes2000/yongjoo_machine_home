  (S
    (PP (IN In)
      (NP (JJ ancient) (NNP Hebrew)))
    (, ,)
    (NP (DT the) (NN word) (NN yom))
    (VP (MD can)
      (VP (VB mean)
        (NP
          (NP
            (NP (NN day))
            (, ,)
            (ADVP (RB as)
              (PP (IN in)
                (NP
                  (QP (CD 12)
                    (CC or)
                    (CD 24))
                  (NN hour) (NNS periods)))))
          (, ,)
          (CC or)
          (RB simply)
          (NP
            (NP (NN period))
            (PP (IN of)
              (NP (NN time)))))))
    (. .)))

amod(Hebrew-3, ancient-2)
