(ROOT
  (S
    (NP (PRP They))
    (VP (VBP understand)
      (NP
        (NP (DT the) (NNS issues))
        (CONJP (RB as) (RB well) (IN as))
        (NP (PRP$ their) (NNS opponents))))
    (. .)))

nsubj(understand-2, They-1)
root(ROOT-0, understand-2)
det(issues-4, the-3)
dobj(understand-2, issues-4)
poss(opponents-9, their-8)
dobj(understand-2, opponents-9)
conj_and(issues-4, opponents-9)

