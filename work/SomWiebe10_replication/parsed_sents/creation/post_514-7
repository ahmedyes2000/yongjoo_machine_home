(ROOT
  (S
    (NP (PRP I))
    (VP (VBP guess)
      (SBAR
        (S
          (NP (DT the) (NNS AUSCS))
          (VP (VBZ thinks)
            (SBAR
              (S
                (NP (NNP Dawkins))
                (VP (VBZ is)
                  (NP (DT a) (NN fool)))))))))
    (. .)))

nsubj(guess-2, I-1)
root(ROOT-0, guess-2)
det(AUSCS-4, the-3)
nsubj(thinks-5, AUSCS-4)
ccomp(guess-2, thinks-5)
nsubj(fool-9, Dawkins-6)
cop(fool-9, is-7)
det(fool-9, a-8)
ccomp(thinks-5, fool-9)

