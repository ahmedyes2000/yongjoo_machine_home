(ROOT
  (S
    (S
      (NP (PRP You))
      (VP (VBP say)
        (SBAR
          (S
            (VP (VBZ is)
              (S
                (NP (PRP it))
                (`` ``)
                (ADJP (RB extremely) (JJ unlikely))
                ('' '')))))))
    (: ...)
    (FRAG
      (NP (NN no.))
      (. .))
    (. .)))

nsubj(say-2, You-1)
root(ROOT-0, say-2)
ccomp(say-2, is-3)
dep(unlikely-7, it-4)
advmod(unlikely-7, extremely-6)
dep(is-3, unlikely-7)
dep(say-2, no.-10)

(ROOT
  (S
    (NP (DT that))
    (VP (VBZ is)
      (VP (VBG putting)
        (NP (PRP it))
        (ADVP (RB ungodly))
        (ADVP (RB lightly))))
    (. .)))

nsubj(putting-3, that-1)
aux(putting-3, is-2)
root(ROOT-0, putting-3)
dobj(putting-3, it-4)
advmod(putting-3, ungodly-5)
advmod(putting-3, lightly-6)

