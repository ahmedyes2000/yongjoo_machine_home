(ROOT
  (S
    (S
      (NP (PRP I))
      (ADVP (RB surely))
      (VP (VBP intend)
        (S
          (VP (TO to)
            (VP (VB do)
              (ADVP (RB so)))))))
    (, ,)
    (CC but)
    (S
      (NP (DT these) (NNS issues))
      (VP (VBP go)
        (PP (IN beyond)
          (NP
            (NP (NNS questions))
            (PP (IN of)
              (NP (NN policy)))))))
    (. .)))

nsubj(intend-3, I-1)
xsubj(do-5, I-1)
advmod(intend-3, surely-2)
root(ROOT-0, intend-3)
aux(do-5, to-4)
xcomp(intend-3, do-5)
advmod(do-5, so-6)
det(issues-10, these-9)
nsubj(go-11, issues-10)
conj_but(intend-3, go-11)
prep_beyond(go-11, questions-13)
prep_of(questions-13, policy-15)

