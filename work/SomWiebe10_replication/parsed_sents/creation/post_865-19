(ROOT
  (S (RB So)
    (ADVP (RB therefore))
    (, ,)
    (NP (EX there))
    (VP (VBD was)
      (NP
        (NP (DT no) (NN problem))
        (PP (IN for)
          (NP (NNP Cain)))
        (S
          (VP (TO to)
            (VP (VB find)
              (NP (DT a) (NN wife)))))))
    (. .)))

advmod(was-5, So-1)
advmod(was-5, therefore-2)
expl(was-5, there-4)
root(ROOT-0, was-5)
det(problem-7, no-6)
nsubj(was-5, problem-7)
prep_for(problem-7, Cain-9)
aux(find-11, to-10)
infmod(problem-7, find-11)
det(wife-13, a-12)
dobj(find-11, wife-13)

