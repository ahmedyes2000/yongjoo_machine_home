(ROOT
  (S
    (NP (PRP$ His) (NN mind))
    (VP (VBD was)
      (VP (VBN made)
        (PRT (RP up))
        (SBAR (IN before)
          (S
            (NP (PRP he))
            (ADVP (RB even))
            (VP (VBD heard)
              (NP (DT the) (NN case)))))))
    (. .)))

poss(mind-2, His-1)
nsubjpass(made-4, mind-2)
auxpass(made-4, was-3)
root(ROOT-0, made-4)
prt(made-4, up-5)
mark(heard-9, before-6)
nsubj(heard-9, he-7)
advmod(heard-9, even-8)
advcl(made-4, heard-9)
det(case-11, the-10)
dobj(heard-9, case-11)

