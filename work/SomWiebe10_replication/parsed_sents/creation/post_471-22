(ROOT
  (S
    (NP (EX There))
    (VP (VBP are)
      (NP
        (NP (DT a) (NN lot))
        (PP (IN of)
          (NP
            (ADJP (JJ inconsistent))
            (NNS Christians))))
      (ADVP (RB around)))
    (. .)))

expl(are-2, There-1)
root(ROOT-0, are-2)
det(lot-4, a-3)
nsubj(are-2, lot-4)
amod(Christians-7, inconsistent-6)
prep_of(lot-4, Christians-7)
advmod(are-2, around-8)

