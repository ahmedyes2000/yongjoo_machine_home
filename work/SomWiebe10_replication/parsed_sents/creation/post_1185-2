(ROOT
  (S
    (NP
      (NP (DT Those))
      (PP (IN of)
        (NP (NN science))))
    (VP (MD can) (RB not)
      (VP (VB answer)
        (NP
          (NP (DT the) (NN question))
          (PP (IN of)
            (SBAR
              (WHADVP (WRB why))
              (S
                (NP (PRP we))
                (VP (VBP are)
                  (ADVP (RB here)))))))))
    (. .)))

nsubj(answer-6, Those-1)
prep_of(Those-1, science-3)
aux(answer-6, can-4)
neg(answer-6, not-5)
root(ROOT-0, answer-6)
det(question-8, the-7)
