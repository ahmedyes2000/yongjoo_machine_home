(ROOT
  (S
    (NP
      (NP (NN Everything))
      (PP (IN in)
        (NP (DT the) (NN universe))))
    (VP (VBZ has)
      (NP (DT the) (JJ same) (NN situation)))
    (. .) (. .)))

nsubj(has-5, Everything-1)
det(universe-4, the-3)
prep_in(Everything-1, universe-4)
root(ROOT-0, has-5)
det(situation-8, the-6)
amod(situation-8, same-7)
