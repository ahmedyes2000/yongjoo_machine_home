(ROOT
  (S ('' '')
    (NP (PRP You))
    (VP (VBP are)
      (VP (VBG trying)
        (S
          (VP (TO to)
            (VP (VB prove)
              (NP (DT a) (JJ negative)))))))
    (. .)))

nsubj(trying-4, You-2)
xsubj(prove-6, You-2)
aux(trying-4, are-3)
root(ROOT-0, trying-4)
aux(prove-6, to-5)
xcomp(trying-4, prove-6)
det(negative-8, a-7)
dobj(prove-6, negative-8)

