  (S ('' '')
    (NP (PRP We))
    (VP (VBP are) (RB not)
      (ADVP (RB quite)
        (PP (IN at)
          (NP (DT the)
            (NP (`` ``)
              (NP (NNS billions)
                (CC and)
                (NNS billions))
              ('' '')
              (PP (IN of)
                (NP (NNS years))))
            (NN point))))
      (: ...)
      (SBAR (RB just) (IN as)
        (S
          (S
            (VP (`` ``) (VB hit)
              (CC or)
              (VB miss)
              (NP (NN chance))
              ('' '')))
          (VP (VBZ is) (RB not)
            (NP
              (NP (DT an)
                (ADJP (JJ accurate))
                (NN description))
              (PP (IN of)
                (NP (NN evolution))))))))
    (. .)))

