(ROOT
  (S
    (NP (DT The) (NN evidence))
    (VP (VBZ is)
      (ADVP (RB everywhere))
      (NP
        (NP (PRP$ its)
          (ADJP (RB just) (DT a))
          (NN matter))
        (PP (IN of)
          (SBAR
            (WHADVP (WRB where))
            (S
              (NP (PRP you))
              (VP (VBP want)
                (S
                  (VP (TO to)
                    (VP (VB put)
                      (NP (PRP$ your) (NN faith)))))))))))
    (. .)))

det(evidence-2, The-1)
nsubj(matter-8, evidence-2)
cop(matter-8, is-3)
advmod(matter-8, everywhere-4)
poss(matter-8, its-5)
advmod(a-7, just-6)
amod(matter-8, a-7)
root(ROOT-0, matter-8)
advmod(want-12, where-10)
nsubj(want-12, you-11)
xsubj(put-14, you-11)
prepc_of(matter-8, want-12)
aux(put-14, to-13)
xcomp(want-12, put-14)
poss(faith-16, your-15)
dobj(put-14, faith-16)

