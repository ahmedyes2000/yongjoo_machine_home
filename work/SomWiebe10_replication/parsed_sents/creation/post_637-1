(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP (MD can)
          (VP (VB find)
            (NP
              (NP (NNS passages))
              (SBAR
                (WHPP (IN in)
                  (WHNP (WDT which)))
                (S
                  (NP
                    (NP (NN god))
                    (CC or)
                    (NP (DT the) (NN supernatural)))
                  (VP (VBZ is)
                    (ADVP (RB directly))
                    (VP (VBN implicated))))))))))
    (, ,)
    (NP (PRP I))
    (VP (MD would)
      (VP (VB change)
        (NP (PRP$ my) (NN mind))))
    (. .)))

