(ROOT
  (S
    (ADVP (RB AFAIK))
    (, ,)
    (NP
      (NP (DT the) (NNS languages))
      (PP (IN of)
        (NP (NNP Genesis))))
    (VP (VBP are)
      (NP (NNP Hebrew)
        (CC and)
        (NNP Aramaic)))
    (. .)))

