(ROOT
  (S
    (NP (NN Mutation)
      (CC and)
      (NN evolution))
    (VP (VBP are)
      (NP (CD two) (JJ different) (NNS things)))
    (. .)))

nsubj(things-7, Mutation-1)
conj_and(Mutation-1, evolution-3)
nsubj(things-7, evolution-3)
cop(things-7, are-4)
num(things-7, two-5)
amod(things-7, different-6)
root(ROOT-0, things-7)

