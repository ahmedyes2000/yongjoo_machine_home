            (NP (NNP Buckingham))
            (VP (VBD had)
              (NP (JJ religious) (NNS motives)))))
        (, ,)
        (CC and)
        (SBAR (IN that)
          (S
            (NP (PRP he))
            (VP (VBD had)
              (NP
                (NP (DT a) (NN memory) (NN slip))
                (PP (VBG regarding)
                  (NP
                    (NP (NN funding))
                    (PP (IN for)
                      (NP (DT the) (NNS books)))
                    (, ,)
                    (X (NN etc.))))))))))
    (. .)))

