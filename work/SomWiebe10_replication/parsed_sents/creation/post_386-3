(ROOT
  (S (IN So)
    (NP (DT that))
    (VP (VBZ is)
      (VP (VBN why) (, ,)
        (PP (IN in)
          (NP (JJ many) (NNS peoples) (NNS opinions)))
        (, ,)
        (PP (VBG including)
          (NP (PRP$ my) (JJ own)))
        (, ,)
        (SBAR (IN that)
          (S
            (NP (PRP we))
            (VP (VBP have)
              (NP (DT this) (CD :1)))))))
    (. .)))

dep(why-4, So-1)
nsubjpass(why-4, that-2)
auxpass(why-4, is-3)
root(ROOT-0, why-4)
amod(opinions-9, many-7)
nn(opinions-9, peoples-8)
prep_in(why-4, opinions-9)
poss(own-13, my-12)
prep_including(why-4, own-13)
complm(have-17, that-15)
nsubj(have-17, we-16)
ccomp(why-4, have-17)
det(:1-19, this-18)
dobj(have-17, :1-19)

