(ROOT
  (S
    (NP (PRP I))
    (VP (VBP think)
      (SBAR (IN that)
        (S
          (NP (PRP$ their) (NN argument))
          (VP (MD may)
            (VP (VB be)
              (ADJP (RBR better) (JJ phrased)
                (PP (IN as) ('' '')
                  (NP
                    (NP
                      (NP (NNP ID) (POS 's))
                      (JJ religious) (NN motivation))
                    (SBAR
                      (S
                        (VP (VBZ helps)
                          (VP (VB explain)
                            (SBAR
                              (WHADVP (WRB why))
                              (S
                                (NP (PRP it))
                                (VP (VBZ has)
                                  (VP (VBN remained)
                                    (NP (JJ scientifically) (NNS vacuous) ('' ''))))))))))))))))))
    (. .)))

nsubj(think-2, I-1)
root(ROOT-0, think-2)
complm(phrased-9, that-3)
poss(argument-5, their-4)
nsubj(phrased-9, argument-5)
aux(phrased-9, may-6)
cop(phrased-9, be-7)
advmod(phrased-9, better-8)
ccomp(think-2, phrased-9)
poss(motivation-15, ID-12)
amod(motivation-15, religious-14)
prep_as(phrased-9, motivation-15)
rcmod(motivation-15, helps-16)
dep(helps-16, explain-17)
advmod(vacuous-23, why-18)
nsubj(vacuous-23, it-19)
aux(vacuous-23, has-20)
cop(vacuous-23, remained-21)
amod(vacuous-23, scientifically-22)
advcl(explain-17, vacuous-23)

