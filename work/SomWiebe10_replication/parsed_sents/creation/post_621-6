(ROOT
  (S
    (ADVP (RB Controversy))
    (, ,)
    (ADVP (RB however))
    (, ,)
    (VP (VBZ is)
      (ADJP (JJ good)
        (PP (IN for)
          (NP (NN science)))))
    (. .)))

advmod(good-6, Controversy-1)
advmod(good-6, however-3)
cop(good-6, is-5)
root(ROOT-0, good-6)
prep_for(good-6, science-8)

