(ROOT
  (S
    (NP (PRP I))
    (VP (VBP wonder)
      (SBAR (IN if)
        (S
          (NP (PRP he))
          (VP (MD will)
            (ADVP (RB ever))
            (VP (VB get)
              (PRT (RP out)))))))
    (. .)))

nsubj(wonder-2, I-1)
root(ROOT-0, wonder-2)
mark(get-7, if-3)
nsubj(get-7, he-4)
aux(get-7, will-5)
advmod(get-7, ever-6)
advcl(wonder-2, get-7)
prt(get-7, out-8)

