(ROOT
  (S
    (NP (PRP I))
    (VP (VBP 'm)
      (ADJP (JJ sure)
        (SBAR
          (S
            (NP (PRP I))
            (VP (MD wo) (RB n't)
              (VP (VB get)
                (VP (VBN fired)
                  (PP (IN for)
                    (S
                      (VP (VBG telling)
                        (S
                          (NP (PRP you))
                          (VP (TO to)
                            (VP (VB do)
                              (NP (DT this)))))))))))))))
    (: ...)))

nsubj(sure-3, I-1)
cop(sure-3, 'm-2)
root(ROOT-0, sure-3)
nsubjpass(fired-8, I-4)
aux(fired-8, wo-5)
neg(fired-8, n't-6)
auxpass(fired-8, get-7)
dep(sure-3, fired-8)
prepc_for(fired-8, telling-10)
nsubj(do-13, you-11)
aux(do-13, to-12)
xcomp(telling-10, do-13)
dobj(do-13, this-14)

