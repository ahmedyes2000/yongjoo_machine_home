(ROOT
  (S
    (NP (DT The) (NN statute))
    (VP (MD must) (RB not)
      (VP (VB have)
        (NP
          (NP (DT the) (JJ primary) (NN purpose))
          (PP (IN of)
            (S
              (VP (VBG advancing)
                (CC or)
                (VBG inhibiting)
                (NP (DT any) (NN religion) (CD .3))))))))
    (. .)))

det(statute-2, The-1)
nsubj(have-5, statute-2)
aux(have-5, must-3)
neg(have-5, not-4)
root(ROOT-0, have-5)
det(purpose-8, the-6)
amod(purpose-8, primary-7)
dobj(have-5, purpose-8)
prepc_of(purpose-8, advancing-10)
prepc_of(purpose-8, inhibiting-12)
