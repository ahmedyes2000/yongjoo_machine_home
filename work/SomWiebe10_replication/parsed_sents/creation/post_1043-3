(ROOT
  (S
    (S
      (ADVP (RB Also))
      (, ,)
      (NP (PRP you))
      (VP (VBP claim)
        (SBAR (IN that)
          (S
            (NP (PRP we))
            (VP (VBP have)
              (ADVP (RB never))
              (VP (VBN observed)
                (S
                  (NP (NNS organisms))
                  (ADJP (VBG evolving)))))))))
    (: :)
    (S
      (NP (DT this))
      (VP (VBZ is)
        (ADJP (RB completely) (JJ untrue))))
    (. .)))

advmod(claim-4, Also-1)
nsubj(claim-4, you-3)
root(ROOT-0, claim-4)
complm(observed-9, that-5)
nsubj(observed-9, we-6)
aux(observed-9, have-7)
neg(observed-9, never-8)
ccomp(claim-4, observed-9)
nsubj(evolving-11, organisms-10)
xcomp(observed-9, evolving-11)
nsubj(untrue-16, this-13)
cop(untrue-16, is-14)
advmod(untrue-16, completely-15)
parataxis(claim-4, untrue-16)

