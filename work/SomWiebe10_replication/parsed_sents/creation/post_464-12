(ROOT
  (S
    (ADVP (RB Apparently))
    (NP (DT all))
    (VP (VBZ is)
      (ADJP (JJ fair)
        (PP (IN in)
          (NP (DT the) (JJ evolution\/ID) (NN war)))))
    (. .)))

advmod(fair-4, Apparently-1)
nsubj(fair-4, all-2)
cop(fair-4, is-3)
root(ROOT-0, fair-4)
det(war-8, the-6)
amod(war-8, evolution\/ID-7)
prep_in(fair-4, war-8)

