(ROOT
  (S
    (NP (PRP$ Your) (NN opinion))
    (VP (VBZ is)
      (SBAR (IN that)
        (S
          (NP (PRP it))
          (VP (VBZ works)
            (ADVP (RB so) (RB well))))))
    (. .)))

poss(opinion-2, Your-1)
nsubj(is-3, opinion-2)
root(ROOT-0, is-3)
complm(works-6, that-4)
nsubj(works-6, it-5)
ccomp(is-3, works-6)
advmod(well-8, so-7)
advmod(works-6, well-8)

