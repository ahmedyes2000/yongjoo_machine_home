(ROOT
  (S
    (NP (DT This))
    (VP (VBZ is)
      (ADJP (RB very) (JJ natural)
        (PP (IN for)
          (NP (JJS most) (NNS people))))
      (SBAR (IN unless)
        (S
          (NP (PRP they))
          (VP (VBP have)
            (NP
              (NP (DT a) (NN problem))
              (PP (IN with)
                (NP (DT the) (JJ straight) (JJ forward) (NNS comments))))))))
    (. .)))

nsubj(natural-4, This-1)
cop(natural-4, is-2)
advmod(natural-4, very-3)
root(ROOT-0, natural-4)
amod(people-7, most-6)
prep_for(natural-4, people-7)
mark(have-10, unless-8)
nsubj(have-10, they-9)
advcl(natural-4, have-10)
det(problem-12, a-11)
dobj(have-10, problem-12)
det(comments-17, the-14)
amod(comments-17, straight-15)
amod(comments-17, forward-16)
prep_with(problem-12, comments-17)

