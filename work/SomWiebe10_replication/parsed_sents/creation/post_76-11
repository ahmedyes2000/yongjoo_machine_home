(ROOT
  (SBARQ (RB So)
    (WHNP (WP what))
    (SQ (VBP have)
      (NP (PRP we))
      (VP (VBN determined)
        (ADVP (RB so) (RB far))))
    (. ?)))

advmod(determined-5, So-1)
dobj(determined-5, what-2)
aux(determined-5, have-3)
nsubj(determined-5, we-4)
root(ROOT-0, determined-5)
advmod(far-7, so-6)
advmod(determined-5, far-7)

