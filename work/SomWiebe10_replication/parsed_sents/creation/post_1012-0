(ROOT
  (S
    (VP (VB Heck)
      (SBAR
        (S
          (NP (PRP$ his) (NN girlfriend))
          (VP (VBZ HAS)
            (NP
              (NP (JJR more) (NN hair))
              (SBAR (IN than)
                (S
                  (NP (PRP she))
                  (VP (VBZ has)
                    (NP (RB now))
                    (, ,)
                    (SBAR (RB even) (IN as)
                      (S
                        (NP (PRP we))
                        (VP (VBP speak))))))))))))
    (. .)))

root(ROOT-0, Heck-1)
poss(girlfriend-3, his-2)
nsubj(HAS-4, girlfriend-3)
ccomp(Heck-1, HAS-4)
amod(hair-6, more-5)
dobj(HAS-4, hair-6)
mark(has-9, than-7)
nsubj(has-9, she-8)
dep(hair-6, has-9)
dobj(has-9, now-10)
