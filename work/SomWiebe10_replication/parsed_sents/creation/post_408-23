  (S (CC And)
    (NP (EX there))
    (VP (VBZ is)
      (NP (JJ solid) (NN evidence))
      (SBAR (IN that)
        (S
          (NP
            (NP (DT a) (JJ huge) (NN variety))
            (PP (IN of)
              (NP
                (NP (JJ different) (NNS kinds))
                (PP (IN of)
                  (NP (VBG living) (NNS things))))))
          (VP
            (VP (VBP exist)
              (NP (NN today))
              (PP (IN on)
                (NP (DT this) (NN earth))))
            (CC and)
            (VP (VBP have)
              (VP (VBN existed)
                (PP (IN in)
                  (NP (DT the) (JJ past) (CD .2)))))))))
    (. .) (-RRB- -RRB-)))

