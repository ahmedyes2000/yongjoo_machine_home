(ROOT
  (S
    (NP (DT The) (NNP Human) (NN species))
    (VP (VBZ has)
      (NP
        (NP (DT a) (JJ huge) (NN variability))
        (PP (IN in)
          (NP
            (NP (JJ physical)
              (CC and)
              (JJ other) (NNS attributes))
            (PRN (, ,)
              (CC but)
              (ADJP (RB genetically)
                (SBAR
                  (S
                    (NP (PRP we))
                    (VP (VBP are) (RB all)
                      (NP (CD one) (NNS species)))))))))))
    (. .)))

