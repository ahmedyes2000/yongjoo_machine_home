(ROOT
  (S
    (NP (EX There))
    (VP (VBZ is)
      (NP
        (NP (DT no) (NN evidence))
        (, ,)
        (PP (IN in)
          (NP
            (NP (DT the) (JJ scientific) (NN sense))
            (, ,)
            (SBAR
              (WHNP (WDT that))
              (S
                (VP (VBZ supports)
                  (NP (PRP it)))))))))
    (. .)))

expl(is-2, There-1)
root(ROOT-0, is-2)
det(evidence-4, no-3)
nsubj(is-2, evidence-4)
det(sense-9, the-7)
amod(sense-9, scientific-8)
prep_in(evidence-4, sense-9)
nsubj(supports-12, sense-9)
rcmod(sense-9, supports-12)
dobj(supports-12, it-13)

