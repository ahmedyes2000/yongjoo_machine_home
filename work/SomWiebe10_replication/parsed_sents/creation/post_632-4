(ROOT
  (S ('' '')
    (NP (PRP You))
    (VP (MD can)
      (VP (VB see)
        (SBAR
          (WHADVP (WRB how) (RB quickly))
          (S
            (NP (PRP$ their) (NN argument))
            (VP (VBZ breaks)
              (PRT (RP down)))))))
    (. .)))

nsubj(see-4, You-2)
aux(see-4, can-3)
root(ROOT-0, see-4)
advmod(quickly-6, how-5)
advmod(breaks-9, quickly-6)
poss(argument-8, their-7)
nsubj(breaks-9, argument-8)
ccomp(see-4, breaks-9)
prt(breaks-9, down-10)

