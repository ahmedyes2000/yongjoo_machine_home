(ROOT
  (S
    (NP (DT The) (NN author))
    (VP (VBZ seems)
      (PP (TO to)
        (NP
          (NP (NN thing))
          (SBAR
            (S
              (NP (EX there))
              (VP (VBZ is)
                (NP
                  (NP (DT no) (JJ extra-biblical) (NN evidence))
                  (PP (IN of)
                    (NP (DT a) (JJ young) (NN earth))))))))))
    (. .)))

det(author-2, The-1)
nsubj(seems-3, author-2)
root(ROOT-0, seems-3)
prep_to(seems-3, thing-5)
expl(is-7, there-6)
rcmod(thing-5, is-7)
det(evidence-10, no-8)
amod(evidence-10, extra-biblical-9)
nsubj(is-7, evidence-10)
det(earth-14, a-12)
amod(earth-14, young-13)
prep_of(evidence-10, earth-14)

