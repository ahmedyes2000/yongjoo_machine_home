(ROOT
  (S
    (NP (PRP I))
    (VP (MD can)
      (VP (VB hear)
        (NP
          (NP (DT the) (NN cock) (NN crow))
          (NP (DT every) (NN day)))
        (SBAR (IN before)
          (S
            (NP (DT the) (NN sun))
            (VP (VBZ comes)
              (NP (CD up3)))))))
    (. .)))

nsubj(hear-3, I-1)
aux(hear-3, can-2)
root(ROOT-0, hear-3)
det(crow-6, the-4)
nn(crow-6, cock-5)
dobj(hear-3, crow-6)
det(day-8, every-7)
dep(crow-6, day-8)
mark(comes-12, before-9)
det(sun-11, the-10)
