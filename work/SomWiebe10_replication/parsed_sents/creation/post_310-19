(ROOT
  (S
    (NP (PRP We))
    (VP (MD can)
      (ADVP (RB only))
      (VP (VB thank)
        (NP
          (NP (NNP Newton))
          (PP (IN for)
            (NP (PRP$ his) (NNS contributions))))
        (PP (TO to)
          (NP (NN science)))
        (SBAR (IN while)
          (S
            (ADVP (RB also))
            (VP (VBG showing)
              (NP (PRP us))
              (SBAR
                (WHADVP (WRB why))
                (S
                  (NP (NNP ID))
                  (VP (VBZ is)
                    (ADJP (VBN doomed)
                      (S
                        (VP (TO to)
                          (VP (VB remain)
                            (NP (JJ scientifically) (NNS vacuous))))))))))))))
    (. .)))

nsubj(thank-4, We-1)
aux(thank-4, can-2)
advmod(thank-4, only-3)
root(ROOT-0, thank-4)
dobj(thank-4, Newton-5)
poss(contributions-8, his-7)
prep_for(Newton-5, contributions-8)
prep_to(thank-4, science-10)
mark(showing-13, while-11)
advmod(showing-13, also-12)
advcl(thank-4, showing-13)
dobj(showing-13, us-14)
advmod(doomed-18, why-15)
nsubjpass(doomed-18, ID-16)
auxpass(doomed-18, is-17)
advcl(showing-13, doomed-18)
aux(vacuous-22, to-19)
cop(vacuous-22, remain-20)
amod(vacuous-22, scientifically-21)
xcomp(doomed-18, vacuous-22)

