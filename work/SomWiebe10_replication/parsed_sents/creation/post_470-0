(ROOT
  (S
    (SBAR
      (PRN (: --)
        (NP (NNP TJ10))
        (: --))
      (S
        (NP (PRP It))
        (VP (VBZ is)
          (NP (DT the) (JJ scientific) (NN evidence)))))
    (NP (DT that))
    (VP (VBZ is)
      (ADJP (JJ important)))
    (. .)))

dep(evidence-8, TJ10-2)
nsubj(evidence-8, It-4)
cop(evidence-8, is-5)
det(evidence-8, the-6)
amod(evidence-8, scientific-7)
dep(important-11, evidence-8)
nsubj(important-11, that-9)
cop(important-11, is-10)
root(ROOT-0, important-11)

