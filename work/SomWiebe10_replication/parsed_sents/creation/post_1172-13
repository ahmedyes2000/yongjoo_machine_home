    (SBAR (IN If)
      (S
        (NP (PRP he))
        (VP (VBD had)
          (NP (NNS lungs)))))
    (, ,)
    (WHADVP (WRB why))
    (SQ (VBD did)
      (NP (PRP he))
      (VP (VB evolve)
        (NP (PRP them))
        (SBAR (IN while)
          (S
            (NP (PRP he))
            (VP (VBD was)
              (PP (IN in)
                (NP (DT the) (NN ocean))))))))
    (. ?)))

