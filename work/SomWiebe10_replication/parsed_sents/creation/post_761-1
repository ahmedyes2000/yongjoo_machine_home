(ROOT
  (S ('' '')
    (NP (NNP and) (NNP God))
    (VP (MD could)
      (VP (VB give)
        (NP (PRP$ his) (NN answer))
        (ADVP (RB directly))
        (PP (TO to)
          (NP
            (NP (NN anyone))
            (SBAR
              (WHNP (WP who))
              (S
                (VP (VBD asked))))))))
    (. .)))

nn(God-3, and-2)
nsubj(give-5, God-3)
aux(give-5, could-4)
root(ROOT-0, give-5)
poss(answer-7, his-6)
dobj(give-5, answer-7)
advmod(give-5, directly-8)
prep_to(give-5, anyone-10)
nsubj(asked-12, anyone-10)
rcmod(anyone-10, asked-12)

