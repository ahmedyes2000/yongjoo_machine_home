(ROOT
  (S
    (VP
      (ADVP (RB Once) (RB again))
      (VBG generalizing)
      (PP (IN on)
        (NP (DT an) (JJ unfair) (NN term))))
    (. .)))

advmod(again-2, Once-1)
advmod(generalizing-3, again-2)
root(ROOT-0, generalizing-3)
det(term-7, an-5)
amod(term-7, unfair-6)
prep_on(generalizing-3, term-7)

