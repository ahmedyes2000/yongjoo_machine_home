(ROOT
  (S ('' '')
    (NP (EX There))
    (VP (VBZ is)
      (NP
        (NP (DT a) (JJ massive) (NN amount))
        (PP (IN of)
          (NP
            (NP (NN evidence))
            (VP (VBG showing)
              (SBAR (IN that)
                (S
                  (NP (DT the) (NNP Egyptians))
                  (VP (VBD built)
                    (NP (DT the) (NNS pyramids))))))))))
    (. .)))

expl(is-3, There-2)
root(ROOT-0, is-3)
det(amount-6, a-4)
amod(amount-6, massive-5)
nsubj(is-3, amount-6)
prep_of(amount-6, evidence-8)
partmod(evidence-8, showing-9)
complm(built-13, that-10)
det(Egyptians-12, the-11)
nsubj(built-13, Egyptians-12)
ccomp(showing-9, built-13)
det(pyramids-15, the-14)
dobj(built-13, pyramids-15)

