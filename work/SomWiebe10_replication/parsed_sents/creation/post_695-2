(ROOT
  (S
    (NP (PRP They))
    (VP (MD may)
      (CC or)
      (MD may) (RB not)
      (VP (VB reflect)
        (NP (DT the) (NN truth))))
    (. .)))

nsubj(reflect-6, They-1)
aux(reflect-6, may-2)
aux(reflect-6, may-4)
conj_or(reflect-6, may-4)
neg(reflect-6, not-5)
root(ROOT-0, reflect-6)
det(truth-8, the-7)
dobj(reflect-6, truth-8)

