(ROOT
  (S (IN So)
    (NP (PRP you))
    (VP
      (VP (VBP choose)
        (NP (NN religion)))
      (CC and)
      (VP (VBP ignore)
        (NP (NN science))))
    (. .)))

dep(choose-3, So-1)
nsubj(choose-3, you-2)
nsubj(ignore-6, you-2)
root(ROOT-0, choose-3)
dobj(choose-3, religion-4)
conj_and(choose-3, ignore-6)
dobj(ignore-6, science-7)

