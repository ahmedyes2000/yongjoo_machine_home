(ROOT
  (S
    (NP (CD Two) (NNS Darwinists))
    (VP (VBD spent)
      (NP
        (NP (NNS hours))
        (VP (VBG standing)
          (PP (IN at)
            (NP
              (NP (DT a) (NN flag) (NN pole))
              (VP (VBG trying)
                (S
                  (VP (TO to)
                    (VP (VB find)
                      (NP
                        (NP (DT the) (NN height))
                        (PP (IN of)
                          (NP (PRP it)))))))))))))
    (. .)))

