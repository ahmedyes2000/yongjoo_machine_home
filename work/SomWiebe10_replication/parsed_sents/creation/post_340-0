(ROOT
  (S
    (NP
      (NP (NNP Science))
      (PP (IN of)
        (NP (NN course))))
    (VP (VBZ has)
      (VP (VBN tested)
        (NP (DT the) (NN principle))
        (, ,)
        (SBAR (RB even) (IN though)
          (S
            (PP (IN for)
              (NP (DT a) (JJ long) (NN time)))
            (NP (PRP it))
            (VP (VBD stood)
              (PP (IN as)
                (NP (DT an) (NN assumption))))))))
    (. .)))

nsubj(tested-5, Science-1)
prep_of(Science-1, course-3)
aux(tested-5, has-4)
root(ROOT-0, tested-5)
det(principle-7, the-6)
