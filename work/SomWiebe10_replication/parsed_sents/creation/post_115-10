(ROOT
  (FRAG
    (SBAR (IN Unless)
      (S
        (NP (PRP we))
        (VP (VBP view)
          (NP
            (NP (DT the) (NN environment))
            (PP (IN as)
              (NP (DT an) (JJ intelligent) (NN entity))))))
      (. .))
    (. .)))

mark(view-3, Unless-1)
nsubj(view-3, we-2)
root(ROOT-0, view-3)
det(environment-5, the-4)
dobj(view-3, environment-5)
det(entity-9, an-7)
amod(entity-9, intelligent-8)
prep_as(environment-5, entity-9)

