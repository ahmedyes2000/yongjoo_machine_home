(ROOT
  (S
    (PP (IN In)
      (NP (PRP$ my) (NN opinion)))
    (, ,)
    (S
      (NP (NNP ID))
      (VP (VBG being)
        (ADJP (JJ wrong))))
    (CC and)
    (S
      (VP (VBG knowing)
        (NP (PRP it))))
    (. .)))

poss(opinion-3, my-2)
prep_in(wrong-7, opinion-3)
nsubj(wrong-7, ID-5)
nsubj(knowing-9, ID-5)
cop(wrong-7, being-6)
root(ROOT-0, wrong-7)
conj_and(wrong-7, knowing-9)
dobj(knowing-9, it-10)

