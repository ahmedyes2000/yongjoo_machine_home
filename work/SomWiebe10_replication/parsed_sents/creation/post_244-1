(ROOT
  (S ('' '')
    (NP (JJ Other))
    (VP (VBZ inconvenient)
      (NP
        (NP (NNS observations))
        (SBAR
          (WHNP (WDT which))
          (S
            (VP (VBP bear)
              (NP (NN inspection))
              (: :)
              (UCP
                (NP (NNS purposefulness))
                (CC and)
                (ADVP (RB intentionality))))))))
    (. .)))

nsubj(inconvenient-3, Other-2)
root(ROOT-0, inconvenient-3)
dobj(inconvenient-3, observations-4)
nsubj(bear-6, observations-4)
rcmod(observations-4, bear-6)
dobj(bear-6, inspection-7)
dobj(bear-6, purposefulness-9)
dobj(bear-6, intentionality-11)
advmod(purposefulness-9, intentionality-11)
conj_and(purposefulness-9, intentionality-11)

