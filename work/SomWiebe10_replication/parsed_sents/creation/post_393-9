(ROOT
  (S
    (NP (PRP I))
    (VP (VBP agree) (, ,)
      (ADVP (RB as) (RB far))
      (PP (IN as)
        (NP
          (NP (DT the) (NN understanding))
          (PP (IN of)
            (NP
              (NP (JJ evolutionary) (NN theory))
              (SBAR
                (S
                  (VP (VBZ is)
                    (ADJP (JJ concerned))))))))))
    (. .)))

nsubj(agree-2, I-1)
root(ROOT-0, agree-2)
advmod(far-5, as-4)
advmod(agree-2, far-5)
det(understanding-8, the-7)
prep_as(agree-2, understanding-8)
amod(theory-11, evolutionary-10)
prep_of(understanding-8, theory-11)
cop(concerned-13, is-12)
rcmod(theory-11, concerned-13)

