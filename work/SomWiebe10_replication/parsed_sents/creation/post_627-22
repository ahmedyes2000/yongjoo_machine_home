(ROOT
  (S
    (NP (PRP I))
    (VP (VBP prefer)
      (S
        (VP (TO to)
          (VP (VB see)
            (NP
              (NP (NN reality))
              (, ,)
              (ADVP (CD metaphysical)
                (CC and)
                (CD physical))
              (ADJP (RB as) (VBN related)))))))
    (. .)))

nsubj(prefer-2, I-1)
xsubj(see-4, I-1)
root(ROOT-0, prefer-2)
