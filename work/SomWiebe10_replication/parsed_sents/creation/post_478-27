(ROOT
  (S
    (NP
      (NP (RB Much))
      (PP (IN of)
        (NP (DT the) (NN evidence))))
    (PRN
      (S
        (NP (PRP they))
        (VP (VBP claim)
          (SBAR
            (S
              (VP (VBZ supports)
                (NP (PRP it)))))))
      (, ,))
    (VP (VBZ is)
      (VP (VBN based)
        (PP (IN on)
          (NP
            (NP (DT this) (NN kind))
            (PP (IN of)
              (NP (JJ circular) (NN reasoning)))))))
    (. .)))

nsubjpass(based-11, Much-1)
det(evidence-4, the-3)
prep_of(Much-1, evidence-4)
