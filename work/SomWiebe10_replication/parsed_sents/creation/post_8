(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP we))
        (VP (VBP are)
          (VP (VBG descended)
            (PP (IN from)
              (NP (NNS animals)))
            (ADVP (RB then))))))
    (NP
      (NP (DT the) (JJ unique) (NN relationship))
      (PP (IN between)
        (NP (NN man)
          (CC and)
          (NN god))))
    (VP (VBZ is)
      (VP (VBN undermined)))
    (. .)))

mark(descended-4, If-1)
nsubj(descended-4, we-2)
aux(descended-4, are-3)
advcl(undermined-16, descended-4)
prep_from(descended-4, animals-6)
advmod(descended-4, then-7)
det(relationship-10, the-8)
amod(relationship-10, unique-9)
nsubjpass(undermined-16, relationship-10)
prep_between(relationship-10, man-12)
prep_between(relationship-10, god-14)
conj_and(man-12, god-14)
auxpass(undermined-16, is-15)
root(ROOT-0, undermined-16)

(ROOT
  (S
    (NP (EX There))
    (VP (VBZ is)
      (NP
        (NP (NN nothing)
          (S
            (VP (TO to)
              (VP (VB separate)
                (NP (PRP us))
                (PP (IN from)
                  (NP (DT the) (NNS beasts)))))))
        (, ,)
        (CC and)
        (NP
          (NP (DT no) (JJ moral) (NN basis))
          (PP (IN for)
            (S
              (VP
                (VP (VBG feeling)
                  (NP (JJ special)))
                (CC and)
                (VP (VBG acting)
                  (ADVP (RB differently))
                  (PP (IN from)
                    (NP (PRP them))))))))))
    (. .)))

expl(is-2, There-1)
root(ROOT-0, is-2)
nsubj(is-2, nothing-3)
aux(separate-5, to-4)
