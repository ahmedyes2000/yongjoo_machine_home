(ROOT
  (S
    (S
      (INTJ (UH No))
      (, ,)
      (NP (EX there))
      (VP (VBZ is) (RB n't)))
    (: ...)
    (S
      (ADVP (RB instead))
      (NP (PRP it))
      (VP (VBZ 's)
        (ADVP (RB just))
        (NP
          (NP (DT a) (NN straw) (NN man))
          (PP (IN by)
            (NP (NNS atheists))))))
    (. .)))

dep(is-4, No-1)
expl(is-4, there-3)
root(ROOT-0, is-4)
neg(is-4, n't-5)
advmod(man-13, instead-7)
nsubj(man-13, it-8)
cop(man-13, 's-9)
advmod(man-13, just-10)
det(man-13, a-11)
nn(man-13, straw-12)
parataxis(is-4, man-13)
prep_by(man-13, atheists-15)

