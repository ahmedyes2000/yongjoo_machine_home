(ROOT
  (S
    (NP (PRP They))
    (VP (VBP do)
      (SBAR (RB so) (IN because)
        (S
          (ADVP (RB invariably))
          (NP (PRP we))
          (VP (VBP know)
            (PP (IN from)
              (NP (NN experience)))
            (SBAR (IN that)
              (S
                (NP
                  (NP (JJ complex) (NNS systems))
                  (VP (VBG possessing)
                    (NP (JJ such) (NNS features))
                    (ADVP (RB always))))
                (VP (VBP arise)
                  (PP (IN from)
                    (NP (JJ intelligent) (NNS causes))))))))))
    (. .)))

nsubj(do-2, They-1)
root(ROOT-0, do-2)
advmod(know-7, so-3)
dep(know-7, because-4)
advmod(know-7, invariably-5)
nsubj(know-7, we-6)
ccomp(do-2, know-7)
prep_from(know-7, experience-9)
complm(arise-17, that-10)
amod(systems-12, complex-11)
nsubj(arise-17, systems-12)
partmod(systems-12, possessing-13)
amod(features-15, such-14)
dobj(possessing-13, features-15)
advmod(possessing-13, always-16)
ccomp(know-7, arise-17)
amod(causes-20, intelligent-19)
prep_from(arise-17, causes-20)

