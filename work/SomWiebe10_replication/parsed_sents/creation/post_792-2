(ROOT
  (S
    (NP (PRP I))
    (VP (VBP am)
      (NP
        (NP (DT the) (NN LORD))
        (, ,)
        (NP
          (NP (DT the) (NNP God))
          (PP (IN of)
            (NP (DT all) (NN mankind))))))
    (. .)))

nsubj(LORD-4, I-1)
cop(LORD-4, am-2)
det(LORD-4, the-3)
root(ROOT-0, LORD-4)
det(God-7, the-6)
appos(LORD-4, God-7)
det(mankind-10, all-9)
prep_of(God-7, mankind-10)

