(ROOT
  (S ('' '')
    (S (CC Yet)
      (NP (EX there))
      (VP (VBP are)
        (NP
          (NP (DT no) (NNS birds))
          (PP (IN before)
            (NP (DT the) (NN archaeoperyx))))))
    (, ,)
    (CC and)
    (S
      (PP (IN after)
        (NP (DT the) (NN archaeoperyx)))
      (NP (DT the) (NNS birds))
      (VP (VBP begin)
        (S
          (VP (TO to)
            (VP (VB appear))))))
    (. ...)))

expl(are-4, there-3)
root(ROOT-0, are-4)
det(birds-6, no-5)
nsubj(are-4, birds-6)
det(archaeoperyx-9, the-8)
prep_before(birds-6, archaeoperyx-9)
det(archaeoperyx-14, the-13)
prep_after(begin-17, archaeoperyx-14)
det(birds-16, the-15)
nsubj(begin-17, birds-16)
xsubj(appear-19, birds-16)
conj_and(are-4, begin-17)
aux(appear-19, to-18)
xcomp(begin-17, appear-19)

