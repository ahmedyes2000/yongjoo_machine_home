(ROOT
  (S
    (NP (EX There))
    (VP (VBZ is)
      (NP
        (NP (DT a) (NNP Buddhist) (NN quote))
        (, ,) (`` ``)
        (S
          (NP (PRP$ My) (NN teaching))
          (VP (VBZ is)
            (PP (IN like)
              (NP
                (NP (DT a) (NN finger))
                (VP (VBG pointing)
                  (PP (TO to)
                    (NP (DT the) (NN moon))))))))))
    (. .)))

expl(is-2, There-1)
root(ROOT-0, is-2)
det(quote-5, a-3)
nn(quote-5, Buddhist-4)
nsubj(is-2, quote-5)
poss(teaching-9, My-8)
nsubj(is-10, teaching-9)
dep(quote-5, is-10)
det(finger-13, a-12)
prep_like(is-10, finger-13)
partmod(finger-13, pointing-14)
det(moon-17, the-16)
prep_to(pointing-14, moon-17)

