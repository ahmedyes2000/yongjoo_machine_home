(ROOT
  (S (CC But)
    (NP (DT both))
    (VP (VBP are)
      (ADJP (JJ important)
        (PP (IN in)
          (NP
            (NP (NNS terms))
            (PP (IN of)
              (S
                (VP (VBG coming)
                  (PP (TO to)
                    (NP
                      (NP (NNS terms))
                      (PP (IN with)
                        (NP (PRP ourselves))))))))))))
    (. .)))

cc(important-4, But-1)
nsubj(important-4, both-2)
cop(important-4, are-3)
root(ROOT-0, important-4)
prep_in(important-4, terms-6)
prepc_of(terms-6, coming-8)
prep_to(coming-8, terms-10)
prep_with(terms-10, ourselves-12)

