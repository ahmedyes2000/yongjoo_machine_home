(ROOT
  (S
    (ADVP (RB Remember))
    (PRN (: --)
      (SBAR (IN because)
        (S
          (NP (PRP you))
          (VP (VBD claimed)
            (SBAR (IN that)
              (S
                (NP (DT no) (JJ such) (NN secular) (NNS purposes))
                (VP (VBP exist)
                  (ADVP (RB whatsoever))))))))
      (: --))
    (NP (PRP you))
    (VP (MD must)
      (VP (VB negate)
        (S
          (NP
            (NP (DT each))
            (CC and)
            (NP (DT every)
              (ADJP (JJ possible)
                (CC and)
                (JJ potential))
              (NN secular) (NN purpose)))
          (VP (TO to)
            (VP (VB prove)
              (NP (PRP$ your) (NN point)))))))
    (. .)))

