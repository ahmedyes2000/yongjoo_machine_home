  (S
    (NP (PRP I))
    (VP (MD could)
      (ADVP (RB never))
      (VP (VB imagine)
        (S
          (VP (VBG being)
            (ADJP (VBN satisfied)
              (PP (IN with)
                (NP
                  (NP (DT a) (`` ``) (NNP God))
                  (VP (VBD did)
                    (NP (PRP it) ('' ''))))))))))
    (. .)))

nsubj(imagine-4, I-1)
aux(imagine-4, could-2)
neg(imagine-4, never-3)
root(ROOT-0, imagine-4)
auxpass(satisfied-6, being-5)
