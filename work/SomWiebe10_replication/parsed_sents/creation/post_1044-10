(ROOT
  (S
    (ADVP (RB Now))
    (, ,)
    (SBAR (IN if)
      (S
        (NP (PRP they))
        (VP (VBD grew)
          (NP (JJ feathered) (NNS wings)))))
    (NP (PRP I))
    (VP (MD 'd)
      (VP (VB be)
        (ADJP (JJ convinced))))
    (. .)))

advmod(convinced-11, Now-1)
mark(grew-5, if-3)
nsubj(grew-5, they-4)
dep(convinced-11, grew-5)
amod(wings-7, feathered-6)
dobj(grew-5, wings-7)
nsubj(convinced-11, I-8)
aux(convinced-11, 'd-9)
cop(convinced-11, be-10)
root(ROOT-0, convinced-11)

