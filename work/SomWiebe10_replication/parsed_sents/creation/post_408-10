(ROOT
  (S
    (ADVP (RB Now))
    (NP (PRP I))
    (VP (MD can)
      (VP (VB sleep)
        (PP (IN at)
          (NP (NN night)))
        (, ,)
        (S
          (VP (VBG knowing)
            (SBAR
              (WHADVP (WRB how))
              (S
                (NP (PRP I))
                (VP (VBD got)
                  (ADVP (RB here)))))))))
    (. .)))

advmod(sleep-4, Now-1)
nsubj(sleep-4, I-2)
aux(sleep-4, can-3)
root(ROOT-0, sleep-4)
prep_at(sleep-4, night-6)
xcomp(sleep-4, knowing-8)
advmod(got-11, how-9)
nsubj(got-11, I-10)
ccomp(knowing-8, got-11)
advmod(got-11, here-12)

