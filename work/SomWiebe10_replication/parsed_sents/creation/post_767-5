(ROOT
  (S
    (NP (NNS Scientists))
    (VP (VBP love)
      (S
        (VP (TO to)
          (VP (VB overturn)
            (NP (VBN established) (NNS theroies))))))
    (. .)))

nsubj(love-2, Scientists-1)
xsubj(overturn-4, Scientists-1)
root(ROOT-0, love-2)
aux(overturn-4, to-3)
xcomp(love-2, overturn-4)
amod(theroies-6, established-5)
dobj(overturn-4, theroies-6)

