(ROOT
  (S ('' '')
    (PP (RB Just) (IN for)
      (NP (JJ further) (NN clarification)))
    (, ,)
    (NP (PRP$ your) (NN reasoning))
    (VP (VBZ is)
      (SBAR (IN as)
        (S
          (VP (VBZ follows)))))
    (: ...)))

advmod(is-9, Just-2)
amod(clarification-5, further-4)
prep_for(is-9, clarification-5)
poss(reasoning-8, your-7)
nsubj(is-9, reasoning-8)
root(ROOT-0, is-9)
mark(follows-11, as-10)
advcl(is-9, follows-11)

