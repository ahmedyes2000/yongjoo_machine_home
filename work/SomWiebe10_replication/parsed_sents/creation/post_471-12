(ROOT
  (S
    (NP (EX There))
    (VP (VBZ is)
      (NP
        (NP (DT no) (JJ rational) (NN reason))
        (SBAR
          (WHADVP (WRB why))
          (S
            (NP (DT this))
            (VP (MD should)
              (VP (VB be)))))))
    (. .)))

expl(is-2, There-1)
root(ROOT-0, is-2)
det(reason-5, no-3)
amod(reason-5, rational-4)
nsubj(is-2, reason-5)
advmod(be-9, why-6)
nsubj(be-9, this-7)
aux(be-9, should-8)
rcmod(reason-5, be-9)

