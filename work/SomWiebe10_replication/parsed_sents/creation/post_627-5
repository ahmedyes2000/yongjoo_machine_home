(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is)
      (ADJP (JJ significant))
      (SBAR (IN that)
        (S
          (NP
            (NP (DT the) (JJ final) (NN product))
            (ADVP (PRP itself)))
          (VP (VBZ is)
            (ADJP (JJ intelligent))))))
    (. .)))

nsubj(significant-3, It-1)
cop(significant-3, is-2)
root(ROOT-0, significant-3)
complm(intelligent-10, that-4)
det(product-7, the-5)
amod(product-7, final-6)
nsubj(intelligent-10, product-7)
advmod(product-7, itself-8)
cop(intelligent-10, is-9)
ccomp(significant-3, intelligent-10)

