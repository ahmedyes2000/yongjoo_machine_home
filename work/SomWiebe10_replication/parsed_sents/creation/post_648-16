(ROOT
  (S
    (NP (DT The) (NN answer))
    (VP (VBZ is)
      (S
        (INTJ (UH no))
        (, ,)
        (NP (PRP it))
        (VP (VBZ does) (RB n't))))
    (. .)))

det(answer-2, The-1)
nsubj(is-3, answer-2)
root(ROOT-0, is-3)
dep(does-7, no-4)
nsubj(does-7, it-6)
ccomp(is-3, does-7)
neg(does-7, n't-8)

