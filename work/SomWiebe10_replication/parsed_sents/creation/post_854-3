(ROOT
  (S
    (NP (NNP AiG))
    (VP (MD should)
      (VP (VB publish)
        (NP
          (NP (DT a) (JJ compelling) (JJ alternative) (NNS hypothesis))
          (SBAR
            (WHNP (WDT that))
            (S
              (ADVP (RB better))
              (VP (VBZ explains)
                (NP (DT all) (NNS observations))))))))
    (. .)))

nsubj(publish-3, AiG-1)
aux(publish-3, should-2)
root(ROOT-0, publish-3)
det(hypothesis-7, a-4)
amod(hypothesis-7, compelling-5)
amod(hypothesis-7, alternative-6)
dobj(publish-3, hypothesis-7)
nsubj(explains-10, hypothesis-7)
advmod(explains-10, better-9)
rcmod(hypothesis-7, explains-10)
det(observations-12, all-11)
dobj(explains-10, observations-12)

