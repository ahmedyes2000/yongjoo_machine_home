(ROOT
  (S
    (ADVP (RB Disagree))
    (NP (DT that) (NN evolution))
    (VP (VBZ is)
      (NP (PDT such) (DT a) (NN field)))
    (. .)))

advmod(field-7, Disagree-1)
det(evolution-3, that-2)
nsubj(field-7, evolution-3)
cop(field-7, is-4)
predet(field-7, such-5)
det(field-7, a-6)
root(ROOT-0, field-7)

