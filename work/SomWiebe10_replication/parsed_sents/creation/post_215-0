(ROOT
  (S
    (NP
      (NP (DT This) (NN post))
      (PP (IN by)
        (NP (NNP Casey) (NNP Luskin))))
    (ADVP (RB graphically))
    (VP (VBZ illustrates)
      (SBAR
        (WHADVP (WRB why))
        (S
          (NP (NNP Intelligent) (NNP Design))
          (VP (VBZ is) (RB not)
            (NP (NN science))))))
    (. .)))

det(post-2, This-1)
nsubj(illustrates-7, post-2)
nn(Luskin-5, Casey-4)
prep_by(post-2, Luskin-5)
advmod(illustrates-7, graphically-6)
root(ROOT-0, illustrates-7)
advmod(science-13, why-8)
nn(Design-10, Intelligent-9)
