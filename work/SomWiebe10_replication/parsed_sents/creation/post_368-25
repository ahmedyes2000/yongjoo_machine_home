(ROOT
  (S
    (NP (DT The) (NN word))
    (VP (VBZ comes)
      (PP (IN from)
        (NP
          (NP (DT the) (NN latin) (NN word))
          (PP (IN for)
            (NP (NN knowledge)))))
      (, ,)
      (ADVP (IN after) (DT all)))
    (. .)))

det(word-2, The-1)
nsubj(comes-3, word-2)
root(ROOT-0, comes-3)
det(word-7, the-5)
