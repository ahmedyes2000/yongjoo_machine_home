(ROOT
  (S
    (NP
      (NP (PRP I))
      (PP (IN for)
        (NP (CD one))))
    (VP (MD would)
      (VP (VB like)
        (S
          (VP (TO to)
            (VP (VB know)
              (SBAR
                (WHADVP (WRB how))
                (S
                  (NP (JJ such) (NNS hypothesis))
                  (VP (MD could)
                    (VP (VB be)
                      (VP (VBN falsified)))))))))))
    (. .)))

nsubj(like-5, I-1)
xsubj(know-7, I-1)
prep_for(I-1, one-3)
aux(like-5, would-4)
root(ROOT-0, like-5)
aux(know-7, to-6)
xcomp(like-5, know-7)
advmod(falsified-13, how-8)
amod(hypothesis-10, such-9)
nsubjpass(falsified-13, hypothesis-10)
aux(falsified-13, could-11)
auxpass(falsified-13, be-12)
ccomp(know-7, falsified-13)

