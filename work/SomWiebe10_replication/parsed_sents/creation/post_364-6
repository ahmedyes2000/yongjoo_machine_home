(ROOT
  (S
    (NP (PRP We))
    (VP (VBP do)
      (VP (VB appreciate)
        (NP
          (NP (DT the) (NNS quarters))
          (SBAR
            (S
              (NP (PRP you))
              (VP (VBP keep)
                (S
                  (VP (VBG dropping)
                    (PP (IN in)
                      (NP (DT the) (NN machine)))))))))))
    (. .)))

nsubj(appreciate-3, We-1)
aux(appreciate-3, do-2)
root(ROOT-0, appreciate-3)
det(quarters-5, the-4)
dobj(appreciate-3, quarters-5)
dobj(keep-7, quarters-5)
nsubj(keep-7, you-6)
rcmod(quarters-5, keep-7)
xcomp(keep-7, dropping-8)
det(machine-11, the-10)
prep_in(dropping-8, machine-11)

