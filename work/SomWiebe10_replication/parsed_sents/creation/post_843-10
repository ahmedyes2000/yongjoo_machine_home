(ROOT
  (S
    (NP (PRP I))
    (ADVP (RB highly))
    (VP (VBP suggest)
      (NP
        (NP (DT the) (JJ following) (NN article))
        (PP (IN for)
          (NP (DT any) (NNP Christian))))
      (PP (IN with)
        (NP
          (NP (NNS concerns))
          (PP (IN over)
            (NP
              (NP (DT the) (NN reliability))
              (PP (IN of)
                (NP (JJ radiometric) (JJ dating) (NNS methods))))))))
    (. .)))

nsubj(suggest-3, I-1)
advmod(suggest-3, highly-2)
root(ROOT-0, suggest-3)
det(article-6, the-4)
amod(article-6, following-5)
dobj(suggest-3, article-6)
det(Christian-9, any-8)
prep_for(article-6, Christian-9)
prep_with(suggest-3, concerns-11)
det(reliability-14, the-13)
prep_over(concerns-11, reliability-14)
amod(methods-18, radiometric-16)
amod(methods-18, dating-17)
prep_of(reliability-14, methods-18)

