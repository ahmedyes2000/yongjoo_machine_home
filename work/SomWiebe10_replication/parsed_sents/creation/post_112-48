(ROOT
  (S
    (NP (PRP it))
    (ADVP (RB truly))
    (VP (VBZ hangs)
      (PRT (RP on))
      (NP (NN nothing))
      (-RRB- -RRB-)
      (ADVP (RB but)
        (ADJP (FW anyway) (. .))))
    (. .)))

nsubj(hangs-3, it-1)
advmod(hangs-3, truly-2)
root(ROOT-0, hangs-3)
prt(hangs-3, on-4)
dobj(hangs-3, nothing-5)
advmod(hangs-3, but-7)
pobj(but-7, anyway-8)

