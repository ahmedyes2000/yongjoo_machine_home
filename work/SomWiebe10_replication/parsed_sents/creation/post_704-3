(ROOT
  (S
    (NP (NNS Revelations))
    (VP (VBZ is)
      (NP
        (NP (DT an) (NN example))
        (SBAR
          (WHADVP (WRB where))
          (S
            (NP (RB clearly) (NN symbolism))
            (VP (VBZ is)
              (VP (VBN used)))))))
    (. .)))

nsubj(example-4, Revelations-1)
cop(example-4, is-2)
det(example-4, an-3)
root(ROOT-0, example-4)
advmod(used-9, where-5)
advmod(symbolism-7, clearly-6)
nsubjpass(used-9, symbolism-7)
auxpass(used-9, is-8)
rcmod(example-4, used-9)

