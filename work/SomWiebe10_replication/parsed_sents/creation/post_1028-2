(ROOT
  (S
    (NP (FW i))
    (VP (VBP think)
      (SBAR
        (S
          (PP (IN in)
            (NP (NNP Logical) (NNS terms)))
          (NP (PRP we))
          (VP (VBP call)
            (ADVP (IN that)
              (NP (DT a) (NN fallacy)))))))
    (. .)))

nsubj(think-2, i-1)
root(ROOT-0, think-2)
