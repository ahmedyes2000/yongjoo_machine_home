(ROOT
  (S
    (NP (PRP You))
    (VP (VBP continue)
      (S
        (VP (TO to)
          (VP (VB quote)
            (NP (JJ dishonest) (NNS sources))))))
    (. .)))

nsubj(continue-2, You-1)
xsubj(quote-4, You-1)
root(ROOT-0, continue-2)
aux(quote-4, to-3)
xcomp(continue-2, quote-4)
amod(sources-6, dishonest-5)
dobj(quote-4, sources-6)

