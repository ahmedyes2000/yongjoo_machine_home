(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is)
      (ADVP (RB only))
      (NP (NN belief)))
    (. .)))

nsubj(belief-4, It-1)
cop(belief-4, is-2)
advmod(belief-4, only-3)
root(ROOT-0, belief-4)

(ROOT
  (S
    (NP (DT No) (NN one))
    (VP (VBZ has)
      (NP (DT any) (JJ real) (NN evidence)))
    (. .)))

det(one-2, No-1)
nsubj(has-3, one-2)
root(ROOT-0, has-3)
det(evidence-6, any-4)
amod(evidence-6, real-5)
dobj(has-3, evidence-6)

