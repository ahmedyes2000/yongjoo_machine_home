(ROOT
  (S
    (PP (IN after)
      (NP (DT the) (JJ entire)
        (ADJP (JJ cain)
          (CC and)
          (JJ able))
        (NN thing)))
    (, ,)
    (NP (DT the) (NN brother))
    (VP (VBD left)
      (PP (TO to)
        (NP
          (NP (DT another) (NN group))
          (PP (IN of)
            (NP (NNS people)))))
      (S
        (VP (TO to)
          (VP (VB find)
            (NP (DT a) (NN wife))))))
    (: ...)))

det(thing-7, the-2)
amod(thing-7, entire-3)
amod(thing-7, cain-4)
conj_and(cain-4, able-6)
