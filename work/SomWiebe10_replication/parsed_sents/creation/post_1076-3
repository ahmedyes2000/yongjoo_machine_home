(ROOT
  (S
    (NP (PRP We))
    (VP (MD can)
      (ADVP (RB even))
      (VP (VB observe)
        (NP
          (NP (NN evolution))
          (VP (VBG taking)
            (NP
              (NP (NN place))
              (PP (IN in)
                (NP
                  (NP (NNS populations))
                  (PP (IN of)
                    (NP (NNS microbes))))))
            (, ,)
            (PP (IN for)
              (NP (NN example)))))))
    (. .)))

nsubj(observe-4, We-1)
aux(observe-4, can-2)
advmod(observe-4, even-3)
root(ROOT-0, observe-4)
dobj(observe-4, evolution-5)
partmod(evolution-5, taking-6)
dobj(taking-6, place-7)
prep_in(place-7, populations-9)
prep_of(populations-9, microbes-11)
prep_for(taking-6, example-14)

