(ROOT
  (S
    (S
      (PP (IN In)
        (NP (NNP Canaan)))
      (, ,)
      (NP (NN drought))
      (VP (VBD was)
        (NP (DT the) (NN enemy))))
    (: ;)
    (S
      (NP (JJ high) (NN summer))
      (VP (VBD was)
        (NP
          (NP (DT the) (NN death))
          (PP (IN of)
            (NP (NN nature))))))
    (. .)))

prep_in(enemy-7, Canaan-2)
nsubj(enemy-7, drought-4)
cop(enemy-7, was-5)
det(enemy-7, the-6)
root(ROOT-0, enemy-7)
