(ROOT
  (S
    (NP (DT This))
    (VP (VBZ is)
      (ADVP (RB simply))
      (NP (NN scare) (NN rhetoric)
        (S
          (VP (TO to)
            (VP (VB get)
              (S
                (NP (NNS people))
                (ADJP (JJ worried))))))))
    (. .)))

nsubj(rhetoric-5, This-1)
cop(rhetoric-5, is-2)
advmod(rhetoric-5, simply-3)
nn(rhetoric-5, scare-4)
root(ROOT-0, rhetoric-5)
aux(get-7, to-6)
infmod(rhetoric-5, get-7)
nsubj(worried-9, people-8)
xcomp(get-7, worried-9)

