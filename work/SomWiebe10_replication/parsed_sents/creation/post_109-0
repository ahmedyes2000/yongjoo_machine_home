(ROOT
  (S
    (NP (PRP I))
    (VP (VBP 'm)
      (ADVP (RB hardly))
      (NP
        (NP (DT the) (NN authority))
        (PP (IN for)
          (NP
            (NP (PDT all) (DT the) (JJ great) (NNS questions))
            (PP (IN in)
              (NP (NN life)))))))
    (. .) (. .)))

nsubj(authority-5, I-1)
cop(authority-5, 'm-2)
advmod(authority-5, hardly-3)
det(authority-5, the-4)
root(ROOT-0, authority-5)
predet(questions-10, all-7)
det(questions-10, the-8)
amod(questions-10, great-9)
prep_for(authority-5, questions-10)
prep_in(questions-10, life-12)

