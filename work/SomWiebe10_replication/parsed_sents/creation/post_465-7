(ROOT
  (FRAG
    (ADVP (RB Especially))
    (SBAR
      (WHADVP (WRB when))
      (S
        (NP (DT some) (NNP ID) (NNS proponents))
        (VP (VBP have)
          (NP (PDT such) (DT a) (JJ poor) (NN record))
          (PP (IN in)
            (S
              (VP (VBG presenting)
                (NP (DT the) (JJ full) (NN story))))))))
    (. .)))

root(ROOT-0, Especially-1)
advmod(have-6, when-2)
det(proponents-5, some-3)
nn(proponents-5, ID-4)
nsubj(have-6, proponents-5)
dep(Especially-1, have-6)
predet(record-10, such-7)
det(record-10, a-8)
amod(record-10, poor-9)
dobj(have-6, record-10)
prepc_in(have-6, presenting-12)
det(story-15, the-13)
amod(story-15, full-14)
dobj(presenting-12, story-15)

