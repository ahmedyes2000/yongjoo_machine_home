(ROOT
  (S
    (PP (IN As)
      (NP (DT a) (NNP Christian)))
    (, ,)
    (NP (PRP I))
    (VP (VBP know)
      (SBAR (IN that)
        (S
          (NP
            (NP (DT the) (NNP God))
            (SBAR
              (WHNP (WP who))
              (S
                (VP (VBD created)
                  (NP (PRP me))))))
          (VP (VBZ has)
            (NP
              (NP (DT a) (JJ one-of-a-kind) (NN plan))
              (PP (IN for)
                (NP (PRP$ my) (NN life))))))))
    (. .)))

det(Christian-3, a-2)
prep_as(know-6, Christian-3)
nsubj(know-6, I-5)
root(ROOT-0, know-6)
complm(has-13, that-7)
det(God-9, the-8)
nsubj(created-11, God-9)
nsubj(has-13, God-9)
rcmod(God-9, created-11)
dobj(created-11, me-12)
ccomp(know-6, has-13)
det(plan-16, a-14)
amod(plan-16, one-of-a-kind-15)
dobj(has-13, plan-16)
poss(life-19, my-18)
prep_for(plan-16, life-19)

