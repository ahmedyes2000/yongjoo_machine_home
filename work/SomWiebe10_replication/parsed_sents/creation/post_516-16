(ROOT
  (FRAG
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP (VBP have)
          (NP (DT the) (NN faith)
            (S
              (VP (TO to)
                (VP (VB believe)
                  (PP (IN in)
                    (NP
                      (NP (NNS millions))
                      (PP (IN of)
                        (NP
                          (NP (JJ naturalistic) (NNS miracles))
                          (PP (IN of)
                            (NP (NN chance))))))))))))))
    (, ,)
    (ADVP (JJ great))
    (. .)))

mark(have-3, If-1)
nsubj(have-3, you-2)
dep(great-16, have-3)
det(faith-5, the-4)
dobj(have-3, faith-5)
aux(believe-7, to-6)
infmod(faith-5, believe-7)
prep_in(believe-7, millions-9)
amod(miracles-12, naturalistic-11)
prep_of(millions-9, miracles-12)
prep_of(miracles-12, chance-14)
root(ROOT-0, great-16)

