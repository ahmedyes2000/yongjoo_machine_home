    (VP (VBZ is)
      (ADJP (JJ false)
        (PP (RB because) (IN of)
          (NP (DT this) (NN bait-and-switch))))
      (SBAR (IN as)
        (S
          (VP (VBD noted)
            (PP (IN by)
              (NP (JJ other) (NN commenter)))))))
    (. .)))

