(ROOT
  (S
    (NP (`` ``) (RB Only) (DT the) (NNS fools))
    (VP (VBP remain)
      (S
        (VP (TO to)
          (VP (VB discuss)
            (NP (PRP$ their) (JJ contrived) (NNS alternatives))))))
    (. .)))

advmod(fools-4, Only-2)
det(fools-4, the-3)
nsubj(remain-5, fools-4)
xsubj(discuss-7, fools-4)
root(ROOT-0, remain-5)
aux(discuss-7, to-6)
xcomp(remain-5, discuss-7)
poss(alternatives-10, their-8)
amod(alternatives-10, contrived-9)
dobj(discuss-7, alternatives-10)

