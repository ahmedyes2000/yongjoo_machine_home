(ROOT
  (S
    (SBAR (IN As)
      (S
        (NP
          (NP (NN simple))
          (PP (IN as)
            (NP (NN gravity))))
        (VP (VBZ appears))))
    (, ,)
    (NP (PRP it))
    (VP (VBZ holds)
      (NP
        (NP (JJ deep) (NN fascination))
        (PP (IN for)
          (NP
            (NP (NN today) (POS 's))
            (NNS scientists)))))
    (. .)))

mark(appears-5, As-1)
nsubj(appears-5, simple-2)
prep_as(simple-2, gravity-4)
advcl(holds-8, appears-5)
nsubj(holds-8, it-7)
root(ROOT-0, holds-8)
amod(fascination-10, deep-9)
dobj(holds-8, fascination-10)
poss(scientists-14, today-12)
prep_for(fascination-10, scientists-14)

