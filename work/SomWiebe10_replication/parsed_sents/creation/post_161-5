(ROOT
  (S
    (PP (IN For)
      (NP (NN instance)))
    (, ,)
    (ADVP (RB no)
      (SBAR
        (WHADVP (WRB where))
        (S
          (VP (VBP do)))))
    (NP (PRP I))
    (VP (VBP find)
      (PP (IN in)
        (NP (DT any) (NNP ID) (NN writing) (NN anything)))
      (SBAR (IN that)
        (S
          (NP (PRP I))
          (VP (MD can)
            (VP (VB use)
              (S
                (VP (TO to)
                  (VP (VB identify)
                    (NP (NNP CSI))))))))))
    (. .)))

prep_for(find-8, instance-2)
advmod(find-8, no-4)
advmod(do-6, where-5)
dep(no-4, do-6)
nsubj(find-8, I-7)
root(ROOT-0, find-8)
det(anything-13, any-10)
nn(anything-13, ID-11)
nn(anything-13, writing-12)
prep_in(find-8, anything-13)
complm(use-17, that-14)
nsubj(use-17, I-15)
xsubj(identify-19, I-15)
aux(use-17, can-16)
ccomp(find-8, use-17)
aux(identify-19, to-18)
xcomp(use-17, identify-19)
dobj(identify-19, CSI-20)

