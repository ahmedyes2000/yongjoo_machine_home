(ROOT
  (S
    (SBAR (IN Before)
      (S
        (NP (DT any) (NN deity))
        (VP (MD can)
          (VP (VB be)
            (VP (VBN included)
              (NP (NN evidence))
              (PP (IN for)
                (NP
                  (NP (DT the) (NN existence))
                  (PP (IN of)
                    (NP (DT that) (NN deity))))))))))
    (VP (VBZ needs)
      (S
        (VP (TO to)
          (VP (VB be)
            (VP (VBN found))))))
    (. .)))

mark(included-6, Before-1)
det(deity-3, any-2)
nsubjpass(included-6, deity-3)
aux(included-6, can-4)
auxpass(included-6, be-5)
csubj(needs-14, included-6)
dobj(included-6, evidence-7)
det(existence-10, the-9)
prep_for(included-6, existence-10)
det(deity-13, that-12)
prep_of(existence-10, deity-13)
root(ROOT-0, needs-14)
aux(found-17, to-15)
auxpass(found-17, be-16)
xcomp(needs-14, found-17)

