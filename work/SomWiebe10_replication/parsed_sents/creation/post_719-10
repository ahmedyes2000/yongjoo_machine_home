(ROOT
  (S
    (S
      (ADVP (RB Also))
      (, ,)
      (NP (PRP it))
      (VP (VBZ 's)
        (ADJP (JJ silly)
          (S
            (VP (TO to)
              (VP (VB assume)
                (SBAR
                  (S
                    (NP (PRP$ our) (NNS eyes))
                    (VP (MD would)
                      (VP (VB adapt)
                        (PP (TO to)
                          (NP
                            (NP (DT the) (NN pitch) (NN black))
                            (PP (IN of)
                              (NP (NN night)))))
                        (SBAR (IN if)
                          (S
                            (NP (EX there))
                            (VP (VBD was)
                              (NP (DT no) (NN moon)))))))))))))))
    (: ...)
    (S
      (NP (PDT half) (DT the) (NN day))
      (NP (PRP we))
      (VP (MD would)
        (ADVP (RB still))
        (VP (VB be)
          (PP (IN in)
            (NP (NN sunlight))))))
    (. .)))

