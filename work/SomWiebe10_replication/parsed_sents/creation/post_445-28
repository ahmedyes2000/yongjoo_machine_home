(ROOT
  (S (CC And)
    (NP (DT that))
    (VP (VBZ 's)
      (NP
        (NP (CD one)
          (ADJP (RB very) (JJ good))
          (NN reason))
        (SBAR
          (WHADVP (WRB why))
          (S
            (NP (EX there))
            (VP (VBZ is)
              (NP
                (NP (DT no) (JJ such) (NN thing))
                (PP (IN as)
                  (NP
                    (NP (DT a) (NN theory))
                    (PP (IN of)
                      (NP (JJ intelligent) (NN design)))))))))))
    (. .)))

cc(reason-7, And-1)
nsubj(reason-7, that-2)
cop(reason-7, 's-3)
num(reason-7, one-4)
advmod(good-6, very-5)
amod(reason-7, good-6)
root(ROOT-0, reason-7)
advmod(is-10, why-8)
expl(is-10, there-9)
rcmod(reason-7, is-10)
det(thing-13, no-11)
amod(thing-13, such-12)
nsubj(is-10, thing-13)
det(theory-16, a-15)
prep_as(thing-13, theory-16)
amod(design-19, intelligent-18)
prep_of(theory-16, design-19)

