(ROOT
  (S (`` ``)
    (NP
      (NP (NNP PS) (NNP INCLUDING) (NNP CREATIONISM) (NNP AS) (NNP A) (NNP N) (NNP OPTION)
        (CC AND)
        (NNP STOPPING) (NNP HYPNOSIS))
      (PP (IN ON)
        (NP (NNS STUDENTS))))
    (VP (VBD WOULD)
      (S
        (VP (VBG BRING)
          (PRT (RP UP))
          (NP
            (NP (NNP THEIR) (NNP IQ) (POS 'S))
            (NN IMMEDIATELY)))))
    (. !)))

