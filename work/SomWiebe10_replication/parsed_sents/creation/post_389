(ROOT
  (S (`` ``)
    (SBAR (IN If)
      (S
        (NP (PRP we))
        (VP (VBP include)
          (NP (NN creationism)))))
    (S
      (NP (PRP we))
      (VP (MD should)
        (VP (VB include)
          (NP (PDT all) (DT the) (JJ creationist) (NNS stories)))))
    (: ...)
    (S
      (NP (DT the) (NN proof))
      (VP (VBZ is)
        (ADJP (RB about) (JJ equal)
          (PP (IN for)
            (NP
              (NP (DT all))
              (PP (IN of)
                (NP (PRP them))))))))
    (. .)))

