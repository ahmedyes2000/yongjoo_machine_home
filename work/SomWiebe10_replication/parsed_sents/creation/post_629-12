(ROOT
  (S (CC And)
    (NP (PRP I))
    (VP (VBP 'm) (RB not)
      (ADJP (RB much) (JJ interested)
        (PP (IN in)
          (NP (NN philosophy))))
      (ADVP
        (ADVP (RB as) (RB long))
        (SBAR (IN as)
          (S
            (NP (PRP you))
            (VP (VBP do) (RB n't)
              (VP (VB try)
                (S
                  (VP (TO to)
                    (VP (VB teach)
                      (NP (PRP it))
                      (PP (IN in)
                        (NP (NN science) (NN class))))))))))))
    (. .)))

