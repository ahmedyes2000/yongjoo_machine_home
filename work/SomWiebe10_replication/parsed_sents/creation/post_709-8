(ROOT
  (S
    (S
      (NP (PRP You))
      (ADVP (RB either))
      (VP (VBP have)
        (NP (NN faith))))
    (CC or)
    (S
      (NP (PRP you))
      (VP (VBP do) (RB n't)))
    (. .)))

nsubj(have-3, You-1)
advmod(have-3, either-2)
root(ROOT-0, have-3)
dobj(have-3, faith-4)
nsubj(do-7, you-6)
conj_or(have-3, do-7)
neg(do-7, n't-8)

