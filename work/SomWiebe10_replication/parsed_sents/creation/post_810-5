(ROOT
  (S ('' '')
    (ADVP (RB So))
    (, ,)
    (NP (PRP$ your) (JJ first) (NN effort))
    (VP (VBD was)
      (NP (DT a) (NN flop)))
    (. .)))

advmod(flop-9, So-2)
poss(effort-6, your-4)
amod(effort-6, first-5)
nsubj(flop-9, effort-6)
cop(flop-9, was-7)
det(flop-9, a-8)
root(ROOT-0, flop-9)

