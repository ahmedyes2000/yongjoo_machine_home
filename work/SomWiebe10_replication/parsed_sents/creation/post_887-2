(ROOT
  (S (CC But)
    (SBAR
      (WHADVP (WRB when))
      (S
        (NP (NNP Darrow))
        (VP (VBP ask)
          (NP (PRP him))
          (SBAR (IN if)
            (S
              (NP (DT the) (NN world))
              (VP (VBD was)
                (VP (VBN created)
                  (PP (IN in)
                    (NP (CD 6) (NNS days))))))))))
    (PRN (, ,)
      (NP (NNP Bryan))
      (VP (VBD said))
      (, ,))
    (NP (RB no) (DT the) (NN Bible))
    (VP (VBZ does) (RB n't)
      (VP (VB say)
        (NP (DT that))))
    (. .)))

cc(say-23, But-1)
advmod(ask-4, when-2)
nsubj(ask-4, Darrow-3)
advcl(say-23, ask-4)
dobj(ask-4, him-5)
mark(created-10, if-6)
det(world-8, the-7)
