(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP (VBP want)
          (S
            (VP (TO to)
              (VP (VB believe)
                (NP (DT the) (NN bible))))))))
    (, ,)
    (VP (VB fine))
    (. .)))

mark(want-3, If-1)
nsubj(want-3, you-2)
xsubj(believe-5, you-2)
advcl(fine-9, want-3)
aux(believe-5, to-4)
xcomp(want-3, believe-5)
det(bible-7, the-6)
dobj(believe-5, bible-7)
root(ROOT-0, fine-9)

