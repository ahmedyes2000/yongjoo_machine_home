(ROOT
  (S
    (NP
      (NP (NNP Hmm))
      (, ,)
      (ADJP (RB too) (JJ hard)
        (S
          (VP (TO to)
            (VP (VB understand)))))
      (, ,))
    (ADVP (RB poof))
    (VP (MD must)
      (VP (VB have)
        (VP (VBN been)
          (VP (VBN designed)))))
    (. .)))

nsubjpass(designed-12, Hmm-1)
advmod(hard-4, too-3)
amod(Hmm-1, hard-4)
aux(understand-6, to-5)
xcomp(hard-4, understand-6)
advmod(designed-12, poof-8)
aux(designed-12, must-9)
aux(designed-12, have-10)
auxpass(designed-12, been-11)
root(ROOT-0, designed-12)

