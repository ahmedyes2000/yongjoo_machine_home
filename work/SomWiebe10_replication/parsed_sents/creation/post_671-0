(ROOT
  (S
    (NP
      (NP (NNP Perhaps))
      (SBAR
        (WHNP (WP what))
        (S
          (VP (VBZ is)
            (VP (VBN needed))))))
    (VP (VBZ is)
      (S
        (VP (TO to)
          (VP (VB distinguish)
            (PP (IN between)
              (NP (JJ academic) (NN freedom)
                (CC and)
                (NN peer) (NN review)))))))
    (. .)))

nsubjpass(needed-4, Perhaps-1)
nsubj(is-5, Perhaps-1)
xsubj(distinguish-7, Perhaps-1)
auxpass(needed-4, is-3)
rcmod(Perhaps-1, needed-4)
root(ROOT-0, is-5)
aux(distinguish-7, to-6)
xcomp(is-5, distinguish-7)
amod(freedom-10, academic-9)
prep_between(distinguish-7, freedom-10)
nn(review-13, peer-12)
prep_between(distinguish-7, review-13)
conj_and(freedom-10, review-13)

