(ROOT
  (S
    (NP (NNP GOD))
    (VP (VBZ is) (RB not)
      (ADJP (JJ physical)
        (PP (IN except)
          (PP (IN as)
            (NP
              (NP (NNP CHRIST)
                (CC &)
                (NP (DT a) (NN CHRISTIAN)))
              (, ,)
              (NP (JJ true) (NNS believers)))))))
    (: ...)))

nsubj(physical-4, GOD-1)
cop(physical-4, is-2)
neg(physical-4, not-3)
root(ROOT-0, physical-4)
prep(physical-4, except-5)
pcomp(except-5, as-6)
prep_as(except-5, CHRIST-7)
det(CHRISTIAN-10, a-9)
prep_as(except-5, CHRISTIAN-10)
conj_and(CHRIST-7, CHRISTIAN-10)
amod(believers-13, true-12)
appos(CHRIST-7, believers-13)

