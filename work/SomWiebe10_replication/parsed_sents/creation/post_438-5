(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP (VBP listen)
          (PP (TO to)
            (NP (DT the) (JJ anti) (NNP ID) (NN side))))))
    (NP (PRP$ our) (NN existence))
    (VP (VBZ is)
      (ADJP (JJ due)
        (PP (TO to)
          (NP (JJ many) (NNS accidents))))
      (NP
        (NP (RB first) (JJ cosmic))
        (CC and)
        (ADVP (RB then))
        (NP (JJ genetic))))
    (. .)))

mark(listen-3, If-1)
nsubj(listen-3, you-2)
advcl(due-12, listen-3)
det(side-8, the-5)
amod(side-8, anti-6)
nn(side-8, ID-7)
prep_to(listen-3, side-8)
poss(existence-10, our-9)
nsubj(due-12, existence-10)
cop(due-12, is-11)
root(ROOT-0, due-12)
amod(accidents-15, many-14)
