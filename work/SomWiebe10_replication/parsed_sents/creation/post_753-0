(ROOT
  (SBARQ (RB So)
    (SBAR (IN if)
      (S
        (NP (NNP God))
        (VP (VBZ is) (RB not)
          (ADJP (RB all) (JJ powerful)))))
    (PRN (CC and)
      (S
        (NP (DT all))
        (VP (VBG knowing)
          (ADVP (RB then))
          (NP (NNP God)))))
    (SQ (VBZ is)
      (NP (NNP fallible)))
    (. ?)))

advmod(is-13, So-1)
mark(powerful-7, if-2)
nsubj(powerful-7, God-3)
cop(powerful-7, is-4)
neg(powerful-7, not-5)
advmod(powerful-7, all-6)
dep(is-13, powerful-7)
dep(knowing-10, and-8)
nsubj(knowing-10, all-9)
dep(is-13, knowing-10)
advmod(knowing-10, then-11)
dobj(knowing-10, God-12)
root(ROOT-0, is-13)
nsubj(is-13, fallible-14)

