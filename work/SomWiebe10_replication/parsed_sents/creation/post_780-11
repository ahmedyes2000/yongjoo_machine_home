(ROOT
  (S
    (ADVP
      (ADVP (RB So) (RB much))
      (PP (IN for)
        (NP (DT that))))
    (, ,)
    (NP (PRP I))
    (VP (VBD did) (RB not)
      (VP (VB come)
        (ADVP (RB here))
        (S
          (VP (TO to)
            (VP (VB split)
              (NP (NNS hairs))
              (PP (IN with)
                (NP (NN anyone))))))))
    (. .)))

advmod(much-2, So-1)
advmod(come-9, much-2)
prep_for(much-2, that-4)
nsubj(come-9, I-6)
xsubj(split-12, I-6)
aux(come-9, did-7)
neg(come-9, not-8)
root(ROOT-0, come-9)
advmod(come-9, here-10)
aux(split-12, to-11)
xcomp(come-9, split-12)
dobj(split-12, hairs-13)
prep_with(split-12, anyone-15)

