(ROOT
  (S
    (NP (DT That))
    (VP (VBZ 's)
      (NP
        (NP (DT the) (JJ main) (NN reason))
        (SBAR
          (WHADVP (WRB why))
          (S
            (NP (PRP I))
            (VP (VBP have)
              (VP (VBN embraced)
                (NP
                  (NP (JJ theistic) (NN evolution))
                  (PP (RB instead) (IN of)
                    (NP (DT the) (NNP Genesis) (NN creation) (NNS accounts))))))))))
    (. .)))

nsubj(reason-5, That-1)
cop(reason-5, 's-2)
det(reason-5, the-3)
amod(reason-5, main-4)
root(ROOT-0, reason-5)
advmod(embraced-9, why-6)
nsubj(embraced-9, I-7)
aux(embraced-9, have-8)
rcmod(reason-5, embraced-9)
amod(evolution-11, theistic-10)
dobj(embraced-9, evolution-11)
det(accounts-17, the-14)
nn(accounts-17, Genesis-15)
nn(accounts-17, creation-16)
prep_instead_of(evolution-11, accounts-17)

