(ROOT
  (S
    (NP
      (NP (DT Those))
      (SBAR
        (WHNP (WP who))
        (S
          (VP (VBD put)
            (NP (PRP$ their) (NN faith))
            (PP (IN in)
              (NP (NNP Genesis)))
            (ADVP (RBR more) (IN than))
            (PP (IN in)
              (NP (NNP Jesus)))))))
    (VP (VBP have)
      (NP
        (NP (DT no) (NN business))
        (VP (VBG calling)
          (NP (PRP themselves))
          (NP (NNS Christians)))))
    (. .)))

nsubj(put-3, Those-1)
nsubj(have-12, Those-1)
rcmod(Those-1, put-3)
poss(faith-5, their-4)
dobj(put-3, faith-5)
prep_in(put-3, Genesis-7)
advmod(than-9, more-8)
advmod(put-3, than-9)
prep_in(put-3, Jesus-11)
root(ROOT-0, have-12)
det(business-14, no-13)
dobj(have-12, business-14)
partmod(business-14, calling-15)
iobj(calling-15, themselves-16)
dobj(calling-15, Christians-17)

