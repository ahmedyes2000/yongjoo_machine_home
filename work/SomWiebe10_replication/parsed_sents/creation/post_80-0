(ROOT
  (S
    (NP
      (NP (DT The) (NN purpose))
      (PP (IN of)
        (NP (DT the) (JJ spiritual) (NN realm))))
    (VP (VBZ has)
      (ADVP (RB never))
      (VP
        (VP (VBN been)
          (S
            (VP (TO to)
              (VP (VB say)
                (NP (WRB how))))))
        (, ,)
        (CC but)
        (VP
          (FRAG
            (WHADVP (WRB why))))))
    (. .)))

det(purpose-2, The-1)
nsubj(been-9, purpose-2)
xsubj(say-11, purpose-2)
det(realm-6, the-4)
amod(realm-6, spiritual-5)
prep_of(purpose-2, realm-6)
aux(been-9, has-7)
neg(been-9, never-8)
root(ROOT-0, been-9)
aux(say-11, to-10)
xcomp(been-9, say-11)
