(ROOT
  (S
    (NP (PRP I))
    (VP (VBZ apologize)
      (PP (IN for)
        (S
          (VP (VBG using)
            (NP (JJ misleading) (NN wording))))))
    (. .)))

nsubj(apologize-2, I-1)
root(ROOT-0, apologize-2)
prepc_for(apologize-2, using-4)
amod(wording-6, misleading-5)
dobj(using-4, wording-6)

