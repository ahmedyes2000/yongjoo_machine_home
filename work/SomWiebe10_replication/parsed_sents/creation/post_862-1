(ROOT
  (S
    (NP (DT This))
    (VP (VBZ is)
      (NP
        (NP (DT a) (JJ valid) (NN discussion))
        (PP (IN between)
          (NP (CD two) (JJ creationist) (NNS groups)))))
    (. .)))

nsubj(discussion-5, This-1)
cop(discussion-5, is-2)
det(discussion-5, a-3)
amod(discussion-5, valid-4)
root(ROOT-0, discussion-5)
num(groups-9, two-7)
amod(groups-9, creationist-8)
prep_between(discussion-5, groups-9)

