(ROOT
  (S ('' '')
    (S
      (SBAR (IN While)
        (S
          (NP (NNP God))
          (VP (VBZ has)
            (VP (VBN been)
              (ADVP (RB mostly))))))
      (VP (VBD removed)))
    (IN from)
    (S
      (NP (DT these))
      (VP (VBZ books))
      (, ,))
    (. ...)))

mark(been-5, While-2)
nsubj(been-5, God-3)
aux(been-5, has-4)
csubj(removed-7, been-5)
advmod(been-5, mostly-6)
root(ROOT-0, removed-7)
dep(removed-7, from-8)
nsubj(books-10, these-9)
ccomp(removed-7, books-10)

