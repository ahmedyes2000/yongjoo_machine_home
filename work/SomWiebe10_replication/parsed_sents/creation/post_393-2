(ROOT
  (S
    (S
      (NP (DT The) (NN conclusion))
      (VP (VBZ is)
        (ADJP (JJ inescapable))))
    (: :)
    (S
      (NP (DT this) (NN structure))
      (VP (VBZ demonstrates)
        (NP
          (NP (DT the) (NN existence))
          (PP (IN of)
            (NP
              (NP (DT an) (JJ evolutionary) (NN driver) (NN operating))
              (SBAR
                (WHADVP (WRB where))
                (S
                  (NP (JJS least))
                  (VP (VBD expected)))))))))
    (. .)))

det(conclusion-2, The-1)
nsubj(inescapable-4, conclusion-2)
cop(inescapable-4, is-3)
root(ROOT-0, inescapable-4)
det(structure-7, this-6)
nsubj(demonstrates-8, structure-7)
parataxis(inescapable-4, demonstrates-8)
det(existence-10, the-9)
dobj(demonstrates-8, existence-10)
det(operating-15, an-12)
amod(operating-15, evolutionary-13)
nn(operating-15, driver-14)
prep_of(existence-10, operating-15)
advmod(expected-18, where-16)
nsubj(expected-18, least-17)
rcmod(operating-15, expected-18)

