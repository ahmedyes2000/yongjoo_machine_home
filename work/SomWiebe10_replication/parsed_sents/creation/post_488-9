  (S
    (NP (PRP I))
    (VP (VBP remember)
      (PP (IN in)
        (NP (DT the) (JJ eighth) (NN grade)))
      (, ,)
      (SBAR
        (WHADVP (WRB when))
        (S
          (NP (PRP we))
          (VP (VBD had)
            (S
              (VP (TO to)
                (VP (VB learn)
                  (PP (IN about)
                    (NP (DT the) (NNP Moon) (NNP Hoax)))
                  (PP (IN as)
                    (NP
                      (NP (DT a) (NN part))
                      (PP (IN of)
                        (NP (NN science) (NN class))))))))))))
    (. .)))

