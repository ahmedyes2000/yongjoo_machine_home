(ROOT
  (S
    (S
      (NP (EX There))
      (VP (VBP are)
        (NP
          (NP (NNS Christians))
          (SBAR
            (WHNP (WP who))
            (S
              (VP (VBP believe)
                (NP (JJ other) (NNS foolishness))))))))
    (, ,)
    (CC but)
    (S
      (NP (PRP they))
      (VP (VBP are)
        (ADVP (RB still))
        (NP (NNS Christians))))
    (. .)))

expl(are-2, There-1)
root(ROOT-0, are-2)
nsubj(are-2, Christians-3)
nsubj(believe-5, Christians-3)
rcmod(Christians-3, believe-5)
amod(foolishness-7, other-6)
dobj(believe-5, foolishness-7)
nsubj(Christians-13, they-10)
cop(Christians-13, are-11)
advmod(Christians-13, still-12)
conj_but(are-2, Christians-13)

