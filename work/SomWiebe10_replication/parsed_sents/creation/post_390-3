(ROOT
  (S
    (NP (DT The) (NN term))
    (VP
      (ADVP (RB simply))
      (VBZ refers)
      (PP (TO to)
        (NP
          (NP (DT a) (JJ basic) (NN set))
          (PP (IN of)
            (NP (CD three)))))
      (S
        (VP (VBG turning)
          (S
            (NP (NNS points)
              (CC or)
              (NNS transitions))
            (NP
              (NP
                (ADJP (JJ visible)
                  (PP (IN in)
                    (NP (NN world))))
                (NN history))
              (, ,)
              (NP
                (NP (DT the) (NN birth))
                (PP (IN of)
                  (NP
                    (NP (NN civilization))
                    (, ,)
                    (NP (DT the) (JJ classical) (NN period))
                    (, ,))))
              (PP (IN with)
                (NP (PRP$ its) (JJ remarkable) (JJ so-called) (NN Axial) (NN Age)))
              (: --))
            (PP (IN Of)
              (NP (NN course)))))))
    (: ...)))

det(term-2, The-1)
nsubj(refers-4, term-2)
advmod(refers-4, simply-3)
root(ROOT-0, refers-4)
det(set-8, a-6)
amod(set-8, basic-7)
prep_to(refers-4, set-8)
