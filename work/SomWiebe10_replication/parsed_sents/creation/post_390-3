amod(history-18, visible-15)
prep_in(visible-15, world-17)
xcomp(turning-11, history-18)
det(birth-21, the-20)
appos(history-18, birth-21)
prep_of(birth-21, civilization-23)
det(period-27, the-25)
amod(period-27, classical-26)
appos(civilization-23, period-27)
poss(Age-34, its-30)
amod(Age-34, remarkable-31)
