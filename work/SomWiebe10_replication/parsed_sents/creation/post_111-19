(ROOT
  (S
    (NP (PRP You))
    (VP (VBP are)
      (VP (VBG kidding)
        (ADVP (RB right))))
    (. ?)))

nsubj(kidding-3, You-1)
aux(kidding-3, are-2)
root(ROOT-0, kidding-3)
advmod(kidding-3, right-4)

(ROOT
  (S
    (INTJ (UH No))
    (, ,)
    (NP (PRP I))
    (VP (VBP m) (RB not))
    (. .)))

dep(m-4, No-1)
nsubj(m-4, I-3)
root(ROOT-0, m-4)
neg(m-4, not-5)

