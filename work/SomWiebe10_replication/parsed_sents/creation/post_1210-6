(ROOT
  (S
    (NP (PRP I))
    (VP (MD could)
      (VP (VB go)
        (PRT (RP on))
        (PP (IN for)
          (NP
            (NP (NNS days))
            (PP (IN about)
              (NP (DT the) (NN proof)))))))
    (. .)))

nsubj(go-3, I-1)
aux(go-3, could-2)
root(ROOT-0, go-3)
