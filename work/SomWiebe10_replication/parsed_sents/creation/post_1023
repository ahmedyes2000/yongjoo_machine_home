(ROOT
  (S
    (ADVP (RB hey))
    (, ,)
    (NP (FW i))
    (VP (VBP feel)
      (NP (DT the) (JJ same) (NN way)))
    (. !)))

advmod(feel-4, hey-1)
nsubj(feel-4, i-3)
root(ROOT-0, feel-4)
det(way-7, the-5)
amod(way-7, same-6)
dobj(feel-4, way-7)

(ROOT
  (SINV
    (ADVP (RB almost))
    (VP (VBD took)
      (NP (DT the) (NN brain)))
    (NP
      (NP (NNS cells))
      (PP (RB out) (IN of)
        (NP (PRP$ my) (NN head))))
    (. !)))

advmod(took-2, almost-1)
root(ROOT-0, took-2)
det(brain-4, the-3)
dobj(took-2, brain-4)
nsubj(took-2, cells-5)
poss(head-9, my-8)
prep_out_of(cells-5, head-9)

