(ROOT
  (S (: --)
    (SBAR
      (WHADVP (WRB When))
      (S
        (VP (VBN held)
          (PP (TO to)
            (NP
              (NP (DT the) (NNS standards))
              (PP (IN of)
                (NP
                  (NP (NN proof))
                  (VP (VBN required)
                    (PP (IN for)
                      (NP (DT all) (JJ other) (JJ scientific) (NNS theories)))))))))))
    (, ,)
    (NP (NNP ID))
    (VP (VBZ fails))
    (. .)))

advmod(held-3, When-2)
advcl(fails-17, held-3)
parataxis(fails-17, held-3)
det(standards-6, the-5)
prep_to(held-3, standards-6)
prep_of(standards-6, proof-8)
partmod(proof-8, required-9)
det(theories-14, all-11)
amod(theories-14, other-12)
amod(theories-14, scientific-13)
prep_for(required-9, theories-14)
nsubj(fails-17, ID-16)
root(ROOT-0, fails-17)

(ROOT
  (S
    (NP (NNP Science))
    (VP (VBZ does) (RB not)
      (VP (VB deal)
        (PP (IN in)
          (NP
            (NP (NN proof))
            (CONJP (CC but)
              (RB rather))
            (NP (DT the) (NN ability))))
        (S
          (VP (TO to)
            (VP (VB withstand)
              (S
                (NP (NNS attempts))
                (VP (TO to)
                  (VP (VB disprove)))))))))
    (. .)))

nsubj(deal-4, Science-1)
xsubj(withstand-12, Science-1)
aux(deal-4, does-2)
neg(deal-4, not-3)
root(ROOT-0, deal-4)
prep_in(deal-4, proof-6)
det(ability-10, the-9)
