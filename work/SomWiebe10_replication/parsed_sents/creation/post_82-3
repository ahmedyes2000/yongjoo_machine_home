(ROOT
  (S
    (PP (IN Like)
      (NP (JJ much) (JJ great) (NN art)))
    (, ,)
    (NP (PRP they))
    (VP (MD can)
      (VP (VB tell)
        (NP (DT a) (NN truth))
        (PP (IN without)
          (S
            (VP (VBG representing)
              (NP (NN fact)))))))
    (. .)))

amod(art-4, much-2)
amod(art-4, great-3)
prep_like(tell-8, art-4)
nsubj(tell-8, they-6)
aux(tell-8, can-7)
root(ROOT-0, tell-8)
det(truth-10, a-9)
dobj(tell-8, truth-10)
prepc_without(tell-8, representing-12)
dobj(representing-12, fact-13)

