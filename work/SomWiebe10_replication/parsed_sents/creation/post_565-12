(ROOT
  (SBARQ
    (WHNP (WDT What) (NN predictions))
    (SQ (MD can)
      (VP (VB be)
        (VP (VBN made)
          (S
            (VP (VBG using)
              (NP
                (NP (DT the) (NN theory))
                (PP (IN of)
                  (NP (NNP ID)))))))))
    (. ?)))

det(predictions-2, What-1)
nsubjpass(made-5, predictions-2)
aux(made-5, can-3)
