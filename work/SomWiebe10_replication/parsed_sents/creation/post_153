(ROOT
  (S
    (NP (EX There))
    (VP (VBZ is)
      (VP (VB know)
        (NP
          (NP (NN bang))
          (SBAR
            (WHNP (WDT that))
            (S
              (VP (MD can)
                (VP (VB do)
                  (NP (DT all)))))))))
    (. .)))

expl(is-2, There-1)
root(ROOT-0, is-2)
xcomp(is-2, know-3)
nsubj(is-2, bang-4)
