(ROOT
  (S
    (NP (NNP Biology))
    (VP (MD can)
      (VP (VB be)
        (VP (VBN studied)
          (PP (IN in)
            (NP (DT a) (JJ creationist) (NN context))))))
    (. .)))

nsubjpass(studied-4, Biology-1)
aux(studied-4, can-2)
auxpass(studied-4, be-3)
root(ROOT-0, studied-4)
det(context-8, a-6)
amod(context-8, creationist-7)
prep_in(studied-4, context-8)

