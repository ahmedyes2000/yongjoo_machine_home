(ROOT
  (SBAR (IN If)
    (S
      (NP (PRP$ your))
      (VP (VBG trying)
        (S
          (VP (TO to)
            (VP (VB find)
              (NP (NN evidence))
              (PP (IN in)
                (NP
                  (NP (DT the) (NNP Bible))
                  (, ,)
                  (NP (PRP$ your) (NN right)))))))))
    (: ...)))

mark(trying-3, If-1)
nsubj(trying-3, your-2)
xsubj(find-5, your-2)
root(ROOT-0, trying-3)
aux(find-5, to-4)
xcomp(trying-3, find-5)
dobj(find-5, evidence-6)
det(Bible-9, the-8)
prep_in(find-5, Bible-9)
poss(right-12, your-11)
appos(Bible-9, right-12)

