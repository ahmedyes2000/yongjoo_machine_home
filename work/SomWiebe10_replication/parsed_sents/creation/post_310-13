(ROOT
  (S
    (ADVP (RB So))
    (, ,)
    (PP (IN for)
      (NP (NN science)
        (S
          (VP (TO to)
            (VP (VB reject)
              (NP (NNP ID)))))))
    (, ,)
    (NP (PRP it))
    (VP (MD must)
      (VP (VB be)
        (SBAR (IN because)
          (S
            (NP (PRP it))
            (VP (VBZ fails)
              (S
                (VP (TO to)
                  (VP (VB contribute)))))))))
    (. .)))

advmod(be-11, So-1)
prep_for(be-11, science-4)
aux(reject-6, to-5)
infmod(science-4, reject-6)
dobj(reject-6, ID-7)
nsubj(be-11, it-9)
aux(be-11, must-10)
root(ROOT-0, be-11)
mark(fails-14, because-12)
