(ROOT
  (S ('' '')
    (S
      (SBAR
        (WHNP (WP What))
        (S
          (NP (PRP we))
          (VP (VBP believe)
            (SBAR
              (S
                (VP (VBZ is)
                  (ADJP (JJ true))))))))
      (VP (VBZ is)
        (ADJP (RB quite) (JJ separate))))
    (IN from)
    (S
      (SBAR
        (WHNP (WP what))
        (S
          (NP (PRP we))
          (VP (MD can)
            (ADVP (RB scientifically))
            (VP (VB demonstrate)))))
      (VP (VBZ is)
        (ADJP (JJ true))))
    (. .)))

dep(believe-4, What-2)
nsubj(true-6, What-2)
nsubj(believe-4, we-3)
csubj(separate-9, believe-4)
cop(true-6, is-5)
ccomp(believe-4, true-6)
cop(separate-9, is-7)
advmod(separate-9, quite-8)
root(ROOT-0, separate-9)
dep(separate-9, from-10)
dobj(demonstrate-15, what-11)
nsubj(demonstrate-15, we-12)
aux(demonstrate-15, can-13)
advmod(demonstrate-15, scientifically-14)
csubj(true-17, demonstrate-15)
cop(true-17, is-16)
ccomp(separate-9, true-17)

