(ROOT
  (S
    (S
      (NP (PRP He))
      (ADVP (RB never))
      (VP (VBZ sets)))
    (: ;)
    (S
      (ADVP (RB indeed))
      (NP (PRP he))
      (VP
        (ADVP (RB never))
        (VBZ sets)))
    (. .)))

nsubj(sets-3, He-1)
neg(sets-3, never-2)
root(ROOT-0, sets-3)
advmod(sets-8, indeed-5)
nsubj(sets-8, he-6)
neg(sets-8, never-7)
parataxis(sets-3, sets-8)

