(ROOT
  (S
    (S
      (NP (NNP ID))
      (VP (VBZ is)
        (ADJP (JJ useless)
          (PP (TO to)
            (NP (PRP me))))
        (PP (IN as)
          (NP (DT a) (NN scientist)))))
    (, ,) (IN so)
    (S
      (NP (PRP I))
      (VP (VBP do) (RB n't)
        (VP (VB bother)
          (PP (IN with)
            (NP (PRP it)))
          (PP (IN in)
            (NP (DT the) (NN lab))))))
    (. .)))

nsubj(useless-3, ID-1)
cop(useless-3, is-2)
root(ROOT-0, useless-3)
prep_to(useless-3, me-5)
det(scientist-8, a-7)
