(ROOT
  (S
    (NP (PRP They))
    (ADVP
      (ADVP (RB all)
        (NP
          (NP (NNS kinds))
          (PP (IN of)
            (NP (NN evidence)))))
      (NP (DT that) (NN evolution)))
    (VP (VBZ exists))
    (. .)))

nsubj(exists-8, They-1)
advmod(exists-8, all-2)
pobj(all-2, kinds-3)
prep_of(kinds-3, evidence-5)
det(evolution-7, that-6)
dep(all-2, evolution-7)
root(ROOT-0, exists-8)

(ROOT
  (SBARQ
    (WHNP (WP What))
    (SQ (VBP are)
      (NP (PRP you))
      (VP (VBG suggesting)))
    (. ?)))

dobj(suggesting-4, What-1)
aux(suggesting-4, are-2)
nsubj(suggesting-4, you-3)
root(ROOT-0, suggesting-4)

(ROOT
  (S
    (NP (DT That))
    (NP (PRP we))
    (VP (VBD were)
      (ADVP (RB just))
      (ADVP (RB here)))
    (. ?)))

nsubj(were-3, That-1)
nsubj(were-3, we-2)
root(ROOT-0, were-3)
advmod(were-3, just-4)
advmod(were-3, here-5)

