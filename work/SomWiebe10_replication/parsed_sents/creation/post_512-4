(ROOT
  (S
    (NP
      (NP (DT The) (NN government) (POS 's))
      (NN action))
    (VP (MD must)
      (VP (VB have)
        (NP
          (NP (DT a) (JJ secular) (JJ legislative) (NN purpose))
          (: ;)
          (NP (CD 2)))))
    (. .)))

det(government-2, The-1)
poss(action-4, government-2)
nsubj(have-6, action-4)
aux(have-6, must-5)
root(ROOT-0, have-6)
det(purpose-10, a-7)
amod(purpose-10, secular-8)
amod(purpose-10, legislative-9)
dobj(have-6, purpose-10)
dep(purpose-10, 2-12)

