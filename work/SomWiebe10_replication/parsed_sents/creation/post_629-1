(ROOT
  (S ('' '')
    (S (CC But)
      (NP (DT the) (NN question))
      (VP (VBZ is)))
    (: :)
    (S
      (S
        (VP (VBZ does)
          (NP (JJ intelligent) (NN design))))
      (VP (VBP have)
        (NP (JJ scientific) (NN merit))))
    (. ?)))

cc(is-5, But-2)
det(question-4, the-3)
nsubj(is-5, question-4)
root(ROOT-0, is-5)
csubj(have-10, does-7)
amod(design-9, intelligent-8)
dobj(does-7, design-9)
parataxis(is-5, have-10)
amod(merit-12, scientific-11)
dobj(have-10, merit-12)

