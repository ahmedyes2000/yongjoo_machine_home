(ROOT
  (S
    (INTJ (UH OK))
    (, ,)
    (NP (PRP you))
    (VP (VBP are)
      (ADVP (RB right)
        (PP (IN on)
          (NP (DT this) (NN point))))
      (ADVP (RB as) (RB well)))
    (. .)))

dep(are-4, OK-1)
nsubj(are-4, you-3)
root(ROOT-0, are-4)
advmod(are-4, right-5)
det(point-8, this-7)
prep_on(right-5, point-8)
advmod(well-10, as-9)
mwe(well-10, as-9)
advmod(are-4, well-10)

