(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP (VBP give)
          (NP (NN way))
          (PP (TO to)
            (NP
              (NP (DT the) (JJ very) (NNS foundations))
              (PP (IN of)
                (NP (DT the) (NN faith))))))))
    (, ,)
    (NP (PRP they))
    (VP (VBP have)
      (VP (VBN won)
        (NP
          (NP (DT the) (NN battle))
          (PP (IN in)
            (NP (DT the) (JJ long) (NN run))))))
    (. .)))

