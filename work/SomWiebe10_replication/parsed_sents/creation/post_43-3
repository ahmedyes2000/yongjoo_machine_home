(ROOT
  (S
    (NP (PRP One))
    (VP (MD should) (RB not)
      (VP (VB confuse)
        (NP
          (NP (DT this))
          (PP (IN with)
            (NP (NNP ID))))
        (S
          (VP (VBG having)
            (NP (DT a) (JJ meaningfull) (NN manner))
            (PP (TO to)
              (NP (NN science)))))))
    (. .)))

nsubj(confuse-4, One-1)
aux(confuse-4, should-2)
neg(confuse-4, not-3)
root(ROOT-0, confuse-4)
dobj(confuse-4, this-5)
prep_with(this-5, ID-7)
xcomp(confuse-4, having-8)
det(manner-11, a-9)
amod(manner-11, meaningfull-10)
dobj(having-8, manner-11)
prep_to(having-8, science-13)

