(ROOT
  (S
    (SBAR
      (WHADVP (WRB How)
        (ADJP (IN about)))
      (S
        (S
          (VP (`` ``) (VB turn)
            (NP (DT the) (JJ other) (NN cheek))
            ('' '')))
        (: ...)
        (CC or)
        (S
          (VP (`` ``) (VBP spare)
            (NP (DT the) (NN rod))))))
    (, ,)
    (VP (VBP spoil)
      (NP (DT the) (NN child) ('' '')))
    (: ...)))

advmod(turn-4, How-1)
dep(How-1, about-2)
dep(spoil-16, turn-4)
det(cheek-7, the-5)
amod(cheek-7, other-6)
dobj(turn-4, cheek-7)
conj_or(turn-4, spare-12)
dep(spoil-16, spare-12)
det(rod-14, the-13)
dobj(spare-12, rod-14)
