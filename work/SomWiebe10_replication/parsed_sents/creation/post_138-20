(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP (VBP do))))
    (, ,)
    (ADVP (RB then))
    (NP (PRP you))
    (VP (VBP have)
      (NP (JJR more) (NN faith))
      (PP (IN than)
        (NP (JJS most) (NNS people))))
    (. .)))

mark(do-3, If-1)
nsubj(do-3, you-2)
advcl(have-7, do-3)
advmod(have-7, then-5)
nsubj(have-7, you-6)
root(ROOT-0, have-7)
amod(faith-9, more-8)
dobj(have-7, faith-9)
amod(people-12, most-11)
prep_than(have-7, people-12)

