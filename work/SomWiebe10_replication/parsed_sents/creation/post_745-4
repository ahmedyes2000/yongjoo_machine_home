(ROOT
  (S
    (S
      (VP (VB Amongst)
        (NP (JJ other) (NNS things))))
    (, ,)
    (NP (PRP they))
    (VP (VBD believed)
      (SBAR (IN that)
        (PRN
          (S
            (NP (DT the) (NN earth))
            (VP (VBD was)
              (ADJP (JJ flat)
                (NP (CD .3)))))
          (-RRB- -RRB-))
        (S
          (NP (DT No) (NN one))
          (ADVP (RB even))
          (VP (VBZ knows)
            (SBAR
              (WHNP (WP who))
              (S
                (VP (VBD wrote)
                  (NP (NNP Genesis)))))))))
    (. .)))

ccomp(believed-6, Amongst-1)
amod(things-3, other-2)
dobj(Amongst-1, things-3)
nsubj(believed-6, they-5)
root(ROOT-0, believed-6)
complm(knows-17, that-7)
det(earth-9, the-8)
nsubj(flat-11, earth-9)
cop(flat-11, was-10)
dep(knows-17, flat-11)
dep(flat-11, .3-12)
det(one-15, No-14)
