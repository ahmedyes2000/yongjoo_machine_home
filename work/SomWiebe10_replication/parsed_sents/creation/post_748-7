(ROOT
  (S
    (NP
      (NP (DT The) (NNS questions))
      (PP (IN of)
        (NP
          (NP (NN evil))
          (PP (IN in)
            (NP (DT the) (NN world))))))
    (VP (MD might)
      (VP (VB be)
        (ADJP (JJ difficult)
          (PP (IN for)
            (NP (PRP you))))))
    (. .) (. .)))

det(questions-2, The-1)
nsubj(difficult-10, questions-2)
prep_of(questions-2, evil-4)
det(world-7, the-6)
prep_in(evil-4, world-7)
aux(difficult-10, might-8)
cop(difficult-10, be-9)
root(ROOT-0, difficult-10)
prep_for(difficult-10, you-12)

