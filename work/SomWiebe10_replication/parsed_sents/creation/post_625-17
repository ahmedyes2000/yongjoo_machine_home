(ROOT
  (S
    (NP
      (NP (NNP Unless))
      (PP (IN of)
        (NP (NN course))))
    (ADVP (RB there))
    (VP (VBZ is)
      (NP
        (NP (DT no) (JJ such) (NN thing))
        (PP (IN as)
          (NP (NN intelligence)))))
    (. .)))

nsubj(thing-8, Unless-1)
prep_of(Unless-1, course-3)
advmod(thing-8, there-4)
cop(thing-8, is-5)
det(thing-8, no-6)
amod(thing-8, such-7)
root(ROOT-0, thing-8)
prep_as(thing-8, intelligence-10)

