(ROOT
  (S
    (NP (PRP She))
    (VP (VBD said)
      (SBAR
        (S
          (NP (PRP she))
          (VP (VBZ 's)
            (ADVP (RB never))
            (VP (VBN seen)
              (S
                (NP (DT an) (NN ape))
                (VP (VB evolve)
                  (PP (IN into)
                    (NP (DT a) (NN human))))))))))
    (. .)))

nsubj(said-2, She-1)
root(ROOT-0, said-2)
nsubjpass(seen-6, she-3)
auxpass(seen-6, 's-4)
neg(seen-6, never-5)
ccomp(said-2, seen-6)
det(ape-8, an-7)
nsubj(evolve-9, ape-8)
ccomp(seen-6, evolve-9)
det(human-12, a-11)
prep_into(evolve-9, human-12)

