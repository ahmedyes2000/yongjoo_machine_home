    (NP (PRP you))
    (VP (MD should)
      (VP (VB have)
        (VP (VBD said)
          (SBAR
            (S
              (NP (EX there))
              (VP (VBZ is)
                (NP (DT no) (`` `) (JJ convincing) ('' ') (NN evidence))))))))
    (. .)))

