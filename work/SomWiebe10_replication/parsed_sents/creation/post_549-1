(ROOT
  (S
    (NP (PRP$ Our) (NN nation))
    (ADVP (RB no) (RB longer))
    (VP (VBZ has)
      (S
        (VP (TO to)
          (VP (VB listen)
            (PP (TO to)
              (NP
                (NP (DT the) (NNS lies))
                (PP (IN of)
                  (NP (DT the) (NNS media)))))))))
    (. .)))

poss(nation-2, Our-1)
nsubj(has-5, nation-2)
xsubj(listen-7, nation-2)
advmod(longer-4, no-3)
advmod(has-5, longer-4)
root(ROOT-0, has-5)
aux(listen-7, to-6)
xcomp(has-5, listen-7)
det(lies-10, the-9)
prep_to(listen-7, lies-10)
det(media-13, the-12)
prep_of(lies-10, media-13)

