(ROOT
  (S
    (NP (PRP I))
    (VP (VBP 'm)
      (ADJP
        (ADJP (RB truly) (RB not) (JJ sure))
        (SBAR (IN that)
          (S
            (NP (DT a) (CD 6) (NN day) (NN creation))
            (VP (VBZ is)
              (VP (VBN hindered)
                (PP (IN by)
                  (NP (DT an) (JJ old) (NN earth)))
                (ADVP (RB anyway)))))
          (. .))))
    (. .)))

nsubj(sure-5, I-1)
cop(sure-5, 'm-2)
advmod(sure-5, truly-3)
neg(sure-5, not-4)
root(ROOT-0, sure-5)
complm(hindered-12, that-6)
det(creation-10, a-7)
num(creation-10, 6-8)
nn(creation-10, day-9)
nsubjpass(hindered-12, creation-10)
auxpass(hindered-12, is-11)
ccomp(sure-5, hindered-12)
det(earth-16, an-14)
amod(earth-16, old-15)
agent(hindered-12, earth-16)
advmod(hindered-12, anyway-17)

