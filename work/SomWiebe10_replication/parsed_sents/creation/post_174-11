(ROOT
  (S
    (NP (PRP I))
    (VP (MD would)
      (VP (VB like)
        (S
          (VP (TO to)
            (VP (VB see)
              (S
                (NP (JJ real) (NNS scientists))
                (VP (VB do)
                  (NP
                    (NP (JJ real) (NNS tests))
                    (PP (IN of)
                      (NP (NNP ID)))))))))))
    (. .)))

nsubj(like-3, I-1)
xsubj(see-5, I-1)
aux(like-3, would-2)
root(ROOT-0, like-3)
aux(see-5, to-4)
