(ROOT
  (S
    (NP (PRP I))
    (VP (VBP believe)
      (SBAR
        (S
          (NP (NN creationism))
          (VP (VBZ develops)
            (NP (DT a) (NN story)
              (S
                (VP (TO to)
                  (VP (VB teach)
                    (NP (NNS children))
                    (SBAR
                      (WHADVP (WRB how))
                      (S
                        (VP (TO to)
                          (VP (VB understand)
                            (NP (DT the) (NN world))))))))))))))
    (. .)))

nsubj(believe-2, I-1)
root(ROOT-0, believe-2)
nsubj(develops-4, creationism-3)
ccomp(believe-2, develops-4)
det(story-6, a-5)
dobj(develops-4, story-6)
aux(teach-8, to-7)
infmod(story-6, teach-8)
dobj(teach-8, children-9)
advmod(understand-12, how-10)
aux(understand-12, to-11)
xcomp(teach-8, understand-12)
det(world-14, the-13)
dobj(understand-12, world-14)

