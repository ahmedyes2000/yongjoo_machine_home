  (S (CC And)
    (NP (DT that))
    (VP (VBZ explains)
      (ADVP (RB fully))
      (SBAR
        (WHADVP (WRB why))
        (S
          (NP (PRP you))
          (VP (MD 'd)
            (VP (VB go)
              (PP (IN with)
                (NP
                  (NP (DT the) (NN fantasy))
                  (PP (RB instead) (IN of)
                    (S
                      (VP (VBG learning)
                        (PP (IN about)
