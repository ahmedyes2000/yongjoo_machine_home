(ROOT
  (S
    (S
      (NP (PRP It))
      (VP (VBZ does) (RB n't)
        (VP (VB matter)
          (SBAR (IN if)
            (S
              (NP (DT the) (NN Creation))
              (VP (VBD happened)
                (PP (IN in)
                  (NP
                    (NP (CD 6) (NNS days))
                    (CC or)
                    (NP (CD 6) (NN millennia))))))))))
    (, ,)
    (CC but)
    (S
      (NP (PRP I))
      (ADVP (RB always))
      (VP (VB choose)
        (NP (DT the) (JJ former))))
    (. .)))

