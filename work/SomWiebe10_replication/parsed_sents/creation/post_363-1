(ROOT
  (S
    (ADVP (RB Otherwise))
    (NP (DT the) (NN word) (`` ``) (NN artifact) ('' ''))
    (VP (MD would) (RB n't)
      (VP (VB have)
        (NP (DT any) (NN meaning))))
    (. .)))

advmod(have-9, Otherwise-1)
det(artifact-5, the-2)
nn(artifact-5, word-3)
nsubj(have-9, artifact-5)
aux(have-9, would-7)
neg(have-9, n't-8)
root(ROOT-0, have-9)
det(meaning-11, any-10)
dobj(have-9, meaning-11)

