(ROOT
  (FRAG
    (NP
      (ADJP (JJ Laying))
      (JJ false) (NN evidence))
    (ADJP (IN about)
      (S
        (VP (TO to)
          (VP (VB ensure)
            (SBAR (IN that)
              (S
                (NP (NNS people))
                (VP (VBP go)
                  (ADJP (JJ wrong)))))))))
    (. ?)))

amod(evidence-3, Laying-1)
amod(evidence-3, false-2)
root(ROOT-0, evidence-3)
dep(evidence-3, about-4)
aux(ensure-6, to-5)
xcomp(about-4, ensure-6)
