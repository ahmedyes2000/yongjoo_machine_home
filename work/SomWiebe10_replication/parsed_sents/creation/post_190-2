(ROOT
  (S
    (NP (DT The) (NN problem))
    (VP (VBD was)
      (ADJP (RB even) (RBR more) (JJ serious))
      (PP (IN at)
        (NP
          (NP (DT the) (NN level))
          (PP (IN of)
            (NP (DT the) (JJR higher) (NNS categories))))))
    (. .)))

det(problem-2, The-1)
nsubj(serious-6, problem-2)
cop(serious-6, was-3)
advmod(serious-6, even-4)
advmod(serious-6, more-5)
root(ROOT-0, serious-6)
det(level-9, the-8)
prep_at(serious-6, level-9)
det(categories-13, the-11)
amod(categories-13, higher-12)
prep_of(level-9, categories-13)

