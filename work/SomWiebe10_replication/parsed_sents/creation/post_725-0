(ROOT
  (S
    (NP (DT some) (NNS people))
    (VP (VBP see)
      (NP (PRP it))
      (SBAR (IN as)
        (S
          (NP
            (NP (DT a) (NN day))
            (PP (TO to)
              (NP (DT the) (NN lord))))
          (VP (VBZ is)
            (NP (DT a) (CD 1000) (NNS years))))))
    (. .)))

det(people-2, some-1)
nsubj(see-3, people-2)
root(ROOT-0, see-3)
dobj(see-3, it-4)
mark(years-14, as-5)
det(day-7, a-6)
nsubj(years-14, day-7)
det(lord-10, the-9)
prep_to(day-7, lord-10)
cop(years-14, is-11)
det(years-14, a-12)
num(years-14, 1000-13)
advcl(see-3, years-14)

