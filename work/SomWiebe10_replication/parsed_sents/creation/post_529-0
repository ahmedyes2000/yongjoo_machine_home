(ROOT
  (S (: --)
    (NP (NNP ID))
    (VP (VBZ explains)
      (NP (DT the) (JJ genetic) (NN code))
      (NP
        (NP (DT the) (JJ same) (NN way))
        (SBAR
          (S
            (NP (PRP we))
            (VP (VBP explain)
              (NP (DT a) (NN computer) (NN code)))))))
    (. .)))

nsubj(explains-3, ID-2)
root(ROOT-0, explains-3)
det(code-6, the-4)
amod(code-6, genetic-5)
iobj(explains-3, code-6)
det(way-9, the-7)
amod(way-9, same-8)
dobj(explains-3, way-9)
nsubj(explain-11, we-10)
rcmod(way-9, explain-11)
det(code-14, a-12)
nn(code-14, computer-13)
dobj(explain-11, code-14)

