(ROOT
  (S
    (NP (JJ Intelligent) (NN design))
    (VP (VBZ does) (RB not)
      (VP (VB try)
        (S
          (VP (TO to)
            (VP (VB address)
              (NP (DT those) (NNS questions))
              (ADVP (RB either)))))))
    (. .)))

amod(design-2, Intelligent-1)
nsubj(try-5, design-2)
xsubj(address-7, design-2)
aux(try-5, does-3)
neg(try-5, not-4)
root(ROOT-0, try-5)
aux(address-7, to-6)
xcomp(try-5, address-7)
det(questions-9, those-8)
dobj(address-7, questions-9)
advmod(address-7, either-10)

