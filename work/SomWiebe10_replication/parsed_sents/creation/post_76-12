  (S
    (NP (DT That) (NNS scientists))
    (VP (MD can)
      (VP (DT both)
        (VP (VB accept)
          (NP
            (NP (DT the) (JJ religious) (NN concept))
            (PP (IN of)
              (NP (`` ``) (NN design) ('' '')))))
        (CC and)
        (VP
          (ADVP (RB still))
          (VB do)
          (NP (NN science))
          (PP (IN without)
            (S
              (VP (VBG accepting)
                (NP
                  (NP (DT the)
                    (ADJP (JJ flawed)
                      (CC and)
                      (JJ scientifically))
                    (JJ vacuous) (NNS approaches))
                  (PP (IN of)
                    (NP (NNP ID))))))))))
    (. .)))

