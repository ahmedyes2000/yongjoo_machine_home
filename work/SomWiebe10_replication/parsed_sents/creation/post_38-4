(ROOT
  (S
    (NP (DT This))
    (VP (VBZ is)
      (ADVP (RB very) (RB likely))
      (NP (DT an) (JJ evolutionary) (NN adaptation)))
    (. .)))

nsubj(adaptation-7, This-1)
cop(adaptation-7, is-2)
advmod(likely-4, very-3)
advmod(adaptation-7, likely-4)
det(adaptation-7, an-5)
amod(adaptation-7, evolutionary-6)
root(ROOT-0, adaptation-7)

