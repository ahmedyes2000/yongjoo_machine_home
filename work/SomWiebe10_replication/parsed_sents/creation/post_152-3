(ROOT
  (S
    (SBAR (RB Even) (IN if)
      (S
        (NP (DT some) (NN force))
        (VP (VBD created)
          (NP (PRP us)))))
    (, ,)
    (NP (NNS humans))
    (VP (MD should) (RB n't)
      (VP (VB take)
        (PP (IN for)
          (S
            (VP (JJ granted))))
        (SBAR (IN that)
          (S
            (NP (PRP it))
            (VP (VBD did)
              (NP (PRP it))
              (SBAR (RB so) (IN that)
                (S
                  (NP (PRP we))
                  (VP (VBZ worship)
                    (NP (PRP it))))))))))
    (. .)))

advmod(created-5, Even-1)
dep(created-5, if-2)
det(force-4, some-3)
nsubj(created-5, force-4)
advcl(take-11, created-5)
dobj(created-5, us-6)
nsubj(take-11, humans-8)
aux(take-11, should-9)
neg(take-11, n't-10)
root(ROOT-0, take-11)
prepc_for(take-11, granted-13)
complm(did-16, that-14)
nsubj(did-16, it-15)
ccomp(take-11, did-16)
dobj(did-16, it-17)
advmod(worship-21, so-18)
dep(worship-21, that-19)
nsubj(worship-21, we-20)
advcl(did-16, worship-21)
dobj(worship-21, it-22)

