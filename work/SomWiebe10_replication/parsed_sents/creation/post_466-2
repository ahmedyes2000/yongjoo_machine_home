(ROOT
  (NP
    (NP
      (NP (NNP EVIDENCE))
      (PP (IN OF)
        (NP (DT AN) (NN INTENT)
          (S
            (VP (TO TO)
              (VP (VB DISCRIMINATE)
                (NP
                  (NP (NNP AGAINST) (NNP SCIENTISTS) (NNP BASED))
                  (PP (IN ON)
                    (NP (NNP THEIR) (NNP OUTSIDE) (NNP ACTIVITIES) (NNP REGARDING) (NNP EVOLUTION))))))))))
    (: ...)))

root(ROOT-0, EVIDENCE-1)
det(INTENT-4, AN-3)
prep_of(EVIDENCE-1, INTENT-4)
aux(DISCRIMINATE-6, TO-5)
infmod(INTENT-4, DISCRIMINATE-6)
nn(BASED-9, AGAINST-7)
nn(BASED-9, SCIENTISTS-8)
dobj(DISCRIMINATE-6, BASED-9)
nn(EVOLUTION-15, THEIR-11)
nn(EVOLUTION-15, OUTSIDE-12)
