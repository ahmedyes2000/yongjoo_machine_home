(ROOT
  (S
    (NP (NNP ID))
    (VP (VBZ is) (RB not)
      (ADJP (JJ scientific))
      (, ,)
      (ADVP
        (ADVP (RB certainly) (RB not))
        (PP (IN in)
          (NP (DT an) (JJ honest) (NN fashion)))))
    (. .)))

nsubj(scientific-4, ID-1)
cop(scientific-4, is-2)
neg(scientific-4, not-3)
root(ROOT-0, scientific-4)
advmod(not-7, certainly-6)
advmod(scientific-4, not-7)
det(fashion-11, an-9)
amod(fashion-11, honest-10)
prep_in(not-7, fashion-11)

