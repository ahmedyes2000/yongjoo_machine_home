(ROOT
  (S
    (NP
      (NP (DT A)
        (ADJP (RBR more) (JJ interesting))
        (NN thought))
      (PP (IN for)
        (NP (PRP me))))
    (VP (VBZ is)
      (SBAR (IN that)
        (S
          (PP (IN in)
            (NP
              (NP (NNP Buddhism))
              (, ,)
              (NP (DT the) (NN question))))
          (NP (PRP itself))
          (VP (VBZ is)
            (VP (VBN considered)
              (S
                (ADJP (JJ irrelevant))))))))
    (. .)))

det(thought-4, A-1)
advmod(interesting-3, more-2)
amod(thought-4, interesting-3)
nsubj(is-7, thought-4)
