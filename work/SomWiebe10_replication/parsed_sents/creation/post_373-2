(ROOT
  (S
    (NP (DT That))
    (VP (VBZ is) (, ,)
      (S
        (NP (PRP they))
        (RB all)
        (VP (VB reflect)
          (NP
            (NP (DT the) (NN work))
            (PP (IN of)
              (NP (DT an) (JJ intelligent) (NN agent)))))))
    (. .)))

nsubj(is-2, That-1)
root(ROOT-0, is-2)
nsubj(reflect-6, they-4)
advmod(reflect-6, all-5)
ccomp(is-2, reflect-6)
det(work-8, the-7)
dobj(reflect-6, work-8)
det(agent-12, an-10)
amod(agent-12, intelligent-11)
prep_of(work-8, agent-12)

