(ROOT
  (S
    (NP (NNS Scientists))
    (VP (VBP take)
      (NP (PRP it))
      (NP
        (NP (RB as) (DT a))
        (PP (VBN given)
          (SBAR
            (WHADVP (WRB when))
            (S
              (NP (PRP it))
              (VP (VBZ is) (RB not)
                (NP (DT a) (JJ given))))))))
    (. .)))

nsubj(take-2, Scientists-1)
root(ROOT-0, take-2)
iobj(take-2, it-3)
advmod(a-5, as-4)
dobj(take-2, a-5)
prep(a-5, given-6)
advmod(given-12, when-7)
nsubj(given-12, it-8)
