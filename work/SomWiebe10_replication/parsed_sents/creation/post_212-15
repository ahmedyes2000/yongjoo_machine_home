(ROOT
  (S (-LRB- -LRB-)
    (NP (VB keep))
    (ADVP (RB in))
    (VP (VBD mind)
      (SBAR
        (S
          (NP (PRP I))
          (VP (VBP use)
            (NP (DT the) (NN word) (NN nature)))))
      (. .))
    (. .)))

nsubj(mind-4, keep-2)
advmod(mind-4, in-3)
root(ROOT-0, mind-4)
nsubj(use-6, I-5)
ccomp(mind-4, use-6)
det(nature-9, the-7)
nn(nature-9, word-8)
dobj(use-6, nature-9)

