(ROOT
  (S
    (SBAR (IN That)
      (S
        (NP (PRP you))
        (VP (VB capitalize)
          (NP (DT the) (NN term)))))
    (VP
      (VP (VBZ demonstrates)
        (NP (PRP$ your) (NN veneration))
        (PP (IN for)
          (NP (PRP it))))
      (, ,)
      (CC and)
      (ADVP (RB quite))
      (VP
        (ADVP (RB simply))
        (VBZ demonstrates)
        (SBAR
          (WHADVP (WRB why))
          (S
            (NP (DT this))
            (VP (VBZ is)
              (NP
                (NP (DT an) (NN argument))
                (PP (IN between)
                  (NP (NN religion)))))))))
    (. .)))

complm(capitalize-3, That-1)
nsubj(capitalize-3, you-2)
csubj(demonstrates-6, capitalize-3)
csubj(demonstrates-15, capitalize-3)
det(term-5, the-4)
dobj(capitalize-3, term-5)
root(ROOT-0, demonstrates-6)
poss(veneration-8, your-7)
dobj(demonstrates-6, veneration-8)
