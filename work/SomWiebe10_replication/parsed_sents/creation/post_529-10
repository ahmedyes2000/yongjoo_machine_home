(ROOT
  (S
    (NP (NNP ROTFL))
    (, ,)
    (ADVP (JJ next) (NN time))
    (NP (PRP you))
    (VP (VBP defend)
      (NP
        (NP (NNP ID))
        (PP (IN in)
          (NP (NN court)))))
    (: ...)))

nsubj(defend-6, ROTFL-1)
advmod(defend-6, next-3)
dep(next-3, time-4)
nsubj(defend-6, you-5)
root(ROOT-0, defend-6)
dobj(defend-6, ID-7)
prep_in(ID-7, court-9)

