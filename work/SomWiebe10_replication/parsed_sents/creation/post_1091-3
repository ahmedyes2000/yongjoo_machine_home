(ROOT
  (S
    (NP (PRP I))
    (VP (VBZ pity)
      (NP
        (NP (DT the) (NN fool))
        (SBAR
          (WHNP (WP who))
          (S
            (VP (VBZ thinks)
              (SBAR
                (S
                  (NP (PRP they))
                  (VP
                    (VP (MD can))
                    (CC or)
                    (VP (VBP have))))))))))
    (. .)))

nsubj(pity-2, I-1)
root(ROOT-0, pity-2)
det(fool-4, the-3)
dobj(pity-2, fool-4)
nsubj(thinks-6, fool-4)
rcmod(fool-4, thinks-6)
nsubj(can-8, they-7)
nsubj(have-10, they-7)
ccomp(thinks-6, can-8)
ccomp(thinks-6, have-10)
conj_or(can-8, have-10)

