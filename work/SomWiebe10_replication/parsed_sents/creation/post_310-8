(ROOT
  (SQ (VBZ Is)
    (NP (EX there))
    (NP
      (NP (NN evidence))
      (PP (IN for)
        (NP
          (NP (NNP ID))
          (PP (IN in)
            (NP (NN nature))))))
    (. .)))

root(ROOT-0, Is-1)
expl(Is-1, there-2)
nsubj(Is-1, evidence-3)
prep_for(evidence-3, ID-5)
prep_in(ID-5, nature-7)

