(ROOT
  (S ('' '')
    (S
      (NP
        (NP (NNP Psalms) (CD 24))
        (: :)
        (NP
          (NP (PRP He))
          (SBAR
            (WHNP (WP who))
            (S
              (VP (VBZ spares)
                (NP (DT the) (NN rod)))))))
      (VP (VBZ hates)
        (NP (PRP$ his) (NN son))))
    (, ,)
    (CC but)
    (S
      (NP
        (NP (PRP he))
        (SBAR
          (WHNP (WP who))
          (S
            (VP (VBZ loves)
              (NP (PRP him))))))
      (VP (VBZ is)
        (ADJP (JJ careful)
          (S
            (VP (TO to)
              (VP (VB discipline)
                (NP (PRP him))))))))
    (. .)))

