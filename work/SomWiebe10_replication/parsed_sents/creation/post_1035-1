(ROOT
  (S
    (PP (IN In)
      (NP (NN fact)))
    (NP
      (NP (RB even) (DT the) (JJ fundamental) (NNS theories))
      (PP (IN of)
        (NP
          (NP (NNS sciences))
          (ADVP (RB still)))))
    (VP (VBP have)
      (NP
        (NP (DT a) (JJ surprising) (NN number))
        (PP (IN of)
          (NP (NNS holes)))))
    (. .)))

prep_in(have-10, fact-2)
advmod(theories-6, even-3)
det(theories-6, the-4)
amod(theories-6, fundamental-5)
nsubj(have-10, theories-6)
prep_of(theories-6, sciences-8)
advmod(sciences-8, still-9)
root(ROOT-0, have-10)
det(number-13, a-11)
amod(number-13, surprising-12)
dobj(have-10, number-13)
prep_of(number-13, holes-15)

