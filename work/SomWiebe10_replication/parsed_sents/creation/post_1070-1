(ROOT
  (S
    (S
      (ADVP (RB Generally))
      (NP (EX there))
      (VP (VBP are)
        (NP
          (NP (JJ many) (NNS supporters))
          (VP (VBG existing)
            (PP (IN for)
              (NP (DT the) (NN creationism)))))))
    (CC but)
    (S
      (NP (PRP they))
      (VP (VBP are)
        (ADVP (RB just))
        (VP (VBG living)
          (NP
            (NP (NN life))
            (PP (IN in)
              (NP (PRP$ their) (JJ own) (NN utopia)))))))
    (. .)))

