(ROOT
  (S
    (NP (PRP You))
    (VP (MD may)
      (VP (VB be)
        (ADJP (JJ right)
          (PP (IN about)
            (NP (PRP him))))
        (S (RB not)
          (VP (VBG losing)
            (NP (PRP$ his) (NN job))))))
    (. .)))

nsubj(right-4, You-1)
aux(right-4, may-2)
cop(right-4, be-3)
root(ROOT-0, right-4)
prep_about(right-4, him-6)
neg(losing-8, not-7)
xcomp(right-4, losing-8)
poss(job-10, his-9)
dobj(losing-8, job-10)

