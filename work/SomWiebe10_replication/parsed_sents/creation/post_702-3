(ROOT
  (S
    (S
      (NP (DT That))
      (VP (VBZ is)
        (ADVP (RB also))
        (SBAR
          (WHADVP (WRB how))
          (S
            (NP (PRP I))
            (VP (VBP feel)
              (PP (IN about)
                (NP (DT this) (NN issue))))))))
    (, ,)
    (CC and)
    (S
      (NP (PRP I))
      (VP (MD would)
        (VP (VB appreciate)
          (NP
            (NP (NN everyone) (POS 's))
            (NN respect))
          (PP (IN on)
            (NP (PRP$ my) (NN opinion))))))
    (. .)))

nsubj(is-2, That-1)
root(ROOT-0, is-2)
advmod(is-2, also-3)
advmod(feel-6, how-4)
nsubj(feel-6, I-5)
ccomp(is-2, feel-6)
det(issue-9, this-8)
prep_about(feel-6, issue-9)
nsubj(appreciate-14, I-12)
aux(appreciate-14, would-13)
conj_and(is-2, appreciate-14)
poss(respect-17, everyone-15)
dobj(appreciate-14, respect-17)
poss(opinion-20, my-19)
prep_on(appreciate-14, opinion-20)

