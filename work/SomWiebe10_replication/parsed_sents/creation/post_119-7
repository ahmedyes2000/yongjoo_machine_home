(ROOT
  (S
    (ADVP (DT No) (NN matter))
    (NP
      (NP (DT the) (NN species))
      (, ,)
      (NP
        (NP (DT a) (JJ great) (NN amount))
        (PP (IN of)
          (NP (NN examination)
            (CC and)
            (NN decision-making)))))
    (VP (VBZ is)
      (VP (VBN involved)
        (NP (CD one) (NN generation))
        (PP (TO to)
          (NP (DT the) (JJ next)))))
    (. .)))

dep(matter-2, No-1)
advmod(involved-14, matter-2)
det(species-4, the-3)
nsubjpass(involved-14, species-4)
det(amount-8, a-6)
amod(amount-8, great-7)
appos(species-4, amount-8)
prep_of(amount-8, examination-10)
prep_of(amount-8, decision-making-12)
conj_and(examination-10, decision-making-12)
auxpass(involved-14, is-13)
root(ROOT-0, involved-14)
num(generation-16, one-15)
dobj(involved-14, generation-16)
det(next-19, the-18)
prep_to(involved-14, next-19)

