(ROOT
  (S
    (S
      (NP (DT The) (JJ fossil) (NN record))
      (VP (VBZ shows)
        (NP
          (NP (DT no) (NN change))
          (PP (IN for)
            (NP
              (NP (DT a) (JJ long) (NNS periods))
              (PP (IN of)
                (NP (NN time))))))))
    (, ,) (RB then)
    (S
      (ADVP (RB abruptly))
      (NP (JJ new) (NNS species))
      (VP (VBD appeared)))
    (. .)))

det(record-3, The-1)
amod(record-3, fossil-2)
nsubj(shows-4, record-3)
root(ROOT-0, shows-4)
det(change-6, no-5)
dobj(shows-4, change-6)
det(periods-10, a-8)
amod(periods-10, long-9)
prep_for(change-6, periods-10)
prep_of(periods-10, time-12)
advmod(shows-4, then-14)
advmod(appeared-18, abruptly-15)
amod(species-17, new-16)
nsubj(appeared-18, species-17)
ccomp(shows-4, appeared-18)

