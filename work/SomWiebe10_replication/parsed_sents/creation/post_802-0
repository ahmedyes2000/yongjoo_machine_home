(ROOT
  (S
    (NP (JJ Many) (NNS people))
    (VP (VBP claim)
      (SBAR (IN that)
        (S
          (NP (JJ certain) (NNS writings))
          (VP (VBP are)
            (NP
              (NP (DT the) (NNP Word))
              (PP (IN of)
                (NP (NNP God))))))))
    (. .)))

amod(people-2, Many-1)
nsubj(claim-3, people-2)
root(ROOT-0, claim-3)
complm(Word-9, that-4)
amod(writings-6, certain-5)
nsubj(Word-9, writings-6)
cop(Word-9, are-7)
det(Word-9, the-8)
ccomp(claim-3, Word-9)
prep_of(Word-9, God-11)

