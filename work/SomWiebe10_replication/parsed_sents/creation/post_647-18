(ROOT
  (S
    (NP
      (NP (RB Even) (DT the) (JJ expert) (NNS witnesses))
      (PP (IN for)
        (NP
          (NP (DT the) (NN defense))
          (, ,)
          (NP (NNP Minnich))
          (CC and)
          (NP (NNP Fuller)))))
    (VP (VBD had)
      (S
        (VP (TO to)
          (VP (VB admit)
            (NP (DT this))))))
    (. .)))

advmod(witnesses-4, Even-1)
det(witnesses-4, the-2)
amod(witnesses-4, expert-3)
nsubj(had-12, witnesses-4)
xsubj(admit-14, witnesses-4)
det(defense-7, the-6)
prep_for(witnesses-4, defense-7)
