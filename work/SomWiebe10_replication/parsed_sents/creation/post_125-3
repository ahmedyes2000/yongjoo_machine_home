(ROOT
  (S
    (PP (IN As)
      (NP
        (NP (JJ such))
        (SBAR
          (WHADVP (WRB when))
          (S
            (NP (DT an) (NN animal))
            (`` ``)
            (VP (VBD evolved))
            ('' '')))))
    (NP
      (NP (DT the) (JJ immune) (NN system))
      (PP (IN of)
        (NP (PRP it))))
    (VP (VBZ 's)
      (ADJP (JJ ancestor))
      (S
        (VP (VB evolve)
          (PP (IN with)
            (NP (PRP it))))))
    (. .)))

prep_such_as(ancestor-15, such-2)
advmod(evolved-7, when-3)
det(animal-5, an-4)
nsubj(evolved-7, animal-5)
dep(ancestor-15, evolved-7)
det(system-11, the-9)
amod(system-11, immune-10)
nsubj(ancestor-15, system-11)
prep_of(system-11, it-13)
cop(ancestor-15, 's-14)
root(ROOT-0, ancestor-15)
ccomp(ancestor-15, evolve-16)
prep_with(evolve-16, it-18)

