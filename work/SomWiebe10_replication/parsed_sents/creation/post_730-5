(ROOT
  (S
    (S
      (NP (NNP Gravity))
      (VP (VBZ is)
        (NP (NN fact))))
    (, ,)
    (S
      (NP (NNP Evolution))
      (VP (VBZ is)
        (NP (NN fact))))
    (, ,)
    (S
      (NP (NNP Creationism))
      (VP (VBZ is)
        (ADJP (RB so) (JJ ludicrous)
          (CC and)
          (JJ moronic)
          (SBAR (IN that)
            (S
              (NP (PRP it))
              (VP (VBZ is) (RB not)
                (ADJP (JJ worth))
                (ADVP (RB even))
                (VP (VBG discussing))))))))
    (. .)))

nsubj(fact-3, Gravity-1)
cop(fact-3, is-2)
root(ROOT-0, fact-3)
nsubj(fact-7, Evolution-5)
cop(fact-7, is-6)
ccomp(fact-3, fact-7)
