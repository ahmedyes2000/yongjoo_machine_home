(ROOT
  (S (CC or)
    (NP
      (QP (DT a) (CD billion))
      (NNS years))
    (ADVP (CC but))
    (VP (VBZ is)
      (ADJP (RB that) (JJ important)))
    (. ?)))

cc(important-8, or-1)
quantmod(billion-3, a-2)
num(years-4, billion-3)
nsubj(important-8, years-4)
advmod(important-8, but-5)
cop(important-8, is-6)
advmod(important-8, that-7)
root(ROOT-0, important-8)

