(ROOT
  (S
    (NP (PRP You))
    (VP (MD ca) (RB n't)
      (VP (VB offer)
        (ADVP (RB anymore))
        (PP (IN than)
          (NP
            (NP (NN theory))
            (PP (IN for)
              (NP
                (NP (CD one))
                (CC or)
                (NP (DT the) (JJ other))))))))
    (. .)))

nsubj(offer-4, You-1)
aux(offer-4, ca-2)
neg(offer-4, n't-3)
root(ROOT-0, offer-4)
advmod(offer-4, anymore-5)
prep_than(offer-4, theory-7)
prep_for(theory-7, one-9)
det(other-12, the-11)
prep_for(theory-7, other-12)
conj_or(one-9, other-12)

