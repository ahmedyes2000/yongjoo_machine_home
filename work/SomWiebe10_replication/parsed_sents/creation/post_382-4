(ROOT
  (INTJ (UH No) (. !)))

root(ROOT-0, No-1)

(ROOT
  (S
    (NP
      (NP (DT The) (NN history))
      (PP (IN of)
        (NP (NN science))))
    (VP (VBZ is)
      (ADJP (JJ full)
        (PP (IN of)
          (NP
            (NP (NNS examples))
            (PP (IN of)
              (NP (NNS scientists))))))
      (S
        (VP (VBG ignoring)
          (NP (NN evidence) (NN contrary))
          (PP (TO to)
            (NP (JJ established) (NN theory))))))
    (. .)))

det(history-2, The-1)
nsubj(full-6, history-2)
prep_of(history-2, science-4)
cop(full-6, is-5)
root(ROOT-0, full-6)
prep_of(full-6, examples-8)
prep_of(examples-8, scientists-10)
xcomp(full-6, ignoring-11)
nn(contrary-13, evidence-12)
