(ROOT
  (S
    (NP (PRP I))
    (VP (VBD was)
      (NP
        (NP (DT a) (JJ happy) (NN person))
        (PP (IN as)
          (NP (DT an) (NN atheist)))))
    (. .) (. .)))

nsubj(person-5, I-1)
cop(person-5, was-2)
det(person-5, a-3)
amod(person-5, happy-4)
root(ROOT-0, person-5)
det(atheist-8, an-7)
prep_as(person-5, atheist-8)

