(ROOT
  (S
    (NP (JJ Medieval) (NNS scholars))
    (VP (VBN translated)
      (NP (NN yom))
      (PP (IN as)
        (NP (NN day)))
      (PP (IN in)
        (NP (NNP English))))
    (. .)))

amod(scholars-2, Medieval-1)
nsubj(translated-3, scholars-2)
root(ROOT-0, translated-3)
dobj(translated-3, yom-4)
prep_as(translated-3, day-6)
prep_in(translated-3, English-8)

