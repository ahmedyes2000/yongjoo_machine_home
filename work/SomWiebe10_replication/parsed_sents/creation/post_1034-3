(ROOT
  (S
    (SBAR (IN That) (IN if)
      (S
        (NP (PRP you))
        (VP (VBP look)
          (PP (IN into)
            (NP (DT all) (NNS beliefs)
              (CC and)
              (NNS theories))))))
    (, ,)
    (NP (PRP you))
    (VP (MD will)
      (VP (VB find)
        (NP (DT a) (NN flaw))))
    (. .)))

complm(look-4, That-1)
dep(look-4, if-2)
nsubj(look-4, you-3)
advcl(find-13, look-4)
ccomp(find-13, look-4)
det(beliefs-7, all-6)
prep_into(look-4, beliefs-7)
prep_into(look-4, theories-9)
conj_and(beliefs-7, theories-9)
nsubj(find-13, you-11)
aux(find-13, will-12)
root(ROOT-0, find-13)
det(flaw-15, a-14)
dobj(find-13, flaw-15)

