(ROOT
  (S
    (PP (IN From)
      (NP (PRP$ your) (JJ further) (NN answer)))
    (PRN
      (NP (PRP I))
      (VP (VBP take)
        (S
          (NP (PRP it))
          (NP (DT that)))))
    (NP (PRP you) (DT a))
    (-RRB- -RRB-)
    (VP (VBP have)
      (VP (VBN read)
        (NP (JJ other) (NNS books))))
    (. .)))

poss(answer-4, your-2)
amod(answer-4, further-3)
prep_from(read-13, answer-4)
nsubj(take-6, I-5)
parataxis(read-13, take-6)
nsubj(that-8, it-7)
xcomp(take-6, that-8)
nsubj(read-13, you-9)
dep(you-9, a-10)
aux(read-13, have-12)
root(ROOT-0, read-13)
amod(books-15, other-14)
dobj(read-13, books-15)

(ROOT
  (S
    (LST (LS b) (-RRB- -RRB-))
    (VP (VB Have)
      (NP
        (NP (DT no) (NN evidence))
        (PP (IN outside)
          (NP (DT a) (JJ single) (NN source)))))
    (. .)))

dep(Have-3, b-1)
root(ROOT-0, Have-3)
det(evidence-5, no-4)
dobj(Have-3, evidence-5)
det(source-9, a-7)
amod(source-9, single-8)
prep_outside(evidence-5, source-9)

