(ROOT
  (S ('' '')
    (S
      (NP (DT Those))
      (VP (VBP do)
        (VP (VB play)
          (NP (DT a) (NN role)))))
    (, ,)
    (CC but)
    (S
      (NP (PRP it))
      (ADVP (RB really))
      (VP (VBZ depends)
        (PP (IN on)
          (NP
            (NP (DT the) (NN feature))
            (PP (IN in)
              (NP (NN question)))))))
    (. .)))

nsubj(play-4, Those-2)
aux(play-4, do-3)
root(ROOT-0, play-4)
det(role-6, a-5)
dobj(play-4, role-6)
nsubj(depends-11, it-9)
advmod(depends-11, really-10)
conj_but(play-4, depends-11)
det(feature-14, the-13)
prep_on(depends-11, feature-14)
prep_in(feature-14, question-16)

