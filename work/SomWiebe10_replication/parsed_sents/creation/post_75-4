(ROOT
  (S
    (ADVP (RB Furthermore))
    (, ,)
    (NP (DT the) (NN research))
    (VP (VBZ shows)
      (SBAR
        (S
          (NP
            (NP (DT a) (NN change))
            (PP (IN of)
              (NP (PRP I))))
          (VP (VBP believe)
            (ADVP
              (NP (CD 18) (NNS nucleotides))
              (IN out))
            (PP (IN of)
              (NP
                (QP (CD 80)
                  (CC or)
                  (RB so))))
            (, ,)
            (ADJP (JJ responsible)
              (PP (IN for)
                (NP (DT the) (NN regulation))))))))
    (. .)))

advmod(shows-5, Furthermore-1)
det(research-4, the-3)
nsubj(shows-5, research-4)
root(ROOT-0, shows-5)
det(change-7, a-6)
nsubj(believe-10, change-7)
prep_of(change-7, I-9)
ccomp(shows-5, believe-10)
num(nucleotides-12, 18-11)
npadvmod(believe-10, nucleotides-12)
prep_out_of(believe-10, 80-15)
prep_out_of(believe-10, so-17)
conj_or(80-15, so-17)
quantmod(80-15, so-17)
acomp(believe-10, responsible-19)
det(regulation-22, the-21)
prep_for(responsible-19, regulation-22)

