(ROOT
  (S
    (NP (PRP They))
    (VP (VBP believe)
      (SBAR
        (S
          (NP (DT every) (NN word))
          (VP (VBD was)
            (VP (VBN overseen)
              (PP (IN by)
                (NP (NNP God))))))))
    (. .)))

nsubj(believe-2, They-1)
root(ROOT-0, believe-2)
det(word-4, every-3)
nsubjpass(overseen-6, word-4)
auxpass(overseen-6, was-5)
ccomp(believe-2, overseen-6)
agent(overseen-6, God-8)

