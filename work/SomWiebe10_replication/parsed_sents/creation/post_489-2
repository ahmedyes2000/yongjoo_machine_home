(ROOT
  (S
    (ADVP (LS Second))
    (, ,)
    (NP (PRP I))
    (VP (VBP 'm) (RB not)
      (ADJP (JJ sure)
        (SBAR
          (S
            (NP (PRP I))
            (VP (VBP follow)
              (NP (PRP$ your) (NN argument)))))))
    (. .)))

advmod(sure-6, Second-1)
nsubj(sure-6, I-3)
cop(sure-6, 'm-4)
neg(sure-6, not-5)
root(ROOT-0, sure-6)
nsubj(follow-8, I-7)
dep(sure-6, follow-8)
poss(argument-10, your-9)
dobj(follow-8, argument-10)

