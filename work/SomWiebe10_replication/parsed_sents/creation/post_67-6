(ROOT
  (S (CC And)
    (NP (EX there))
    (VP (VBP are)
      (NP
        (QP (JJR more) (IN than)
          (NP (RB just) (CD two) (NNS options)))))
    (. .)))

cc(are-3, And-1)
expl(are-3, there-2)
root(ROOT-0, are-3)
quantmod(than-5, more-4)
nsubj(are-3, than-5)
advmod(options-8, just-6)
num(options-8, two-7)
dep(than-5, options-8)

