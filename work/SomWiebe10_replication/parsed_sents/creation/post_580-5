  (S
    (NP
      (NP (JJ Religious) (NNS motives))
      (PP (IN of)
        (NP (NNP ID) (NNS proponents))))
    (VP (VBP help)
      (VP (VB keep)
        (NP (JJ non-science) (NN nonsense))
        (PRT (RP out))
        (PP (IN of)
          (NP (DT the) (JJ public) (JJ high) (NN school) (NN classroom)))))
    (. .)))

