(ROOT
  (S
    (PP (IN For)
      (NP (PRP you)))
    (, ,) (`` ``)
    (NP (PRP I))
    (VP (VBP know)
      (NP (PRP it))
      (SBAR
        (WHADVP (WRB when))
        (S
          (NP (PRP I))
          (VP (VBP see)
            (SBAR
              (S
                (NP (PRP it))
                ('' '')
                (VP (VBZ works)
                  (ADJP (JJ fine))
                  (SBAR (IN because)
                    (S
                      (NP (PRP you))
                      (VP (VBP are)
                        (NP (DT an) (NN idiot))))))))))))
    (. .)))

prep_for(know-6, you-2)
nsubj(know-6, I-5)
root(ROOT-0, know-6)
dobj(know-6, it-7)
advmod(see-10, when-8)
nsubj(see-10, I-9)
advcl(know-6, see-10)
nsubj(works-13, it-11)
ccomp(see-10, works-13)
acomp(works-13, fine-14)
mark(idiot-19, because-15)
nsubj(idiot-19, you-16)
cop(idiot-19, are-17)
det(idiot-19, an-18)
advcl(works-13, idiot-19)

