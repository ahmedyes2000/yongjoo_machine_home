  (S
    (S
      (VP (VBG Looking)
        (PP (IN for)
          (NP
            (NP (NN design))
            (PP (IN in)
              (NP (NN nature)))))))
    (VP (VBZ is)
      (NP (DT a) (JJ great) (NN way)
        (S
          (VP (TO to)
            (VP (VB do)
              (NP (NN science)))))))
    (. .)))

csubj(way-9, Looking-1)
prep_for(Looking-1, design-3)
prep_in(design-3, nature-5)
