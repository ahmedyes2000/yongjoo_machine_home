(ROOT
  (S
    (NP (NNP Granted) (NNP creationism))
    (VP (VBZ is) (RB not)
      (ADJP (JJ provable)
        (PP (IN through)
          (NP
            (NP (DT the) (NN use))
            (PP (IN of)
              (NP (DT the) (JJ scientific) (NN method)))))))
    (. .)))

nn(creationism-2, Granted-1)
nsubj(provable-5, creationism-2)
cop(provable-5, is-3)
neg(provable-5, not-4)
root(ROOT-0, provable-5)
det(use-8, the-7)
prep_through(provable-5, use-8)
det(method-12, the-10)
amod(method-12, scientific-11)
prep_of(use-8, method-12)

