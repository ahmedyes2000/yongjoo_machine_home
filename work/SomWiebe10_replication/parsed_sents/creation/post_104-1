(ROOT
  (S
    (NP (PRP I))
    (VP (VBP do) (RB n't)
      (VP (VB anticipate)
        (NP (DT any) (NN collapse))
        (: ...)
        (SBARQ
          (WHADVP (WRB why))
          (SQ (MD would)
            (NP (PRP it))
            (ADVP (RB still))
            (VP (VB be)
              (VP (VBG accelerating))))
          (. ?))))))

nsubj(anticipate-4, I-1)
aux(anticipate-4, do-2)
neg(anticipate-4, n't-3)
root(ROOT-0, anticipate-4)
det(collapse-6, any-5)
dobj(anticipate-4, collapse-6)
advmod(accelerating-13, why-8)
aux(accelerating-13, would-9)
nsubj(accelerating-13, it-10)
advmod(accelerating-13, still-11)
aux(accelerating-13, be-12)
parataxis(anticipate-4, accelerating-13)

(ROOT
  (S
    (NP (DT the) (NNP Bible))
    (VP (VBZ mentions)
      (NP (DT a) (NN hint))
      (NP (CD 11) (NNS times) (RB even))
      (PP (IN in)
        (NP (CD 5) (NNS chapters))))
    (: ...)))

det(Bible-2, the-1)
nsubj(mentions-3, Bible-2)
root(ROOT-0, mentions-3)
det(hint-5, a-4)
iobj(mentions-3, hint-5)
num(times-7, 11-6)
dobj(mentions-3, times-7)
advmod(times-7, even-8)
num(chapters-11, 5-10)
prep_in(mentions-3, chapters-11)

