(ROOT
  (S
    (NP (DT a) (NN conspicuously))
    (VP (VBD planned)
      (NP
        (NP (NN appearance))
        (SBAR
          (WHNP (WDT which))
          (S
            (VP (VBZ defies)
              (NP (JJ logical) (NN explanation))
              (PP (IN by)
                (NP (NN chance))))))))
    (. .)))

det(conspicuously-2, a-1)
nsubj(planned-3, conspicuously-2)
root(ROOT-0, planned-3)
dobj(planned-3, appearance-4)
nsubj(defies-6, appearance-4)
rcmod(appearance-4, defies-6)
amod(explanation-8, logical-7)
dobj(defies-6, explanation-8)
prep_by(defies-6, chance-10)

