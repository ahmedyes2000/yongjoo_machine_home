(ROOT
  (S
    (NP (PRP It))
    (VP (VBD made)
      (NP (NNS predictions))
      (SBAR (IN that)
        (S
          (NP (JJ further) (NN investigation))
          (VP (VBD found)
            (S
              (VP (TO to)
                (VP (VB be)
                  (ADJP (JJ false)))))))))
    (. .)))

nsubj(made-2, It-1)
root(ROOT-0, made-2)
dobj(made-2, predictions-3)
complm(found-7, that-4)
amod(investigation-6, further-5)
nsubj(found-7, investigation-6)
xsubj(false-10, investigation-6)
ccomp(made-2, found-7)
aux(false-10, to-8)
cop(false-10, be-9)
xcomp(found-7, false-10)

