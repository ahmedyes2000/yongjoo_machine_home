(ROOT
  (S
    (ADVP (LS Third))
    (, ,)
    (NP (EX there))
    (VP (VBZ is)
      (NP
        (NP (DT no) (JJ such) (NN thing))
        (PP (IN as)
          (NP (DT a) (VBG missing) (NN link)))))
    (. .)))

advmod(is-4, Third-1)
expl(is-4, there-3)
root(ROOT-0, is-4)
det(thing-7, no-5)
amod(thing-7, such-6)
nsubj(is-4, thing-7)
det(link-11, a-9)
amod(link-11, missing-10)
prep_as(thing-7, link-11)

