(ROOT
  (S ('' '')
    (ADVP (RB So))
    (, ,)
    (S
      (VP (TO to)
        (VP (VB be)
          (ADJP (RB very) (JJ clear)))))
    (, ,)
    (NP (NNP ID))
    (VP (VBZ posits)
      (SBAR (IN that)
        (S
          (NP (EX there))
          (VP (VBZ is)
            (NP
              (NP (DT an) (NN entity))
              (SBAR
                (WHNP (WDT which))
                (S
                  (VP (VBZ designs)
                    (NP (NNS things))))))))))
    (. .)))

advmod(posits-10, So-2)
aux(clear-7, to-4)
cop(clear-7, be-5)
advmod(clear-7, very-6)
purpcl(posits-10, clear-7)
nsubj(posits-10, ID-9)
root(ROOT-0, posits-10)
complm(is-13, that-11)
expl(is-13, there-12)
ccomp(posits-10, is-13)
det(entity-15, an-14)
nsubj(is-13, entity-15)
nsubj(designs-17, entity-15)
rcmod(entity-15, designs-17)
dobj(designs-17, things-18)

