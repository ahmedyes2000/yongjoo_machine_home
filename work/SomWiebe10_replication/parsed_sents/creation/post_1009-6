(ROOT
  (S
    (PP (IN For)
      (NP (DT the) (JJ same) (NN reason)))
    (, ,)
    (NP (PRP it))
    (VP (VBZ is) (RB not)
      (NP (DT a) (JJ one-generation) (NN process)))
    (. .)))

det(reason-4, the-2)
amod(reason-4, same-3)
prep_for(process-11, reason-4)
nsubj(process-11, it-6)
cop(process-11, is-7)
neg(process-11, not-8)
