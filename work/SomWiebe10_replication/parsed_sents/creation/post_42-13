(ROOT
  (S
    (NP (PRP You))
    (VP (VB be)
      (ADJP (JJ mad)
        (S
          (VP (TO to)
            (VP (VB continue)
              (NP (NN banking))
              (PP (IN with)
                (NP (PRP them))))))))
    (. .)))

nsubj(mad-3, You-1)
cop(mad-3, be-2)
root(ROOT-0, mad-3)
aux(continue-5, to-4)
xcomp(mad-3, continue-5)
dobj(continue-5, banking-6)
prep_with(continue-5, them-8)

