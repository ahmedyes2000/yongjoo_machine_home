(ROOT
  (S (: --)
    (NP
      (NP (DT The) (JJ bottom) (NN line))
      (SBAR
        (S
          (VP (VBZ is)
            (NP
              (NP (DT some) (NNS interpretations))
              (PP (IN of)
                (NP (NN evolution))))))))
    (VP (VBP leave)
      (NP
        (NP (JJ little) (NN room))
        (PP (IN for)
          (NP (DT a) (JJ caring) (NN God)))))
    (. .)))

det(line-4, The-2)
amod(line-4, bottom-3)
nsubj(leave-10, line-4)
cop(interpretations-7, is-5)
det(interpretations-7, some-6)
rcmod(line-4, interpretations-7)
prep_of(interpretations-7, evolution-9)
root(ROOT-0, leave-10)
amod(room-12, little-11)
dobj(leave-10, room-12)
det(God-16, a-14)
amod(God-16, caring-15)
prep_for(room-12, God-16)

