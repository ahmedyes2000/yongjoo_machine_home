(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP (VBP prove)
          (SBAR (IN that)
            (S
              (NP (PRP they))
              (VP (VBP re)
                (S
                  (VP (VBG building)
                    (PP (IN on)
                      (NP (DT the) (NN sand)))))))))))
    (, ,)
    (NP (EX there))
    (VP (VBZ s)
      (ADJP (NN nothing) (VBN left)))
    (. .)))

mark(prove-3, If-1)
nsubj(prove-3, you-2)
advcl(s-13, prove-3)
complm(re-6, that-4)
nsubj(re-6, they-5)
