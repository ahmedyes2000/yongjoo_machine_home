(ROOT
  (S
    (S
      (NP (JJ Separate) (NNS disciplines))
      (VP (VBP are)
        (ADVP (RB certainly))
        (ADJP (JJ fruitful))))
    (CC but)
    (S
      (NP (DT both) (NN science)
        (CC and)
        (NN theology))
      (VP (VBP teach)
        (NP (PRP us))
        (SBAR (IN that)
          (S
            (NP (DT a) (JJR bigger) (NN picture))
            (VP (VBZ adds)
              (NP (DT a) (JJ new) (NN dimension)))))))
    (. .)))

amod(disciplines-2, Separate-1)
nsubj(fruitful-5, disciplines-2)
cop(fruitful-5, are-3)
