(ROOT
  (S
    (SBAR
      (WHADVP (WRB WHEN))
      (S
        (NP (DT the) (NNS worlds)
          (CC or)
          (NNS EARTH))
        (VP (VBD was)
          (VP (VBN created)
            (PP (IN by)
              (NP (NNP GOD)))))))
    (NP (DT the) (NNS writings))
    (VP (VBP refer)
      (S
        (VP (TO to)))
      (, ,)
      (S (`` ``)
        (NP (DT the) (NN SPIRIT))
        (VP (VBD moved)
          (PP (IN upon)
            (NP (DT the)
              (NP (CD EARTH)
                (CC &))
              (CC or)
              (NP (NNS WATERS)))))
        ('' '')))
    (. ...)))

advmod(created-7, WHEN-1)
det(worlds-3, the-2)
nsubjpass(created-7, worlds-3)
conj_or(worlds-3, EARTH-5)
nsubjpass(created-7, EARTH-5)
auxpass(created-7, was-6)
advcl(refer-12, created-7)
agent(created-7, GOD-9)
det(writings-11, the-10)
nsubj(refer-12, writings-11)
xsubj(to-13, writings-11)
root(ROOT-0, refer-12)
xcomp(refer-12, to-13)
det(SPIRIT-17, the-16)
nsubj(moved-18, SPIRIT-17)
ccomp(refer-12, moved-18)
dep(EARTH-21, the-20)
prep_upon(moved-18, EARTH-21)
prep_upon(moved-18, WATERS-24)
conj_or(EARTH-21, WATERS-24)

