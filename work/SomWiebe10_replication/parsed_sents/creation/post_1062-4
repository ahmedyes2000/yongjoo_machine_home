(ROOT
  (S
    (NP
      (NP (DT An) (NN example))
      (PP (IN of)
        (NP (DT a) (VBN closed) (NN system))))
    (VP (VBZ is)
      (NP (DT a) (JJ small) (JJR lighter))
      (, ,)
      (SBAR (IN after)
        (S
          (NP (RB so) (JJ many))
          (VP (VBZ uses)
            (SBAR
              (S
                (NP (PRP it))
                (ADVP (RB just))
                (VP (MD wo) (RB n't)
                  (VP (VB work)
                    (ADVP (RB anymore))))))))))
    (. .)))

det(example-2, An-1)
nsubj(lighter-10, example-2)
det(system-6, a-4)
amod(system-6, closed-5)
prep_of(example-2, system-6)
cop(lighter-10, is-7)
det(lighter-10, a-8)
amod(lighter-10, small-9)
root(ROOT-0, lighter-10)
mark(uses-15, after-12)
