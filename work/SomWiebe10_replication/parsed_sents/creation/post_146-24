(ROOT
  (S
    (NP
      (ADJP (RB so) (JJ free))
      (NN will))
    (VP (VBZ is)
      (NP (DT another) (CD 1:1) (NN chance)))
    (. .) (. .)))

advmod(free-2, so-1)
amod(will-3, free-2)
nsubj(chance-7, will-3)
cop(chance-7, is-4)
det(chance-7, another-5)
num(chance-7, 1:1-6)
root(ROOT-0, chance-7)

