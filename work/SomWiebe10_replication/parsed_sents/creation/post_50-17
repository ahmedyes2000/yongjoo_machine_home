(ROOT
  (S
    (PP (IN By)
      (NP (JJ determinate) (NN principle)))
    (NP (PRP I))
    (VP (VBP refer)
      (PP (TO to)
        (NP
          (NP (DT a) (NN proposition))
          (PP (IN of)
            (NP (JJ central) (NN importance)))))
      (PP (TO to)
        (NP
          (NP (DT the) (NN theory))
          (PP (IN at)
            (NP (NN issue)))
          (SBAR
            (WHPP (IN in)
              (WHNP (WDT which)))
            (S
              (NP
                (NP (DT the) (NNS extensions))
                (PP (IN of)
                  (NP (PRP$ its) (NNS terms))))
              (VP (VBP are)
                (ADVP (RB clearly))
                (VP (VBN defined))))))))
    (. .)))

