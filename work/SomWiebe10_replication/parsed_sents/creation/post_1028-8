(ROOT
  (S
    (ADVP (RB Obviously))
    (NP (PRP we))
    (VP
      (ADVP (RB hardly))
      (VBP know)
      (NP (NN shit))
      (PP (IN about)
        (NP (DT the) (NNP Universe)
          (CC and)
          (NNP Time))))
    (. .)))

advmod(know-4, Obviously-1)
nsubj(know-4, we-2)
advmod(know-4, hardly-3)
root(ROOT-0, know-4)
dobj(know-4, shit-5)
det(Universe-8, the-7)
prep_about(know-4, Universe-8)
prep_about(know-4, Time-10)
conj_and(Universe-8, Time-10)

