(ROOT
  (S
    (S
      (NP (EX There))
      (VP (VBZ is)
        (NP (DT no) (NN reason)
          (S
            (VP (TO to)
              (VP (VB pick)
                (NP
                  (QP (RB just) (CD one)))
                (S
                  (VP (TO to)
                    (VP (VB believe)
                      (PRT (RP in)))))))))))
    (, ,) (IN so)
    (S
      (NP (PRP I))
      (VP (VBP guess)
        (SBAR
          (S
            (NP (PRP we))
            (VP (MD should)
              (VP (VB believe)
                (NP (NN everything))))))))
    (. .)))

expl(is-2, There-1)
root(ROOT-0, is-2)
det(reason-4, no-3)
nsubj(is-2, reason-4)
aux(pick-6, to-5)
infmod(reason-4, pick-6)
quantmod(one-8, just-7)
dobj(pick-6, one-8)
aux(believe-10, to-9)
xcomp(pick-6, believe-10)
prt(believe-10, in-11)
dep(is-2, so-13)
nsubj(guess-15, I-14)
ccomp(is-2, guess-15)
nsubj(believe-18, we-16)
aux(believe-18, should-17)
ccomp(guess-15, believe-18)
dobj(believe-18, everything-19)

