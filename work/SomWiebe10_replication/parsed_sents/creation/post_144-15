(ROOT
  (S
    (NP (PRP You))
    (ADVP (RB also))
    (VP (VBD missed)
      (NP (DT the) (NN point))
      (PP (IN about)
        (NP (NN purpose))))
    (. .)))

nsubj(missed-3, You-1)
advmod(missed-3, also-2)
root(ROOT-0, missed-3)
det(point-5, the-4)
dobj(missed-3, point-5)
prep_about(missed-3, purpose-7)

