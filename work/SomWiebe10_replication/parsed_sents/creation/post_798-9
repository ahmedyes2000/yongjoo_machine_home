(ROOT
  (S
    (S
      (NP (DT Some) (NNS things))
      (VP (VBP are)
        (NP
          (NP (DT a) (NN mystery))
          (PP (IN for)
            (ADJP (JJ sure))))))
    (, ,)
    (CC but)
    (S
      (NP (DT that))
      (VP (VBZ is)
        (SBAR
          (WHADVP (WRB why))
          (S
            (NP (PRP it))
            (VP (VBZ takes)
              (NP (NN faith))
              (S
                (VP (TO to)
                  (VP (VB please)
                    (NP (NNP God))))))))))
    (. .)))

det(things-2, Some-1)
nsubj(mystery-5, things-2)
cop(mystery-5, are-3)
det(mystery-5, a-4)
root(ROOT-0, mystery-5)
prep_for(mystery-5, sure-7)
nsubj(is-11, that-10)
conj_but(mystery-5, is-11)
advmod(takes-14, why-12)
nsubj(takes-14, it-13)
advcl(is-11, takes-14)
dobj(takes-14, faith-15)
aux(please-17, to-16)
xcomp(takes-14, please-17)
dobj(please-17, God-18)

