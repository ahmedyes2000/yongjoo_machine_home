(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP (VBP think)
          (SBAR
            (S
              (NP (EX there))
              (VP (VBZ is)
                (NP (DT a) (JJ different) (NN explanation))
                (ADVP (RB then))))))))
    (VP (VBP please)
      (VP (VB post)
        (NP (PRP it))))
    (. .)))

mark(think-3, If-1)
nsubj(think-3, you-2)
csubj(please-10, think-3)
expl(is-5, there-4)
ccomp(think-3, is-5)
det(explanation-8, a-6)
amod(explanation-8, different-7)
nsubj(is-5, explanation-8)
advmod(is-5, then-9)
root(ROOT-0, please-10)
xcomp(please-10, post-11)
dobj(post-11, it-12)

