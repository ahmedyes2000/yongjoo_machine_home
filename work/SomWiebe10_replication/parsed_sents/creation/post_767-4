(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP they))
        (ADVP (RB truly))
        (VP (VBP have)
          (VP (VBN found)
            (S
              (VP (VBG convincing)
                (NP (NN evidence))))))))
    (, ,)
    (NP (PRP they))
    (VP (MD should)
      (VP (VB be)
        (NP (NN household) (NNS names))))
    (. .)))

mark(found-5, If-1)
nsubj(found-5, they-2)
advmod(found-5, truly-3)
aux(found-5, have-4)
advcl(names-13, found-5)
xcomp(found-5, convincing-6)
dobj(convincing-6, evidence-7)
nsubj(names-13, they-9)
aux(names-13, should-10)
cop(names-13, be-11)
nn(names-13, household-12)
root(ROOT-0, names-13)

