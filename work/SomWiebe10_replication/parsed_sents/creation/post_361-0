  (S
    (NP
      (NP (DT The) (JJ only) (NN way))
      (SBAR (IN that)
        (S
          (NP (NN design))
          (VP (MD could)
            (VP (VB be)
              (VP (VBN detected)
                (PP (IN in)
                  (NP (NN nature)))))))))
    (VP (VBZ is)
      (SBAR (IN if)
        (S
          (NP (EX there))
          (VP (VBD were)
            (NP
              (NP (NNS objects))
              (PP (IN around)
                (SBAR
                  (WHNP (WDT that))
                  (S
                    (VP (VBD were) (RB n't)
                      (VP (VBN designed)))))))))))
    (. .)))

