(ROOT
  (S
    (NP
      (NP (DT The) (NN fact))
      (PP (IN of)
        (NP (NN evolution))))
    (VP (VBZ is)
      (ADJP (RB so) (JJ strong))
      (SBAR (IN that)
        (S
          (S
            (VP (VBG denying)
              (NP (PRP it))))
          (VP (VBZ is)
            (ADJP (JJ foolish))))))
    (. .)))

det(fact-2, The-1)
nsubj(strong-7, fact-2)
prep_of(fact-2, evolution-4)
cop(strong-7, is-5)
advmod(strong-7, so-6)
root(ROOT-0, strong-7)
complm(foolish-12, that-8)
csubj(foolish-12, denying-9)
dobj(denying-9, it-10)
cop(foolish-12, is-11)
ccomp(strong-7, foolish-12)

