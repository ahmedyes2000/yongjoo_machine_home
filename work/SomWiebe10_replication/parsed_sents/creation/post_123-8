(ROOT
  (S
    (S
      (S
        (VP (VB Evo) (: :)
          (ADVP (RB Here))))
      (VP (VBZ s)
        (NP
          (NP
            (NP (DT a) (NN bunch))
            (PP (IN of)
              (NP (JJ detailed))))
          (CC &)
          (NP
            (NP (JJ tested) (NN research) (NNS papers))
            (PP (IN on)
              (NP
                (NP (DT the) (NN evolution))
                (PP (IN of)
                  (NP (NN system) (NNP X.) (NNP ID)))))))))
    (: :)
    (FRAG (RB Not)
      (ADJP (JJ detailed) (RB enough)))
    (. .)))

