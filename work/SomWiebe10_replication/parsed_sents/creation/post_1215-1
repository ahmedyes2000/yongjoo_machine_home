(ROOT
  (S
    (VP
      (VP (VB \/) (-RRB- -RRB-)
        (NP (NNS \/))
        (-RRB- -RRB-)
        (ADVP (RB Take))
        (NP (DT a) (NN minute)))
      (CC and)
      (VP (VB think)
        (PP (IN about)
          (NP (DT that) (NN statement)))))
    (. .)))

root(ROOT-0, \/-1)
dobj(\/-1, \/-3)
advmod(\/-1, Take-5)
det(minute-7, a-6)
dobj(\/-1, minute-7)
conj_and(\/-1, think-9)
det(statement-12, that-11)
prep_about(think-9, statement-12)

