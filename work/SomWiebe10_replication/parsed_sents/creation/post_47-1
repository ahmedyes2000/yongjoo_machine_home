(ROOT
  (S
    (S
      (NP (NNP ID))
      (VP (VBZ is)
        (NP (PDT such) (DT a) (NN theory))))
    (: ;)
    (S
      (S
        (NP (NN everything))
        (VP (VBZ is)
          (ADJP (JJ consistent)
            (PP (IN with)
              (NP (PRP it))))))
      (, ,)
      (S
        (NP (NN nothing) (NNS disproves))
        (NP (PRP it))))
    (. .)))

nsubj(theory-5, ID-1)
cop(theory-5, is-2)
predet(theory-5, such-3)
det(theory-5, a-4)
root(ROOT-0, theory-5)
nsubj(consistent-9, everything-7)
cop(consistent-9, is-8)
parataxis(theory-5, consistent-9)
prep_with(consistent-9, it-11)
nn(disproves-14, nothing-13)
dep(it-15, disproves-14)
dep(consistent-9, it-15)

