  (S
    (NP (NNP ID))
    (VP (VBZ is)
      (ADVP (RB merely))
      (NP (DT a) (JJ useless) (`` ``) (JJ dead) (NN end) ('' ''))
      (S
        (VP (TO to)
          (VP (VB try)
            (S
              (VP (TO to)
                (VP (VB allow)
                  (S
                    (NP (NNS creationists))
                    (VP (TO to)
                      (VP (VB claim)
                        (SBAR (IN that)
                          (S
                            (NP (NN evolution))
                            (VP (MD might)
                              (VP (VB be)
                                (ADJP (JJ wrong))))))))))))))))
    (. .)))

nsubj(end-8, ID-1)
cop(end-8, is-2)
advmod(end-8, merely-3)
det(end-8, a-4)
