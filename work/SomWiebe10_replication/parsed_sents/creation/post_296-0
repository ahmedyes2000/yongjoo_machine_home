(ROOT
  (S
    (NP (PRP I))
    (VP (VBD was)
      (VP (VBG using)
        (NP (PRP$ your) (JJ rhetorical) (NN question))
        (S
          (VP (TO to)
            (VP (VB attempt)
              (S
                (VP (TO to)
                  (VP (VB get)
                    (NP
                      (NP (DT a) (NN discussion))
                      (VP (VBG going)))))))))))
    (. .)))

nsubj(using-3, I-1)
aux(using-3, was-2)
root(ROOT-0, using-3)
poss(question-6, your-4)
amod(question-6, rhetorical-5)
dobj(using-3, question-6)
aux(attempt-8, to-7)
xcomp(using-3, attempt-8)
aux(get-10, to-9)
xcomp(attempt-8, get-10)
det(discussion-12, a-11)
dobj(get-10, discussion-12)
partmod(discussion-12, going-13)

