(ROOT
  (S
    (NP
      (NP (DT The) (NN theory))
      (PP (IN of)
        (NP (NN evolution))))
    (VP (VBZ has)
      (ADVP (RB also))
      (VP (VBN proved)
        (NP (PRP itself))
        (PP (IN in)
          (NP (NN practice)))))
    (. .)))

det(theory-2, The-1)
nsubj(proved-7, theory-2)
prep_of(theory-2, evolution-4)
aux(proved-7, has-5)
advmod(proved-7, also-6)
root(ROOT-0, proved-7)
