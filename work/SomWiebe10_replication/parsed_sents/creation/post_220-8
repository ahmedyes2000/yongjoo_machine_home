(ROOT
  (S
    (VP (VB Ask)
      (S
        (NP (PRP them))
        (VP (TO to)
          (VP (VB document)
            (NP (PRP$ their) (NN prediction))))))
    (. .)))

root(ROOT-0, Ask-1)
nsubj(document-4, them-2)
aux(document-4, to-3)
xcomp(Ask-1, document-4)
poss(prediction-6, their-5)
dobj(document-4, prediction-6)

