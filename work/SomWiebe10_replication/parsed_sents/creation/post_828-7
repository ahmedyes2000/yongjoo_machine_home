(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (DT the) (NN gospel) (NN message))
        (VP (VBZ is)
          (ADJP (JJ dependent)
            (PP (IN on)
              (NP (DT the) (NNP Genesis) (NNS myths)))))))
    (, ,)
    (NP (PRP we))
    (VP (VBP are)
      (ADJP (JJR better) (RP off))
      (PP (IN without)
        (NP (PRP it))))
    (. .)))

