(ROOT
  (S (RB Not)
    (S
      (VP (VBG knowing)
        (CC or)
        (VBG caring)))
    (VP (MD would)
      (ADVP (RB just) (RB about))
      (VP (VB make)
        (S
          (NP (PRP you))
          (NP
            (NP (DT a) (NN monkey))
            (PP (IN after)
              (NP (DT all) (NNPS Pyrus)))))))
    (. .)))

neg(make-8, Not-1)
csubj(make-8, knowing-2)
conj_or(knowing-2, caring-4)
csubj(make-8, caring-4)
aux(make-8, would-5)
advmod(about-7, just-6)
advmod(make-8, about-7)
root(ROOT-0, make-8)
nsubj(monkey-11, you-9)
det(monkey-11, a-10)
xcomp(make-8, monkey-11)
det(Pyrus-14, all-13)
prep_after(monkey-11, Pyrus-14)

