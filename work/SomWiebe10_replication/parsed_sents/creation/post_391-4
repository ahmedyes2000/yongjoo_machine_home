  (S
    (VP (VB Compare)
      (NP
        (NP (DT this))
        (PP (IN with)
          (NP
            (NP (DT the) (JJ immense) (NN progress))
            (VP (VBN made)
              (PP (IN by)
                (NP (JJ evolutionary) (NN science)))
              (PP
                (ADVP (RB even))
                (IN in)
                (NP (DT the) (JJ last) (NN decade))))))))
    (. .)))

root(ROOT-0, Compare-1)
dobj(Compare-1, this-2)
det(progress-6, the-4)
