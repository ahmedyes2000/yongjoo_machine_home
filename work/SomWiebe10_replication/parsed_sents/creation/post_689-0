(ROOT
  (S
    (S
      (NP (PRP You))
      (VP (VBP say)
        (NP (DT this))))
    (, ,)
    (CC and)
    (S
      (ADVP (RB yet))
      (NP (PRP you))
      (VP (VBP use)
        (NP (DT a) (NN computer))))
    (. .)))

nsubj(say-2, You-1)
root(ROOT-0, say-2)
dobj(say-2, this-3)
advmod(use-8, yet-6)
nsubj(use-8, you-7)
conj_and(say-2, use-8)
det(computer-10, a-9)
dobj(use-8, computer-10)

