(ROOT
  (S
    (NP (DT The) (NN following))
    (VP (VBZ is)
      (NP (DT a) (NN quote))
      (PP (IN from)
        (NP (PRP$ your) (NNS points)))
      (: :)
      (NP
        (NP (NNP Quote))
        ('' '')
        (NP
          (NP (DT The) (JJR more) (NNS parts))
          (SBAR
            (WHNP (WDT that))
            (S
              (VP (VBP appear)
                (S
                  (VP (TO to)
                    (VP (VB be)
                      (VP (VBN arranged)
                        (PP (IN for)
                          (NP (DT a) (NN purpose)))))))))))
        (, ,)
        (CC and)
        (NP
          (NP (DT the) (JJR more) (NN precisely))
          (SBAR
            (S
              (NP (PRP they))
              (VP (VBP fit)
                (NP (DT each) (JJ other))
                (S
                  (VP (TO to)
                    (VP (VB achieve)
                      (SBAR (IN that)
                        (S
                          (NP (NN purpose))
                          (, ,)
                          (X (DT the)
                            (ADJP
                              (ADJP (JJR more))
                              (CC and)
                              (ADJP (RBR more) (JJ confident))))
                          (NP (PRP we))
                          (VP (MD can)
                            (VP (VB be)
                              (PP (IN in)
                                (NP
                                  (NP (PRP$ our) (NN conclusion))
                                  (PP (IN of)
                                    (NP (NN design))))))))))))))))))
    (. .)))

