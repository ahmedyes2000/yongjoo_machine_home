(ROOT
  (S ('' '')
    (NP (PRP We))
    (VP (VBP CAN)
      (VP (VB discuss)
        (SBAR (IN that)
          (S
            (NP
              (NP (NN issue) (NN AFTER))
              (SBAR
                (S
                  (NP (PRP we))
                  (VP (VB settle)
                    (NP
                      (NP (DT the) (NN question))
                      (PP (IN of)
                        (NP (NNP ID))))))))
            (VP (SYM =)
              (NP (NNP Religion) (JJ first)))))))
    (. .)))

nsubj(discuss-4, We-2)
aux(discuss-4, CAN-3)
root(ROOT-0, discuss-4)
complm(Religion-15, that-5)
nn(AFTER-7, issue-6)
nsubj(Religion-15, AFTER-7)
nsubj(settle-9, we-8)
rcmod(AFTER-7, settle-9)
det(question-11, the-10)
dobj(settle-9, question-11)
prep_of(question-11, ID-13)
dep(Religion-15, =-14)
ccomp(discuss-4, Religion-15)
amod(Religion-15, first-16)

