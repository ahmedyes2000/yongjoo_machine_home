(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP we))
        (VP (VBD knew)
          (NP (DT the) (NN designer))
          (ADVP (RB then)))))
    (NP (PRP we))
    (VP (MD would) (RB n't)
      (VP (VB have)
        (NP (DT a) (NN design) (NN inference))
        (SBAR (IN as)
          (S
            (NP (NN design))
            (VP (MD would)
              (VP (VB be)
                (NP (DT a) (JJ given))))))))
    (. .)))

mark(knew-3, If-1)
nsubj(knew-3, we-2)
advcl(have-10, knew-3)
det(designer-5, the-4)
dobj(knew-3, designer-5)
advmod(knew-3, then-6)
nsubj(have-10, we-7)
aux(have-10, would-8)
neg(have-10, n't-9)
root(ROOT-0, have-10)
det(inference-13, a-11)
nn(inference-13, design-12)
dobj(have-10, inference-13)
mark(given-19, as-14)
nsubj(given-19, design-15)
aux(given-19, would-16)
cop(given-19, be-17)
