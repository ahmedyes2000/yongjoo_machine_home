(ROOT
  (S (-LRB- -LRB-)
    (VP (VB Let)
      (S
        (S
          (NP (PRP me))
          (VP (VBP make)
            (S
              (NP (NNP PERFECTLY))
              (ADJP (JJ clear)))))
        (, ,)
        (S
          (NP (DT this))
          (VP (VBZ is)
            (NP
              (NP (DT all))
              (SBAR
                (S
                  (NP (PRP I))
                  (VP (VBP am)
                    (VP (VBG discussing))))))))))
    (. .) (-RRB- -RRB-)))

root(ROOT-0, Let-2)
nsubj(make-4, me-3)
dep(Let-2, make-4)
nsubj(clear-6, PERFECTLY-5)
xcomp(make-4, clear-6)
nsubj(all-10, this-8)
cop(all-10, is-9)
dep(make-4, all-10)
dobj(discussing-13, all-10)
nsubj(discussing-13, I-11)
aux(discussing-13, am-12)
rcmod(all-10, discussing-13)

