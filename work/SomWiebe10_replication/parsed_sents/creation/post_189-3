(ROOT
  (FRAG ('' '')
    (NP (NNP Evolution))
    (PP (IN as)
      (NP
        (NP (NNP Fact)
          (CC and)
          (NNP Theory))
        ('' '')
        (PP (IN in)
          (NP
            (NP
              (NP (NNS Hens))
              (NP (NNP Teeth)
                (CC and)
                (NP (NNP Horse) (POS 's))
                (NNPS Toes)))
            (: :)
            (NP
              (NP (JJ Further) (NNS Reflections))
              (PP (IN in)
                (NP (JJ Natural) (NN History))))))))
    (. .)))

root(ROOT-0, Evolution-2)
dep(Evolution-2, as-3)
pobj(as-3, Fact-4)
pobj(as-3, Theory-6)
conj_and(Fact-4, Theory-6)
prep_in(Fact-4, Hens-9)
nn(Toes-14, Teeth-10)
poss(Hens-9, Horse-12)
conj_and(Toes-14, Horse-12)
poss(Hens-9, Toes-14)
amod(Reflections-17, Further-16)
dep(Hens-9, Reflections-17)
amod(History-20, Natural-19)
prep_in(Reflections-17, History-20)

