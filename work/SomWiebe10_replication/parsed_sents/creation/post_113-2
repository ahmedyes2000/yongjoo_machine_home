(ROOT
  (S
    (PP (IN In)
      (NP (NN science)))
    (NP (EX there))
    (VP (VBZ is)
      (NP
        (NP (DT a) (JJ first) (NN cause))
        (SBAR
          (WHNP (WDT that))
          (S
            (VP (VBZ is)
              (PP (IN beyond)
                (NP (JJ direct) (NN observation))))))))
    (. .)))

prep_in(is-4, science-2)
expl(is-4, there-3)
root(ROOT-0, is-4)
det(cause-7, a-5)
amod(cause-7, first-6)
nsubj(is-4, cause-7)
nsubj(is-9, cause-7)
rcmod(cause-7, is-9)
amod(observation-12, direct-11)
prep_beyond(is-9, observation-12)

