(ROOT
  (S
    (NP (NNP \/) (NNP -RRB-))
    (ADVP (WRB Why))
    (VP (MD ca)
      (PRN (RB n't)
        (NP
          (NP (NNP God))
          (CC and)
          (NP (NN evolution))))
      (VP (VB be)
        (ADJP (JJ compatible))))
    (. .)))

nsubj(compatible-10, \/-1)
advmod(compatible-10, Why-3)
aux(compatible-10, ca-4)
dep(God-6, n't-5)
dep(compatible-10, God-6)
conj_and(God-6, evolution-8)
dep(compatible-10, evolution-8)
cop(compatible-10, be-9)
root(ROOT-0, compatible-10)

