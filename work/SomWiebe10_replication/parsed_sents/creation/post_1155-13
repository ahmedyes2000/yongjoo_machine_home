(ROOT
  (S
    (NP
      (NP (DT The) (NN theory))
      (PP (IN of)
        (NP (NN evolution))))
    (VP (VBZ is)
      (ADJP
        (ADJP (DT no) (RBR less) (JJ valid))
        (PP (IN than)
          (NP
            (NP (DT any))
            (PP (IN of)
              (NP (DT the) (NNS theories))))))
      (PP (VBN given)
        (PP (IN above))))
    (. .)))

det(theory-2, The-1)
nsubj(valid-8, theory-2)
prep_of(theory-2, evolution-4)
cop(valid-8, is-5)
