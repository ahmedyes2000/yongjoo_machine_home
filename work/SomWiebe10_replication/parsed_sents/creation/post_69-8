  (S
    (NP
      (NP (NNS Thanks))
      (PP (IN for)
        (NP
          (NP (PRP$ your) (NN contribution))
          (SBAR (IN as)
            (S
              (NP (PRP it))
              (VP (VBZ demonstrates)
                (NP
                  (NP (DT the) (NN vacuity))
                  (PP (IN of)
                    (NP (NNP ID))))
                (PP (VBN combined)
                  (PP (IN with)
                    (NP (DT the) (NN ignorance))))))))))
    (VP (VBD needed)
      (S
        (VP (TO to)
          (VP (VB reach)
            (NP (PDT such) (DT a) (NN conclusion))))))
    (. .)))

