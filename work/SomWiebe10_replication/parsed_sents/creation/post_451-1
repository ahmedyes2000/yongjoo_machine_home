(ROOT
  (S
    (NP
      (NP (DT The) (NN cause))
      (PP (IN of)
        (NP (DT the) (JJ big) (NN bang))))
    (VP (VBZ is)
      (ADJP (JJ unknown)))
    (. .)))

det(cause-2, The-1)
nsubj(unknown-8, cause-2)
det(bang-6, the-4)
amod(bang-6, big-5)
prep_of(cause-2, bang-6)
cop(unknown-8, is-7)
root(ROOT-0, unknown-8)

