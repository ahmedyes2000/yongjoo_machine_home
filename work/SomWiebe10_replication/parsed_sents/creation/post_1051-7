(ROOT
  (S
    (NP (DT The) (JJ last) (NN point))
    (VP (VBZ is)
      (NP
        (NP (DT the) (JJ fossil) (NN record))
        (SBAR
          (S
            (VP (VBZ is)
              (ADJP (JJ incomplete)))))))
    (. .)))

det(point-3, The-1)
amod(point-3, last-2)
nsubj(record-7, point-3)
cop(record-7, is-4)
det(record-7, the-5)
amod(record-7, fossil-6)
root(ROOT-0, record-7)
cop(incomplete-9, is-8)
rcmod(record-7, incomplete-9)

