(ROOT
  (S (: --)
    (S
      (NP (NNP ID))
      (VP (MD may)
        (VP (VB assert))))
    (CC but)
    (S
      (PP (IN as)
        (NP (DT a) (NN science)))
      (NP (PRP$ their) (NNS contributions))
      (VP (VBP remain)
        (ADVP (RB largely))
        (PP (IN with)
          (NP (NN content)))))
    (. .)))

nsubj(assert-4, ID-2)
aux(assert-4, may-3)
root(ROOT-0, assert-4)
det(science-8, a-7)
prep_as(remain-11, science-8)
poss(contributions-10, their-9)
nsubj(remain-11, contributions-10)
conj_but(assert-4, remain-11)
advmod(remain-11, largely-12)
prep_with(remain-11, content-14)

