(ROOT
  (S
    (NP (PRP You))
    (VP (VBP are)
      (VP (VBG trying)
        (S
          (VP (TO to)
            (VP (VB prove)
              (NP (DT a) (JJ negative)))))))
    (. .)))

nsubj(trying-3, You-1)
xsubj(prove-5, You-1)
aux(trying-3, are-2)
root(ROOT-0, trying-3)
aux(prove-5, to-4)
xcomp(trying-3, prove-5)
det(negative-7, a-6)
dobj(prove-5, negative-7)

