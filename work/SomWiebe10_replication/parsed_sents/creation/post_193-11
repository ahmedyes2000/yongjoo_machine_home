(ROOT
  (SBARQ
    (FRAG (CC And)
      (PP (IN if) (RB not)))
    (, ,)
    (SBARQ
      (WHADVP (WRB how))
      (SQ (MD can)
        (NP (PRP you))
        (VP (VB use)
          (S
            (NP (PRP it))
            (VP (TO to)
              (VP (VB support)
                (NP (NNP ID))))))))
    (. ?)))

dep(use-8, And-1)
dep(And-1, if-2)
neg(if-2, not-3)
