(ROOT
  (SQ (VBZ Is)
    (NP (EX there))
    (NP
      (NP (JJ credible) (JJ scientific) (NN evidence))
      (SBAR (IN that)
        (S
          (NP (NN life))
          (VP (VBD was)
            (VP (VBN designed))))))
    (. ?)))

root(ROOT-0, Is-1)
expl(Is-1, there-2)
amod(evidence-5, credible-3)
amod(evidence-5, scientific-4)
nsubj(Is-1, evidence-5)
complm(designed-9, that-6)
nsubjpass(designed-9, life-7)
auxpass(designed-9, was-8)
dep(evidence-5, designed-9)

