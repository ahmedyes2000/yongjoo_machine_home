(ROOT
  (S ('' '')
    (NP (WP What))
    (VP (VBD specific)
      (SBAR
        (S
          (NP
            (NP (NNS pieces))
            (PP (IN of)
              (NP (DT the) (JJ fossil) (NN record))))
          (VP (VBP prove)
            (SBAR (IN that)
              (S
                (NP (EX there))
                (VP (VBD were)
                  (NP
                    (NP (NNS changes))
                    (PP (IN in)
                      (NP
                        (NP (NNS lifeforms))
                        (VP (VBN separated)
                          (PP (IN by)
                            (NP (NN time))))))))))))))
    (. ?)))

nsubj(specific-3, What-2)
root(ROOT-0, specific-3)
nsubj(prove-9, pieces-4)
det(record-8, the-6)
amod(record-8, fossil-7)
prep_of(pieces-4, record-8)
ccomp(specific-3, prove-9)
complm(were-12, that-10)
expl(were-12, there-11)
ccomp(prove-9, were-12)
nsubj(were-12, changes-13)
prep_in(changes-13, lifeforms-15)
partmod(lifeforms-15, separated-16)
agent(separated-16, time-18)

