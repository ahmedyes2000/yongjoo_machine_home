(ROOT
  (S
    (NP (NNP Mr.) (NNP Behe))
    (VP (VBZ makes)
      (NP
        (NP (CD two) (JJ key) (NNS claims))
        (PP (IN in)
          (NP (DT this) (JJ short) (NN essay)))))
    (. .)))

nn(Behe-2, Mr.-1)
nsubj(makes-3, Behe-2)
root(ROOT-0, makes-3)
num(claims-6, two-4)
amod(claims-6, key-5)
dobj(makes-3, claims-6)
det(essay-10, this-8)
amod(essay-10, short-9)
