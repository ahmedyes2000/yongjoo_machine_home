(ROOT
  (S
    (NP (PRP They))
    (VP (VBP claim)
      (SBAR (IN that)
        (S
          (NP (DT these) (NNS teachers))
          (VP (VBP are)
            (ADJP (JJ evolutionists))))))
    (. .)))

nsubj(claim-2, They-1)
root(ROOT-0, claim-2)
complm(evolutionists-7, that-3)
det(teachers-5, these-4)
nsubj(evolutionists-7, teachers-5)
cop(evolutionists-7, are-6)
ccomp(claim-2, evolutionists-7)

