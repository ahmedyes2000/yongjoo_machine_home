(ROOT
  (S ('' '')
    (NP (DT The) (NN reason))
    (ADVP (PRP we))
    (VP (VBP shun)
      (S
        (NP
          (NP (DT the) (NN idea))
          (PP (IN of)
            (NP (NN something))))
        (VP (VBG creating)
          (SBAR
            (S
              (NP (PRP us))
              (VP (VBZ is)
                (SBAR (IN because)
                  (S
                    (NP (PRP we))
                    (VP (VBP believe)
                      (SBAR (IN that)
                        (S
                          (NP
                            (NP (DT the) (NN beginning))
                            (PP (IN of)
                              (NP (NN life))))
                          (VP (VBD started)
                            (PP (IN on)
                              (NP (DT this) (NN planet)))))))))))))))
    (. .)))

det(reason-3, The-2)
nsubj(shun-5, reason-3)
advmod(shun-5, we-4)
root(ROOT-0, shun-5)
det(idea-7, the-6)
nsubj(creating-10, idea-7)
prep_of(idea-7, something-9)
dep(shun-5, creating-10)
nsubj(is-12, us-11)
ccomp(creating-10, is-12)
mark(believe-15, because-13)
nsubj(believe-15, we-14)
advcl(is-12, believe-15)
complm(started-21, that-16)
det(beginning-18, the-17)
nsubj(started-21, beginning-18)
prep_of(beginning-18, life-20)
ccomp(believe-15, started-21)
det(planet-24, this-23)
prep_on(started-21, planet-24)

