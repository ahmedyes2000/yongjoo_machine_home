(ROOT
  (S
    (S
      (NP (DT The) (JJ fundamentalist))
      (VP (VBD had)
        (VP (VBN been)
          (ADJP (JJ right)))))
    (CC and)
    (S
      (NP (NNP Darwin))
      (VP (VBD had)
        (VP (VBN been)
          (ADJP (JJ wrong)))))
    (. !)))

det(fundamentalist-2, The-1)
