(ROOT
  (S
    (NP (DT The) (NNP Bible))
    (VP (VBZ does) (RB n't)
      (VP (VB stand)
        (CC or)
        (VB fall)
        (PP (IN on)
          (NP (DT that) (NN statement)))))
    (. .)))

det(Bible-2, The-1)
nsubj(stand-5, Bible-2)
nsubj(fall-7, Bible-2)
aux(stand-5, does-3)
neg(stand-5, n't-4)
root(ROOT-0, stand-5)
conj_or(stand-5, fall-7)
det(statement-10, that-9)
prep_on(stand-5, statement-10)

