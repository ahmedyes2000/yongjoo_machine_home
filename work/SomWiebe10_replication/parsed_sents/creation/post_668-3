(ROOT
  (S
    (NP (PRP You))
    (VP (MD may)
      (VP (VB be)
        (ADJP (RB too) (JJ harsh)
          (PP (IN towards)
            (NP (NNP ID) (NNS proponents))))))
    (. .)))

nsubj(harsh-5, You-1)
aux(harsh-5, may-2)
cop(harsh-5, be-3)
advmod(harsh-5, too-4)
root(ROOT-0, harsh-5)
nn(proponents-8, ID-7)
prep_towards(harsh-5, proponents-8)

