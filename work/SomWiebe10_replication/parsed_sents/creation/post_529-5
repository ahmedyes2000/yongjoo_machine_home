(ROOT
  (S
    (VP (VB Let)
      (SBAR
        (S
          (NP (PRP me))
          (VP (VBP know)
            (SBAR
              (WHADVP (WRB when))
              (S
                (NP (PRP you))
                (VP (VBP have)
                  (VP (VBN located)
                    (NP (DT the) (NN compiler))))))))))
    (. .)))

root(ROOT-0, Let-1)
nsubj(know-3, me-2)
ccomp(Let-1, know-3)
advmod(located-7, when-4)
nsubj(located-7, you-5)
aux(located-7, have-6)
advcl(know-3, located-7)
det(compiler-9, the-8)
dobj(located-7, compiler-9)

