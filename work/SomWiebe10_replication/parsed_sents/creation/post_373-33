(ROOT
  (S
    (S
      (NP (NNP ID))
      (VP (VBZ is) (RB not)
        (NP (DT a) (NN theory))))
    (CC and)
    (S
      (S
        (NP (PRP it))
        (VP (MD can)
          (ADVP (RB never))
          (VP (VB be)
            (NP (NN science)))))
      (CC and)
      (S
        (ADVP (RB therefore))
        (NP (PRP it))
        (VP (VBZ is)
          (ADVP (RB properly))
          (VP (VBN rejected)
            (PP (IN in)
              (NP (JJ scientific) (NNS discussions)))))))
    (. .)))

nsubj(theory-5, ID-1)
cop(theory-5, is-2)
neg(theory-5, not-3)
det(theory-5, a-4)
root(ROOT-0, theory-5)
