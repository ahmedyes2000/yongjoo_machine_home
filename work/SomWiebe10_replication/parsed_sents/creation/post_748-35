(ROOT
  (S
    (NP (DT The) (NN point))
    (VP (VBZ is)
      (UCP
        (NP
          (NP (DT any) (NN line))
          (PP (IN in)
            (NP (DT the) (NNP Bible))))
        (CC or)
        (S
          (NP (JJ religious) (NN concept))
          (VP (MD can)
            (VP (VB be)
              (ADJP (JJ difficult))
              (SBAR (IN if)
                (S
                  (NP (PRP you))
                  (VP (VBP come)
                    (PP (IN at)
                      (NP (PRP it)))
                    (PP (IN without)
                      (NP (NN understanding)))))))))
        (. .)))
    (. .)))

det(point-2, The-1)
nsubj(line-5, point-2)
cop(line-5, is-3)
det(line-5, any-4)
root(ROOT-0, line-5)
det(Bible-8, the-7)
prep_in(line-5, Bible-8)
amod(concept-11, religious-10)
nsubj(difficult-14, concept-11)
aux(difficult-14, can-12)
cop(difficult-14, be-13)
conj_or(line-5, difficult-14)
mark(come-17, if-15)
nsubj(come-17, you-16)
