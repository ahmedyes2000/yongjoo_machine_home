  (S
    (NP (NNP Casey) (NNP Luskin))
    (VP (VBZ knows)
      (SBAR
        (WHADVP (WRB how))
        (S
          (VP (TO to)
            (VP (VB use)
              (NP (JJ impressive) (JJ multi) (JJ syllabic) (NNS words))
              (S
                (VP (TO to)
                  (VP (VB suggest)
                    (SBAR (IN that)
                      (S
                        (NP (PRP he))
                        (VP (VBZ 's)
                          (ADJP (JJ intelligent)))))))))))))
    (. .)))

