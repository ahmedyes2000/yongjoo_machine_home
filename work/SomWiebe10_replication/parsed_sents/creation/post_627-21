(ROOT
  (S
    (NP (PRP You))
    (VP (VBP are)
      (NP
        (NP (DT a) (JJ good) (NN apologist))
        (PP (IN for)
          (NP (NN naturalism)))))
    (. .)))

nsubj(apologist-5, You-1)
cop(apologist-5, are-2)
det(apologist-5, a-3)
amod(apologist-5, good-4)
root(ROOT-0, apologist-5)
prep_for(apologist-5, naturalism-7)

