(ROOT
  (S
    (SBAR
      (WHNP (WP What))
      (S
        (VP (VBZ is)
          (ADJP (JJ fascinating)
            (PP (TO to)
              (NP (PRP me)))))))
    (VP (VBZ is)
      (SBAR
        (WHADVP (WRB how))
        (S
          (NP (NNP Jay))
          (VP (VBZ continues)
            (S
              (VP (TO to)
                (VP
                  (VP (VB quote)
                    (NP (NN mine) (NNS Darwin)))
                  (CC and)
                  (VP (VB misrepresent)
                    (NP (DT the) (NN science))
                    (SBAR
                      (WHADVP (WRB when))
                      (S
                        (NP (PRP it))
                        (VP (VBZ comes)
                          (PP (IN for)
                            (NP (NN instance)))
                          (PP (TO to)
                            (NP (NNS intermediates))))))))))))))
    (. .)))

nsubj(fascinating-3, What-1)
cop(fascinating-3, is-2)
csubj(is-6, fascinating-3)
prep_to(fascinating-3, me-5)
root(ROOT-0, is-6)
advmod(continues-9, how-7)
nsubj(continues-9, Jay-8)
