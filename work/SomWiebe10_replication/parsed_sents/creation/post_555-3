(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP it))
        (VP (VBD were)
          (NP (DT an) (JJ open) (NN debate))
          (SBAR (IN that)
            (S
              (NP (NNP ID))
              (VP (VBD were)
                (ADJP (JJ interested)
                  (PP (IN in)
                    (NP (RB then))))))))))
    (NP (DT the) (NN issue))
    (VP (VBZ has)
      (ADVP (RB long) (IN since))
      (VP (VBN been)
        (VP (VBN settled))))
    (. .)))

