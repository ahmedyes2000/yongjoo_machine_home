  (S
    (NP
      (NP (DT The) (NN theory))
      (PP (IN of)
        (NP (NN evolution))))
    (VP (VBZ is) (RB not)
      (ADJP (JJ falsifiable)))
    (. .)))

det(theory-2, The-1)
nsubj(falsifiable-7, theory-2)
prep_of(theory-2, evolution-4)
cop(falsifiable-7, is-5)
neg(falsifiable-7, not-6)
