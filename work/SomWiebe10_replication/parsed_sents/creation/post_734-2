(ROOT
  (S
    (NP (PRP They))
    (VP (VBD did) (RB n't)
      (ADVP (RB even))
      (VP (VB hang)
        (PRT (RP around))
        (S
          (VP (TO to)
            (VP (VB defend)
              (NP (PRP$ their) (NN position)))))))
    (. .)))

nsubj(hang-5, They-1)
xsubj(defend-8, They-1)
aux(hang-5, did-2)
neg(hang-5, n't-3)
advmod(hang-5, even-4)
root(ROOT-0, hang-5)
prt(hang-5, around-6)
aux(defend-8, to-7)
xcomp(hang-5, defend-8)
poss(position-10, their-9)
dobj(defend-8, position-10)

