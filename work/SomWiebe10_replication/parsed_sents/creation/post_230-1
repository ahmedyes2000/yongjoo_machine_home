(ROOT
  (S (CC And)
    (NP (NNS people))
    (VP (VBP wonder)
      (SBAR
        (WHADVP (WRB why))
        (S
          (NP (NNS scientists))
          (VP (VBP have)
            (VP (VBN come)
              (S
                (VP (TO to)
                  (VP (VB find)
                    (S
                      (NP (NNP ID))
                      (VP (TO to)
                        (VP (VB be)
                          (NP (JJ scientifically) (NNS vacuous)))))))))))))
    (: ...)))

cc(wonder-3, And-1)
nsubj(wonder-3, people-2)
root(ROOT-0, wonder-3)
advmod(come-7, why-4)
nsubj(come-7, scientists-5)
xsubj(find-9, scientists-5)
aux(come-7, have-6)
advcl(wonder-3, come-7)
aux(find-9, to-8)
xcomp(come-7, find-9)
nsubj(vacuous-14, ID-10)
aux(vacuous-14, to-11)
cop(vacuous-14, be-12)
amod(vacuous-14, scientifically-13)
xcomp(find-9, vacuous-14)

