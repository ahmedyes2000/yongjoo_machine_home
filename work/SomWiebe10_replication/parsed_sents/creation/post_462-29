(ROOT
  (S
    (SBAR
      (WHNP (WP What))
      (S
        (VP (VBD worldview)
          (SBAR
            (S
              (NP (DT a) (NN person))
              (VP (VBZ holds)))))))
    (VP (MD should) (RB not)
      (VP (VB be)
        (NP (DT the) (NN issue))))
    (. .)))

nsubj(worldview-2, What-1)
csubj(issue-10, worldview-2)
det(person-4, a-3)
nsubj(holds-5, person-4)
ccomp(worldview-2, holds-5)
aux(issue-10, should-6)
neg(issue-10, not-7)
cop(issue-10, be-8)
det(issue-10, the-9)
root(ROOT-0, issue-10)

