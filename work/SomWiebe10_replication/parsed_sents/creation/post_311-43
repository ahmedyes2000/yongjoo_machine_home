(ROOT
  (S ('' '')
    (NP (PRP We))
    (VP (MD can)
      (ADVP (RB only))
      (VP (VB thank)
        (NP
          (NP (NNP Newton))
          (PP (IN for)
            (NP (PRP$ his) (NNS contributions))))
        (PP (TO to)
          (NP (NN science)))
        (SBAR (IN while)
          (S
            (ADVP (RB also))
            (VP (VBG showing)
              (NP (PRP us))
              (SBAR
                (WHADVP (WRB why))
                (S
                  (NP (NNP ID))
                  (VP (VBZ is)
                    (ADJP (VBN doomed)
                      (S
                        (VP (TO to)
                          (VP (VB remain)
                            (NP (JJ scientifically) (NNS vacuous))))))))))))))
    (. .)))

nsubj(thank-5, We-2)
aux(thank-5, can-3)
advmod(thank-5, only-4)
root(ROOT-0, thank-5)
dobj(thank-5, Newton-6)
poss(contributions-9, his-8)
prep_for(Newton-6, contributions-9)
prep_to(thank-5, science-11)
mark(showing-14, while-12)
advmod(showing-14, also-13)
advcl(thank-5, showing-14)
dobj(showing-14, us-15)
advmod(doomed-19, why-16)
nsubjpass(doomed-19, ID-17)
auxpass(doomed-19, is-18)
advcl(showing-14, doomed-19)
aux(vacuous-23, to-20)
cop(vacuous-23, remain-21)
amod(vacuous-23, scientifically-22)
xcomp(doomed-19, vacuous-23)

