(ROOT
  (S
    (VP
      (VP (VB Try))
      (CC and)
      (VP (VB quote)
        (NP (DT the) (JJ right) (NN text))
        (NP (JJ next) (NN time))))
    (. .)))

root(ROOT-0, Try-1)
conj_and(Try-1, quote-3)
det(text-6, the-4)
amod(text-6, right-5)
iobj(quote-3, text-6)
amod(time-8, next-7)
dobj(quote-3, time-8)

