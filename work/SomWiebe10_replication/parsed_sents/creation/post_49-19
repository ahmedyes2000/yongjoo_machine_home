(ROOT
  (NP
    (NP (NNP Highlander))
    (: --)
    (S
      (NP (PRP I))
      (VP (VB invite)
        (S
          (NP (NNP PvM))
          (VP (TO to)
            (VP (VB cite)
              (NP
                (NP (DT any) (VBG leading) (NNP ID) (NN theorist))
                (VP (VBG filling)
                  (NP
                    (NP (NNS gaps))
                    (PP (IN in)
                      (NP (JJ scientific) (NN knowledge))))
                  (PP (IN with)
                    (NP (NNP ID)))
                  (, ,)
                  (SBAR (IN as)
                    (S
                      (VP (VBD claimed)
                        (ADVP (RB above))))))))))))
    (. .)))

root(ROOT-0, Highlander-1)
nsubj(invite-4, I-3)
dep(Highlander-1, invite-4)
