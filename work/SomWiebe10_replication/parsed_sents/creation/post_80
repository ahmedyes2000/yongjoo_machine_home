(ROOT
  (S
    (NP
      (NP (DT The) (NN purpose))
      (PP (IN of)
        (NP (DT the) (JJ spiritual) (NN realm))))
    (VP (VBZ has)
      (ADVP (RB never))
      (VP
        (VP (VBN been)
          (S
            (VP (TO to)
              (VP (VB say)
                (NP (WRB how))))))
        (, ,)
        (CC but)
        (VP
          (FRAG
            (WHADVP (WRB why))))))
    (. .)))

