(ROOT
  (S
    (ADVP (RB Furthermore))
    (, ,)
    (NP (PRP you))
    (VP (VBP conflate)
      (NP (NN cosmology))
      (PP (IN with)
        (NP (NN biology))))
    (. .)))

advmod(conflate-4, Furthermore-1)
nsubj(conflate-4, you-3)
root(ROOT-0, conflate-4)
dobj(conflate-4, cosmology-5)
prep_with(conflate-4, biology-7)

