(ROOT
  (S ('' '')
    (NP (DT This))
    (VP (VBD led)
      (PP (TO to)
        (NP
          (NP
            (NP (DT the) (NN appearance))
            (PP (IN of)
              (NP
                (NP (DT the)
                  (ADJP (RB very) (JJ first))
                  (NNS forms))
                (PP (IN of)
                  (NP (NN life))))))
          (: ...)
          (SBAR
            (WHNP (WDT which))
            (S
              (VP (VBD were)
                (ADJP (RB vastly) (JJR simpler))
                (ADVP (RB then))
                (NP
                  (NP (RB even) (DT the) (JJS most) (NN basic))
                  (PP (IN of)
                    (NP (NNS cells))))))))))
    (. .)))

nsubj(led-3, This-2)
root(ROOT-0, led-3)
det(appearance-6, the-5)
prep_to(led-3, appearance-6)
det(forms-11, the-8)
advmod(first-10, very-9)
amod(forms-11, first-10)
prep_of(appearance-6, forms-11)
prep_of(forms-11, life-13)
nsubj(simpler-18, which-15)
cop(simpler-18, were-16)
advmod(simpler-18, vastly-17)
rcmod(appearance-6, simpler-18)
advmod(simpler-18, then-19)
advmod(basic-23, even-20)
det(basic-23, the-21)
amod(basic-23, most-22)
attr(simpler-18, basic-23)
prep_of(basic-23, cells-25)

