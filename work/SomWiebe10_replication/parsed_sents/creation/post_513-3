(ROOT
  (S
    (NP (PRP I))
    (VP (VBP have)
      (VP (VBN numbered)
        (NP (DT the) (NNS points))
        (PP (IN below)
          (ADVP (RB simply)))
        (PP (IN for)
          (NP (NN convenience) (CD .1)))))
    (. .)))

nsubj(numbered-3, I-1)
aux(numbered-3, have-2)
root(ROOT-0, numbered-3)
det(points-5, the-4)
dobj(numbered-3, points-5)
prep(numbered-3, below-6)
pcomp(below-6, simply-7)
prep_for(numbered-3, convenience-9)
num(convenience-9, .1-10)

