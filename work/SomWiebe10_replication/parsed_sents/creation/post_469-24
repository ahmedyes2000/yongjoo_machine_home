(ROOT
  (S
    (ADVP (RB Therefore))
    (PRN (, ,)
      (S
        (NP (DT this) (NN incident))
        (VP (MD should)
          (VP (VB be)
            (VP (VBN handled)
              (ADVP (RB carefully))))))
      (, ,))
    (NP (PRP I))
    (VP (VBP believe))
    (. .)))

advmod(believe-11, Therefore-1)
det(incident-4, this-3)
nsubjpass(handled-7, incident-4)
aux(handled-7, should-5)
auxpass(handled-7, be-6)
parataxis(believe-11, handled-7)
advmod(handled-7, carefully-8)
nsubj(believe-11, I-10)
root(ROOT-0, believe-11)

