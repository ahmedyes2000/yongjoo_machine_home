(ROOT
  (S
    (SBAR
      (WHADVP (WRB When))
      (S
        (NP (PRP you))
        (VP (VBP see)
          (S
            (NP (NNS things))
            (VP (VB take)
              (NP (NN place))
              (PP (IN in)
                (NP (NN science))))))))
    (NP (PRP they))
    (VP (VBP happen)
      (PP (RB because) (IN of)
        (NP (PDT all) (DT the) (NNS laws)
          (SBAR
            (WHNP (WDT that))
            (S
              (VP (VBD were)
                (VP (VBN put)
                  (PP (IN into)
                    (NP (NN place)))
                  (PP (IN by)
                    (NP (DT an) (JJ intelligent) (NN designer))))))))))
    (. .)))

advmod(see-3, When-1)
nsubj(see-3, you-2)
advcl(happen-10, see-3)
nsubj(take-5, things-4)
ccomp(see-3, take-5)
