(ROOT
  (S
    (NP (PRP You))
    (VP (VBD suggested)
      (NP (DT a) (NN conspiracy) (NN theory))
      (S
        (VP (TO to)
          (VP (VB force)
            (NP
              (NP (DT the) (NN theory))
              (PP (IN of)
                (NP (NN evolution))))
            (PP (IN into)
              (NP (NNS schools)))))))
    (. .)))

nsubj(suggested-2, You-1)
root(ROOT-0, suggested-2)
det(theory-5, a-3)
nn(theory-5, conspiracy-4)
dobj(suggested-2, theory-5)
aux(force-7, to-6)
xcomp(suggested-2, force-7)
det(theory-9, the-8)
dobj(force-7, theory-9)
prep_of(theory-9, evolution-11)
