(ROOT
  (S
    (SBAR (IN If)
      (FRAG
        (ADVP (RB somewhere))))
    (, ,)
    (NP (RB then) (NN existence))
    (VP (VBD was)
      (ADVP (RB already))
      (ADVP (RB there))
      (SBAR (IN before)
        (S
          (NP (DT the) (NNS bodiless) (NN consciousness))
          (VP (VBD came)
            (PP (IN on)
              (NP (DT the) (NN scene)))))))
    (. .)))

mark(somewhere-2, If-1)
advcl(was-6, somewhere-2)
advmod(existence-5, then-4)
nsubj(was-6, existence-5)
root(ROOT-0, was-6)
advmod(was-6, already-7)
advmod(was-6, there-8)
mark(came-13, before-9)
det(consciousness-12, the-10)
nn(consciousness-12, bodiless-11)
nsubj(came-13, consciousness-12)
advcl(was-6, came-13)
det(scene-16, the-15)
prep_on(came-13, scene-16)

