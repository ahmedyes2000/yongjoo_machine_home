  (S
    (S
      (NP (PRP They))
      (VP (MD should)
        (VP (VB go)
          (PRT (RP back))
          (PP (TO to)
            (S
              (VP (VBG playing)
                (PP (IN with)
                  (NP (NNS toys)))))))))
    (, ,)
    (NP (PRP I))
    (VP (MD 'll)
      (VP (VB find)
        (NP (DT a)
