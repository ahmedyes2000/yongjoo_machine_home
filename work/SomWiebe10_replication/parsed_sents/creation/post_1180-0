(ROOT
  (S
    (NP (PRP I))
    (VP (VB interpret)
      (NP (DT the) (NN creation) (NN story))
      (ADVP (RB as)
        (NP (DT a) (NN metaphor)))
      (: ;)
      (PP (IN with)
        (NP
          (NP (NNP Science))
          (SBAR (IN as)
            (S
              (NP (NNP HOW) (NNP God))
              (VP (VBD made)
                (NP (NNS humans))))))))
    (. .)))

nsubj(interpret-2, I-1)
root(ROOT-0, interpret-2)
det(story-5, the-3)
nn(story-5, creation-4)
dobj(interpret-2, story-5)
advmod(interpret-2, as-6)
det(metaphor-8, a-7)
pobj(as-6, metaphor-8)
prep_with(interpret-2, Science-11)
mark(made-15, as-12)
nn(God-14, HOW-13)
nsubj(made-15, God-14)
dep(Science-11, made-15)
dobj(made-15, humans-16)

