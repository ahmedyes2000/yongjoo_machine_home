(ROOT
  (S
    (NP (PRP I))
    (VP (VBP have)
      (NP
        (NP (DT no) (NN patience))
        (PP (IN for)
          (NP
            (NP (NNS people))
            (PP (IN like)
              (NP (PRP you)))))))
    (. .)))

nsubj(have-2, I-1)
root(ROOT-0, have-2)
det(patience-4, no-3)
dobj(have-2, patience-4)
prep_for(patience-4, people-6)
prep_like(people-6, you-8)

