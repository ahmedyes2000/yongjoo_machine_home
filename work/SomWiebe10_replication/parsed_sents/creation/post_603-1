(ROOT
  (S
    (NP (DT No) (NN one))
    (VP (MD would)
      (VP (VB think)
        (SBAR (IN that)
          (S
            (NP (PRP it))
            (VP (VBZ is)
              (PP (IN by)
                (NP (JJ undirected) (NN chance)))
              (SBAR (IN that)
                (S
                  (NP
                    (NP (PDT all) (DT the) (NNS names))
                    (PP (IN on)
                      (NP (DT the) (NN list))))
                  (VP (VBP are)
                    (NP (NNP Steve))))))))))
    (. .)))

det(one-2, No-1)
nsubj(think-4, one-2)
