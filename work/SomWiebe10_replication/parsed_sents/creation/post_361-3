(ROOT
  (S
    (S
      (NP (PRP It))
      (VP (VBZ follows)
        (SBAR (IN that)
          (S
            (NP (RB absolutely) (NN everything))
            (VP (VBD was)
              (VP (VBN designed)))))))
    (CC and)
    (S
      (NP (EX there))
      (VP (VBZ is)
        (NP
          (NP (DT no) (NN way))
          (PP (IN of)
            (S
              (VP (VBG singling)
                (PRT (RP out))
                (NP
                  (NP (VBG living) (NNS things))
                  (VP
                    (ADVP (RB as) (RBR more))
                    (VBN designed)
                    (PP (IN than)
                      (NP (NN anything) (RB else)))))))))))
    (. .)))

nsubj(follows-2, It-1)
root(ROOT-0, follows-2)
complm(designed-7, that-3)
advmod(everything-5, absolutely-4)
nsubjpass(designed-7, everything-5)
auxpass(designed-7, was-6)
ccomp(follows-2, designed-7)
expl(is-10, there-9)
conj_and(follows-2, is-10)
det(way-12, no-11)
nsubj(is-10, way-12)
prepc_of(way-12, singling-14)
prt(singling-14, out-15)
amod(things-17, living-16)
dobj(singling-14, things-17)
advmod(more-19, as-18)
advmod(designed-20, more-19)
