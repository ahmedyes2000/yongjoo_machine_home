(ROOT
  (S (: --)
    (NP (NNP ICDESIGNAs))
    (VP
      (ADVP (RB far)
        (PP (IN as)
          (NP (JJ scientific) (NNS findings))))
      (VBP go)
      (NP
        (NP (DT the) (NNS numbers))
        (PP (IN of)
          (NP (NN evolution))))
      (S
        (VP (VBG believing)
          (SBAR
            (S
              (NP (NNP scientist) (POS 's))
              (VP (VBZ is)
                (PP (IN on)
                  (NP (DT a) (JJ major) (NN decline)))))))))
    (. .)))

nsubj(go-7, ICDESIGNAs-2)
advmod(go-7, far-3)
amod(findings-6, scientific-5)
prep_as(far-3, findings-6)
root(ROOT-0, go-7)
det(numbers-9, the-8)
dobj(go-7, numbers-9)
prep_of(numbers-9, evolution-11)
xcomp(go-7, believing-12)
nsubj(is-15, scientist-13)
possessive(scientist-13, 's-14)
ccomp(believing-12, is-15)
det(decline-19, a-17)
amod(decline-19, major-18)
prep_on(is-15, decline-19)

