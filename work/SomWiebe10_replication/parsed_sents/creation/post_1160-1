(ROOT
  (S
    (S
      (PP (IN Like)
        (ADJP (JJS most)))
      (NP (JJ scientific) (NNS theories))
      (VP (VBZ is) (RB not)
        (NP
          (ADJP (CD 100) (NN %))
          (JJ defensible))))
    (, ,)
    (CC but)
    (S
      (PP (IN on)
        (NP
          (NP (DT a) (NN balance))
          (PP (IN of)
            (NP (NNS probabilities)))))
      (, ,)
      (NP (PRP it))
      (VP (VBZ is)
        (NP (DT the)
          (ADJP (RBS most) (JJ likely))
          (NN scenario))))
    (. .)))

