(ROOT
  (S
    (PP (IN By)
      (S
        (VP (VBG using)
          (NP (NNS forensics)))))
    (, ,)
    (NP (PRP we))
    (VP (MD can)
      (VP (VB determine)
        (SBAR
          (WHNP (WP what))
          (S
            (VP (VBD happened)
              (PP (IN in)
                (NP (DT the) (NN past)))
              (PP (IN without)
                (S
                  (VP (VBG having)
                    (S
                      (VP (TO to)
                        (ADVP (RB physically))
                        (VP (VB be)
                          (ADVP (RB there)))))))))))))
    (. .)))

