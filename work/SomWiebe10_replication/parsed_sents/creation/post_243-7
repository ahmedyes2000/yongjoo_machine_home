(ROOT
  (S
    (ADVP (RB Then))
    (NP
      (NP (DT a) (JJ sudden) (NN change))
      (PP (IN in)
        (NP (DT the) (NN environment))))
    (VP (VBZ causes)
      (NP (DT the) (VBG remaining) (NNS specialists))
      (S
        (VP (TO to)
          (VP (VB die)
            (PRT (RP out))))))
    (. .)))

advmod(causes-8, Then-1)
det(change-4, a-2)
amod(change-4, sudden-3)
nsubj(causes-8, change-4)
det(environment-7, the-6)
prep_in(change-4, environment-7)
root(ROOT-0, causes-8)
det(specialists-11, the-9)
amod(specialists-11, remaining-10)
dobj(causes-8, specialists-11)
aux(die-13, to-12)
xcomp(causes-8, die-13)
prt(die-13, out-14)

