(ROOT
  (S
    (NP (DT The) (JJ human) (NN body))
    (VP (VBZ is)
      (ADVP (RB far)
        (PP (IN from)
          (ADJP (JJ perfect))))
      (ADJP (RB especially)
        (PP (IN with)
          (NP (NN regard))))
      (PP (TO to)
        (NP (PRP$ his) (NN physiology))))
    (. .)))

det(body-3, The-1)
amod(body-3, human-2)
nsubj(especially-8, body-3)
cop(especially-8, is-4)
prep_far_from(especially-8, perfect-7)
root(ROOT-0, especially-8)
prep_with(especially-8, regard-10)
poss(physiology-13, his-12)
prep_to(especially-8, physiology-13)

