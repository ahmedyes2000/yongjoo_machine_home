(ROOT
  (S
    (NP
      (NP (DT The) (NNS bacteria))
      (PP (IN in)
        (NP (DT the) (NNS flasks))))
    (NP (NN today))
    (VP (VBP breed)
      (PP
        (ADVP
          (NP (CD 75) (NN %))
          (RBR faster))
        (IN on)
        (NP
          (NP (NN average))
          (PP (IN than)
            (NP (PRP$ their) (JJ original) (NN ancestor))))))
    (. .)))

det(bacteria-2, The-1)
nsubj(breed-7, bacteria-2)
det(flasks-5, the-4)
