(ROOT
  (S
    (NP (NN Anyone))
    (VP (MD would)
      (VP (VB say)
        (SBAR
          (S
            (NP (PRP it))
            (VP (VBZ 's)
              (NP (DT a)
                (ADJP (JJ crazy) (, ,) (JJ flawed) (, ,)
                  (CC and)
                  (RB obviously) (JJ wrong))
                (NN assumption)))))))
    (. .)))

nsubj(say-3, Anyone-1)
aux(say-3, would-2)
root(ROOT-0, say-3)
nsubj(assumption-14, it-4)
cop(assumption-14, 's-5)
det(assumption-14, a-6)
dep(wrong-13, crazy-7)
