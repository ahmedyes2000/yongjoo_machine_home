(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP it))
        (VP (VBZ is)
          (NP (RB only) (DT a) (`` ``) (NN theory) ('' '')))))
    (: ...)
    (NP (DT that))
    (VP (VBZ is)
      (NP
        (NP (DT all))
        (SBAR
          (S
            (NP (PRP it))
            (VP (VBZ has)
              (S
                (VP (TO to)
                  (VP (VB be)))))))))
    (. .)))

mark(theory-7, If-1)
nsubj(theory-7, it-2)
cop(theory-7, is-3)
advmod(theory-7, only-4)
det(theory-7, a-5)
advcl(all-12, theory-7)
nsubj(all-12, that-10)
cop(all-12, is-11)
root(ROOT-0, all-12)
dobj(has-14, all-12)
nsubj(has-14, it-13)
xsubj(be-16, it-13)
rcmod(all-12, has-14)
aux(be-16, to-15)
xcomp(has-14, be-16)

