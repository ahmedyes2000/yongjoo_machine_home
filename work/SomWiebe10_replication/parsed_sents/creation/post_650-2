(ROOT
  (S
    (S
      (NP (DT Either) (NN creation))
      (VP (VBZ is)
        (VP (VBN defined)
          (ADVP (RB independently))
          (PP (IN of)
            (NP (DT the) (NNS facts))))))
    (CC and)
    (RB then)
    (S
      (S
        (NP (DT the) (NNS data))
        (VP (VBP show)
          (NP (NN support))))
      (CC or)
      (S
        (NP (PRP we))
        (VP (VBP have)
          (NP (DT a) (JJ tautological) (NN statement)))))
    (. .)))

det(creation-2, Either-1)
nsubjpass(defined-4, creation-2)
auxpass(defined-4, is-3)
root(ROOT-0, defined-4)
advmod(defined-4, independently-5)
det(facts-8, the-7)
prep_of(defined-4, facts-8)
advmod(defined-4, then-10)
conj_and(defined-4, then-10)
det(data-12, the-11)
nsubj(show-13, data-12)
ccomp(defined-4, show-13)
dobj(show-13, support-14)
nsubj(have-17, we-16)
ccomp(defined-4, have-17)
conj_or(show-13, have-17)
det(statement-20, a-18)
amod(statement-20, tautological-19)
dobj(have-17, statement-20)

