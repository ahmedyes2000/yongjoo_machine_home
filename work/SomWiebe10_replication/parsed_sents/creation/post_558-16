(ROOT
  (S
    (NP (NNP ID))
    (VP (MD can)
      (ADVP (RB only))
      (VP (VB be)
        (VP (VBN declared)
          (ADJP (JJ scientific))
          (SBAR (IN if)
            (S
              (NP
                (NP (DT the) (RBS most) (JJ basic) (NNS principles))
                (PP (IN of)
                  (NP (JJ scientific) (NN inquiry))))
              (VP (VBP are)
                (VP (VBN mocked))))))))
    (. .)))

nsubjpass(declared-5, ID-1)
aux(declared-5, can-2)
advmod(declared-5, only-3)
auxpass(declared-5, be-4)
root(ROOT-0, declared-5)
acomp(declared-5, scientific-6)
mark(mocked-16, if-7)
det(principles-11, the-8)
advmod(principles-11, most-9)
amod(principles-11, basic-10)
nsubjpass(mocked-16, principles-11)
amod(inquiry-14, scientific-13)
prep_of(principles-11, inquiry-14)
auxpass(mocked-16, are-15)
advcl(declared-5, mocked-16)

