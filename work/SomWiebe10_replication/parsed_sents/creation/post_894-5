(ROOT
  (S
    (S
      (INTJ (UH No))
      (, ,)
      (NP (NN nobody))
      (VP (VBZ knows)
        (NP
          (NP (DT the) (NN age))
          (PP (IN of)
            (NP (DT the) (NN earth))))))
    (CC and)
    (S
      (NP (PRP we))
      (ADVP (RB never))
      (VP (MD will)))
    (. .)))

dep(knows-4, No-1)
nsubj(knows-4, nobody-3)
root(ROOT-0, knows-4)
det(age-6, the-5)
dobj(knows-4, age-6)
det(earth-9, the-8)
prep_of(age-6, earth-9)
nsubj(will-13, we-11)
neg(will-13, never-12)
conj_and(knows-4, will-13)

