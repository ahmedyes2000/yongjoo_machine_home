                          (SBAR
                            (WHNP (WP who))
                            (S
                              (VP (VBP make)
                                (NP (NNS mistakes))))))))))))))))
    (. .)))

det(question-3, The-1)
amod(question-3, real-2)
nsubj(is-4, question-3)
root(ROOT-0, is-4)
