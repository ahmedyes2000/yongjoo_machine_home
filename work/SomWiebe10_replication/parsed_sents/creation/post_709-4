  (S
    (NP (DT The) (JJ real) (NN question))
    (VP (VBZ is)
      (SBARQ
        (WHADVP (WRB why))
        (SQ (VBD did) (RB n't)
          (NP (PRP he))
          (VP
            (ADVP (RB just))
            (VB write)
            (S
              (NP (PRP it))
              (NP
                (NP (PRP himself))
                (PP (RB instead) (IN of)
                  (S
