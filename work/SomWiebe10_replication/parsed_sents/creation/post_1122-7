(ROOT
  (S
    (NP (DT The) (JJ human) (NN life))
    (VP (VBZ is)
      (PP (IN like)
        (NP
          (NP (DT a) (NN vapor))
          (PP (IN of)
            (NP (NN smoke)))))
      (PP (TO to)
        (NP (NNP God))))
    (. .)))

det(life-3, The-1)
amod(life-3, human-2)
nsubj(is-4, life-3)
root(ROOT-0, is-4)
det(vapor-7, a-6)
prep_like(is-4, vapor-7)
prep_of(vapor-7, smoke-9)
prep_to(is-4, God-11)

