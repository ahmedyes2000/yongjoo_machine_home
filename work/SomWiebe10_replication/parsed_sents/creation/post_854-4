(ROOT
  (SBARQ
    (SBAR (IN If)
      (S
        (NP
          (NP (DT the) (JJ fossil) (NN record))
          (CC and)
          (NP (PDT all) (DT the) (NN death)))
        (ADVP (RB therein))
        (VP (VBD was)
          (ADVP (RB indeed))
          (VP (VBN deposited)
            (PP (IN within)
              (NP (JJ human) (NN history)))))))
    (, ,)
    (WHADVP (WRB why))
    (SQ (VBP are)
      (NP (EX there))
      (NP
        (NP (DT no) (NNS mammals))
        (PP (IN except)
          (NP (DT the) (JJ upper) (NNS layers)))))
    (. ?)))

mark(deposited-12, If-1)
det(record-4, the-2)
amod(record-4, fossil-3)
nsubjpass(deposited-12, record-4)
predet(death-8, all-6)
det(death-8, the-7)
conj_and(record-4, death-8)
nsubjpass(deposited-12, death-8)
advmod(deposited-12, therein-9)
auxpass(deposited-12, was-10)
advmod(deposited-12, indeed-11)
advcl(are-18, deposited-12)
amod(history-15, human-14)
prep_within(deposited-12, history-15)
advmod(are-18, why-17)
root(ROOT-0, are-18)
expl(are-18, there-19)
det(mammals-21, no-20)
nsubj(are-18, mammals-21)
det(layers-25, the-23)
amod(layers-25, upper-24)
prep_except(mammals-21, layers-25)

