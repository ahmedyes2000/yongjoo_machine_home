(ROOT
  (S
    (NP (NNP Science))
    (VP (VBZ has)
      (VP (VBN uncovered)
        (NP
          (NP (DT a) (JJ vast) (NN amount))
          (PP (IN of)
            (NP
              (NP (NNS fossils))
              (CC and)
              (NP (JJ other) (NN evidence))))
          (SBAR
            (WHNP (WDT that))
            (S
              (VP (VBP show)
                (SBAR
                  (WHADVP (WRB how))
                  (S
                    (NP (NN man))
                    (VP (VBD evolved)
                      (VP (VB form)
                        (NP (DT a) (JJ common) (NN ancestor))))))))))))
    (. .)))

nsubj(uncovered-3, Science-1)
aux(uncovered-3, has-2)
root(ROOT-0, uncovered-3)
det(amount-6, a-4)
amod(amount-6, vast-5)
dobj(uncovered-3, amount-6)
nsubj(show-13, amount-6)
prep_of(amount-6, fossils-8)
