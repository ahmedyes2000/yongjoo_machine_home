(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (EX there))
        (VP (VBZ is)
          (NP (DT a) (NN Designer)))))
    (, ,) (RB then)
    (NP (PRP it))
    (ADVP (RB naturally))
    (VP (VBZ follows)
      (SBAR (IN that)
        (S
          (NP (EX there))
          (VP (MD should)
            (VP (VB be)
              (NP
                (NP (NN evidence))
                (PP (IN for)
                  (NP (NN design)))))))))
    (. .)))

mark(is-3, If-1)
expl(is-3, there-2)
advcl(follows-10, is-3)
det(Designer-5, a-4)
nsubj(is-3, Designer-5)
advmod(follows-10, then-7)
nsubj(follows-10, it-8)
advmod(follows-10, naturally-9)
root(ROOT-0, follows-10)
complm(evidence-15, that-11)
expl(evidence-15, there-12)
aux(evidence-15, should-13)
cop(evidence-15, be-14)
ccomp(follows-10, evidence-15)
prep_for(evidence-15, design-17)

