(ROOT
  (S
    (NP (DT This))
    (VP (VBZ is)
      (ADVP (RB open))
      (NP
        (NP (PRP$ your) (NN interpretation))
        (PP (IN of)
          (NP (DT the) (NNP Bible)))))
    (. .)))

nsubj(interpretation-5, This-1)
cop(interpretation-5, is-2)
advmod(interpretation-5, open-3)
poss(interpretation-5, your-4)
root(ROOT-0, interpretation-5)
det(Bible-8, the-7)
prep_of(interpretation-5, Bible-8)

(ROOT
  (NP
    (NP (NNP Study))
    (PP (IN for)
      (NP (PRP$ your) (NN self)))
    (. !)))

root(ROOT-0, Study-1)
poss(self-4, your-3)
prep_for(Study-1, self-4)

