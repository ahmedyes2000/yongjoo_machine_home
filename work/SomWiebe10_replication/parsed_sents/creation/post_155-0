(ROOT
  (S
    (NP (NNP Satan))
    (VP (VBZ is)
      (ADJP (JJ sure)
        (PP (IN behind)
          (NP
            (NP (DT all))
            (PP (IN of)
              (NP (DT these) (NNS things)))))))
    (. .)))

nsubj(sure-3, Satan-1)
cop(sure-3, is-2)
root(ROOT-0, sure-3)
prep_behind(sure-3, all-5)
det(things-8, these-7)
prep_of(all-5, things-8)

