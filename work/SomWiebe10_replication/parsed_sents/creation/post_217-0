(ROOT
  (S (`` ``)
    (NP (NNP ID))
    (VP (VBZ has)
      (NP
        (NP
          (UCP (JJ legal)
            (CC and)
            (JJ educational))
          (NN merit))
        (SBAR (IN because)
          (S
            (NP (PRP it))
            (VP (VBZ is)
              (NP (DT a)
                (ADJP (JJ bona) (JJ fide))
                (JJ scientific) (NN theory)))))))
    (: ...)))

nsubj(has-3, ID-2)
root(ROOT-0, has-3)
amod(merit-7, legal-4)
conj_and(legal-4, educational-6)
amod(merit-7, educational-6)
dobj(has-3, merit-7)
mark(theory-15, because-8)
nsubj(theory-15, it-9)
cop(theory-15, is-10)
det(theory-15, a-11)
amod(fide-13, bona-12)
amod(theory-15, fide-13)
amod(theory-15, scientific-14)
dep(merit-7, theory-15)

