(ROOT
  (S
    (ADVP (RB Perhaps))
    (NP (NNP Highlander))
    (VP (VBZ knows)
      (NP
        (NP (NN something))
        (SBAR
          (S
            (NP (PRP we))
            (VP (MD may) (RB not)
              (VP (VB know)))))))
    (. ?)))

advmod(knows-3, Perhaps-1)
nsubj(knows-3, Highlander-2)
root(ROOT-0, knows-3)
dobj(knows-3, something-4)
dobj(know-8, something-4)
nsubj(know-8, we-5)
aux(know-8, may-6)
neg(know-8, not-7)
rcmod(something-4, know-8)

