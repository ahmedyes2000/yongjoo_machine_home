  (S
    (NP (PRP I))
    (VP (MD 'd)
      (VP (VB hate)
        (S
          (VP (TO to)
            (VP (VB see)
              (NP
                (NP (PRP$ your) (NN case))
                (VP (VBN weakened))))))))
    (. .)))

nsubj(hate-3, I-1)
