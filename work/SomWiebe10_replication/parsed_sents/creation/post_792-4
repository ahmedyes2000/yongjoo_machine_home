(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (ADVP (RB truly))
        (VP (VBP want)
          (S
            (VP (TO to)
              (VP (VB find)
                (NP (DT a) (JJ great) (NN relationship))
                (PP (IN with)
                  (NP (NNP God)))))))))
    (, ,)
    (ADVP (RB just))
    (VP (VB believe)
      (NP (PRP Him)))
    (. .)))

mark(want-4, If-1)
nsubj(want-4, you-2)
xsubj(find-6, you-2)
advmod(want-4, truly-3)
advcl(believe-14, want-4)
aux(find-6, to-5)
xcomp(want-4, find-6)
det(relationship-9, a-7)
amod(relationship-9, great-8)
dobj(find-6, relationship-9)
prep_with(find-6, God-11)
advmod(believe-14, just-13)
root(ROOT-0, believe-14)
dobj(believe-14, Him-15)

