(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is)
      (ADJP (JJ impossible)
        (S
          (VP (TO to)
            (VP (VB know)
              (WHNP (WDT which))))))
      (, ,)
      (SBAR (IN if)
        (S
          (NP
            (NP (DT any))
            (, ,)
            (PP (IN of)
              (NP (DT these) (JJ contradictory) (NNS opinions))))
          (VP (VBZ is)
            (ADJP (JJ true))))))
    (. .)))

nsubj(impossible-3, It-1)
cop(impossible-3, is-2)
root(ROOT-0, impossible-3)
aux(know-5, to-4)
xcomp(impossible-3, know-5)
dobj(know-5, which-6)
mark(true-16, if-8)
nsubj(true-16, any-9)
det(opinions-14, these-12)
amod(opinions-14, contradictory-13)
prep_of(any-9, opinions-14)
cop(true-16, is-15)
advcl(impossible-3, true-16)

