(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ 's)
      (NP
        (NP (DT a) (NN code))
        (, ,)
        (NP (RB basically) (DT a) (NN language))
        (, ,)
        (CC and)
        (NP
          (NP (DT a) (NN storage) (NN device))
          (PP (IN for)
            (NP (NN information))))))
    (. .)))

nsubj(code-4, It-1)
cop(code-4, 's-2)
det(code-4, a-3)
root(ROOT-0, code-4)
advmod(language-8, basically-6)
det(language-8, a-7)
conj_and(code-4, language-8)
det(device-13, a-11)
nn(device-13, storage-12)
conj_and(code-4, device-13)
prep_for(device-13, information-15)

