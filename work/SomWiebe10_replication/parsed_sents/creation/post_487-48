(ROOT
  (FRAG
    (S
      (SBAR (IN If)
        (S
          (NP (DT this))
          (VP (VBZ is)
            (NP (DT the) (NN case)))))
      (: --)
      (ADVP (RB then))
      (NP (NN someone))
      (VP (VBP please)
        (VP (VB explain)
          (PP (TO to)
            (NP (PRP me)))
          (SBAR
            (WHADVP (WRB why))
            (S
              (NP (PRP they))
              (VP (MD would)
                (VP (VB answer) ('' '')
                  (NP (NNP God))))))
          ('' '')
          (PP (TO to)
            (NP (DT the) (NN question)))
          (`` ``)
          (SBARQ
            (WHNP (WP who))
            (SQ (VBD was)
              (NP (DT the) (NN designer)))
            (. ?)))))))

mark(case-5, If-1)
nsubj(case-5, this-2)
cop(case-5, is-3)
det(case-5, the-4)
advcl(please-9, case-5)
