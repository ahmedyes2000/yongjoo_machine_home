(ROOT
  (S
    (NP (NN Somebody))
    (VP (VBD designed)
      (NP (PRP it)))
    (. .)))

nsubj(designed-2, Somebody-1)
root(ROOT-0, designed-2)
dobj(designed-2, it-3)

(ROOT
  (S
    (S
      (VP (VBG Pointing)
        (PRT (RP out))
        (NP
          (NP (NN design))
          (PP (IN in)
            (NP (NN nature))))))
    (VP (VBZ does) (RB not)
      (VP (VB make)
        (S
          (NP (NNP ID))
          (ADJP (JJ unconstitutional)))))
    (. !)))

csubj(make-8, Pointing-1)
prt(Pointing-1, out-2)
dobj(Pointing-1, design-3)
prep_in(design-3, nature-5)
aux(make-8, does-6)
neg(make-8, not-7)
root(ROOT-0, make-8)
nsubj(unconstitutional-10, ID-9)
xcomp(make-8, unconstitutional-10)

