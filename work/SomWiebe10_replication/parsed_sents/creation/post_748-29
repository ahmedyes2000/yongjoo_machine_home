(ROOT
  (S
    (PP (IN beyond)
      (NP (DT that)))
    (NP (PRP they))
    (VP (VBP are)
      (ADVP (RB simply))
      (NP
        (NP (JJ different) (NNS splinters))
        (PP (IN of)
          (NP (NNP Christianity)))))
    (. .)))

prep_beyond(splinters-7, that-2)
nsubj(splinters-7, they-3)
cop(splinters-7, are-4)
advmod(splinters-7, simply-5)
amod(splinters-7, different-6)
root(ROOT-0, splinters-7)
prep_of(splinters-7, Christianity-9)

