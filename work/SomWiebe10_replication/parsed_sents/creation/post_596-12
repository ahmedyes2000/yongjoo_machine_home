(ROOT
  (S
    (NP (PRP You))
    (VP (VBP write)
      (SBAR
        (S
          (NP (NNP ID))
          (VP (VBZ is)
            (ADVP (RB merely))
            (NP (DT a) (JJ negative) (NN argument))))))
    (. .)))

nsubj(write-2, You-1)
root(ROOT-0, write-2)
nsubj(argument-8, ID-3)
cop(argument-8, is-4)
advmod(argument-8, merely-5)
det(argument-8, a-6)
amod(argument-8, negative-7)
ccomp(write-2, argument-8)

