(ROOT
  (S
    (NP
      (NP (-LRB- -LRB-)
        (QP (CD 2))
        (-RRB- -RRB-))
      (SBAR
        (S (-LRB- -LRB-)
          (NP (NNP CSI))
          (VP (VBD was)))))
    (VP (VBD described)
      (PP (IN in)
        (NP
          (NP (JJR greater) (NN detail))
          (PP (IN in)
            (NP (PRP$ my) (JJ first) (NN opening) (NN statement))))))
    (. .) (-RRB- -RRB-)))

nsubj(described-7, 2-2)
nsubj(was-6, CSI-5)
rcmod(2-2, was-6)
root(ROOT-0, described-7)
amod(detail-10, greater-9)
prep_in(described-7, detail-10)
poss(statement-15, my-12)
amod(statement-15, first-13)
nn(statement-15, opening-14)
prep_in(detail-10, statement-15)

