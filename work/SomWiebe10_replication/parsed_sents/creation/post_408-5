(ROOT
  (S
    (NP (EX There))
    (VP (VBZ is)
      (NP (DT no) (NN evidence)
        (S
          (VP (TO to)
            (VP (VB support)
              (NP (DT this) (NN idea)))))))
    (. .)))

expl(is-2, There-1)
root(ROOT-0, is-2)
det(evidence-4, no-3)
nsubj(is-2, evidence-4)
aux(support-6, to-5)
infmod(evidence-4, support-6)
det(idea-8, this-7)
dobj(support-6, idea-8)

