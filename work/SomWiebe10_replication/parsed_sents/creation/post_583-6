(ROOT
  (S ('' '')
    (NP (RB So) (JJ then) (NN Evolution))
    (VP (VBZ has)
      (NP
        (NP (DT no) (NN merit))
        (SBAR (IN because)
          (S
            (NP (PRP it))
            (VP (VBZ fits)
              (NP
                (NP (NN anything)
                  (CC and)
                  (NN everything))
                (PP (IN in)
                  (NP (NN biology)))))))))
    (. .)))

advmod(Evolution-4, So-2)
amod(Evolution-4, then-3)
nsubj(has-5, Evolution-4)
root(ROOT-0, has-5)
det(merit-7, no-6)
dobj(has-5, merit-7)
mark(fits-10, because-8)
nsubj(fits-10, it-9)
dep(merit-7, fits-10)
dobj(fits-10, anything-11)
dobj(fits-10, everything-13)
conj_and(anything-11, everything-13)
prep_in(anything-11, biology-15)

