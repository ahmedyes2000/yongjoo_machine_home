(ROOT
  (S
    (NP (PRP They))
    (VP (MD would)
      (SBAR (IN if)
        (S
          (NP (PRP you))
          (VP (MD would)
            (VP (VB allow)
              (S
                (NP (PRP them))
                (VP (TO to)
                  (VP (VB do)
                    (NP (PRP it))))))))))
    (. .)))

nsubj(would-2, They-1)
root(ROOT-0, would-2)
mark(allow-6, if-3)
nsubj(allow-6, you-4)
aux(allow-6, would-5)
advcl(would-2, allow-6)
nsubj(do-9, them-7)
aux(do-9, to-8)
xcomp(allow-6, do-9)
dobj(do-9, it-10)

