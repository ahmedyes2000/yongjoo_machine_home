(ROOT
  (S
    (NP (NNS Others))
    (VP (VBP have)
      (ADVP (RB similarly))
      (VP (VBN shown)
        (SBAR
          (WHADVP (WRB how))
          (S
            (NP
              (NP (NNP Behe) (POS 's))
              (NNS claims))
            (VP (VBP are)
              (VP (VBN contradicted)
                (PP (IN by)
                  (NP (DT the) (NN evidence)))))))))
    (. .)))

nsubj(shown-4, Others-1)
aux(shown-4, have-2)
advmod(shown-4, similarly-3)
root(ROOT-0, shown-4)
advmod(contradicted-10, how-5)
poss(claims-8, Behe-6)
nsubjpass(contradicted-10, claims-8)
auxpass(contradicted-10, are-9)
ccomp(shown-4, contradicted-10)
det(evidence-13, the-12)
agent(contradicted-10, evidence-13)

