(ROOT
  (S
    (NP (DT The) (NN world))
    (VP (VBZ is)
      (NP
        (NP (NNP neither) (CD 4,000) (, ,) (CD 6,000))
        (, ,)
        (CC or)
        (ADJP
          (NP (CD 37,000) (NNS years))
          (JJ old))))
    (. .)))

det(world-2, The-1)
nsubj(neither-4, world-2)
nsubj(old-12, world-2)
cop(neither-4, is-3)
root(ROOT-0, neither-4)
num(neither-4, 4,000-5)
num(neither-4, 6,000-7)
num(years-11, 37,000-10)
npadvmod(old-12, years-11)
conj_or(neither-4, old-12)

