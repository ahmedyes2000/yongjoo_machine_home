(ROOT
  (S
    (S
      (NP (DT Some))
      (VP (VBP make)
        (NP
          (NP (DT the) (NN statement))
          (SBAR
            (S
              (NP (PRP it))
              (VP (MD may)
                (VP (VB appear)
                  (VP (VBN designed)))))))))
    (CC but)
    (S
      (NP (PRP it))
      (VP (VBZ is) (RB not)))
    (. .)))

nsubj(make-2, Some-1)
root(ROOT-0, make-2)
det(statement-4, the-3)
dobj(make-2, statement-4)
nsubjpass(designed-8, it-5)
aux(designed-8, may-6)
auxpass(designed-8, appear-7)
rcmod(statement-4, designed-8)
nsubj(is-11, it-10)
conj_but(make-2, is-11)
neg(is-11, not-12)

