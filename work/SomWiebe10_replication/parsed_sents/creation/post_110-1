(ROOT
  (S (CC and)
    (NP (DT these) (NNS debates))
    (VP (VBP make)
      (SBAR
        (S
          (NP (PRP me))
          (VP (VBP feel)
            (SBAR (IN as)
              (FRAG
                (SBAR (IN if)
                  (S
                    (NP (PRP I))
                    (VP (VBP 'm)
                      (VP (VBN submitted)
                        (PP (IN as)
                          (S
                            (VP (VBG being)
                              (ADVP (RB so)))))))))
                (. .)))))))
    (. .)))

cc(make-4, and-1)
det(debates-3, these-2)
nsubj(make-4, debates-3)
root(ROOT-0, make-4)
nsubj(feel-6, me-5)
ccomp(make-4, feel-6)
mark(submitted-11, as-7)
mark(submitted-11, if-8)
nsubjpass(submitted-11, I-9)
auxpass(submitted-11, 'm-10)
advcl(feel-6, submitted-11)
prepc_as(submitted-11, being-13)
advmod(being-13, so-14)

