  (S
    (NP (DT Those) (NNS assumptions))
    (VP (VBP allow)
      (S
        (NP (NNP Dawkins))
        (VP (TO to)
          (VP (VB be)
            (NP (DT an)
              (ADJP (RB intellectually) (JJ fulfilled))
              (NN atheist))))))
    (. .)))

det(assumptions-2, Those-1)
nsubj(allow-3, assumptions-2)
root(ROOT-0, allow-3)
