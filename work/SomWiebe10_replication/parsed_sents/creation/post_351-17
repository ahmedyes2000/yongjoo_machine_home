(ROOT
  (S
    (ADVP (IN At) (JJS best))
    (NP (PRP one))
    (VP (MD may)
      (VP (VB argue)
        (SBAR (IN that)
          (S
            (NP (PRP it))
            (VP (VBZ is)
              (ADJP (JJ consistent))
              (PP (IN with)
                (NP (JJ such)))
              (, ,)
              (ADJP (IN although))
              (SBAR (RB even) (IN that)
                (S
                  (NP (PRP one))
                  (VP (VBZ lacks)
                    (PP (IN in)
                      (NP (JJ sufficient) (JJ theoretical) (NN evidence)))))))))))
    (. .)))

advmod(argue-5, At-1)
pobj(At-1, best-2)
nsubj(argue-5, one-3)
aux(argue-5, may-4)
root(ROOT-0, argue-5)
complm(consistent-9, that-6)
nsubj(consistent-9, it-7)
cop(consistent-9, is-8)
