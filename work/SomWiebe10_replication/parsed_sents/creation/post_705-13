              (PP (IN from)
                (NP (DT a) (JJ Christian) (NN perspective)))
              (NP
                (NP (PRP it))
                (VP (VBG claiming)
                  (SBAR (IN that)
                    (S
                      (NP (DT the) (NNP Bible))
                      (VP (VBZ is)
                        (PP (IN of)
                          (NP (JJ divine) (NN origin))))))))
              (VP (VBZ is)
                (NP
                  (NP (DT an) (JJ important) (NN part))
                  (PP (IN of)
                    (NP (DT the) (JJ Christian) (NN faith))))))))))
    (CC But)
    (S
      (NP (DT the) (JJ same))
      (VP (VBZ does) (RB not)
        (VP (VB hold)
          (PP (IN for)
            (NP
              (NP (DT those))
              (SBAR
                (WHNP (WP who))
                (S
                  (VP (VBP are) (RB not)
                    (NP (NNP Christian))))))))))
    (. .)))

