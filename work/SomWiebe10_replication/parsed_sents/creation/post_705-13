(ROOT
  (S
    (S
      (NP (DT each) (NN section))
      (VP (VBZ has)
        (S
          (VP (TO to)
            (VP (VB be)
              (VP (VBN interpreted)
                (ADVP (RB individually))
                (PP (IN for)
                  (NP
                    (NP (PRP it) (POS 's))
                    (VBN given) (NN intent)))))))))
    (: ...)
    (S
      (NP (PRP it))
      (VP (VBZ 's)
        (ADVP (RB simply))
        (NP
          (NP (RB not) (DT an))
          (`` ``)
          (NP (DT all)
            (CC or)
            (NN nothing))
          ('' '')))
      (. .))
    (. .)))

