(ROOT
  (S ('' '')
    (NP (EX There))
    (VP (VBZ is)
      (NP
        (NP (DT no) (NN proof))
        (PP (IN of)
          (NP
            (NP (DT either) (NN side))
            (PP (IN in)
              (NP (DT this) (NN issue)))))))
    (. .)))

expl(is-3, There-2)
root(ROOT-0, is-3)
det(proof-5, no-4)
nsubj(is-3, proof-5)
det(side-8, either-7)
prep_of(proof-5, side-8)
det(issue-11, this-10)
prep_in(side-8, issue-11)

