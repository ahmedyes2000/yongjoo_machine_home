aux(refuted-13, have-11)
auxpass(refuted-13, been-12)
ccomp(assert-2, refuted-13)
conj_and(heard-7, refuted-13)
advmod(ignored-19, thus-15)
nsubjpass(ignored-19, they-16)
auxpass(ignored-19, are-17)
advmod(ignored-19, legitimately-18)
conj_and(assert-2, ignored-19)
mark(come-22, until-20)
nsubj(come-22, they-21)
