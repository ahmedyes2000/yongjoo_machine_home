(ROOT
  (S
    (NP (PRP I))
    (VP (VBP think)
      (SBAR
        (S
          (NP (PRP you))
          (VP (VBD hit)
            (NP (DT the) (NN nail))
            (PP (IN on)
              (NP (DT the) (NN head)))
            (ADVP (RB here))))))
    (. .)))

nsubj(think-2, I-1)
root(ROOT-0, think-2)
nsubj(hit-4, you-3)
ccomp(think-2, hit-4)
det(nail-6, the-5)
dobj(hit-4, nail-6)
det(head-9, the-8)
prep_on(hit-4, head-9)
advmod(hit-4, here-10)

