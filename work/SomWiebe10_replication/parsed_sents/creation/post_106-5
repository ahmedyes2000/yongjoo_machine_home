(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (DT that))
        (VP (VBZ is)
          (ADVP (RB so))
          (ADVP (RB then)))))
    (NP (DT the) (NN question))
    (VP (VBZ is)
      (VP (VBN mooted)))
    (. .)))

mark(is-3, If-1)
nsubj(is-3, that-2)
advcl(mooted-9, is-3)
advmod(is-3, so-4)
advmod(is-3, then-5)
det(question-7, the-6)
nsubjpass(mooted-9, question-7)
auxpass(mooted-9, is-8)
root(ROOT-0, mooted-9)

