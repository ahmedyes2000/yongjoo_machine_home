  (S
    (NP (PRP It))
    (VP (VBZ is)
      (NP (DT the) (NN argument))
      (, ,) (RB not)
      (NP
        (NP (DT the) (JJ personal) (NN philosophy)
          (CC or)
          (NN belief))
        (PP (IN of)
          (NP
            (NP (DT the) (NN scientist))
            (, ,)
            (SBAR
              (WHNP (WDT that))
              (S
                (VP (MD should)
                  (VP (VB be)
                    (SBAR
                      (WHNP (WP what))
                      (S
                        (VP (VBZ is)
                          (ADJP (JJ important)))))))))))))
    (. .)))

