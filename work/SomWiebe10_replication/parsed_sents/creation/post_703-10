(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is)
      (NP (DT a) (NN love) (NN story))
      (SBAR (IN that)
        (S
          (NP (NNP God))
          (VP (VBD came)
            (PP (TO to)
              (NP (DT this) (NN world)))
            (S
              (VP (TO to)
                (VP (VB save)
                  (NP (PRP us))
                  (PP (IN from)
                    (NP (DT the) (JJ destructive) (NN sin))))))))))
    (. .)))

nsubj(story-5, It-1)
cop(story-5, is-2)
det(story-5, a-3)
nn(story-5, love-4)
root(ROOT-0, story-5)
complm(came-8, that-6)
nsubj(came-8, God-7)
xsubj(save-13, God-7)
ccomp(story-5, came-8)
det(world-11, this-10)
prep_to(came-8, world-11)
aux(save-13, to-12)
xcomp(came-8, save-13)
dobj(save-13, us-14)
det(sin-18, the-16)
amod(sin-18, destructive-17)
prep_from(save-13, sin-18)

