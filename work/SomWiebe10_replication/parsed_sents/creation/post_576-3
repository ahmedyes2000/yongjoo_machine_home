(ROOT
  (S
    (S
      (VP (VB shoot)))
    (, ,)
    (VP (VBZ seems)
      (SBAR (IN like)
        (S
          (NP (NN anything))
          (VP (VBZ is)
            (VP (VBN tried)
              (NP (DT these) (NNS days)))))))
    (. .)))

ccomp(seems-3, shoot-1)
root(ROOT-0, seems-3)
mark(tried-7, like-4)
nsubjpass(tried-7, anything-5)
auxpass(tried-7, is-6)
advcl(seems-3, tried-7)
det(days-9, these-8)
tmod(tried-7, days-9)

