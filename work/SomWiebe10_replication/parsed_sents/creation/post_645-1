(ROOT
  (S
    (NP
      (NP (DT The) (NN term) (NN creation))
      (CC and)
      (NP (NNS creationists)))
    (VP (VBP have)
      (NP
        (ADJP (RB very) (JJ clear)
          (CC and)
          (JJ religious))
        (NNS meanings)))
    (. .)))

det(creation-3, The-1)
nn(creation-3, term-2)
nsubj(have-6, creation-3)
conj_and(creation-3, creationists-5)
nsubj(have-6, creationists-5)
root(ROOT-0, have-6)
advmod(clear-8, very-7)
amod(meanings-11, clear-8)
conj_and(clear-8, religious-10)
amod(meanings-11, religious-10)
dobj(have-6, meanings-11)

