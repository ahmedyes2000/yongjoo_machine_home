(ROOT
  (SBARQ
    (PP (IN In)
      (NP (JJ other) (NNS words)))
    (, ,)
    (SQ (VBZ are) (RB n't)
      (NP (NNS people))
      (ADJP (JJ wonderful))
      (SBAR
        (ADVP (RB simply))
        (IN because)
        (S
          (NP (PRP they))
          (VP (VBP are)
            (NP (NNS people))))))
    (. ?)))

amod(words-3, other-2)
prep_in(wonderful-8, words-3)
cop(wonderful-8, are-5)
neg(wonderful-8, n't-6)
nsubj(wonderful-8, people-7)
root(ROOT-0, wonderful-8)
advmod(people-13, simply-9)
dep(people-13, because-10)
nsubj(people-13, they-11)
cop(people-13, are-12)
dep(wonderful-8, people-13)

