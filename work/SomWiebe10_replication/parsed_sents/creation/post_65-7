(ROOT
  (S
    (NP (PDT All) (PRP$ our) (NN exoperience))
    (VP (VBZ tells)
      (NP (PRP us))
      (NP
        (NP (DT a) (NN designer))
        (SBAR
          (S
            (VP (VBZ is)
              (VP (VBN required)))))))
    (. .)))

predet(exoperience-3, All-1)
poss(exoperience-3, our-2)
nsubj(tells-4, exoperience-3)
root(ROOT-0, tells-4)
iobj(tells-4, us-5)
det(designer-7, a-6)
dobj(tells-4, designer-7)
auxpass(required-9, is-8)
rcmod(designer-7, required-9)

