(ROOT
  (S ('' '')
    (NP (PRP I))
    (VP (MD would)
      (ADVP (RB hardly))
      (VP (VB call)
        (SBAR
          (WHNP (WP what))
          (S
            (NP (DT the) (NNP NCSE))
            (VP (VBZ puts)
              (PRT (RP out))
              (PP (IN as)
                (ADJP (JJ neutral)))
              (PP (IN in)
                (NP
                  (NP (DT the) (NN matter))
                  (PP (IN of)
                    (NP (JJ public) (NNS relations))))))))))
    (. .)))

nsubj(call-5, I-2)
aux(call-5, would-3)
advmod(call-5, hardly-4)
root(ROOT-0, call-5)
dobj(puts-9, what-6)
det(NCSE-8, the-7)
nsubj(puts-9, NCSE-8)
ccomp(call-5, puts-9)
prt(puts-9, out-10)
prep_as(puts-9, neutral-12)
det(matter-15, the-14)
prep_in(puts-9, matter-15)
amod(relations-18, public-17)
prep_of(matter-15, relations-18)

