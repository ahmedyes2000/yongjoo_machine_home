(ROOT
  (S
    (NP (PRP We))
    (VP (VBD encounter)
      (NP
        (NP (NNS galaxies))
        (PP (IN of)
          (NP
            (NP (NNS people))
            (NP (DT every) (NN day)))))
      (, ,)
      (PP (IN on)
        (NP (DT the) (NN street)))
      (, ,)
      (PP (IN at)
        (NP (NN work)))
      (, ,)
      (PP
        (PP (IN in)
          (NP (PRP$ our) (NNS neighborhoods)))
        (, ,)
        (CC and)
        (PP (IN on)
          (NP (DT the) (NNS streets)
            (CC and)
            (NNS highways)))))
    (. .)))

nsubj(encounter-2, We-1)
nsubj(encounter-2', We-1)
root(ROOT-0, encounter-2)
conj_and(encounter-2, encounter-2')
dobj(encounter-2, galaxies-3)
prep_of(galaxies-3, people-5)
det(day-7, every-6)
dep(people-5, day-7)
det(street-11, the-10)
prep_on(encounter-2, street-11)
prep_at(encounter-2, work-14)
poss(neighborhoods-18, our-17)
prep_in(encounter-2, neighborhoods-18)
det(streets-23, the-22)
prep_on(encounter-2', streets-23)
prep_on(encounter-2', highways-25)
conj_and(streets-23, highways-25)

