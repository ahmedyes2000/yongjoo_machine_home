(ROOT
  (S
    (NP (PRP We))
    (VP
      (VP (VBP 're)
        (ADJP (RB perfectly) (VBN placed)
          (PP (IN in)
            (NP (DT the) (NN universe)))))
      (CC and)
      (VP (VBP thrive)
        (PP (RB because) (IN of)
          (NP (PRP it)))))
    (. .)))

nsubjpass(placed-4, We-1)
nsubj(thrive-9, We-1)
auxpass(placed-4, 're-2)
advmod(placed-4, perfectly-3)
root(ROOT-0, placed-4)
det(universe-7, the-6)
prep_in(placed-4, universe-7)
conj_and(placed-4, thrive-9)
prep_because_of(thrive-9, it-12)

