(ROOT
  (S
    (ADVP (UH OK))
    (NP (PRP I))
    (VP (VBP 'm)
      (VP (VBG going)
        (S
          (VP (TO to)
            (VP (VB stop)
              (ADVP (RB there)))))))
    (. .)))

advmod(going-4, OK-1)
nsubj(going-4, I-2)
xsubj(stop-6, I-2)
aux(going-4, 'm-3)
root(ROOT-0, going-4)
aux(stop-6, to-5)
xcomp(going-4, stop-6)
advmod(stop-6, there-7)

