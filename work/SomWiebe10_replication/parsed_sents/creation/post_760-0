(ROOT
  (S
    (S
      (NP (PRP You))
      (VP (MD ca) (RB n't)
        (VP (VB use)
          (S
            (NP (DT the) (NNP Bible))
            (VP (TO to)
              (VP (VB prove)
                (NP (DT the) (NNP Bible))))))))
    (, ,) (RB thus)
    (S
      (NP (DT that))
      (VP (VBZ is)
        (NP (DT an)
          (ADJP (JJ invalid))
          (NN argument))))
    (. .)))

nsubj(use-4, You-1)
aux(use-4, ca-2)
neg(use-4, n't-3)
root(ROOT-0, use-4)
det(Bible-6, the-5)
nsubj(prove-8, Bible-6)
aux(prove-8, to-7)
xcomp(use-4, prove-8)
det(Bible-10, the-9)
dobj(prove-8, Bible-10)
advmod(use-4, thus-12)
nsubj(argument-17, that-13)
cop(argument-17, is-14)
det(argument-17, an-15)
amod(argument-17, invalid-16)
ccomp(use-4, argument-17)

