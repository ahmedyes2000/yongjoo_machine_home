(ROOT
  (S
    (NP (PRP I))
    (VP (VBP 'm)
      (VP (VBN flawed)
        (PP (IN like)
          (NP (NN everyone)))))
    (. .)))

nsubjpass(flawed-3, I-1)
auxpass(flawed-3, 'm-2)
root(ROOT-0, flawed-3)
prep_like(flawed-3, everyone-5)

(ROOT
  (S (CC But)
    (NP (PRP they))
    (VP (VBP are)
      (NP
        (NP (DT the) (NNS answers))
        (SBAR
          (S
            (NP (PRP you))
            (VP (VBP have)
              (VP (VBN come)
                (S
                  (VP (TO to)))))))))
    (. .)))

cc(answers-5, But-1)
nsubj(answers-5, they-2)
cop(answers-5, are-3)
det(answers-5, the-4)
root(ROOT-0, answers-5)
dobj(come-8, answers-5)
nsubj(come-8, you-6)
xsubj(to-9, you-6)
aux(come-8, have-7)
rcmod(answers-5, come-8)
xcomp(come-8, to-9)

