(ROOT
  (S
    (NP (JJ YOUR) (NN position))
    (, ,)
    (ADVP (RB however))
    (, ,)
    (VP (VBZ counts)
      (PP (IN on)
        (NP (NN ignorance))))
    (. .)))

amod(position-2, YOUR-1)
nsubj(counts-6, position-2)
advmod(counts-6, however-4)
root(ROOT-0, counts-6)
prep_on(counts-6, ignorance-8)

