(ROOT
  (S
    (NP (EX There))
    (VP (VBZ is)
      (NP (DT no) (NN reason)
        (S
          (VP (TO to)
            (VP (VB disallow)
              (NP
                (NP (DT all) (NNS views))
                (PP (CC but)
                  (NP
                    (NP (NNS ones))
                    (SBAR
                      (WHNP (WDT that))
                      (S
                        (VP (VBP fit)
                          (PP (IN within)
                            (NP
                              (NP (DT the) (JJ beloved) (NN worldview))
                              (PP (IN of)
                                (NP (NN naturalism))))))))))))))))
    (. .)))

expl(is-2, There-1)
root(ROOT-0, is-2)
det(reason-4, no-3)
nsubj(is-2, reason-4)
aux(disallow-6, to-5)
infmod(reason-4, disallow-6)
det(views-8, all-7)
dobj(disallow-6, views-8)
prep(views-8, but-9)
