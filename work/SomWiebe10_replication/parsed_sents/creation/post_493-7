(ROOT
  (S
    (FRAG
      (NP (NNS Defendants)))
    ('' '')
    (NP (JJ own) (NN expert) (NNS witnesses))
    (VP (VBD acknowledged)
      (NP (DT this) (NN point)))
    (. .)))

dep(acknowledged-6, Defendants-1)
amod(witnesses-5, own-3)
nn(witnesses-5, expert-4)
nsubj(acknowledged-6, witnesses-5)
root(ROOT-0, acknowledged-6)
det(point-8, this-7)
dobj(acknowledged-6, point-8)

