(ROOT
  (NP
    (NP (JJ Other))
    (PP (IN than)
      (NP (DT these) (JJ nasty) (NNS tsunamis)
        (CC and)
        (NNS hurricanes)))
    (: ...)))

root(ROOT-0, Other-1)
det(tsunamis-5, these-3)
amod(tsunamis-5, nasty-4)
prep_than(Other-1, tsunamis-5)
prep_than(Other-1, hurricanes-7)
conj_and(tsunamis-5, hurricanes-7)

