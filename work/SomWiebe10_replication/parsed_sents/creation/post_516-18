(ROOT
  (S (CC But)
    (NP (RB not) (NN everyone))
    (VP (VBZ has)
      (ADVP (RB as) (RB much))
      (VP (VBN faith)
        (PP (IN in)
          (NP
            (NP (DT the) (JJ random) (NNS powers))
            (PP (IN of)
              (NP (NN evolution)))))))
    (. .)))

cc(faith-7, But-1)
neg(everyone-3, not-2)
nsubj(faith-7, everyone-3)
aux(faith-7, has-4)
advmod(much-6, as-5)
advmod(faith-7, much-6)
root(ROOT-0, faith-7)
det(powers-11, the-9)
