(ROOT
  (S (CC And)
    (ADVP
      (ADVP (RB as) (RB far))
      (PP (IN as)
        (NP
          (NP (PRP$ your) (NN question))
          (PP (IN about)
            (NP (WDT what) (NN merit))))))
    (NP (DT the) (NNP Bible))
    (VP (VBZ has)
      (VP
        (PP (TO to)
          (NP (NNS non-Christians)))))
    (: ...)))

cc(to-13, And-1)
advmod(far-3, as-2)
advmod(to-13, far-3)
poss(question-6, your-5)
prep_as(far-3, question-6)
dep(merit-9, what-8)
prep_about(question-6, merit-9)
det(Bible-11, the-10)
nsubj(to-13, Bible-11)
aux(to-13, has-12)
root(ROOT-0, to-13)
pobj(to-13, non-Christians-14)

