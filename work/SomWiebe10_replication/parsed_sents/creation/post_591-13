(ROOT
  (S
    (NP (JJ Next) (NN time))
    (NP (PRP I))
    (VP (VBP respond)
      (SBAR
        (S
          (NP (PRP I))
          (VP (VBP do)
            (NP (NN plan))
            (PP (IN on)
              (S
                (VP (VBG giving)
                  (NP (JJR more) (NN evidence))
                  (PP
                    (PP (IN for)
                      (NP (NN creation)))
                    (CC and)
                    (PP (IN against)
                      (NP (NN evolution)))))))))))
    (. .)))

amod(time-2, Next-1)
nsubj(respond-4, time-2)
nsubj(respond-4, I-3)
root(ROOT-0, respond-4)
nsubj(do-6, I-5)
ccomp(respond-4, do-6)
dobj(do-6, plan-7)
prepc_on(do-6, giving-9)
prepc_on(do-6, giving-9')
conj_and(giving-9, giving-9')
amod(evidence-11, more-10)
dobj(giving-9, evidence-11)
prep_for(giving-9, creation-13)
prep_against(giving-9', evolution-16)

