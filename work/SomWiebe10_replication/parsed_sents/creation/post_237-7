(ROOT
  (S
    (NP (PRP We))
    (VP (MD would)
      (VP (VB look)
        (PP (IN for)
          (NP
            (NP (NN evidence))
            (PP (IN of)
              (NP
                (NP (DT that) (NN being))
                (CC or)
                (NP (NNS others))))))
        (PP (IN like)
          (NP (PRP it)))))
    (. .)))

nsubj(look-3, We-1)
aux(look-3, would-2)
root(ROOT-0, look-3)
prep_for(look-3, evidence-5)
det(being-8, that-7)
prep_of(evidence-5, being-8)
prep_of(evidence-5, others-10)
conj_or(being-8, others-10)
prep_like(look-3, it-12)

