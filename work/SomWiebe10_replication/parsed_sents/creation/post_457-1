  (S
    (S
      (NP (PRP You))
      (VP (VBP call)
        (S
          (NP (PRP it))
          (NP (DT a) (NN tactic)))))
    (, ,)
    (NP (PRP I))
    (VP (VBP call)
      (S
        (NP (PRP it))
        (VP (VB reason))))
    (. .)))

nsubj(call-2, You-1)
ccomp(call-8, call-2)
nsubj(tactic-5, it-3)
