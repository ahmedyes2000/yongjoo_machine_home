(ROOT
  (S
    (S
      (NP (DT Both))
      (VP (VBD put)
        (NP (NN man))
        (PP (IN in)
          (NP (DT a) (JJ primary) (NN position)))))
    (, ,)
    (CC but)
    (S
      (NP (PRP they))
      (VP (VBP have)
        (NP (JJ different) (NNS orders))))
    (. .)))

nsubj(put-2, Both-1)
root(ROOT-0, put-2)
dobj(put-2, man-3)
det(position-7, a-5)
amod(position-7, primary-6)
prep_in(put-2, position-7)
nsubj(have-11, they-10)
conj_but(put-2, have-11)
amod(orders-13, different-12)
dobj(have-11, orders-13)

