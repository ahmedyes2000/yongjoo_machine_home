(ROOT
  (S
    (SBAR
      (WHADVP (WRB Whenever))
      (S
        (NP (NN naturalism))
        (VP (VBZ goes)
          (PP (IN beyond)
            (NP (PRP$ its) (NN methodology))))))
    (, ,)
    (NP (PRP it))
    (VP (VBZ becomes)
      (NP (DT an) (NN ideology))
      (, ,)
      (PP (RB not) (IN unlike)
        (NP (NN religion))))
    (. .)))

advmod(goes-3, Whenever-1)
nsubj(goes-3, naturalism-2)
advcl(ideology-11, goes-3)
poss(methodology-6, its-5)
prep_beyond(goes-3, methodology-6)
nsubj(ideology-11, it-8)
cop(ideology-11, becomes-9)
det(ideology-11, an-10)
root(ROOT-0, ideology-11)
neg(ideology-11, not-13)
prep_unlike(ideology-11, religion-15)

