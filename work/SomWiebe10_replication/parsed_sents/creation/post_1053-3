(ROOT
  (S
    (NP (PRP We))
    (VP (MD 'll)
      (VP (VB start)
        (PP (IN with)
          (NP
            (NP (NN evidence))
            (PP (IN for)
              (NP (NN design)))))))
    (. .)))

nsubj(start-3, We-1)
aux(start-3, 'll-2)
root(ROOT-0, start-3)
prep_with(start-3, evidence-5)
prep_for(evidence-5, design-7)

