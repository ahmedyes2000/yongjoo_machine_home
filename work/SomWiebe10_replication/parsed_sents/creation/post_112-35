  (S
    (S
      (NP (EX there))
      (VP (VBD were)
        (NP
          (NP (DT those))
          (SBAR
            (WHNP (WP who))
            (S
              (VP (VBZ witnessed)
                (FRAG
                  (NP (DT these) (NNS things)))
                (PRN
                  (S
                    (NP (DT that))
                    (ADVP (RB still))
                    (VP (VBD did) (RB n't)
                      (VP (VB believe))))
                  (-RRB- -RRB-))))))))
    (CC But)
    (FRAG
      (ADJP (RB generally) (JJ speaking))
      (, ,)
      (VP (VBG parting)
        (NP (DT the) (JJ red) (NN sea)))
      (. .))
    (. .)))

