(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ 's)
      (VP (VBN called)
        (NP
          (NP (DT the) (NN THEORY))
          (PP (IN of)
            (NP (NN evolution))))
        (PP (IN for)
          (NP (DT a) (NN reason)))))
    (. .)))

nsubjpass(called-3, It-1)
auxpass(called-3, 's-2)
root(ROOT-0, called-3)
det(THEORY-5, the-4)
dobj(called-3, THEORY-5)
prep_of(THEORY-5, evolution-7)
det(reason-10, a-9)
prep_for(called-3, reason-10)

