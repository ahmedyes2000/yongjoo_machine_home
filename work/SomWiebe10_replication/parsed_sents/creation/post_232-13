(ROOT
  (S
    (NP (EX There))
    (VP (VBP are)
      (NP
        (NP (JJ several) (NNS cases))
        (SBAR
          (WHADVP (WRB where))
          (S
            (NP
              (NP (NNS bacteria))
              (PP (IN in)
                (NP (DT the) (JJ wild))))
            (VP (VBP have)
              (VP (VBN developed)
                (NP (DT the) (NN ability)
                  (S
                    (VP (TO to)
                      (VP (VB break)
                        (PRT (RP down))
                        (NP (NNS compounds))
                        (SBAR (IN that)
                          (FRAG
                            (ADJP (RB purely) (JJ anthropogenic))))))))))))
        (SBAR
          (WHNP (WDT that))
          (S
            (VP (VBZ is) (, ,)
              (FRAG
                (NP
                  (NP (NNS chemicals))
                  (SBAR
                    (WHNP (WDT that))
                    (S
                      (VP (VBD did) (RB not)
                        (VP (VB exist)
                          (PP (IN in)
                            (NP (DT the) (NN environment)))
                          (SBAR (IN until)
                            (S
                              (NP (NNS humans))
                              (VP (VBD manufactured)
                                (NP (PRP them))))))))))))))))
    (. .)))

expl(are-2, There-1)
root(ROOT-0, are-2)
amod(cases-4, several-3)
nsubj(are-2, cases-4)
advmod(developed-11, where-5)
nsubj(developed-11, bacteria-6)
det(wild-9, the-8)
prep_in(bacteria-6, wild-9)
aux(developed-11, have-10)
rcmod(cases-4, developed-11)
det(ability-13, the-12)
dobj(developed-11, ability-13)
aux(break-15, to-14)
infmod(ability-13, break-15)
prt(break-15, down-16)
dobj(break-15, compounds-17)
complm(anthropogenic-20, that-18)
advmod(anthropogenic-20, purely-19)
dep(break-15, anthropogenic-20)
nsubj(is-22, that-21)
rcmod(cases-4, is-22)
dep(is-22, chemicals-24)
nsubj(exist-28, chemicals-24)
aux(exist-28, did-26)
neg(exist-28, not-27)
rcmod(chemicals-24, exist-28)
det(environment-31, the-30)
prep_in(exist-28, environment-31)
mark(manufactured-34, until-32)
nsubj(manufactured-34, humans-33)
advcl(exist-28, manufactured-34)
dobj(manufactured-34, them-35)

