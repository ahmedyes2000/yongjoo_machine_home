(ROOT
  (S
    (NP (PRP You))
    (VP (VBP 're)
      (ADVP (RB right)
        (PP (IN about)
          (NP
            (NP (NNP ID))
            (RB not))))
      (VP (VBG being)
        (VP (VBN taught)
          (PP (IN as)
            (NP (DT a) (NN science))))))
    (. .)))

nsubjpass(taught-8, You-1)
aux(taught-8, 're-2)
advmod(taught-8, right-3)
prep_about(right-3, ID-5)
neg(ID-5, not-6)
auxpass(taught-8, being-7)
root(ROOT-0, taught-8)
det(science-11, a-10)
prep_as(taught-8, science-11)

