(ROOT
  (S
    (PP (IN In)
      (NP (NN fact)))
    (, ,)
    (NP (DT the) (NN document))
    (VP (VBD was)
      (VP (VBN written)
        (NP (NNS centuries))
        (PP (IN before)
          (NP (DT the) (JJ scientific) (NN method)))))
    (. .)))

prep_in(written-7, fact-2)
det(document-5, the-4)
nsubjpass(written-7, document-5)
auxpass(written-7, was-6)
root(ROOT-0, written-7)
dobj(written-7, centuries-8)
det(method-12, the-10)
amod(method-12, scientific-11)
prep_before(written-7, method-12)

