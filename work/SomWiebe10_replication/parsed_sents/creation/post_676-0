(ROOT
  (S (: --)
    (NP (PRP You))
    (VP (MD ca) (RB n't)
      (VP (VB offer)
        (ADVP (RB anymore))
        (PP (IN than)
          (NP
            (NP (NN theory))
            (PP (IN for)
              (NP
                (NP (CD one))
                (CC or)
                (NP (DT the) (JJ other))))))))
    (. .)))

nsubj(offer-5, You-2)
aux(offer-5, ca-3)
neg(offer-5, n't-4)
root(ROOT-0, offer-5)
advmod(offer-5, anymore-6)
prep_than(offer-5, theory-8)
prep_for(theory-8, one-10)
det(other-13, the-12)
prep_for(theory-8, other-13)
conj_or(one-10, other-13)

