(ROOT
  (S ('' '')
    (S
      (ADVP (RB ----------------------))
      (NP (PRP We))
      (VP (VBP do)
        (VP (VB understand))))
    (, ,)
    (NP (NNP Joe))
    (VP (VBP ol)
      (VP ('' '')
        (NP (NNP pal) (. .))))
    (. .)))

advmod(understand-5, -----------------------2)
nsubj(understand-5, We-3)
aux(understand-5, do-4)
ccomp(ol-8, understand-5)
nsubj(ol-8, Joe-7)
root(ROOT-0, ol-8)
dep(ol-8, pal-10)

