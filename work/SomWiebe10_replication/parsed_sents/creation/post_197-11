(ROOT
  (S
    (PP (IN From)
      (NP (DT a) (JJ theological) (NN perspective)))
    (, ,)
    (NP
      (NP (DT the) (NN concept))
      (PP (IN of)
        (NP (NNP ID))))
    (VP (VBZ seems)
      (ADJP (RB rather) (JJ dangerous))
      (SBAR (IN as)
        (S
          (NP (PRP it))
          (VP (VBZ pretends)
            (S
              (VP (TO to)
                (VP
                  (VP (VB open)
                    (PRT (RP up))
                    (NP (JJ religious) (NN faith))
                    (PP (TO to)
                      (NP (JJ scientific) (NN scrutiny))))
                  (CC and)
                  (ADVP (RB thus))
                  (VP (VB disproof)))))))))
    (. .)))

det(perspective-4, a-2)
amod(perspective-4, theological-3)
prep_from(dangerous-12, perspective-4)
det(concept-7, the-6)
nsubj(dangerous-12, concept-7)
prep_of(concept-7, ID-9)
cop(dangerous-12, seems-10)
advmod(dangerous-12, rather-11)
root(ROOT-0, dangerous-12)
mark(pretends-15, as-13)
nsubj(pretends-15, it-14)
xsubj(open-17, it-14)
advcl(dangerous-12, pretends-15)
aux(open-17, to-16)
xcomp(pretends-15, open-17)
prt(open-17, up-18)
amod(faith-20, religious-19)
dobj(open-17, faith-20)
amod(scrutiny-23, scientific-22)
prep_to(open-17, scrutiny-23)
xcomp(pretends-15, thus-25)
advmod(open-17, thus-25)
conj_and(open-17, thus-25)
xcomp(pretends-15, disproof-26)
conj_and(open-17, disproof-26)

