(ROOT
  (S
    (NP
      (NP (JJ long) (NN explanation))
      (PP (IN on)
        (SBAR
          (WHADVP (WRB how))
          (S
            (NP (PRP$ my) (NN mind))
            (VP (VBZ works) (: ...)
              (NP (FW i)))))))
    (VP (MD 'd)
      (ADVP (RB rather) (RB not)))
    (. .)))

amod(explanation-2, long-1)
nsubj('d-10, explanation-2)
advmod(works-7, how-4)
poss(mind-6, my-5)
