(ROOT
  (S
    (NP (NNP Evolution))
    (VP (VBZ grows)
      (S
        (ADJP (RBR more) (JJ established))
        (NP (RB literally) (DT every) (NN day))))
    (. .)))

nsubj(grows-2, Evolution-1)
root(ROOT-0, grows-2)
advmod(established-4, more-3)
acomp(grows-2, established-4)
advmod(day-7, literally-5)
det(day-7, every-6)
dep(established-4, day-7)

