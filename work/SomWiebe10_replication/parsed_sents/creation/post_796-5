(ROOT
  (SBARQ
    (WHNP (WP What))
    (SQ
      (NP (NN man))
      (VP (VBZ has) (RB not)
        (VP (VBD lied)
          (ADVP (RB once)
            (CC or)
            (RB twice)))))
    (. ?)))

dobj(lied-5, What-1)
nsubj(lied-5, man-2)
aux(lied-5, has-3)
neg(lied-5, not-4)
root(ROOT-0, lied-5)
advmod(lied-5, once-6)
advmod(lied-5, twice-8)
conj_or(once-6, twice-8)

