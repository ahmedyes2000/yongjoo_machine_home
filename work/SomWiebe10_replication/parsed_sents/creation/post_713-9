(ROOT
  (FRAG
    (NP (RB Now))
    (, ,)
    (PP (IN with)
      (NP
        (NP (DT an) (NN understanding))
        (PP (IN of)
          (NP
            (NP (NNP God) (POS 's))
            (NN existence))))
      (. .))
    (. .)))

root(ROOT-0, Now-1)
dep(Now-1, with-3)
det(understanding-5, an-4)
pobj(with-3, understanding-5)
poss(existence-9, God-7)
prep_of(understanding-5, existence-9)

