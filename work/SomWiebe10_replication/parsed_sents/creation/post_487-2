(ROOT
  (SBARQ (RB So)
    (WHNP (WP what))
    (SQ
      (VP (IN about)
        (VP (VBZ intelligent)
          (NP
            (NP (NN design))
            (CC and)
            (NP (DT the) (NNP US) (NNP Constitution))))))
    (. ?)))

advmod(intelligent-4, So-1)
nsubj(intelligent-4, what-2)
dep(intelligent-4, about-3)
root(ROOT-0, intelligent-4)
dobj(intelligent-4, design-5)
det(Constitution-9, the-7)
nn(Constitution-9, US-8)
dobj(intelligent-4, Constitution-9)
conj_and(design-5, Constitution-9)

