(ROOT
  (S ('' '')
    (ADVP (RB Instead))
    (, ,)
    (NP (PRP we))
    (VP (VBP believe)
      (S
        (NP (DT that) (NN life))
        (NP
          (NP (DT the) (NN result))
          (PP (IN of)
            (NP
              (NP (DT the) (JJ uniform) (NN operation))
              (PP (IN of)
                (NP (JJ natural) (NNS laws))))))))
    (. .)))

advmod(believe-5, Instead-2)
nsubj(believe-5, we-4)
root(ROOT-0, believe-5)
det(life-7, that-6)
nsubj(result-9, life-7)
det(result-9, the-8)
xcomp(believe-5, result-9)
det(operation-13, the-11)
amod(operation-13, uniform-12)
prep_of(result-9, operation-13)
amod(laws-16, natural-15)
prep_of(operation-13, laws-16)

