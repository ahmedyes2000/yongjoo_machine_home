(ROOT
  (S
    (NP (NNP Causality))
    (VP (VBZ is)
      (SBAR
        (WHNP (WP what))
        (S
          (NP (PRP we))
          (VP (VBP are)
            (ADJP (JJ interested)
              (PP (IN in)))
            (, ,)
            (SBAR
              (WHADVP (WRB wherever))
              (S
                (NP (DT the) (NN evidence))
                (VP (VBZ takes)
                  (NP (PRP us)))))))))
    (. .)))

nsubj(is-2, Causality-1)
root(ROOT-0, is-2)
dep(interested-6, what-3)
nsubj(interested-6, we-4)
cop(interested-6, are-5)
ccomp(is-2, interested-6)
prep(interested-6, in-7)
advmod(takes-12, wherever-9)
det(evidence-11, the-10)
nsubj(takes-12, evidence-11)
advcl(interested-6, takes-12)
dobj(takes-12, us-13)

