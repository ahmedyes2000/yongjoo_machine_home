(ROOT
  (S
    (NP (NNP Historically) (JJ such) (NNS predictions))
    (VP (VBP have)
      (VP (VBN failed)
        (S
          (VP (TO to)
            (VP (VB come)
              (NP (JJ true) (NN time))
              (PP (IN after)
                (NP (NN time))))))))
    (. .)))

nn(predictions-3, Historically-1)
amod(predictions-3, such-2)
nsubj(failed-5, predictions-3)
xsubj(come-7, predictions-3)
aux(failed-5, have-4)
root(ROOT-0, failed-5)
aux(come-7, to-6)
xcomp(failed-5, come-7)
amod(time-9, true-8)
dobj(come-7, time-9)
prep_after(come-7, time-11)

