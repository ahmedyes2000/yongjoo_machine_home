(ROOT
  (S
    (SBAR (IN If)
      (S
        (VP (VBD confirmed))))
    (, ,)
    (NP (DT these))
    (VP (MD can)
      (VP (VB have)
        (NP
          (NP (JJ important) (NNS consequences))
          (PP (IN for)
            (NP
              (NP (PRP$ our) (NN understanding))
              (PP (IN of)
                (NP (DT the) (NNP Universe))))))))
    (. .)))

mark(confirmed-2, If-1)
advcl(have-6, confirmed-2)
nsubj(have-6, these-4)
aux(have-6, can-5)
root(ROOT-0, have-6)
amod(consequences-8, important-7)
dobj(have-6, consequences-8)
poss(understanding-11, our-10)
prep_for(consequences-8, understanding-11)
det(Universe-14, the-13)
prep_of(understanding-11, Universe-14)

