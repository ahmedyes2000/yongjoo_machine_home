(ROOT
  (S
    (NP (PRP I))
    (VP (VBP am)
      (ADJP (JJ sure)
        (SBAR
          (S
            (NP (PRP you))
            (VP (VBD read)
              (NP (DT the) (NN article))
              (SBAR (IN so)
                (S
                  (NP (PRP I))
                  (VP (MD wo) (RB n't)
                    (VP (VB cut)
                      (CC and)
                      (VB paste)
                      (NP (PRP them))
                      (ADVP (RB here)))))))))))
    (. .)))

nsubj(sure-3, I-1)
cop(sure-3, am-2)
root(ROOT-0, sure-3)
nsubj(read-5, you-4)
dep(sure-3, read-5)
det(article-7, the-6)
dobj(read-5, article-7)
mark(cut-12, so-8)
nsubj(cut-12, I-9)
nsubj(paste-14, I-9)
aux(cut-12, wo-10)
neg(cut-12, n't-11)
