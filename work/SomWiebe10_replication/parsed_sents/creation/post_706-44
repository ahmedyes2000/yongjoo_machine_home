(ROOT
  (PRN
    (S
      (ADVP (RB well) (RB obviously)
        (NP (RB little) (RB else)))
      (NP (PRP they))
      (VP (MD would)
        (VP (VB be)
          (ADJP (RB Christians) (JJ =)))))
    (-RRB- -RRB-)))

advmod(obviously-2, well-1)
advmod(=-9, obviously-2)
advmod(else-4, little-3)
pobj(obviously-2, else-4)
nsubj(=-9, they-5)
aux(=-9, would-6)
cop(=-9, be-7)
advmod(=-9, Christians-8)
root(ROOT-0, =-9)

