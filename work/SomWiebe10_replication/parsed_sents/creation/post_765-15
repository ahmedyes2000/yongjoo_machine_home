(ROOT
  (S
    (S
      (NP (NNP Darwin))
      (VP (VBD was)
        (ADJP (JJ wrong)
          (PP (IN in)
            (NP (JJ many) (NNS respects))))))
    (, ,)
    (CC and)
    (S
      (NP (NNS scientists))
      (ADVP (RB rightfully))
      (VP (VBD discarded)
        (NP (DT those) (NNS ideas))))
    (. .)))

nsubj(wrong-3, Darwin-1)
cop(wrong-3, was-2)
root(ROOT-0, wrong-3)
amod(respects-6, many-5)
prep_in(wrong-3, respects-6)
nsubj(discarded-11, scientists-9)
advmod(discarded-11, rightfully-10)
conj_and(wrong-3, discarded-11)
det(ideas-13, those-12)
dobj(discarded-11, ideas-13)

