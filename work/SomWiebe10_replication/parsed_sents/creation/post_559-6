(ROOT
  (S
    (NP (PRP I))
    (VP (VBD was)
      (ADVP (RB just))
      (VP (VBG using)
        (NP (DT that))
        (PP (IN as)
          (NP
            (NP (DT an) (NN example))
            (PP (IN of)
              (NP (CD one) (NN possibility)))))))
    (. .)))

nsubj(using-4, I-1)
aux(using-4, was-2)
advmod(using-4, just-3)
root(ROOT-0, using-4)
dobj(using-4, that-5)
det(example-8, an-7)
prep_as(using-4, example-8)
num(possibility-11, one-10)
prep_of(example-8, possibility-11)

