(ROOT
  (SINV
    (PP (IN on)
      (SBAR
        (WHNP (WDT that))
        (S
          (VP (VBP note)
            (SBAR
              (S
                (NP (PRP you))
                (ADVP (RB also))
                (VP (MD ca) (RB n't)
                  (VP (VB prove)
                    (NP (NN creationism))))))))))
    (VP
      (ADVP (RB ever))
      (VBD took)
      (NP (NN place)))
    (SBAR (IN because)
      (S
        (NP (PRP it))
        (ADVP (RB just))
        (VP (VBD happened))))
    (. .)))

dep(took-11, on-1)
nsubj(note-3, that-2)
pcomp(on-1, note-3)
nsubj(prove-8, you-4)
advmod(prove-8, also-5)
aux(prove-8, ca-6)
neg(prove-8, n't-7)
ccomp(note-3, prove-8)
dobj(prove-8, creationism-9)
advmod(took-11, ever-10)
root(ROOT-0, took-11)
dobj(took-11, place-12)
mark(happened-16, because-13)
nsubj(happened-16, it-14)
advmod(happened-16, just-15)
dep(took-11, happened-16)

