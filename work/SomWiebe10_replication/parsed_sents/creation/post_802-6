(ROOT
  (S
    (NP (NNPS Muslims))
    (VP
      (VP (VBP regard)
        (S
          (NP (DT the) (JJ above) (NNS scriptures))
          (ADJP (RB as) (JJ corrupted))))
      (CC and)
      (VP (VBP accept)
        (S
          (NP (DT the) (NNP Qur))
          (NP
            (NP (`` `) (DT an))
            (PP (IN as)
              (NP
                (NP (DT the) (JJ true) (NN Word))
                (PP (IN of)
                  (NP (NNP God)))))))))
    (. .)))

nsubj(regard-2, Muslims-1)
nsubj(accept-9, Muslims-1)
root(ROOT-0, regard-2)
det(scriptures-5, the-3)
