(ROOT
  (S
    (NP (DT That))
    (VP (VBZ is) (RB not)
      (NP (DT a)
        (ADJP (JJ subject)
          (PP (IN for)
            (NP (NN science))))
        (NN class)))
    (. .)))

nsubj(class-8, That-1)
cop(class-8, is-2)
neg(class-8, not-3)
det(class-8, a-4)
amod(class-8, subject-5)
prep_for(subject-5, science-7)
root(ROOT-0, class-8)

