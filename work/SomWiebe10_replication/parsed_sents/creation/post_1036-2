(ROOT
  (S (CC But)
    (NP (PRP I))
    (VP (VBP wonder)
      (SBAR
        (WHNP (WP who))
        (S
          (VP (VBZ 's)
            (NP (DT the) (JJ next))))))
    (: ...)))

cc(wonder-3, But-1)
nsubj(wonder-3, I-2)
root(ROOT-0, wonder-3)
nsubj(next-7, who-4)
cop(next-7, 's-5)
det(next-7, the-6)
ccomp(wonder-3, next-7)

