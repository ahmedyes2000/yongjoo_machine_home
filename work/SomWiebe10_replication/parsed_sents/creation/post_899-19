(ROOT
  (S
    (NP (PRP$ His) (NN theory))
    (VP (VBD presupposed)
      (NP
        (NP (DT the) (NN existence))
        (PP (IN of)
          (NP (NN life))))
      (S
        (VP (TO to)
          (VP (VB explain)
            (NP
              (NP (DT the) (NN development))
              (PP (IN of)
                (NP (NN biodiversity))))))))
    (. .)))

poss(theory-2, His-1)
nsubj(presupposed-3, theory-2)
root(ROOT-0, presupposed-3)
det(existence-5, the-4)
dobj(presupposed-3, existence-5)
prep_of(existence-5, life-7)
aux(explain-9, to-8)
xcomp(presupposed-3, explain-9)
det(development-11, the-10)
