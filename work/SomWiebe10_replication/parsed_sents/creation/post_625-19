(ROOT
  (S ('' '')
    (PP (VBG Which)
      (NP (NNP assertion) (, ,) (NNP God) (, ,)
        (CC or)
        (NNP God)))
    (VP (VBZ allows))
    (. .)))

prep_which(allows-9, assertion-3)
conj_or(assertion-3, God-5)
prep_which(allows-9, God-5)
conj_or(assertion-3, God-8)
prep_which(allows-9, God-8)
root(ROOT-0, allows-9)

