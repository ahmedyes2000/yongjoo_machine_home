(ROOT
  (S
    (ADVP (RB Perhaps))
    (NP (PRP you))
    (VP (VBP do) (RB n't)
      (VP (VB understand)
        (NP
          (NP (DT the) (NN concept))
          (PP (IN of)
            (NP (DT this) (NN site))))))
    (. .)))

advmod(understand-5, Perhaps-1)
nsubj(understand-5, you-2)
aux(understand-5, do-3)
neg(understand-5, n't-4)
root(ROOT-0, understand-5)
det(concept-7, the-6)
dobj(understand-5, concept-7)
det(site-10, this-9)
prep_of(concept-7, site-10)

