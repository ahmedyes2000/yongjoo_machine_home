(ROOT
  (S
    (SBAR (IN Though)
      (S
        (NP (PRP you))
        (VP (VBP disagree)
          (PP (IN with)
            (NP
              (NP (DT the) (NN validity))
              (PP (IN of)
                (NP (PRP$ his) (NN argument))))))))
    (, ,)
    (NP (PRP you))
    (VP (VBP do) (RB n't)
      (VP (VB address)
        (NP (PRP it))))
    (. .)))

