(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ seems)
      (PP (TO to)
        (NP (PRP me)))
      (SBAR (IN that)
        (S
          (NP (NNP Luskin))
          (VP (VBZ is)
            (VP (VBG being)
              (ADJP (`` ``)
                (ADJP (RBR more) (JJ scientific))
                ('' '')
                (PP (IN in)
                  (NP
                    (NP (DT that) (PRP he) (NNS talks))
                    (PP (IN about)
                      (NP (DT a) (NN method))))))
              (PP (IN for)
                (S
                  (VP (VBG testing)
                    (NP (NNP ID))))))))))
    (. .)))

