(ROOT
  (S ('' '')
    (NP (DT That) (NN sentence))
    (ADVP (RB alone))
    (VP
      (VP (VBZ tells)
        (NP (PRP me))
        (NP (PRP you)))
      (CC either)
      (VP (VBP need)
        (S
          (VP (TO to)
            (VP (VB get)
              (PRT (RP out))
              (NP (JJR more))))))
      (, ,)
      (CC or)
      (VP (VBP need)
        (S
          (VP (TO to)
            (VP
              (ADVP (IN at) (JJS least))
              (VB read)
              (SBAR
                (WHNP (WP what))
                (S
                  (NP (DT the) (NN blog))
                  (VP (VBZ is)
                    (ADVP (RB actually))
                    (ADJP (IN about))))))))))
    (. .)))

det(sentence-3, That-2)
nsubj(tells-5, sentence-3)
nsubj(need-9, sentence-3)
xsubj(get-11, sentence-3)
nsubj(need-16, sentence-3)
xsubj(read-20, sentence-3)
advmod(tells-5, alone-4)
root(ROOT-0, tells-5)
iobj(tells-5, me-6)
dobj(tells-5, you-7)
preconj(tells-5, either-8)
conj_or(tells-5, need-9)
aux(get-11, to-10)
xcomp(need-9, get-11)
prt(get-11, out-12)
dobj(get-11, more-13)
conj_or(tells-5, need-16)
aux(read-20, to-17)
advmod(read-20, at-18)
pobj(at-18, least-19)
xcomp(need-16, read-20)
dobj(about-26, what-21)
det(blog-23, the-22)
nsubj(about-26, blog-23)
cop(about-26, is-24)
advmod(about-26, actually-25)
ccomp(read-20, about-26)

