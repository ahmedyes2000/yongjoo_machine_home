  (S
    (ADVP (RB Now))
    (NP (PRP I))
    (VP (VBP understand)
      (NP
        (NP (DT the) (NN article) (NN bombing))
        (PP (IN on)
          (NP
            (NP (NNP Crowther) (POS 's))
            (NN part)))))
    (. .)))

