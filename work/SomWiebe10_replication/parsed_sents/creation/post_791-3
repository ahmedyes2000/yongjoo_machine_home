(ROOT
  (S
    (VP (VB Assumes)
      (NP (NNS facts))
      (PP (RB not) (IN in)
        (NP
          (NP (NN evidence))
          (, ,)
          (PP (VBG including)
            (NP (DT the) (NN claim)))))
      (SBAR (IN that)
        (S
          (NP (DT the) (NNP Bible))
          (VP (VBZ is)
            (NP
              (NP (DT the) (NNP Word))
              (PP (IN of)
                (NP (NNP God))))))))
    (. .)))

root(ROOT-0, Assumes-1)
dobj(Assumes-1, facts-2)
neg(Assumes-1, not-3)
prep_in(Assumes-1, evidence-5)
det(claim-9, the-8)
prep_including(evidence-5, claim-9)
complm(Word-15, that-10)
det(Bible-12, the-11)
nsubj(Word-15, Bible-12)
cop(Word-15, is-13)
det(Word-15, the-14)
ccomp(Assumes-1, Word-15)
prep_of(Word-15, God-17)

