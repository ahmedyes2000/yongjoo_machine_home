(ROOT
  (S
    (NP (DT This) (JJ whole) (NN argument))
    (VP (VBZ is)
      (ADVP (RB just))
      (VP (VBG using)
        (NP
          (NP (NNS flaws))
          (PP (IN in)
            (NP (PRP$ our) (NNS definitions))))))
    (. .) (. .)))

det(argument-3, This-1)
amod(argument-3, whole-2)
nsubj(using-6, argument-3)
aux(using-6, is-4)
advmod(using-6, just-5)
root(ROOT-0, using-6)
dobj(using-6, flaws-7)
poss(definitions-10, our-9)
prep_in(flaws-7, definitions-10)

