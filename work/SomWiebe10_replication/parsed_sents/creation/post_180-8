(ROOT
  (S ('' '')
    (NP (PRP I))
    (VP (VBP agree)
      (SBAR (IN that)
        (S
          (NP (NNS theories))
          (VP (MD can)
            (VP (VB be)
              (VP (VBN revised)))))))
    (. .)))

nsubj(agree-3, I-2)
root(ROOT-0, agree-3)
complm(revised-8, that-4)
nsubjpass(revised-8, theories-5)
aux(revised-8, can-6)
auxpass(revised-8, be-7)
ccomp(agree-3, revised-8)

