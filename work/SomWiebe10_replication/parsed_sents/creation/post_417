(ROOT
  (S
    (ADVP (RB So))
    (: ...)
    (NP (PRP we))
    (VP (VBP do) (RB n't)
      (VP (VB teach)
        (NP (NN religion))
        (PP (IN in)
          (NP (DT a) (NN science) (NN class)))))
    (. .)))

advmod(teach-6, So-1)
nsubj(teach-6, we-3)
aux(teach-6, do-4)
neg(teach-6, n't-5)
root(ROOT-0, teach-6)
dobj(teach-6, religion-7)
det(class-11, a-9)
nn(class-11, science-10)
prep_in(teach-6, class-11)

