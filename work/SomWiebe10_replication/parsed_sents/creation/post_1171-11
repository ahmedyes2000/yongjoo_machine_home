(ROOT
  (S
    (SBAR
      (WHADVP (WRB When))
      (S
        (NP (NNP John) (NNP Dalton))
        (VP (VBD made)
          (NP (PRP$ his) (JJ atomic) (NN theory)))))
    (NP (PRP it))
    (VP (VBD was)
      (ADJP (JJ brilliant)
        (PP (IN at)
          (NP (DT the) (NN time)))))
    (. .)))

advmod(made-4, When-1)
nn(Dalton-3, John-2)
nsubj(made-4, Dalton-3)
advcl(brilliant-10, made-4)
poss(theory-7, his-5)
amod(theory-7, atomic-6)
dobj(made-4, theory-7)
nsubj(brilliant-10, it-8)
cop(brilliant-10, was-9)
root(ROOT-0, brilliant-10)
det(time-13, the-12)
prep_at(brilliant-10, time-13)

