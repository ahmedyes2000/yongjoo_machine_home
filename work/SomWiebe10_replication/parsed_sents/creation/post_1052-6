(ROOT
  (S (CC But)
    (PP (IN in)
      (NP (DT this) (NN case)))
    (NP (PRP I))
    (VP (VBP do) (RB n't)
      (VP (VB believe)
        (SBAR
          (S
            (NP (PRP you))
            (VP (VBP are)
              (ADJP (JJ correct)))))))
    (. .)))

cc(believe-8, But-1)
det(case-4, this-3)
prep_in(believe-8, case-4)
nsubj(believe-8, I-5)
aux(believe-8, do-6)
neg(believe-8, n't-7)
root(ROOT-0, believe-8)
nsubj(correct-11, you-9)
cop(correct-11, are-10)
ccomp(believe-8, correct-11)

