(ROOT
  (S
    (NP (DT These))
    (VP (VBP are)
      (ADVP (RB then))
      (VP (VBN combined)
        (S
          (VP (TO to)
            (VP (VB create)
              (NP
                (NP (NNS theorems))
                (PRN (-LRB- -LRB-)
                  (SBAR
                    (WHNP (WDT which))
                    (S
                      (VP (MD can) (RB then)
                        (VP (VB be)
                          (VP (VBN used)
                            (S
                              (VP (TO to)
                                (VP (VB create)
                                  (NP (JJR more) (JJ elaborate) (NNS theorems))))))))))
                  (-RRB- -RRB-))))))))
    (. .)))

nsubjpass(combined-4, These-1)
xsubj(create-6, These-1)
auxpass(combined-4, are-2)
advmod(combined-4, then-3)
root(ROOT-0, combined-4)
aux(create-6, to-5)
