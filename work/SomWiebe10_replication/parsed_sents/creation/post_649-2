(ROOT
  (S ('' '')
    (ADVP (RB Even)
      (NP (DT the) (JJ expert) (NNS witnesses))
      (PP (IN for)
        (NP (DT the) (NN defense))))
    (, ,)
    (NP (NNP Minnich)
      (CC and)
      (NNP Fuller))
    (VP (VBD had)
      (S
        (VP (TO to)
          (VP (VB admit)
            (NP (DT this))))))
    (. .)))

advmod(had-13, Even-2)
det(witnesses-5, the-3)
amod(witnesses-5, expert-4)
pobj(Even-2, witnesses-5)
det(defense-8, the-7)
prep_for(Even-2, defense-8)
nsubj(had-13, Minnich-10)
xsubj(admit-15, Minnich-10)
conj_and(Minnich-10, Fuller-12)
nsubj(had-13, Fuller-12)
xsubj(admit-15, Fuller-12)
root(ROOT-0, had-13)
aux(admit-15, to-14)
xcomp(had-13, admit-15)
dobj(admit-15, this-16)

