(ROOT
  (S
    (NP (PRP I))
    (VP (VBD was)
      (ADVP (RB once))
      (SBAR
        (WHADVP (WRB where))
        (S
          (NP (PRP you))
          (VP (VBP are)
            (PP (IN at))))))
    (. .)))

nsubj(was-2, I-1)
root(ROOT-0, was-2)
advmod(was-2, once-3)
advmod(are-6, where-4)
nsubj(are-6, you-5)
advcl(was-2, are-6)
prep(are-6, at-7)

