(ROOT
  (SBARQ
    (PP (IN In)
      (NP
        (NP (NN layman) (POS 's))
        (NNS terms)))
    (, ,)
    (WHNP (WP what))
    (SQ (VBZ does)
      (NP (JJ evolutionary) (NN theory))
      (VP (VB postulate)))
    (. ?)))

poss(terms-4, layman-2)
prep_in(postulate-10, terms-4)
dobj(postulate-10, what-6)
aux(postulate-10, does-7)
amod(theory-9, evolutionary-8)
nsubj(postulate-10, theory-9)
root(ROOT-0, postulate-10)

