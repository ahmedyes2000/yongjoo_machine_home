(ROOT
  (S
    (NP (DT This))
    (VP (VBD was)
      (SBAR
        (WHNP (WP what))
        (S
          (NP (NNP Herbert) (NNP Simon))
          (VP (VBD argued)
            (PP (IN in)
              (NP (PRP$ his) (JJ early) (JJ 20th) (NN century) (NNS papers)))
            (PP (IN on)
              (NP (NN complexity)
                (CC and)
                (NN modularity)))))))
    (. .)))

nsubj(was-2, This-1)
root(ROOT-0, was-2)
dobj(argued-6, what-3)
nn(Simon-5, Herbert-4)
nsubj(argued-6, Simon-5)
ccomp(was-2, argued-6)
poss(papers-12, his-8)
