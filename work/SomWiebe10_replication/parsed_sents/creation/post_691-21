(ROOT
  (S
    (S
      (NP (PRP One))
      (VP (VBZ is)
        (NP (NN conflict))))
    (, ,)
    (NP (DT the) (JJ other))
    (VP (VBZ is)
      (NP (DT a) (NN separation)))
    (. .)))

nsubj(conflict-3, One-1)
cop(conflict-3, is-2)
ccomp(separation-9, conflict-3)
det(other-6, the-5)
