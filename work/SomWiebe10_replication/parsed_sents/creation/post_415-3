(ROOT
  (S ('' '')
    (PP
      (NP (DT The) (NNS chances))
      (IN of)
      (NP
        (NP (DT that))
        (VP (VBG occuring)
          (ADVP (RB randomly)))))
    (VP (VBP are)
      (ADJP (RB absurdly) (JJ small)))
    (. .)))

det(chances-3, The-2)
npadvmod(small-10, chances-3)
prep_of(small-10, that-5)
partmod(that-5, occuring-6)
advmod(occuring-6, randomly-7)
cop(small-10, are-8)
advmod(small-10, absurdly-9)
root(ROOT-0, small-10)

