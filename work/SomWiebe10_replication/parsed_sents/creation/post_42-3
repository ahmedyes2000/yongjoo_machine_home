(ROOT
  (S
    (NP (DT This))
    (VP (VBZ is)
      (SBAR
        (WHADVP (WRB why))
        (S
          (NP (PRP they))
          (VP (VBD figured)
            (PRT (RP out))
            (SBAR
              (WHNP (WP what))
              (S
                (NP (PRP they))
                (VP (VBD did))))))))
    (. .)))

nsubj(is-2, This-1)
root(ROOT-0, is-2)
advmod(figured-5, why-3)
nsubj(figured-5, they-4)
advcl(is-2, figured-5)
prt(figured-5, out-6)
dobj(did-9, what-7)
nsubj(did-9, they-8)
ccomp(figured-5, did-9)

