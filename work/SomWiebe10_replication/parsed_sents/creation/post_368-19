(ROOT
  (S
    (NP
      (NP (DT The) (NN nature))
      (PP (IN of)
        (NP (DT the) (NN design))))
    (VP (VBZ is)
      (ADJP (RB inextricably) (VBN related)
        (PP (TO to)
          (NP
            (NP (DT the) (NN nature))
            (PP (IN of)
              (NP
                (NP (DT the) (NN designer))
                (ADVP (RB however))))))))
    (. .)))

det(nature-2, The-1)
nsubjpass(related-8, nature-2)
det(design-5, the-4)
prep_of(nature-2, design-5)
auxpass(related-8, is-6)
advmod(related-8, inextricably-7)
root(ROOT-0, related-8)
det(nature-11, the-10)
prep_to(related-8, nature-11)
det(designer-14, the-13)
