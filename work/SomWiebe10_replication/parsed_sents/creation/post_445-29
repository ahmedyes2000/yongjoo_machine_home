(ROOT
  (S ('' '')
    (PP
      (ADVP (RB Thus))
      (IN for)
      (NP
        (NP (DT the) (JJ scientific) (NN theory))
        (PP (IN of)
          (NP (NNP ID)))
        (S
          (VP (TO to)
            (VP (VB try)
              (S
                (VP (TO to)
                  (VP (VB identify)
                    (NP (DT the) (NN designer))))))))))
    (VP (MD would)
      (VP (VB be)
        (S
          (VP (TO to)
            (VP
              (ADVP (RB inappropriately))
              (VB conflate)
              (NP (NN science))
              (PP (IN with)
                (NP (NN religion))))))))
    (. .)))

advmod(be-16, Thus-2)
det(theory-6, the-4)
amod(theory-6, scientific-5)
prep_for(be-16, theory-6)
prep_of(theory-6, ID-8)
aux(try-10, to-9)
infmod(theory-6, try-10)
aux(identify-12, to-11)
xcomp(try-10, identify-12)
det(designer-14, the-13)
dobj(identify-12, designer-14)
aux(be-16, would-15)
root(ROOT-0, be-16)
aux(conflate-19, to-17)
advmod(conflate-19, inappropriately-18)
xcomp(be-16, conflate-19)
dobj(conflate-19, science-20)
prep_with(conflate-19, religion-22)

