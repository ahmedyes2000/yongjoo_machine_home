(ROOT
  (S
    (NP (DT The) (JJ same))
    (VP (MD can)
      (VP (VB be)
        (VP (VBN said)
          (PP (IN for)
            (NP (JJ invisible) (NNS gnomes)))
          (S
            (VP (VBG building)
              (NP (NN everything)))))))
    (. .)))

det(same-2, The-1)
nsubjpass(said-5, same-2)
aux(said-5, can-3)
auxpass(said-5, be-4)
root(ROOT-0, said-5)
amod(gnomes-8, invisible-7)
prep_for(said-5, gnomes-8)
xcomp(said-5, building-9)
dobj(building-9, everything-10)

