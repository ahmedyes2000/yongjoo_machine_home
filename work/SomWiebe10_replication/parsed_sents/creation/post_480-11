  (S
    (NP (PRP I))
    (VP (MD 'll)
      (VP (VB tell)
        (NP (PRP you))
        (ADVP (RB up) (RB front))
        (, ,)
        (SBAR (IN that)
          (S
            (NP (PRP I))
            (VP (VBP do) (RB n't)
              (VP (VB have)
                (NP (DT the) (NN faith)
                  (S
                    (VP (TO to)
                      (VP (VB believe)
                        (PP (IN in)
                          (NP
                            (NP (DT a) (NN chance) (NN occurrence))
                            (PP (IN like)
                              (NP (DT that)))))))))))))))
    (. .)))

