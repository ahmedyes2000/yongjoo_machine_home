(ROOT
  (S
    (NP
      (NP (NNS Scientists))
      (PP (IN in)
        (NP
          (NP (NN support))
          (PP (IN of)
            (NP (NN evolution))))))
    (ADVP (RB even))
    (VP (VBP state)
      (NP (DT that)))
    (. .)))

nsubj(state-7, Scientists-1)
prep_in(Scientists-1, support-3)
prep_of(support-3, evolution-5)
advmod(state-7, even-6)
root(ROOT-0, state-7)
dobj(state-7, that-8)

