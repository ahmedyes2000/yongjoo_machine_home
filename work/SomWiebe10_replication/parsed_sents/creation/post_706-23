(ROOT
  (S
    (NP (EX there))
    (VP (VBZ is)
      (NP
        (NP (DT no) (`` ``) (JJ true) ('' '') (NN definition))
        (PP (IN for)
          (NP (DT any) (NN word)))))
    (: ...)))

expl(is-2, there-1)
root(ROOT-0, is-2)
det(definition-7, no-3)
amod(definition-7, true-5)
nsubj(is-2, definition-7)
det(word-10, any-9)
prep_for(definition-7, word-10)

