(ROOT
  (S
    (NP (PRP He))
    (VP (VBD considered)
      (NP (PRP himself))
      (PP (IN of)
        (NP (DT an) (NN agnostic)))
      (SBAR (IN because)
        (S
          (NP (PRP he))
          (VP (VBD was)
            (VP (VBN astonished)
              (PP (IN by)
                (NP
                  (NP (DT the) (NN brilliance))
                  (PP (IN behind)
                    (NP (DT the) (NNP Universe))))))))))
    (. .)))

nsubj(considered-2, He-1)
root(ROOT-0, considered-2)
dobj(considered-2, himself-3)
det(agnostic-6, an-5)
prep_of(considered-2, agnostic-6)
mark(astonished-10, because-7)
nsubjpass(astonished-10, he-8)
auxpass(astonished-10, was-9)
advcl(considered-2, astonished-10)
det(brilliance-13, the-12)
agent(astonished-10, brilliance-13)
det(Universe-16, the-15)
prep_behind(brilliance-13, Universe-16)

