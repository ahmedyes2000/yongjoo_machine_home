(ROOT
  (S ('' '')
    (S
      (ADVP (RB Then))
      (NP (PRP you))
      (VP (VBP claim) ('' '')))
    (CC But)
    (S
      (NP (NNP ID))
      (VP (VBZ is) (RB not)
        (VP (VBN focused)
          (PP (IN on)
            (S
              (VP (VBG studying)
                (S
                  (NP (DT the) (JJ actual) (JJ intelligent) (NN cause))
                  (ADJP (JJ responsible)
                    (PP (IN for)
                      (NP (NN life)))))))))))
    (. .)))

advmod(claim-4, Then-2)
nsubj(claim-4, you-3)
root(ROOT-0, claim-4)
nsubjpass(focused-10, ID-7)
auxpass(focused-10, is-8)
neg(focused-10, not-9)
conj_but(claim-4, focused-10)
prepc_on(focused-10, studying-12)
det(cause-16, the-13)
amod(cause-16, actual-14)
amod(cause-16, intelligent-15)
nsubj(responsible-17, cause-16)
xcomp(studying-12, responsible-17)
prep_for(responsible-17, life-19)

