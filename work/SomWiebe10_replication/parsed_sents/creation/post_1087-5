(ROOT
  (S
    (ADJP
      (ADJP
        (ADVP (RB Far) (RB far) (RB far))
        (RBR more) (JJ laughable))
      (PP (IN than)
        (NP (DT the) (NNS holes))))
    (NP (PRP you))
    (VP (VBP say)
      (VP (VB exist)
        (PP (IN in)
          (NP (NN evolution)))))
    (. .)))

advmod(far-3, Far-1)
advmod(far-3, far-2)
advmod(laughable-5, far-3)
advmod(laughable-5, more-4)
dep(say-10, laughable-5)
det(holes-8, the-7)
prep_than(laughable-5, holes-8)
nsubj(say-10, you-9)
root(ROOT-0, say-10)
dep(say-10, exist-11)
prep_in(exist-11, evolution-13)

