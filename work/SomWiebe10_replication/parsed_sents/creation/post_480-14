(ROOT
  (S
    (VP (VB RIGHT))
    (. !)))

root(ROOT-0, RIGHT-1)

(ROOT
  (S
    (ADVP (RB Again))
    (, ,)
    (NP (PRP it))
    (VP (VBZ is)
      (ADJP (JJ amazing))
      (SBAR
        (WHADVP (WRB how))
        (S
          (NP (JJ gullible) (NNS evolutionists))
          (VP (VBP think)
            (SBAR
              (S
                (NP (NNS people))
                (VP (VBP are))))))))
    (. !)))

advmod(amazing-5, Again-1)
nsubj(amazing-5, it-3)
cop(amazing-5, is-4)
root(ROOT-0, amazing-5)
