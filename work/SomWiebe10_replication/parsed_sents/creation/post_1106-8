(ROOT
  (S
    (S
      (NP (RB Only) (DT the) (NN teacher))
      (VP (MD can)
        (VP (VB avoid))))
    (CC and)
    (S
      (NP (NN nothing))
      (ADJP (RB else)))
    (. .)))

advmod(teacher-3, Only-1)
det(teacher-3, the-2)
nsubj(avoid-5, teacher-3)
aux(avoid-5, can-4)
root(ROOT-0, avoid-5)
dep(else-8, nothing-7)
conj_and(avoid-5, else-8)

