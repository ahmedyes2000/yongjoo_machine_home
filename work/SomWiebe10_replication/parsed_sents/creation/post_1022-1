(ROOT
  (S
    (ADVP (RB However))
    (, ,)
    (NP (FW i))
    (VP (VBP think)
      (SBAR
        (S
          (NP (DT the) (CD two))
          (VP (VBP are)
            (ADJP (RB closely) (VBN linked))))))
    (. .)))

advmod(think-4, However-1)
nsubj(think-4, i-3)
root(ROOT-0, think-4)
det(two-6, the-5)
nsubjpass(linked-9, two-6)
auxpass(linked-9, are-7)
advmod(linked-9, closely-8)
ccomp(think-4, linked-9)

