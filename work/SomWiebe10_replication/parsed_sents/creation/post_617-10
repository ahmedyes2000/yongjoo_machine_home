(ROOT
  (S
    (NP (NNP Unpopular) (NNS ideas))
    (ADVP (RB seldom))
    (VP (VBP get)
      (NP (DT a) (JJ fair) (NN hearing)))
    (. .)))

nn(ideas-2, Unpopular-1)
nsubj(get-4, ideas-2)
advmod(get-4, seldom-3)
root(ROOT-0, get-4)
det(hearing-7, a-5)
amod(hearing-7, fair-6)
dobj(get-4, hearing-7)

