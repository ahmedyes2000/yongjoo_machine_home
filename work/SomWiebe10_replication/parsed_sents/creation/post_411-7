(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ 's)
      (ADVP (RB still))
      (NP (DT a)
        (ADJP (RB very) (JJ new)
          (CC and)
          (JJ basic))
        (NN theory)))
    (. .)))

nsubj(theory-9, It-1)
cop(theory-9, 's-2)
advmod(theory-9, still-3)
det(theory-9, a-4)
advmod(new-6, very-5)
amod(theory-9, new-6)
conj_and(new-6, basic-8)
amod(theory-9, basic-8)
root(ROOT-0, theory-9)

