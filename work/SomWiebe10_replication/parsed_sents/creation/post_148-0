(ROOT
  (S
    (NP (DT The) (NN name) (NNP Intelligent) (NNP Design))
    (VP (VBZ is)
      (ADVP (PRP itself))
      (NP (DT a) (NN hoax)))
    (. .)))

det(Design-4, The-1)
nn(Design-4, name-2)
nn(Design-4, Intelligent-3)
nsubj(hoax-8, Design-4)
cop(hoax-8, is-5)
advmod(hoax-8, itself-6)
det(hoax-8, a-7)
root(ROOT-0, hoax-8)

