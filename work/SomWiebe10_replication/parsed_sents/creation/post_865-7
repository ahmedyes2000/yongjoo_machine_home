(ROOT
  (S
    (NP (PRP We))
    (VP (VBP recognize)
      (NP
        (NP (DT the) (NN story))
        (PP (IN about)
          (NP (NNP Cain)
            (CC and)
            (NNP Abel)))))
    (. .)))

nsubj(recognize-2, We-1)
root(ROOT-0, recognize-2)
det(story-4, the-3)
dobj(recognize-2, story-4)
prep_about(story-4, Cain-6)
prep_about(story-4, Abel-8)
conj_and(Cain-6, Abel-8)

