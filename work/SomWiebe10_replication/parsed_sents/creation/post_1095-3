(ROOT
  (FRAG
    (PP (IN Over)
      (NP
        (NP (NNS millions))
        (PP (IN of)
          (NP
            (NP (NNS years))
            (PP (IN of)
              (NP (NN course)))))))
    (. .)))

root(ROOT-0, Over-1)
pobj(Over-1, millions-2)
prep_of(millions-2, years-4)
prep_of(years-4, course-6)

