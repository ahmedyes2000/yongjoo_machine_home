(ROOT
  (S
    (ADVP (RB Putatively))
    (, ,)
    (NP (PRP I))
    (VP (VBP believe)
      (PRN (, ,)
        (S
          (NP (DT a) (NN gene) (NN duplication))
          (VP (VBZ has)
            (VP (VBN occurred))))
        (, ,))
      (PP (JJ such))
      (SBAR (IN that)
        (S
          (NP (DT the) (JJ cit) (NNP +) (NNP E.) (NNS coli))
          (VP (VBP have) (RB not)
            (VP (VBN lost)
              (NP (DT any)
                (ADJP (JJ ancestral))
                (NN function)))))))
    (. .)))

advmod(believe-4, Putatively-1)
nsubj(believe-4, I-3)
root(ROOT-0, believe-4)
det(duplication-8, a-6)
nn(duplication-8, gene-7)
nsubj(occurred-10, duplication-8)
aux(occurred-10, has-9)
parataxis(believe-4, occurred-10)
prep(believe-4, such-12)
complm(lost-21, that-13)
det(coli-18, the-14)
amod(coli-18, cit-15)
nn(coli-18, +-16)
nn(coli-18, E.-17)
nsubj(lost-21, coli-18)
aux(lost-21, have-19)
neg(lost-21, not-20)
ccomp(believe-4, lost-21)
det(function-24, any-22)
amod(function-24, ancestral-23)
dobj(lost-21, function-24)

