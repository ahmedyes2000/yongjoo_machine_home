(ROOT
  (S
    (S
      (NP (DT The) (NNS facts))
      (VP (MD will)
        (VP (VB stand)
          (PRT (RP up))
          (PP (IN for)
            (NP (PRP themselves))))))
    (PRN
      (S
        (NP (PRP he))
        (VP (VBZ has)
          (NP (NN none))))
      (, ,))
    (CC and)
    (S
      (NP (PRP he))
      (VP (VBZ is)
        (ADJP (RB wilfully) (JJ ignorant)
          (PP (IN of)
            (NP (DT the) (NNS facts))))))
    (. .)))

det(facts-2, The-1)
nsubj(stand-4, facts-2)
aux(stand-4, will-3)
root(ROOT-0, stand-4)
prt(stand-4, up-5)
prep_for(stand-4, themselves-7)
nsubj(has-9, he-8)
dep(stand-4, has-9)
dobj(has-9, none-10)
nsubj(ignorant-16, he-13)
cop(ignorant-16, is-14)
advmod(ignorant-16, wilfully-15)
conj_and(stand-4, ignorant-16)
det(facts-19, the-18)
prep_of(ignorant-16, facts-19)

