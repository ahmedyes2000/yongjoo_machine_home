(ROOT
  (S
    (NP (EX There))
    (VP (VBZ is)
      (ADVP (RB absolutely))
      (NP
        (NP (DT no) (NN proof))
        (PP (IN for)
          (NP (NNP Evolution)))))
    (. .)))

expl(is-2, There-1)
root(ROOT-0, is-2)
advmod(is-2, absolutely-3)
det(proof-5, no-4)
nsubj(is-2, proof-5)
prep_for(proof-5, Evolution-7)

