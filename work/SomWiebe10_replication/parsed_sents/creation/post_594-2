(ROOT
  (S
    (ADVP (RB Perhaps))
    (NP (PRP you))
    (VP (MD can)
      (VP (VB tell)
        (NP (PRP me))
        (SBAR
          (WHADVP (WRB how))
          (S
            (NP (PRP you))
            (VP (VBP believe)
              (SBAR
                (S
                  (NP (NNP ID))
                  (VP (MD can)
                    (VP (VB be)
                      (VP (VBN tested)))))))))))
    (. .)))

advmod(tell-4, Perhaps-1)
nsubj(tell-4, you-2)
aux(tell-4, can-3)
root(ROOT-0, tell-4)
dobj(tell-4, me-5)
advmod(believe-8, how-6)
nsubj(believe-8, you-7)
dep(tell-4, believe-8)
nsubjpass(tested-12, ID-9)
aux(tested-12, can-10)
auxpass(tested-12, be-11)
ccomp(believe-8, tested-12)

