(ROOT
  (S
    (SBAR (IN Since)
      (S
        (NP (PRP you))
        (VP (VBD said)
          (SBAR
            (S
              (NP (PRP it))
              (VP (MD would)
                (VP (VB be)
                  (ADJP (JJ easy)))))))))
    (, ,)
    (NP (PRP I))
    (ADVP (RB eagerly))
    (VP (VBP await)
      (NP (DT the) (NN citation)))
    (. .)))

mark(said-3, Since-1)
