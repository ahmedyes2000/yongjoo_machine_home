(ROOT
  (SBAR (IN If)
    (S
      (NP (PRP you))
      (VP (VBP look)
        (PP (IN at)
          (NP (PRP it)))
        (PP (IN as)
          (S
            (ADVP (RB simply))
            (VP (VBG being)
              (UCP (`` ``)
                (ADJP (JJ improbable))
                ('' '')
                (CC but)
                (ADJP (RB so))
                (NP (WP what))))))))
    (: ...)))

mark(look-3, If-1)
nsubj(look-3, you-2)
root(ROOT-0, look-3)
prep_at(look-3, it-5)
advmod(being-8, simply-7)
prepc_as(look-3, being-8)
dep(being-8, improbable-10)
dep(being-8, so-13)
conj_but(improbable-10, so-13)
dep(improbable-10, what-14)

