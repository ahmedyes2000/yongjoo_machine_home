(ROOT
  (S
    (NP (PDT All) (DT this))
    (VP (VBZ happens)
      (PP (IN in)
        (NP
          (NP (DT a)
            (ADJP
              (ADVP (RB constantly))
              (VBG changing))
            (NN environment))
          (SBAR
            (WHADVP (WRB where))
            (S
              (NP (JJ multiple) (NNS pressures))
              (VP (VBP exist)))))))
    (. .)))

predet(this-2, All-1)
nsubj(happens-3, this-2)
root(ROOT-0, happens-3)
det(environment-8, a-5)
advmod(changing-7, constantly-6)
amod(environment-8, changing-7)
prep_in(happens-3, environment-8)
advmod(exist-12, where-9)
amod(pressures-11, multiple-10)
nsubj(exist-12, pressures-11)
rcmod(environment-8, exist-12)

