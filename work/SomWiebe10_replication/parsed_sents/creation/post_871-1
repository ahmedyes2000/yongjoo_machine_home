(ROOT
  (FRAG
    (NP
      (NP (NN Someone))
      (SBAR
        (WHNP (WP who))
        (S
          (ADVP (RB perhaps))
          (VP (VBZ does) (RB n't)
            (VP (VB own)
              (NP (DT a) (NN bible)))))))
    (. ?)))

