(ROOT
  (S
    (NP (DT That))
    (VP (VBZ is)
      (SBAR
        (WHADJP (RB exactly) (WP what))
        (S
          (NP (DT the) (NNP DI))
          (VP (VBD tried)
            (S
              (VP (TO to)
                (VP (VB argue))))
            (, ,)
            (SBAR (IN that)
              (S
                (NP (NNP ID))
                (VP (VBZ has)
                  (NP (DT some) (NN secular) (NN purpose)))))))))
    (. .)))

nsubj(is-2, That-1)
root(ROOT-0, is-2)
dep(tried-7, exactly-3)
dep(exactly-3, what-4)
det(DI-6, the-5)
nsubj(tried-7, DI-6)
xsubj(argue-9, DI-6)
