(ROOT
  (S ('' '')
    (NP (NNP The))
    (VP (VBZ universe)
      (SBAR (IN that)
        (S
          (NP (PRP we))
          (VP (MD can)
            (VP (VB see)
              (ADVP (RB so) (RB far)
                (SBAR
                  (S
                    (VP (VBZ is)
                      (ADJP (RB quite) (JJ simple)))))))))))
    (. .)))

nsubj(universe-3, The-2)
root(ROOT-0, universe-3)
complm(see-7, that-4)
nsubj(see-7, we-5)
aux(see-7, can-6)
ccomp(universe-3, see-7)
advmod(far-9, so-8)
advmod(see-7, far-9)
cop(simple-12, is-10)
advmod(simple-12, quite-11)
dep(far-9, simple-12)

