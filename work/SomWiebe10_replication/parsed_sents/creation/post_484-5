(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP (VBP are)
          (ADJP (JJ interested)
            (PP (IN in)
              (S
                (VP (VBG discussing)
                  (NP (NN evolution)))))))))
    (, ,)
    (VP (VB give)
      (NP (PRP it))
      (NP (PRP$ your) (JJS best) (NN shot)))
    (. .)))

mark(interested-4, If-1)
nsubj(interested-4, you-2)
cop(interested-4, are-3)
advcl(give-9, interested-4)
prepc_in(interested-4, discussing-6)
dobj(discussing-6, evolution-7)
root(ROOT-0, give-9)
iobj(give-9, it-10)
poss(shot-13, your-11)
amod(shot-13, best-12)
dobj(give-9, shot-13)

