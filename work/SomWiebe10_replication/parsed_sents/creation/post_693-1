(ROOT
  (S
    (NP (PRP I))
    (VP (VBP believe)
      (SBAR
        (S
          (NP (DT the) (NN term) (JJ many) (NN use))
          (VP (VBZ is) (`` ``)
            (ADJP (JJ inerrant))
            ('' '')))))
    (. .)))

nsubj(believe-2, I-1)
root(ROOT-0, believe-2)
det(use-6, the-3)
nn(use-6, term-4)
amod(use-6, many-5)
nsubj(inerrant-9, use-6)
cop(inerrant-9, is-7)
