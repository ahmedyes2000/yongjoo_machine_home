(ROOT
  (S
    (NP
      (NP (DT Every) (VBG living) (NN thing))
      (PP (IN on)
        (NP (DT this) (NN earth))))
    (VP (VBZ is)
      (VP (VBN created)
        (PP (IN by)
          (NP (NNP God)))
        (PP (IN with)
          (NP (NN miracle)))))
    (. .)))

det(thing-3, Every-1)
amod(thing-3, living-2)
nsubjpass(created-8, thing-3)
det(earth-6, this-5)
prep_on(thing-3, earth-6)
auxpass(created-8, is-7)
root(ROOT-0, created-8)
agent(created-8, God-10)
prep_with(created-8, miracle-12)

