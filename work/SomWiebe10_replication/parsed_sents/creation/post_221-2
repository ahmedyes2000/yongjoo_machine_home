(ROOT
  (S
    (NP (PRP It))
    (VP (MD would)
      (VP (VB seem)
        (SBAR (IN that)
          (S
            (NP (PRP you))
            (VP (VBP claim)
              (SBAR
                (SBAR (IN that)
                  (S
                    (NP (PRP they))
                    (VP
                      (VP (VBP deserve)
                        (S
                          (VP (TO to)
                            (VP (VB be)
                              (VP (VBN heard))))))
                      (CC and)
                      (VP (VBP have) (RB not)
                        (VP (VBN been)
                          (VP (VBN heard)))))))
                (CC but)
                (SBAR
                  (WHNP (WDT that))
                  (S
                    (VP (VBZ is)
                      (ADVP (RB also))
                      (PP (IN under)
                        (NP (NN dispute))))))))))))
    (. .)))

