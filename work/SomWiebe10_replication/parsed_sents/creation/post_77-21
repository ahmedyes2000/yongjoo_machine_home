(ROOT
  (S
    (VP (VB Be)
      (ADJP (JJ sure)
        (SBAR
          (S
            (NP (PRP you))
            (VP (VBP know)
              (SBAR
                (WHNP (WP what))
                (S
                  (NP (NNS people))
                  (VP (VBP mean)
                    (SBAR
                      (WHADVP (WRB when))
                      (S
                        (NP (PRP they))
                        (VP (VBP say)
                          (NP (NN evolution)))))))))))))
    (. .)))

cop(sure-2, Be-1)
root(ROOT-0, sure-2)
nsubj(know-4, you-3)
dep(sure-2, know-4)
dep(mean-7, what-5)
nsubj(mean-7, people-6)
ccomp(know-4, mean-7)
advmod(say-10, when-8)
nsubj(say-10, they-9)
advcl(mean-7, say-10)
dobj(say-10, evolution-11)

(ROOT
  (S
    (NP (JJ Intelligent) (NN design))
    (VP (VBZ is) (RB not)
      (NP (DT the) (JJ same))
      (PP (IN as)
        (NP (NN creationism))))
    (. .)))

amod(design-2, Intelligent-1)
nsubj(same-6, design-2)
cop(same-6, is-3)
neg(same-6, not-4)
det(same-6, the-5)
root(ROOT-0, same-6)
prep_as(same-6, creationism-8)

