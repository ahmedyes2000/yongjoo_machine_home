  (S (CC And)
    (NP
      (NP (DT the) (NNS followers))
      (PP (IN of)
        (NP (JJ other) (NNS faiths)))
      (, ,)
      (PP (IN in)
        (NP (NN turn)))
      (, ,))
    (VP (MD would)
      (VP (VB deny)
        (NP
          (NP (DT the) (NNP Bible))
          (PP (RB because) (IN of)
            (NP (JJ THEIR) (NN faith))))))
    (. .)))

cc(deny-12, And-1)
det(followers-3, the-2)
nsubj(deny-12, followers-3)
amod(faiths-6, other-5)
