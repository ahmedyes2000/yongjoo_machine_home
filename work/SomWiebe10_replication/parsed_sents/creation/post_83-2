(ROOT
  (S (CC But)
    (ADVP (RB even) (RBR more) (RB so))
    (VP (VBZ does)
      (SBAR
        (WHNP (WP what))
        (S
          (NP (PRP we))
          (VP (VBP study)))))
    (, ,)
    (CC and)
    (S
      (NP (DT the) (NN drive))
      (VP (TO to)
        (VP (VB study)
          (PP (IN in)
            (NP (PRP itself))))))
    (. .)))

advmod(so-4, even-2)
advmod(so-4, more-3)
advmod(does-5, so-4)
root(ROOT-0, does-5)
dobj(study-8, what-6)
nsubj(study-8, we-7)
ccomp(does-5, study-8)
det(drive-12, the-11)
nsubj(study-14, drive-12)
aux(study-14, to-13)
conj_and(does-5, study-14)
prep_in(study-14, itself-16)

