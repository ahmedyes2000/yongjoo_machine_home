(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ does) (RB n't)
      (VP (VB make)
        (NP (NNP ANY) (NNS predictions))
        (PP (IN about)
          (NP (DT the) (NN past)))
        (, ,)
        (SBAR (IN because)
          (S
            (NP (DT that))
            (VP (VBZ 's)
              (ADJP (JJ impossible)))))))
    (. .)))

nsubj(make-4, It-1)
aux(make-4, does-2)
neg(make-4, n't-3)
root(ROOT-0, make-4)
nn(predictions-6, ANY-5)
dobj(make-4, predictions-6)
det(past-9, the-8)
prep_about(make-4, past-9)
mark(impossible-14, because-11)
nsubj(impossible-14, that-12)
cop(impossible-14, 's-13)
advcl(make-4, impossible-14)

