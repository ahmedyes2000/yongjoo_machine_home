(ROOT
  (FRAG
    (SBAR
      (WHPP (TO To)
        (WHNP (WDT that)))
      (S
        (NP (PRP I))
        (VP (VBP say) (, ,) (VBP Hallelujah) (, ,)
          (ADVP (RB Amen)))))
    (. !)))

dep(say-4, To-1)
pobj(To-1, that-2)
nsubj(say-4, I-3)
root(ROOT-0, say-4)
dep(say-4, Hallelujah-6)
advmod(say-4, Amen-8)

