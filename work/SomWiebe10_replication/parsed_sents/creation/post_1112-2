(ROOT
  (S
    (NP (DT No) (NN I))
    (VP (VBP am) (RB not)
      (ADJP
        (ADJP (JJ deluded))
        (, ,)
        (ADJP (JJ brainwashed))
        (, ,)
        (CC or)
        (ADJP (JJ crazy))))
    (. .)))

det(I-2, No-1)
nsubj(deluded-5, I-2)
nsubj(brainwashed-7, I-2)
nsubj(crazy-10, I-2)
cop(deluded-5, am-3)
neg(deluded-5, not-4)
root(ROOT-0, deluded-5)
conj_or(deluded-5, brainwashed-7)
conj_or(deluded-5, crazy-10)

