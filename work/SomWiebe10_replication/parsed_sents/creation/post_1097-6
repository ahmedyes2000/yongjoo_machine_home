  (S
    (NP (DT This))
    (VP (VBZ makes)
      (S
        (NP (NN evolution))
        (ADJP (`` ``) (JJ true) ('' '')
          (PP (IN in)
            (NP (DT a) (NN sense))))))
    (. .)))

