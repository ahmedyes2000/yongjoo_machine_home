(ROOT
  (S
    (S
      (NP (NNP Well))
      (VP (VBD said)))
    (CC and)
    (S
      (NP (PRP it))
      (VP (MD should)
        (VP (VB inspire)
          (NP (JJ other) (NNS Christians))
          (S
            (VP (TO to)
              (VP
                (ADVP (RB critically))
                (VB review)
                (NP (PRP$ their) (NN position))
                (PP (IN on)
                  (NP (NN science)
                    (CC and)
                    (NN theology)))))))))
    (. .)))

nsubj(said-2, Well-1)
root(ROOT-0, said-2)
nsubj(inspire-6, it-4)
aux(inspire-6, should-5)
conj_and(said-2, inspire-6)
amod(Christians-8, other-7)
dobj(inspire-6, Christians-8)
aux(review-11, to-9)
advmod(review-11, critically-10)
xcomp(inspire-6, review-11)
poss(position-13, their-12)
dobj(review-11, position-13)
prep_on(review-11, science-15)
prep_on(review-11, theology-17)
conj_and(science-15, theology-17)

