(ROOT
  (S (CC And)
    (NP (PRP it))
    (VP (VBZ is)
      (VP (VBN known)
        (S
          (VP (TO to)
            (VP (VB occur)
              (PP (IN in)
                (NP (DT the) (JJ wild))))))))
    (. .)))

cc(known-4, And-1)
nsubjpass(known-4, it-2)
xsubj(occur-6, it-2)
auxpass(known-4, is-3)
root(ROOT-0, known-4)
aux(occur-6, to-5)
xcomp(known-4, occur-6)
det(wild-9, the-8)
