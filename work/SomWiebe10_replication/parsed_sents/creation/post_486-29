(ROOT
  (S
    (S
      (NP (DT This) (NN question))
      (VP (VBZ is) (RB not)
        (NP (DT a) (JJ scientific) (NN one))))
    (: --)
    (S
      (NP (PRP it))
      (VP (VBZ is)
        (NP (DT a) (JJ legal) (NN one))))
    (. .)))

det(question-2, This-1)
nsubj(one-7, question-2)
cop(one-7, is-3)
neg(one-7, not-4)
det(one-7, a-5)
amod(one-7, scientific-6)
root(ROOT-0, one-7)
nsubj(one-13, it-9)
cop(one-13, is-10)
det(one-13, a-11)
amod(one-13, legal-12)
parataxis(one-7, one-13)

