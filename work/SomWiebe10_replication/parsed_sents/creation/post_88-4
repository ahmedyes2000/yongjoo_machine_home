(ROOT
  (S (CC But)
    (NP (PRP he))
    (VP (VBZ is)
      (ADJP (JJ wrong))
      (SBAR (IN that)
        (S
          (NP (NNS scientists))
          (ADVP (RB falsely))
          (VP (VBZ claims)
            (SBAR (IN that)
              (S
                (NP (NN science))
                (VP (MD can) (RB not)
                  (VP (VB detect)
                    (NP (NNS creators))))))))))
    (. .)))

cc(wrong-4, But-1)
nsubj(wrong-4, he-2)
cop(wrong-4, is-3)
root(ROOT-0, wrong-4)
complm(claims-8, that-5)
nsubj(claims-8, scientists-6)
advmod(claims-8, falsely-7)
ccomp(wrong-4, claims-8)
complm(detect-13, that-9)
nsubj(detect-13, science-10)
aux(detect-13, can-11)
neg(detect-13, not-12)
ccomp(claims-8, detect-13)
dobj(detect-13, creators-14)

