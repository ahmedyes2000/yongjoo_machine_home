(ROOT
  (S
    (NP (DT No) (CD two) (NNS persons))
    (VP (VBP experience)
      (NP (DT the) (NN divine))
      (NP (DT the) (JJ same))
      (SBAR (RB even) (IN if)
        (S
          (NP (PRP they))
          (VP (VBP are)
            (PP (IN of)
              (NP
                (NP (DT the) (JJ same) (NN faith)
                  (CC and)
                  (NN subsect))
                (PP (IN of)
                  (NP (DT that) (NN faith)))))))))
    (. .)))

det(persons-3, No-1)
num(persons-3, two-2)
nsubj(experience-4, persons-3)
root(ROOT-0, experience-4)
det(divine-6, the-5)
iobj(experience-4, divine-6)
det(same-8, the-7)
dobj(experience-4, same-8)
advmod(are-12, even-9)
dep(are-12, if-10)
nsubj(are-12, they-11)
dep(experience-4, are-12)
det(faith-16, the-14)
amod(faith-16, same-15)
prep_of(are-12, faith-16)
prep_of(are-12, subsect-18)
conj_and(faith-16, subsect-18)
det(faith-21, that-20)
prep_of(faith-16, faith-21)

