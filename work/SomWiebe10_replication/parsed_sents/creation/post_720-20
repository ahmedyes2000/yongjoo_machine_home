(ROOT
  (S
    (NP (RB Even) (JJ nocturnal) (NNS animals))
    (VP (VBP require)
      (FRAG
        (ADJP (JJ light)
          (PP (IN at)
            (NP (NN night))))
        (S
          (VP (TO to)
            (VP (VB see))))
        (. .)))
    (. .)))

advmod(animals-3, Even-1)
amod(animals-3, nocturnal-2)
nsubj(require-4, animals-3)
root(ROOT-0, require-4)
dep(require-4, light-5)
prep_at(light-5, night-7)
aux(see-9, to-8)
dep(light-5, see-9)

