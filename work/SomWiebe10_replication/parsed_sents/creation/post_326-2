(ROOT
  (S
    (NP (RB Only) (DT the) (NNS fools))
    (VP (VBP remain)
      (S
        (VP (TO to)
          (VP (VB discuss)
            (NP (PRP$ their) (JJ contrived) (NNS alternatives))))))
    (. .)))

advmod(fools-3, Only-1)
det(fools-3, the-2)
nsubj(remain-4, fools-3)
xsubj(discuss-6, fools-3)
root(ROOT-0, remain-4)
aux(discuss-6, to-5)
xcomp(remain-4, discuss-6)
poss(alternatives-9, their-7)
amod(alternatives-9, contrived-8)
dobj(discuss-6, alternatives-9)

