(ROOT
  (S ('' '')
    (NP (EX There))
    (VP (VBZ is)
      (NP (DT a) (JJ wonderful) (NN world) (JJ wide) (NN web))
      (PP (IN out)
        (NP (RB there)))
      (PP (IN with)
        (NP
          (NP (DT a) (NN lot))
          (PP (IN of)
            (NP (NN substance))))))
    (. .)))

expl(is-3, There-2)
root(ROOT-0, is-3)
det(web-8, a-4)
amod(web-8, wonderful-5)
nn(web-8, world-6)
amod(web-8, wide-7)
nsubj(is-3, web-8)
prep(is-3, out-9)
pobj(out-9, there-10)
det(lot-13, a-12)
prep_with(is-3, lot-13)
prep_of(lot-13, substance-15)

