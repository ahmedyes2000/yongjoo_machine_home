(ROOT
  (S (-LRB- -LRB-)
    (PP (IN by)
      (NP (DT the) (NN way)))
    (, ,)
    (NP
      (NP (VBG using) (NN capital) (NNS letters))
      (PP (IN for)
        (NP (NN emphasis))))
    (VP (VBZ detracts)
      (PP (IN from)
        (NP (DT the) (JJ overall) (NN argument))))
    (. .)))

det(way-4, the-3)
prep_by(detracts-11, way-4)
amod(letters-8, using-6)
nn(letters-8, capital-7)
nsubj(detracts-11, letters-8)
prep_for(letters-8, emphasis-10)
root(ROOT-0, detracts-11)
det(argument-15, the-13)
amod(argument-15, overall-14)
prep_from(detracts-11, argument-15)

