(ROOT
  (S
    (NP (JJ Many) (NNS groups))
    (VP (VBP contend)
      (SBAR (IN that)
        (S
          (NP (NN evolution))
          (ADVP (RB directly))
          (VP (VBZ instructs)
            (SBAR (IN that)
              (S
                (NP (NNP Genesis))
                (VP (VBZ is)
                  (ADJP (JJ wrong)))))))))
    (. .)))

amod(groups-2, Many-1)
nsubj(contend-3, groups-2)
root(ROOT-0, contend-3)
complm(instructs-7, that-4)
nsubj(instructs-7, evolution-5)
advmod(instructs-7, directly-6)
ccomp(contend-3, instructs-7)
complm(wrong-11, that-8)
nsubj(wrong-11, Genesis-9)
cop(wrong-11, is-10)
ccomp(instructs-7, wrong-11)

