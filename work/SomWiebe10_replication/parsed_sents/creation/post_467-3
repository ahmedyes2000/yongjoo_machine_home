(ROOT
  (S
    (NP (PRP I))
    (VP (VBD told)
      (NP (PRP you))
      (, ,)
      (S
        (VP (VB rely)
          (PP (IN on)
            (NP (DT the) (JJ original) (NNS sources))))))))

nsubj(told-2, I-1)
root(ROOT-0, told-2)
dobj(told-2, you-3)
dep(told-2, rely-5)
det(sources-9, the-7)
amod(sources-9, original-8)
prep_on(rely-5, sources-9)

