(ROOT
  (S
    (VP (VB Try)
      (S
        (VP (VBG reading)
          (NP (NNP Chaucer))
          (ADVP (IN out) (RB loud))
          (PP (IN in)
            (NP (PRP$ his) (NN language))))))
    (. .)))

root(ROOT-0, Try-1)
xcomp(Try-1, reading-2)
dobj(reading-2, Chaucer-3)
advmod(reading-2, out-4)
advmod(out-4, loud-5)
poss(language-8, his-7)
prep_in(reading-2, language-8)

