(ROOT
  (S
    (S
      (NP (NNP Scientific))
      (VP (VBD thought)
        (NP
          (NP (JJ abandons) (NNS theories)
            (CC and)
            (NNS notions))
          (SBAR
            (WHNP (WDT that))
            (S
              (VP (VBP are) (RB not)
                (VP (VBN buttressed)
                  (PP (IN by)
                    (NP (NNS data)
                      (CC or)
                      (NN logic))))))))))
    (: ;)
    (S
      (NP (NNP ID))
      (VP (VBZ has)
        (ADVP (RB never))
        (VP (VBN withstood)
          (NP (JJ such) (NN scrutiny)))))
    (. .)))

nsubj(thought-2, Scientific-1)
root(ROOT-0, thought-2)
amod(theories-4, abandons-3)
dobj(thought-2, theories-4)
