  (S
    (PP
      (ADVP (RB Unfortunately))
      (IN for)
      (NP (NNP Behe)))
    (, ,)
    (NP
      (NP (NNP Lenski) (POS 's))
      (NN work))
    (VP (VBZ is) (RB not)
      (ADJP (JJ isolated)))
    (. .)))

