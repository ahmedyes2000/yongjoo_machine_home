(ROOT
  (S
    (NP (PRP It))
    (VP (VBN employed)
      (NP (DT the) (JJ scientific) (NN method)
        (S
          (VP (TO to)
            (VP (VB come)
              (PRT (RP up))
              (PP (IN with)
                (NP
                  (NP (DT an) (NN idea))
                  (SBAR
                    (WHNP (WDT that))
                    (S
                      (VP (VBD changed)
                        (NP
                          (NP (DT the) (NN way))
                          (SBAR
                            (S
                              (NP (PRP we))
                              (VP (VBP view)
                                (NP (PRP$ our) (NN world))))))))))))))))
    (. .)))

nsubj(employed-2, It-1)
root(ROOT-0, employed-2)
det(method-5, the-3)
amod(method-5, scientific-4)
dobj(employed-2, method-5)
aux(come-7, to-6)
infmod(method-5, come-7)
prt(come-7, up-8)
det(idea-11, an-10)
prep_with(come-7, idea-11)
nsubj(changed-13, idea-11)
rcmod(idea-11, changed-13)
det(way-15, the-14)
dobj(changed-13, way-15)
nsubj(view-17, we-16)
rcmod(way-15, view-17)
poss(world-19, our-18)
dobj(view-17, world-19)

