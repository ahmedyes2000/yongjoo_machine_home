(ROOT
  (S
    (NP (NNP Hi) (NNP PvM))
    (, ,)
    (NP (PRP they))
    (ADVP (RB finally))
    (VP (VBD got)
      (SBAR
        (S
          (NP (NNS things))
          (VP (VBD straightened)
            (PRT (RP out))))))
    (. .)))

nn(PvM-2, Hi-1)
nsubj(got-6, PvM-2)
nsubj(got-6, they-4)
advmod(got-6, finally-5)
root(ROOT-0, got-6)
