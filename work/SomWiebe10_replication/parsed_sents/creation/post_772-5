(ROOT
  (S
    (NP (NNS Rivers))
    (VP (VBP do) (RB n't)
      (VP (VB stay)
        (NP (DT the) (JJ same))
        (PP (IN from)
          (NP
            (NP (CD one) (NN year))
            (PP (TO to)
              (NP (DT the) (JJ next)))))))
    (. .)))

nsubj(same-6, Rivers-1)
aux(same-6, do-2)
neg(same-6, n't-3)
cop(same-6, stay-4)
det(same-6, the-5)
root(ROOT-0, same-6)
num(year-9, one-8)
