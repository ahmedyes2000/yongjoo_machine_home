(ROOT
  (S
    (S
      (NP (NNS Parents))
      (VP (VBP have)
        (VP (VBN had)
          (NP (RB enough)))))
    (CC and)
    (S
      (NP (PRP I))
      (VP (VBP predict)
        (NP
          (NP (DT a) (JJ serious) (NN decline))
          (PP (IN in)
            (NP (JJ public) (NN school) (NN attendance))))
        (NP
          (NP (DT this) (JJ next) (NN year))
          (CC and)
          (NP (DT a) (NN half)))))
    (. .)))

nsubj(had-3, Parents-1)
aux(had-3, have-2)
root(ROOT-0, had-3)
dobj(had-3, enough-4)
nsubj(predict-7, I-6)
