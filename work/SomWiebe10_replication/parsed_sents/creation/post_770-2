(ROOT
  (S
    (NP (PRP YOU))
    (VP (MD can) (RB not)
      (VP (VB PROVE)
        (NP (NNP SPIRIT))
        (, ,)
        (PP (IN except) (IN for)
          (NP
            (NP
              (NP (PRP it) (POS 's))
              (NN intervention))
            (PP (IN upon)
              (NP (NNS men)))))))
    (: ...)))

nsubj(PROVE-4, YOU-1)
aux(PROVE-4, can-2)
neg(PROVE-4, not-3)
root(ROOT-0, PROVE-4)
dobj(PROVE-4, SPIRIT-5)
poss(intervention-11, it-9)
prep_except_for(PROVE-4, intervention-11)
prep_upon(intervention-11, men-13)

