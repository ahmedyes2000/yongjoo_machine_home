(ROOT
  (S
    (NP (DT The) (JJ same) (NN reason))
    (NP (PRP we))
    (VP (VBP do) (RB n't)
      (VP (VB teach)
        (NP (NN literature))
        (PP (IN in)
          (NP (DT a) (NN home) (NNS economics) (NN class)))))
    (. .)))

det(reason-3, The-1)
amod(reason-3, same-2)
nsubj(teach-7, reason-3)
nsubj(teach-7, we-4)
aux(teach-7, do-5)
neg(teach-7, n't-6)
root(ROOT-0, teach-7)
dobj(teach-7, literature-8)
det(class-13, a-10)
nn(class-13, home-11)
nn(class-13, economics-12)
