(ROOT
  (S
    (NP (DT This))
    (VP (VBZ means)
      (SBAR
        (S
          (NP (PRP it))
          (VP (VBZ is) (RB not)
            (ADVP (RB really))
            (NP
              (NP (DT a) (NN theory))
              (, ,)
              (NP
                (NP (RBR more) (DT a) (NN kind))
                (PP (IN of)
                  (NP (NN problem) (NN statement)))))))))
    (. .)))

nsubj(means-2, This-1)
root(ROOT-0, means-2)
nsubj(theory-8, it-3)
cop(theory-8, is-4)
neg(theory-8, not-5)
advmod(theory-8, really-6)
det(theory-8, a-7)
ccomp(means-2, theory-8)
advmod(kind-12, more-10)
det(kind-12, a-11)
appos(theory-8, kind-12)
nn(statement-15, problem-14)
