(ROOT
  (S
    (NP (DT The) (JJ Abrahamic) (NNS religions))
    (VP (VBP center)
      (ADVP (RBS most) (RB strongly))
      (PP (IN around)
        (NP (DT a) (JJ judgemental) (NN god))))
    (. .)))

det(religions-3, The-1)
amod(religions-3, Abrahamic-2)
nsubj(center-4, religions-3)
root(ROOT-0, center-4)
advmod(strongly-6, most-5)
advmod(center-4, strongly-6)
det(god-10, a-8)
amod(god-10, judgemental-9)
prep_around(center-4, god-10)

