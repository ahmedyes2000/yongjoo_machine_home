(ROOT
  (S
    (ADVP (RB Thus))
    (, ,)
    (ADVP (RB here))
    (NP (PRP we))
    (VP (VBD performed)
      (NP (DT the) (JJ same) (NN analysis))
      (PP (IN for)
        (NP (DT a) (NN map)))
      (SBAR
        (WHADVP (WRB where))
        (S
          (NP
            (NP (NNS regions))
            (PP (IN with)
              (NP (JJ high) (NN foreground) (NN contamination))))
          (VP (VBD were)
            (VP (VBN removed))))))
    (. .)))

advmod(performed-5, Thus-1)
advmod(performed-5, here-3)
nsubj(performed-5, we-4)
root(ROOT-0, performed-5)
det(analysis-8, the-6)
amod(analysis-8, same-7)
dobj(performed-5, analysis-8)
det(map-11, a-10)
prep_for(performed-5, map-11)
advmod(removed-19, where-12)
nsubjpass(removed-19, regions-13)
amod(contamination-17, high-15)
nn(contamination-17, foreground-16)
prep_with(regions-13, contamination-17)
auxpass(removed-19, were-18)
advcl(performed-5, removed-19)

