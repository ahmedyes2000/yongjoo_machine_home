(ROOT
  (S
    (NP (DT The) (JJ full) (NNS drafts))
    (VP
      (VP (VBD were) (RB all)
        (VP (VBN entered)
          (PP (IN as)
            (NP (JJ evidential) (NNS exhibits)))
          (PP (IN with)
            (NP (DT the) (NN court)))))
      (CC and)
      (VP (VBP are)
        (PP (IN on)
          (S
            (VP (NN file)
              (ADVP (RB there)))))))
    (. .)))

det(drafts-3, The-1)
amod(drafts-3, full-2)
nsubjpass(entered-6, drafts-3)
nsubj(are-14, drafts-3)
auxpass(entered-6, were-4)
advmod(entered-6, all-5)
root(ROOT-0, entered-6)
amod(exhibits-9, evidential-8)
prep_as(entered-6, exhibits-9)
det(court-12, the-11)
prep_with(entered-6, court-12)
