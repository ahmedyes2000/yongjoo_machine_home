(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP (VBP want)
          (NP (JJ strong) (NN evidence)))))
    (NP (PRP you))
    (VP (MD could)
      (ADVP (RB also))
      (VP (VB have)
        (S
          (VP (VB see)
            (NP (DT some) (NN literature))
            (PP (IN from)
              (NP (DT any) (JJ evolutionary) (NN biologist)))))))
    (. .)))

mark(want-3, If-1)
nsubj(want-3, you-2)
advcl(have-9, want-3)
amod(evidence-5, strong-4)
dobj(want-3, evidence-5)
nsubj(have-9, you-6)
aux(have-9, could-7)
advmod(have-9, also-8)
root(ROOT-0, have-9)
ccomp(have-9, see-10)
det(literature-12, some-11)
dobj(see-10, literature-12)
det(biologist-16, any-14)
amod(biologist-16, evolutionary-15)
prep_from(see-10, biologist-16)

