(ROOT
  (S
    (NP (DT The) (NN report))
    (VP (VBD listed)
      (NP
        (NP (JJ numerous) (NNS examples))
        (PP (IN of)
          (NP (VBG spying))))
      (, ,)
      (S
        (VP
          (VP (VBG plotting))
          (, ,)
          (VP (VBG lying))
          (, ,)
          (VP (VBG scheming)
            (CC and)
            (VBG creating)
            (NP
              (NP (DT a) (JJ hostile) (NN environment))
              (PP (IN against)
                (NP (NNP Dr.) (NNP Sternberg))))
            (PP (IN by)
              (NP (NNP Smithsonian) (NN staff))))
          (PP (IN with)
            (NP (DT the) (NNP NCSE) (NNP s) (NN help))))))
    (. .)))

det(report-2, The-1)
nsubj(listed-3, report-2)
root(ROOT-0, listed-3)
amod(examples-5, numerous-4)
dobj(listed-3, examples-5)
prep_of(examples-5, spying-7)
dep(listed-3, plotting-9)
dep(plotting-9, lying-11)
