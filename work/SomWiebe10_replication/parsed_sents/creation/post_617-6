(ROOT
  (S
    (NP (PRP I))
    (VP (VBP have)
      (VP (VBN heard)
        (SBAR
          (S
            (NP (PRP$ their) (NNS opponents))
            (VP
              (ADVP (RP over)
                (CC and)
                (RP over))
              (VBP try)
