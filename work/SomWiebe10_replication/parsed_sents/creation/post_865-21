(ROOT
  (S
    (NP (PRP They))
    (VP (VBD had)
      (VP (VBN been)
        (NP
          (NP (NN part))
          (PP (IN of)
            (NP
              (NP (DT the) (JJ tribal) (NNS traditions))
              (PP (IN for)
                (NP (DT a) (JJ long) (NN time))))))))
    (. .)))

nsubj(part-4, They-1)
aux(part-4, had-2)
cop(part-4, been-3)
root(ROOT-0, part-4)
det(traditions-8, the-6)
amod(traditions-8, tribal-7)
prep_of(part-4, traditions-8)
det(time-12, a-10)
amod(time-12, long-11)
prep_for(traditions-8, time-12)

