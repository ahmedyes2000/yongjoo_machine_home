(ROOT
  (S
    (NP (DT That))
    (VP (VBZ 's)
      (NP
        (NP (DT a) (JJ good) (NN topic))
        (PP (IN for)
          (NP (NNP Sunday) (NN school)))))
    (. .)))

nsubj(topic-5, That-1)
cop(topic-5, 's-2)
det(topic-5, a-3)
amod(topic-5, good-4)
root(ROOT-0, topic-5)
nn(school-8, Sunday-7)
prep_for(topic-5, school-8)

