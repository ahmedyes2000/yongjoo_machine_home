(ROOT
  (S
    (S
      (NP (PRP It))
      (CONJP (RB not) (JJ only))
      (VP (VBZ halts)
        (NP (NN knowledge))))
    (, ,)
    (NP (PRP it))
    (VP (VBZ canonizes)
      (NP (NN ignorance)))
    (. .)))

ccomp(canonizes-8, It-1)
neg(only-3, not-2)
dep(It-1, only-3)
conj(It-1, halts-4)
dobj(halts-4, knowledge-5)
nsubj(canonizes-8, it-7)
root(ROOT-0, canonizes-8)
dobj(canonizes-8, ignorance-9)

