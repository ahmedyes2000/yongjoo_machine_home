(ROOT
  (S ('' '')
    (PP (IN Can)
      (NP
        (NP (NN someone))
        (SBAR
          (WHNP (WDT that))
          (S
            (VP (VBZ believes)
              (PP (IN in)
                (NP (NNP ID))))))))
    (VP (VBP please)
      (VP (VB explain)
        (NP
          (NP (DT this) (`` ``) (JJ abrupt) ('' '') (NN design))
          (PP (IN of)
            (NP (NNS species))))))
    (. .)))

nsubj(believes-5, someone-3)
prep_can(please-8, someone-3)
rcmod(someone-3, believes-5)
prep_in(believes-5, ID-7)
root(ROOT-0, please-8)
xcomp(please-8, explain-9)
det(design-14, this-10)
amod(design-14, abrupt-12)
dobj(explain-9, design-14)
prep_of(design-14, species-16)

