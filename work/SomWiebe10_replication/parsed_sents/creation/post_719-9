(ROOT
  (S
    (NP (PRP I))
    (VP (VBP 'm)
      (ADJP (JJ afraid)
        (SBAR
          (S
            (NP (PRP you))
            (VP (VBP are)
              (ADVP (RB completely))
              (VP (VBG missing)
                (NP (DT that) (NN point))
                (ADVP (RB though))))))))
    (: ...)))

nsubj(afraid-3, I-1)
cop(afraid-3, 'm-2)
root(ROOT-0, afraid-3)
nsubj(missing-7, you-4)
aux(missing-7, are-5)
advmod(missing-7, completely-6)
dep(afraid-3, missing-7)
det(point-9, that-8)
dobj(missing-7, point-9)
advmod(missing-7, though-10)

