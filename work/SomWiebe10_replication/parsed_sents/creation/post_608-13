(ROOT
  (NP
    (NP (JJ Little)
      (CC or)
      (DT no) (NN room))
    (PP (IN for)
      (NP (NNP God)))
    (PP (IN in)
      (NP (DT this) (NN interpretation)))
    (. .)))

amod(room-4, Little-1)
conj_or(Little-1, no-3)
amod(room-4, no-3)
root(ROOT-0, room-4)
prep_for(room-4, God-6)
det(interpretation-9, this-8)
prep_in(room-4, interpretation-9)

