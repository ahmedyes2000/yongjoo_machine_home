(ROOT
  (S
    (NP (DT That))
    (VP (VBZ is)
      (NP
        (NP (DT a) (NN fact))
        (, ,) (RB not)
        (NP (DT an) (NN opinion))))
    (. .)))

nsubj(fact-4, That-1)
cop(fact-4, is-2)
det(fact-4, a-3)
root(ROOT-0, fact-4)
neg(fact-4, not-6)
det(opinion-8, an-7)
dep(fact-4, opinion-8)

