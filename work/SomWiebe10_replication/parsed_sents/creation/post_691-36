(ROOT
  (S
    (NP
      (ADJP (RB Too) (JJ many))
      (NNS Christians))
    (VP (VBP have)
      (VP (VBN mistaken)
        (NP
          (NP
            (NP (DT the) (NNP Bible))
            (PRN (-LRB- -LRB-)
              (NP (DT the) (NN finger))
              (-RRB- -RRB-)))
          (PP (IN for)
            (NP
              (NP (DT the) (NN moon))
              (PRN (-LRB- -LRB-)
                (NP
                  (NP (DT the) (NN will))
                  (PP (IN of)
                    (NP (NNP God))))
                (-RRB- -RRB-)))))))
    (. .)))

advmod(many-2, Too-1)
amod(Christians-3, many-2)
nsubj(mistaken-5, Christians-3)
aux(mistaken-5, have-4)
root(ROOT-0, mistaken-5)
det(Bible-7, the-6)
dobj(mistaken-5, Bible-7)
det(finger-10, the-9)
appos(Bible-7, finger-10)
det(moon-14, the-13)
