(ROOT
  (S
    (NP (NNP Darwin))
    (VP (VBD had)
      (NP (DT the) (JJ right) (NN idea)
        (CC and)
        (NNS scientists))
      (SBAR (IN after)
        (S
          (NP (PRP him))
          (VP (VBP have)
            (VP (VBN researched)
              (S
                (NP (PRP it))
                (ADVP (RB further))))))))
    (. .)))

nsubj(had-2, Darwin-1)
root(ROOT-0, had-2)
det(idea-5, the-3)
amod(idea-5, right-4)
dobj(had-2, idea-5)
dobj(had-2, scientists-7)
conj_and(idea-5, scientists-7)
mark(researched-11, after-8)
nsubj(researched-11, him-9)
aux(researched-11, have-10)
advcl(had-2, researched-11)
dep(researched-11, it-12)
advmod(it-12, further-13)

