(ROOT
  (S
    (ADVP (RB Perhaps))
    (NP (PRP we))
    (VP (VBP understand)
      (NP
        (NP (DT a) (NN lot))
        (PP (IN of)
          (NP
            (NP (NNS things))
            (PP (IN about)
              (NP (JJ human) (NN intelligence)))))))
    (. .)))

advmod(understand-3, Perhaps-1)
nsubj(understand-3, we-2)
root(ROOT-0, understand-3)
det(lot-5, a-4)
dobj(understand-3, lot-5)
prep_of(lot-5, things-7)
amod(intelligence-10, human-9)
prep_about(things-7, intelligence-10)

