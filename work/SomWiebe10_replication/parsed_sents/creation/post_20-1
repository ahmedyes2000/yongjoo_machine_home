  (S
    (NP (DT Some))
    (VP (VBP argue)
      (SBAR (IN that)
        (S
          (S
            (VP (VBG looking)
              (PP (IN at)
                (NP
                  (NP (DT the) (NN way))
                  (SBAR
                    (WHPP (IN in)
                      (WHNP (WDT which)))
                    (S
                      (NP (JJ different) (NNS organisms) (NN work))
