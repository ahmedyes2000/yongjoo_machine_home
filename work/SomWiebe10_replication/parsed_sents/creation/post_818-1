(ROOT
  (S
    (NP (PRP$ His) (JJ first) (NN year))
    (NP (PRP he))
    (VP (VBD was)
      (VP
        (ADVP (RB repeatedly))
        (VBN moved) (, ,)
        (SBAR
          (WHNP (WDT which))
          (S
            (NP
              (NP (DT a) (NN method))
              (PP (IN of)
                (S
                  (VP (VBG isolating)
                    (NP (DT a) (NN prisoner))))))
            (VP
              (VP (VBZ socially))
              (CC and)
              (VP (VBZ psychologically)))))))
    (. .)))

poss(year-3, His-1)
amod(year-3, first-2)
nsubjpass(moved-7, year-3)
tmod(moved-7, year-3)
nsubjpass(moved-7, he-4)
auxpass(moved-7, was-5)
advmod(moved-7, repeatedly-6)
root(ROOT-0, moved-7)
dobj(socially-16, which-9)
det(method-11, a-10)
nsubj(socially-16, method-11)
nsubj(psychologically-18, method-11)
prepc_of(method-11, isolating-13)
det(prisoner-15, a-14)
dobj(isolating-13, prisoner-15)
ccomp(moved-7, socially-16)
ccomp(moved-7, psychologically-18)
conj_and(socially-16, psychologically-18)

