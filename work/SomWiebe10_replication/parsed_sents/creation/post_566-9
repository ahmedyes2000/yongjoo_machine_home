(ROOT
  (S
    (NP (PRP We))
    (VP (VBP 've)
      (ADVP (RB never))
      (VP (VBN seen)
        (NP (DT any) (NN evidence))
        (SBAR (IN that)
          (S
            (NP (CD one) (NNS species))
            (VP (MD can)
              (VP (VB change)
                (PP (IN into)
                  (NP (DT a)
                    (ADJP (RB completely) (JJ different))
                    (NN species)))))))))
    (. .)))

nsubj(seen-4, We-1)
aux(seen-4, 've-2)
neg(seen-4, never-3)
root(ROOT-0, seen-4)
det(evidence-6, any-5)
dobj(seen-4, evidence-6)
complm(change-11, that-7)
num(species-9, one-8)
nsubj(change-11, species-9)
aux(change-11, can-10)
ccomp(seen-4, change-11)
det(species-16, a-13)
advmod(different-15, completely-14)
amod(species-16, different-15)
prep_into(change-11, species-16)

