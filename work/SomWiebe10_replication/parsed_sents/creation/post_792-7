(ROOT
  (S
    (NP (EX There))
    (VP (VBZ is) (RB not)
      (NP (JJ good) (NN reason)
        (S
          (VP (TO to)
            (VP (VB believe)
              (ADVP (RB otherwise)))))))
    (. .)))

expl(is-2, There-1)
root(ROOT-0, is-2)
neg(is-2, not-3)
amod(reason-5, good-4)
nsubj(is-2, reason-5)
aux(believe-7, to-6)
infmod(reason-5, believe-7)
advmod(believe-7, otherwise-8)

