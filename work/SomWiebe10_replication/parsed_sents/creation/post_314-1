(ROOT
  (S (CC And)
    (NP (DT that))
    (VP (VBZ 's)
      (SBAR
        (WHADVP (WRB why))
        (S
          (NP (DT this) (`` ``) (NN theory) ('' ''))
          (VP (VBZ is) (RB not)
            (NP (NN science))))))
    (. .)))

cc('s-3, And-1)
nsubj('s-3, that-2)
root(ROOT-0, 's-3)
advmod(science-11, why-4)
det(theory-7, this-5)
nsubj(science-11, theory-7)
cop(science-11, is-9)
neg(science-11, not-10)
advcl('s-3, science-11)

