(ROOT
  (S
    (NP (PRP They))
    (VP (VBD wished)
      (S
        (VP (TO to)
          (VP (VB illuminate)
            (NP
              (NP (DT the) (JJ human) (NN condition))
              (CC and)
              (NP (PRP$ its) (NN relationship)))
            (PP (TO to)
              (NP (NNP God)))))))
    (. .)))

nsubj(wished-2, They-1)
xsubj(illuminate-4, They-1)
root(ROOT-0, wished-2)
