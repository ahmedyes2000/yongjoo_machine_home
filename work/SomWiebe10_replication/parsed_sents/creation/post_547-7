(ROOT
  (S
    (INTJ (UH Yes))
    (, ,)
    (NP (DT this))
    (VP (VBD was)
      (ADVP (RB also))
      (VP
        (VP (VBN argued)
          (PP (IN in)
            (NP (NN court))))
        (CC and)
        (VP (VBN rejected)
          (PP (IN by)
            (NP
              (NP (DT the) (NN court))
              (PP (IN for)
                (NP (JJ good) (NNS reasons))))))))
    (. .)))

dep(argued-6, Yes-1)
nsubjpass(argued-6, this-3)
nsubjpass(rejected-10, this-3)
auxpass(argued-6, was-4)
advmod(argued-6, also-5)
root(ROOT-0, argued-6)
prep_in(argued-6, court-8)
conj_and(argued-6, rejected-10)
det(court-13, the-12)
