(ROOT
  (S
    (NP (NNP GOD))
    (VP (VBD said)
      (NP (CD 6) (NNS days))
      (SBAR
        (S
          (NP (PRP I))
          (VP (VBP believe)
            (NP (PRP him))
            (PP (IN over)
              (NP
                (NP (PRP you))
                (CC or)
                (NP (NNP SCIENCE) (. .))))))))
    (. .)))

nsubj(said-2, GOD-1)
root(ROOT-0, said-2)
num(days-4, 6-3)
tmod(said-2, days-4)
nsubj(believe-6, I-5)
dep(said-2, believe-6)
dobj(believe-6, him-7)
prep_over(believe-6, you-9)
prep_over(believe-6, SCIENCE-11)
conj_or(you-9, SCIENCE-11)

