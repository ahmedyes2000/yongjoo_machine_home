(ROOT
  (S
    (S
      (NP (JJ Intelligent) (NN design))
      (VP (VBZ constitutes)
        (SBAR (IN that)
          (S
            (NP (NN something))
            (VP (VBD created)
              (NP (DT the) (NN universe)))))))
    (: ;)
    (S
      (NP (NNP Aithiesm))
      (VP (VBZ constitutes)
        (SBAR (IN that)
          (S
            (NP (NNP NOTHING))
            (VP (VBD created)
              (NP (DT the) (NN universe)))))))
    (. .)))

