(ROOT
  (S
    (PP (IN In)
      (ADJP (JJ short)))
    (, ,)
    (NP
      (NP (RB not) (CD one) (NN word))
      (PP (IN of)
        (NP
          (NP (NNP Allogic) (POS 's))
          (NN statement))))
    (VP (VBZ rings)
      (ADJP (JJ true)
        (PP (TO to)
          (NP (PRP me)))))
    (. .)))

prep_in(rings-11, short-2)
neg(word-6, not-4)
num(word-6, one-5)
nsubj(rings-11, word-6)
poss(statement-10, Allogic-8)
