(ROOT
  (S
    (NP (DT No) (PRP it))
    (VP (VBZ is) (RB n't))
    (. .)))

det(it-2, No-1)
nsubj(is-3, it-2)
root(ROOT-0, is-3)
neg(is-3, n't-4)

(ROOT
  (S
    (NP
      (NP
        (NP (PRP It) (POS 's))
        (NN BEEN))
      (VP (VBG falling)
        (ADVP (RB apart))))
    (VP (VBZ is)
      (ADJP
        (ADJP (JJR more))
        (PP (IN like)
          (NP (PRP it)))))
    (. .)))

poss(BEEN-3, It-1)
nsubj(more-7, BEEN-3)
partmod(BEEN-3, falling-4)
advmod(falling-4, apart-5)
cop(more-7, is-6)
root(ROOT-0, more-7)
prep_like(more-7, it-9)

