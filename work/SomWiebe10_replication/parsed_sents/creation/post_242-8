(ROOT
  (S
    (NP (DT The) (NNP Holy) (NNP Bible))
    (VP (VBZ is)
      (NP
        (NP (DT the) (JJS most) (VBN read) (NN text))
        (PP (IN in)
          (NP
            (NP (DT the) (NN world))
            (CC and)
            (NP (JJ reprinted) (NNS millions))))
        (PP (IN of)
          (NP (NNS times)))))
    (. .)))

det(Bible-3, The-1)
nn(Bible-3, Holy-2)
nsubj(text-8, Bible-3)
cop(text-8, is-4)
det(text-8, the-5)
amod(text-8, most-6)
amod(text-8, read-7)
root(ROOT-0, text-8)
det(world-11, the-10)
prep_in(text-8, world-11)
