(ROOT
  (S
    (NP (PRP They))
    (VP (VBP say)
      (SBAR
        (S
          (NP (DT the) (JJ new) (NN face) (NNS transplants))
          (VP (VBP are)
            (NP (DT a) (JJ huge) (NN success))))))
    (. .)))

nsubj(say-2, They-1)
root(ROOT-0, say-2)
det(transplants-6, the-3)
amod(transplants-6, new-4)
nn(transplants-6, face-5)
nsubj(success-10, transplants-6)
cop(success-10, are-7)
det(success-10, a-8)
amod(success-10, huge-9)
ccomp(say-2, success-10)

