(ROOT
  (S
    (NP (PRP$ Our) (NN planet))
    (VP (VBZ has)
      (VP (VBN been)
        (ADVP (RB here))
        (PP (IN for)
          (NP
            (NP
              (QP (IN at) (JJS least) (CD 4.5) (CD billion))
              (NNS years))
            (, ,)
            (VP (VBG measuring)
              (PP (IN from)
                (NP
                  (NP
                    (NP (DT the) (NN planet) (POS 's))
                    (JJ own) (NN frame))
                  (PP (IN of)
                    (NP (NN reference))))))))))
    (. .)))

poss(planet-2, Our-1)
nsubj(been-4, planet-2)
aux(been-4, has-3)
root(ROOT-0, been-4)
advmod(been-4, here-5)
quantmod(billion-10, at-7)
mwe(at-7, least-8)
pobj(at-7, least-8)
number(billion-10, 4.5-9)
num(years-11, billion-10)
prep_for(been-4, years-11)
partmod(years-11, measuring-13)
det(planet-16, the-15)
poss(frame-19, planet-16)
amod(frame-19, own-18)
prep_from(measuring-13, frame-19)
prep_of(frame-19, reference-21)

