(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (DT that))
        (VP (VBZ is)
          (NP (DT the) (NN case)))))
    (, ,)
    (ADVP (RB then))
    (NP (PRP you))
    (VP (VBP are)
      (ADVP (RB severely))
      (VP (VBN mistaken)))
    (. .)))

mark(case-5, If-1)
nsubj(case-5, that-2)
cop(case-5, is-3)
det(case-5, the-4)
advcl(mistaken-11, case-5)
advmod(mistaken-11, then-7)
nsubjpass(mistaken-11, you-8)
auxpass(mistaken-11, are-9)
advmod(mistaken-11, severely-10)
root(ROOT-0, mistaken-11)

