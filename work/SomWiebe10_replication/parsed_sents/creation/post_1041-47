(ROOT
  (S
    (NP (EX There))
    (VP (VBP are)
      (NP
        (NP (JJ numerous) (NNS examples))
        (PP (IN of)
          (NP (JJ observed) (NN speciation))))
      (, ,)
      (ADVP (RB especially))
      (PP (IN among)
        (NP (NNS bacteria))))
    (. .)))

expl(are-2, There-1)
root(ROOT-0, are-2)
amod(examples-4, numerous-3)
nsubj(are-2, examples-4)
amod(speciation-7, observed-6)
prep_of(examples-4, speciation-7)
advmod(are-2, especially-9)
prep_among(are-2, bacteria-11)

