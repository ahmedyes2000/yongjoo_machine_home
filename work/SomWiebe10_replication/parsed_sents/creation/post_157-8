      (SBAR
        (WHNP (WDT which))
        (S
          (VP (MD can)
            (VP (VB compete)
              (PP (IN with)
                (NP (JJ other) (NNS explanations))))))))
    (. .)))

cc(assigns-5, But-1)
det(science-4, that-3)
