(ROOT
  (S ('' '')
    (S
      (NP (PRP He))
      (VP (VBZ is) (RB n't)
        (VP (VBG doing)
          (NP (NN science)))))
    (, ,)
    (NP (PRP he))
    (VP (VBZ 's)
      (ADVP (RB just))
      (NP (NN name-calling)))
    (. .)))

nsubj(doing-5, He-2)
aux(doing-5, is-3)
neg(doing-5, n't-4)
ccomp(name-calling-11, doing-5)
dobj(doing-5, science-6)
nsubj(name-calling-11, he-8)
cop(name-calling-11, 's-9)
advmod(name-calling-11, just-10)
root(ROOT-0, name-calling-11)

