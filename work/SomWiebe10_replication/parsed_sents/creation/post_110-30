(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (DT the) (NN statement))
        (VP (VBD was)
          (VP (VBN translated)
            (PP (IN as) (IN above)
              (NP
                (NP (DT the) (NN sphere))
                (PP (IN of)
                  (NP (DT the) (NN Earth)))))
            (ADVP (RB then))))))
    (NP (DT a) (NN case))
    (VP (MD could)
      (VP (VB be)
        (VP (VBN made))))
    (. .)))

mark(translated-5, If-1)
det(statement-3, the-2)
nsubjpass(translated-5, statement-3)
auxpass(translated-5, was-4)
advcl(made-18, translated-5)
dep(translated-5, as-6)
det(sphere-9, the-8)
prep_above(translated-5, sphere-9)
det(Earth-12, the-11)
prep_of(sphere-9, Earth-12)
advmod(translated-5, then-13)
det(case-15, a-14)
nsubjpass(made-18, case-15)
aux(made-18, could-16)
auxpass(made-18, be-17)
root(ROOT-0, made-18)

