(ROOT
  (S ('' '')
    (NP (PRP It))
    (VP (VBZ is)
      (ADVP (RB also))
      (ADJP (JJ illogical)
        (S
          (VP (TO to)
            (VP (VB include)
              (NP
                (NP (NNP God))
                (PP (IN in)
                  (NP
                    (NP (DT the) (NN study))
                    (PP (IN of)
                      (NP (NNP Science))))))))))
      (SBAR (RB just) (IN because)
        (S
          (NP (PRP you))
          (VP (VBP want)
            (S
              (NP (PRP him))
              (VP (TO to)
                (VP (VB exist))))))))
    (. .)))

nsubj(illogical-5, It-2)
cop(illogical-5, is-3)
advmod(illogical-5, also-4)
root(ROOT-0, illogical-5)
aux(include-7, to-6)
xcomp(illogical-5, include-7)
dobj(include-7, God-8)
det(study-11, the-10)
prep_in(God-8, study-11)
prep_of(study-11, Science-13)
advmod(want-17, just-14)
dep(want-17, because-15)
nsubj(want-17, you-16)
ccomp(illogical-5, want-17)
nsubj(exist-20, him-18)
aux(exist-20, to-19)
xcomp(want-17, exist-20)

