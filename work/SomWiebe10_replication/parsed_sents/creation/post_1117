(ROOT
  (S
    (ADVP (RB Yes))
    (NP (PRP I))
    (VP (VBP believe)
      (SBAR
        (S
          (NP
            (NP (DT the) (NN theory))
            (PP (IN of)
              (NP (NN evolution))))
          (VP (VBZ is)
            (ADJP (JJ wrong))
            (SBAR (IN as)
              (S
                (NP (EX there))
                (VP (VBP are)
                  (NP
                    (NP (DT no) (JJ scientific) (NNS facts))
                    (PP (IN about)
                      (NP (DT that)))))))))))
    (. .)))

