(ROOT
  (S
    (NP (PRP It))
    (VP (MD can)
      (VP (VB be)
        (VP (VBN shown)
          (SBAR
            (SBAR (IN that)
              (S
                (NP
                  (NP (DT the) (JJ physical) (NN complexity))
                  (PP (IN of)
                    (NP
                      (NP (DT the) (NNS genomes))
                      (PP (IN of)
                        (NP (JJ clonal) (NNS organisms))))))
                (VP (MD must)
                  (VP (VB increase)
                    (PP (IN in)
                      (NP (NN evolution)))))))
            (, ,)
            (SBAR (IN if)
              (S
                (NP (PRP they))
                (VP (VB occupy)
                  (NP (DT a) (JJ single) (NN niche)))))
            (CC and)
            (SBAR (IN if)
              (S
                (NP (DT the) (NN environment))
                (VP (VBZ does) (RB not)
                  (VP (VB change)))))))))
    (. .)))

