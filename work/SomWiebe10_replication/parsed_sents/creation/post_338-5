(ROOT
  (S
    (PP (IN In)
      (NP (NN fact)))
    (NP (RB even) (NNPS Krauss))
    (VP (VBD stated)
      (SBAR (IN that)
        (S
          (NP (EX there))
          (VP (VBD were)
            (NP
              (NP (JJ various) (JJ plausible) (NNS explanations))
              (PP (IN for)
                (NP (DT the) (NNS observations))))))))
    (. .)))

prep_in(stated-5, fact-2)
advmod(Krauss-4, even-3)
nsubj(stated-5, Krauss-4)
root(ROOT-0, stated-5)
complm(were-8, that-6)
expl(were-8, there-7)
ccomp(stated-5, were-8)
amod(explanations-11, various-9)
amod(explanations-11, plausible-10)
nsubj(were-8, explanations-11)
det(observations-14, the-13)
prep_for(explanations-11, observations-14)

