        (PP (IN of)
          (NP (NNS words)))
        (, ,)
        (PP (IN by)
          (NP (DT the) (NN way)))))
    (: ...)
    (S
      (NP (NNS atheists))
      (VP (VBP believe)
        (SBAR (IN that)
          (S
            (NP (NNP God))
            (VP (VBZ IS)
              (NP
                (NP (NN Nothing))
                (PRN (-LRB- -LRB-)
                  (PP (IN in)
                    (NP (JJ other) (NNS words))
                    (: ...))
                  (NP
                    (NP (NN something))
                    (SBAR
                      (WHNP (WDT that))
                      (S
                        (VP (VBZ is)
                          (ADJP (JJ non-existent))))))
                  (-RRB- -RRB-))))))))
    (. .)))

