(ROOT
  (FRAG
    (NP (PRP$ My) (NN point) (NN being))
    (: :)
    (S
      (NP (PRP You))
      (VP (MD can) (RB not)
        (VP (VB manipulate)
          (NP (NN time))
          (SBAR (IN as)
            (S
              (NP (PRP you))
              (VP (VBP please)
                (S
                  (VP (TO to)
                    (VP (VB suit)
                      (NP
                        (NP (PRP$ your) (NN interpretation))
                        (PP (IN of)
                          (NP (DT the) (NNP Bible)))))))))))))
    (. .)))

poss(being-3, My-1)
nn(being-3, point-2)
root(ROOT-0, being-3)
nsubj(manipulate-8, You-5)
aux(manipulate-8, can-6)
neg(manipulate-8, not-7)
dep(being-3, manipulate-8)
