(ROOT
  (S (CC Yet)
    (ADVP (DT all) (IN of))
    (NP (DT these))
    (VP (VBP are)
      (ADVP (RB far) (RBR more))
      (VP (VBN accepted)
        (PP (IN than)
          (NP (NN evolution)))))
    (: ...)))

cc(accepted-8, Yet-1)
dep(of-3, all-2)
advmod(accepted-8, of-3)
nsubjpass(accepted-8, these-4)
auxpass(accepted-8, are-5)
advmod(more-7, far-6)
advmod(accepted-8, more-7)
root(ROOT-0, accepted-8)
prep_than(accepted-8, evolution-10)

