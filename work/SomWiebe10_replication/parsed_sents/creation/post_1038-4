  (S
    (S
      (NP (PRP We))
      (VP (MD ca) (RB n't)
        (VP (VB indoctrinate)
          (NP (PRP$ our) (NNS students))
          (NP
            (NP (CD one) (NN way))
            (CC or)
            (NP (DT the) (JJ other))))))
    (, ,)
    (CC and)
    (S
      (S
        (VP (VBG being)
          (ADJP (JJ fair)
            (PP (IN with)
              (NP (JJ religious) (NN education))))))
      (VP (MD would)
        (VP (VB mean)
          (SBAR
            (SBAR
              (S
                (NP (PRP we))
                (VP (VBP spend)
                  (NP
                    (NP (NNS years))
