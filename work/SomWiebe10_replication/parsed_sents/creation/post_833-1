(ROOT
  (S
    (ADVP (RB Specifically))
    (NP (DT the) (NNS days))
    (VP (VBD named)
      (PP (IN after)
        (NP
          (NP (NN pagan) (NNS Gods))
          (: --)
          (NP
            (NP
              (NP (NNP Odin) (POS 's))
              (NN day))
            (, ,)
            (NP
              (NP (NNP Thor) (POS 's))
              (NN Day))
            (, ,)
            (NP
              (NP (NNP Freya) (POS 's))
              (NN Day)))
          (, ,)
          (X (NN etc.)))))
    (. .)))

advmod(named-4, Specifically-1)
det(days-3, the-2)
nsubj(named-4, days-3)
root(ROOT-0, named-4)
nn(Gods-7, pagan-6)
prep_after(named-4, Gods-7)
poss(day-11, Odin-9)
dep(Gods-7, day-11)
poss(Day-15, Thor-13)
appos(day-11, Day-15)
poss(Day-19, Freya-17)
appos(day-11, Day-19)
dep(Gods-7, etc.-21)

(ROOT
  (SQ (MD Would)
    (NP (NNP God))
    (VP (VB have)
      (VP (VBN built)
        (NP
          (NP (DT a) (NN pattern))
          (PP (IN of)
            (NP (NN idolatry))))
        (PP (IN into)
          (NP
            (NP (NN something))
            (ADJP (RB that) (JJ significant))))))
    (. ?)))

aux(built-4, Would-1)
nsubj(built-4, God-2)
aux(built-4, have-3)
root(ROOT-0, built-4)
det(pattern-6, a-5)
dobj(built-4, pattern-6)
prep_of(pattern-6, idolatry-8)
prep_into(built-4, something-10)
advmod(significant-12, that-11)
amod(something-10, significant-12)

