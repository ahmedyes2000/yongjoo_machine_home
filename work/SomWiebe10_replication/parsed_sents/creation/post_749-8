    (SBAR (IN If)
      (S
        (NP (PRP it))
        (VP (VBD was)
          (NP (DT an) (JJ intentional) (NN creation))
          (ADVP (RB then)))))
    (WHNP (WP what))
    (SQ
      (VP (VBD was)
        (NP
          (NP (DT the) (NN motive))
          (PP (IN behind)
            (NP (PRP$ its) (NN creation))))))
    (. ?)))

