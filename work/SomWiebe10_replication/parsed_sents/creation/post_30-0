    (. .)))

csubj(violating-8, Preventing-1)
dobj(Preventing-1, creationism-2)
aux(taught-5, to-3)
auxpass(taught-5, be-4)
xcomp(Preventing-1, taught-5)
aux(violating-8, is-6)
neg(violating-8, not-7)
