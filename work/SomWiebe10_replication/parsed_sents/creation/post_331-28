(ROOT
  (S ('' '')
    (NP (NNP Religion))
    (ADVP (RB simply))
    (VP (VBZ is) (RB not)
      (VP (VBN concerned)
        (PP (IN with)
          (NP
            (NP (DT the) (NN truth))
            (PP (IN of)
              (NP (NN anything)))))))
    (. .)))

nsubjpass(concerned-6, Religion-2)
advmod(concerned-6, simply-3)
auxpass(concerned-6, is-4)
neg(concerned-6, not-5)
root(ROOT-0, concerned-6)
det(truth-9, the-8)
prep_with(concerned-6, truth-9)
prep_of(truth-9, anything-11)

