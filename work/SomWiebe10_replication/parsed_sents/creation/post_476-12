  (S
    (S
      (VP (VBG Insisting)
        (SBAR (IN that)
          (S
            (NP (NNP God))
            (VP (VBZ reveals)
              (NP (PRP Himself))
              (PP (IN in)
                (NP
                  (NP (PRP$ His) (NN Creation))
                  (PP (TO to)
                    (NP (JJ scientific) (NN scrutiny))))))))))
    (VP (VBZ seems)
      (PP (TO to)
        (NP (PRP me)))
      (PP (IN as)
