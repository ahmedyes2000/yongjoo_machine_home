(ROOT
  (S
    (NP (JJ Theistic) (NN evolution))
    (VP (VBZ is)
      (NP
        (NP (NN nonsense))
        (PP (IN for)
          (NP (DT the) (JJ same) (NN reason)))))
    (. .)))

amod(evolution-2, Theistic-1)
nsubj(nonsense-4, evolution-2)
cop(nonsense-4, is-3)
root(ROOT-0, nonsense-4)
det(reason-8, the-6)
amod(reason-8, same-7)
prep_for(nonsense-4, reason-8)

