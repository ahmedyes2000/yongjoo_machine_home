(ROOT
  (S
    (S
      (ADVP (RB Now))
      (NP (PRP we))
      (VP (MD must)
        (VP (VB insist)
          (S
            (NP (DT the) (JJ big) (NN bird) (NN theory))
            (VP (VB be)
              (VP (VBN taught)
                (PP (IN in)
                  (NP
                    (NP (NNS schools))
                    (CC and)
                    (NP (DT both) (NNP Evolution))))))))))
    (CC and)
    (S
      (NP
        (NP (NNP God) (POS 's))
        (NNP Creationism))
      (VP (VBP are)
        (S
          (VP (TO to)
            (VP (VB be)
              (VP (VBN debunked) ('' '')))))))
    (. .)))

advmod(insist-4, Now-1)
nsubj(insist-4, we-2)
aux(insist-4, must-3)
root(ROOT-0, insist-4)
det(theory-8, the-5)
