(ROOT
  (S
    (NP (PRP I))
    (VP (VBP know)
      (SBAR
        (S
          (NP (PRP you))
          (VP (VBP believe)
            (SBAR (IN that)
              (S
                (NP (NNP ID))
                (VP (VBZ is)
                  (VP (VBN excluded)
                    (PP (IN from)
                      (NP
                        (NP (JJ scientific) (NNS texts))
                        (PP (RB because) (IN of)
                          (NP (PRP$ its) (JJ religious) (NNS connotations)))))))))))))
    (. .)))

nsubj(know-2, I-1)
root(ROOT-0, know-2)
nsubj(believe-4, you-3)
ccomp(know-2, believe-4)
complm(excluded-8, that-5)
nsubjpass(excluded-8, ID-6)
auxpass(excluded-8, is-7)
ccomp(believe-4, excluded-8)
amod(texts-11, scientific-10)
prep_from(excluded-8, texts-11)
poss(connotations-16, its-14)
amod(connotations-16, religious-15)
prep_because_of(texts-11, connotations-16)

