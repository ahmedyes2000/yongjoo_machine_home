(ROOT
  (S
    (NP (DT The) (JJ above) (NN statement))
    (VP (VBZ is)
      (ADJP (JJ true)
        (PP (IN with)
          (NP (DT this) (NN substitution)))))
    (. .)))

det(statement-3, The-1)
amod(statement-3, above-2)
nsubj(true-5, statement-3)
cop(true-5, is-4)
root(ROOT-0, true-5)
det(substitution-8, this-7)
prep_with(true-5, substitution-8)

