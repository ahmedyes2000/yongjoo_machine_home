(ROOT
  (S
    (NP (EX There))
    (VP (VBP are)
      (NP
        (NP (CD two) (JJ basic) (NNS reasons))
        (PP (TO to)
          (NP (JJ postulate) (NN anything)))))
    (. .)))

expl(are-2, There-1)
root(ROOT-0, are-2)
num(reasons-5, two-3)
amod(reasons-5, basic-4)
nsubj(are-2, reasons-5)
amod(anything-8, postulate-7)
prep_to(reasons-5, anything-8)

