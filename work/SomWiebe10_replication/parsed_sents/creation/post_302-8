(ROOT
  (S ('' '')
    (PP (VBG Any)
      (PP (IN of)
        (NP (PRP them))))
    (VP (MD can)
      (VP (VB be)
        (NP (DT a) (NN possibility))))
    (. .)))

prep(possibility-8, Any-2)
pcomp(Any-2, of-3)
prep_of(Any-2, them-4)
aux(possibility-8, can-5)
cop(possibility-8, be-6)
det(possibility-8, a-7)
root(ROOT-0, possibility-8)

