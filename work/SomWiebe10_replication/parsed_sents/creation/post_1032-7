(ROOT
  (S (CC Or)
    (ADVP (RB maybe) (RB not))
    (, ,)
    (NP (NNP Philosophy))
    (VP (VBZ is)
      (NP
        (NP (DT a)
          (ADJP (RB pretty) (JJ harsh))
          (NN subject))
        (VP
          (ADVP (RB now))
          (NP (DT a) (NNS days)))))
    (. .)))

cc(subject-10, Or-1)
advmod(not-3, maybe-2)
neg(subject-10, not-3)
nsubj(subject-10, Philosophy-5)
cop(subject-10, is-6)
det(subject-10, a-7)
advmod(harsh-9, pretty-8)
amod(subject-10, harsh-9)
root(ROOT-0, subject-10)
advmod(days-13, now-11)
det(days-13, a-12)
dep(subject-10, days-13)

