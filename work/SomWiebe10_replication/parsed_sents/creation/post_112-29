(ROOT
  (S
    (PP (IN for)
      (NP (DT that) (NN matter)))
    (NP
      (NP (DT the) (NN reason))
      (SBAR
        (S
          (NP (PRP I))
          (VP (VBP 'm)
            (ADJP (RB so) (JJ positive)))))
      (PP (IN of)
        (NP
          (NP (NNP God) (POS 's))
          (NN existence))))
    (VP (VBZ is)
      (PP (IN through)
        (NP
          (NP (NNS things))
          (SBAR
            (WHNP (WDT which))
            (S
              (VP (VBD happened)
                (PP (IN in)
                  (NP (PRP$ my) (JJ own) (NN life)))))))))
    (. .)))

det(matter-3, that-2)
prep_for(is-14, matter-3)
det(reason-5, the-4)
nsubj(is-14, reason-5)
nsubj(positive-9, I-6)
cop(positive-9, 'm-7)
advmod(positive-9, so-8)
rcmod(reason-5, positive-9)
poss(existence-13, God-11)
prep_of(reason-5, existence-13)
root(ROOT-0, is-14)
prep_through(is-14, things-16)
nsubj(happened-18, things-16)
rcmod(things-16, happened-18)
poss(life-22, my-20)
amod(life-22, own-21)
prep_in(happened-18, life-22)

