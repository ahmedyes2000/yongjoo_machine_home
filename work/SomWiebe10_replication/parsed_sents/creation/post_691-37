(ROOT
  (S
    (NP (NNS Irenaeus))
    (VP (VBD taught)
      (SBAR (IN that)
        (S
          (NP
            (NP (DT all))
            (SBAR
              (WHNP (WDT that))
              (S
                (VP (VBD was)
                  (VP (VBN needed))))))
          (VP (VBD was)
            (NP (NN faith))))))
    (. .)))

nsubj(taught-2, Irenaeus-1)
root(ROOT-0, taught-2)
complm(faith-9, that-3)
nsubjpass(needed-7, all-4)
nsubj(faith-9, all-4)
auxpass(needed-7, was-6)
rcmod(all-4, needed-7)
cop(faith-9, was-8)
ccomp(taught-2, faith-9)

