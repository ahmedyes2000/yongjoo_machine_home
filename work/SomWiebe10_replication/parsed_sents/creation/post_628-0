(ROOT
  (S
    (NP (PRP I))
    (VP (VBP appreciate)
      (SBAR (IN that)
        (S
          (NP (PRP you))
          (VP (VBP are)
            (VP (VBG making)
              (NP (NNS attempts))
              (S
                (VP (TO to)
                  (VP (VB answer)
                    (NP (PDT all) (DT these) (NNS issues))))))))))
    (. .)))

nsubj(appreciate-2, I-1)
root(ROOT-0, appreciate-2)
complm(making-6, that-3)
nsubj(making-6, you-4)
aux(making-6, are-5)
ccomp(appreciate-2, making-6)
dobj(making-6, attempts-7)
aux(answer-9, to-8)
xcomp(making-6, answer-9)
predet(issues-12, all-10)
det(issues-12, these-11)
dobj(answer-9, issues-12)

