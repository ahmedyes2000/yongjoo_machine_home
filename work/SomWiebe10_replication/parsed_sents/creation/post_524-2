(ROOT
  (FRAG
    (NP (NNP QED))
    (: --)
    (S (CC And)
      (NP (DT the) (JJ bottom) (NN line))
      (VP (VBZ is)
        (SBAR (IN if)
          (S
            (NP (DT the) (JJ anti) (NNP ID) (NN side))
            (ADVP (RB actually))
            (VP (VBD had)
              (NP
                (NP (DT some) (JJ scientific) (NN evidence))
                (PP (IN for)
                  (NP (PRP$ their) (NN position)))
                (SBAR
                  (S
                    (NP (NNP ID))
                    (VP (MD would)
                      (VP (VB go)
                        (PRT (RP away))))))))))))
    (. .)))

root(ROOT-0, QED-1)
cc(is-7, And-3)
det(line-6, the-4)
amod(line-6, bottom-5)
nsubj(is-7, line-6)
dep(QED-1, is-7)
mark(had-14, if-8)
det(side-12, the-9)
amod(side-12, anti-10)
nn(side-12, ID-11)
nsubj(had-14, side-12)
advmod(had-14, actually-13)
advcl(is-7, had-14)
det(evidence-17, some-15)
amod(evidence-17, scientific-16)
dobj(had-14, evidence-17)
dobj(go-23, evidence-17)
poss(position-20, their-19)
prep_for(evidence-17, position-20)
nsubj(go-23, ID-21)
aux(go-23, would-22)
rcmod(evidence-17, go-23)
prt(go-23, away-24)

