(ROOT
  (S
    (S (CC And)
      (VP (VB believe)
        (NP (PRP me))))
    (, ,)
    (NP (DT those) (NNS commandments))
    (VP (VBD were) (RB n't)
      (VP (VBN invented)
        (PP (IN at)
          (NP (DT that) (NN time)))))
    (. .)))

cc(believe-2, And-1)
ccomp(invented-9, believe-2)
dobj(believe-2, me-3)
det(commandments-6, those-5)
nsubjpass(invented-9, commandments-6)
auxpass(invented-9, were-7)
neg(invented-9, n't-8)
root(ROOT-0, invented-9)
det(time-12, that-11)
prep_at(invented-9, time-12)

