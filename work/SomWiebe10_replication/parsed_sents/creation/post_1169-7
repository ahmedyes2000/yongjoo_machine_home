(ROOT
  (S
    (NP (PRP$ My) (NN answer))
    (VP (VBZ is)
      (SBAR (IN that)
        (S
          (NP
            (NP (NNP God) (POS 's))
            (NN clock))
          (VP (VBZ is) (RB not)
            (NP (PRP$ our) (NN clock))))))
    (. .)))

poss(answer-2, My-1)
nsubj(is-3, answer-2)
root(ROOT-0, is-3)
complm(clock-11, that-4)
poss(clock-7, God-5)
nsubj(clock-11, clock-7)
cop(clock-11, is-8)
neg(clock-11, not-9)
poss(clock-11, our-10)
ccomp(is-3, clock-11)

