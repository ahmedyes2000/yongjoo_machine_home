(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ 's)
      (ADJP (JJ hard)
        (S
          (VP (TO to)
            (VP (VB argue)
              (SBAR (IN that)
                (S
                  (VP (VBG teaching)
                    (CC or)
                    (VBG discussing)
                    (NP (JJ intelligent) (NN design) (NNS advances)))
                  (NP
                    (NP (DT any) (NN one))
                    (NP (NN religion))))))))))
    (. .)))

nsubj(hard-3, It-1)
cop(hard-3, 's-2)
root(ROOT-0, hard-3)
aux(argue-5, to-4)
xcomp(hard-3, argue-5)
complm(teaching-7, that-6)
ccomp(argue-5, teaching-7)
