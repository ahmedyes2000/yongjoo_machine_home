(ROOT
  (S
    (ADVP (RB Yes))
    (, ,)
    (SBAR (IN if)
      (S
        (NP (PRP we))
        (VP (VBP teach))))
    (NP (DT those))
    (ADVP
      (NP (CD 5) (NNS laws))
      (RB as))
    (VP (VBZ originating)
      (PP (IN from)
        (NP (DT a) (JJ divine) (NN source))))
    (. .)))

advmod(originating-10, Yes-1)
mark(teach-5, if-3)
nsubj(teach-5, we-4)
dep(originating-10, teach-5)
nsubj(originating-10, those-6)
num(laws-8, 5-7)
npadvmod(as-9, laws-8)
advmod(originating-10, as-9)
root(ROOT-0, originating-10)
det(source-14, a-12)
amod(source-14, divine-13)
prep_from(originating-10, source-14)

