(ROOT
  (S (: --)
    (CC Or)
    (NP (DT that) (NN evolution))
    (VP (VBZ is)
      (NP
        (NP (DT a) (NN process))
        (PP (VBG involving)
          (NP
            (NP (NN randomness))
            (CC and)
            (NP (JJ natural) (NN selection))))))
    (. .)))

det(evolution-4, that-3)
conj_or(process-7, evolution-4)
nsubj(process-7, evolution-4)
cop(process-7, is-5)
det(process-7, a-6)
root(ROOT-0, process-7)
