(ROOT
  (S
    (NP (JJ Creation\/intelligent) (NN design))
    (VP (VBZ does) (RB n't)
      (VP (VB need)
        (NP (DT this) (NN step))))
    (. .)))

amod(design-2, Creation\/intelligent-1)
