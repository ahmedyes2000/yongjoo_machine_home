(ROOT
  (S
    (VP (VB Let)
      (NP (PRP s))
      (ADVP (RB actually))
      (VP (VB be)
        (ADJP (JJ scientific)
          (PP (IN about)
            (NP (DT this))))))
    (. !)))

root(ROOT-0, Let-1)
dobj(Let-1, s-2)
advmod(Let-1, actually-3)
cop(scientific-5, be-4)
dep(Let-1, scientific-5)
prep_about(scientific-5, this-7)

