(ROOT
  (S
    (NP (PRP He))
    (VP (VBD was)
      (VP (VBG using)
        (NP
          (NP (JJ holy))
          (PP (IN as)
            (NP
              (NP (DT the) (NN opposite))
              (PP (IN of)
                (NP
                  (NP (NN profane))
                  (NP (DT the) (NN day)))))))
        (PP (TO to)
          (NP
            (NP (NN day))
            (PP (IN versus)
              (NP (DT the) (RB beyond) (JJ human) (NN understanding)))))))
    (. .)))

nsubj(using-3, He-1)
aux(using-3, was-2)
root(ROOT-0, using-3)
dobj(using-3, holy-4)
det(opposite-7, the-6)
prep_as(holy-4, opposite-7)
prep_of(opposite-7, profane-9)
det(day-11, the-10)
dep(profane-9, day-11)
prep_to(using-3, day-13)
det(understanding-18, the-15)
advmod(understanding-18, beyond-16)
amod(understanding-18, human-17)
prep_versus(day-13, understanding-18)

