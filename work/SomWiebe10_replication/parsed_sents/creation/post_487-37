(ROOT
  (S
    (ADVP (RB However))
    (, ,)
    (NP (DT the) (NNP ACLU))
    (VP (VBD mentioned)
      (NP
        (NP (CD one) (JJ possible) (NN scenario))
        (PP (IN in)
          (NP
            (NP (PRP$ their) (NN brief))
            (VP (VBN submitted)
              (PP (TO to)
                (NP (NNP Judge) (NNP Jones))))))))
    (. .)))

advmod(mentioned-5, However-1)
det(ACLU-4, the-3)
nsubj(mentioned-5, ACLU-4)
root(ROOT-0, mentioned-5)
num(scenario-8, one-6)
amod(scenario-8, possible-7)
dobj(mentioned-5, scenario-8)
poss(brief-11, their-10)
prep_in(scenario-8, brief-11)
partmod(brief-11, submitted-12)
nn(Jones-15, Judge-14)
prep_to(submitted-12, Jones-15)

