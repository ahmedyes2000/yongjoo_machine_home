(ROOT
  (S
    (NP (PRP It))
    (ADVP (RB always))
    (VP (VBZ devolves)
      (PP (TO to)
        (NP
          (NP (DT this))
          (SBAR
            (WHNP
              (WHADVP (WRB when))
              (NN talking))
            (S
              (VP (TO to)
                (VP (VB ID)
                  (NP (NNS proponents)))))))))
    (. .)))

nsubj(devolves-3, It-1)
