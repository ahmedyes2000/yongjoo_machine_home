(ROOT
  (S
    (NP
      (ADJP (NNP Young) (, ,) (JJ impressionable))
      (NNS students))
    (VP (VBP are)
      (ADJP (RBR more) (JJ likely)
        (S
          (VP (TO to)
            (VP (VB believe)
              (NP (JJ such) (NNS teachings)))))))
    (. .)))

amod(impressionable-3, Young-1)
amod(students-4, impressionable-3)
nsubj(likely-7, students-4)
cop(likely-7, are-5)
advmod(likely-7, more-6)
root(ROOT-0, likely-7)
aux(believe-9, to-8)
xcomp(likely-7, believe-9)
amod(teachings-11, such-10)
dobj(believe-9, teachings-11)

