  (S
    (NP (DT The) (NN point))
    (VP (VBZ is)
      (SBAR (IN that)
        (S
          (S
            (PP (IN in)
              (NP (DT this) (NN case)))
            (NP (DT the) (JJ fundamentalist))
            (VP (VBD were)
              (ADJP (JJ right))))
          (CC and)
          (S
            (NP (DT the) (NNS scientists))
            (VP (VBD were)
              (ADJP (JJ wrong)))))))
    (. .)))

