    (VP (VBP have)
      (VP (VBN been)
        (NP
          (NP (DT the) (JJ same))
          (PP (IN since)
            (NP
              (NP (DT the) (NN time))
              (PP (IN of)
                (NP (DT the) (NNS dinasors))))))
        (PP (IN with)
          (NP
            (NP (RB little))
            (CC or)
            (NP (DT no) (NN change))))))
    (: ...)))

nsubj(same-5, Sharks-1)
aux(same-5, have-2)
cop(same-5, been-3)
det(same-5, the-4)
root(ROOT-0, same-5)
