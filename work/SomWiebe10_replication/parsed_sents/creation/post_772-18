(ROOT
  (S ('' '')
    (NP (RB Now) (PRP you))
    (VP (VBP 're)
      (ADVP (RB just))
      (VP (VBN confused)
        (PP (IN in)
          (NP (NN general)))))
    (. .)))

advmod(you-3, Now-2)
nsubjpass(confused-6, you-3)
auxpass(confused-6, 're-4)
advmod(confused-6, just-5)
root(ROOT-0, confused-6)
prep_in(confused-6, general-8)

