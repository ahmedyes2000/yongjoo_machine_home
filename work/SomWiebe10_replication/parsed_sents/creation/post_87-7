(ROOT
  (S
    (NP (PRP They))
    (VP (VBP are)
      (UCP
        (ADJP (JJ entrenched))
        (CC and)
        (VP (VBG defending)
          (NP (PRP$ their) (NN hold))
          (PP (IN on)
            (NP (NN science)))
          (, ,)
          (ADVP (RB furiously)))))
    (. .)))

nsubj(entrenched-3, They-1)
nsubj(defending-5, They-1)
cop(entrenched-3, are-2)
root(ROOT-0, entrenched-3)
conj_and(entrenched-3, defending-5)
poss(hold-7, their-6)
dobj(defending-5, hold-7)
prep_on(defending-5, science-9)
advmod(defending-5, furiously-11)

