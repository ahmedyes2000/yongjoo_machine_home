(ROOT
  (S
    (NP
      (NP (DT The) (NN evidence))
      (PP (IN for)
        (NP (DT an) (JJ ancient) (NN earth))))
    (VP (VBZ is)
      (ADJP (JJ overwhelming)))
    (. .)))

det(evidence-2, The-1)
nsubj(overwhelming-8, evidence-2)
det(earth-6, an-4)
amod(earth-6, ancient-5)
prep_for(evidence-2, earth-6)
cop(overwhelming-8, is-7)
root(ROOT-0, overwhelming-8)

