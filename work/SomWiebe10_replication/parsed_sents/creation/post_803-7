(ROOT
  (S
    (NP (NNP God))
    (VP (VBZ takes)
      (PP (IN on)
        (NP
          (NP (DT the) (NN behavior))
          (PP (IN of)
            (NP (DT a) (NNP Ceasar)))))
      (SBAR (IN once)
        (S
          (NP (NNP Ceasar))
          (VP (VBD co-opted)
            (NP (DT the) (NN bible))))))
    (. .)))

nsubj(takes-2, God-1)
root(ROOT-0, takes-2)
det(behavior-5, the-4)
prep_on(takes-2, behavior-5)
det(Ceasar-8, a-7)
prep_of(behavior-5, Ceasar-8)
mark(co-opted-11, once-9)
nsubj(co-opted-11, Ceasar-10)
advcl(takes-2, co-opted-11)
det(bible-13, the-12)
