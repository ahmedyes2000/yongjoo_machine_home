(ROOT
  (S
    (NP (RB Even) (DT the) (NNP Discovery) (NNP Institute))
    (VP (VBD was)
      (PP (IN against)
        (SBAR
          (WHNP (WP what))
          (S
            (NP (PRP they))
            (VP (VBD were)
              (VP (VBG trying)
                (S
                  (VP (TO to)
                    (VP (VB do))))))))))
    (. .)))

advmod(Institute-4, Even-1)
det(Institute-4, the-2)
nn(Institute-4, Discovery-3)
nsubj(was-5, Institute-4)
root(ROOT-0, was-5)
dobj(trying-10, what-7)
nsubj(trying-10, they-8)
xsubj(do-12, they-8)
aux(trying-10, were-9)
prepc_against(was-5, trying-10)
aux(do-12, to-11)
xcomp(trying-10, do-12)

