(ROOT
  (S
    (NP (PRP You))
    (VP (VBP are)
      (VP (VBG talking)
        (PP (IN about)
          (NP
            (NP (PRP$ your) (NN opinion))
            (PP (IN of)
              (SBAR
                (WHNP (WP what))
                (S
                  (NP (PRP you))
                  (VP (VBP see)
                    (PP (IN in)
                      (NP (NN nature)))))))))))
    (. .)))

nsubj(talking-3, You-1)
aux(talking-3, are-2)
root(ROOT-0, talking-3)
poss(opinion-6, your-5)
prep_about(talking-3, opinion-6)
dobj(see-10, what-8)
nsubj(see-10, you-9)
prepc_of(opinion-6, see-10)
prep_in(see-10, nature-12)

