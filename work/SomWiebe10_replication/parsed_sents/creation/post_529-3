(ROOT
  (S
    (VP (VB Let)
      (NP (PRP 's))
      (ADVP (RB thus))
      (VP (VB explore)
        (SBAR
          (WHADVP (WRB why))
          (S
            (NP (NNP ID))
            (VP
              (ADVP (RB once) (RB again))
              (VBZ remains)
              (NP (JJ scientifically) (NNS vacuous)))))))
    (. .)))

root(ROOT-0, Let-1)
dobj(Let-1, 's-2)
advmod(Let-1, thus-3)
dep(Let-1, explore-4)
advmod(vacuous-11, why-5)
nsubj(vacuous-11, ID-6)
advmod(again-8, once-7)
advmod(vacuous-11, again-8)
cop(vacuous-11, remains-9)
amod(vacuous-11, scientifically-10)
advcl(explore-4, vacuous-11)

