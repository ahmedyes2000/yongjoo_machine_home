(ROOT
  (S
    (NP (PRP I))
    (VP (MD would)
      (VP (VB be)
        (ADJP (JJ interested)
          (S
            (VP (TO to)
              (VP (VB know)
                (SBAR
                  (WHNP (WP what))
                  (S
                    (NP
                      (NP (DT the) (NN evidence))
                      (PP (IN against)
                        (NP (NN design))))
                    (VP (MD could)
                      (VP (VB be)))))))))))
    (. .)))

nsubj(interested-4, I-1)
aux(interested-4, would-2)
cop(interested-4, be-3)
root(ROOT-0, interested-4)
aux(know-6, to-5)
xcomp(interested-4, know-6)
dobj(be-13, what-7)
det(evidence-9, the-8)
nsubj(be-13, evidence-9)
prep_against(evidence-9, design-11)
aux(be-13, could-12)
ccomp(know-6, be-13)

