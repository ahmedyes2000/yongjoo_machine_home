(ROOT
  (S ('' '')
    (ADVP (RB First))
    (, ,)
    (NP (DT a) (NN religion))
    (VP (VBZ addresses)
      (NP (JJ fundamental)
        (CC and)
        (JJ ultimate) (NNS questions))
      (S
        (VP (VBG having)
          (S
            (VP (TO to)
              (VP (VB do)
                (PP (IN with)
                  (NP
                    (ADJP (JJ deep)
                      (CC and)
                      (JJ imponderable))
                    (NNS matters)))))))))
    (. .)))

advmod(addresses-6, First-2)
det(religion-5, a-4)
nsubj(addresses-6, religion-5)
root(ROOT-0, addresses-6)
amod(questions-10, fundamental-7)
conj_and(fundamental-7, ultimate-9)
amod(questions-10, ultimate-9)
dobj(addresses-6, questions-10)
xcomp(addresses-6, having-11)
aux(do-13, to-12)
xcomp(having-11, do-13)
amod(matters-18, deep-15)
conj_and(deep-15, imponderable-17)
amod(matters-18, imponderable-17)
prep_with(do-13, matters-18)

