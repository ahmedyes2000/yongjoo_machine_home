(ROOT
  (S
    (NP (DT This) (NN principle))
    (VP (VBZ offers)
      (NP
        (NP (NN immense))
        (, ,)
        (CC and)
        (NP (JJ verifiable) (, ,) (JJ predictive) (NN power))))
    (. .)))

det(principle-2, This-1)
nsubj(offers-3, principle-2)
root(ROOT-0, offers-3)
dobj(offers-3, immense-4)
amod(power-10, verifiable-7)
amod(power-10, predictive-9)
dobj(offers-3, power-10)
conj_and(immense-4, power-10)

