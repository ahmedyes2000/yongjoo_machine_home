(ROOT
  (FRAG
    (NP (NNP PS))
    (: :)
    (S
      (SBAR (IN Since)
        (S
          (NP (PRP you))
          (VP (VBD used)
            (NP (JJ double) (NNS quotes))
            (S
              (VP (TO to)
                (VP (VB describe)
                  (NP
                    (NP (NNP Einstein) (POS 's))
                    (NN response))))))))
      (, ,)
      (NP (PRP it))
      (VP (MD would)
        (VP (VB be)
          (ADJP (JJ helpful)
            (S
              (VP (TO to)
                (VP
                  (ADVP (RB correctly))
                  (VB quote)
                  (NP (PRP him)))))))))
    (. .)))

root(ROOT-0, PS-1)
mark(used-5, Since-3)
nsubj(used-5, you-4)
advcl(helpful-17, used-5)
amod(quotes-7, double-6)
dobj(used-5, quotes-7)
aux(describe-9, to-8)
xcomp(used-5, describe-9)
poss(response-12, Einstein-10)
dobj(describe-9, response-12)
nsubj(helpful-17, it-14)
aux(helpful-17, would-15)
cop(helpful-17, be-16)
dep(PS-1, helpful-17)
aux(quote-20, to-18)
advmod(quote-20, correctly-19)
xcomp(helpful-17, quote-20)
dobj(quote-20, him-21)

