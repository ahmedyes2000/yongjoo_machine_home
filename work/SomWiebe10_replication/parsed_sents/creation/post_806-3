(ROOT
  (S
    (NP (DT That))
    (VP (VBZ 's)
      (ADVP (RB basically))
      (SBAR
        (WHNP (WP what))
        (S
          (NP (PRP you))
          (VP (VBD said) (, ,)
            (ADVP (RB just)
              (PP (IN with)
                (NP (DT the) (NNP Bible))))))))
    (. .)))

nsubj('s-2, That-1)
root(ROOT-0, 's-2)
advmod('s-2, basically-3)
dobj(said-6, what-4)
nsubj(said-6, you-5)
ccomp('s-2, said-6)
advmod(said-6, just-8)
det(Bible-11, the-10)
prep_with(just-8, Bible-11)

