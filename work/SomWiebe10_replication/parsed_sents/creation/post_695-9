(ROOT
  (S
    (S
      (NP (PRP They))
      (VP (VBP continue)
        (S
          (VP (TO to)
            (VP (VB create))))))
    (CC and)
    (S
      (NP (PRP we) (DT each))
      (ADVP (RB too))
      (VP (VBP are)
        (NP
          (NP (NNS instruments))
          (PP (IN of)
            (NP (DT that) (VBG continuing) (NN creation))))))
    (. .)))

nsubj(continue-2, They-1)
xsubj(create-4, They-1)
root(ROOT-0, continue-2)
aux(create-4, to-3)
xcomp(continue-2, create-4)
nsubj(instruments-10, we-6)
det(we-6, each-7)
advmod(instruments-10, too-8)
cop(instruments-10, are-9)
conj_and(continue-2, instruments-10)
det(creation-14, that-12)
amod(creation-14, continuing-13)
prep_of(instruments-10, creation-14)

