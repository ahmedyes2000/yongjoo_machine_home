(ROOT
  (S
    (NP (DT The) (NNS parents))
    (VP (VBP pass)
      (PP (IN on)
        (NP
          (NP (PRP$ their) (NNS traits))
          (PRN (-LRB- -LRB-)
            (NP
              (NP
                (NP (NNP Thick))
                (NP (NN shell)))
              (, ,)
              (NP (JJR Bigger) (NNS claws))
              (, ,) (FW etc))
            (-RRB- -RRB-))))
      (PP (TO to)
        (NP (PRP$ their) (NN offspring))))
    (. .)))

det(parents-2, The-1)
nsubj(pass-3, parents-2)
root(ROOT-0, pass-3)
poss(traits-6, their-5)
prep_on(pass-3, traits-6)
dep(traits-6, Thick-8)
dep(Thick-8, shell-9)
amod(claws-12, Bigger-11)
