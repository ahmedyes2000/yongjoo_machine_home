(ROOT
  (S
    (NP (NNP God))
    (VP (VBZ is)
      (ADJP (JJ separate))
      (S
        (VP (VBG inhabiting)
          (NP (DT a) (DT another) (NN sphere)))))
    (. .)))

nsubj(separate-3, God-1)
cop(separate-3, is-2)
root(ROOT-0, separate-3)
xcomp(separate-3, inhabiting-4)
predet(sphere-7, a-5)
det(sphere-7, another-6)
dobj(inhabiting-4, sphere-7)

