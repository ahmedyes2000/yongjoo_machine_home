(ROOT
  (S
    (ADVP (RB Fortunately))
    (, ,)
    (NP (DT no) (NN one))
    (VP (VBZ has)
      (S
        (VP (TO to)
          (VP (VB be)
            (NP (DT that) (JJ narrow) (JJ minded)
              (CC or)
              (JJ irrational))))))
    (. .)))

advmod(has-5, Fortunately-1)
det(one-4, no-3)
nsubj(has-5, one-4)
xsubj(minded-10, one-4)
root(ROOT-0, has-5)
aux(minded-10, to-6)
cop(minded-10, be-7)
dep(minded-10, that-8)
amod(minded-10, narrow-9)
xcomp(has-5, minded-10)
xcomp(has-5, irrational-12)
conj_or(minded-10, irrational-12)

