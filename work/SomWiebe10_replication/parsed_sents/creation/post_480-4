(ROOT
  (FRAG
    (NP (NNP TJ))
    (: :)
    (S
      (NP (PRP I))
      (VP (VBP 'm) (RB not)
        (ADJP (JJ sure))
        (SBAR
          (WHADVP (WRB how))
          (S
            (NP (PRP I))
            (VP (VBP am)
              (ADJP (JJ ill-informed)))))))
    (. .)))

root(ROOT-0, TJ-1)
nsubj(sure-6, I-3)
cop(sure-6, 'm-4)
neg(sure-6, not-5)
dep(TJ-1, sure-6)
advmod(ill-informed-10, how-7)
nsubj(ill-informed-10, I-8)
cop(ill-informed-10, am-9)
ccomp(sure-6, ill-informed-10)

