(ROOT
  (NP (: --)
    (NP
      (NP (NNP IDNET))
      (: --)
      (S
        (NP
          (NP (DT The) (NN writer))
          (PP (IN of)
            (NP (DT this) (NN piece))))
        (VP (VBZ is)
          (VP (VBG working)
            (PP (IN for)
              (NP (DT a) (NN salary)))))))
    (. .)))

root(ROOT-0, IDNET-2)
det(writer-5, The-4)
nsubj(working-10, writer-5)
det(piece-8, this-7)
prep_of(writer-5, piece-8)
aux(working-10, is-9)
dep(IDNET-2, working-10)
det(salary-13, a-12)
prep_for(working-10, salary-13)

