  (S
    (S
      (NP (NNP Human) (NNS beings))
      (VP (VBD added)
        (NP (NNS names))
        (PP (TO to)
          (NP
            (NP (DT the) (NNS days))
            (PP (IN of)
              (NP (NN week)))))))
    (, ,)
    (NP (PRP they))
    (VP
      (VP (VBD were)
        (ADVP (RB originally)))
      (CC and)
      (VP
        (ADVP (RB simply))
        (NP (NNS numbers))))
    (. .)))

