(ROOT
  (NP
    (NP (DT A) (NN word))
    (PP (TO to)
      (NP
        (NP (DT any))
        (PP (IN out)
          (NP (RB there)))
        (SBAR
          (WHNP (WP who))
          (S
            (VP (MD may)
              (VP (VB fit)
                (NP (DT that) (NN category))))))))
    (. .)))

det(word-2, A-1)
root(ROOT-0, word-2)
prep_to(word-2, any-4)
nsubj(fit-9, any-4)
prep(any-4, out-5)
pobj(out-5, there-6)
aux(fit-9, may-8)
