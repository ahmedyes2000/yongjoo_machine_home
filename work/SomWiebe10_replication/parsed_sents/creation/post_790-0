(ROOT
  (S
    (NP
      (NP (DT The) (JJ first) (NN problem))
      (PP (IN with)
        (NP (DT the) (NN debate) (NN question))))
    (VP (VBZ is)
      (SBAR (IN that)
        (S
          (NP (CC neither)
            (NP (NN time))
            (CC nor)
            (NP (NN space)))
          (VP (VBZ exists)
            (ADVP (FW per) (FW se))))))
    (. .)))

det(problem-3, The-1)
amod(problem-3, first-2)
nsubj(is-8, problem-3)
det(question-7, the-5)
nn(question-7, debate-6)
prep_with(problem-3, question-7)
root(ROOT-0, is-8)
complm(exists-14, that-9)
preconj(time-11, neither-10)
nsubj(exists-14, time-11)
conj_nor(time-11, space-13)
nsubj(exists-14, space-13)
ccomp(is-8, exists-14)
nn(se-16, per-15)
advmod(exists-14, se-16)

