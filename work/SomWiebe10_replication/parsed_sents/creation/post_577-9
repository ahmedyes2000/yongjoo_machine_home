(ROOT
  (FRAG ('' '')
    (NP (NNP Synthesis))
    (PP (IN of)
      (NP
        (NP (JJ activated) (NN pyrimidine) (NNS ribonucleotides))
        (PP (IN in)
          (NP
            (ADJP (RB prebiotically) (JJ plausible))
            (NNS conditions)))
        ('' '')))
    (. .)))

root(ROOT-0, Synthesis-2)
dep(Synthesis-2, of-3)
amod(ribonucleotides-6, activated-4)
nn(ribonucleotides-6, pyrimidine-5)
pobj(of-3, ribonucleotides-6)
