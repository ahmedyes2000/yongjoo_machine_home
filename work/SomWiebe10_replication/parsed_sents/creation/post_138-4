(ROOT
  (S
    (NP (PRP I))
    (VP (VBD heard)
      (NP
        (NP (DT a) (NN statistic))
        (SBAR
          (WHNP (WDT that))
          (S
            (VP (VBD said)
              (SBAR
                (S
                  (NP
                    (NP (DT the) (NN possibility))
                    (PP (IN of)
                      (NP (DT The) (NNP Big) (NNP Bang) (NN happening))))
                  (VP (VBZ is)
                    (PP (IN like)
                      (NP
                        (NP (DT a) (NN tornado))
                        (VP
                          (VP (VBG going)
                            (PP (IN through)
                              (NP (DT a) (NN junkyard))))
                          (CC and)
                          (VP (VBG assembling)
                            (NP (DT A) (NN WORKING))
                            (S
                              (VP (VBG BOEING)
                                (NP (CD 747))))))))))))))))
    (. .)))

nsubj(heard-2, I-1)
root(ROOT-0, heard-2)
det(statistic-4, a-3)
dobj(heard-2, statistic-4)
nsubj(said-6, statistic-4)
rcmod(statistic-4, said-6)
det(possibility-8, the-7)
nsubj(is-14, possibility-8)
det(happening-13, The-10)
nn(happening-13, Big-11)
nn(happening-13, Bang-12)
prep_of(possibility-8, happening-13)
ccomp(said-6, is-14)
det(tornado-17, a-16)
prep_like(is-14, tornado-17)
dep(tornado-17, going-18)
det(junkyard-21, a-20)
prep_through(going-18, junkyard-21)
dep(tornado-17, assembling-23)
conj_and(going-18, assembling-23)
det(WORKING-25, A-24)
dobj(assembling-23, WORKING-25)
xcomp(assembling-23, BOEING-26)
dobj(BOEING-26, 747-27)

(ROOT
  (S
    (VP (VB Look)
      (PRT (RP around))
      (NP (PRP you)))
    (. .)))

