(ROOT
  (S ('' '')
    (PP (IN Any)
      (NP
        (NP (NN observer))
        (PP (IN in)
          (NP (DT the) (VBN known) (NN universe)))))
    (VP (MD would)
      (VP (VB have)
        (NP
          (NP (JJ similar) (NNS estimates))
          (PP (IN for)
            (NP
              (NP (DT the) (NN age))
              (PP (IN of)
                (NP (PRP$ our) (NN planet))))))))
    (. .)))

prep_any(have-9, observer-3)
det(universe-7, the-5)
amod(universe-7, known-6)
prep_in(observer-3, universe-7)
aux(have-9, would-8)
root(ROOT-0, have-9)
amod(estimates-11, similar-10)
dobj(have-9, estimates-11)
det(age-14, the-13)
prep_for(estimates-11, age-14)
poss(planet-17, our-16)
prep_of(age-14, planet-17)

