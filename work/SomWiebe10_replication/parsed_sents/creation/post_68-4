(ROOT
  (FRAG
    (NP (NNP PvM))
    (: :)
    (S
      (NP (RB No) (DT the) (JJ scientific) (NN position))
      (VP (VBZ is)
        (S (`` ``)
          (NP (PRP we))
          (VP (VBP do) (RB n't)
            (VP (VB know)))
          ('' ''))))
    (. .)))

root(ROOT-0, PvM-1)
advmod(position-6, No-3)
det(position-6, the-4)
amod(position-6, scientific-5)
nsubj(is-7, position-6)
dep(PvM-1, is-7)
nsubj(know-12, we-9)
aux(know-12, do-10)
neg(know-12, n't-11)
ccomp(is-7, know-12)

