(ROOT
  (FRAG ('' '')
    (NP (NNP But) (NNP what))
    (PP (IN about)
      (NP
        (NP (PRP$ your) (NN opinion))
        (PP (IN on)
          (NP (NNP ID)))))
    (. ?)))

nn(what-3, But-2)
root(ROOT-0, what-3)
dep(what-3, about-4)
poss(opinion-6, your-5)
pobj(about-4, opinion-6)
prep_on(opinion-6, ID-8)

