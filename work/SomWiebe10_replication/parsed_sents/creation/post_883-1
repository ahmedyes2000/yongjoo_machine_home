(ROOT
  (S
    (NP
      (NP (DT Both))
      (PP (IN of)
        (NP (DT these))))
    (VP (VBP have)
      (NP
        (ADJP (JJ continuous) (VBN spoken))
        (NNS traditions)))
    (. .)))

nsubj(have-4, Both-1)
prep_of(Both-1, these-3)
root(ROOT-0, have-4)
dep(spoken-6, continuous-5)
amod(traditions-7, spoken-6)
dobj(have-4, traditions-7)

