(ROOT
  (S
    (NP (DT This))
    (VP (VBZ is)
      (NP
        (NP (DT the) (JJ strict) (NN process))
        (PP (IN of)
          (NP (DT the) (JJ scientific) (NN method)))))
    (. .)))

nsubj(process-5, This-1)
cop(process-5, is-2)
det(process-5, the-3)
amod(process-5, strict-4)
root(ROOT-0, process-5)
det(method-9, the-7)
amod(method-9, scientific-8)
prep_of(process-5, method-9)

