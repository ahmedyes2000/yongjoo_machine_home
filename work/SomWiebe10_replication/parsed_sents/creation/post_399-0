(ROOT
  (S
    (PP (IN Hello)
      (NP (NNP Richard)))
    (, ,)
    (NP (PRP I))
    (VP (VBP admire)
      (NP (PRP$ your) (NN wealth))
      (PP (IN in)
        (NP (NN cash) (NNS funds))))
    (. .)))

prep_hello(admire-5, Richard-2)
nsubj(admire-5, I-4)
root(ROOT-0, admire-5)
poss(wealth-7, your-6)
dobj(admire-5, wealth-7)
nn(funds-10, cash-9)
prep_in(admire-5, funds-10)

