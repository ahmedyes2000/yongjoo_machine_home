  (S
    (NP (PRP It))
    (VP (VBZ is) (RB not)
      (ADJP (`` ``) (JJ appropriate) ('' '')
        (S
          (VP (TO to)
            (VP (VB teach)
              (NP (NN creationism))
              (PP (IN as)
                (NP (DT a) (NNS means)))
              (PP (IN for)
                (S
                  (VP (VBG upholding)
                    (NP (DT the) (NN bible))))))))))
    (. .)))

