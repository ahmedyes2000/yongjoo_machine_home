(ROOT
  (S
    (PP (IN Like)
      (NP (JJ modern) (JJ evolutionary) (NN theory)))
    (NP (EX there))
    (VP (VBZ is)
      (ADVP (RBR more) (IN than))
      (NP
        (NP (RB just) (CD one) (NN side))
        (PP (TO to)
          (NP (DT the) (NN debate)))))
    (. .)))

amod(theory-4, modern-2)
amod(theory-4, evolutionary-3)
prep_like(is-6, theory-4)
expl(is-6, there-5)
root(ROOT-0, is-6)
advmod(than-8, more-7)
advmod(is-6, than-8)
advmod(side-11, just-9)
num(side-11, one-10)
nsubj(is-6, side-11)
det(debate-14, the-13)
prep_to(side-11, debate-14)

