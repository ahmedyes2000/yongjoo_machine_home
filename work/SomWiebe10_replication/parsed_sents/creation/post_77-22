(ROOT
  (S
    (S
      (NP (NNP Creationism))
      (VP (VBZ is)
        (ADVP (RB usually))
        (VP (VBN defined)
          (PP (IN by)
            (NP (DT these) (NNS tenets))))))
    (: :)
    (S
      (NP (DT The) (NN universe) (, ,) (NN energy)
        (CC and)
        (NN life))
      (VP (VBD were)
        (VP (VBN created)
          (PP (IN from)
            (NP (NN nothing))))))
    (. .)))

nsubjpass(defined-4, Creationism-1)
auxpass(defined-4, is-2)
advmod(defined-4, usually-3)
root(ROOT-0, defined-4)
det(tenets-7, these-6)
agent(defined-4, tenets-7)
det(universe-10, The-9)
nsubjpass(created-16, universe-10)
conj_and(universe-10, energy-12)
