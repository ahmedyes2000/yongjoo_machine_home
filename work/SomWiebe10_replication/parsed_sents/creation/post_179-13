(ROOT
  (S
    (PP (IN As)
      (PP (TO to)
        (NP (DT the) (`` ``) (NNP Cambrian) (NNP Explosion) ('' ''))))
    (: ...)
    (NP (PRP it))
    (VP (VBD took)
      (NP (NN place))
      (PP (IN over)
        (NP
          (NP (DT the) (NN span))
          (PP (IN of)
            (NP
              (NP
                (NP (NNP ~))
                (NP
                  (QP (CD 10) (CD million))
                  (NNS years)))
              (, ,) (RB hardly)
              (NP (DT a) (JJ sudden) (NN change)))))))
    (. .)))

prepc_as_to(took-10, to-2)
det(Explosion-6, the-3)
nn(Explosion-6, Cambrian-5)
pobj(took-10, Explosion-6)
nsubj(took-10, it-9)
root(ROOT-0, took-10)
dobj(took-10, place-11)
det(span-14, the-13)
prep_over(took-10, span-14)
prep_of(span-14, ~-16)
number(million-18, 10-17)
num(years-19, million-18)
dep(~-16, years-19)
advmod(~-16, hardly-21)
det(change-24, a-22)
amod(change-24, sudden-23)
dep(~-16, change-24)

