  (S
    (S
      (NP (NNP PvM))
      (VP (VBD wrote)))
    (: :)
    (S
      (ADVP (RB However))
      (NP (DT this))
      (VP (VBZ is)
        (ADJP (JJ false)
          (CC and)
          (JJ misleading))))
    (. .)))

nsubj(wrote-2, PvM-1)
root(ROOT-0, wrote-2)
advmod(false-7, However-4)
nsubj(false-7, this-5)
nsubj(misleading-9, this-5)
