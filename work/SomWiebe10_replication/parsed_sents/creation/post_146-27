(ROOT
  (S
    (NP (PRP we))
    (VP (VBP need)
      (S
        (VP (TO to)
          (VP (VB give)
            (NP (DT this) (NN creation))
            (NP (DT some) (NN purpose))))))
    (. .) (. .)))

nsubj(need-2, we-1)
xsubj(give-4, we-1)
root(ROOT-0, need-2)
aux(give-4, to-3)
xcomp(need-2, give-4)
det(creation-6, this-5)
iobj(give-4, creation-6)
det(purpose-8, some-7)
dobj(give-4, purpose-8)

