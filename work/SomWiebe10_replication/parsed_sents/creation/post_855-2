(ROOT
  (S
    (S
      (NP (DT The) (NN universe))
      (VP (VBZ is)
        (ADJP
          (NP
            (QP (RB around) (CD 13.7) (CD billion))
            (NNS years))
          (JJ old))))
    (, ,)
    (S
      (NP (PRP$ our) (NN planet))
      (VP (VBZ is)
        (ADJP
          (NP
            (QP (RB about) (CD 4.7) (CD billion))
            (NNS years))
          (JJ old))))
    (, ,)
    (S
      (NP (PRP$ our) (JJ current) (JJ human) (NNS species))
      (VP (VBZ is)
        (ADJP
          (NP
            (QP (RB somewhere) (IN around) (CD 150,000))
            (NNS years))
          (JJ old))))
    (. .)))

