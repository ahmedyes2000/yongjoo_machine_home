(ROOT
  (S
    (ADVP (RB However))
    (, ,)
    (ADVP (RB once) (RB again))
    (, ,)
    (NP (NNP Behe))
    (VP (VBZ presents)
      (NP
        (NP (DT an)
          (ADJP (RB overly) (JJ simplistic))
          (NN strawman))
        (PP (IN of)
          (NP (NN evolution)))))
    (. .)))

advmod(presents-7, However-1)
advmod(again-4, once-3)
advmod(presents-7, again-4)
nsubj(presents-7, Behe-6)
root(ROOT-0, presents-7)
det(strawman-11, an-8)
advmod(simplistic-10, overly-9)
amod(strawman-11, simplistic-10)
dobj(presents-7, strawman-11)
prep_of(strawman-11, evolution-13)

