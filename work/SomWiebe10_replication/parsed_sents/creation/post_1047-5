(ROOT
  (S ('' '')
    (NP (NNP Then) (NNP why))
    (VP (VBP have)
      (VP (VBN fossilized)
        (NP
          (NP (NNS whales))
          (VP (VBN been)
            (VP (VBN found)
              (S
                (VP (VBG standing)
                  (ADVP (RB vertically))
                  (PP (IN on)
                    (NP (PRP$ their) (NNS tails))))))))))
    (. ?)))

nn(why-3, Then-2)
nsubj(fossilized-5, why-3)
aux(fossilized-5, have-4)
root(ROOT-0, fossilized-5)
dobj(fossilized-5, whales-6)
auxpass(found-8, been-7)
partmod(whales-6, found-8)
xcomp(found-8, standing-9)
advmod(standing-9, vertically-10)
poss(tails-13, their-12)
prep_on(standing-9, tails-13)

