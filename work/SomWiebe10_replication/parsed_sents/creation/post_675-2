  (S
    (PP (IN As)
      (PP (IN for)
        (NP (NNP Darwin))))
    (, ,)
    (S
      (VP (VB have)
        (S
          (NP (PRP you))
          (VP
            (VP (VB read)
              (NP (PRP$ his) (NNS books)))
            (CC or)
            (VP (VBP are)
              (ADVP (RB just))
              (NP (NN brain)))))))
    (VP (VBD washed)
      (PP (IN into)
        (S
          (VP (VBG thinking)
            (SBAR (IN that)
              (S
                (NP (NN science))
                (VP (VBZ is)
                  (ADVP (RB always))
                  (ADJP (JJ right)))))))))
    (. .)))

