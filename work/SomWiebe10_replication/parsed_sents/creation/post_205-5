(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP it))
        (VP (VBZ does))))
    (, ,)
    (ADVP (RB then))
    (NP (PRP it))
    (VP (MD can)
      (ADVP (RB never))
      (VP (VB be)
        (NP (NN science))))
    (. .)))

mark(does-3, If-1)
nsubj(does-3, it-2)
advcl(science-10, does-3)
advmod(science-10, then-5)
nsubj(science-10, it-6)
aux(science-10, can-7)
neg(science-10, never-8)
cop(science-10, be-9)
root(ROOT-0, science-10)

