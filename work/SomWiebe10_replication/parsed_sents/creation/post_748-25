  (S
    (NP
      (NP (DT Any) (NN sentence))
      (PP (IN in)
        (NP (DT the) (NN bible))))
    (VP (MD can)
      (VP (VB be)
        (VP (VBN misinterpreted))))
    (. .) (. .)))

