(ROOT
  (S
    (S
      (NP (PRP You))
      (ADVP (RB probably))
      (VP (VBP wont)
        (VP (VB believe)
          (NP (PRP me)))))
    (CC but)
    (S
      (NP (NN nothing))
      (VP (MD could)
        (VP (VB be)
          (ADVP (RBR further))
          (PP (IN from)
            (NP (DT the) (NN truth))))))
    (. .)))

nsubj(wont-3, You-1)
advmod(wont-3, probably-2)
root(ROOT-0, wont-3)
xcomp(wont-3, believe-4)
dobj(believe-4, me-5)
nsubj(be-9, nothing-7)
aux(be-9, could-8)
conj_but(wont-3, be-9)
advmod(be-9, further-10)
det(truth-13, the-12)
prep_from(be-9, truth-13)

