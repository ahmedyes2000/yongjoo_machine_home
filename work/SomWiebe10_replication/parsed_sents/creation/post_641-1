(ROOT
  (S
    (S
      (VP
        (PRN (: ;)
          (NP (NNP D))
          (: --))
        (PP (RB Not)
          (ADVP (RB really)))))
    (, ,)
    (NP (PRP he))
    (VP
      (VP (VBZ tries)
        (ADJP (JJ hard)))
      (CC but)
      (VP (VBZ fails)))
    (. .)))

ccomp(tries-8, D-2)
prep(D-2, Not-4)
dep(Not-4, really-5)
nsubj(tries-8, he-7)
nsubj(fails-11, he-7)
root(ROOT-0, tries-8)
acomp(tries-8, hard-9)
conj_but(tries-8, fails-11)

