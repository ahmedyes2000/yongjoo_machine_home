(ROOT
  (S
    (PP (IN For)
      (NP
        (NP (NN instance))
        (PP (IN in)
          (NP (NN criminology)))))
    (NP (PRP we))
    (VP (VBP look)
      (PP (IN at)
        (NP (NNS means) (, ,) (NNS motives)
          (CC and)
          (NNS opportunities)))
      (, ,)
      (PP (VBN combined)
        (PP (IN with)
          (NP
            (NP (NNS eye-witnesses))
            (, ,)
            (NP (NNS alibis))
            (CC and)
            (NP (JJ physical) (NN evidence)))))
      (S
        (VP (TO to)
          (VP (VB generate)
            (NP
              (NP (DT a) (JJ plausible) (NNS hypothesis))
              (SBAR
                (WHNP (WDT which))
                (S
                  (VP (MD can)
                    (VP (VB establish) (`` ``)
                      (ADVP (IN beyond)
                        (NP (JJ reasonable) (NN doubt)))
                      ('' '')
                      (NP
                        (NP (DT the) (NN guilt))
                        (CC or)
                        (NP (NN innocence))))))))))))
    (. .)))

prep_for(look-6, instance-2)
prep_in(instance-2, criminology-4)
nsubj(look-6, we-5)
