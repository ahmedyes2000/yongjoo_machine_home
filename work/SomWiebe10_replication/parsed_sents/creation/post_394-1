    (NP
      (NP (DT The) (NN research))
      (VP (VBN discussed)
        (PP (IN in)
          (NP (DT this) (NN book)))))
    (VP (VBZ leads)
      (PP (TO to)
        (NP (DT the) (NN conclusion)))
      (SBAR
        (SBAR (IN that)
          (S
            (NP (NNS mutations))
            (VP (VBP are) (RB not)
              (NP (DT all) (NNS accidents)))))
        (CC and)
        (SBAR (IN that)
          (S
            (NP (NNS mutations))
            (VP (VBP are) (RB not)
              (ADVP (RB always))
              (ADJP (JJ random)))))))
    (. .)))

