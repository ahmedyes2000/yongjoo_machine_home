  (S
    (NP
      (NP (DT The)
        (ADJP (RB only) (JJ unscientific))
        (NN feature))
      (PP (IN of)
        (NP (JJ modern) (JJ intelligent) (NN design))))
    (VP (VBZ is)
      (NP
        (NP (DT the) (NN unwillingness))
        (PP (IN of)
          (NP (JJ many)))
        (S
          (VP (TO to)
            (VP (VB abandon)
              (NP (PRP it))
              (PP (IN in)
                (NP
                  (NP (NN light))
                  (PP (IN of)
                    (NP (DT these) (JJ new) (NNS observations))))))))))
    (. .)))

