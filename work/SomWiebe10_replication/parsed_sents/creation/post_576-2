(ROOT
  (S ('' '')
    (NP (JJ Or) (NN convection))
    (VP
      (VP (VBZ currents))
      (, ,)
      (CC or)
      (VP
        (PP (IN towards)
          (NP
            (NP (DT the) (NN earth) (POS 's))
            (NN core)))
        (, ,)
        (ADVP (CC or))))
    (. ...)))

amod(convection-3, Or-2)
nsubj(currents-4, convection-3)
root(ROOT-0, currents-4)
conj_or(currents-4, towards-7)
det(earth-9, the-8)
poss(core-11, earth-9)
pobj(towards-7, core-11)
advmod(towards-7, or-13)

