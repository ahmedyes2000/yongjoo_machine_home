(ROOT
  (S
    (NP
      (NP
        (NP (CD One) (NN basin))
        (PP (IN of)
          (NP (NN water))))
      (CC and)
      (NP
        (NP (CD one) (NN spot))
        (PP (IN of)
          (NP (JJ dry) (NN land)))))
    (VP (VBZ sounds)
      (NP
        (NP (DT a) (NN lot))
        (PP (IN like)
          (NP (NNP Pangea))))
      (PP (TO to)
        (NP (PRP me))))
    (. .)))

num(basin-2, One-1)
nsubj(sounds-11, basin-2)
prep_of(basin-2, water-4)
num(spot-7, one-6)
conj_and(basin-2, spot-7)
nsubj(sounds-11, spot-7)
amod(land-10, dry-9)
prep_of(spot-7, land-10)
root(ROOT-0, sounds-11)
det(lot-13, a-12)
dobj(sounds-11, lot-13)
prep_like(lot-13, Pangea-15)
prep_to(sounds-11, me-17)

(ROOT
  (S
    (NP (NNP God))
    (ADVP (RB also))
    (VP (VBZ creates)
      (NP (NNS plants)))
    (. .)))

nsubj(creates-3, God-1)
advmod(creates-3, also-2)
root(ROOT-0, creates-3)
dobj(creates-3, plants-4)

