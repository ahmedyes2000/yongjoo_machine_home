(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (NN EVERYONE))
        (VP (VBD carried)
          (NP (NNS guns)))))
    (, ,)
    (NP (NNS people))
    (VP (MD would)
      (VP (VB shoot)
        (NP (DT each) (JJ other))
        (NP (PDT all) (DT the) (NN time))))))

mark(carried-3, If-1)
nsubj(carried-3, EVERYONE-2)
advcl(shoot-8, carried-3)
dobj(carried-3, guns-4)
nsubj(shoot-8, people-6)
aux(shoot-8, would-7)
root(ROOT-0, shoot-8)
det(other-10, each-9)
iobj(shoot-8, other-10)
predet(time-13, all-11)
det(time-13, the-12)
dobj(shoot-8, time-13)

