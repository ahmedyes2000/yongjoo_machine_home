(ROOT
  (S
    (NP (DT That))
    (VP (VBZ has)
      (ADVP (RB always))
      (VP (VBN been)
        (NP (PRP$ my) (NNS thoughts) (NNS states) (NN side))))
    (. .)))

nsubj(side-8, That-1)
aux(side-8, has-2)
advmod(side-8, always-3)
cop(side-8, been-4)
poss(side-8, my-5)
nn(side-8, thoughts-6)
nn(side-8, states-7)
root(ROOT-0, side-8)

