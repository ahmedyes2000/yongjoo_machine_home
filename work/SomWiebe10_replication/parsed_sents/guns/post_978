(ROOT
  (S
    (NP (PRP I))
    (VP (VBP agree)
      (SBAR (IN since)
        (S
          (NP
            (NP (PRP I))
            (ADJP (RB personally) (DT the)))
          (VP (VBP think)
            (NP
              (NP
                (ADVP (RB generally))
                (NP (DT the) (JJR less) (NNS guns))
                (ADJP (JJ available)))
              (, ,)
              (NP (DT the)
                (ADJP (RBR less) (RB gun) (VBN related))
                (NN crime)))))))
    (. .)))

nsubj(agree-2, I-1)
root(ROOT-0, agree-2)
mark(think-7, since-3)
nsubj(think-7, I-4)
advmod(the-6, personally-5)
amod(I-4, the-6)
advcl(agree-2, think-7)
advmod(guns-11, generally-8)
det(guns-11, the-9)
amod(guns-11, less-10)
dobj(think-7, guns-11)
amod(guns-11, available-12)
det(crime-18, the-14)
advmod(related-17, less-15)
advmod(related-17, gun-16)
amod(crime-18, related-17)
appos(guns-11, crime-18)

