(ROOT
  (INTJ (UH Yes)
    (PP (IN for)
      (S
        (VP (VBG defending)
          (SBAR
            (S
              (NP (PRP them))
              (VP (VBZ selves)
                (S (RB not)
                  (VP (VBG committing)
                    (NP (NNS crimes))))))))))
    (. .)))

root(ROOT-0, Yes-1)
dep(Yes-1, for-2)
pcomp(for-2, defending-3)
nsubj(selves-5, them-4)
ccomp(defending-3, selves-5)
neg(committing-7, not-6)
xcomp(selves-5, committing-7)
dobj(committing-7, crimes-8)

