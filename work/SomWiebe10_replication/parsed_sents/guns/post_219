(ROOT
  (S
    (NP (PRP I))
    (VP (VBD served)
      (NP (CD 21) (JJ +) (NNS years)))
    (. .)))

nsubj(served-2, I-1)
root(ROOT-0, served-2)
num(years-5, 21-3)
amod(years-5, +-4)
tmod(served-2, years-5)

(ROOT
  (S
    (NP (DT The) (NN military))
    (VP (MD will)
      (VP (VB follow)
        (NP
          (NP (NNS orders) (NN form) (NNS others))
          (SBAR
            (WHNP (WP who))
            (S
              (VP (VBP are)
                (VP (VBG following)
                  (NP (NNS orders)))))))))
    (. .)))

det(military-2, The-1)
nsubj(follow-4, military-2)
aux(follow-4, will-3)
root(ROOT-0, follow-4)
nn(others-7, orders-5)
nn(others-7, form-6)
dobj(follow-4, others-7)
nsubj(following-10, others-7)
aux(following-10, are-9)
rcmod(others-7, following-10)
dobj(following-10, orders-11)

(ROOT
  (S
    (PP (RB Even)
      (PP (IN from)
        (NP
          (NP (DT a) (NNP CIC))
          (SBAR
            (WHNP (WP who))
            (S
              (VP (VBP o)))))))
    (: ;)
    (VP (VBZ pposes)
      (NP (DT the) (NN constitution)))
    (. ...)))

advmod(pposes-8, Even-1)
advcl(pposes-8, from-2)
det(CIC-4, a-3)
nsubj(o-6, CIC-4)
prep_from(pposes-8, CIC-4)
rcmod(CIC-4, o-6)
root(ROOT-0, pposes-8)
det(constitution-10, the-9)
dobj(pposes-8, constitution-10)

(ROOT
  (S
    (ADVP (RB So))
    (, ,)
    (VP (VB keep)
      (S
        (NP (PRP$ your) (NN powder))
        (ADJP (JJ dry))))
    (. .)))

advmod(keep-3, So-1)
root(ROOT-0, keep-3)
poss(powder-5, your-4)
nsubj(dry-6, powder-5)
xcomp(keep-3, dry-6)

