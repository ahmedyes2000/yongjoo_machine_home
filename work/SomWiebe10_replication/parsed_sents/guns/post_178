(ROOT
  (S
    (NP (JJ Very) (JJ few) (NNS countries))
    (VP (VBP offer)
      (NP (JJ individual) (NN gun) (NNS rights))
      (PP (TO to)
        (NP (NNS citizens))))))

amod(countries-3, Very-1)
amod(countries-3, few-2)
nsubj(offer-4, countries-3)
root(ROOT-0, offer-4)
amod(rights-7, individual-5)
nn(rights-7, gun-6)
dobj(offer-4, rights-7)
prep_to(offer-4, citizens-9)

