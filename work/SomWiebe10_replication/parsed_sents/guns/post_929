(ROOT
  (S
    (NP (NNS Guns))
    (VP (VBP do) (RB n't)
      (VP (VB kill)
        (NP (NNS people))))
    (. .)))

nsubj(kill-4, Guns-1)
aux(kill-4, do-2)
neg(kill-4, n't-3)
root(ROOT-0, kill-4)
dobj(kill-4, people-5)

