(ROOT
  (FRAG
    (ADVP (RB Not)
      (S
        (VP
          (ADVP (RB exactly))
          (VBG disagreeing)
          (PP (IN with)
            (NP (PRP$ your) (NN argument))))))
    (, ,)
    (CC but)
    (S
      (NP (NNS guns))
      (VP (MD can)
        (ADVP (RB also))
        (VP (VB be)
          (VP (VBN used)
            (PP (IN for)
              (NP (JJ personal) (NN safety)))))))
    (. .)))

root(ROOT-0, Not-1)
advmod(disagreeing-3, exactly-2)
dep(Not-1, disagreeing-3)
poss(argument-6, your-5)
prep_with(disagreeing-3, argument-6)
dep(Not-1, but-8)
nsubjpass(used-13, guns-9)
aux(used-13, can-10)
advmod(used-13, also-11)
auxpass(used-13, be-12)
dep(Not-1, used-13)
amod(safety-16, personal-15)
prep_for(used-13, safety-16)

(ROOT
  (NP
    (NP (RB Not) (RB just))
    (PP (IN for)
      (S
        (VP (VBG killing) (`` ``)
          (NP (JJ little) (NNS deer))
          ('' ''))))
    (. .)))

neg(just-2, Not-1)
root(ROOT-0, just-2)
prepc_for(just-2, killing-4)
amod(deer-7, little-6)
dobj(killing-4, deer-7)

