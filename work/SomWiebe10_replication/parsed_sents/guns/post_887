(ROOT
  (S
    (SBAR (IN if)
      (S
        (NP (DT a) (NN robber))
        (VP (VBD came)
          (PP (IN in)
            (NP (PRP$ your) (NN house))))))
    (NP (PRP you))
    (VP (MD would)
      (VP (VB want)
        (NP (DT a) (NN gun))))
    (. .)))

mark(came-4, if-1)
det(robber-3, a-2)
nsubj(came-4, robber-3)
advcl(want-10, came-4)
poss(house-7, your-6)
prep_in(came-4, house-7)
nsubj(want-10, you-8)
aux(want-10, would-9)
root(ROOT-0, want-10)
det(gun-12, a-11)
dobj(want-10, gun-12)

