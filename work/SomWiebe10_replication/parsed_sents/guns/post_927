(ROOT
  (S
    (NP (NNS Guns))
    (VP (VBP kill)
      (ADJP (JJ peope))
      (: ;)
      (SBAR (RB just) (IN like)
        (S
          (NP (NNS spoons))
          (VP (VBD made)
            (S
              (NP (NNP Rosie) (NNP O'Donnel))
              (ADJP (JJ fat)))))))
    (. .)))

nsubj(kill-2, Guns-1)
root(ROOT-0, kill-2)
acomp(kill-2, peope-3)
advmod(made-8, just-5)
dep(made-8, like-6)
nsubj(made-8, spoons-7)
ccomp(kill-2, made-8)
parataxis(kill-2, made-8)
nn(O'Donnel-10, Rosie-9)
nsubj(fat-11, O'Donnel-10)
xcomp(made-8, fat-11)

