(ROOT
  (NP
    (NP (NNP HUNTING)
      (CC AND)
      (NNP SELF-DEFENSE))
    (, ,)
    (NP (NN time)
      (S
        (VP (TO to)
          (VP (VB look)
            (PP (IN past)
              (NP (PRP$ your) (JJ little) (JJ inner) (NN city) (NN cocoon)))))))
    (. .)))

root(ROOT-0, HUNTING-1)
conj_and(HUNTING-1, SELF-DEFENSE-3)
appos(HUNTING-1, time-5)
aux(look-7, to-6)
infmod(time-5, look-7)
poss(cocoon-13, your-9)
amod(cocoon-13, little-10)
amod(cocoon-13, inner-11)
nn(cocoon-13, city-12)
prep_past(look-7, cocoon-13)

(ROOT
  (S
    (NP (EX There))
    (VP (VBZ is)
      (NP
        (NP (DT a) (JJ big) (NN place))
        (VP (VBN called)
          (NP
            (NP (DT the) (NN rest))
            (PP (IN of)
              (NP (NNP North) (NNP America)))))))
    (. !)))

expl(is-2, There-1)
root(ROOT-0, is-2)
det(place-5, a-3)
amod(place-5, big-4)
nsubj(is-2, place-5)
partmod(place-5, called-6)
det(rest-8, the-7)
dobj(called-6, rest-8)
nn(America-11, North-10)
prep_of(rest-8, America-11)

