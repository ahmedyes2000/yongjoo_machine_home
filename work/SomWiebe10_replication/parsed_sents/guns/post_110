(ROOT
  (S
    (NP (JJ Numerous) (NNP DC) (NNS groups)
      (CC and)
      (NNS organizations))
    (VP (VBP oppose)
      (NP (DT the) (NN ban)))))

amod(groups-3, Numerous-1)
nn(groups-3, DC-2)
nsubj(oppose-6, groups-3)
conj_and(groups-3, organizations-5)
nsubj(oppose-6, organizations-5)
root(ROOT-0, oppose-6)
det(ban-8, the-7)
dobj(oppose-6, ban-8)

