  (S
    (NP (NNS Pilots))
    (VP (MD should)
      (VP (VB be)
        (VP (VBN armed)
          (PP (IN with)
            (NP
              (NP (NNS stun-guns))
              (, ,) (RB not)
              (NP (JJ real) (NNS guns)))))))
    (. .)))

