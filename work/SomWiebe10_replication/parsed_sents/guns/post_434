(ROOT
  (S
    (NP (DT No) (NNS sources))
    (VP (VBD cited))
    (. .)))

det(sources-2, No-1)
nsubj(cited-3, sources-2)
root(ROOT-0, cited-3)

(ROOT
  (S
    (VP
      (ADVP (RB Rather) (RB poorly))
      (VBN constructed)
      (NP (NN argument)))
    (. .)))

advmod(poorly-2, Rather-1)
advmod(constructed-3, poorly-2)
root(ROOT-0, constructed-3)
dobj(constructed-3, argument-4)

