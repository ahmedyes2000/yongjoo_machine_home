  (S
    (NP (PRP I))
    (ADVP (RB really))
    (VP (MD ca) (RB n't)
      (VP (VB understand)
        (SBAR
          (WHADVP (WRB how))
          (S
            (NP (RB so) (JJ many))
            (VP (MD can)
              (VP (VB be)
                (ADJP (RB so) (JJ mislead))))))))
    (. .)))

