(ROOT
  (S
    (VP (VB shut)
      (PRT (RP up))
      (NP (PRP you))
      (S
        (VP (VB commie)
          (SBAR (IN before)
            (S
              (NP (FW i))
              (VP (VBP show)
                (NP (PRP you))
                (SBAR
                  (WHNP (WP what))
                  (S
                    (NP (DT a) (JJ responsible) (NN gun) (NN owner))
                    (VP (MD can)
                      (VP (VB do)
                        (NP (NNS !!!))))))))))))))

root(ROOT-0, shut-1)
prt(shut-1, up-2)
dobj(shut-1, you-3)
dep(shut-1, commie-4)
mark(show-7, before-5)
nsubj(show-7, i-6)
advcl(commie-4, show-7)
dobj(show-7, you-8)
dobj(do-15, what-9)
det(owner-13, a-10)
amod(owner-13, responsible-11)
nn(owner-13, gun-12)
nsubj(do-15, owner-13)
aux(do-15, can-14)
dep(show-7, do-15)
dobj(do-15, !!!-16)

(ROOT
  (NP (NNP you#%&@%)))

root(ROOT-0, you#%&@%-1)

