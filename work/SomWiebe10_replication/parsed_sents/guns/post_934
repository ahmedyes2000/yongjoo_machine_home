(ROOT
  (PP (IN on)
    (NP
      (NP (NN behalf))
      (PP (IN of)
        (NP (PRP$ my) (NN hubby))))
    (: ...)))

root(ROOT-0, on-1)
pobj(on-1, behalf-2)
poss(hubby-5, my-4)
prep_of(behalf-2, hubby-5)

(ROOT
  (FRAG (: ...)
    (VP (VBP have)
      (NP (DT an) (NN upvote)))
    (. !)))

root(ROOT-0, have-2)
det(upvote-4, an-3)
dobj(have-2, upvote-4)

(ROOT
  (S
    (VP (VB ...)
      (SBAR
        (S
          (NP (PRP he))
          (VP (VBD liked)
            (NP (DT this) (NN one.) (: ;) (JJ -RRB-))))))))

nsubj(liked-3, he-2)
root(ROOT-0, liked-3)
det(one.-5, this-4)
dobj(liked-3, one.-5)

