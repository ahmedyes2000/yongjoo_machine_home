(ROOT
  (FRAG
    (ADVP (RB Yes))
    (SBAR (IN because)
      (S
        (NP (PRP they))
        (VP (MD can)
          (ADVP (RB always))
          (VP (VB end)
            (PRT (RP up))
            (PP (IN in)
              (NP (DT the) (JJ wrong) (NNS hands)))))))))

root(ROOT-0, Yes-1)
mark(end-6, because-2)
nsubj(end-6, they-3)
aux(end-6, can-4)
advmod(end-6, always-5)
dep(Yes-1, end-6)
prt(end-6, up-7)
det(hands-11, the-9)
amod(hands-11, wrong-10)
prep_in(end-6, hands-11)

