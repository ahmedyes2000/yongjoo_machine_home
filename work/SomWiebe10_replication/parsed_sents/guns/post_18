(ROOT
  (S
    (NP (DT This))
    (VP (VBZ is)
      (ADJP (RB directly) (JJ proportional)
        (PP (TO to)
          (SBAR
            (WHADJP (WRB how) (JJ many))
            (S
              (NP (NNS people))
              (VP (MD can)
                (VP (VB be)
                  (VP (VBN killed)))))))))
    (. .)))

