(ROOT
  (S
    (NP (JJ Assault) (NNS weapons))
    (VP (VBP are)
      (VP (VBN used)
        (S
          (VP (TO to)
            (VP (VB kill)
              (NP
                (NP (DT a) (JJR greater) (NN number))
                (PP (IN of)
                  (NP (NNS people)))))))))
    (. .)))

amod(weapons-2, Assault-1)
nsubjpass(used-4, weapons-2)
xsubj(kill-6, weapons-2)
auxpass(used-4, are-3)
root(ROOT-0, used-4)
aux(kill-6, to-5)
xcomp(used-4, kill-6)
det(number-9, a-7)
amod(number-9, greater-8)
dobj(kill-6, number-9)
prep_of(number-9, people-11)

