(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (NNS guns))
        (VP (VBD were) (RB n't)
          (ADJP (RB so) (RB freely) (JJ available))
          (PP (IN in)
            (NP (DT the) (NNP US)))
          (NP (RB then)))))
    (NP (EX there))
    (VP (MD would) (RB n't)
      (VP (VB be)
        (VP
          (ADVP (RB so)
            (NP (JJ many) (NNS people)))
          (VBG getting)
          (NP (NN shot)))))
    (. .)))

mark(available-7, If-1)
nsubj(available-7, guns-2)
cop(available-7, were-3)
neg(available-7, n't-4)
advmod(available-7, so-5)
advmod(available-7, freely-6)
