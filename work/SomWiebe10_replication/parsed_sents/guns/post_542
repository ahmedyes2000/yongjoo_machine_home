(ROOT
  (FRAG
    (PP
      (ADVP (RB Clearly))
      (IN in)
      (NP
        (NP (NN violation))
        (PP (IN of)
          (NP (DT the) (NNP Second) (NNP Amendment)))))
    (. .)))

advmod(in-2, Clearly-1)
root(ROOT-0, in-2)
pobj(in-2, violation-3)
det(Amendment-7, the-5)
nn(Amendment-7, Second-6)
prep_of(violation-3, Amendment-7)

(ROOT
  (S
    (NP (JJ Good) (NN thing))
    (NP (PRP it))
    (VP (VBD was)
      (VP (VBN overturned)))
    (. .)))

amod(thing-2, Good-1)
nsubjpass(overturned-5, thing-2)
nsubjpass(overturned-5, it-3)
auxpass(overturned-5, was-4)
root(ROOT-0, overturned-5)

