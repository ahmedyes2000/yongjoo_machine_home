(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ 's)
      (ADJP (JJ similar)
        (PP (TO to)
          (NP (DT the) (NN argument))))
      (SBAR (IN that)
        (S
          (S
            (VP (VBG having)
              (NP
                (NP (NN insulin))
                (PP (IN in)
                  (NP (DT the) (NN house))))))
          (VP (VBZ increases)
            (NP
              (NP (DT the) (NN risk))
              (PP (IN of)
                (NP (NN diabetes))))))))
    (. .)))

nsubj(similar-3, It-1)
cop(similar-3, 's-2)
root(ROOT-0, similar-3)
det(argument-6, the-5)
prep_to(similar-3, argument-6)
complm(increases-13, that-7)
csubj(increases-13, having-8)
dobj(having-8, insulin-9)
det(house-12, the-11)
prep_in(insulin-9, house-12)
ccomp(similar-3, increases-13)
det(risk-15, the-14)
dobj(increases-13, risk-15)
prep_of(risk-15, diabetes-17)

