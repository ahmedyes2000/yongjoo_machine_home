(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (EX there))
        (VP (VBZ is)
          (NP
            (NP (DT any) (NN concern))
            (PP (IN about)
              (NP
                (NP (DT the) (NN ability))
                (PP (IN of)
                  (NP (DT a) (NN gun) (NN owner)))))
            (S
              (VP (TO to)
                (VP (VB wield)
                  (NP (DT a) (NN shotgun)
                    (CC or)
                    (NN pistol))
                  (ADVP (RB properly))
                  (PP (JJ such)
                    (NP (DT that))))))))))
    (NP (PRP they))
    (VP (MD can)
      (VP (VB kill)
        (NP (JJ multiple) (NNS assailants))
        (, ,)
        (SBAR (IN than)
          (S
            (NP (PRP they))
            (VP (MD should)
              (VP (VB seek)
                (NP
                  (NP (JJ enhanced) (NN training))
                  (PP (RB instead) (IN of)
                    (NP (DT an) (NN assault) (NN weapon))))))))))
    (. .)))

