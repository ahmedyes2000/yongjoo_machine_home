(ROOT
  (S
    (NP (EX There))
    (VP (VBP are)
      (NP
        (NP (JJ adequate) (NNS alternatives))
        (PP (TO to)
          (NP
            (NP (NN assault) (NNS weapons))
            (PP (IN for)
              (NP (NN self-defense)))))))
    (. .)))

expl(are-2, There-1)
root(ROOT-0, are-2)
amod(alternatives-4, adequate-3)
nsubj(are-2, alternatives-4)
nn(weapons-7, assault-6)
prep_to(alternatives-4, weapons-7)
prep_for(weapons-7, self-defense-9)

