(ROOT
  (S
    (VP (VB See)
      (PP (IN for)
        (NP
          (NP (NN example))
          (, ,)
          (NP
            (NP (NN today) (POS 's))
            (NN post))))
      (PP (IN at)
        (NP (NNP StopTheWorldIWannaGetOff.com))))))

