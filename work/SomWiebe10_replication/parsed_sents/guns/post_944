(ROOT
  (S
    (SBAR (RB Even) (IN if)
      (S
        (NP (NNS guns))
        (VP (VBD were)
          (VP (VBN banned)
            (S
              (ADJP (JJ poeple)))))))
    (VP (MD could)
      (ADVP (RB still))
      (VP (VB make)
        (S
          (NP (EX there))
          (NP (JJ own) (NNS guns)))))
    (. .)))

advmod(banned-5, Even-1)
dep(banned-5, if-2)
nsubjpass(banned-5, guns-3)
auxpass(banned-5, were-4)
advcl(make-9, banned-5)
csubj(make-9, banned-5)
acomp(banned-5, poeple-6)
aux(make-9, could-7)
advmod(make-9, still-8)
root(ROOT-0, make-9)
expl(guns-12, there-10)
nsubj(guns-12, there-10)
amod(guns-12, own-11)
xcomp(make-9, guns-12)

