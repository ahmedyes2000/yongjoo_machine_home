(ROOT
  (S
    (ADVP (RB Honestly))
    (, ,)
    (NP (PRP I))
    (VP (VBP 've)
      (ADVP (RB never))
      (VP (VBN known)
        (S
          (NP (DT an) (NNP Uzi))
          (VP (TO to)
            (VP (VB fire)
              (NP (PRP itself)))))))
    (. .)))

advmod(known-6, Honestly-1)
nsubj(known-6, I-3)
aux(known-6, 've-4)
neg(known-6, never-5)
root(ROOT-0, known-6)
det(Uzi-8, an-7)
nsubj(fire-10, Uzi-8)
aux(fire-10, to-9)
xcomp(known-6, fire-10)
dobj(fire-10, itself-11)

