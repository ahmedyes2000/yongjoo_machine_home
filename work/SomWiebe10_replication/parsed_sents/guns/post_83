ccomp(indicate-3, decreases-8)
amod(rates-11, violent-9)
nn(rates-11, crime-10)
dobj(decreases-8, rates-11)

(ROOT
  (S
    (NP (DT The) (NN correlation))
    (VP (VBZ is) (RB not)
      (ADJP (JJ surprising)))
    (. .)))

det(correlation-2, The-1)
nsubj(surprising-5, correlation-2)
cop(surprising-5, is-3)
neg(surprising-5, not-4)
root(ROOT-0, surprising-5)

