(ROOT
  (S
    (NP
      (NP (DT The) (NN opposite))
      (PP (IN of)
        (NP (DT this))))
    (VP (VBZ is)
      (NP
        (NP (DT the) (NN use))
        (PP (IN of)
          (NP (NN force)))
        (S
          (VP (TO to)
            (VP (VB take)
              (NP (NNS guns))
              (PRT (RP away))
              (PP (IN from)
                (NP (NNS citizens))))))))
    (. .)))

det(opposite-2, The-1)
nsubj(use-7, opposite-2)
prep_of(opposite-2, this-4)
cop(use-7, is-5)
