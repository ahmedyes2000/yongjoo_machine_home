(ROOT
  (S
    (NP (DT A) (NN handgun) (NN ban))
    (VP (VBZ lowers)
      (NP
        (NP (DT the) (NN availability))
        (PP (IN of)
          (NP
            (NP (NNS guns))
            (VP (VBN used)
              (PP (IN in)
                (NP (JJ domestic) (NN passion) (NNS murders))))))))))

det(ban-3, A-1)
nn(ban-3, handgun-2)
nsubj(lowers-4, ban-3)
root(ROOT-0, lowers-4)
det(availability-6, the-5)
dobj(lowers-4, availability-6)
