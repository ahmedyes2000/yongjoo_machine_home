(ROOT
  (S
    (NP
      (NP (JJS Most) (NNS individuals))
      (VP (VBG seeking)
        (NP
          (NP (DT a) (NN ban))
          (PP (IN on)
            (NP (NN assault) (NNS weapons))))))
    (ADVP (RB just))
    (VP (VBP want)
      (S
        (NP (NN assault) (NNS weapons))
        (VP (TO to)
          (VP (VB be)
            (VP (VBN banned))))))
    (. .)))

