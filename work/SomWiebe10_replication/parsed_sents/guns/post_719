(ROOT
  (SBARQ
    (WHADVP (WRB where))
    (SQ (VBP are)
      (NP (PRP$ your) (NNS statistics)))
    (. ?)))

advmod(are-2, where-1)
root(ROOT-0, are-2)
poss(statistics-4, your-3)
nsubj(are-2, statistics-4)

(ROOT
  (S
    (SBAR (IN if)
      (S
        (NP (EX there))
        (VP (VBD were)
          (NP (JJ such) (NNS incidents)))))
    (, ,)
    (NP (DT the) (NNS media))
    (VP (MD would)
      (VP (VB be)
        (NP
          (NP (DT all))
          (PP (IN over)
            (NP (PRP it))))))
    (. .)))

mark(were-3, if-1)
expl(were-3, there-2)
advcl(all-11, were-3)
amod(incidents-5, such-4)
nsubj(were-3, incidents-5)
det(media-8, the-7)
nsubj(all-11, media-8)
aux(all-11, would-9)
cop(all-11, be-10)
root(ROOT-0, all-11)
prep_over(all-11, it-13)

