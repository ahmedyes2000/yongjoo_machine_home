(ROOT
  (S (`` ``)
    (NP (NNP Armed) (NNS pilots))
    (VP (VBP equal)
      (NP (JJR safer) (NNS airplanes)))
    ('' '') (. .)))

nn(pilots-3, Armed-2)
nsubj(equal-4, pilots-3)
root(ROOT-0, equal-4)
amod(airplanes-6, safer-5)
dobj(equal-4, airplanes-6)

(ROOT
  (NP (NNP USA) (NNP Today) (. .)))

nn(Today-2, USA-1)
root(ROOT-0, Today-2)

(ROOT
  (NP
    (NP (NNP February) (NNP 14th))
    (, ,)
    (NP (CD 2007))))

