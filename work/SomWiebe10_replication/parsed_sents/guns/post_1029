(ROOT
  (S
    (INTJ (UH Yes))
    (, ,)
    (NP (NN everybody))
    (VP (MD should)
      (VP (VB have)
        (NP (DT the) (NN right)
          (S
            (VP (TO to)
              (VP (VB have)
                (S
                  (NP (NNS guns))
                  (VP (TO to)
                    (VP (VB protect)
                      (NP (PRP themselves)))))))))))
    (. .)))

dep(have-5, Yes-1)
nsubj(have-5, everybody-3)
aux(have-5, should-4)
root(ROOT-0, have-5)
det(right-7, the-6)
dobj(have-5, right-7)
aux(have-9, to-8)
infmod(right-7, have-9)
nsubj(protect-12, guns-10)
aux(protect-12, to-11)
ccomp(have-9, protect-12)
xcomp(have-9, protect-12)
dobj(protect-12, themselves-13)

