  (S
    (NP
      (NP (CD 200,000,000) (JJ dead) (NNS people))
      (, ,)
      (VP (VBN killed)
        (PP (IN by)
          (NP (PRP$ their) (JJ own) (NNS governments)))
        (PP (IN in)
          (NP (RB just) (DT the) (JJ last) (CD 100) (NNS years))))
      (, ,))
    (VP (VBP disagree)
      (PP (IN with)
        (NP (PRP you))))
    (. .)))

