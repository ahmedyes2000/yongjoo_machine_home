(ROOT
  (S
    (NP (NNP Militia))
    (VP (VBZ means)
      (NP
        (NP (DT the) (JJ whole) (NN body))
        (PP (IN of)
          (NP
            (NP (NNS individuals))
            (PP (IN within)
              (NP (DT the) (NNP US)))))))))

nsubj(means-2, Militia-1)
root(ROOT-0, means-2)
det(body-5, the-3)
amod(body-5, whole-4)
dobj(means-2, body-5)
prep_of(body-5, individuals-7)
det(US-10, the-9)
prep_within(individuals-7, US-10)

