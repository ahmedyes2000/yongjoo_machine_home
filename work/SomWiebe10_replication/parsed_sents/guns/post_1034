(ROOT
  (S
    (NP (NN Everyone))
    (VP (MD should)
      (VP
        (VP (VB have)
          (NP
            (NP (JJ several))
            (PP (IN of)
              (NP (PRP them)))))
        (CC and)
        (VP (VB carry)
          (NP
            (QP (IN at) (JJS least) (CD one)))
          (ADVP (RB around))
          (PP (IN at)
            (NP (DT all) (NNS times))))))
    (. .)))

nsubj(have-3, Everyone-1)
nsubj(carry-8, Everyone-1)
aux(have-3, should-2)
root(ROOT-0, have-3)
dobj(have-3, several-4)
prep_of(several-4, them-6)
