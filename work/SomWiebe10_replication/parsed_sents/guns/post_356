(ROOT
  (S
    (NP (NNP Gun) (NN control))
    (VP (VBZ does) (RB n't)
      (VP (VB work)))
    (. .)))

nn(control-2, Gun-1)
nsubj(work-5, control-2)
aux(work-5, does-3)
neg(work-5, n't-4)
root(ROOT-0, work-5)

(ROOT
  (S
    (NP
      (NP (JJ Keeping) (NNS guns))
      (PP (IN from)
        (NP (NN law) (NN abiding) (NNS citizens))))
    (VP (MD will)
      (ADVP (RB only))
      (VP (VB help)
        (NP (DT the) (NNS criminals))))))

amod(guns-2, Keeping-1)
nsubj(help-9, guns-2)
nn(citizens-6, law-4)
nn(citizens-6, abiding-5)
prep_from(guns-2, citizens-6)
aux(help-9, will-7)
advmod(help-9, only-8)
root(ROOT-0, help-9)
det(criminals-11, the-10)
dobj(help-9, criminals-11)

