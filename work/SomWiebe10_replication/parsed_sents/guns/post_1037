(ROOT
  (S
    (NP (PRP I))
    (VP (VBP think)
      (SBAR
        (S
          (NP (DT every) (NN one))
          (VP (MD should)
            (S
              (VP
                (VP (TO to)
                  (VP (VB protect)
                    (NP (PRP themselves))))
                (CC and)
                (VP (TO to)
                  (VP (VB use)
                    (SBAR
                      (WHADVP (WRB when))
                      (FRAG
                        (NP (PRP$ your) (NN hunting))))))))))))
    (. !)))

nsubj(think-2, I-1)
root(ROOT-0, think-2)
det(one-4, every-3)
nsubj(should-5, one-4)
ccomp(think-2, should-5)
aux(protect-7, to-6)
ccomp(should-5, protect-7)
dobj(protect-7, themselves-8)
aux(use-11, to-10)
ccomp(should-5, use-11)
conj_and(protect-7, use-11)
advmod(hunting-14, when-12)
poss(hunting-14, your-13)
advcl(use-11, hunting-14)

