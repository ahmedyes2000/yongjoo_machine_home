        (PP (IN for)
          (S
            (VP (VBG having)
              (NP
                (NP (DT an) (NN alarm))
                (CC and)
                (NP (DT a) (NN dog))))))
        (CC and)
        (PP (IN for)
          (S
            (VP (VBG educating)
              (NP (PRP$ your) (NNS children))
              (SBAR (IN that)
                (S
                  (NP (NNS guns))
                  (VP (VBP are)
                    (UCP
                      (NP (RB not) (NNS toys))
                      (CC and)
                      (PP (IN on)
                        (NP (JJ responsible) (NN gun) (NN ownership))))))))))))
    (. .)))

