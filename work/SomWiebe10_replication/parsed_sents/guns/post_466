(ROOT
  (S
    (NP (PRP I))
    (VP (VBP carry)
      (NP (DT a) (NN glock))
      (PP
        (ADVP
          (NP (CD 21))
          (RB just))
        (IN in)
        (NP (NN case))))
    (. !)))

