(ROOT
  (FRAG
    (NP
      (NP (DT A) (NN person))
      (PP (IN of)
        (NP (JJ vocabulistic) (NN exhibitionism))))
    (: ...)
    (SINV
      (PP (IN in)
        (NP
          (NP (PRP$ your) (JJ next) (NN excursion))
          (PP (IN of)
            (NP (JJ verbal) (NN spewism)))))
      (, ,)
      (VP (VBP remember)
        (SBAR (IN that)
          (S
            (NP (FW masse))
            (VP (VBZ refers)
              (PP (TO to)
                (NP (DT a) (JJ billard) (NN shot)))))))
      (NP
        (NP (RB not) (JJ large) (NNS numbers))
        (PP (IN of)
          (NP (NNS people)))))
    (. !)))

