(ROOT
  (S
    (S
      (ADVP (RB Perhaps))
      (NP (JJ anti) (NN armor) (NNS rockets))
      (VP (MD should)
        (VP (VB have)
          (NP (DT a) (JJ unique) (NN identifier)))))
