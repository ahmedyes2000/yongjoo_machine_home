    (CC but)
    (S
      (NP (JJ bad) (NNS things))
      (VP (VBP happen)
        (SBAR
          (WHADVP (WRB when))
          (S
            (NP (JJ armed) (NN revolt))
            (VP (VBZ is)
              (ADJP (JJ impossible)))))))
    (. .)))

