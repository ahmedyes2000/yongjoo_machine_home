(ROOT
  (S (-LRB- -LRB-)
    (NP (PRP we))
    (VP (VBP have)
      (PP (IN by)
        (NP
          (NP (JJ far))
          (, ,)
          (ADVP
            (NP (JJ my) (NNS guns))
            (IN than))
          (NP (NN anybody) (RB else))
          (-RRB- -RCB-)
          (ADJP (JJ OK))))
      (ADVP (IN than)))
    (. .)))

