nsubj(has-6, criminal-2)
det(street-5, the-4)
prep_on(criminal-2, street-5)
root(ROOT-0, has-6)
det(gun-8, a-7)
dobj(has-6, gun-8)

(ROOT
  (S
    (SBAR (IN Whether)
      (S
        (NP (PRP$ its))
        (VP (VBD pointed)
          (PP (IN at)
            (NP
              (NP (PRP you))
              (CC or)
              (NP
                (NP (CD one))
                (PP (IN of)
                  (NP (PRP$ your) (NNS neighbors)
                    (CC or)
                    (NNS friends)))))))))
    (NP (PRP it))
    (VP (VBZ makes)
      (S
        (NP
          (NP (NN sense))
          (PP (IN for)
            (NP (PRP you))))
        (VP (TO to)
          (VP (VB have)
            (NP (DT the) (NN right)
              (S
                (VP (TO to)
                  (VP (VB protect)
                    (NP (PRP yourself))
                    (PP (IN from)
                      (NP
                        (NP (DT these) (NNS dregs))
                        (PP (IN of)
                          (NP (NN society)))))))))))))
    (. .)))

complm(pointed-3, Whether-1)
nsubj(pointed-3, its-2)
ccomp(makes-14, pointed-3)
