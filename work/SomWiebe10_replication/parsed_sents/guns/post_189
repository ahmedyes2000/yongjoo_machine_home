(ROOT
  (S
    (NP (DT That) (NNP 's) ('' '') (NNP Mousy))
    (VP (VBP Tongue) ('' ''))
    (. .)))

det(Mousy-4, That-1)
nn(Mousy-4, 's-2)
nsubj(Tongue-5, Mousy-4)
root(ROOT-0, Tongue-5)

(ROOT
  (S
    (NP (NNP ANY) (NNP serviceman))
    (VP (MD can)
      (VP (VB tell)
        (NP (PRP you))
        (NP (DT that) (NN !!))))))

nn(serviceman-2, ANY-1)
nsubj(tell-4, serviceman-2)
aux(tell-4, can-3)
root(ROOT-0, tell-4)
iobj(tell-4, you-5)
det(!!-7, that-6)
dobj(tell-4, !!-7)

