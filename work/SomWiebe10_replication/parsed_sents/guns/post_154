(ROOT
  (S
    (NP
      (NP (DT The) (NN idea))
      (PP (IN of)
        (NP
          (NP (DT a) (NN militia))
          (VP (VBG protecting)
            (PP (IN against)
              (NP (JJ domestic) (NN tyranny)))))))
    (VP (VBZ is)
      (NP (DT a) (NN myth)))))

det(idea-2, The-1)
nsubj(myth-12, idea-2)
det(militia-5, a-4)
prep_of(idea-2, militia-5)
partmod(militia-5, protecting-6)
amod(tyranny-9, domestic-8)
prep_against(protecting-6, tyranny-9)
cop(myth-12, is-10)
det(myth-12, a-11)
root(ROOT-0, myth-12)

