(ROOT
  (S
    (S
      (NP (NNPS Americans))
      (VP (VBP are)
        (VP (VBN obsessed)
          (PP (IN with)
            (NP (NNS guns)
              (CC and)
              (NN killing))))))
    (: ...)
    (S
      (NP (RB even) (DT the) (NN law))
      (VP (VBG abiding)
        (NP (NNS ones))))
    (. .)))

nsubjpass(obsessed-3, Americans-1)
auxpass(obsessed-3, are-2)
root(ROOT-0, obsessed-3)
prep_with(obsessed-3, guns-5)
prep_with(obsessed-3, killing-7)
conj_and(guns-5, killing-7)
advmod(law-11, even-9)
det(law-11, the-10)
nsubj(abiding-12, law-11)
parataxis(obsessed-3, abiding-12)
dobj(abiding-12, ones-13)

(ROOT
  (S
    (VP (VB Its)
      (NP (DT a) (JJ mental) (NN disorder)))
    (. .)))

root(ROOT-0, Its-1)
det(disorder-4, a-2)
amod(disorder-4, mental-3)
dobj(Its-1, disorder-4)

