(ROOT
  (S
    (NP (PRP You))
    (VP (VBP blame)
      (NP
        (NP (DT a) (JJ single) (NN object))
        (PP (IN on)
          (NP (DT a) (JJ single) (NN factor)))))
    (. .)))

