(ROOT
  (S
    (NP
      (NP (JJ Stricter) (NN control))
      (CC or)
      (NP (RB not) (DT the) (JJ crazy) (NNS people)))
    (VP (VBP are)
      (ADJP (JJ gonna)
        (SBAR
          (S
            (VP (VBP get)
              (NP (NNS guns))
              (ADVP (RB regardless)))))))))

amod(control-2, Stricter-1)
nsubj(gonna-9, control-2)
neg(people-7, not-4)
det(people-7, the-5)
amod(people-7, crazy-6)
conj_or(control-2, people-7)
nsubj(gonna-9, people-7)
cop(gonna-9, are-8)
root(ROOT-0, gonna-9)
dep(gonna-9, get-10)
dobj(get-10, guns-11)
advmod(get-10, regardless-12)

