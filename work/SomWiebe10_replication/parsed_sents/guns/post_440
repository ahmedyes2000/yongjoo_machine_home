(ROOT
  (S
    (NP (EX There))
    (VP (VBP are)
      (NP
        (NP (CD three) (NNS kinds))
        (PP (IN of)
          (NP (NNS lies)))))
    (. .)))

expl(are-2, There-1)
root(ROOT-0, are-2)
num(kinds-4, three-3)
nsubj(are-2, kinds-4)
prep_of(kinds-4, lies-6)

(ROOT
  (NP
    (NP (NNS Lies))
    (, ,)
    (NP (JJ damned) (NNS lies)
      (CC and)
      (NNS statistics))
    (. .)))

root(ROOT-0, Lies-1)
amod(lies-4, damned-3)
appos(Lies-1, lies-4)
appos(Lies-1, statistics-6)
conj_and(lies-4, statistics-6)

