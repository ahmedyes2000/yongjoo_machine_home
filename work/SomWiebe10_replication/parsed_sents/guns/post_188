    (S
      (NP (DT that) (JJ political) (NN change))
      (VP (VBZ is)
        (VP (VBG being)
          (VP (VBD promulgated)
            (PP (IN from)
              (NP
                (NP (DT the) (NN barrel))
                (PP (IN of)
                  (NP (DT the) (NN gun)))))))))
    (: ...)))

