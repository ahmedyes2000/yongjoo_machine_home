  (S
    (SBAR
      (ADVP (RB Yes))
      (IN if)
      (S
        (NP (NN someone))
        (VP
          (VP (VBZ has)
            (NP (DT a) (DT a) (NN gun)))
          (CC and)
          (VP (VBZ is)
            (ADJP (IN about)
              (S
                (VP (TO to)
                  (VP (VB shoot)
                    (NP (NN someone))))))))))
    (, ,)
    (NP (NN someone) (RB else))
    (VP (MD could)
      (VP
        (VP (VB get)
          (NP (PRP$ their) (NN gun)))
        (CC and)
        (VP (VB shoot)
          (NP (PRP them)))))
    (. !)))

