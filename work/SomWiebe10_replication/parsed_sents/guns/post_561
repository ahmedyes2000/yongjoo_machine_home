(ROOT
  (S
    (PP (IN As)
      (PP (IN in)))
    (, ,)
    (NP (DT the) (DT those) (NNS things))
    (NP (PRP you))
    (VP (MD can)
      (VP (VB wave)
        (PRT (RP around))
        (SBAR
          (WHADVP (WRB when))
          (S
            (NP (PRP you))
            (VP (VBP 're)
              (VP (VBG dancing)
                (SBAR (IN that)
                  (S
                    (NP (PRP$ your) (NNS hands))
                    (VP (VBP attach)
                      (PP (TO to)))))))))))
    (. .)))

prep(wave-9, As-1)
pcomp(As-1, in-2)
prep(As-1, in-2)
predet(things-6, the-4)
det(things-6, those-5)
nsubj(wave-9, things-6)
nsubj(wave-9, you-7)
aux(wave-9, can-8)
root(ROOT-0, wave-9)
prt(wave-9, around-10)
advmod(dancing-14, when-11)
nsubj(dancing-14, you-12)
aux(dancing-14, 're-13)
advcl(wave-9, dancing-14)
complm(attach-18, that-15)
poss(hands-17, your-16)
nsubj(attach-18, hands-17)
ccomp(dancing-14, attach-18)
prep(attach-18, to-19)

