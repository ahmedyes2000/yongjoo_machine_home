(ROOT
  (S
    (S
      (NP (NNP Sorry))
      (VP (TO to)
        (VP (VB say)
          (NP (PRP it)))))
    (, ,)
    (CC but)
    (S
      (NP (PRP it))
      (VP (VBZ is)
        (ADVP (RB probably))
        (ADJP (JJ true))))
    (. .)))

nsubj(say-3, Sorry-1)
aux(say-3, to-2)
root(ROOT-0, say-3)
dobj(say-3, it-4)
nsubj(true-10, it-7)
cop(true-10, is-8)
advmod(true-10, probably-9)
conj_but(say-3, true-10)

