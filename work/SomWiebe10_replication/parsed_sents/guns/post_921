(ROOT
  (S
    (NP (NNS Guns))
    (VP (MD should)
      (VP (VB be)
        (VP (VBN banned)
          (ADVP (RB everywhere))
          (PP (IN except)
            (NP (NNP America))))))
    (. .)))

nsubjpass(banned-4, Guns-1)
aux(banned-4, should-2)
auxpass(banned-4, be-3)
root(ROOT-0, banned-4)
advmod(banned-4, everywhere-5)
prep_except(banned-4, America-7)

