(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP (VBP make)
          (NP (NN gun) (NN control) (NNS laws)))))
    (NP (PRP you))
    (VP (MD 'll)
      (ADVP (RB only))
      (VP (VB take)
        (NP (NNS guns))
        (PRT (RP away))
        (PP (IN from)
          (NP (JJ good) (, ,) (NN law) (JJ abiding) (NNS citizens)))))
    (. .)))

