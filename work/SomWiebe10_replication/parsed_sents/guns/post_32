(ROOT
  (S
    (NP (JJS Most) (JJ automatic) (NNS weapons))
    (VP (VBD were)
      (ADJP (RB already) (JJ illegal))
      (PP (IN under)
        (NP
          (NP (DT the) (NNP National) (NNPS Firearms) (NNP Act))
          (PP (IN of)
            (NP (CD 1934))))))))

amod(weapons-3, Most-1)
amod(weapons-3, automatic-2)
nsubj(illegal-6, weapons-3)
cop(illegal-6, were-4)
advmod(illegal-6, already-5)
root(ROOT-0, illegal-6)
det(Act-11, the-8)
nn(Act-11, National-9)
nn(Act-11, Firearms-10)
prep_under(illegal-6, Act-11)
