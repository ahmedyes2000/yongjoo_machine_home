            (PP (IN by)
              (NP (DT the) (NNS rules)))))))
    (VP (VBP are)
      (ADVP (RB always))
      (VP (VBN persecuted)
        (PP (IN for)
          (NP
            (NP (DT the) (NNS actions))
            (PP (IN of)
              (NP
                (NP (DT those))
                (SBAR
                  (WHNP (WP who))
                  (S
                    (VP (VBP do) (RB not))))))))))
    (. .)))

