(ROOT
  (NP (DT No) (NN way)
    (SBAR
      (WHNP (WDT that))
      (S
        (VP (MD would)
          (VP (VB make)
            (S
              (NP (PRP it))
              (ADJP (RB very) (JJ dangerous)))))))
    (. !)))

det(way-2, No-1)
root(ROOT-0, way-2)
nsubj(make-5, that-3)
aux(make-5, would-4)
ccomp(way-2, make-5)
nsubj(dangerous-8, it-6)
advmod(dangerous-8, very-7)
xcomp(make-5, dangerous-8)

