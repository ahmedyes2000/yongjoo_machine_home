(ROOT
  (S
    (NP (PRP You))
    (VP (VBD got)
      (NP (DT that) (NN right)))
    (. .)))

nsubj(got-2, You-1)
root(ROOT-0, got-2)
det(right-4, that-3)
dobj(got-2, right-4)

(ROOT
  (S
    (VP (VB Fight)
      (PP (TO to)
        (NP (DT the) (NN end))))
    (. !)))

root(ROOT-0, Fight-1)
det(end-4, the-3)
prep_to(Fight-1, end-4)

(ROOT
  (FRAG
    (ADVP (RB Never))
    (ADVP (RB back) (RB down) (. !))))

root(ROOT-0, Never-1)
advmod(down-3, back-2)
dep(Never-1, down-3)

