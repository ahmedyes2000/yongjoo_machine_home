(ROOT
  (S
    (NP (PRP$ My) (NN teacher))
    (VP (VBD said)
      (SBAR (IN if)
        (S
          (NP (PRP you))
          (VP (VBP threaten)
            (NP (NN someone))
            (PP (IN with)
              (NP
                (NP (DT a) (NN gun-unarmed))
                (SBAR
                  (S
                    (NP (PRP they))
                    (VP (VBP are)
                      (ADJP (RBR less) (JJ likely)
                        (S
                          (VP (TO to)
                            (VP (VB shoot)
                              (NP (PRP you)))))))))))))))
    (. .)))

poss(teacher-2, My-1)
nsubj(said-3, teacher-2)
root(ROOT-0, said-3)
