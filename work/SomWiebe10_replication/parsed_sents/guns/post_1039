xcomp(likely-14, shoot-16)
dobj(shoot-16, you-17)

(ROOT
  (S
    (NP (NNP HOWEVER))
    (, ,)
    (SBAR (IN if)
      (S
        (NP (PRP you))
        (VP (VBP have)
          (NP (DT a) (NN gun)))))
    (, ,)
    (NP (PRP they))
    (VP (MD 'll)
      (VP (VB shoot)
        (NP (PRP you))
        (ADVP (RB first))
        (PP (IN for)
          (NP (NN fear) (NN YOU) (NNP 'LL) (NNP shoot) (NNP THEM)))))))

