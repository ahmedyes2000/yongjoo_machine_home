(ROOT
  (S
    (VP (VB Imagine)
      (NP (DT the) (NN crime) (NN rate))
      (SBAR (IN if)
        (S
          (NP (NN everyone))
          (VP (VBD had)
            (NP
              (NP (DT a) (NN gun))
              (PP (IN in)
                (NP (DT this) (NN world))))))))
    (. .)))

root(ROOT-0, Imagine-1)
det(rate-4, the-2)
nn(rate-4, crime-3)
dobj(Imagine-1, rate-4)
mark(had-7, if-5)
nsubj(had-7, everyone-6)
advcl(Imagine-1, had-7)
det(gun-9, a-8)
dobj(had-7, gun-9)
det(world-12, this-11)
prep_in(gun-9, world-12)

