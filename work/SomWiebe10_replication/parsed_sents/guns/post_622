(ROOT
  (S
    (S
      (NP (NNP Well))
      (VP (VBD said)))
    (, ,)
    (NP (DT Some) (NNS people))
    (VP (MD will)
      (ADVP (RB never))
      (VP (VB give)
        (PRT (RP up))
        (S
          (VP (VBG trying)
            (S
              (VP (TO to)
                (VP (VB take)
                  (PRT (RP away))
                  (NP (PRP$ our) (NNS freedoms)))))))))
    (. .)))

nsubj(said-2, Well-1)
ccomp(give-8, said-2)
det(people-5, Some-4)
nsubj(give-8, people-5)
aux(give-8, will-6)
neg(give-8, never-7)
root(ROOT-0, give-8)
prt(give-8, up-9)
xcomp(give-8, trying-10)
aux(take-12, to-11)
xcomp(trying-10, take-12)
prt(take-12, away-13)
poss(freedoms-15, our-14)
dobj(take-12, freedoms-15)

