(ROOT
  (S
    (NP (NNS people))
    (VP (MD will)
      (ADVP (RB still))
      (VP (VB get)
        (NP
          (NP (NNS guns)
            (CC and)
            (NNS whats))
          (VP (VBG going)
            (S
              (VP (TO to)
                (VP (VB happen)
                  (PP (TO to)
                    (NP (PDT all) (DT the) (NNS guns)))
                  (PP (DT all) (IN over)
                    (NP (DT the) (NN world))))))))))
    (. .)))

nsubj(get-4, people-1)
aux(get-4, will-2)
advmod(get-4, still-3)
root(ROOT-0, get-4)
dobj(get-4, guns-5)
xsubj(happen-10, guns-5)
dobj(get-4, whats-7)
conj_and(guns-5, whats-7)
xsubj(happen-10, whats-7)
partmod(guns-5, going-8)
aux(happen-10, to-9)
xcomp(going-8, happen-10)
predet(guns-14, all-12)
det(guns-14, the-13)
prep_to(happen-10, guns-14)
dep(happen-10, all-15)
det(world-18, the-17)
prep_over(happen-10, world-18)

(ROOT
  (S
    (NP (EX there))
    (VP (MD will)
      (ADVP (RB never))
      (VP (VB be)
        (NP (NN world) (NN peace))))
    (. .)))

