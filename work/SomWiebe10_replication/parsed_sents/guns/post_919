(ROOT
  (S
    (NP (PRP I))
    (VP (VBD meant)
      (SBAR (IN that)
        (S
          (NP (PRP I))
          (VP (VBD wanted)
            (NP (NNS guns))
            (S
              (VP (TO to)
                (VP (VB be)
                  (VP (VBN kept)
                    (PP (IN in)
                      (NP (DT the) (JJ right) (NNS hands)))))))))))
    (. .)))

nsubj(meant-2, I-1)
root(ROOT-0, meant-2)
complm(wanted-5, that-3)
nsubj(wanted-5, I-4)
ccomp(meant-2, wanted-5)
dobj(wanted-5, guns-6)
aux(kept-9, to-7)
auxpass(kept-9, be-8)
xcomp(wanted-5, kept-9)
det(hands-13, the-11)
amod(hands-13, right-12)
prep_in(kept-9, hands-13)

