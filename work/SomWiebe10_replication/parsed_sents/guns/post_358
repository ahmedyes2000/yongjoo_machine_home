(ROOT
  (S
    (NP (PRP I))
    (ADVP (RB totally))
    (VP (VBP agree)
      (PP (IN with)
        (NP (DT the) (NN argument)))
      (SBAR (IN that)
        (S
          (NP (NNS guns))
          (VP (VBP make)
            (S
              (NP (PRP you))
              (ADJP (JJR safer)))))))
    (. .)))

