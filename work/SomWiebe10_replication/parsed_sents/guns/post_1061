(ROOT
  (FRAG
    (ADJP (FW No.) (. .))
    (. .)))

root(ROOT-0, No.-1)

(ROOT
  (NP (: ...) (NN period) (. .)))


(ROOT
  (S
    (NP (PRP$ My) (NN son))
    (VP (VBZ hunts)
      (CC and)
      (VBZ enjoys)
      (NP (DT the) (NN sport)))
    (. .)))

poss(son-2, My-1)
nsubj(hunts-3, son-2)
nsubj(enjoys-5, son-2)
root(ROOT-0, hunts-3)
conj_and(hunts-3, enjoys-5)
det(sport-7, the-6)
dobj(hunts-3, sport-7)

