nsubj(breaks-3, someone-2)
advcl(has-11, breaks-3)
det(home-6, a-5)
prep_into(breaks-3, home-6)
det(homeowner-8, the-7)
nsubj(has-11, homeowner-8)
advmod(always-10, almost-9)
advmod(has-11, always-10)
root(ROOT-0, has-11)
det(advantage-13, the-12)
dobj(has-11, advantage-13)

