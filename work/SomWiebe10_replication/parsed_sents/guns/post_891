(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (NN someone))
        (VP (VBZ breaks)
          (PP (IN into)
            (NP (DT a) (NN home))))))
    (NP (DT the) (NN homeowner))
    (ADVP (RB almost) (RB always))
    (VP (VBZ has)
      (NP (DT the) (NN advantage)))
    (. .)))

