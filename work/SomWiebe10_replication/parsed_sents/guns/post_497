prep_for(is-3, self-defense-6)

(ROOT
  (S
    (NP (NNS Thats))
    (VP (VBP like)
      (S
        (VP (VBG having)
          (NP (DT a) (NN car))
          (PP (IN with)
            (NP (DT no) (NN engine))))))
    (. .)))

nsubj(like-2, Thats-1)
root(ROOT-0, like-2)
xcomp(like-2, having-3)
det(car-5, a-4)
