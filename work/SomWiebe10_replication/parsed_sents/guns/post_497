(ROOT
  (SBARQ
    (WHADVP (WRB Why))
    (SQ
      (VP (VBP have)
        (NP (DT a) (NN gun))
        (UCP
          (PP (IN if)
            (NP (PRP$ its) (NN unloaded)))
          (CC and)
          (VP (VBN locked)
            (PRT (RP up))))))
    (. ?)))

advmod(have-2, Why-1)
root(ROOT-0, have-2)
det(gun-4, a-3)
dobj(have-2, gun-4)
dep(have-2, if-5)
poss(unloaded-7, its-6)
pobj(if-5, unloaded-7)
dep(have-2, locked-9)
conj_and(if-5, locked-9)
prt(locked-9, up-10)

(ROOT
  (SBARQ
    (WHADJP (WRB How) (JJ good))
    (SQ (VBZ is)
      (NP (PRP it))
      (PP (IN for)
        (NP (NN self-defense))))
    (. ?)))

