(ROOT
  (S
    (SBAR (RB Even) (IN if)
      (S
        (NP (VBG disarming))
        (VP (VBZ is)
          (VP (VBN deemed)
            (S
              (ADJP (JJ necessary)))))))
    (, ,)
    (NP (PRP it))
    (VP (MD will)
      (VP (VB be)
        (ADJP (JJ unconstitutional))))))

advmod(deemed-5, Even-1)
dep(deemed-5, if-2)
nsubjpass(deemed-5, disarming-3)
auxpass(deemed-5, is-4)
advcl(unconstitutional-11, deemed-5)
acomp(deemed-5, necessary-6)
nsubj(unconstitutional-11, it-8)
aux(unconstitutional-11, will-9)
cop(unconstitutional-11, be-10)
root(ROOT-0, unconstitutional-11)

