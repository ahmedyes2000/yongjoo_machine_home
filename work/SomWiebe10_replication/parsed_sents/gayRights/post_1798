(ROOT
  (S
    (INTJ (UH Oh))
    (INTJ (UH yes))
    (, ,)
    (NP (PRP it))
    (VP (VBZ is)
      (NP (DT a) (NN sin)))
    (. .)))

dep(sin-7, Oh-1)
dep(sin-7, yes-2)
nsubj(sin-7, it-4)
cop(sin-7, is-5)
det(sin-7, a-6)
root(ROOT-0, sin-7)

(ROOT
  (S
    (NP (PRP I))
    (VP (VBP 'm) (RB not)
      (VP (VBG acting)
        (PP (IN on)
          (NP
            (NP (DT the) (NNP Bible) (POS 's))
            (NN word)))))
    (. .)))

nsubj(acting-4, I-1)
aux(acting-4, 'm-2)
neg(acting-4, not-3)
root(ROOT-0, acting-4)
det(Bible-7, the-6)
poss(word-9, Bible-7)
prep_on(acting-4, word-9)

