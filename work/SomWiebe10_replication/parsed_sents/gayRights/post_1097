(ROOT
  (S
    (PP (IN By)
      (NP (DT that) (NN argument)))
    (, ,)
    (NP (JJ interracial) (NN marriage))
    (VP (MD should)
      (ADVP (RB never))
      (VP (VB have)
        (VP (VBN been)
          (VP (VBN allowed) (, ,)
            (SBAR (IN since)
              (S
                (NP (PRP it))
                (ADVP (RB previously))
                (VP (VBD was) (RB n't))))))))
    (. .)))

det(argument-3, that-2)
agent(allowed-11, argument-3)
amod(marriage-6, interracial-5)
nsubjpass(allowed-11, marriage-6)
aux(allowed-11, should-7)
neg(allowed-11, never-8)
aux(allowed-11, have-9)
auxpass(allowed-11, been-10)
root(ROOT-0, allowed-11)
mark(was-16, since-13)
nsubj(was-16, it-14)
advmod(was-16, previously-15)
advcl(allowed-11, was-16)
neg(was-16, n't-17)

(ROOT
  (FRAG
    (NP (NNP Hell))
    (, ,)
    (SBAR
      (WHADVP (WRB why))
      (S
        (VP (VBP change)
          (NP (NN anything)))))
    (. ?)))

root(ROOT-0, Hell-1)
advmod(change-4, why-3)
dep(Hell-1, change-4)
dobj(change-4, anything-5)

