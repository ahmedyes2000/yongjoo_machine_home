(ROOT
  (S
    (NP (PRP I))
    (VP (VBP applaud)
      (NP (PRP$ your) (NN ability)
        (S
          (VP (TO to)
            (VP (RB not) (VB impose)
              (NP (PRP$ your) (JJ personal) (NNS beliefs))
              (PP (IN on)
                (NP (NNS others))))))))
    (. .)))

nsubj(applaud-2, I-1)
root(ROOT-0, applaud-2)
poss(ability-4, your-3)
dobj(applaud-2, ability-4)
aux(impose-7, to-5)
neg(impose-7, not-6)
infmod(ability-4, impose-7)
poss(beliefs-10, your-8)
amod(beliefs-10, personal-9)
dobj(impose-7, beliefs-10)
prep_on(impose-7, others-12)

(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (RB only) (JJR more) (NN religious) (NNS people))
        (VP (MD could)
          (VP (VB be)))))
    (VP (VBP like)
      (NP (PRP you)))
    (. .)))

mark(be-7, If-1)
advmod(people-5, only-2)
amod(people-5, more-3)
nn(people-5, religious-4)
nsubj(be-7, people-5)
aux(be-7, could-6)
csubj(like-8, be-7)
root(ROOT-0, like-8)
dobj(like-8, you-9)

