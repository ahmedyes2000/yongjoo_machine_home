(ROOT
  (S
    (VP (VB Forget)
      (SBAR
        (S
          (NP (PRP I))
          (VP (VBD said)
            (S
              (ADJP (NN anything))
              (SBAR
                (X (SYM \*))
                (S
                  (NP
                    (NP (DT The) (JJ minimum) (NN length))
                    (PP (IN for)
                      (NP (DT an) (NN argument))))
                  (VP (VBZ is)
                    (NP (CD 50) (NNS characters))))))))))
    (. .)))

