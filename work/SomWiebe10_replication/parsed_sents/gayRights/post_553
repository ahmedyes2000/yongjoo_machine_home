  (S
    (NP (PRP He))
    (VP (VBZ has)
      (ADVP (RB just) (RB as) (RB much) (RB right))
      (S
        (VP (TO to)
          (VP (VB do)
            (CC and)
            (VB believe)
            (SBAR
              (WHNP (WP what))
              (S
                (NP (PRP he))
                (VP (VBZ feels)
                  (SBAR
                    (S
                      (VP (VBZ is)
                        (ADJP (JJ right))
                        (SBAR (IN as)
                          (S
                            (NP (PRP you))
                            (VP (VBP do))))))))))))))
    (. .)))

