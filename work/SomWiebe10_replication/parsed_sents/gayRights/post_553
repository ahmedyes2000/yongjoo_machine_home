(ROOT
  (S
    (NP (PRP I))
    (VP (VBP wish)
      (SBAR
        (S
          (NP (EX there))
          (VP (VBD was)
            (ADJP (RBR more) (JJ civility))
            (PP (IN in)
              (NP (DT these) (NNS discussions)))))))
    (. .)))

