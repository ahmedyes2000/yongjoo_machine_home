(ROOT
  (S
    (S
      (UCP
        (NP (JJ =) (NN p))
        (CC But)
        (ADJP (JJ other)
          (PP (IN than)
            (NP (DT that))))))
    (, ,)
    (NP (PRP I))
    (VP (VBP keep)
      (SBAR
        (S
          (NP (PRP them))
          (VP (VBP secular)))))
    (. .)))

