(ROOT
  (S
    (S
      (INTJ (UH Yes))
      (NP (PRP you))
      (VP (MD can)))
    (, ,)
    (S
      (NP (NN atheism))
      (VP (VBZ is) (RB n't)
        (NP (DT a) (NN religion))))
    (, ,)
    (S
      (NP (PRP it))
      (VP (VBZ 's)
        (NP
          (NP (DT the) (NN rejection))
          (PP (IN of)
            (NP (NN religion))))))
    (. .)))

dep(can-3, Yes-1)
nsubj(can-3, you-2)
root(ROOT-0, can-3)
nsubj(religion-9, atheism-5)
cop(religion-9, is-6)
neg(religion-9, n't-7)
det(religion-9, a-8)
ccomp(can-3, religion-9)
nsubj(rejection-14, it-11)
cop(rejection-14, 's-12)
det(rejection-14, the-13)
ccomp(can-3, rejection-14)
prep_of(rejection-14, religion-16)

