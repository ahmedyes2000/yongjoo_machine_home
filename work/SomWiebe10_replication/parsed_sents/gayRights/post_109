        (VP (VB be)
          (PP (IN about)
            (NP (NN love))))))
    (, ,) (RB not)
    (S
      (ADVP (RB simply))
      (S
        (VP (TO to)
          (VP (VB have)
            (NP
              (NP (NNS children))
              (VP (VBG Having)
                (NP (NNS children)))))))
      (VP (VBZ has)
        (NP (NN nothing)
          (S
            (VP (TO to)
              (VP (VB do)
                (PP (IN with)
                  (S
                    (VP (VBG getting)
                      (ADJP (JJ married)))))))))))
    (. .)))

