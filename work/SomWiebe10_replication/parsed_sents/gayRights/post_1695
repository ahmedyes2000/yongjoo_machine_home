      (VP (VB impose)
        (NP (PRP$ your) (NN bigotry))
        (PP (IN as)
          (NP
            (NP (DT some) (JJ great) (JJ universal) ('' '') (NNP Christian))
            ('' '')
            (VP (VBG teaching) (, ,)
              (SBAR (IN because)
                (S
                  (NP (PRP it))
                  (VP (VBZ IS)
                    (NP (NNP NOT))))))))))
    (. .)))

