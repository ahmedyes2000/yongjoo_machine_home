(ROOT
  (S
    (VP (VBG oh)
      (SBAR
        (S
          (NP (PRP we))
          (VP (VBP 're)
            (ADJP (JJ sorry))))))
    (: ...)))

root(ROOT-0, oh-1)
nsubj(sorry-4, we-2)
cop(sorry-4, 're-3)
ccomp(oh-1, sorry-4)

(ROOT
  (FRAG
    (NP (CD 99.95) (NN %))
    (. .)))

num(%-2, 99.95-1)
root(ROOT-0, %-2)

(ROOT
  (S
    (NP (EX There))
    (, ,)
    (VP (VBZ does)
      (S
        (NP (DT that))
        (VP (VB make)
          (S
            (NP (PRP it))
            (ADJP (JJR better))))))
    (. ?)))

expl(does-3, There-1)
root(ROOT-0, does-3)
nsubj(make-5, that-4)
ccomp(does-3, make-5)
nsubj(better-7, it-6)
xcomp(make-5, better-7)

