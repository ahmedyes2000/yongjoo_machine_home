(ROOT
  (S
    (S
      (NP (PRP They))
      (VP (VBP say)
        (SBAR (IN that)
          (S
            (NP (PRP they))
            (VP (VBP want)
              (S
                (VP (TO to)
                  (VP (VB be)
                    (VP (VBN accepted))))))))))
    (CC but)
    (S
      (NP (PRP they))
      (VP (VBP are)
        (ADJP (JJ slanted)
          (PP (IN against)
            (S
              (VP (VBG being)
                (ADJP (JJ fair)
                  (PP (TO to)
                    (NP (DT the) (JJ straight) (NN community))))))))))
    (. .)))

