(ROOT
  (S
    (S
      (NP (DT That))
      (VP (VBZ is)
        (NP (DT an) (JJ interesting) (NNS hypothesis))))
    (: ...)
    (CC but)
    (S
      (S
        (NP (DT the) (NN evidence))
        (VP (VBZ does) (RB not)
          (VP (VB support)
            (NP (PRP$ your) (NNS claims)))))
      (, ,)
      (ADVP (RB rather))
      (S
        (NP (PRP it))
        (VP (VBZ shows)
          (SBAR (IN that)
            (S
              (NP
                (NP (NNS children))
                (PP (IN in)
                  (NP (JJ same) (NN sex) (NNS families))))
              (VP (VBP are)
                (ADVP (RB as) (RB well))
                (VP (VBN adjusted)
                  (PP (IN as)
                    (NP
                      (NP (DT those))
                      (PP (IN in)
                        (NP (JJ heterosexual) (NNS families))))))))))))
    (. .)))

nsubj(hypothesis-5, That-1)
cop(hypothesis-5, is-2)
det(hypothesis-5, an-3)
amod(hypothesis-5, interesting-4)
root(ROOT-0, hypothesis-5)
det(evidence-9, the-8)
nsubj(support-12, evidence-9)
aux(support-12, does-10)
neg(support-12, not-11)
conj_but(hypothesis-5, support-12)
poss(claims-14, your-13)
dobj(support-12, claims-14)
advmod(support-12, rather-16)
nsubj(shows-18, it-17)
dep(support-12, shows-18)
complm(adjusted-28, that-19)
nsubjpass(adjusted-28, children-20)
amod(families-24, same-22)
nn(families-24, sex-23)
prep_in(children-20, families-24)
auxpass(adjusted-28, are-25)
advmod(well-27, as-26)
mwe(well-27, as-26)
advmod(adjusted-28, well-27)
ccomp(shows-18, adjusted-28)
prep_as(adjusted-28, those-30)
amod(families-33, heterosexual-32)
prep_in(those-30, families-33)

