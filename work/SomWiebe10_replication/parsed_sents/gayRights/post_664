(ROOT
  (S
    (NP (NNP Homosexuality))
    (VP (VBZ is) (RB not)
      (NP (DT a) (NN sin)))
    (. .)))

nsubj(sin-5, Homosexuality-1)
cop(sin-5, is-2)
neg(sin-5, not-3)
det(sin-5, a-4)
root(ROOT-0, sin-5)

(ROOT
  (S
    (NP (NNP Sodomy))
    (VP (VBZ is)
      (NP (DT a) (NN sin))
      (, ,)
      (PP (DT both) (IN within)
        (CC and)
        (IN without)
        (NP (NN marriage))))
    (. .)))

nsubj(sin-4, Sodomy-1)
cop(sin-4, is-2)
det(sin-4, a-3)
root(ROOT-0, sin-4)
conj_and(sin-4, sin-4')
preconj(sin-4, both-6)
prep_within(sin-4, marriage-10)
prep_without(sin-4', marriage-10)

