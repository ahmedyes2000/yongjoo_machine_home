  (S
    (INTJ (UH No))
    (, ,)
    (NP (PRP It))
    (VP (VBZ 's) (RB not))
    (. .)))

dep('s-4, No-1)
nsubj('s-4, It-3)
root(ROOT-0, 's-4)
neg('s-4, not-5)

(ROOT
  (S
    (NP (NNS Gays))
    (VP
      (VP (MD can)
        (VP (VB be)
          (NP (NNP Christian))))
      (, ,)
      (CC and)
      (VP (MD can)
        (VP (VB be)
