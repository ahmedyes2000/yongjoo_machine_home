(ROOT
  (SINV
    (S
      (NP (PRP I))
      (VP (VBP say)))
    (, ,)
    (VP (VBP keep)
      (NP (JJ gay) (NN marriage)))
    (NP
      (NP (NNP OUT))
      (PP (IN of)
        (NP (NN religion))))
    (. .)))

