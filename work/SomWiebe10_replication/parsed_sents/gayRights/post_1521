(ROOT
  (SQ (MD Should) (RB n't)
    (NP (JJ gay) (NNS people))
    (VP (VB be)
      (VP (VBN allowed)
        (S
          (VP (TO to)
            (VP (VB be)
              (ADJP (JJ happy))
              (PRN (-LRB- -LRB-)
                (CC or)
                (FRAG
                  (X (SYM gay))
                  (: ...)
                  (NP (NN lol)))
                (-RRB- -RRB-)))))
        (ADVP (RB as) (RB well))))
    (. .)))

aux(allowed-6, Should-1)
neg(allowed-6, n't-2)
amod(people-4, gay-3)
nsubjpass(allowed-6, people-4)
auxpass(allowed-6, be-5)
root(ROOT-0, allowed-6)
aux(happy-9, to-7)
