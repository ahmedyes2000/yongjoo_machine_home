(ROOT
  (S
    (S
      (VP
        (ADVP (RB Merely))
        (VBG dismissing)
        (NP (DT the) (JJ other) (NN side))))
    (VP (VBZ is) (RB not)
      (NP (DT an) (NN argument)))
    (. .)))

advmod(dismissing-2, Merely-1)
csubj(argument-9, dismissing-2)
det(side-5, the-3)
amod(side-5, other-4)
dobj(dismissing-2, side-5)
cop(argument-9, is-6)
neg(argument-9, not-7)
