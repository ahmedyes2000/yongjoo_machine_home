(ROOT
  (S
    (NP (DT This) (NN post))
    (VP (VBZ is)
      (ADJP (RB almost) (JJ insulting)
        (PP (IN in)
          (NP
            (NP
              (NP (PRP$ its) (JJ complete) (NN lack))
              (PP (IN of)
                (NP (NN evidence))))
            (CC or)
            (NP (RB even) (DT a)
              (ADJP (VBN reasoned))
              (NN argument))))))
    (. .)))

det(post-2, This-1)
nsubj(insulting-5, post-2)
cop(insulting-5, is-3)
advmod(insulting-5, almost-4)
root(ROOT-0, insulting-5)
poss(lack-9, its-7)
