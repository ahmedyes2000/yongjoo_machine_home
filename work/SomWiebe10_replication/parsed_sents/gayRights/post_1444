    (CC but)
    (S
      (NP (PRP they))
      (VP
        (VP (MD can)
          (VP (VB care)
            (PP
              (PP (IN for)
                (NP
                  (NP (DT the) (JJ unwanted) (NNS children))
                  (PP (IN of)
                    (NP (NNS heterosexuals)))))
              (CC and)
              (PP (IN in)
                (NP (DT some) (NNS cases))))))
        (PRN (-LRB- -LRB-) (VB lesbians) (-RRB- -RRB-))
        (VP (VB use)
          (NP (JJ artificial) (NN impregnation) (NNS techniques))
