(ROOT
  (S
    (VP (VBG Responding)
      (ADVP (RB once) (RB again))
      (SBAR (IN that)
        (S
          (NP (EX there))
          (VP (VBZ 's)
            (NP
              (NP (DT no) (JJ such) (NN thing))
              (PP (IN as)
                (NP (NNP Judeo-Christian))))))))
    (. .)))

root(ROOT-0, Responding-1)
advmod(again-3, once-2)
advmod(Responding-1, again-3)
complm('s-6, that-4)
expl('s-6, there-5)
ccomp(Responding-1, 's-6)
det(thing-9, no-7)
amod(thing-9, such-8)
nsubj('s-6, thing-9)
prep_as(thing-9, Judeo-Christian-11)

(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ 's)
      (NP (RB just) (JJ Christian)))
    (. .)))

