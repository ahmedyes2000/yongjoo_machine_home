(ROOT
  (S
    (NP
      (NP (NN something))
      (PP (IN like)
        (NP (DT that))))
    (VP (VBZ has)
      (S
        (VP (TO to)
          (VP (VB be)
            (VP (VBN KIND)
              (PP (IN OF)
                (ADJP (JJ true))))))))
    (. .)))

nsubj(has-4, something-1)
xsubj(KIND-7, something-1)
prep_like(something-1, that-3)
root(ROOT-0, has-4)
aux(KIND-7, to-5)
auxpass(KIND-7, be-6)
xcomp(has-4, KIND-7)
prep_of(KIND-7, true-9)

