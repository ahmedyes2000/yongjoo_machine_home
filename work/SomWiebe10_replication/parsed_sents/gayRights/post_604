(ROOT
  (S
    (NP (PRP I))
    (VP (VBP have)
      (NP
        (NP (DT no) (JJ earthly) (NN understanding))
        (PP (IN as) (TO to)
          (SBAR
            (WHADVP (WRB why))
            (S
              (NP (PRP they))
              (VP (VBP act)
                (SBAR (IN as)
                  (S
                    (NP (PRP they))
                    (VP (VBP do))))))))))
    (. .)))

nsubj(have-2, I-1)
root(ROOT-0, have-2)
det(understanding-5, no-3)
amod(understanding-5, earthly-4)
dobj(have-2, understanding-5)
advmod(act-10, why-8)
nsubj(act-10, they-9)
prepc_as_to(understanding-5, act-10)
mark(do-13, as-11)
nsubj(do-13, they-12)
advcl(act-10, do-13)

(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ 's)
      (ADVP (RB hard) (RB enough) (RB just))
      (VP (VBG figuring)
        (NP (PRP us))
        (PRT (RP out))))
    (. .)))

nsubj(figuring-6, It-1)
aux(figuring-6, 's-2)
advmod(just-5, hard-3)
advmod(just-5, enough-4)
advmod(figuring-6, just-5)
root(ROOT-0, figuring-6)
dobj(figuring-6, us-7)
prt(figuring-6, out-8)

