(ROOT
  (S
    (NP (DT That))
    (VP (VBZ 's)
      (ADJP (JJ funny)))
    (. .)))

nsubj(funny-3, That-1)
cop(funny-3, 's-2)
root(ROOT-0, funny-3)

(ROOT
  (NP
    (NP (CD One))
    (PP (IN up)
      (NP
        (NP (NN vote))
        (PP (IN for)
          (NP (PRP you))
          (: ;))))
    (-RRB- -RRB-)))

root(ROOT-0, One-1)
prep_up(One-1, vote-3)
prep_for(vote-3, you-5)

