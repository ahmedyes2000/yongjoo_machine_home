(ROOT
  (S
    (NP (PRP I))
    (VP (VBP hope)
      (S
        (NP (PRP$ your) (NN situation)
          (CC and)
          (NNS others))
        (VP (VB be)
          (VP (VBN taken)
            (ADVP (RBR more) (RB seriously))
            (PP (IN with)
              (NP
                (NP (RB all) (DT the) (JJ other))
                (VP
                  (ADVP (RB so))
                  (VBN called)
                  (NP (JJ scientific) (NNS data)))))))))
    (. .)))

nsubj(hope-2, I-1)
root(ROOT-0, hope-2)
poss(situation-4, your-3)
nsubjpass(taken-8, situation-4)
conj_and(situation-4, others-6)
nsubjpass(taken-8, others-6)
auxpass(taken-8, be-7)
ccomp(hope-2, taken-8)
advmod(seriously-10, more-9)
advmod(taken-8, seriously-10)
advmod(other-14, all-12)
det(other-14, the-13)
prep_with(taken-8, other-14)
advmod(called-16, so-15)
partmod(other-14, called-16)
amod(data-18, scientific-17)
