(ROOT
  (NP (NNP NO.)))

root(ROOT-0, NO.-1)

(ROOT
  (S
    (NP (DT The) (NNP Bible))
    (ADVP (RB clearly))
    (VP (VBZ states)
      (SBAR (IN that)
        (S
          (NP (NN marriage))
          (VP (VBZ is)
            (NP
              (NP (DT a) (JJ sacred) (NN union))
              (PP (IN between)
                (NP (NN male)
                  (CC and)
                  (NN female))))))))
    (. .)))

det(Bible-2, The-1)
nsubj(states-4, Bible-2)
advmod(states-4, clearly-3)
root(ROOT-0, states-4)
complm(union-10, that-5)
nsubj(union-10, marriage-6)
cop(union-10, is-7)
det(union-10, a-8)
amod(union-10, sacred-9)
ccomp(states-4, union-10)
prep_between(union-10, male-12)
prep_between(union-10, female-14)
conj_and(male-12, female-14)

