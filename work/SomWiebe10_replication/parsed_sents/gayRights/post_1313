(ROOT
  (S
    (NP (PRP I))
    (VP (VBP do) (RB n't)
      (VP (VB want)
        (NP (PRP$ my) (NNS children))
        (S
          (VP (TO to)
            (VP (VB hang)
              (PRT (RP out))
              (PP (IN with)
                (NP
                  (NP (NN someone))
                  (SBAR
                    (WHNP (WP who))
                    (S
                      (VP (VBZ likes)
                        (NP (DT the) (JJ color) (NN red))))))))))))
    (. .)))

nsubj(want-4, I-1)
aux(want-4, do-2)
neg(want-4, n't-3)
root(ROOT-0, want-4)
poss(children-6, my-5)
dobj(want-4, children-6)
aux(hang-8, to-7)
xcomp(want-4, hang-8)
prt(hang-8, out-9)
prep_with(hang-8, someone-11)
nsubj(likes-13, someone-11)
rcmod(someone-11, likes-13)
det(red-16, the-14)
amod(red-16, color-15)
dobj(likes-13, red-16)

