(ROOT
  (S
    (SBAR (IN After)
      (S
        (NP (DT the) (NN baby))
        (VP (VBZ is)
          (VP (VBN born) (, ,)
            (PP (RB instead) (IN of)
              (NP
                (NP
                  (NP (DT the) (NN father) (POS 's))
                  (NN name))
                (VP (VBG being)
                  (VP (VBN used)))))))))
    (, ,)
    (NP
      (NP (DT the) (JJ other) (NN spouse) (POS 's))
      (NNS names))
    (VP (MD can)
      (VP (VB be)
        (VP (VBN stated))))
    (. .)))

