  (S
    (NP (DT Some) (JJ married) (JJ homosexual) (NNS women))
    (ADVP (RB actually))
    (VP (VBP do)
      (VP
        (VP (VB have)
          (NP
            (NP (NNS babies))
            (SBAR
              (S
                (PP (IN In)
                  (NP (DT this) (NN procedure)))
                (, ,)
                (NP
                  (NP (CD one) (NN woman) (POS 's))
                  (NN egg))
                (VP (VBZ is)
                  (VP (VBN implanted)
                    (PP (IN into)
                      (NP
                        (NP (DT the) (JJ other) (NN woman) (POS 's))
                        (NN body)))))))))
        (CC and)
        (ADVP (RB then))
        (VP (VB fertilize)
          (PP (IN with)
            (NP
              (NP (DT an) (JJ unknown) (NN donor) (POS 's))
              (NN sperm))))))
    (. .)))

