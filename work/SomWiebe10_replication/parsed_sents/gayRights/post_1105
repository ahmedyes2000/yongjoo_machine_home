(ROOT
  (S
    (NP (PRP I))
    (VP (MD would)
      (VP (VB have)
        (VP (VBN agreed)
          (PP (IN with)
            (NP (PRP you))))))
    (. .)))

nsubj(agreed-4, I-1)
aux(agreed-4, would-2)
aux(agreed-4, have-3)
root(ROOT-0, agreed-4)
prep_with(agreed-4, you-6)

(ROOT
  (S
    (VP (VB Forgive)
      (NP (PRP me))
      (PP (IN for)
        (S (RB not)
          (VP (VBG having)
            (VP (VBP read)
              (NP (PRP$ your) (JJ entire) (NN argument))
              (, ,)
              (ADVP (RB then)))))))
    (. .)))

root(ROOT-0, Forgive-1)
dobj(Forgive-1, me-2)
neg(read-6, not-4)
aux(read-6, having-5)
prepc_for(Forgive-1, read-6)
poss(argument-9, your-7)
amod(argument-9, entire-8)
dobj(read-6, argument-9)
advmod(read-6, then-11)

