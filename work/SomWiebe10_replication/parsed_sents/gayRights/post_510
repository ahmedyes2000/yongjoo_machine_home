(ROOT
  (S
    (VP (VB Yep) (, ,)
      (SBAR (IN like)
        (S
          (NP (PRP I))
          (VP (VBD said)))))
    (: ...)))

root(ROOT-0, Yep-1)
mark(said-5, like-3)
nsubj(said-5, I-4)
advcl(Yep-1, said-5)

(ROOT
  (S
    (NP (NNP Jesus))
    (VP (VBD uttered)
      (NP
        (NP (NNP NOT) (NNP ONE) (NNP WORD))
        (PP (IN about)
          (NP (NN homosexuality)))))
    (. .)))

nsubj(uttered-2, Jesus-1)
root(ROOT-0, uttered-2)
nn(WORD-5, NOT-3)
nn(WORD-5, ONE-4)
dobj(uttered-2, WORD-5)
prep_about(WORD-5, homosexuality-7)

