(ROOT
  (S
    (ADVP (RB Well))
    (NP (PRP I))
    (VP (VBP guess)
      (SBAR
        (S
          (NP (PRP you))
          (VP (VBP are)
            (ADJP (JJ right))))))
    (. .)))

advmod(guess-3, Well-1)
nsubj(guess-3, I-2)
root(ROOT-0, guess-3)
nsubj(right-6, you-4)
cop(right-6, are-5)
ccomp(guess-3, right-6)

(ROOT
  (S
    (SBAR (IN Because)
      (S
        (NP (PRP it))
        (VP (VBZ is)
          (ADJP (JJ impossible)
            (S
              (VP (TO to)
                (VP (VB argue)
                  (PP (IN with)
                    (NP (DT an) (JJ imaginary) (NN god))))))))))
    (VP (VBZ 's)
      (NP (NN word)))
    (. .)))

mark(impossible-4, Because-1)
nsubj(impossible-4, it-2)
cop(impossible-4, is-3)
csubj(word-12, impossible-4)
aux(argue-6, to-5)
xcomp(impossible-4, argue-6)
det(god-10, an-8)
amod(god-10, imaginary-9)
prep_with(argue-6, god-10)
cop(word-12, 's-11)
root(ROOT-0, word-12)

(ROOT
  (NP (NN lol)))

root(ROOT-0, lol-1)

