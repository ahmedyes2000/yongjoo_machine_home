(ROOT
  (S
    (S
      (NP (PRP He))
      (VP (VBD did)
        (SBAR
          (S
            (VP (VBP know)
              (SBAR
                (WHADVP (WRB where))
                (S
                  (NP (PRP$ my) (NN heart))
                  (VP (VBD was)))))))))
    (, ,)
    (CC and)
