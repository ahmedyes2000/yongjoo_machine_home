(ROOT
  (S
    (NP (PRP I))
    (VP (VBD found)
      (NP (PRP$ your) (NN response)
        (S
          (VP (TO to)
            (VP (VB be)
              (ADJP (JJ humble)
                (CC and)
                (JJ genuine)))))))
    (. .)))

