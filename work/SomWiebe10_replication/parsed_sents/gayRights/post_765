root(ROOT-0, found-2)
poss(response-4, your-3)
dobj(found-2, response-4)
aux(humble-7, to-5)
cop(humble-7, be-6)
infmod(response-4, humble-7)
infmod(response-4, genuine-9)
conj_and(humble-7, genuine-9)

