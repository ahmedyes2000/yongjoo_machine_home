(ROOT
  (S
    (ADVP (RB lol))
    (PRN (, ,)
      (S
        (NP (PRP I))
        (VP (VBP like)
          (NP (PRP$ your) (NN logic))))
      (, ,))
    (ADVP (RB now))
    (NP (PRP I))
    (VP (VBP wish)
      (SBAR
        (S
          (NP (PRP I))
          (VP (VBD had)
            (NP (DT a) (JJ gay) (NN brother))))))
    (. .)))

advmod(wish-10, lol-1)
nsubj(like-4, I-3)
parataxis(wish-10, like-4)
poss(logic-6, your-5)
dobj(like-4, logic-6)
