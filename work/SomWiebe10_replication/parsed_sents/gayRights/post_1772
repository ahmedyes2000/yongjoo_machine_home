(ROOT
  (S
    (NP (CD 2) (NNS parents))
    (, ,)
    (VP (VBP say)
      (NP (DT the) (NN research))
      (, ,)
      (ADVP (RB regardless)
        (PP (IN of)
          (NP (NN gender)))))
    (. .)))

num(parents-2, 2-1)
nsubj(say-4, parents-2)
root(ROOT-0, say-4)
det(research-6, the-5)
dobj(say-4, research-6)
prep_regardless_of(say-4, gender-10)

(ROOT
  (S
    (NP (DT That))
    (VP (VBZ 's)
      (NP
        (NP (DT the) (NN formula))
        (PP (IN for)
          (NP (JJ healthy) (NN upbringing)))))
    (. .)))

nsubj(formula-4, That-1)
cop(formula-4, 's-2)
det(formula-4, the-3)
root(ROOT-0, formula-4)
amod(upbringing-7, healthy-6)
prep_for(formula-4, upbringing-7)

