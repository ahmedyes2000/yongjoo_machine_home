(ROOT
  (S
    (NP (PRP I))
    (VP (VBP find)
      (NP
        (NP
          (NP (DT that) (NNS people))
          (SBAR
            (WHNP (WP who))
            (S
              (VP
                (VP (VB follow)
                  (NP (PRP$ their) (JJ own) (JJ instinctual) (NNS morals)))
                (, ,)
                (VP (VB free)
                  (PP (IN from)
                    (NP (JJ religious) (NN influence))))
                (, ,)
                (VP (VB treat)
                  (NP (NNS humans))
                  (PP (IN as)
                    (NP (NNS humans))))))))
        (, ,)
        (CONJP (RB not) (IN as))
        (NP (NNS creatures)
          (S
            (VP (TO to)
              (VP (VB be)
                (VP (VBN judged))))))))
    (. .)))

nsubj(find-2, I-1)
root(ROOT-0, find-2)
det(people-4, that-3)
dobj(find-2, people-4)
nsubj(follow-6, people-4)
rcmod(people-4, follow-6)
poss(morals-10, their-7)
