dobj(treat-17, humans-18)
prep_as(treat-17, humans-20)
dobj(find-2, creatures-24)
conj_negcc(people-4, creatures-24)
nsubj(follow-6, creatures-24)
aux(judged-27, to-25)
auxpass(judged-27, be-26)
infmod(creatures-24, judged-27)

