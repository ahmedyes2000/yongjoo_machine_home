  (S
    (NP (NNS People))
    (VP (VBP do) (RB n't)
      (VP (VB have)
        (NP (`` ``)
          (NP (NNS rights))
          ('' '')
          (PP (IN as)
            (NP
              (NP
                (NP (NNS members))
                (PP (IN of)
                  (NP (DT a) (NN group))))
              (, ,)
              (CONJP (CC but)
                (RB as))
              (NP
                (NP (NNS individuals))
                (PP (IN in)
                  (NP (NN society)))))))))
    (. .)))

