(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ 's)
      (VP (VBN called)
        (NP
          (NP (DT an) (NN analogy))
          (, ,)
          (NP (NN genius)))))
    (. .)))

nsubjpass(called-3, It-1)
auxpass(called-3, 's-2)
root(ROOT-0, called-3)
det(analogy-5, an-4)
dobj(called-3, analogy-5)
appos(analogy-5, genius-7)

(ROOT
  (S
    (NP (PRP He))
    (VP (VBZ 's)
      (VP (VBG saying)
        (SBAR (IN that)
          (S
            (NP
              (NP (VBG basing) (NNS things))
              (PP (IN on)
                (NP (NN tradition))))
            (VP (VBZ is)
              (NP (DT a) (JJ bad) (NN idea)))))))
    (. .)))

nsubj(saying-3, He-1)
aux(saying-3, 's-2)
root(ROOT-0, saying-3)
complm(idea-12, that-4)
amod(things-6, basing-5)
nsubj(idea-12, things-6)
prep_on(things-6, tradition-8)
cop(idea-12, is-9)
det(idea-12, a-10)
amod(idea-12, bad-11)
ccomp(saying-3, idea-12)

