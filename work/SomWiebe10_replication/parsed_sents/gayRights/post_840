(ROOT
  (S
    (NP (NNP Heterosexuality))
    (VP (VBZ is) (RB n't)
      (ADJP (JJ sinful)
        (PP (IN in)
          (NP (NN marriage)))))
    (. .)))

nsubj(sinful-4, Heterosexuality-1)
cop(sinful-4, is-2)
neg(sinful-4, n't-3)
root(ROOT-0, sinful-4)
prep_in(sinful-4, marriage-6)

(ROOT
  (SQ (VBP Do)
    (NP (PRP you))
    (VP (VB believe)
      (SBAR
        (S
          (NP (JJ homosexual) (NNS marriages))
          (VP (VBP are)
            (NP (NNP Scriptural))))))
    (. ?)))

