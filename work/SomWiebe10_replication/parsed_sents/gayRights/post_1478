(ROOT
  (S
    (VP (VB Do)
      (NP (PRP it))
      (SBAR (IN till)
        (S
          (NP (PRP$ your) (NN finger))
          (VP (VBZ falls)
            (PRT (RP off))
            (PP (IN for)
              (NP
                (NP (DT all) (NN I) (NN care))
                (: ;)))
            (-RRB- -RRB-)))))))

root(ROOT-0, Do-1)
dobj(Do-1, it-2)
mark(falls-6, till-3)
poss(finger-5, your-4)
nsubj(falls-6, finger-5)
advcl(Do-1, falls-6)
prt(falls-6, off-7)
det(care-11, all-9)
nn(care-11, I-10)
prep_for(falls-6, care-11)

