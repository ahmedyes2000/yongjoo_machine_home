(ROOT
  (S
    (PP (IN Like))
    (NP (PRP you))
    (ADVP (RB ever))
    (VP (VBP try)
      (S
        (VP (TO to)
          (VP (VB have)
            (NP (DT a) (JJ logical) (NN debate))))))
    (: ...)))

prep(try-4, Like-1)
nsubj(try-4, you-2)
xsubj(have-6, you-2)
advmod(try-4, ever-3)
root(ROOT-0, try-4)
aux(have-6, to-5)
xcomp(try-4, have-6)
det(debate-9, a-7)
amod(debate-9, logical-8)
dobj(have-6, debate-9)

