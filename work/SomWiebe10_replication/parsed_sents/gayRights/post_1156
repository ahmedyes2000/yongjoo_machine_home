(ROOT
  (S
    (S
      (NP (PRP I))
      (VP (VBP 'm)
        (VP (VBG hoping)
          (SBAR
            (S
              (NP (EX there))
              (VP (VBZ 's)
                (ADJP
                  (ADJP (JJR more))
                  (PP (TO to)
                    (NP (PRP it))))
                (PP (IN than)
                  (NP (DT that)))))))))
    (, ,)
    (CC but)
    (S
      (NP (PRP I))
      (VP (VBP hope)
        (NP
          (NP (DT that) (NN hope))
          (PP (IN about)
            (NP (DT all) (NNS people))))))
    (. .)))

