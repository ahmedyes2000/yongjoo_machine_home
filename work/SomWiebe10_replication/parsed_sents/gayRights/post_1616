(ROOT
  (S
    (NP (NNS Atheists))
    (VP (MD should) (RB not)
      (VP (VB marry) (, ,)
        (SBAR (IN if)
          (S
            (NP (PRP they))
            (VP (VBP are)
              (VP (VBG rejecting)
                (NP (NN religion))))))))
    (. .)))

