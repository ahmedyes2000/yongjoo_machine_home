(ROOT
  (S
    (SBAR
      (WHNP (WP What))
      (S
        (NP (PRP I))
        (VP (VBP am)
          (VP (VBG trying)
            (S
              (VP (TO to)
                (VP (VB say))))))))
    (VP (VBZ is)
      (SBAR
        (S
          (NP (EX there))
          (VP (MD should) (RB not)
            (VP (VB be)
              (NP (JJ non-religous) (NN marriage)))))))
    (. .)))

dobj(trying-4, What-1)
nsubj(trying-4, I-2)
xsubj(say-6, I-2)
aux(trying-4, am-3)
