(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (DT this))
        (VP (VBD was)
          (VP (VBN followed)))))
    (, ,)
    (NP (NNP THEN) (PRP you))
    (VP (MD could) (RB n't)
      (VP (VB have)
        (NP (DT a) (JJ non-religious) (NN marriage))))
    (. .)))

mark(followed-4, If-1)
nsubjpass(followed-4, this-2)
auxpass(followed-4, was-3)
advcl(have-10, followed-4)
nsubj(have-10, THEN-6)
dep(THEN-6, you-7)
aux(have-10, could-8)
neg(have-10, n't-9)
root(ROOT-0, have-10)
det(marriage-13, a-11)
