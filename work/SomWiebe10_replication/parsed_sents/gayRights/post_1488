prep_from(piece-2, machonazi-6)

(ROOT
  (S
    (NP (DT That))
    (VP (VBZ is)
      (ADVP (RB now))
      (NP
        (NP (DT the) (NN counter))
        (PP (TO to)
          (NP (NNS feminazi)))))
    (: ...)))

nsubj(counter-5, That-1)
cop(counter-5, is-2)
advmod(counter-5, now-3)
det(counter-5, the-4)
root(ROOT-0, counter-5)
prep_to(counter-5, feminazi-7)

(ROOT
  (INTJ (UH .)))


(ROOT
  (FRAG
    (SBAR
      (WHNP (WP what))
      (S
        (NP (PRP you))
        (VP (VBP call)
          (NP (NNS women)))))
    (. !)))

