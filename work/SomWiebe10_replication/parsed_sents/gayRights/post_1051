  (S
    (NP (PRP It))
    (VP (VBZ seems)
      (SBAR (IN like)
        (S
          (NP (EX there))
          (VP (MD would)
            (VP (VB be)
              (NP (DT no) (NN reason)
                (S
                  (VP (TO to)
                    (VP (VB ban)
                      (NP (JJ gay) (NN marriage))
                      (SBAR (IN if)
                        (S
                          (NP (PRP it))
                          (VP (VBD was) (RB n't)
                            (PP (IN for)
                              (NP (NN religion)))))))))))))))
    (. .)))

