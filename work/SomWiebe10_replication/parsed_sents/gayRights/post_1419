    (NP (DT Neither))
    (VP (VBZ does)
      (NP (DT the) (NN ability)
        (S
          (VP (TO to)
            (VP (VB conceive)
              (NP (DT a) (NN child)))))))
    (. .)))

nsubj(does-3, Neither-2)
root(ROOT-0, does-3)
det(ability-5, the-4)
dobj(does-3, ability-5)
