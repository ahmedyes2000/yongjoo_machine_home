(ROOT
  (S (`` ``)
    (NP (DT The) (NN ability)
      (S
        (VP (TO to)
          (VP (VB fabricate)
            (NP (DT an) (NN aircraft))))))
    (VP (VBZ does) (RB not)
      (VP (VB evidence)
        (NP (DT an) (NN ability)
          (S
            (VP (TO to)
              (VP (VB raise)
                (CC and)
                (VB love)
                (NP (DT a) (NN child))))))))
    (. .)))

det(ability-3, The-2)
nsubj(evidence-10, ability-3)
aux(fabricate-5, to-4)
infmod(ability-3, fabricate-5)
det(aircraft-7, an-6)
dobj(fabricate-5, aircraft-7)
aux(evidence-10, does-8)
neg(evidence-10, not-9)
root(ROOT-0, evidence-10)
det(ability-12, an-11)
dobj(evidence-10, ability-12)
aux(raise-14, to-13)
infmod(ability-12, raise-14)
infmod(ability-12, love-16)
conj_and(raise-14, love-16)
det(child-18, a-17)
dobj(raise-14, child-18)

(ROOT
