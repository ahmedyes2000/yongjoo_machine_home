(ROOT
  (S
    (VP (VB Prominent)
      (NP (JJ faith-based) (NNS cases))
      (SBAR (IN that)
        (S
          (NP (NN marriage))
          (VP (VBZ is)
            (PP (IN between)
              (NP
                (NP (DT a) (NN man))
                (CC and)
                (NP (DT a) (NN woman))))))))))

root(ROOT-0, Prominent-1)
amod(cases-3, faith-based-2)
dobj(Prominent-1, cases-3)
complm(is-6, that-4)
nsubj(is-6, marriage-5)
ccomp(Prominent-1, is-6)
det(man-9, a-8)
prep_between(is-6, man-9)
det(woman-12, a-11)
prep_between(is-6, woman-12)
conj_and(man-9, woman-12)

