(ROOT
  (S
    (ADVP (RB Clearly))
    (NP (PRP you))
    (VP (VBP are) (RB not)
      (NP
        (ADJP (JJ sure)
          (PP (IN of)
            (NP
              (NP (NN anything))
              (: ;)
              (PP (IN in)
                (NP (NN particular)))
              (, ,))))
        (NN reality)))
    (. .)))

advmod(reality-12, Clearly-1)
nsubj(reality-12, you-2)
cop(reality-12, are-3)
neg(reality-12, not-4)
amod(reality-12, sure-5)
prep_of(sure-5, anything-7)
prep_in(anything-7, particular-10)
root(ROOT-0, reality-12)

(ROOT
  (S
    (NP (PRP I))
    (VP (MD will)
      (VP (VB continue)
        (S
          (VP (TO to)
            (VP (VB point)
              (PRT (RP out))
              (NP (PRP$ your) (NNS shortcomings)))))
        (SBAR (IN until)
          (S
            (NP (PRP you))
            (VP (VB reform))))))
    (. .)))

nsubj(continue-3, I-1)
xsubj(point-5, I-1)
aux(continue-3, will-2)
root(ROOT-0, continue-3)
aux(point-5, to-4)
xcomp(continue-3, point-5)
prt(point-5, out-6)
poss(shortcomings-8, your-7)
dobj(point-5, shortcomings-8)
mark(reform-11, until-9)
nsubj(reform-11, you-10)
advcl(continue-3, reform-11)

