(ROOT
  (S
    (NP
      (NP (NNS Christians))
      (CC and)
      (NP (NNPS Jews)))
    (VP (VBP say)
      (SBAR (IN that)
        (S
          (NP (PRP it))
          (VP (VBZ 's)
            (ADJP (JJ immoral))))))
    (. .)))

nsubj(say-4, Christians-1)
conj_and(Christians-1, Jews-3)
nsubj(say-4, Jews-3)
root(ROOT-0, say-4)
complm(immoral-8, that-5)
nsubj(immoral-8, it-6)
cop(immoral-8, 's-7)
ccomp(say-4, immoral-8)

