(ROOT
  (S
    (NP (FW i))
    (VP (VBP do) (RB n't)
      (VP (VB make)
        (NP (DT a) (NN distinction))
        (ADVP (RB there))))
    (. .)))

nsubj(make-4, i-1)
aux(make-4, do-2)
neg(make-4, n't-3)
root(ROOT-0, make-4)
det(distinction-6, a-5)
dobj(make-4, distinction-6)
advmod(make-4, there-7)

