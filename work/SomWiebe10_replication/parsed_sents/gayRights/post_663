(ROOT
  (S
    (S
      (NP (NNP Homosexuality))
      (VP (VBZ is)
        (ADVP (DT no) (RBR more))
        (NP
          (NP (DT a) (NN sin))
          (PP (IN than)
            (NP (NN heterosexuality))))))
    (: ...)
    (S
      (ADVP (RB however))
      (, ,)
      (NP
        (NP (DT any) (JJ sexual) (NN act) (NN outside))
        (PP (IN of)
          (NP (JJ lawful) (NN marriage))))
      (VP (VBZ is)
        (NP (DT a) (NN sin))))
    (. .)))

nsubj(sin-6, Homosexuality-1)
cop(sin-6, is-2)
dep(more-4, no-3)
advmod(sin-6, more-4)
det(sin-6, a-5)
root(ROOT-0, sin-6)
