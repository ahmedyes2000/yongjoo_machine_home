(ROOT
  (S
    (NP (PRP I))
    (VP (VBP 'm)
      (ADJP (JJ glad)
        (SBAR (IN that)
          (S
            (NP (NN sentiment))
            (VP (VBZ is)
              (ADJP
                (ADJP (JJ alive))
                (CC and)
                (ADJP
                  (ADVP (RB well)
                    (PP (IN within)
                      (NP (PRP you))))
                  (VBN Joe))))))))
    (. .)))

nsubj(glad-3, I-1)
cop(glad-3, 'm-2)
root(ROOT-0, glad-3)
complm(alive-7, that-4)
nsubjpass(alive-7, sentiment-5)
nsubjpass(Joe-12, sentiment-5)
auxpass(alive-7, is-6)
cop(alive-7, is-6)
ccomp(glad-3, alive-7)
advmod(Joe-12, well-9)
prep_within(well-9, you-11)
ccomp(glad-3, Joe-12)
conj_and(alive-7, Joe-12)

