(ROOT
  (S
    (SBAR (IN As)
      (S
        (NP (PRP I))
        (VP (VBP know))))
    (, ,)
    (NP (DT the) (NNP God))
    (ADVP (RB always))
    (VP (VB remind)
      (S
        (NP (PRP us))
        (VP (TO to)
          (VP
            (VP (VB live)
              (ADVP (RB happily)))
            (CC and)
            (VP (VB harmoniously)
              (PP (IN with)
                (NP
                  (NP (PRP$ our) (NNS spouses))
                  (CC and)
                  (NP (DT the) (NNS people))))
              (PP (IN around)
                (NP (PRP us))))))))
    (. .)))

