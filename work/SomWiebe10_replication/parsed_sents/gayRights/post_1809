(ROOT
  (S
    (ADVP (RB Therefore))
    (, ,)
    (NP (PRP it))
    (VP (VBZ is) (RB not)
      (NP (DT a) (NN sin))
      (SBAR (IN if)
        (S
          (NP (DT both) (JJ homosexual) (NN couple))
          (VP (MD can)
            (VP (VB live)
              (ADVP (RB harmoniously))
              (, ,)
              (ADVP (RB parallel)
                (PP (TO to)
                  (SBAR
                    (WHNP (WP what))
                    (S
                      (NP (NNP God))
                      (VP (VBZ has)
                        (VP (VBN reminded)
                          (NP (PRP us)))))))))))))
    (. .)))

