(ROOT
  (S
    (NP (PRP$ My) (JJ second) (NN sentence))
    (ADVP (RB above))
    (VP (MD should)
      (VP (VB read) (, ,) (`` ``)
        (S
          (SBAR (IN If)
            (S
              (NP (PRP one))
              (VP (VBZ wishes)
                (S
                  (VP (TO to)
                    (VP (VB PROVE)
                      (NP (DT that)))))))))))
    (: ...)))

poss(sentence-3, My-1)
amod(sentence-3, second-2)
nsubj(read-6, sentence-3)
advmod(read-6, above-4)
aux(read-6, should-5)
root(ROOT-0, read-6)
mark(wishes-11, If-9)
nsubj(wishes-11, one-10)
xsubj(PROVE-13, one-10)
dep(read-6, wishes-11)
aux(PROVE-13, to-12)
xcomp(wishes-11, PROVE-13)
dobj(PROVE-13, that-14)

