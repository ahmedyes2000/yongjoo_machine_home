(ROOT
  (S
    (ADVP (RB Maybe))
    (NP (PRP he))
    (VP (VBZ is)
      (ADJP (JJ incapable)
        (PP (IN of)
          (S
            (VP (VBG forming)
              (NP (DT a) (JJ monogamous) (NN relationship)))))))
    (. .)))

advmod(incapable-4, Maybe-1)
nsubj(incapable-4, he-2)
cop(incapable-4, is-3)
root(ROOT-0, incapable-4)
prepc_of(incapable-4, forming-6)
det(relationship-9, a-7)
amod(relationship-9, monogamous-8)
dobj(forming-6, relationship-9)

(ROOT
  (FRAG
    (WHNP (WDT What))
    (SBAR
      (X (RB ever))
      (S
        (NP (PRP he))
        (VP (VBD did)
          (SBAR
            (WHNP (WP what))
            (S
              (VP (VBD made)
                (S
                  (NP (PRP him))
                  (ADJP (JJ happy)))))))))
    (. .)))

dep(did-4, What-1)
dep(did-4, ever-2)
nsubj(did-4, he-3)
root(ROOT-0, did-4)
nsubj(made-6, what-5)
ccomp(did-4, made-6)
nsubj(happy-8, him-7)
xcomp(made-6, happy-8)

