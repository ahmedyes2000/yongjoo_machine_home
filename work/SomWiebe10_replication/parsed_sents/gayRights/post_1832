(ROOT
  (SBAR
    (WHNP (WDT that))
    (S
      (ADVP (RB only))
      (VP (VBZ says)
        (SBAR (IN whether)
          (S
            (NP (JJ gay) (NN sex))
            (VP (VBZ is)
              (ADJP (JJ usefull)
                (PP (IN for)
                  (NP
                    (NP (NN procreation) (NN nothing))
                    (PP (IN about)
                      (NP (PRP$ its) (NN morality)))))))))))))

nsubj(says-3, that-1)
advmod(says-3, only-2)
root(ROOT-0, says-3)
complm(usefull-8, whether-4)
amod(sex-6, gay-5)
nsubj(usefull-8, sex-6)
cop(usefull-8, is-7)
ccomp(says-3, usefull-8)
nn(nothing-11, procreation-10)
prep_for(usefull-8, nothing-11)
poss(morality-14, its-13)
prep_about(nothing-11, morality-14)

