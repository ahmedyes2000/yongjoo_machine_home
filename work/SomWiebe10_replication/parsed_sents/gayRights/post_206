(ROOT
  (S
    (NP
      (NP (DT The) (JJ only) (NN way))
      (SBAR
        (S
          (NP (DT the) (NN child))
          (VP (MD could)
            (VP (VB get)
              (ADJP (JJ confused)))))))
    (VP (VBZ is)
      (SBAR (IN if)
        (S
          (NP (DT the) (NN parent) (POS 's))
          (VP (VBD were) (RB not)
            (ADJP (JJ honest)
              (PP (IN with)
                (NP
                  (NP (DT the) (NN child))
                  (PP (IN about)
                    (NP (DT the) (NN situation))))))))))
    (: ...)))

det(way-3, The-1)
amod(way-3, only-2)
dobj(confused-8, way-3)
nsubj(is-9, way-3)
det(child-5, the-4)
nsubj(confused-8, child-5)
aux(confused-8, could-6)
dep(confused-8, get-7)
rcmod(way-3, confused-8)
root(ROOT-0, is-9)
mark(honest-16, if-10)
det(parent-12, the-11)
nsubj(honest-16, parent-12)
possessive(parent-12, 's-13)
cop(honest-16, were-14)
neg(honest-16, not-15)
advcl(is-9, honest-16)
det(child-19, the-18)
prep_with(honest-16, child-19)
det(situation-22, the-21)
prep_about(child-19, situation-22)

