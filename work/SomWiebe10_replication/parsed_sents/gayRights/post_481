(ROOT
  (S
    (NP (DT The) (JJ religious) (NN right))
    (VP (VBZ believes)
      (SBAR (IN that)
        (S
          (NP (NN someone))
          (VP (MD can)
            (VP
              (VP (VB be)
                (VP (VBN forced)))
              (, ,)
              (CC or)
              (VP (VB will)
                (NP (PRP themselves))
                (, ,)
                (PP (IN into)
                  (NP
                    (NP (DT an) (NN end))
                    (PP (IN of)
                      (NP (DT the) (NN spectrum)))))))))))
    (. .)))

