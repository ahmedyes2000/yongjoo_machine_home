(ROOT
  (S
    (PP
      (PP (IN from)
        (NP (DT the) (NNP Bible)))
      (, ,)
      (CC or)
      (PP (IN from)
        (NP (JJ scientific) (NNS studies))))
    (, ,)
    (S
      (VP (VBG showing)
        (SBAR (IN that)
          (S
            (NP (DT the) (NNP devil))
            (VP (VBZ is)
              (ADVP (RB behind)))))))
    (VP (VBP homosexuality))
    (. .)))

det(Bible-3, the-2)
prep_from(homosexuality-16, Bible-3)
amod(studies-8, scientific-7)
conj_or(Bible-3, studies-8)
prep_from(homosexuality-16, studies-8)
csubj(homosexuality-16, showing-10)
complm(is-14, that-11)
det(devil-13, the-12)
nsubj(is-14, devil-13)
ccomp(showing-10, is-14)
advmod(is-14, behind-15)
root(ROOT-0, homosexuality-16)

