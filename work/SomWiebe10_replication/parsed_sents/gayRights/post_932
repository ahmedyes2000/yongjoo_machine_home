(ROOT
  (S
    (S
      (NP (PRP$ Our) (JJ founding) (NNS fathers))
      (VP (VBD did) (RB n't)
        (VP (VB think)
          (SBAR
            (S
              (NP (NNS women))
              (VP (MD should)
                (VP (VB vote))))))))
    (CC nor)
    (S
      (NP (JJ interratial) (NN marriage))
      (VP (MD should)
        (VP (VB be)
          (VP (VBN allowed)))))
    (. .)))

poss(fathers-3, Our-1)
amod(fathers-3, founding-2)
nsubj(think-6, fathers-3)
aux(think-6, did-4)
neg(think-6, n't-5)
root(ROOT-0, think-6)
nsubj(vote-9, women-7)
aux(vote-9, should-8)
ccomp(think-6, vote-9)
amod(marriage-12, interratial-11)
nsubjpass(allowed-15, marriage-12)
aux(allowed-15, should-13)
auxpass(allowed-15, be-14)
conj_nor(think-6, allowed-15)

