(ROOT
  (S
    (NP (PRP You))
    (VP (VBP need)
      (S
        (VP (TO to)
          (VP (VB add)
            (NP (DT the) (NN clause))
            (: :)
            (S (`` ``)
              (ADJP
                (ADJP (RB as) (JJ long))
                (SBAR (IN as)
                  (S
                    (NP (PRP it))
                    (VP (VBZ does)
                      (NP (DT no) (JJ forseeable) (NN harm))
                      (PP (TO to)
                        (NP (NNS others)))))))
              ('' ''))))))
    (. .)))

