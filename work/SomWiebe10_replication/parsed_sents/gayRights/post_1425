        (, ,)
        (NP
          (NP (RB second) (RB only))
          (PP (TO to)
            (NP (NN humility))))))
    (. .)))

advmod(attribute-7, However-1)
nsubj(attribute-7, patience-3)
cop(attribute-7, is-4)
poss(attribute-7, my-5)
amod(attribute-7, greatest-6)
root(ROOT-0, attribute-7)
