(ROOT
  (S
    (NP (PRP I))
    (VP (VBP 'm)
      (ADJP (JJ ready) (RB now)
        (S
          (VP (TO to)
            (VP (VB layout)
              (NP (PRP$ my) (NN case))
              (PP (IN against)
                (NP (NN adoption))))))))
    (. .)))

nsubj(ready-3, I-1)
cop(ready-3, 'm-2)
root(ROOT-0, ready-3)
advmod(ready-3, now-4)
aux(layout-6, to-5)
xcomp(ready-3, layout-6)
poss(case-8, my-7)
dobj(layout-6, case-8)
prep_against(layout-6, adoption-10)

(ROOT
  (S
    (ADVP (RB However))
    (, ,)
    (NP (NN patience))
