(ROOT
  (S
    (NP
      (NP (DT All))
      (PP (IN of)
        (NP
          (NP (DT the) (NNS arguments))
          (VP (VBG negating)
            (NP (DT this) (NN question))))))
    (VP (VBP are)
      (VP (VBG comparing)
        (PP (IN against)
          (NP
            (NP (DT the) (NNS dysfunctions))
            (VP (VBG occurring)
              (PP (IN in)
                (NP (JJR worse) (NN case) (NNS marriages))))))))
    (. .)))

