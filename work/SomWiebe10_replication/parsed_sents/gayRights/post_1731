(ROOT
  (S ('' '')
    (S
      (FRAG (: ...)
        (ADVP (RB bases))
        (NP
          (NP (PRP it))
          (NP
            (NP (PRP 's))
            (NNS laws))))
      (NP (JJ off))
      (VP
        (ADVP (IN of))
        (VBP rationality)))
    (CC and)
    (S
      (NP (RB not))
      (VP (VBZ religion)))
    (. .)))

