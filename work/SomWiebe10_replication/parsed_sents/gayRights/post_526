        (SBAR
          (WHPP (IN in)
            (WHNP (WDT which)))
          (S
            (NP (NNS scientists))
            (VP (VBP have)
              (VP (VBN found)
                (NP
                  (NP (DT the) (JJ gay) (NN gene))
                  (PP (IN in)
                    (NP (NN fruit) (NNS flies))))))))))
    (. .)))

