  (S
    (VP (VBP Are)
      (NP
        (NP
          (NP (PRP you))
          (VP (VBG going)
            (S
              (VP (TO to)
                (VP (VB leave)
                  (NP (PRP me))
                  (ADVP (RB alone)))))))
        (CC or)
        (SBAR
          (WHNP (WP what)))))
    (. ?)))

cop(you-2, Are-1)
root(ROOT-0, you-2)
xsubj(leave-5, you-2)
partmod(you-2, going-3)
aux(leave-5, to-4)
xcomp(going-3, leave-5)
