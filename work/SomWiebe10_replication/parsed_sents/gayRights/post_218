(ROOT
  (S
    (S
      (NP (PRP$ Its))
      (VP (VBP clear)
        (SBAR (IN that)
          (S
            (NP (PRP you))
            (VP (VBP have)
              (NP
                (NP (DT no) (NN research))
                (SBAR
                  (WHNP (WDT that))
                  (S
                    (VP (VBG supporting)
                      (NP (PRP$ your) (NN argument)))))))))))
    (CC but)
    (S
      (NP (PRP you))
      (VP (VBP try)
        (S
          (VP (TO to)
            (VP (VB spin)
              (NP (JJ other) (NN research))
              (S
                (VP (TO to)
                  (VP (VB help)
                    (NP (PRP you))
                    (PRT (RP out))))))))))
    (. .)))

nsubj(clear-2, Its-1)
root(ROOT-0, clear-2)
complm(have-5, that-3)
nsubj(have-5, you-4)
ccomp(clear-2, have-5)
det(research-7, no-6)
dobj(have-5, research-7)
nsubj(supporting-9, research-7)
rcmod(research-7, supporting-9)
poss(argument-11, your-10)
dobj(supporting-9, argument-11)
nsubj(try-14, you-13)
xsubj(spin-16, you-13)
conj_but(clear-2, try-14)
aux(spin-16, to-15)
xcomp(try-14, spin-16)
amod(research-18, other-17)
dobj(spin-16, research-18)
aux(help-20, to-19)
xcomp(spin-16, help-20)
dobj(help-20, you-21)
prt(help-20, out-22)

