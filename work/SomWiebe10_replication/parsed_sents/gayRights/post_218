(ROOT
  (S
    (NP (PRP I))
    (VP (VBP think)
      (SBAR
        (S
          (NP (PRP you))
          (VP (VBP 're)
            (VP (VBG making)
              (NP (DT the) (JJ wrong) (NNS comparisons)))))))
    (. .)))

nsubj(think-2, I-1)
root(ROOT-0, think-2)
nsubj(making-5, you-3)
aux(making-5, 're-4)
ccomp(think-2, making-5)
det(comparisons-8, the-6)
amod(comparisons-8, wrong-7)
dobj(making-5, comparisons-8)

(ROOT
  (S
