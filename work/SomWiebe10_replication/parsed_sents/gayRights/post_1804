(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is)
      (PP (IN in)
        (NP (DT a) (JJ Biblical) (NN context))))
    (. .)))

nsubj(is-2, It-1)
root(ROOT-0, is-2)
det(context-6, a-4)
amod(context-6, Biblical-5)
prep_in(is-2, context-6)

(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is) (RB n't)
      (ADJP (JJ wrong)
        (PP (IN from)
          (NP
            (NP (DT the) (NN viewpoint))
            (PP (IN of)
              (NP (DT a) (JJ rationally) (NN thinking) (NN person)))))))
    (. .)))

nsubj(wrong-4, It-1)
cop(wrong-4, is-2)
neg(wrong-4, n't-3)
root(ROOT-0, wrong-4)
det(viewpoint-7, the-6)
prep_from(wrong-4, viewpoint-7)
det(person-12, a-9)
amod(person-12, rationally-10)
nn(person-12, thinking-11)
prep_of(viewpoint-7, person-12)

