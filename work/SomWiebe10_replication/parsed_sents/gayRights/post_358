(ROOT
  (S
    (INTJ (UH Well))
    (VP (VB thank)
      (NP (PRP you))
      (PP (IN for)
        (NP (DT the)
          (ADJP (JJ short)
            (CC but)
            (JJ kind))
          (NN comment))))
    (. .)))

dep(thank-2, Well-1)
root(ROOT-0, thank-2)
dobj(thank-2, you-3)
det(comment-9, the-5)
amod(comment-9, short-6)
conj_but(short-6, kind-8)
amod(comment-9, kind-8)
prep_for(thank-2, comment-9)

(ROOT
  (S
    (NP (PRP I))
    (VP (VBP do)
      (VP (VB appreciate)
        (NP (DT the) (NN support))))
    (. !)))

nsubj(appreciate-3, I-1)
aux(appreciate-3, do-2)
root(ROOT-0, appreciate-3)
det(support-5, the-4)
dobj(appreciate-3, support-5)

