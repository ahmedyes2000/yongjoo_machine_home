(ROOT
  (S
    (NP (PRP$ My) (NN GOD))
    (, ,)
    (NP (PRP you))
    (VP (VBP ARE)
      (NP (DT A) (NNP MENTAL) (NNP MIDGET)))
    (. !)))

poss(GOD-2, My-1)
nsubj(MIDGET-8, GOD-2)
nsubj(MIDGET-8, you-4)
cop(MIDGET-8, ARE-5)
det(MIDGET-8, A-6)
nn(MIDGET-8, MENTAL-7)
root(ROOT-0, MIDGET-8)

(ROOT
  (FRAG
    (ADJP (JJ laddie) (, ,) (JJ laidie))
    (, ,)
    (NP
      (NP (NN guy))
      (, ,)
      (NP (NNP LOL))
      (. !))))

amod(laidie-3, laddie-1)
dep(guy-5, laidie-3)
root(ROOT-0, guy-5)
appos(guy-5, LOL-7)

(ROOT
  (SBARQ
    (WHNP (WDT What)
      (NP
        (NP (DT a) (JJ demented) (JJ little) (NN creature))
        (NP (PRP you))))
    (SQ
      (VP (VBP are)))
    (. .)))

det(creature-5, What-1)
det(creature-5, a-2)
amod(creature-5, demented-3)
amod(creature-5, little-4)
nsubj(are-7, creature-5)
dep(creature-5, you-6)
root(ROOT-0, are-7)

