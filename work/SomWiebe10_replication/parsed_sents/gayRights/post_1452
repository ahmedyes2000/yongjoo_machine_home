(ROOT
  (S
    (NP (DT That) (NN kid))
    (VP (VBZ is)
      (ADJP (RB so) (JJ nuts)
        (SBAR
          (S
            (NP (PRP it))
            (VP (VBZ makes)
              (NP (PRP$ your) (NN head) (NN spin)))))))
    (. !)))

det(kid-2, That-1)
nsubj(nuts-5, kid-2)
cop(nuts-5, is-3)
advmod(nuts-5, so-4)
root(ROOT-0, nuts-5)
nsubj(makes-7, it-6)
dep(nuts-5, makes-7)
poss(spin-10, your-8)
nn(spin-10, head-9)
dobj(makes-7, spin-10)

(ROOT
  (NP (NNP LMAO) (: ...)))

root(ROOT-0, LMAO-1)

(ROOT
  (NP (NNS BLANKETS) (. !)))

root(ROOT-0, BLANKETS-1)

