(ROOT
  (S
    (NP
      (NP (PRP Me))
      (VP (VBG saying)
        (SBAR (IN that)
          (S
            (NP (PRP you))
            (VP (VBP 're)
              (NP (DT a) (NN chore)))))))
    (VP (VBD was)
      (ADVP (RB never))
      (NP (DT an) (NN argument)))
    (. .)))

