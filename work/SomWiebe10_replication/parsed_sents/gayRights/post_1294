(ROOT
  (S
    (SBAR
      (WHNP (WP What))
      (S
        (VP (VBZ is)
          (ADJP (JJ relevant)))))
    (VP (VBZ is)
      (NP
        (NP (DT the) (NN fact))
        (SBAR (IN that)
          (S
            (NP (PRP we))
            (VP (VBP live)
              (PP (IN in)
                (NP
                  (NP (DT a) (NN country))
                  (PP (IN of)
                    (NP (JJ religious) (NN freedom)))))))))
      (: ...)
      (SBAR
        (WHADVP (WRB where))
        (S
          (NP (NN religion))
          (VP (VBZ has)
            (NP
              (NP (DT the) (NN freedom))
              (PP (TO to)
                (NP (NNS oppress))))))))
    (. .)))

