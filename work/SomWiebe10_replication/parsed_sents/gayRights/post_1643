(ROOT
  (S (CC And)
    (NP (NN child) (NN birth))
    (VP (MD will)
      (VP (VB be)
        (VP (VB disturb)
          (SBAR (IN that)
            (S
              (VP (VBZ means)
                (SBAR
                  (S
                    (NP (PRP you))
                    (VP (VBP are)
                      (VP (VBG hurting)
                        (NP (PRP$ your) (NN countary))
                        (NP (PRP$ your) (NN economy))))))))))))
    (: ...)))

