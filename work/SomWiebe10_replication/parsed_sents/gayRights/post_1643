(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (NNP Man))
        (VP (VB marry)
          (NP (NN Man))
          (ADVP (RB then)))))
    (NP (WDT what))
    (VP (MD will)
      (VP (VB happen)
        (PP (TO to)
          (NP
            (NP (DT those) (NNPS Girls))
            (SBAR
              (WHNP (WP who))
              (S
                (VP (VBP are)
                  (ADVP (RB already))
                  (PP (IN in)
                    (NP (JJ large) (NNS numbers))))))))))
    (. .)))

