(ROOT
  (S
    (NP (NNS Stereotypes))
    (VP (VBP are)
      (NP (NNS statistics)))
    (. .)))

nsubj(statistics-3, Stereotypes-1)
cop(statistics-3, are-2)
root(ROOT-0, statistics-3)

(ROOT
  (S
    (NP (PRP You))
    (VP (VBZ wanna)
      (VP
        (VP (VB ignore)
          (NP (NNS stereotypes)))
        (, ,)
        (VP
          (ADVP (RB then))
          (VB go)
          (ADVP (RB ahead)))
        (CC and)
        (VP (VB ignore)
          (NP (NNS statistics))
