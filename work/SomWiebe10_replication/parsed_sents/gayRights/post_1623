    (CC but)
    (S
      (NP (FW i))
      (VP (VBP do) (RB n't)
        (VP (VB have)
          (NP
            (NP (DT any) (NN tolerance))
            (PP (IN for)
              (NP (PRP it)))))))
    (. .)))

