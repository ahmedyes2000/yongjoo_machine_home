  (S
    (S
      (NP (JJ Same) (NN sex) (NN marriage))
      (VP (VBZ does) (RB not)
        (VP (`` ``) (VB yield)
          (NP (DT a) (NN contradiction))
          ('' ''))))
    (PRN (-LRB- -LRB-)
      (SBAR
        (WHNP (WDT whatever))
        (S
          (NP (DT that))
          (VP (VBZ means))))
      (-RRB- -RRB-))
    (. .)))

