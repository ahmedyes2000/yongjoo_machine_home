(ROOT
  (S
    (VP (VB Show)
      (S
        (NP (PRP me))
        (NP (DT the) (NN study)))
      (SBAR (IN because)
        (S
          (NP (PRP I))
          (VP (VBP 've)
            (ADVP (RB never))
            (VP (VBN had)
              (NP (DT the) (NN chance))
              (S
                (VP (TO to)
                  (VP (VB see)
                    (NP (PRP it))))))))))
    (. .)))

root(ROOT-0, Show-1)
nsubj(study-4, me-2)
det(study-4, the-3)
xcomp(Show-1, study-4)
mark(had-9, because-5)
nsubj(had-9, I-6)
aux(had-9, 've-7)
neg(had-9, never-8)
advcl(Show-1, had-9)
det(chance-11, the-10)
dobj(had-9, chance-11)
aux(see-13, to-12)
xcomp(had-9, see-13)
dobj(see-13, it-14)

