(ROOT
  (S
    (NP (PRP I))
    (VP (VBP have)
      (VP (VBN used)
        (NP
          (NP (DT this) (JJ particular) (NNP Bible) (NN verse))
          (PP (IN in)
            (NP
              (QP (RB about) (CD 4))
              (NNS debates))))
        (ADVP (RB already))))
    (. .)))

nsubj(used-3, I-1)
aux(used-3, have-2)
root(ROOT-0, used-3)
det(verse-7, this-4)
amod(verse-7, particular-5)
nn(verse-7, Bible-6)
dobj(used-3, verse-7)
quantmod(4-10, about-9)
num(debates-11, 4-10)
prep_in(verse-7, debates-11)
advmod(used-3, already-12)

(ROOT
  (S
    (S
      (NP (NNP Levicticus) (CD 18:22) (NNP Thou))
      (VP (VBP shalt) (RB not)
        (VP (VB lie)
          (PP (IN with)
            (NP (NN mankind)))
          (, ,)
          (ADVP (RB as)
            (PP (IN with)
              (ADJP (JJ womankind)))))))
    (: ;)
    (S
      (NP (PRP it))
      (VP (VBZ is)
        (ADJP (JJ abomination))))
    (. .)))

nn(Thou-3, Levicticus-1)
num(Thou-3, 18:22-2)
nsubj(shalt-4, Thou-3)
root(ROOT-0, shalt-4)
neg(shalt-4, not-5)
dep(shalt-4, lie-6)
prep_with(lie-6, mankind-8)
advmod(lie-6, as-10)
prep_with(as-10, womankind-12)
nsubj(abomination-16, it-14)
cop(abomination-16, is-15)
parataxis(shalt-4, abomination-16)

