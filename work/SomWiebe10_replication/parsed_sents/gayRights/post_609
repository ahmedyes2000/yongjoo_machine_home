(ROOT
  (S
    (NP (PRP$ My) (JJ last) (NN sentence))
    (VP (VBD was)
      (VP (VBN meant)
        (PP (IN towards)
          (NP
            (NP (DT the) (JJ other) (NN side))
            (PP (IN of)
              (NP
                (NP (DT this) (NN argument))
                (, ,) (RB not)
                (NP (PRP you))))))))
    (. .)))

poss(sentence-3, My-1)
amod(sentence-3, last-2)
nsubjpass(meant-5, sentence-3)
auxpass(meant-5, was-4)
root(ROOT-0, meant-5)
det(side-9, the-7)
amod(side-9, other-8)
prep_towards(meant-5, side-9)
det(argument-12, this-11)
prep_of(side-9, argument-12)
neg(argument-12, not-14)
dep(argument-12, you-15)

