(ROOT
  (S (`` ``)
    (NP (JJ Gay) (NN adoption))
    (VP (VBZ is)
      (ADJP (JJ wrong)))
    ('' '') (. .)))

amod(adoption-3, Gay-2)
nsubj(wrong-5, adoption-3)
cop(wrong-5, is-4)
root(ROOT-0, wrong-5)

(ROOT
  (NP (NNP The) (NNP Daily) (NNP Free) (NNP Press) (. .)))

nn(Press-4, The-1)
nn(Press-4, Daily-2)
nn(Press-4, Free-3)
root(ROOT-0, Press-4)

(ROOT
  (NP
    (NP (NNP April) (CD 1st))
    (, ,)
    (NP (CD 2004))))

