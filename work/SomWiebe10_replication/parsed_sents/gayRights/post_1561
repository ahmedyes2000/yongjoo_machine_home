(ROOT
  (S
    (SBAR (IN Unless)
      (S
        (NP (PRP you))
        (VP (MD can)
          (VP (VB provide)
            (NP (JJ scientific) (NNS data))
            (PP (IN on)
              (NP (DT this)))))))
    (, ,)
    (NP (PRP you))
    (VP (VBP are)
      (ADVP (RB just))
      (VP (VBG making)
        (NP (NNS things))
        (PRT (RP up))))
    (. !)))

