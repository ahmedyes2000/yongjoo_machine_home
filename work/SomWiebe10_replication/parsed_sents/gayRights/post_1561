(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP (VBP provide)
          (NP
            (NP (DT a) (NN link))
            (PP (IN for)
              (NP (DT a) (NN reference)))))))
    (NP (DT that))
    (VP (MD would)
      (VP (VB be)
        (ADJP (JJ helpful)
          (PP (IN in)
            (NP
              (NP (PRP$ your) (NN debate))
              (SBAR
                (WHNP (WDT which))
                (S
                  (VP (MD should)
                    (VP (VB be)
                      (VP (VBN based)
                        (PP (IN on)
                          (NP (NNS facts)))
                        (PP (RB not)
                          (NP (PRP$ your) (NN opinion)))))))))))))
    (. !)))

mark(provide-3, If-1)
