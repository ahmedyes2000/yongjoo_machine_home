  (S
    (NP (NN None))
    (VP
      (VP (VBN taken))
      (, ,)
      (NP
        (NP (PRP$ your) (NN tactic))
        (PP (IN of)
          (S
            (VP (VBG ignoring)
              (NP
                (NP (DT those))
                (SBAR
                  (WHNP (WDT that))
                  (S
                    (VP (VBP offer)
                      (NP (JJ reasonable) (NNS counter-arguments))
                      (PP (RB rather)
                        (ADJP (JJ obvious)))
                      (PP (IN at)
                        (NP (DT this) (NN point))))))))))))
    (. .)))

nsubj(taken-2, None-1)
root(ROOT-0, taken-2)
poss(tactic-5, your-4)
dobj(taken-2, tactic-5)
prepc_of(tactic-5, ignoring-7)
dobj(ignoring-7, those-8)
nsubj(offer-10, those-8)
