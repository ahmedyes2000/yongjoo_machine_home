(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ 's)
      (NP
        (NP (DT a) (NN website))
        (SBAR
          (WHNP (WDT that))
          (S
            (VP (VBZ lists)
              (NP
                (NP (NNS churches))
                (SBAR
                  (WHNP (WDT that))
                  (S
                    (VP (VBP welcome)
                      (NP (JJ LGBT) (NNS people)))))))))))
    (. .)))

nsubj(website-4, It-1)
cop(website-4, 's-2)
det(website-4, a-3)
root(ROOT-0, website-4)
nsubj(lists-6, website-4)
rcmod(website-4, lists-6)
dobj(lists-6, churches-7)
nsubj(welcome-9, churches-7)
rcmod(churches-7, welcome-9)
amod(people-11, LGBT-10)
dobj(welcome-9, people-11)

