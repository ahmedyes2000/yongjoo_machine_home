(ROOT
  (S
    (NP
      (NP (DT The) (NNS lists))
      (PP (IN of)
        (SBAR
          (WHNP (WP who))
          (S
            (VP (VBZ goes)
              (PP (TO to)
                (NP (NN hell))))))))
    (VP (VBP are)
      (ADJP (RB somewhat) (JJ irrellevant)))
    (. .)))

det(lists-2, The-1)
nsubj(irrellevant-10, lists-2)
nsubj(goes-5, who-4)
prepc_of(lists-2, goes-5)
prep_to(goes-5, hell-7)
cop(irrellevant-10, are-8)
advmod(irrellevant-10, somewhat-9)
root(ROOT-0, irrellevant-10)

