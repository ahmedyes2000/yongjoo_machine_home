(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is)
      (VP (VBN called)
        (NP (JJ human) (NNS rights) (NNS people))))
    (. !)))

nsubjpass(called-3, It-1)
auxpass(called-3, is-2)
root(ROOT-0, called-3)
amod(people-6, human-4)
nn(people-6, rights-5)
dobj(called-3, people-6)

(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ has)
      (ADVP (RB already))
      (VP (VBN been)
        (VP (VBN proven)
          (PP (IN through)
            (NP (JJ tedious) (NNS studies)))
          (SBAR (IN that)
            (S
              (NP (NNS children))
              (VP (VBD adopted)
                (PP (IN into)
                  (NP (JJ same) (NN sex) (NNS families)))
                (S
                  (VP (VB turn)
                    (PRT (RP out))
                    (NP
                      (NP (RB just) (DT the) (JJ same))
                      (PP (IN as)
                        (NP (DT any) (JJ other) (NN kid))))))))))))
    (. .)))

