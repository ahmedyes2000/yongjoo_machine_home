det(kid-24, any-22)
amod(kid-24, other-23)
prep_as(same-20, kid-24)

(ROOT
  (S (IN So)
    (NP (PRP I))
    (VP (VBP have)
      (S
        (VP (TO to)
          (VP (VB say)
            (INTJ (UH yes))
            (PP (RB because) (TO to)
              (NP
                (NP (PRP me))
                (SBAR
                  (S
                    (NP (DT the) (NN answer))
                    (VP (VBZ is)
                      (ADJP (JJ obvious)))))))))))
    (. .)))

dep(have-3, So-1)
nsubj(have-3, I-2)
xsubj(say-5, I-2)
root(ROOT-0, have-3)
aux(say-5, to-4)
xcomp(have-3, say-5)
dep(say-5, yes-6)
advmod(say-5, because-7)
prep_to(say-5, me-9)
det(answer-11, the-10)
nsubj(obvious-13, answer-11)
cop(obvious-13, is-12)
rcmod(me-9, obvious-13)

