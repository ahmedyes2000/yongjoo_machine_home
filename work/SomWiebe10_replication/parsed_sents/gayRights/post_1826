(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP (VBP 're)
          (ADJP (JJ gay)))))
    (NP (PRP you))
    (VP (MD can)
      (VP (VB go)
        (VP (VB kill)
          (NP (PRP yourself)))))
    (. .)))

mark(gay-4, If-1)
nsubj(gay-4, you-2)
cop(gay-4, 're-3)
advcl(go-7, gay-4)
nsubj(go-7, you-5)
aux(go-7, can-6)
root(ROOT-0, go-7)
xcomp(go-7, kill-8)
dobj(kill-8, yourself-9)

(ROOT
  (SINV
    (S
      (NP (PRP I))
      (VP (MD 'll)
        (ADVP (RB even))
        (VP (VB make)
