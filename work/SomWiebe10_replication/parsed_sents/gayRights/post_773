(ROOT
  (S
    (NP (DT That))
    (VP (VBD was)
      (NP (DT a)
        (ADJP (JJ marvelous))
        (NN testimony)))
    (. .)))

nsubj(testimony-5, That-1)
cop(testimony-5, was-2)
det(testimony-5, a-3)
amod(testimony-5, marvelous-4)
root(ROOT-0, testimony-5)

(ROOT
  (S
    (NP (NNP God))
    (VP (VB bless)
      (NP (PRP you)))
    (. .)))

nsubj(bless-2, God-1)
root(ROOT-0, bless-2)
dobj(bless-2, you-3)

