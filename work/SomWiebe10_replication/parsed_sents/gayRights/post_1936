  (S
    (S
      (NP (NNP Levicticus) (CD 18:22) (NNP Thou))
      (VP (VBP shalt) (RB not)
        (VP (VB lie)
          (PP (IN with)
            (NP (NN mankind)))
          (, ,)
          (ADVP (RB as)
            (PP (IN with)
              (ADJP (JJ womankind)))))))
    (: ;)
    (S
      (NP (PRP it))
      (VP (VBZ is)
        (ADJP (JJ abomination))))
    (. .)))

