(ROOT
  (SBARQ
    (SBAR (RB Even) (IN if)
      (S
        (NP (NNP God))
        (VP (VBZ exists))))
    (, ,)
    (WHADVP (WRB why))
    (SQ (MD would)
      (NP (PRP he))
      (VP (VB give)
        (NP (DT a) (NN shit))
        (SBAR (IN if)
          (S
            (NP (CD two) (NNS men))
            (VP (VBD kissed)
              (NP (DT each) (JJ other)))))))
    (. ?)))

advmod(exists-4, Even-1)
dep(exists-4, if-2)
nsubj(exists-4, God-3)
dep(give-9, exists-4)
advmod(give-9, why-6)
aux(give-9, would-7)
nsubj(give-9, he-8)
root(ROOT-0, give-9)
det(shit-11, a-10)
dobj(give-9, shit-11)
mark(kissed-15, if-12)
num(men-14, two-13)
nsubj(kissed-15, men-14)
advcl(give-9, kissed-15)
det(other-17, each-16)
dobj(kissed-15, other-17)

