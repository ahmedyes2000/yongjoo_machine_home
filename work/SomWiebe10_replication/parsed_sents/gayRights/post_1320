(ROOT
  (S
    (S
      (NP (PRP I))
      (VP (VBP 'm) (RB not)
        (ADJP (JJ perfect))))
    (, ,)
    (S
      (NP (PRP$ my) (NN mind))
      (VP (VBZ is)
        (ADJP (JJ open))))
    (CC and)
    (S
      (NP
        (NP
          (QP (RB just) (RB about) (DT all)))
        (PP (IN of)
          (NP
            (NP (PRP$ your) (NNS assumptions))
            (PP (IN about)
              (NP (PRP me))))))
      (VP (VBP are)
        (ADJP (JJ wrong))))
    (. .)))

