(ROOT
  (S
    (ADVP (IN So)
      (PP (IN from)
        (NP
          (NP (DT all))
          (PP (IN of)
            (NP (DT that))))))
    (, ,)
    (NP (PRP you))
    (VP (VBP continue)
      (S
        (VP (TO to)
          (VP
            (VP (VB call)
              (NP (PRP me))
              (NP (NNS names)))
            (, ,)
            (VP (VB show)
              (NP (PRP$ your) (NN pride)))
            (, ,)
            (CC and)
            (VP (VB insist)
              (SBAR (IN that)
                (S
                  (NP (PRP I))
                  (VP (VBP am)
                    (NP
                      (NP (DT a) (JJ blind) (NN follower))
                      (PP (IN of)
                        (NP (PRP$ my) (NN religion))))))))))))
    (. .)))

