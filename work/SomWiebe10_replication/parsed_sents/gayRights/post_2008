(ROOT
  (S
    (ADVP (RB Yes))
    (NP (JJ gay) (NNS people))
    (VP (MD should)
      (VP (VB have)
        (NP
          (NP (DT that) (NN right))
          (SBAR
            (S
              (NP (PRP they))
              (VP (VBP are)
                (ADVP (RB no)
                  (ADJP (JJ different)))
                (RB then)
                (NP (PRP us))
                (PP (IN except)
                  (NP (RB there)))
                (ADVP (RB sexuality))))))))
    (. .)))

advmod(have-5, Yes-1)
amod(people-3, gay-2)
nsubj(have-5, people-3)
aux(have-5, should-4)
root(ROOT-0, have-5)
det(right-7, that-6)
dobj(have-5, right-7)
nsubj(us-13, they-8)
cop(us-13, are-9)
advmod(us-13, no-10)
pobj(no-10, different-11)
advmod(us-13, then-12)
rcmod(right-7, us-13)
prep(us-13, except-14)
pobj(except-14, there-15)
advmod(us-13, sexuality-16)

