(ROOT
  (S
    (SBAR
      (WHADVP (WRB When))
      (S
        (NP (NN divorce))
        (VP (VBD was)
          (ADJP (JJ legal)))))
    (, ,)
    (NP (JJ Judeo-Christian) (NN folk))
    (VP (VBD said)
      (SBAR
        (S
          (NP (DT the) (NN sky))
          (VP (VBD was)
            (VP (VBG falling))))))
    (. .)))

advmod(legal-4, When-1)
nsubj(legal-4, divorce-2)
cop(legal-4, was-3)
advcl(said-8, legal-4)
amod(folk-7, Judeo-Christian-6)
nsubj(said-8, folk-7)
root(ROOT-0, said-8)
det(sky-10, the-9)
nsubj(falling-12, sky-10)
aux(falling-12, was-11)
ccomp(said-8, falling-12)

