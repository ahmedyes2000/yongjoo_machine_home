    (, ,)
    (NP (JJ Judeo-Christian) (NN folk))
    (VP (MD will)
      (VP (VB say)
        (SBAR (IN that)
          (S
            (NP (DT the) (NN sky))
            (VP (VBZ is)
              (VP (VBG falling)))))))
    (. .)))

