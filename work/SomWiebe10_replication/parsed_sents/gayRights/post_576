(ROOT
  (S (`` ``)
    (FRAG (: Is)
      (ADVP (RB there))
      (NP
        (NP (DT a) (JJ biological) (NN component))
        (PP (TO to)
          (NP (JJ sexual) (NN orientation)))))
    ('' '')
    (VP (MD would)
      (VP (VB be)
        (ADJP (RBR more) (JJ appropriate))))
    (. .)))

punct(component-6, Is-2)
dep(component-6, there-3)
det(component-6, a-4)
amod(component-6, biological-5)
dep(appropriate-14, component-6)
amod(orientation-9, sexual-8)
prep_to(component-6, orientation-9)
aux(appropriate-14, would-11)
cop(appropriate-14, be-12)
advmod(appropriate-14, more-13)
root(ROOT-0, appropriate-14)

