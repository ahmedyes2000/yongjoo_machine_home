(ROOT
  (S
    (NP (DT this))
    (VP (VBZ is)
      (NP (PDT such) (DT an) (JJ ignorant) (NN argument)))
    (. .)))

nsubj(argument-6, this-1)
cop(argument-6, is-2)
predet(argument-6, such-3)
det(argument-6, an-4)
amod(argument-6, ignorant-5)
root(ROOT-0, argument-6)

(ROOT
  (S
    (NP (NNS gays))
    (VP (MD can)
      (VP (VB marry)
        (SBAR
          (WHNP (WP whoever) (DT the) (NN hell))
          (S
            (NP (PRP they))
            (VP (VBP want))))))
    (. .)))

nsubj(marry-3, gays-1)
aux(marry-3, can-2)
root(ROOT-0, marry-3)
det(hell-6, whoever-4)
det(hell-6, the-5)
dobj(want-8, hell-6)
nsubj(want-8, they-7)
ccomp(marry-3, want-8)

(ROOT
  (S
    (NP (PRP IT))
    (VP (VBZ 'S)
      (NP (DT A) (NNP CIVIL) (NNP RIGHT)))
    (. .)))

nsubj(RIGHT-5, IT-1)
cop(RIGHT-5, 'S-2)
det(RIGHT-5, A-3)
nn(RIGHT-5, CIVIL-4)
root(ROOT-0, RIGHT-5)

