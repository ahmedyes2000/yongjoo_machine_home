(ROOT
  (FRAG
    (NP (NNP Nooooo))
    (. .)))

root(ROOT-0, Nooooo-1)

(ROOT
  (S
    (S
      (VP (VBG Being)
        (NP (DT a) (NN homo))))
    (VP (VBZ is)
      (ADVP (RB just)
        (PP (IN as)
          (NP
            (NP (RB much) (DT a) (NN sin))
            (PP (IN as)
              (NP (DT the) (NN act))))))
      (NP (PRP itself)))
    (. .)))

cop(homo-3, Being-1)
det(homo-3, a-2)
csubj(itself-13, homo-3)
cop(itself-13, is-4)
advmod(itself-13, just-5)
advmod(sin-9, much-7)
det(sin-9, a-8)
prep_as(just-5, sin-9)
det(act-12, the-11)
prep_as(sin-9, act-12)
root(ROOT-0, itself-13)

