(ROOT
  (S
    (NP (PRP I))
    (VP (MD would) (RB n't)
      (VP (VB put)
        (NP (PRP it))
        (PP (IN past)
          (NP (PRP him)))))
    (. .)))

nsubj(put-4, I-1)
aux(put-4, would-2)
neg(put-4, n't-3)
root(ROOT-0, put-4)
dobj(put-4, it-5)
prep_past(put-4, him-7)

(ROOT
  (S
    (NP (PRP I))
    (ADVP (NN kind) (IN of))
    (VP (VBP feel)
      (ADJP (JJ bad))
      (PP (IN for)
        (NP (PRP him)))
      (ADVP (RB though)))
    (. .)))

nsubj(feel-4, I-1)
dep(of-3, kind-2)
advmod(feel-4, of-3)
root(ROOT-0, feel-4)
acomp(feel-4, bad-5)
prep_for(feel-4, him-7)
advmod(feel-4, though-8)

(ROOT
  (S
    (NP (PRP$ His) (NN worldview))
    (VP (VBZ seems)
      (ADJP (RB awfully) (JJ depressing)))
    (. .)))

poss(worldview-2, His-1)
nsubj(depressing-5, worldview-2)
cop(depressing-5, seems-3)
advmod(depressing-5, awfully-4)
root(ROOT-0, depressing-5)

