  (S
    (NP
      (NP (NNS fathers))
      (CC and)
      (NP (DT a) (NNS daughters) (NN cant)))
    (VP (VBP get)
      (NP (JJ married) (NN bc))
      (SBAR
        (WHADVP (WRB when))
        (S
          (NP (PRP you))
          (VP (VBD inbreed)
            (S
              (NP (DT the) (NNS children))
              (VP
                (VP (VB come)
                  (PRT (RP out))
                  (PP (IN with)
                    (NP (VBG missing) (NNS chromosomes))))
                (CC and)
                (VP (VB cross)
                  (NP (NNS eyes)))))))))
    (. .)))

