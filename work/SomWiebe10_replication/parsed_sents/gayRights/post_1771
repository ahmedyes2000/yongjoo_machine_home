              (PP (IN about)
                (NP (DT the) (NN fact))))
            (SBAR (IN that)
              (S
                (NP (DT the) (NNP U.S.))
                (VP (VBZ does) (RB n't)
                  (VP (VB vote)
                    (PP (IN on)
                      (NP
                        (NP (NNS things))
                        (SBAR
                          (WHNP (WDT that))
                          (S
                            (VP (MD should) (RB not)
                              (VP (VB be)
                                (VP (VBN voted)
                                  (PRT (RP on)))))))))))))))))
    (. .)))

