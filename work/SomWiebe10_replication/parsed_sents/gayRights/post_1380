(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (CD two) (NNS men))
        (VP (VBP adopt))))
    (NP (DT a) (NN girl))
    (ADVP (WRB how))
    (VP (VBP are)
      (NP
        (NP (PRP they))
        (VP (VBG going)
          (S
            (VP (TO to)
              (VP (VB explain)
                (NP (NN girl) (NN stuff))))))))
    (. .)))

