(ROOT
  (S
    (NP (PRP You))
    (VP (VBP are)
      (ADJP
        (ADJP (RB far) (RB too) (JJ young))
        (SBAR (IN for)
          (S
            (NP (PRP me))
            (VP (TO to)
              (VP
                (ADVP (RB even))
                (VB explain)
                (PP (TO to)
                  (NP (PRP you)))
                (SBAR
                  (WHADVP (WRB why))
                  (S
                    (NP (FW i))
                    (VP (VBP disagree))))))))))
    (: ...)))

nsubj(young-5, You-1)
cop(young-5, are-2)
advmod(young-5, far-3)
advmod(young-5, too-4)
root(ROOT-0, young-5)
mark(explain-10, for-6)
nsubj(explain-10, me-7)
aux(explain-10, to-8)
advmod(explain-10, even-9)
dep(young-5, explain-10)
prep_to(explain-10, you-12)
advmod(disagree-15, why-13)
nsubj(disagree-15, i-14)
advcl(explain-10, disagree-15)

