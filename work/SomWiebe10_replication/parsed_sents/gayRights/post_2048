(ROOT
  (S
    (S
      (NP (PRP He))
      (VP (MD will)
        (ADVP (RB probably))
        (VP (VB say)
          (NP (JJ interracial) (NN marriage)))))
    (CC and)
    (S
      (NP (JJ same-sex) (NN marriage))
      (VP (VBP are)
        (ADJP (RB completely) (JJ different))
        (SBAR (RB even) (IN though)
          (S
            (NP (PRP they))
            (VP (VBP are)
              (ADJP (RB quite) (JJ similar)))))))
    (. .)))

nsubj(say-4, He-1)
aux(say-4, will-2)
advmod(say-4, probably-3)
root(ROOT-0, say-4)
amod(marriage-6, interracial-5)
dobj(say-4, marriage-6)
amod(marriage-9, same-sex-8)
nsubj(different-12, marriage-9)
cop(different-12, are-10)
advmod(different-12, completely-11)
conj_and(say-4, different-12)
advmod(similar-18, even-13)
dep(similar-18, though-14)
nsubj(similar-18, they-15)
cop(similar-18, are-16)
advmod(similar-18, quite-17)
ccomp(different-12, similar-18)

