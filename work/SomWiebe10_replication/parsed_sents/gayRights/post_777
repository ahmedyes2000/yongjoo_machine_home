(ROOT
  (S
    (S
      (NP (PRP You))
      (VP (VBP make)
        (NP
          (NP (DT an) (NN error))
          (SBAR
            (WHADVP (WRB when))
            (S
              (NP (PRP you))
              (VP (VBP state)
                (SBAR (IN that)
                  (S
                    (NP (NNS sins))
                    (VP (VBZ is) (RB not)
                      (VP (VBG abiding)
                        (PP (IN by)
                          (NP (DT the) (NN law)))))))))))))
    (: ...)
    (S
      (NP (PRP it))
      (ADVP (RB clearly))
      (VP (VBZ states)
        (SBAR (IN that)
          (S
            (NP (DT a) (NN man))
            (VP (MD shall) (RB not)
              (VP (VB lie)
                (PP (IN with)
                  (NP (DT a) (NN man)))
                (PP (IN as)
                  (PP (IN with)
                    (NP (DT a) (NN woman))))))))))
    (. .)))

nsubj(make-2, You-1)
root(ROOT-0, make-2)
det(error-4, an-3)
dobj(make-2, error-4)
