(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (NNP God))
        (VP (VBZ says)
          (NP (NN something)))))
    (, ,)
    (NP (PRP it))
    (VP (VBZ is)
      (SBAR
        (WHNP (WP what))
        (S
          (NP (PRP He))
          (VP (VBZ says)
            (SBAR
              (S
                (NP (PRP it))
                (VP (VBZ is)
                  (SBAR (IN because)
                    (S
                      (NP (PRP He))
                      (VP (VBZ says)
                        (SBAR
                          (S
                            (NP (PRP it))
                            (VP (VBZ is))))))))))))))
    (. .)))

