dep(error-4, state-7)
complm(abiding-12, that-8)
nsubj(abiding-12, sins-9)
aux(abiding-12, is-10)
neg(abiding-12, not-11)
ccomp(state-7, abiding-12)
det(law-15, the-14)
prep_by(abiding-12, law-15)
nsubj(states-19, it-17)
advmod(states-19, clearly-18)
parataxis(make-2, states-19)
complm(lie-25, that-20)
det(man-22, a-21)
nsubj(lie-25, man-22)
aux(lie-25, shall-23)
neg(lie-25, not-24)
