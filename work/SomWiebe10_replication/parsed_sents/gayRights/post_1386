nsubj(are-2, you-3)

(ROOT
  (S
    (NP (PRP I))
    (VP (VBD guessing)
      (SBAR
        (S
          (NP (PRP it))
          (VP (VBZ is)
            (ADJP (JJ andrew))))))
    (. !)))

