(ROOT
  (S
    (S
      (NP (NNP God))
      (VP (VBZ is)
        (ADJP (JJ alive))))
    (, ,)
    (CC and)
    (S
      (NP (NNS works))
      (VP (VBZ miracles)
        (NP
          (NP (DT all))
          (PP (IN over)
            (NP (DT the) (NN world))))))
    (. .)))

nsubj(alive-3, God-1)
cop(alive-3, is-2)
root(ROOT-0, alive-3)
nsubj(miracles-7, works-6)
conj_and(alive-3, miracles-7)
dobj(miracles-7, all-8)
det(world-11, the-10)
prep_over(all-8, world-11)

