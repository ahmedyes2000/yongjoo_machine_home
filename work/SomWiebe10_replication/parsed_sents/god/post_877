  (S
    (NP (PRP I))
    (VP (VBP know)
      (PP (IN for)
        (ADJP (JJ sure)))
      (SBAR (IN that)
        (S
          (S
            (SBAR
              (WHADVP (WRB when))
              (S
                (NP (FW i))
                (VP (VBP die))))
            (, ,)
            (NP (FW i))
            (VP (MD will)
              (VP (VB live)
                (PP (IN with)
                  (NP (PRP him)))
                (PP (IN in)
                  (NP (NN heaven)))
                (SBAR (IN because)
                  (S
                    (NP (FW i))
                    (VP (VBP believe)
                      (SBAR
                        (S
                          (NP (NNS jesus))
                          (VP (VBD died)
                            (PP (IN for)
                              (NP (PRP us))))))))))))
          (CC and)
          (S
            (NP (FW i))
            (VP (VBP love)
              (NP
                (NP (NN god)
                  (CC and)
                  (NN trust))
                (PP (IN in)
                  (NP (PRP him)))))))))
    (. .)))

