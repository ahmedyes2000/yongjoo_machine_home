(ROOT
  (S
    (NP (PRP I))
    (ADVP (RB proudly))
    (VP (VBP say)
      (SBAR
        (S
          (NP (PRP I))
          (VP (VBP am)
            (NP (DT a) (NNP Christian))))))
    (. .)))

