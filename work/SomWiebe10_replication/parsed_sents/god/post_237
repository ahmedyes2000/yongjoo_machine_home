    (NP (DT The) (NN universe))
    (VP (MD can) (RB not)
      (VP (VB be)
        (ADJP (RB infinetly) (JJ old)
          (PRN (, ,)
            (PP (VBG according)
              (PP (TO to)
                (NP (PRP$ my)
                  (ADJP (RB very) (VBN limited))
                  (NN knowledge))))
            (, ,))
          (SBAR (IN because)
            (S
              (NP (NN heat))
              (VP (VBZ flows)
                (PP (IN from)
                  (ADJP (JJ warm)))
                (PP (TO to)
                  (NP (JJ cold) (NNS bodies)))))))))
    (. .)))

