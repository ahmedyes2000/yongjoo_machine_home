      (NP (PRP it))
      (VP (VBZ 's) (RB not)
        (ADJP (JJ hard)
          (S
            (VP (TO to)
              (VP (VB tell)
                (PP (IN without)
                  (NP (NNS doubts)))))))))
    (NP
      (NP (JJ such) (NNP God))
      (PRN (-LRB- -LRB-) (PRP s) (-RRB- -RRB-)))
    (VP (VBZ does) (RB not)
      (VP (VB exist)))
    (. .)))

