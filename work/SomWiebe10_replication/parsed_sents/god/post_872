(ROOT
  (S
    (NP (WP What) (NN Pyrus))
    (VP (VBD said))
    (. .)))

dep(Pyrus-2, What-1)
nsubj(said-3, Pyrus-2)
root(ROOT-0, said-3)

(ROOT
  (S
    (NP (DT The) (NNP Bible))
    (VP (VBZ is)
      (NP
        (NP (NN proof))
        (PP (IN of)
          (NP (PRP$ His) (NN existence)))))
    (. .)))

det(Bible-2, The-1)
nsubj(proof-4, Bible-2)
cop(proof-4, is-3)
root(ROOT-0, proof-4)
poss(existence-7, His-6)
prep_of(proof-4, existence-7)

