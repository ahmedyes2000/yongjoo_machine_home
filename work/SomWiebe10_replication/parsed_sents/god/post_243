  (S
    (S
      (NP (NNP Matter))
      (VP (VBZ is) (RB not)
        (VP (VBN destroyed)
          (SBAR
            (WHADVP (WRB when))
            (S
              (NP (PRP it))
              (VP (VBZ enters)
                (NP (DT a) (JJ black) (NN hole))))))))
    (: ...)
    (S
      (NP (PRP it))
      (VP (VBZ adds)
        (PP (TO to)
          (NP (DT the) (NN mass)))))
    (. .)))

