(ROOT
  (S
    (NP (PRP You))
    (VP (MD would)
      (VP (VB like)
        (S
          (VP (TO to)
            (VP (VB believe)
              (SBAR (IN that)
                (S
                  (NP (CC but)
                    (NP
                      (NP (DT the) (JJ actual) (NNS facts))
                      (PP (IN of)
                        (NP
                          (NP (DT the) (NN matter))
                          (CC and)
                          (NP (NNS statistics))))))
                  (VP (VB prove)
                    (S
                      (NP (PRP you))
                      (ADJP (JJ wrong)))))))))))
    (. .)))

nsubj(like-3, You-1)
xsubj(believe-5, You-1)
aux(like-3, would-2)
root(ROOT-0, like-3)
aux(believe-5, to-4)
xcomp(like-3, believe-5)
complm(prove-16, that-6)
cc(facts-10, but-7)
det(facts-10, the-8)
amod(facts-10, actual-9)
nsubj(prove-16, facts-10)
det(matter-13, the-12)
prep_of(facts-10, matter-13)
prep_of(facts-10, statistics-15)
conj_and(matter-13, statistics-15)
ccomp(believe-5, prove-16)
nsubj(wrong-18, you-17)
xcomp(prove-16, wrong-18)

