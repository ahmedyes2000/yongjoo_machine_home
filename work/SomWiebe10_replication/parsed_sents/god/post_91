(ROOT
  (NP
    (NP (NNP Quit))
    (VP (VBG confusing)
      (NP (NNS Christians)
        (CC and)
        (NNPS Muslims))
      (PP (IN with)
        (NP
          (NP (NN everyone))
          (SBAR
            (WHNP (WP who))
            (S
              (VP (VBZ believes)
                (PP (IN in)
                  (NP (NNP God)))))))))
    (. .)))

root(ROOT-0, Quit-1)
partmod(Quit-1, confusing-2)
dobj(confusing-2, Christians-3)
dobj(confusing-2, Muslims-5)
conj_and(Christians-3, Muslims-5)
prep_with(confusing-2, everyone-7)
nsubj(believes-9, everyone-7)
rcmod(everyone-7, believes-9)
prep_in(believes-9, God-11)

(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ 's)
      (ADJP (JJ offensive)))
    (. .)))

nsubj(offensive-3, It-1)
cop(offensive-3, 's-2)
root(ROOT-0, offensive-3)

