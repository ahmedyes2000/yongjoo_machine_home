(ROOT
  (S
    (SBAR (IN Until)
      (S
        (NP
          (NP (NN god))
          (, ,)
          (NP (DT any) (NN god))
          (, ,))
        (VP
          (VP (VBZ shows)
            (NP (NN him\/herself))
            (PP (IN in)
              (NP (JJ plain) (NN sight))))
          (, ,)
          (CC and)
          (VP (VBZ performs)
            (NP (DT a) (JJ bonafide) (NN miracle))))))
    (, ,)
    (ADVP (RB then))
    (NP (PRP I))
    (VP (MD will)
      (VP (VB remain)
        (NP (DT an) (NN atheist))))
    (. .)))

