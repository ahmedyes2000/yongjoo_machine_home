(ROOT
  (S
    (NP (EX There))
    (VP (VBZ 's)
      (NP (DT no) (NN proof))
      (SBAR (IN that)
        (S
          (NP (DT a) (NN god))
          (VP (VBZ exists)))
        (CC or)
        (RB not)))
    (. .)))

expl('s-2, There-1)
root(ROOT-0, 's-2)
det(proof-4, no-3)
nsubj('s-2, proof-4)
complm(exists-8, that-5)
det(god-7, a-6)
nsubj(exists-8, god-7)
ccomp('s-2, exists-8)
ccomp('s-2, not-10)
conj_or(exists-8, not-10)

