    (NP (NNP Science))
    (VP (VBZ is) (RB not)
      (NP
        (NP (DT a) (`` ``) (NN matter))
        (PP (IN of)
          (NP
            (NP (NN faith))
            ('' '')
            (SBAR
              (S
                (NP (PRP it))
                (VP (VBZ 's)
                  (NP
                    (NP (DT a) (NN matter))
                    (PP (IN of)
                      (NP (NN evidence)))))))))))
    (. .)))

