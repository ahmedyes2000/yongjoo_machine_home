(ROOT
  (S (CC But)
    (NP (DT the) (NNP Bible))
    (VP (VBZ says)
      (SBAR (IN that)
        (S
          (VP (VBZ 's)
            (ADJP (JJ okay))))))
    (. !)))

cc(says-4, But-1)
det(Bible-3, the-2)
nsubj(says-4, Bible-3)
root(ROOT-0, says-4)
complm(okay-7, that-5)
cop(okay-7, 's-6)
ccomp(says-4, okay-7)

(ROOT
  (S
    (NP (DT The) (NN logic))
    (VP (VBZ is)
      (ADJP (JJ flawless)))
    (. .)))

det(logic-2, The-1)
nsubj(flawless-4, logic-2)
cop(flawless-4, is-3)
root(ROOT-0, flawless-4)

