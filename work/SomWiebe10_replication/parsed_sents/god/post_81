(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP it))
        (VP (VBZ is)
          (NP (DT a) (NN question)))))
    (VP (VB ask)
      (NP (PRP$ your) (NN self)))
    (. .)))

mark(question-5, If-1)
nsubj(question-5, it-2)
cop(question-5, is-3)
det(question-5, a-4)
csubj(ask-6, question-5)
root(ROOT-0, ask-6)
poss(self-8, your-7)
dobj(ask-6, self-8)

(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP it))
        (VP (VBZ is)
          (NP (DT an) (NN argument)))))
    (NP (RB only) (NN time))
    (VP (MD will)
      (VP (VB explain)
        (NP (PRP you))
        (ADVP (RB clearly))))
    (. .)))

mark(argument-5, If-1)
nsubj(argument-5, it-2)
cop(argument-5, is-3)
det(argument-5, an-4)
advcl(explain-9, argument-5)
advmod(time-7, only-6)
nsubj(explain-9, time-7)
aux(explain-9, will-8)
root(ROOT-0, explain-9)
dobj(explain-9, you-10)
advmod(explain-9, clearly-11)

