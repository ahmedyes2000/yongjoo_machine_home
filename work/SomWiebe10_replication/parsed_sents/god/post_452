(ROOT
  (S
    (NP (PRP I))
    (VP (VBP refuse)
      (S
        (VP (TO to)
          (VP (VB believe)
            (PP (IN in)
              (NP (NNP God)))
            (SBAR (IN until)
              (S
                (NP (NN proof))
                (VP (VBZ presents)
                  (NP (PRP itself)))))))))
    (. .)))

