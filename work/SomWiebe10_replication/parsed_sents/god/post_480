(ROOT
  (NP (CD Hell) (RB no) (. !)))

root(ROOT-0, Hell-1)
advmod(Hell-1, no-2)

(ROOT
  (SINV
    (VP
      (VP (VB Im)
        (NP (DT a)
          (ADJP (RB very) (JJ proud))
          (NN Atheist)))
      (CC and)
      (VP
        (ADVP (RB ill))
        (VB keep)
        (NP (PRP it))))
    (NP (DT that) (NN way))))

root(ROOT-0, Im-1)
det(Atheist-5, a-2)
advmod(proud-4, very-3)
