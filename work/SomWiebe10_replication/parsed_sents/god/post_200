(ROOT
  (S
    (NP (PRP I))
    (ADVP (RB just))
    (VP (VBD pointed)
      (PRT (RP out))
      (SBAR (IN that)
        (S
          (NP
            (NP (DT the) (NNP OT))
            (, ,)
            (NP (NNP NT))
            (, ,)
            (CC and)
            (NP
              (NP (DT the) (NN totality))
              (PP (IN of)
                (NP (JJ Christian) (NN theology)))))
          (VP (VBP do) (RB not)
            (VP (VB agree)
              (PP (IN with)
                (NP (PRP$ your) (NNS statements))))))))
    (. .)))

