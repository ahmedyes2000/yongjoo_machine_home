(ROOT
  (S
    (NP (PRP You))
    (VP (VBP are)
      (NP
        (NP (DT the) (NN one))
        (SBAR
          (WHNP (WP who))
          (S
            (VP (VBZ is)
              (VP (VBG anthropomorphizing)
                (NP (DT the) (NNP Unified) (NNP Field) (NNS theories))))))))
    (: ...)))

