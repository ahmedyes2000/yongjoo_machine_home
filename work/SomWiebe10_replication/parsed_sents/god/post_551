  (S
    (S
      (VP (VBG Believing)
        (PP (IN in)
          (NP (DT any) (NN religion) (NN specific)))))
    (NP (NNP God))
    (VP (VBZ is)
      (ADJP (JJ foolish))
      (SBAR (IN because)
        (S
          (NP (DT that) (NN belief))
          (VP (VBZ has)
            (NP (DT no) (JJ rational) (NN foundation))))))
    (. .)))

