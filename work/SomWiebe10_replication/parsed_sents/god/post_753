(ROOT
  (PRN
    (S
      (NP (JJ sure))
      (VP (VBZ is) (, ,)
        (SBAR (IN if)
          (S
            (VP (VBZ thats)
              (SBAR
                (WHNP (WP what))
                (S
                  (VP (VBZ gets)
                    (NP (PRP you))
                    (PP (IN through)
                      (NP (DT the) (NN night)))))))
            (: ;)))))
    (-RRB- -RRB-)))

nsubj(is-2, sure-1)
root(ROOT-0, is-2)
mark(thats-5, if-4)
advcl(is-2, thats-5)
nsubj(gets-7, what-6)
ccomp(thats-5, gets-7)
dobj(gets-7, you-8)
det(night-11, the-10)
prep_through(gets-7, night-11)

