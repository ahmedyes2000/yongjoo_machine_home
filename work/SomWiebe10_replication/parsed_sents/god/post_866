  (S
    (NP
      (NP (PDT All) (DT the) (JJ religious) (NNS fanatics))
      (PP (IN in)
        (NP (DT the) (NN world))))
    (VP (MD can) (RB not)
      (VP (VB show)
        (S
          (NP (PRP you))
          (NP
            (NP (CD one) (NN piece))
            (PP (IN of)
              (NP
                (NP (JJ tangible) (NN proof))
                (PP (IN of)
                  (NP (PRP$ their) (NN belief)))))))))
    (. .)))

