aux(exist-20, do-18)
neg(exist-20, n't-19)
parataxis(seen-8, exist-20)
dep(matter-22, no-21)
advmod(exist-20, matter-22)
advmod(many-24, how-23)
amod(people-25, many-24)
nsubj(claim-26, people-25)
dep(matter-22, claim-26)
nsubj(do-28, they-27)
ccomp(claim-26, do-28)

(ROOT
  (S ('' '')
    (PP (VBG Claims)
      (PP (IN of)
        (NP (JJ supernatural) (NNS powers))))
    (PRN (: :)
      (S
        (VP (VBN Confirmed)
          (PP (IN by)
            (NP (NN experiment)))))
      (: :))
    (NP (CD 0) (NN %))
    (ADVP
      (NP (NNP Refuted))
      (IN by))
    (VP (VBZ experiment) (: :)
      (NP (CD 100) (NN %)))))

