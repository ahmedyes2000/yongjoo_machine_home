(ROOT
  (S (`` ``)
    (INTJ (UH No))
    (, ,)
    (PP (IN because)
      (NP (FW i)))
    (VP (VBP 've)
      (ADVP (RB never))
      (VP (VBN seen)
        (NP
          (NP (DT any) (NN evidence))
          (VP (VBG saying)
            (ADVP (RB otherwise))))))
    (PRN (-LRB- -LRB-)
      (CC and)
      (S
        (NP (NNS psychics)
          (CC and)
          (NNS ghosts))
        (VP (VBP do) (RB n't)
          (VP (VB exist)
            (ADVP (DT no) (NN matter)
              (SBAR
                (WHNP
                  (WHADJP (WRB how) (JJ many))
                  (NNS people))
                (S
                  (VP (VBP claim)
                    (SBAR
                      (S
                        (NP (PRP they))
                        (VP (VBP do)))))))))))
      (-RRB- -RRB-))
    (. .)))

