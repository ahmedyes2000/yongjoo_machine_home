(ROOT
  (S
    (NP (PRP I))
    (VP (VBP think)
      (PRN (, ,)
        (SBAR (RB even) (IN if)
          (S
            (NP (NNP God) (NNP doesnt))
            (VP (VBP exist))))
        (, ,))
      (SBAR
        (WHNP (WDT which))
        (S
          (VP (VBP im)
            (S (RB not)
              (VP (VBG saying)
                (SBAR
                  (S
                    (NP (PRP he))
                    (VP (VBZ does))))))))))
    (: ...)))

