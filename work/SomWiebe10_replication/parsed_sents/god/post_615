ccomp(saying-13, does-15)

(ROOT
  (S
    (S
      (NP (PRP I))
      (VP (VBP believe)
        (NP
          (NP (CD 100) (NN %))
          (SBAR
            (S
              (NP (PRP He))
              (VP (VBZ does)))))))
    (, ,)
    (CC but)
    (S
      (SBAR (RB even) (IN if)
        (S
          (NP (PRP he))
          (VP (VBD did) (RB n't))))
      (NP
        (NP (DT the) (JJ mere) (NN thought))
        (PP (IN of)
          (NP (PRP$ his) (NN existance))))
      (VP (VBZ has)
        (VP (VBN made)
          (NP
            (NP (JJ many) (NNS people))
            (ADJP
              (ADJP (RB morally) (JJR better))
              (, ,)
              (CC and)
              (ADJP (RB just) (JJR happier))
              (CC and)
              (ADJP (JJR nicer)))))))
    (. .)))

