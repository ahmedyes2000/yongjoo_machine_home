(ROOT
  (S
    (NP (PRP I))
    (VP (VBP do) (RB not)
      (VP (VB believe)
        (PP (IN in)
          (NP (NN god)))
        (SBAR (IN as)
          (S
            (NP
              (NP (DT a) (`` ``) (NN god) ('' ''))
              (PP (IN like)
                (NP (DT the) (NN bible))))
            (VP (VBZ describes)
              (S
                (NP (PRP him))
                (VP (TO to)
                  (VP (VB be)))))))))
    (. .)))

nsubj(believe-4, I-1)
aux(believe-4, do-2)
neg(believe-4, not-3)
root(ROOT-0, believe-4)
prep_in(believe-4, god-6)
mark(describes-15, as-7)
det(god-10, a-8)
nsubj(describes-15, god-10)
det(bible-14, the-13)
prep_like(god-10, bible-14)
advcl(believe-4, describes-15)
nsubj(be-18, him-16)
aux(be-18, to-17)
xcomp(describes-15, be-18)

