(ROOT
  (S
    (NP (PRP It))
    (VP (MD can) (RB not)
      (VP
        (VP (VB speak))
        (, ,)
        (VP (VB hear)
          (NP (PRP us)))
        (CC or)
        (VP (VB give)
          (NP (PRP us))
          (NP
            (NP (NN advice))
            (PP (IN of)
              (NP (DT any) (NN sort)))))))
    (. .)))

