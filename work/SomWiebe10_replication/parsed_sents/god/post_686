(ROOT
  (S
    (NP
      (NP (PRP$ Your)
        (NX
          (NP (DT an) (NN athiest))
          (PP (IN for)
            (NP (JJ other) (NNS reasons)))))
      (PP (IN than)
        (NP (DT that))))
    (VP (VBP do) (RB n't)
      (VP (VB kid)
        (NP (PRP yourself))))
    (. .)))

poss(athiest-3, Your-1)
det(athiest-3, an-2)
nsubj(kid-11, athiest-3)
amod(reasons-6, other-5)
prep_for(athiest-3, reasons-6)
prep_than(athiest-3, that-8)
aux(kid-11, do-9)
neg(kid-11, n't-10)
root(ROOT-0, kid-11)
dobj(kid-11, yourself-12)

