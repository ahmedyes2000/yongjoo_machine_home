(ROOT
  (S
    (NP (WP What) (NN absurdity))
    (VP (VBZ is)
      (NP
        (NP (DT this) (NNP DOG))
        (SBAR
          (S
            (VP (VBZ is)
              (NP
                (NP (DT a) (VBG living) (NN thing))
                (VP (VBN created)
                  (PP (IN by)
                    (NP (DT the) (NNP SCIENCE))))))))))
    (: ...)))

dep(absurdity-2, What-1)
nsubj(DOG-5, absurdity-2)
cop(DOG-5, is-3)
det(DOG-5, this-4)
root(ROOT-0, DOG-5)
cop(thing-9, is-6)
det(thing-9, a-7)
amod(thing-9, living-8)
rcmod(DOG-5, thing-9)
partmod(thing-9, created-10)
det(SCIENCE-13, the-12)
agent(created-10, SCIENCE-13)

(ROOT
  (S
    (NP (NNP God))
    (VP (VBZ is)
      (ADVP (RB just))
      (NP
        (NP (DT an) (JJ imaginary) (NN figure))
        (VP (VBN created)
          (PP (IN by)
            (NP (DT the) (JJ lazy) (JJ human) (NN mind)))
          (S
            (VP (TO to)
              (VP (VB have)
                (NP (JJ lame) (NNS excuses))))))))
    (: ...)))

nsubj(figure-6, God-1)
cop(figure-6, is-2)
advmod(figure-6, just-3)
det(figure-6, an-4)
amod(figure-6, imaginary-5)
root(ROOT-0, figure-6)
xsubj(have-14, figure-6)
partmod(figure-6, created-7)
det(mind-12, the-9)
amod(mind-12, lazy-10)
amod(mind-12, human-11)
agent(created-7, mind-12)
aux(have-14, to-13)
