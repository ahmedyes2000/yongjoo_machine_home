          (CC and)
          (S
            (NP (DT that))
            (VP (VBZ is) (RB not)
              (NP (JJ blue) (NNS genes))
              (SBAR (IN for)
                (S
                  (NP (PRP you))
                  (VP (VBZ morons))))))
          (-RRB- -RRB-))))
    (. .)))

