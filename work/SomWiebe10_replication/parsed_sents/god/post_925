  (S
    (NP (NNP God))
    (VP
      (VP (VBZ does)
        (S
          (VP (VB exist)
            (SBAR
              (S
                (NP (PRP he))
                (VP (VBZ works)
                  (PP (IN in)
                    (NP (PRP$ our) (NNS lives)))
                  (ADVP (RB daily))))))))
      (CC and)
      (VP (VBZ watches)
        (PRT (RP over))
        (NP (PRP us)))
      (NP (PDT all) (DT the) (NN time)))
    (. .)))

