(ROOT
  (S
    (NP (PRP You))
    (VP (VBP 're)
      (VP (VBG assuming)
        (S
          (NP (NNS things))
          (VP (VB happen)
            (PP (IN for)
              (NP (DT a) (NN reason)))))))
    (. .)))

nsubj(assuming-3, You-1)
aux(assuming-3, 're-2)
root(ROOT-0, assuming-3)
nsubj(happen-5, things-4)
ccomp(assuming-3, happen-5)
det(reason-8, a-7)
prep_for(happen-5, reason-8)

(ROOT
  (SQ (MD Could)
    (NP (PRP it))
    (ADVP (RB not) (RB all))
    (VP (VB be)
      (NP (NN chance)))
    (. ?)))

aux(chance-6, Could-1)
nsubj(chance-6, it-2)
dep(all-4, not-3)
advmod(chance-6, all-4)
cop(chance-6, be-5)
root(ROOT-0, chance-6)

