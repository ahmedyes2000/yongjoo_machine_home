(ROOT
  (S
    (NP (EX There))
    (VP (VBZ is)
      (NP
        (NP (DT no) (NN god))
        (PP (IN of)
          (NP
            (NP (DT any) (NN kind))
            (PP (IN for)
              (NP (DT any) (NN religion)))))))
    (. .)))

expl(is-2, There-1)
root(ROOT-0, is-2)
det(god-4, no-3)
nsubj(is-2, god-4)
det(kind-7, any-6)
prep_of(god-4, kind-7)
det(religion-10, any-9)
prep_for(kind-7, religion-10)

(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is) (RB all)
      (ADJP (JJ bullshit)))
    (. .)))

nsubj(bullshit-4, It-1)
cop(bullshit-4, is-2)
advmod(bullshit-4, all-3)
root(ROOT-0, bullshit-4)

