(ROOT
  (FRAG
    (NP
      (NP (PRP He))
      (, ,)
      (NP (PRP she))
      (, ,)
      (NP (PRP it))
      (: ...)
      (NP (NNP whatever)))
    (. .)))

root(ROOT-0, He-1)
appos(He-1, she-3)
appos(He-1, it-5)
dep(He-1, whatever-7)

(ROOT
  (S
    (NP (DT The) (NN human))
    (VP (VBZ is)
      (NP
        (NP (DT an) (NN animal))
        (PP (IN with)
          (NP
            (NP
              (QP (CD 95) (NN %)
                (CC or)
                (JJR more))
              (JJ genetic) (NN similarity))
            (PP (TO to)
              (NP
                (NP (JJ several) (JJ other) (NNS species))
                (PP (IN of)
                  (NP
                    (NP (NNS animals))
                    (VP
                      (ADVP (RB also))
                      (VBN found)
                      (PP (IN on)
                        (NP (DT this) (NN planet))))))))))))
    (. .)))

det(human-2, The-1)
nsubj(animal-5, human-2)
cop(animal-5, is-3)
det(animal-5, an-4)
root(ROOT-0, animal-5)
number(%-8, 95-7)
num(similarity-12, %-8)
conj_or(%-8, more-10)
quantmod(%-8, more-10)
num(similarity-12, more-10)
amod(similarity-12, genetic-11)
prep_with(animal-5, similarity-12)
amod(species-16, several-14)
amod(species-16, other-15)
prep_to(similarity-12, species-16)
prep_of(species-16, animals-18)
advmod(found-20, also-19)
partmod(animals-18, found-20)
det(planet-23, this-22)
prep_on(found-20, planet-23)

(ROOT
  (NP
    (NP
      (NP (NNP Made))
      (PP (IN in)
        (NP
          (NP (NNP God) (POS 's))
          (NN image))))
    (: ...)
    (NP
      (NP (JJR more))
      (PP (IN like)
        (NP (NNS chimps))))
    (. !)))

root(ROOT-0, Made-1)
poss(image-5, God-3)
prep_in(Made-1, image-5)
dep(Made-1, more-7)
prep_like(more-7, chimps-9)

