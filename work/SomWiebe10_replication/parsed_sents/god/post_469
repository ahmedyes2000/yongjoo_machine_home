(ROOT
  (S
    (ADVP (RB Sorry))
    (, ,)
    (NP (EX there))
    (VP (VBZ is)
      (NP (DT no) (NN god)))
    (. .)))

advmod(is-4, Sorry-1)
expl(is-4, there-3)
root(ROOT-0, is-4)
det(god-6, no-5)
nsubj(is-4, god-6)

(ROOT
  (S
    (NP (PRP I))
    (VP (MD will) (RB not)
      (VP (VB accept)
        (NP
          (NP (DT any) (NN claim))
          (SBAR
            (WHNP (WDT that))
            (S
              (VP (VBZ comes)
                (PP (IN without)
                  (NP (DT any) (NN evidence)))))))))
    (. .)))

nsubj(accept-4, I-1)
aux(accept-4, will-2)
neg(accept-4, not-3)
root(ROOT-0, accept-4)
det(claim-6, any-5)
dobj(accept-4, claim-6)
nsubj(comes-8, claim-6)
rcmod(claim-6, comes-8)
det(evidence-11, any-10)
prep_without(comes-8, evidence-11)

