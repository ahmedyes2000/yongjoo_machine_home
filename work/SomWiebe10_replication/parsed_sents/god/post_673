(ROOT
  (FRAG
    (WHADVP (WRB Why))
    (RB not)
    (VP (VB give)
      (NP
        (NP (DT the) (NN judgment))
        (PP (IN of)
          (NP
            (NP (NN charity))
            (CC and)
            (NP (RB not) (NN condemnation))))))
    (. .)))

dep(not-2, Why-1)
root(ROOT-0, not-2)
dep(not-2, give-3)
det(judgment-5, the-4)
dobj(give-3, judgment-5)
prep_of(judgment-5, charity-7)
neg(condemnation-10, not-9)
