(ROOT
  (FRAG
    (S
      (S
        (NP (DT This))
        (VP (VBZ is)
          (NP (DT a) (JJ close) (NN one))))
      (, ,)
      (CC but)
      (S
        (NP (PRP I))
        (VP (MD 'd)
          (VP (VB have)
            (S
              (VP (TO to)
                (VP (VB say)
                  (NP (NN no.)))))))))))

nsubj(one-5, This-1)
cop(one-5, is-2)
det(one-5, a-3)
amod(one-5, close-4)
root(ROOT-0, one-5)
nsubj(have-10, I-8)
xsubj(say-12, I-8)
aux(have-10, 'd-9)
conj_but(one-5, have-10)
aux(say-12, to-11)
xcomp(have-10, say-12)
dobj(say-12, no.-13)

