(ROOT
  (S
    (S
      (NP (DT this))
      (VP (VBZ is)
        (NP (PRP$ my) (JJ first) (NN argument) (: :) (NN ahem))))
    (: ...)
    (S
      (NP (NNS atheists)
        (CC and)
        (NNS theists))
      (VP (VBP are)
        (NP
          (NP (CD two) (NNS sides))
          (PP (IN of)
            (NP (DT the) (JJ same) (NN coin))))))
    (. .)))

