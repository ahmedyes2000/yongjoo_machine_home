(ROOT
  (S
    (NP (EX There))
    (VP (VBZ is)
      (NP (DT no) (NN evidence)
        (S
          (VP (TO to)
            (VP (VB show)
              (SBAR
                (S
                  (NP (NNP God))
                  (VP (VBZ does)
                    (S
                      (VP (VB exist)
                        (ADVP (RB so))
                        (, ,)
                        (SBAR (IN as)
                          (S
                            (PP (IN for)
                              (NP (RB now)))
                            (NP (NNP God))
                            (VP (VBZ does) (RB not)
                              (VP (VB exist)))))))))))))))
    (. .)))

expl(is-2, There-1)
root(ROOT-0, is-2)
det(evidence-4, no-3)
nsubj(is-2, evidence-4)
aux(show-6, to-5)
infmod(evidence-4, show-6)
nsubj(does-8, God-7)
ccomp(show-6, does-8)
ccomp(does-8, exist-9)
advmod(exist-9, so-10)
mark(exist-18, as-12)
prep(exist-18, for-13)
pobj(for-13, now-14)
nsubj(exist-18, God-15)
aux(exist-18, does-16)
neg(exist-18, not-17)
advcl(exist-9, exist-18)

