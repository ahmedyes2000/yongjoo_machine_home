(ROOT
  (SBARQ
    (SBAR (IN If)
      (S
        (NP (NN god))
        (VP (VBD was) (RB n't)
          (ADJP (JJ real)))))
    (, ,)
    (WHADVP (WRB why))
    (SQ (VBP are)
      (NP (PRP we))
      (ADJP (JJ alive)))
    (. .)))

mark(real-5, If-1)
nsubj(real-5, god-2)
cop(real-5, was-3)
neg(real-5, n't-4)
advcl(alive-10, real-5)
advmod(alive-10, why-7)
cop(alive-10, are-8)
nsubj(alive-10, we-9)
root(ROOT-0, alive-10)

