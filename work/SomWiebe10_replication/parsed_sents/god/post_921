(ROOT
  (S
    (NP (PRP I))
    (VP (VBP believe)
      (ADVP (RB so)))
    (. .)))

nsubj(believe-2, I-1)
root(ROOT-0, believe-2)
advmod(believe-2, so-3)

(ROOT
  (S
    (NP (PRP$ Your) (NN story))
    (VP (VBZ sounds)
      (ADJP (JJ interesting)))
    (. .)))

poss(story-2, Your-1)
nsubj(sounds-3, story-2)
root(ROOT-0, sounds-3)
acomp(sounds-3, interesting-4)

(ROOT
  (S
    (NP (PRP You))
    (VP (MD should)
      (VP (VB tell)
        (NP (PRP me))
        (PP (IN about)
          (NP (PRP it)))))
    (. .)))

nsubj(tell-3, You-1)
aux(tell-3, should-2)
root(ROOT-0, tell-3)
dobj(tell-3, me-4)
prep_about(tell-3, it-6)

