            (NP
              (NP (DT the) (NN rest))
              (PP (IN of)
                (NP (PRP us)))
              (SBAR
                (S
                  (VP (MD might)
                    (VP (VB put)
                      (NP (PRP you))
                      (PP (IN in)
                        (NP
                          (NP (NN prison))
                          (CC or)
                          (NP (DT a) (JJ mental) (NN institution)))))))))))))
    (. .)))

