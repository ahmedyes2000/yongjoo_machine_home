(ROOT
  (S
    (NP (DT This) (JJ personal) (, ,) (JJ religious) (NN belief))
    (VP (VBZ is)
      (ADVP (RB NOT))
      (NP
        (NP (DT any) (`` ``) (NN proof) ('' ''))
        (SBAR (IN that)
          (S
            (NP (NNP God))
            (VP (VBZ exists))))
        (, ,)
        (NP (NN period))))
    (. .)))

det(belief-5, This-1)
amod(belief-5, personal-2)
amod(belief-5, religious-4)
nsubj(proof-10, belief-5)
cop(proof-10, is-6)
neg(proof-10, NOT-7)
det(proof-10, any-8)
root(ROOT-0, proof-10)
dobj(exists-14, proof-10)
complm(exists-14, that-12)
nsubj(exists-14, God-13)
dep(proof-10, exists-14)
appos(proof-10, period-16)

