(ROOT
  (S
    (NP (EX There))
    (VP (VBP are)
      (NP
        (NP (JJ many) (JJ other) (JJ scientific) (NNS reasons))
        (SBAR
          (WHADVP (WRB why))
          (S
            (NP (DT the) (NN universe))
            (VP (VBD was)
              (VP (VBN created)))))))))

expl(are-2, There-1)
root(ROOT-0, are-2)
amod(reasons-6, many-3)
amod(reasons-6, other-4)
amod(reasons-6, scientific-5)
nsubj(are-2, reasons-6)
advmod(created-11, why-7)
det(universe-9, the-8)
nsubjpass(created-11, universe-9)
auxpass(created-11, was-10)
rcmod(reasons-6, created-11)

