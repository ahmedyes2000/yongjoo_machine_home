(ROOT
  (S
    (NP (DT That))
    (VP (VBZ is) (RB all)
      (ADJP (JJ true)))
    (. .)))

nsubj(true-4, That-1)
cop(true-4, is-2)
advmod(true-4, all-3)
root(ROOT-0, true-4)

(ROOT
  (S
    (NP (PRP I))
    (ADVP (RB already))
    (VP (VBD knew)
      (PP (IN about)
        (NP (NNP Christmas)))
      (S
        (VP (VBG coming)
          (PP (IN from)
            (NP (DT the) (NNS pagans))))))
    (. .)))

nsubj(knew-3, I-1)
advmod(knew-3, already-2)
root(ROOT-0, knew-3)
prep_about(knew-3, Christmas-5)
xcomp(knew-3, coming-6)
det(pagans-9, the-8)
prep_from(coming-6, pagans-9)

(ROOT
  (S
    (NP (NNS Whats))
    (ADJP (JJ wrong)
      (PP (IN with)
        (NP (NN faith))))
    (. .)))

dep(wrong-2, Whats-1)
root(ROOT-0, wrong-2)
prep_with(wrong-2, faith-4)

(ROOT
  (S
    (NP (PRP$ My) (NN religion))
    (VP (VBZ has)
      (ADVP (RB always))
      (VP (VBN had)
        (NP (DT that) (NN position))))
    (. .)))

poss(religion-2, My-1)
nsubj(had-5, religion-2)
aux(had-5, has-3)
advmod(had-5, always-4)
root(ROOT-0, had-5)
det(position-7, that-6)
dobj(had-5, position-7)

