(ROOT
  (S
    (NP (NNP God))
    (VP (VBZ is)
      (ADVP (RB just))
      (NP
        (NP (DT an) (JJ imaginary) (NN figure))
        (VP (VBN created)
          (PP (IN by)
            (NP (DT the) (NNS humans))))))
    (: ...)))

nsubj(figure-6, God-1)
cop(figure-6, is-2)
advmod(figure-6, just-3)
det(figure-6, an-4)
amod(figure-6, imaginary-5)
root(ROOT-0, figure-6)
partmod(figure-6, created-7)
det(humans-10, the-9)
agent(created-7, humans-10)

(ROOT
  (S (: ...)
    (S
      (NP (NNS terrorists)
        (CC and)
        (NNS wrongdoers))
      (VP (VB escape)
        (NP (DT the) (NN punishment))
        (PP (IN in)
          (NP
            (NP (DT the) (NN name))
            (PP (IN of)
              (NP (NN god)))))))
    (. ...)))

nsubj(escape-5, terrorists-2)
conj_and(terrorists-2, wrongdoers-4)
nsubj(escape-5, wrongdoers-4)
root(ROOT-0, escape-5)
det(punishment-7, the-6)
dobj(escape-5, punishment-7)
det(name-10, the-9)
prep_in(escape-5, name-10)
prep_of(name-10, god-12)

(ROOT
  (S
    (NP (NNP Hence) (NNP God))
    (VP (VBZ is)
      (NP
        (NP (DT a) (JJ lame) (NN excuse))
        (PP (IN for)
          (NP (DT the) (NNS people)))))))

