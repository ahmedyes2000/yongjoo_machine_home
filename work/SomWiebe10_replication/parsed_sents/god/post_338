(ROOT
  (S
    (PP (IN For)
      (NP (JJ obvious) (NNS reasons)))
    (: :)
    (NP (NNS religions))
    (VP (VBZ claims)
      (S
        (VP (TO to)
          (VP (VB explain)
            (NP
              (NP (DT the) (NN foundation)
                (CC and)
                (NN structure))
              (PP (IN of)
                (NP (DT the) (NN universe))))
            (PP (IN without)
              (NP (JJ good) (JJ enough) (NN evidence)))))))
    (. .)))

amod(reasons-3, obvious-2)
prep_for(claims-6, reasons-3)
nsubj(claims-6, religions-5)
xsubj(explain-8, religions-5)
root(ROOT-0, claims-6)
aux(explain-8, to-7)
xcomp(claims-6, explain-8)
det(foundation-10, the-9)
dobj(explain-8, foundation-10)
dobj(explain-8, structure-12)
conj_and(foundation-10, structure-12)
det(universe-15, the-14)
prep_of(foundation-10, universe-15)
amod(evidence-19, good-17)
amod(evidence-19, enough-18)
prep_without(explain-8, evidence-19)

(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is)
      (ADVP (RB therefore))
      (NP
        (NP (DT the) (NN duty))
        (PP (IN of)
          (NP (JJ scientific) (NN thinking) (NNS people))))
      (S
        (VP (TO to)
          (VP (VB point)
            (ADVP (DT that) (IN out))
            (SBAR (IN that)
              (S
                (NP (DT the) (NN evidence))
                (VP (VBZ is)
                  (ADJP (JJ flawed)))))))))))

nsubj(duty-5, It-1)
cop(duty-5, is-2)
advmod(duty-5, therefore-3)
det(duty-5, the-4)
root(ROOT-0, duty-5)
amod(people-9, scientific-7)
nn(people-9, thinking-8)
prep_of(duty-5, people-9)
aux(point-11, to-10)
xcomp(duty-5, point-11)
dep(out-13, that-12)
advmod(point-11, out-13)
complm(flawed-18, that-14)
det(evidence-16, the-15)
nsubj(flawed-18, evidence-16)
cop(flawed-18, is-17)
ccomp(point-11, flawed-18)

