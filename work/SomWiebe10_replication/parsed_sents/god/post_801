(ROOT
  (S
    (S
      (VP
        (ADVP (RB Perhaps))
        (VBG believing)
        (ADVP (RB just))
        (PP (IN for)
          (NP (DT the) (NNS benefits)))))
    (VP (VBZ is)
      (NP
        (NP (DT a) (NN sin))
        (SBAR
          (WHNP (DT all)
            (WHPP (IN on)
              (WHNP (PRP it))))
          (S
            (VP (VBZ 's)
              (ADJP (JJ own)))))))
    (. .)))

advmod(believing-2, Perhaps-1)
csubj(sin-9, believing-2)
advmod(believing-2, just-3)
det(benefits-6, the-5)
