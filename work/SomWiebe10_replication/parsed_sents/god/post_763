(ROOT
  (SINV
    (VP (VBD Yes)
      (SBAR
        (S
          (NP (PRP I))
          (VP (VBP think)
            (SBAR
              (S
                (S
                  (VP (VBG believing)
                    (PP (IN in)
                      (NP (NN god)))))
                (VP (VBZ is)
                  (ADJP (JJ worth)))))))))
    (NP (PRP it))
    (SBAR (IN because)
      (S
        (NP (PRP he))
        (VP (VBP make)
          (NP (DT the) (NN world)))))))

root(ROOT-0, Yes-1)
nsubj(think-3, I-2)
ccomp(Yes-1, think-3)
csubj(worth-8, believing-4)
prep_in(believing-4, god-6)
cop(worth-8, is-7)
ccomp(think-3, worth-8)
nsubj(Yes-1, it-9)
mark(make-12, because-10)
nsubj(make-12, he-11)
dep(Yes-1, make-12)
det(world-14, the-13)
dobj(make-12, world-14)

