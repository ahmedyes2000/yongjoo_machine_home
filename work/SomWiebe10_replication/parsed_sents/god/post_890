(ROOT
  (S
    (VP (VB Althought)
      (SBAR
        (S
          (NP (EX there))
          (VP (VBZ is) (RB neither)
            (SBAR (IN that)
              (S
                (NP (PRP he))
                (VP
                  (VP (VBZ exists))
                  (CC or)
                  (RB not)
                  (VP (CC but)
                    (VP (VB neverthe)
                      (ADVP
                        (ADVP (RBR less))
                        (S (MD would)
                          (NP (PRP it))
                          (VP (VB hurt)
                            (SBAR (IN for)
                              (S
                                (NP
                                  (NP (PRP us))
                                  (PP (TO to)
                                    (NP (JJ main))))
                                (VP (VBP believe))))))))
                    (CC or)
                    (VP (VB have)
                      (NP (NN respect)))
                    (PP (IN in)
                      (NP
                        (ADJP (RB sombody) (JJ elses))
                        (NN view)))))))))))))

root(ROOT-0, Althought-1)
expl(is-3, there-2)
ccomp(Althought-1, is-3)
advmod(is-3, neither-4)
complm(exists-7, that-5)
nsubj(exists-7, he-6)
ccomp(is-3, exists-7)
ccomp(is-3, not-9)
conj_or(exists-7, not-9)
neg(exists-7, not-9)
dep(exists-7, neverthe-11)
advmod(neverthe-11, less-12)
dep(hurt-15, would-13)
nsubj(hurt-15, it-14)
dep(less-12, hurt-15)
mark(believe-20, for-16)
nsubj(believe-20, us-17)
prep_to(us-17, main-19)
advcl(hurt-15, believe-20)
dep(exists-7, have-22)
conj_or(neverthe-11, have-22)
dobj(have-22, respect-23)
advmod(elses-26, sombody-25)
amod(view-27, elses-26)
prep_in(neverthe-11, view-27)

