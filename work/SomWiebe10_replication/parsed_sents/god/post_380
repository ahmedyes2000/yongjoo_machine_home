(ROOT
  (S
    (NP (FW i))
    (VP (VBP believe)
      (PP (IN in)
        (NP (NN god)))
      (SBAR (IN so)
        (S
          (NP (FW i))
          (VP (MD wo) (RB n't)
            (VP (VB go)
              (PP (TO to)
                (NP (NN hell) (NNS ^))))))))
    (. .)))

nsubj(believe-2, i-1)
root(ROOT-0, believe-2)
prep_in(believe-2, god-4)
mark(go-9, so-5)
