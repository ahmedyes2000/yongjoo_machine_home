(ROOT
  (S
    (S
      (NP (EX There))
      (VP (VBP are)
        (NP
          (NP (JJ large) (NNP Hindu) (NNS populations))
          (PP (IN in)
            (NP
              (NP (NNP Bangladesh) (, ,) (NNP Maritius) (, ,) (NNP Guyana) (, ,) (NNP Nepal) (, ,))
              (CC and)
              (NP (JJ many) (JJ other) (NNS countries)))))))
    (, ,) (IN so)
    (S
      (NP (NNP Hinduism))
      (VP (VBZ is) (RB n't)
        (ADVP (RB just))
        (PP (IN in)
          (NP (NNP India)))))
    (. .)))

expl(are-2, There-1)
root(ROOT-0, are-2)
amod(populations-5, large-3)
nn(populations-5, Hindu-4)
nsubj(are-2, populations-5)
nn(Nepal-13, Bangladesh-7)
appos(Nepal-13, Maritius-9)
appos(Nepal-13, Guyana-11)
prep_in(populations-5, Nepal-13)
amod(countries-18, many-16)
amod(countries-18, other-17)
prep_in(populations-5, countries-18)
conj_and(Nepal-13, countries-18)
dep(are-2, so-20)
nsubj(is-22, Hinduism-21)
ccomp(are-2, is-22)
neg(is-22, n't-23)
advmod(is-22, just-24)
prep_in(is-22, India-26)

