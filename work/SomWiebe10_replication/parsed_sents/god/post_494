(ROOT
  (S
    (NP
      (NP (DT The) (NN ending))
      (PP (IN of)
        (NP (DT this))))
    (VP (VBZ is)
      (ADJP (JJ fantastic)))
    (. !)))

det(ending-2, The-1)
nsubj(fantastic-6, ending-2)
prep_of(ending-2, this-4)
cop(fantastic-6, is-5)
root(ROOT-0, fantastic-6)

(ROOT
  (S
    (NP (PRP I))
    (VP
      (ADVP (RB never))
      (VBD thought)
      (PP (IN of)
        (NP (PRP it)))
      (PP (IN in)
        (NP (DT that) (NN fashion))))))

nsubj(thought-3, I-1)
neg(thought-3, never-2)
root(ROOT-0, thought-3)
prep_of(thought-3, it-5)
det(fashion-8, that-7)
prep_in(thought-3, fashion-8)

