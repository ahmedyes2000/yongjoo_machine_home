            (S
              (NP (DT a) (NN chance))
              (VP (TO to)
                (VP (VB see)
                  (NP (DT a) (JJ fresh) (JJ dug) (NN grave))))))))
      (, ,)
      (VP (VB stare)
        (PP (IN at)
          (NP
            (NP (PRP it))
            (PP (IN for)
              (NP (DT a) (NN while)))))
        (SBAR (IN because) (IN that)
          (S
            (NP (PRP$ my) (NN friend))
            (VP (VBZ is)
              (NP (PRP$ your) (NN destiny)))))))
    (. .)))

