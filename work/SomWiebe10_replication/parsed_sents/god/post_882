(ROOT
  (S
    (NP (NNP God))
    (VP (VBZ has)
      (VP (VBN been)
        (PP (IN around)
          (ADVP
            (NP (DT a) (NN lot))
            (RBR longer)))
        (PP (IN than)
          (NP (DT this) (NN debate)))))
    (. !)))

nsubj(been-3, God-1)
aux(been-3, has-2)
root(ROOT-0, been-3)
det(lot-6, a-5)
npadvmod(longer-7, lot-6)
prepc_around(been-3, longer-7)
det(debate-10, this-9)
prep_than(been-3, debate-10)

