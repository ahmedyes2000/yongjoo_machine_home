        (SBAR
          (WHADVP (WRB why))
          (S
            (NP (NNS people))
            (VP (MD can) (RB not)
              (VP (VB accept)
                (SBAR (IN that)
                  (S
                    (NP
                      (NP (NN religion))
                      (CC and)
                      (NP (NNP God)))
                    (VP (VBP exist)
                      (ADVP (RB simultaneously))
                      (PP (IN with)
                        (NP (NN science))))))))))))
    (. .)))

