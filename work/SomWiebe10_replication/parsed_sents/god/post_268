  (S (RB Then)
    (NP (PRP it))
    (VP (VBZ stands)
      (PP (TO to)
        (NP
          (NP (NN reason))
          (: :)
          (SQ (VBZ is)
            (NP (PRP it))
            (ADJP (JJ useful)
              (S
                (VP (TO to)
                  (VP (VB believe)
                    (PP (IN in)
                      (NP (NN god)))
                    (PP (IN without)
                      (NP (DT a) (JJ religious) (NN basis)))))))))))
    (. ?)))

advmod(stands-3, Then-1)
nsubj(stands-3, it-2)
root(ROOT-0, stands-3)
