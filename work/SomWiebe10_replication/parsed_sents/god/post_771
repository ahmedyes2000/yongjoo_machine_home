(ROOT
  (S
    (S
      (VP (VBG Believing)
        (PP (IN in)
          (NP (NN god)))))
    (VP (VBZ is)
      (ADJP (JJ important))
      (SBAR (IN because)
        (S
          (NP (PRP it))
          (VP (VBZ gives)
            (NP (NN humanity))
            (NP
              (NP (DT a) (NN reason))
              (PP (IN for)
                (NP (NN existence))))))))
    (. .)))

csubj(important-5, Believing-1)
prep_in(Believing-1, god-3)
cop(important-5, is-4)
root(ROOT-0, important-5)
mark(gives-8, because-6)
nsubj(gives-8, it-7)
advcl(important-5, gives-8)
iobj(gives-8, humanity-9)
det(reason-11, a-10)
dobj(gives-8, reason-11)
prep_for(reason-11, existence-13)

