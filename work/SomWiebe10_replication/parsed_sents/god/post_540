  (S
    (NP (PRP I))
    (VP (VBP say)
      (ADVP (RB no)
        (SBAR (IN because)
          (S
            (NP (PRP they))
            (VP (VBD made)
              (NP (PRP him))
              (PRT (RP up))
              (PP (IN as)
                (NP (DT a) (JJ lazy) (NN excuse)))
              (PP (IN for)
                (SBAR
                  (WHADVP (WRB how))
                  (S
                    (NP (NN everything))
                    (VP (VBD was)
                      (VP (VBN made)
                        (SBAR (IN because)
                          (S
                            (ADVP (RB back) (RB then))
                            (NP (PRP they))
                            (VP (VBD did) (RB n't)
                              (VP (VB know)
                                (PP (IN about)
                                  (SBAR
                                    (WHNP (WP what))
                                    (S
                                      (VP (VBD was)
                                        (VP (VBG going)
                                          (PRT (RP on))
                                          (PP (IN in)
                                            (NP (NN space))))))))))))))))))))))
    (. .)))

