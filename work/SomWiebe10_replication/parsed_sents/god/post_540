(ROOT
  (S (CC But)
    (NP (NN today) (NNS people))
    (VP (MD can)
      (VP (VB make)
        (NP (DT a) (JJR better) (NN guess))
        (PP (IN on)
          (SBAR
            (WHADVP (WRB how))
            (S
              (NP (DT the) (NN earth))
              (VP (VB and)
                (NP (PRP us))
                (SBAR
                  (WHADVP (WRB where))
                  (S
                    (VP (VBD made))))))))))
    (. .)))

