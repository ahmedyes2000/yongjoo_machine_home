(ROOT
  (S
    (S
      (VP
        (ADVP (RB never))
        (VBN been)))
    (VP (VBD proved)
      (PP (TO to)
        (NP (PRP me)))
      (, ,)
      (ADVP (RB ever)))
    (. .)))

