    (ADVP (RB Well))
    (NP (EX there))
    (VP (VBZ has)
      (VP (VBN been)
        (NP (JJ many) (DT a) (NN convert))
        (PP (IN on)
          (NP (DT the) (NN battlefield)))
        (, ,)
        (PP (IN in)
          (NP
            (NP (DT the) (NNS moments))
            (PP (IN between)
              (NP
                (NP (DT the) (NN saddle))
                (CC and)
                (NP (DT the) (NN ground))))))))
    (. .)))

