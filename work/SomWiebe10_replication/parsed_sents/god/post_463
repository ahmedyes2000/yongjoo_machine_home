(ROOT
  (S
    (NP (PRP I))
    (VP (VBP am)
      (VP (VBN allowed)
        (S
          (VP (TO to)
            (VP (VB believe)
              (PP (IN in)
                (NP
                  (NP (DT both) (NNP God))
                  (CC and)
                  (NP (DT the) (NNP Big) (NNP Bang)))))))))
    (. .)))

nsubjpass(allowed-3, I-1)
xsubj(believe-5, I-1)
auxpass(allowed-3, am-2)
root(ROOT-0, allowed-3)
aux(believe-5, to-4)
xcomp(allowed-3, believe-5)
det(God-8, both-7)
prep_in(believe-5, God-8)
det(Bang-12, the-10)
nn(Bang-12, Big-11)
prep_in(believe-5, Bang-12)
conj_and(God-8, Bang-12)

(ROOT
  (S
    (NP (PRP It))
    (ADVP (RB just) (RB so))
    (VP (VBZ happens)
      (SBAR (IN that)
        (S
          (NP (PRP I))
          (VP (VBP believe)
            (SBAR
              (S
                (NP (DT the) (NN creation) (NNS stories))
                (VP (VBP are)
                  (VP (VBG mythical)
                    (PP (IN in)
                      (NP (PRP$ their) (NN nature)))))))))))
    (. .)))

nsubj(happens-4, It-1)
advmod(so-3, just-2)
advmod(happens-4, so-3)
root(ROOT-0, happens-4)
complm(believe-7, that-5)
