(ROOT
  (S
    (NP (NN Nobody) (JJ important))
    (ADVP (RB ever))
    (VP (VBD said)
      (S (`` ``)
        (NP (DT the) (NNP Bible))
        (VP (VBZ is)
          (NP
            (ADJP (CD 100) (NN %))
            (NN fact))
          (, ,) ('' '')
          (PP (IN except) (IN for)
            (NP
              (NP (RB maybe) (DT a) (JJ few) (NNS Popes))
              (PP (IN throughout)
                (NP (DT the) (NNS ages))))))))
    (. .)))

