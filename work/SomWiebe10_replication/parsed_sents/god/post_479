(ROOT
  (S
    (NP (PRP I))
    (VP (VBP BELIEVE)
      (SBAR (IN that)
        (S
          (NP (NNP G.O.D))
          (VP
            (VP (VBZ stands)
              (PP (IN for)
                (NP
                  (NP (NNS governers))
                  (PP (IN of)
                    (NP (NN destiny))))))
            (CC and)
            (VP (VBZ represents)
              (NP
                (NP (DT the) (NNS governments))
                (PP (IN of)
                  (NP (DT the) (NN world)))))))))
    (. .)))

nsubj(BELIEVE-2, I-1)
root(ROOT-0, BELIEVE-2)
complm(stands-5, that-3)
nsubj(stands-5, G.O.D-4)
nsubj(represents-11, G.O.D-4)
ccomp(BELIEVE-2, stands-5)
