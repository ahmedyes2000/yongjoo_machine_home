det(world-16, the-15)
prep_of(governments-13, world-16)

(ROOT
  (S
    (S
      (NP (NNP Life))
      (VP (VBZ is)
        (NP
          (NP (JJ THEIR) (NN game))
          (PP (IN of)
            (NP (NN chess))))))
    (CC and)
    (S
      (NP
        (NP (NNP WE))
        (PRN (-LRB- -LRB-)
          (NP
            (NP (PRP YOU))
            (CC &)
            (NP (PRP I)))
          (-RRB- -RRB-)))
      (VP (VBP are)
        (NP (DT the) (NNS pawns))))
    (. .)))

nsubj(game-4, Life-1)
cop(game-4, is-2)
amod(game-4, THEIR-3)
root(ROOT-0, game-4)
prep_of(game-4, chess-6)
nsubj(pawns-16, WE-8)
dep(WE-8, YOU-10)
