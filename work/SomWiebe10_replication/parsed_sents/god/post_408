(ROOT
  (S
    (SBAR (RB just) (IN because)
      (S
        (NP (PRP you))
        (VP
          (VP (VBP have)
            (NP (NN faith)))
          (CC and)
          (VP (VBP believe)
            (PP (IN in)
              (NP (NNS miracles)))))))
    (NP (PRP you))
    (VP (VBP believe)
      (PP (IN in)
        (NP (NN god))))
    (. ...)))

advmod(have-4, just-1)
dep(have-4, because-2)
nsubj(have-4, you-3)
nsubj(believe-7, you-3)
advcl(believe-11, have-4)
dobj(have-4, faith-5)
conj_and(have-4, believe-7)
advcl(believe-11, believe-7)
prep_in(believe-7, miracles-9)
nsubj(believe-11, you-10)
root(ROOT-0, believe-11)
prep_in(believe-11, god-13)

(ROOT
  (INTJ (UH .)))


(ROOT
  (S
    (VP (VB point)
      (PRT (RP out))
      (NP (DT the) (JJ godly) (NNS miracles)))
    (: ...)))

root(ROOT-0, point-1)
prt(point-1, out-2)
det(miracles-5, the-3)
amod(miracles-5, godly-4)
dobj(point-1, miracles-5)

