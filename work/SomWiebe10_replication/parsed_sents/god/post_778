(ROOT
  (S
    (S
      (NP (PRP it))
      (VP (VBZ is)
        (ADJP (JJ worth)
          (NP (PRP it)))))
    (: ...)
    (S
      (S
        (NP (NN god))
        (VP (VBZ gives)
          (NP (PRP you))
          (NP
            (NP (NN hope))
            (PP (IN of)
              (NP (JJ everlasting) (NN life))))))
      (CC and)
      (S
        (NP (PRP you))
        (VP (VBP do) (RB n't)
          (ADVP (RB just))
          (VP (VB rot)
            (PP (IN in)
              (NP (DT the) (NN ground) (NN nothing)))
            (SBAR (IN after)
              (S
                (NP (PRP you))
                (VP (VBP know)
                  (NP
                    (NP (PDT all) (DT that) (NN jazz))
                    (SBAR
                      (S
                        (NP (PRP you))
                        (VP (VBP say)
                          (SBAR
                            (S
                              (NP (EX there))
                              (VP (VBZ is)
                                (NP
                                  (NP (DT a) (NN thing))
                                  (VP (VBN called)
                                    (NP (NN resurection) (NNS people))))))))))))))))))
    (. !)))

