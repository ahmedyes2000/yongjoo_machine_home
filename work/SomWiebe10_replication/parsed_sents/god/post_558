(ROOT
  (S
    (NP (NNP God))
    (VP (VBZ is)
      (VP (VBN made)
        (PP (IN for)
          (NP
            (NP (JJ stupid) (NNS adults))
            (CC and)
            (NP (JJ young) (NNS children))))
        (S
          (VP (TO to)
            (VP (VB give)
              (NP (PRP them))
              (NP (DT a) (NN conscience)))))))
    (. .)))

nsubjpass(made-3, God-1)
xsubj(give-11, God-1)
auxpass(made-3, is-2)
root(ROOT-0, made-3)
amod(adults-6, stupid-5)
prep_for(made-3, adults-6)
amod(children-9, young-8)
prep_for(made-3, children-9)
conj_and(adults-6, children-9)
aux(give-11, to-10)
xcomp(made-3, give-11)
iobj(give-11, them-12)
det(conscience-14, a-13)
dobj(give-11, conscience-14)

(ROOT
  (NP
    (NP (NN Nothing))
    (NP
      (NP (JJR more) (NN nothing))
      (NP (JJR less)))))

root(ROOT-0, Nothing-1)
amod(nothing-3, more-2)
dep(Nothing-1, nothing-3)
dep(nothing-3, less-4)

