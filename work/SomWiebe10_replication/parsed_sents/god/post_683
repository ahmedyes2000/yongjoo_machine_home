(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP (VBP 're)
          (VP (VBG arguing)
            (PP (IN for)
              (NP (NN atheism)))))))
    (NP (PRP it))
    (ADVP (RB sure))
    (VP (VBZ sounds)
      (SBAR (IN like)
        (S
          (NP (PRP you))
          (VP (VBP are)
            (VP (VBG borrowing)
              (NP (NN language))
              (PP (IN from)
                (NP (DT another) (NN worldview)
                  (S
                    (VP (TO to)
                      (VP (VB support)
                        (NP (PRP$ your) (NN view))))))))))))
    (. .)))

