(ROOT
  (S
    (NP (WDT what))
    (VP (VBP are)
      (NP (DT those) (NNS miracles)))
    (: ...)))

nsubj(miracles-4, what-1)
cop(miracles-4, are-2)
det(miracles-4, those-3)
root(ROOT-0, miracles-4)

(ROOT
  (FRAG (: ...)
    (ADVP (RB miracles))
    (NP
      (NP (JJ single) (NN handedly) (NN can))
      (NP (RB not)
        (ADJP (JJ JUSTIFY)
          (NP (DT the) (NN phrase)))
        (NN GOD) (NNS EXISTS)))))

dep(can-5, miracles-2)
amod(can-5, single-3)
nn(can-5, handedly-4)
root(ROOT-0, can-5)
neg(EXISTS-11, not-6)
amod(EXISTS-11, JUSTIFY-7)
det(phrase-9, the-8)
dep(JUSTIFY-7, phrase-9)
nn(EXISTS-11, GOD-10)
dep(can-5, EXISTS-11)

