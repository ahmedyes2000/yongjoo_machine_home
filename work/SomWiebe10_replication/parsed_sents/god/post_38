(ROOT
  (S
    (VP (VBG Doing)
      (SBAR
        (S
          (NP
            (NP (DT all))
            (PP (IN of)
              (NP (DT those) (NNS things))))
          (VP (VBZ makes)
            (S
              (NP (PRP you))
              (ADJP (JJ Hitler-like)))))))))

root(ROOT-0, Doing-1)
nsubj(makes-6, all-2)
det(things-5, those-4)
prep_of(all-2, things-5)
ccomp(Doing-1, makes-6)
nsubj(Hitler-like-8, you-7)
xcomp(makes-6, Hitler-like-8)

