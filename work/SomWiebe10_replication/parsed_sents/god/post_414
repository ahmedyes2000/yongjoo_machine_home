(ROOT
  (S
    (NP (PRP I))
    (VP (VBP believe)
      (PP (IN in)
        (NP (PRP$ my) (NNP god) (NNP Jesus) (NNP Christ)))
      (, ,)
      (SBAR (IN because)
        (S
          (NP (EX there))
          (VP (VBZ is)
            (NP
              (NP (NN proof))
              (PP (IN of)
                (S
                  (NP (PRP him))
                  (VP (VBG being)
                    (NP
                      (NP (RB here))
                      (PP (IN on)
                        (NP (NN earth))))))))))))
    (. .)))

