(ROOT
  (FRAG
    (S
      (ADVP (RB just))
      (VP (VBG making)
        (SBAR
          (S
            (NP (JJ random) (NNS statements))
            (VP (VBZ is) (RB not)
              (NP
                (NP (DT an) (NN argument))
                (PP (IN in)
                  (NP
                    (NP (NN support))
                    (PP (IN of)
                      (NP (DT the) (NN question))))))
              (SBAR (IN if)
                (S
                  (NP (PRP you))
                  (ADVP (RB just))
                  (VP (VBP want)
                    (S
                      (VP (TO to)
                        (VP (VB say)
                          (S
                            (NP (PRP you))
                            (VP
                              (VP (VB believe)
                                (PP (IN in)
                                  (NP (NN god))))
                              (, ,)
                              (VP (VB go)
                                (PP (TO to)
                                  (NP (NN church)))))))))))))))))))

advmod(making-2, just-1)
root(ROOT-0, making-2)
amod(statements-4, random-3)
nsubj(argument-8, statements-4)
cop(argument-8, is-5)
neg(argument-8, not-6)
det(argument-8, an-7)
ccomp(making-2, argument-8)
prep_in(argument-8, support-10)
det(question-13, the-12)
prep_of(support-10, question-13)
mark(want-17, if-14)
nsubj(want-17, you-15)
xsubj(say-19, you-15)
advmod(want-17, just-16)
advcl(argument-8, want-17)
aux(say-19, to-18)
xcomp(want-17, say-19)
nsubj(believe-21, you-20)
ccomp(say-19, believe-21)
prep_in(believe-21, god-23)
dep(believe-21, go-25)
prep_to(go-25, church-27)

