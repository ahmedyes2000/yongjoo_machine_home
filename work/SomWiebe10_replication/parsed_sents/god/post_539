(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is)
      (ADJP (RB completely) (JJ foolish)
        (PP (IN on)
          (NP
            (NP (DT the) (NN face))
            (PP (IN of)
              (NP (PRP it)))))))
    (. !)))

