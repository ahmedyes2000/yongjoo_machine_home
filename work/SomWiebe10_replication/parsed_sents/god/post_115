(ROOT
  (S
    (S
      (NP (NNP Very))
      (ADVP (RB well))
      (VP (VBD said)))
    (: ;)
    (S
      (NP (PRP$ my) (NNS hats))
      (ADJP (IN off)
        (PP (TO to)
          (NP (PRP you)))))
    (. .)))

nsubj(said-3, Very-1)
advmod(said-3, well-2)
root(ROOT-0, said-3)
poss(hats-6, my-5)
dep(off-7, hats-6)
parataxis(said-3, off-7)
prep_to(off-7, you-9)

