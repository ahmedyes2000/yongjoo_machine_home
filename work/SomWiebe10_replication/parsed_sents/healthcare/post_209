(ROOT
  (S
    (NP (NNP Universal) (NN health) (NN care))
    (VP (VBZ creates)
      (NP
        (NP (DT a) (JJ big) (, ,) (JJ inefficient) (NN bureaucracy))
        (SBAR
          (S
            (NP
              (NP (DT The) (NN cause))
              (PP (IN of)
                (NP (DT this))))
            (VP (VBZ is)
              (NP
                (NP (JJR greater) (JJ bureaucratic) (NN oversight))
                (CC and)
                (NP (JJR more) (NN paperwork))))))))
    (. .)))

