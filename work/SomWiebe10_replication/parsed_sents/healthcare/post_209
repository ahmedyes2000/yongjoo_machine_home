(ROOT
  (S
    (NP (DT This))
    (VP
      (VP (VBZ lowers)
        (NP
          (NP (DT the) (NN productivity))
          (PP (IN of)
            (NP (NNS hospitals)))))
      (, ,)
      (CC and)
      (VP
        (ADVP (RB thus))
        (VBZ increases)
        (NP (DT the) (NNS costs))
        (PP (IN for)
          (NP
            (NP (DT the) (JJ same) (NN amount))
            (PP (IN of)
              (NP
                (NP (NN work))
                (VP (VBN done))))))))
    (. .)))

