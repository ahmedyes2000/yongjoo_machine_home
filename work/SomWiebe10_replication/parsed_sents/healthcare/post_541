(ROOT
  (S
    (ADVP (RB Man))
    (, ,)
    (NP (PRP it))
    (VP (VBZ 's)
      (NP
        (NP (DT the) (NN government) (POS 's))
        (NN job)
        (S
          (VP (TO to)
            (VP (VB protect)
              (NP (DT the) (NNS people)))))))
    (. .)))

advmod(job-8, Man-1)
nsubj(job-8, it-3)
cop(job-8, 's-4)
det(government-6, the-5)
poss(job-8, government-6)
root(ROOT-0, job-8)
aux(protect-10, to-9)
infmod(job-8, protect-10)
det(people-12, the-11)
dobj(protect-10, people-12)

(ROOT
  (S
    (NP (NNP Universal) (NN health) (NN care))
    (VP (MD would)
      (VP (VB be)
        (NP
          (NP (DT a) (NN godsend))
          (PP (IN in)
            (NP (PRP$ our) (JJ current) (NN state))))))
    (. .)))

nn(care-3, Universal-1)
nn(care-3, health-2)
nsubj(godsend-7, care-3)
aux(godsend-7, would-4)
cop(godsend-7, be-5)
det(godsend-7, a-6)
root(ROOT-0, godsend-7)
poss(state-11, our-9)
