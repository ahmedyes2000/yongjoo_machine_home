(ROOT
  (S
    (NP (EX There))
    (VP (VBP are)
      (NP (JJ many) (NNS ways)
        (S
          (VP (TO to)
            (VP (VB pay)
              (PP (IN for)
                (NP
                  (NP (DT any) (JJ added) (NNS costs))
                  (PP (IN of)
                    (NP (JJ universal) (NN health) (NN care)))))
              (NP
                (NP (DT Some) (VBN proposed) (NN means))
                (PP (IN of)
                  (S
                    (VP
                      (VP (VBG dealing)
                        (SBAR (IN with)
                          (S
                            (NP (DT these) (VBN added) (NNS costs))
                            (VP (VBP include)
                              (NP (DT a) (NN cigarette) (NN tax))))))
                      (CC and)
                      (VP (VBG repealing)
                        (NP (NN tax) (NNS cuts))
                        (PP (IN for)
                          (NP (DT the) (JJ wealthy)))))))))))))
    (. .)))

expl(are-2, There-1)
root(ROOT-0, are-2)
amod(ways-4, many-3)
nsubj(are-2, ways-4)
aux(pay-6, to-5)
infmod(ways-4, pay-6)
det(costs-10, any-8)
amod(costs-10, added-9)
prep_for(pay-6, costs-10)
amod(care-14, universal-12)
nn(care-14, health-13)
prep_of(costs-10, care-14)
det(means-17, Some-15)
amod(means-17, proposed-16)
dobj(pay-6, means-17)
prepc_of(means-17, dealing-19)
mark(include-24, with-20)
det(costs-23, these-21)
amod(costs-23, added-22)
nsubj(include-24, costs-23)
advcl(dealing-19, include-24)
det(tax-27, a-25)
nn(tax-27, cigarette-26)
dobj(include-24, tax-27)
prepc_of(means-17, repealing-29)
conj_and(dealing-19, repealing-29)
nn(cuts-31, tax-30)
dobj(repealing-29, cuts-31)
det(wealthy-34, the-33)
prep_for(repealing-29, wealthy-34)

