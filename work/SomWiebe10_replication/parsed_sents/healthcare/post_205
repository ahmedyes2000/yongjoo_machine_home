(ROOT
  (S
    (ADVP (RB Therefore))
    (, ,)
    (PP (IN in)
      (NP
        (NP (NNS countries))
        (SBAR
          (WHADVP (WRB where))
          (S
            (NP (DT the) (NN population))
            (VP (VBZ is)
              (VP (VBG aging)))))))
    (, ,)
    (NP (PRP it))
    (VP (MD may)
      (VP (VB be)
        (ADJP (JJ important)
          (S
            (VP (TO to)
              (VP (VB avoid)
                (NP (DT a) (JJ universal) (NN health) (NN care) (NN program))))))))
    (. .)))

