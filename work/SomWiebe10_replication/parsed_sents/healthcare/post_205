    (, ,)
    (NP
      (NP (DT the) (NN strain))
      (PP (IN of)
        (NP (DT a) (JJ universal) (NN health) (NN care) (NN program))))
    (VP (VBZ grows) (, ,)
      (PP (IN with)
        (NP
          (NP (DT a) (JJR larger) (NN percentage))
          (PP (IN of)
            (NP (DT the) (NN population)))))
      (S
        (VP (VBG reaching)
          (NP
            (NP (DT an) (NN age))
