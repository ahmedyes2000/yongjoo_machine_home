det(U.S.-23, the-22)
prep_in(living-20, U.S.-23)

(ROOT
  (S
    (S
      (NP (PRP We))
      (VP (VBP do)
        (ADVP (RB live))
        (PP (IN in)
          (NP (DT the) (JJ 21st) (NN century)))))
    (CC and)
    (S
      (NP (PRP we))
      (VP (VBP have)
        (VP
          (ADVP (RB just)
            (PP (IN about)
              (NP (NN everything))))
          (VBN imaginable) (, ,)
          (PP (IN except)
            (NP (JJ Universal) (NN health) (NN care))))))
    (. .)))

