  (S
    (NP (PRP I))
    (VP (VBP think)
      (SBAR (IN that)
        (S
          (NP (DT the) (NN government))
          (VP (MD should)
            (VP (VB offer)
              (NP (JJ Universal) (NN health) (NN care))
              (SBAR (IN because)
                (S
                  (NP (EX there))
                  (VP (VBZ is)
                    (ADVP (RB already))
                    (PP (IN over)
                      (NP
                        (NP
                          (QP (CD 40) (CD million))
                          (JJ uninsured) (NNS Americans))
                        (VP (VBG living)
                          (PP (IN in)
                            (NP (DT the) (NNP U.S.))))))))))))))
    (. .)))

