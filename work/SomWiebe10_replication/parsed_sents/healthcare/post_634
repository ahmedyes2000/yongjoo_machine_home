(ROOT
  (FRAG
    (PP (IN In)
      (NP (DT an) (JJ ideal) (NN world) (RB maybe)))
    (. .)))

root(ROOT-0, In-1)
det(world-4, an-2)
amod(world-4, ideal-3)
pobj(In-1, world-4)
advmod(world-4, maybe-5)

(ROOT
  (S
    (NP
      (NP (NNS People) (POS 's))
      (NNS wages))
    (VP (VBD needed)
      (S
        (VP (VBG paying))))
    (. .)))

poss(wages-3, People-1)
nsubj(needed-4, wages-3)
root(ROOT-0, needed-4)
xcomp(needed-4, paying-5)

