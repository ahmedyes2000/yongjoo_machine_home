(ROOT
  (S
    (NP (PRP You))
    (VP (MD should)
      (VP (VB have)
        (S
          (VP (TO to)
            (VP (VB work)
              (PP (IN for)
                (NP (PRP$ your) (NN stuff))))))))
    (. .)))

nsubj(have-3, You-1)
xsubj(work-5, You-1)
aux(have-3, should-2)
root(ROOT-0, have-3)
aux(work-5, to-4)
xcomp(have-3, work-5)
poss(stuff-8, your-7)
prep_for(work-5, stuff-8)

