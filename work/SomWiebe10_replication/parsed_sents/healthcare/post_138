(ROOT
  (S
    (NP (DT The)
      (ADJP (RB well) (VBN insured)))
    (ADVP (RB also))
    (VP (VBP face)
      (NP (NNS risks))
      (PP (IN without)
        (NP
          (NP (JJ universal) (NN health) (NN care))
          (SBAR
            (S
              (NP (DT The) (JJ uninsured))
              (VP (VBP are) (RB not)
                (NP
                  (NP (DT the) (JJ only) (NN concern))
                  (SBAR
                    (WHADVP (WRB when))
                    (S
                      (NP (DT a) (NN society))
                      (VP (VBZ lacks)
                        (NP (JJ universal) (NN health) (NN care))))))))))))
    (. .)))

det(insured-3, The-1)
advmod(insured-3, well-2)
nsubj(face-5, insured-3)
advmod(face-5, also-4)
root(ROOT-0, face-5)
dobj(face-5, risks-6)
amod(care-10, universal-8)
nn(care-10, health-9)
prep_without(face-5, care-10)
det(uninsured-12, The-11)
nsubj(concern-17, uninsured-12)
cop(concern-17, are-13)
neg(concern-17, not-14)
det(concern-17, the-15)
amod(concern-17, only-16)
rcmod(care-10, concern-17)
tmod(lacks-21, concern-17)
advmod(lacks-21, when-18)
det(society-20, a-19)
nsubj(lacks-21, society-20)
dep(concern-17, lacks-21)
amod(care-24, universal-22)
nn(care-24, health-23)
dobj(lacks-21, care-24)

(ROOT
  (S
    (NP (DT The) (NN insured))
    (VP (VBP are)
      (ADJP (JJ subject))
      (PP (TO to)
        (NP (JJ major) (NNS costs)
          (CC and)
          (NNS risks)))
      (, ,)
      (PP (IN for)
        (NP
          (NP (NN instance))
          (SBAR
            (WHADVP (WRB when))
            (S
              (NP (PRP they))
              (VP
                (VP (VBP are)
                  (VP (VBN fired)
                    (PP (IN from)
                      (NP (DT a) (NN job)))))
                (CC and)
                (VP (VBP lose)
                  (NP (PRP$ their) (JJ employer-provide) (NN health) (NN insurance)))))))))
    (. .)))

