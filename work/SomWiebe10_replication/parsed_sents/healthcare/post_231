(ROOT
  (S
    (NP (NNP US) (NN health) (NN care) (NNS costs))
    (ADJP (JJR more)
      (SBAR (IN because)
        (S
          (NP (NNPS Americans))
          (VP (VBP pay)
            (S
              (VP (TO to)
                (VP (VB avoid)
                  (S
                    (VP (VBG waiting))))))))))))

nn(costs-4, US-1)
nn(costs-4, health-2)
nn(costs-4, care-3)
dep(more-5, costs-4)
root(ROOT-0, more-5)
mark(pay-8, because-6)
nsubj(pay-8, Americans-7)
xsubj(avoid-10, Americans-7)
ccomp(more-5, pay-8)
