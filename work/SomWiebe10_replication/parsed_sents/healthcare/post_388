(ROOT
  (S
    (VP (VB Thank)
      (NP (PRP you)))
    (. .)))

root(ROOT-0, Thank-1)
dobj(Thank-1, you-2)

(ROOT
  (S
    (NP (DT That) (JJ first) (NN paragraph))
    (VP (VBP couldnt)
      (VP (VB be)
        (ADJP (RBR more) (JJ true))))
    (. .)))

det(paragraph-3, That-1)
amod(paragraph-3, first-2)
nsubj(couldnt-4, paragraph-3)
root(ROOT-0, couldnt-4)
cop(true-7, be-5)
advmod(true-7, more-6)
dep(couldnt-4, true-7)

(ROOT
  (S
    (VP (VB Im)
      (PP (IN with)
        (NP
          (NP (PRP you))
          (ADJP (CD 100) (NN %)))))))

root(ROOT-0, Im-1)
prep_with(Im-1, you-3)
number(%-5, 100-4)
amod(you-3, %-5)

