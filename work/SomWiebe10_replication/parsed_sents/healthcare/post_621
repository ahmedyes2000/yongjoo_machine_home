(ROOT
  (FRAG
    (ADVP (RB Absolutely))
    (. .)))

root(ROOT-0, Absolutely-1)

(ROOT
  (S
    (NP (DT That))
    (VP (VBZ 's)
      (SBAR
        (S
          (NP (DT all) (FW i))
          (VP (VBP have)
            (S
              (VP (TO to)
                (VP (VB say))))))))
    (. .)))

nsubj('s-2, That-1)
root(ROOT-0, 's-2)
det(i-4, all-3)
nsubj(have-5, i-4)
xsubj(say-7, i-4)
ccomp('s-2, have-5)
aux(say-7, to-6)
xcomp(have-5, say-7)

