(ROOT
  (S
    (NP (EX There))
    (VP (VBP are)
      (NP (JJ positive) (NNS rights)))
    (. .)))

expl(are-2, There-1)
root(ROOT-0, are-2)
amod(rights-4, positive-3)
nsubj(are-2, rights-4)

(ROOT
  (S
    (NP
      (NP (DT A) (NN right))
      (PP (TO to)
        (NP (NN life))))
    (VP (VBZ is)
      (NP
        (NP (CD one))
        (PP (IN of)
          (NP (DT those) (NNS rights)))))
    (. .)))

det(right-2, A-1)
nsubj(one-6, right-2)
prep_to(right-2, life-4)
cop(one-6, is-5)
root(ROOT-0, one-6)
det(rights-9, those-8)
prep_of(one-6, rights-9)

