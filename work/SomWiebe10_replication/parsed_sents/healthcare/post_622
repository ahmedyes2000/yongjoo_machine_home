(ROOT
  (S
    (NP (NNP Health) (NN care))
    (VP (MD should)
      (VP (VB be)
        (ADJP (JJ free)
          (PP (TO to)
            (NP
              (NP (DT all))
              (PP (IN upon)
                (NP (NN entry))))))
        (PP (IN like)
          (NP (DT the) (NNP NHSin) (NNP UK)))))
    (. .)))

nn(care-2, Health-1)
