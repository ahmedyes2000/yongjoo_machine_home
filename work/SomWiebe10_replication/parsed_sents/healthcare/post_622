(ROOT
  (S
    (NP (DT The) (JJ only) (NN problem))
    (VP (VBZ is)
      (SBAR (IN that)
        (S
          (NP (DT the) (JJ private) (NN sector))
          (VP (VBP take)
            (S
              (NP (RB away) (DT the) (NNS resources)
                (CC and)
                (NN drug) (NNS companies))
              (VP (VB rip)
                (PRT (RP off))
                (NP (DT the) (NN government))))))))
    (. .)))

det(problem-3, The-1)
