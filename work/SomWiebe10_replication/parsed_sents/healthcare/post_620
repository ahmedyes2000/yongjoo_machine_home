(ROOT
  (FRAG
    (WHNP (WDT What))
    (PP (IN about)
      (NP (JJ handicapped) (NNS people)))
    (. ?)))

root(ROOT-0, What-1)
dep(What-1, about-2)
amod(people-4, handicapped-3)
pobj(about-2, people-4)

(ROOT
  (S
    (NP (PRP I))
    (VP (VBP think)
      (SBAR
        (S
          (NP (PRP they))
          (VP (MD should)
            (VP (VB get)
              (S
                (NP (PRP it))
                (ADJP (JJ free))))))))
    (. .)))

nsubj(think-2, I-1)
root(ROOT-0, think-2)
nsubj(get-5, they-3)
aux(get-5, should-4)
ccomp(think-2, get-5)
nsubj(free-7, it-6)
xcomp(get-5, free-7)

