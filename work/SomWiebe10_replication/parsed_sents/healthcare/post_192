(ROOT
  (S
    (NP (NNP Universal) (NN health) (NN care))
    (VP (VBZ makes)
      (S
        (NP (NNS people) (JJR healthier))
        (ADJP (RB so) (RBR more) (JJ productive))))
    (. .)))

nn(care-3, Universal-1)
nn(care-3, health-2)
nsubj(makes-4, care-3)
root(ROOT-0, makes-4)
dep(healthier-6, people-5)
nsubj(productive-9, healthier-6)
advmod(productive-9, so-7)
advmod(productive-9, more-8)
xcomp(makes-4, productive-9)

