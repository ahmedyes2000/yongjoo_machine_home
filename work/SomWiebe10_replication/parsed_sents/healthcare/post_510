(ROOT
  (S
    (NP (PRP You))
    (VP (VBP 're)
      (VP (VBG quoting)
        (NP
          (NP (DT The) (NNP N.Y.) (NNP Times))
          (PP (IN as)
            (NP (PRP$ your) (NN source))))))
    (. ?)))

