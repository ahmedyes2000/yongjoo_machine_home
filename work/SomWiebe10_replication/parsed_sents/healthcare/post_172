(ROOT
  (S
    (NP (DT This))
    (VP (VBZ cuts)
      (NP (NN health) (NN care) (NNS costs))
      (ADVP (RB substantially))
      (PP (IN down)
        (NP (DT the) (NN line))))
    (. .)))

nsubj(cuts-2, This-1)
root(ROOT-0, cuts-2)
nn(costs-5, health-3)
nn(costs-5, care-4)
dobj(cuts-2, costs-5)
advmod(cuts-2, substantially-6)
det(line-9, the-8)
prep_down(cuts-2, line-9)

