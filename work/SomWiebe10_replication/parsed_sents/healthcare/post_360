  (S (IN So)
    (NP (DT the) (NN trade) (NNS Unions))
    (VP (VBP have)
      (NP
        (NP (DT no) (NN say))
        (PP (IN in)
          (NP (NN government)))))
    (. .)))

dep(have-5, So-1)
det(Unions-4, the-2)
