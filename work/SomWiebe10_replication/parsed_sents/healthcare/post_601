(ROOT
  (SINV
    (PP (IN Because)
      (NP (NN everyone)))
    (VP (VBZ deserves))
    (NP (DT the) (NN right)
      (S
        (VP (TO to)
          (VP (VB receive)
            (NP (JJ good) (JJ medical) (NN attention))
            (PP (IN without)
              (S
                (VP (VBG having)
                  (S
                    (VP (TO to)
                      (VP (VB be)
                        (PP (IN in)
                          (NP
                            (NP (NN debt))
                            (PP (IN for)
                              (NP (NN life)))))))))))))))
    (. .)))

prep_because(deserves-3, everyone-2)
root(ROOT-0, deserves-3)
det(right-5, the-4)
nsubj(deserves-3, right-5)
aux(receive-7, to-6)
