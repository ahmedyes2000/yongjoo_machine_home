(ROOT
  (S
    (VP (VBG being)
      (NP (DT a) (JJ smart) (NN ass)))
    (. .)))

cop(ass-4, being-1)
det(ass-4, a-2)
amod(ass-4, smart-3)
root(ROOT-0, ass-4)

(ROOT
  (S
    (S
      (NP (PRP you))
      (VP (VBD told)
        (NP (DT this))
        (PP (TO to)
          (NP (JJ other) (NNS people)))
        (PP (IN without)
          (NP
            (NP (NN proof))
            (PP (IN of)
              (S
                (NP (PRP them))
                (VP (VBG doing)
                  (ADVP (RB so)))))))))
    (, ,) (IN so)
    (S
      (NP (FW i))
      (VP (VBD said)
        (NP (PRP it))
        (PP (TO to)
          (NP (PRP you)))))
    (. .)))

nsubj(told-2, you-1)
root(ROOT-0, told-2)
dobj(told-2, this-3)
amod(people-6, other-5)
prep_to(told-2, people-6)
