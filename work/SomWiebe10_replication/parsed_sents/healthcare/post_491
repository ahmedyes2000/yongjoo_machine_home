(ROOT
  (S
    (NP (DT The) (JJ operative) (NN word))
    (ADVP (RB here))
    (VP (VBZ is) (`` ``)
      (ADJP (JJ free)))
    (. .)))

det(word-3, The-1)
amod(word-3, operative-2)
nsubj(free-7, word-3)
advmod(free-7, here-4)
cop(free-7, is-5)
root(ROOT-0, free-7)

(ROOT
  (SBARQ
    (WHADVP (WRB ''))
    (S (CC And)
      (ADVP (RB thus))
      (NP (DT the) (NN answer))
      (VP (VBZ is)
        (ADJP (JJ no.))))))

cc(no.-7, And-2)
advmod(no.-7, thus-3)
det(answer-5, the-4)
nsubj(no.-7, answer-5)
cop(no.-7, is-6)
root(ROOT-0, no.-7)

(ROOT
  (S
    (NP (PRP You))
    (VP (MD should) (RB either)
      (VP (VB have)
        (S
          (VP (TO to)
            (VP
              (VP (VB pay)
                (NP (PRP it))
                (ADVP (RB back))
                (SBAR (IN once)
                  (S
                    (NP (PRP you))
                    (VP (VBP get)
                      (ADVP (RB back))
                      (PP (IN on)
                        (NP (PRP$ your) (NNS feet)))))))
              (CC or)
              (VP (VB do)
                (NP (NN something))
                (PP (IN for)
                  (NP (DT the) (NN government)))))))
        (SBAR (IN while)
          (S
            (NP (PRP you))
            (VP (VBP 're)
              (VP (VBG receiving)
                (NP (DT a) (NN check))
                (PP (IN from)
                  (NP (DT the) (NN government)))))))))
    (. .)))

nsubj(have-4, You-1)
xsubj(pay-6, You-1)
aux(have-4, should-2)
advmod(have-4, either-3)
root(ROOT-0, have-4)
aux(pay-6, to-5)
xcomp(have-4, pay-6)
dobj(pay-6, it-7)
advmod(pay-6, back-8)
mark(get-11, once-9)
