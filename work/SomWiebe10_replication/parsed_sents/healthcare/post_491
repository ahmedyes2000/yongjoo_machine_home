advmod(get-11, back-12)
poss(feet-15, your-14)
prep_on(get-11, feet-15)
xcomp(have-4, do-17)
conj_or(pay-6, do-17)
dobj(do-17, something-18)
det(government-21, the-20)
prep_for(do-17, government-21)
mark(receiving-25, while-22)
nsubj(receiving-25, you-23)
aux(receiving-25, 're-24)
