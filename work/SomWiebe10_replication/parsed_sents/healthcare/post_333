(ROOT
  (S
    (NP (DT That))
    (VP (VBD did) (RB n't)
      (VP (VB format)
        (ADVP (RB well))))
    (. .)))

nsubj(format-4, That-1)
aux(format-4, did-2)
neg(format-4, n't-3)
root(ROOT-0, format-4)
advmod(format-4, well-5)

(ROOT
  (NP
    (NP (NNP Sorry))
    (PP (IN about)
      (NP (DT that)))
    (. .)))

root(ROOT-0, Sorry-1)
prep_about(Sorry-1, that-3)

