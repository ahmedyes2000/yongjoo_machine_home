(ROOT
  (S
    (NP
      (NP (NNP Yes))
      (PP (IN after)
        (NP
          (NP (DT an) (JJ initial) (NN payment))
          (PP (IN of)
            (NP (NNP lets))))))
    (VP (VBP say)
      (NP
        (NP (DT a) (NN pound))
        (CC or)
        (NP
          (NP (CD two))
          (PP (IN per)
            (NP (NN visit))))))
    (. .)))

nsubj(say-8, Yes-1)
det(payment-5, an-3)
amod(payment-5, initial-4)
prep_after(Yes-1, payment-5)
prep_of(payment-5, lets-7)
root(ROOT-0, say-8)
det(pound-10, a-9)
dobj(say-8, pound-10)
dobj(say-8, two-12)
conj_or(pound-10, two-12)
prep_per(two-12, visit-14)

