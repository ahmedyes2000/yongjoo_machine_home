            (S
              (NP (DT a) (NN sperm) (NN +) (NN ovum))
              (VP (VBZ is)
                (NP (DT a) (JJ human) (NN being))))))))
    (, ,)
    (NP (PRP I))
    (VP (VBP doubt)
      (SBAR
        (S
          (NP (PRP you))
          (VP (VBP have)
            (NP (DT the) (`` ``) (NN capacity) ('' ''))))))
    (. .)))

