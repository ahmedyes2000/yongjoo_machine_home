(ROOT
  (S
    (NP (DT The)
      (ADJP (RB only) (JJ safe))
      (NN sex))
    (VP (VBZ is)
      (NP
        (NP (DT that))
        (PP (IN in)
          (NP (DT a) (JJ monogamous) (NN marriage)))))
    (. .)))

det(sex-4, The-1)
advmod(safe-3, only-2)
amod(sex-4, safe-3)
nsubj(that-6, sex-4)
cop(that-6, is-5)
root(ROOT-0, that-6)
det(marriage-10, a-8)
amod(marriage-10, monogamous-9)
prep_in(that-6, marriage-10)

