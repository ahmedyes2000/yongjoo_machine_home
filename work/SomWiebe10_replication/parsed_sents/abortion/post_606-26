(ROOT
  (S
    (NP (DT This))
    (VP (VBZ is)
      (NP (DT a)
        (ADJP (RB very) (JJ emotive))
        (NN issue))
      (SBAR (IN that)
        (S
          (NP (JJ pro-abortion) (NNS advocates))
          (VP (VBP love)
            (S
              (VP (TO to)
                (VP (VB expose))))))))
    (. .)))

nsubj(issue-6, This-1)
cop(issue-6, is-2)
det(issue-6, a-3)
advmod(emotive-5, very-4)
amod(issue-6, emotive-5)
root(ROOT-0, issue-6)
complm(love-10, that-7)
amod(advocates-9, pro-abortion-8)
nsubj(love-10, advocates-9)
xsubj(expose-12, advocates-9)
ccomp(issue-6, love-10)
aux(expose-12, to-11)
xcomp(love-10, expose-12)

