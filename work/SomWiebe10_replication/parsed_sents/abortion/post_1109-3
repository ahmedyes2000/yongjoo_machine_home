(ROOT
  (S
    (S
      (NP (PRP You))
      (VP (MD would)
        (ADVP (RB apparently))
        (VP (VB say)
          (INTJ (UH no)))))
    (CC and)
    (S
      (NP (DT that) (NN man))
      (VP (VBZ gets)
        (S
          (VP (TO to)
            (VP (VB define)
              (NP (DT that) (NN nature)))))))
    (. .)))

nsubj(say-4, You-1)
aux(say-4, would-2)
advmod(say-4, apparently-3)
root(ROOT-0, say-4)
dep(say-4, no-5)
det(man-8, that-7)
nsubj(gets-9, man-8)
xsubj(define-11, man-8)
conj_and(say-4, gets-9)
aux(define-11, to-10)
xcomp(gets-9, define-11)
det(nature-13, that-12)
dobj(define-11, nature-13)

