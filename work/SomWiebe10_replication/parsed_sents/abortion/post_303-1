(ROOT
  (S
    (ADVP (RB Again))
    (, ,)
    (NP (PRP we))
    (VP (VBP 're)
      (VP (VBG talking)
        (PP (IN about)
          (NP (NNP HUMAN) (NNP DNA)))))
    (. .)))

advmod(talking-5, Again-1)
nsubj(talking-5, we-3)
aux(talking-5, 're-4)
root(ROOT-0, talking-5)
nn(DNA-8, HUMAN-7)
prep_about(talking-5, DNA-8)

