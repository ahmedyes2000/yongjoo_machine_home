(ROOT
  (S
    (PP (IN For)
      (NP (CD 25) (NNS years)))
    (NP (DT these) (NNS deceivers))
    (VP (VBP have)
      (VP (VBN distorted)
        (CC and)
        (VBN distracted)
        (NP (NNPS Americans))
        (PP (IN from)
          (NP
            (NP (DT the) (JJ proper) (NN business))
            (PP (IN of)
              (NP
                (NP (NN government))
                (, ,)
                (SBAR
                  (WHNP (WDT which))
                  (S
                    (VP (VBZ is)
                      (NP (NN business)))))))))))
    (. .)))

num(years-3, 25-2)
prep_for(distorted-7, years-3)
det(deceivers-5, these-4)
nsubj(distorted-7, deceivers-5)
