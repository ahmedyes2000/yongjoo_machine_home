(ROOT
  (S
    (NP (NNS Humans))
    (VP (VBP have)
      (NP
        (NP (DT those) (JJ same) (NNS desires))
        (: --)
        (ADJP (JJ smart)
          (CC or)
          (RB not))))
    (. .)))

nsubj(have-2, Humans-1)
root(ROOT-0, have-2)
det(desires-5, those-3)
amod(desires-5, same-4)
dobj(have-2, desires-5)
amod(desires-5, smart-7)
amod(desires-5, not-9)
conj_or(smart-7, not-9)
neg(smart-7, not-9)

