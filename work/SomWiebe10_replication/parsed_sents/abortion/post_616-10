(ROOT
  (S
    (NP
      (NP (NNS Scientists))
      (PP (IN as)
        (NP (DT a) (NN group))))
    (VP (VBP do) (RB not)
      (VP (VB agree)
        (PP (IN on)
          (NP (DT this) (JJ very) (NN debate)))))
    (. .)))

nsubj(agree-7, Scientists-1)
det(group-4, a-3)
prep_as(Scientists-1, group-4)
aux(agree-7, do-5)
neg(agree-7, not-6)
root(ROOT-0, agree-7)
det(debate-11, this-9)
amod(debate-11, very-10)
prep_on(agree-7, debate-11)

