(ROOT
  (S
    (ADJP (VB No.))
    (. .)))

root(ROOT-0, No.-1)

(ROOT
  (S
    (ADVP (RB Again))
    (, ,)
    (NP (DT a) (NN potential))
    (VP (VBZ is) (RB not)
      (NP (DT an) (JJ actual)))
    (. .)))

advmod(actual-8, Again-1)
det(potential-4, a-3)
nsubj(actual-8, potential-4)
cop(actual-8, is-5)
neg(actual-8, not-6)
det(actual-8, an-7)
root(ROOT-0, actual-8)

