(ROOT
  (S
    (NP (PRP He))
    (VP (VBZ 's)
      (VP (VBG trying)
        (S
          (VP (TO to)
            (VP (VB have)
              (S
                (NP (PRP it))
                (NP (DT both) (NNS ways))))))))
    (. .)))

nsubj(trying-3, He-1)
xsubj(have-5, He-1)
aux(trying-3, 's-2)
root(ROOT-0, trying-3)
aux(have-5, to-4)
xcomp(trying-3, have-5)
nsubj(ways-8, it-6)
det(ways-8, both-7)
xcomp(have-5, ways-8)

