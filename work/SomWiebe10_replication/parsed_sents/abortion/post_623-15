(ROOT
  (S
    (NP (DT That))
    (VP (VBZ 's)
      (NP
        (NP (DT the) (NN kind))
        (PP (IN of)
          (NP (NN thing))))
      (SBAR (IN that)
        (S
          (NP (JJ misogynistic) (JJ religious) (NN dogma))
          (VP (VBZ does)
            (PP (TO to)
              (NP (DT a) (NN child) (NN rape) (NN victim)))))))
    (. .)))

nsubj(kind-4, That-1)
cop(kind-4, 's-2)
det(kind-4, the-3)
root(ROOT-0, kind-4)
prep_of(kind-4, thing-6)
complm(does-11, that-7)
amod(dogma-10, misogynistic-8)
amod(dogma-10, religious-9)
nsubj(does-11, dogma-10)
ccomp(kind-4, does-11)
det(victim-16, a-13)
nn(victim-16, child-14)
nn(victim-16, rape-15)
prep_to(does-11, victim-16)

