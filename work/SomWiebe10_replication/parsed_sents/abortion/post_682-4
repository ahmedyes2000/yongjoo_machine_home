(ROOT
  (S
    (NP
      (NP (PRP$ My) (JJ various) (NNS posts))
      (, ,)
      (SBAR
        (WHNP (WDT which))
        (S
          (NP (PRP you))
          (VP (VBP have)
            (VP (VBN responded)
              (S
                (VP (TO to)))))))
      (, ,))
    (VP (VBP have)
      (VP (VBN been)
        (ADVP (PRP themselves))
        (NP (NNS responses))))
    (. .)))

poss(posts-3, My-1)
amod(posts-3, various-2)
dobj(responded-8, posts-3)
nsubj(responses-14, posts-3)
rel(responded-8, which-5)
nsubj(responded-8, you-6)
xsubj(to-9, you-6)
aux(responded-8, have-7)
rcmod(posts-3, responded-8)
xcomp(responded-8, to-9)
aux(responses-14, have-11)
cop(responses-14, been-12)
advmod(responses-14, themselves-13)
root(ROOT-0, responses-14)

