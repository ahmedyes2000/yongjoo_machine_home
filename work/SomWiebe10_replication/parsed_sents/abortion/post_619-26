(ROOT
  (S ('' '')
    (ADVP (RB Frankly))
    (, ,)
    (NP (PRP I))
    (VP (VBP am)
      (VP (VBN confused)
        (PP (IN by)
          (NP (DT this)))))
    (. .)))

advmod(confused-6, Frankly-2)
nsubjpass(confused-6, I-4)
auxpass(confused-6, am-5)
root(ROOT-0, confused-6)
agent(confused-6, this-8)

