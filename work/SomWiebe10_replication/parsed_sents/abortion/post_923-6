(ROOT
  (S
    (VP (VB Consider)
      (NP (DT the) (NN fact))
      (SBAR (IN that)
        (S
          (SBAR (IN if)
            (S
              (NP (PRP you))
              (VP (VBP kill)
                (NP
                  (NP (DT a) (NN woman))
                  (PP (IN with)
                    (NP (NN child)))))))
          (: ;)
          (NP (PRP you))
          (VP (MD will)
            (VP (VB be)
              (VP (VBN charged)
                (PP (IN for)
