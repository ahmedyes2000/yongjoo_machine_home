(ROOT
  (S
    (NP (PRP I))
    (VP (VBP find)
      (S
        (NP (DT the) (JJ whole) (`` ``) (NN brain) (NN wave) (NN argument) ('' ''))
        (VP (TO to)
          (VP (VB be)
            (NP (DT another) (NN evasion))))))
    (. .)))

nsubj(find-2, I-1)
root(ROOT-0, find-2)
det(argument-8, the-3)
amod(argument-8, whole-4)
nn(argument-8, brain-6)
nn(argument-8, wave-7)
nsubj(evasion-13, argument-8)
aux(evasion-13, to-10)
cop(evasion-13, be-11)
det(evasion-13, another-12)
xcomp(find-2, evasion-13)

