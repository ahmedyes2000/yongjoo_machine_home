(ROOT
  (S
    (NP
      (QP (JJR More) (IN than) (CD 28,000))
      (JJ American) (NNS babies))
    (VP (VBP die)
      (PP (IN before)
        (NP (PRP$ their) (JJ first) (NN birthday))))
    (. .)))

mwe(than-2, More-1)
quantmod(28,000-3, than-2)
num(babies-5, 28,000-3)
amod(babies-5, American-4)
nsubj(die-6, babies-5)
root(ROOT-0, die-6)
poss(birthday-10, their-8)
amod(birthday-10, first-9)
prep_before(die-6, birthday-10)

