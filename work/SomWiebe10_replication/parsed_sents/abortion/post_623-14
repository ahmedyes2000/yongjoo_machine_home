(ROOT
  (S
    (S
      (NP (EX There))
      (VP (VBZ is)
        (NP
          (NP (DT no) (JJ other) (NN explanation))
          (PP (TO to)
            (NP (DT a) (NN child))))))
    (, ,)
    (CC and)
    (S
      (NP (JJ religious) (NNS teachings))
      (VP (VBP serve)
        (S
          (VP (TO to)
            (VP (VB reinforce)
              (NP (PRP it)))))))
    (. .)))

expl(is-2, There-1)
root(ROOT-0, is-2)
det(explanation-5, no-3)
amod(explanation-5, other-4)
nsubj(is-2, explanation-5)
det(child-8, a-7)
prep_to(explanation-5, child-8)
amod(teachings-12, religious-11)
nsubj(serve-13, teachings-12)
xsubj(reinforce-15, teachings-12)
conj_and(is-2, serve-13)
aux(reinforce-15, to-14)
xcomp(serve-13, reinforce-15)
dobj(reinforce-15, it-16)

