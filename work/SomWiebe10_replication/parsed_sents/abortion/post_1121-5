(ROOT
  (S (CC So)
    (SBAR
      (WHNP (WP what))
      (S
        (NP (PRP I))
        (VP (VBP hear)
          (S
            (NP (PRP you))
            (VP (VBG saying))))))
    (VP (VBZ is)
      (SBAR (IN that)
        (S
          (NP (PRP we))
          (VP (MD should)
            (ADVP (RB never))
            (VP (VB grow)
              (PRT (RP up)))))))
    (. .)))

cc(is-7, So-1)
dobj(hear-4, what-2)
nsubj(hear-4, I-3)
csubj(is-7, hear-4)
nsubj(saying-6, you-5)
dep(hear-4, saying-6)
root(ROOT-0, is-7)
complm(grow-12, that-8)
nsubj(grow-12, we-9)
aux(grow-12, should-10)
neg(grow-12, never-11)
ccomp(is-7, grow-12)
prt(grow-12, up-13)

