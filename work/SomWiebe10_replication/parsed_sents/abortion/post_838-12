(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ does) (RB not)
      (ADVP (RB even))
      (VP (VB have)
        (NP
          (NP (DT the) (NN capacity))
          (PP (IN for)
            (NP (NN suffering))))))
    (. .)))

nsubj(have-5, It-1)
aux(have-5, does-2)
neg(have-5, not-3)
advmod(have-5, even-4)
root(ROOT-0, have-5)
det(capacity-7, the-6)
dobj(have-5, capacity-7)
prep_for(capacity-7, suffering-9)

