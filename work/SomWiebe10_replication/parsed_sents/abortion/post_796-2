(ROOT
  (SINV (RB So)
    (VP (VBP are))
    (NP
      (NP (NNS executions))
      (, ,)
      (NP (NN euthanasia))
      (, ,)
      (CC and)
      (NP
        (NP (JJ stepping))
        (PP (IN on)
          (NP (NNS snails)))))
    (. .)))

advmod(are-2, So-1)
root(ROOT-0, are-2)
nsubj(are-2, executions-3)
nsubj(are-2, euthanasia-5)
conj_and(executions-3, euthanasia-5)
nsubj(are-2, stepping-8)
conj_and(executions-3, stepping-8)
prep_on(stepping-8, snails-10)

