  (S (IN So)
    (NP (DT an) (JJ individual) (NN organism))
    (VP (MD can)
      (ADVP (RB simply))
      (VP (VB become)
        (NP
          (NP (DT a) (NN part))
          (PP (IN of)
            (NP (DT another) (NN organism))))))
    (. .)))

