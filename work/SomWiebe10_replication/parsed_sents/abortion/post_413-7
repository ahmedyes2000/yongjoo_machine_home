(ROOT
  (S
    (NP (PRP I))
    (VP (VBP have)
      (NP
        (NP (DT every) (NN intention))
        (PP (IN of)
          (S
            (VP (VBG creating)
              (NP (DT a) (JJ true) (NN debate)))))))
    (. .)))

nsubj(have-2, I-1)
root(ROOT-0, have-2)
det(intention-4, every-3)
dobj(have-2, intention-4)
prepc_of(intention-4, creating-6)
det(debate-9, a-7)
amod(debate-9, true-8)
dobj(creating-6, debate-9)

