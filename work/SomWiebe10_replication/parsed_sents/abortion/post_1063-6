(ROOT
  (S
    (S
      (NP (DT A) (NN slave))
      (VP (VBZ is)
        (NP (NN property))))
    (CC and)
    (S
      (ADVP (RB therefore))
      (NP (PRP one))
      (VP (MD can)
        (VP (VB do)
          (PP (IN with)
            (NP (PRP it)))
          (SBAR (IN as)
            (S
              (NP (PRP one))
              (VP (VBZ sees)
                (ADJP (JJ fit))))))))
    (. .)))

det(slave-2, A-1)
nsubj(property-4, slave-2)
cop(property-4, is-3)
root(ROOT-0, property-4)
advmod(do-9, therefore-6)
nsubj(do-9, one-7)
aux(do-9, can-8)
