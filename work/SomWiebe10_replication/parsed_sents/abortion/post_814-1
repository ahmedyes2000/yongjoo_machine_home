(ROOT
  (S
    (S
      (VP (VBG Punishing)
        (NP
          (NP (DT the) (NN baby))
          (PP (IN for)
            (SBAR
              (WHNP (WP what))
              (S
                (NP (DT some) (NN wacko))
                (VP (VBD did))))))))
    (VP (VBZ is)
      (ADJP (JJ wrong)))
    (. .)))

csubj(wrong-10, Punishing-1)
det(baby-3, the-2)
dobj(Punishing-1, baby-3)
dobj(did-8, what-5)
det(wacko-7, some-6)
nsubj(did-8, wacko-7)
prepc_for(baby-3, did-8)
cop(wrong-10, is-9)
root(ROOT-0, wrong-10)

