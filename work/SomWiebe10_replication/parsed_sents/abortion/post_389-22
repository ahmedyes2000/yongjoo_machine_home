(ROOT
  (S
    (NP
      (NP (DT The) (JJ essential) (NN defining) (NN characteristic))
      (PP (IN of)
        (NP (DT a) (NN human) (VBG being))))
    (VP (VBZ is)
      (NP (JJ conceptual) (NN consciousness)))
    (. .)))

det(characteristic-4, The-1)
amod(characteristic-4, essential-2)
nn(characteristic-4, defining-3)
nsubj(consciousness-11, characteristic-4)
det(human-7, a-6)
prep_of(characteristic-4, human-7)
amod(human-7, being-8)
cop(consciousness-11, is-9)
amod(consciousness-11, conceptual-10)
root(ROOT-0, consciousness-11)

