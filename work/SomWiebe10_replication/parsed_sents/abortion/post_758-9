(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP it))
        (VP (VBD were)
          (NP (PRP me))
          (ADVP (RB personally)))))
    (NP (PRP I))
    (VP (MD would)
      (VP (VB put)
        (NP (DT the) (NN baby))
        (PRT (RP up))
        (PP (IN for)
          (NP (NN adoption)))))
    (. .)))

mark(me-4, If-1)
nsubj(me-4, it-2)
cop(me-4, were-3)
advcl(put-8, me-4)
advmod(me-4, personally-5)
nsubj(put-8, I-6)
aux(put-8, would-7)
root(ROOT-0, put-8)
det(baby-10, the-9)
dobj(put-8, baby-10)
prt(put-8, up-11)
prep_for(put-8, adoption-13)

