(ROOT
  (S
    (SBAR
      (WHADVP (WRB When))
      (S
        (NP (PRP I))
        (VP (VBD was)
          (NP (JJR younger)))))
    (NP (PRP I))
    (VP (VBD took)
      (PRT (RP apart))
      (NP (DT a) (NN calculator))
      (ADVP (RB once)))
    (. .)))

advmod(younger-4, When-1)
nsubj(younger-4, I-2)
cop(younger-4, was-3)
advcl(took-6, younger-4)
nsubj(took-6, I-5)
root(ROOT-0, took-6)
prt(took-6, apart-7)
det(calculator-9, a-8)
dobj(took-6, calculator-9)
advmod(took-6, once-10)

