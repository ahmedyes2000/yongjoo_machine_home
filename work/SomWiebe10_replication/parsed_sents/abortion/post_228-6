(ROOT
  (S
    (PP (IN in)
      (NP (JJ many) (NNS cases)))
    (NP (DT this))
    (VP (VBZ is)
      (VP (VBN taken)
        (PP (IN from)
          (NP (PRP them)))))
    (. .)))

amod(cases-3, many-2)
prep_in(taken-6, cases-3)
nsubjpass(taken-6, this-4)
auxpass(taken-6, is-5)
root(ROOT-0, taken-6)
prep_from(taken-6, them-8)

