(ROOT
  (S
    (NP (PRP She))
    (VP (VBD said)
      (SBAR
        (S
          (NP (PRP$ her) (NN husband))
          (VP (VBD had)
            (NP (DT a) (NN vasectomy))))))
    (. .)))

nsubj(said-2, She-1)
root(ROOT-0, said-2)
poss(husband-4, her-3)
nsubj(had-5, husband-4)
ccomp(said-2, had-5)
det(vasectomy-7, a-6)
dobj(had-5, vasectomy-7)

