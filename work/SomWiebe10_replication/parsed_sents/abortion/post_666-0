(ROOT
  (S
    (ADVP (RB Actually))
    (NP (PRP it))
    (VP (VBZ 's)
      (ADJP (JJ amazing))
      (SBAR
        (WHADVP (WRB how))
        (S
          (NP (JJ moral) (NNS codes))
          (ADVP (RB regardless)
            (PP (IN of)
              (NP (NN belief) (NNS systems))))
          (VP (VBP are)
            (ADJP (JJ homogeneous))))))
    (. .)))

advmod(amazing-4, Actually-1)
nsubj(amazing-4, it-2)
cop(amazing-4, 's-3)
root(ROOT-0, amazing-4)
advmod(homogeneous-13, how-5)
amod(codes-7, moral-6)
nsubj(homogeneous-13, codes-7)
nn(systems-11, belief-10)
prep_regardless_of(homogeneous-13, systems-11)
cop(homogeneous-13, are-12)
ccomp(amazing-4, homogeneous-13)

