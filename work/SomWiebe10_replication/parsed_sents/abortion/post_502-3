(ROOT
  (S
    (NP (PRP It))
    (VP (MD should)
      (VP (VB be)
        (VP
          (ADVP (RB up)
            (PP (TO to)
              (NP (DT the) (NN person))))
          (VBG facing)
          (NP (DT the) (NN choice)))))
    (. .)))

nsubj(facing-8, It-1)
aux(facing-8, should-2)
aux(facing-8, be-3)
advmod(facing-8, up-4)
det(person-7, the-6)
prep_to(up-4, person-7)
root(ROOT-0, facing-8)
det(choice-10, the-9)
dobj(facing-8, choice-10)

