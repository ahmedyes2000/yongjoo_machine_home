(ROOT
  (S
    (SBAR
      (WHADVP (WRB When))
      (S
        (NP (DT a) (NN woman))
        (VP (VBZ chooses)
          (SBAR (IN whether)
            (S
              (VP (TO to)
                (VP (VB have)
                  (NP (DT an) (NN abortion)))))))))
    (, ,)
    (NP (PRP they))
    (VP
      (VP (MD may)
        (VP (VB feel)
          (ADJP (JJ horrible))
          (PP (IN about)
            (NP (PRP it)))))
      (, ,)
      (CC but)
      (VP (VBP know)
        (SBAR (IN that)
          (S
            (NP (PRP it))
            (VP (VBZ is)
              (NP (DT the) (JJS best) (NN decision)))))))
    (. .)))

advmod(chooses-4, When-1)
det(woman-3, a-2)
nsubj(chooses-4, woman-3)
