  (S
    (S
      (NP (PRP You))
      (VP (VBP are)
        (ADVP (RB obviously))
        (ADJP (RB completely) (RB philosophically) (JJ uninitiated))))
    (, ,) (IN so)
    (S
      (NP (PRP I))
      (VP (MD could)
        (VP (VB have)
          (NP
            (NP (DT a) (NN lot))
            (PP (IN of)
              (NP (NN fun))))
          (ADVP (RB here)))))
    (. .)))

