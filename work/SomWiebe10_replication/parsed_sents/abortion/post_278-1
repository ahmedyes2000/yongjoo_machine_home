(ROOT
  (S
    (NP (PRP$ my) (NN mother))
    (VP (VBZ says)
      (NP (DT this))
      (PP (IN after)
        (NP (PRP$ her) (NN abortion))
        (ADVP
          (NP (CD 27) (NNS years))
          (RB ago))))))

poss(mother-2, my-1)
nsubj(says-3, mother-2)
root(ROOT-0, says-3)
dobj(says-3, this-4)
poss(abortion-7, her-6)
prep_after(says-3, abortion-7)
num(years-9, 27-8)
npadvmod(ago-10, years-9)
advmod(says-3, ago-10)

