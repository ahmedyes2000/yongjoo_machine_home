  (S
    (NP (NNP God))
    (VP (VBZ chooses)
      (S
        (NP (JJ many) (NNS couples))
        (VP (TO to)
          (VP (VB be)
            (ADJP (JJ available)
              (PP (IN for)
                (S
                  (VP (VBG adopting)
                    (NP (DT these) (NNS children))
                    (PP (IN by)
                      (NP
                        (NP (NN way))
                        (PP (IN of)
                          (NP (NN impotence)))))
                    (PP (IN by)
                      (NP
                        (NP (CD one))
                        (CC or)
                        (NP (DT both) (NNS people))))))))))))
    (. .)))

