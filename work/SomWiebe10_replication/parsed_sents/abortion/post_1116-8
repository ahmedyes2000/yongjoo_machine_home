(ROOT
  (S
    (NP
      (NP (DT The) (NN fact))
      (SBAR (IN that)
        (S
          (NP (EX there))
          (VP (VBZ is)
            (NP
              (NP (PDT such) (DT a) (NN thing))
              (PP (IN as)
                (NP
                  (NP (DT this) (NN debate))
                  (PP (IN about)
                    (NP (NN abortion))))))))))
    (VP (VBZ proves)
      (SBAR
        (S
          (NP (EX there))
          (VP (VBZ is)
            (NP (DT a) (NN problem))))))
    (. .)))

det(fact-2, The-1)
nsubj(proves-14, fact-2)
complm(is-5, that-3)
expl(is-5, there-4)
dep(fact-2, is-5)
predet(thing-8, such-6)
det(thing-8, a-7)
nsubj(is-5, thing-8)
det(debate-11, this-10)
prep_as(thing-8, debate-11)
prep_about(debate-11, abortion-13)
root(ROOT-0, proves-14)
expl(is-16, there-15)
ccomp(proves-14, is-16)
det(problem-18, a-17)
nsubj(is-16, problem-18)

