(ROOT
  (S
    (NP (PRP One))
    (VP (MD could)
      (VP (VB say)
        (NP (RB exactly) (DT the) (JJ same) (NN thing))
        (PP (IN about)
          (NP
            (NP (NNS assassins))
            (, ,)
            (NP (NNS vigilantes))
            (, ,)
            (NP (NNS robbers))
            (, ,)
            (CC and)
            (NP (JJ other) (NNS criminals))))))
    (. .)))

nsubj(say-3, One-1)
aux(say-3, could-2)
root(ROOT-0, say-3)
advmod(thing-7, exactly-4)
det(thing-7, the-5)
amod(thing-7, same-6)
dobj(say-3, thing-7)
prep_about(say-3, assassins-9)
prep_about(say-3, vigilantes-11)
conj_and(assassins-9, vigilantes-11)
prep_about(say-3, robbers-13)
conj_and(assassins-9, robbers-13)
amod(criminals-17, other-16)
prep_about(say-3, criminals-17)
conj_and(assassins-9, criminals-17)

(ROOT
  (S
    (PP (IN In)
      (NP (NN effect)))
    (S
      (NP (DT this) (NN argument))
      (VP (VBZ is)))
    (: :) (`` ``)
    (S
      (NP (NNS politicians))
      (VP (MD should) (RB n't)
        (VP (VB restrict)
          (NP (JJ private) (NNS murders))
          (SBAR (IN because)
            (S
              (NP (PRP they))
              (VP (VBP interfere)
                (PP (IN with)
                  (NP (DT the) (JJ murderer-victim) (NN relationship)))))))))
    (. .)))

prep_in(is-5, effect-2)
det(argument-4, this-3)
nsubj(is-5, argument-4)
root(ROOT-0, is-5)
nsubj(restrict-11, politicians-8)
