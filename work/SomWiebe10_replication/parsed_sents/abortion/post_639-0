(ROOT
  (S
    (S
      (VP (VBG Deciding)
        (SBAR
          (WHADVP (WRB when))
          (S
            (NP (NN someone))
            (VP (VBZ has)
              (NP (DT a) (NN soul)))))))
    (VP (VBZ is)
      (NP (DT a) (JJ religious) (NN question)))
    (. .)))

csubj(question-10, Deciding-1)
advmod(has-4, when-2)
nsubj(has-4, someone-3)
advcl(Deciding-1, has-4)
det(soul-6, a-5)
dobj(has-4, soul-6)
cop(question-10, is-7)
det(question-10, a-8)
amod(question-10, religious-9)
root(ROOT-0, question-10)

