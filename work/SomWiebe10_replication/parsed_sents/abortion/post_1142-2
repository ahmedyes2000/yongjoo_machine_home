(ROOT
  (S
    (NP (JJ 3rd) (NN trimester))
    (VP (MD should)
      (VP (VB be)
        (ADJP (JJ illegal)
          (SBAR
            (WHNP (WDT that))
            (S
              (VP (VBZ 's)
                (ADJP (RB just) (JJ wrong))))))))
    (. .)))

amod(trimester-2, 3rd-1)
nsubj(illegal-5, trimester-2)
aux(illegal-5, should-3)
cop(illegal-5, be-4)
root(ROOT-0, illegal-5)
nsubj(wrong-9, that-6)
cop(wrong-9, 's-7)
advmod(wrong-9, just-8)
dep(illegal-5, wrong-9)

