(ROOT
  (S (IN So)
    (NP (DT the) (NN doctor))
    (VP
      (VP (VBD took)
        (NP (DT the) (NN baby)))
      (CC and)
      (VP (VBD killed)
        (NP (PRP him))))
    (. .)))

dep(took-4, So-1)
det(doctor-3, the-2)
nsubj(took-4, doctor-3)
nsubj(killed-8, doctor-3)
root(ROOT-0, took-4)
det(baby-6, the-5)
dobj(took-4, baby-6)
conj_and(took-4, killed-8)
dobj(killed-8, him-9)

