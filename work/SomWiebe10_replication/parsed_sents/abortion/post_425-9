(ROOT
  (S
    (NP (DT This) (PRP we))
    (VP
      (VP (VBP know))
      (CC and)
      (VP (MD must)
        (VP (VB deal)
          (PP (IN with)))))
    (. .)))

det(we-2, This-1)
