(ROOT
  (S
    (NP (NNP Women))
    (VP (MD can)
      (VP (VB reproduce)
        (NP (JJ new) (NNS children))
        (PP (IN with)
          (NP (JJ equal) (NN value)))
        (PP (TO to)
          (NP (DT the) (JJ aborted)))))
    (. .)))

nsubj(reproduce-3, Women-1)
aux(reproduce-3, can-2)
root(ROOT-0, reproduce-3)
amod(children-5, new-4)
dobj(reproduce-3, children-5)
amod(value-8, equal-7)
prep_with(reproduce-3, value-8)
det(aborted-11, the-10)
prep_to(reproduce-3, aborted-11)

