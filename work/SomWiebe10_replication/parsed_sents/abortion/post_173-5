(ROOT
  (S ('' '')
    (S
      (NP (NNP John) (NNP Paul) (NNP II))
      (VP (VBD stated)
        (PP (IN as)
          (NP (PRP$ his) (JJ own) (JJ personal) (NN opinion)))))
    (, ,)
    (CC and)
    (ADVP (RB expressly))
    (S
      (NP
        (NP (RB not) (RB as) (NNP Church) (NN doctrine))
        (, ,)
        (SBAR (IN that)
          (S
            (NP (PRP he))
            (VP (VBD did) (RB not)
              (VP (VB believe)
                (NP
                  (NP (DT the) (NN invasion))
                  (PP (IN of)
                    (NP (NNP Iraq)))))))))
      (VP (VBD was)
        (ADJP (JJ justified))))
    (. .)))

nn(II-4, John-2)
nn(II-4, Paul-3)
nsubj(stated-5, II-4)
root(ROOT-0, stated-5)
poss(opinion-10, his-7)
amod(opinion-10, own-8)
amod(opinion-10, personal-9)
prep_as(stated-5, opinion-10)
advmod(stated-5, expressly-13)
conj_and(stated-5, expressly-13)
neg(doctrine-17, not-14)
advmod(doctrine-17, as-15)
nn(doctrine-17, Church-16)
nsubj(justified-29, doctrine-17)
dep(believe-23, that-19)
nsubj(believe-23, he-20)
aux(believe-23, did-21)
neg(believe-23, not-22)
dep(doctrine-17, believe-23)
det(invasion-25, the-24)
dobj(believe-23, invasion-25)
prep_of(invasion-25, Iraq-27)
cop(justified-29, was-28)
ccomp(stated-5, justified-29)
conj_and(stated-5, justified-29)

