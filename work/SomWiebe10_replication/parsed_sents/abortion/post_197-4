  (S
    (S
      (NP (NNP Pope) (NNP John) (NNP Paul) (NNP II))
      (VP (VBD wrote)
        (NP (DT the) (JJ encyclical) (NNP Evangelium) (NNP Vitae))
        (PP (IN for)
          (NP (DT a) (JJ similar) (NN purpose)))))
    (: --)
    (S
      (NP (DT the) (NNS bishops))
      (VP (VBD had)
        (VP (VBN asked)
          (S
            (NP (PRP him))
            (VP (TO to)
              (VP (VB clarify\/codify)
                (NP
                  (NP (JJ Catholic) (NN teaching))
                  (PP (VBG regarding)
                    (NP
                      (NP (NN life) (NNS issues))
                      (PP (JJ such) (IN as)
                        (NP (NN abortion)
                          (CC and)
                          (NN euthanasia))))))))))))
    (. .)))

