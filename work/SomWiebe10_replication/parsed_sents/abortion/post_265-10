(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is)
      (VP (VBG making)
        (S
          (NP (PRP her))
          (NP
            (NP (DT a) (JJ second) (NN class) (NN citizen))
            (CC and)
            (NP (DT a) (NN prisoner))))
        (PP (TO to)
          (NP (PRP$ her) (JJ own) (NN womb)))))
    (. .)))

nsubj(making-3, It-1)
aux(making-3, is-2)
root(ROOT-0, making-3)
nsubj(citizen-8, her-4)
det(citizen-8, a-5)
amod(citizen-8, second-6)
nn(citizen-8, class-7)
xcomp(making-3, citizen-8)
det(prisoner-11, a-10)
xcomp(making-3, prisoner-11)
conj_and(citizen-8, prisoner-11)
poss(womb-15, her-13)
amod(womb-15, own-14)
prep_to(making-3, womb-15)

