(ROOT
  (S ('' '')
    (ADVP (RB Abortion))
    (, ,)
    (ADVP (RB however))
    (, ,)
    (VP (VBZ is) (RB not)
      (NP
        (NP (CD one))
        (PP (IN of)
          (NP (DT these) (NNS things)))))
    (. .)))

advmod(one-8, Abortion-2)
advmod(one-8, however-4)
cop(one-8, is-6)
neg(one-8, not-7)
root(ROOT-0, one-8)
det(things-11, these-10)
prep_of(one-8, things-11)

