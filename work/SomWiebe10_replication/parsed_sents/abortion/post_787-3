(ROOT
  (S
    (NP (PRP You))
    (VP (VBP 're)
      (ADVP (RB just))
      (VP (VBG seeing)
        (S
          (VP (VB want)
            (SBAR
              (S
                (NP (PRP you))
                (VP (VBP want)
                  (S
                    (VP (TO to)
                      (VP (VB see)))))))))))
    (. .)))

nsubj(seeing-4, You-1)
aux(seeing-4, 're-2)
advmod(seeing-4, just-3)
root(ROOT-0, seeing-4)
ccomp(seeing-4, want-5)
nsubj(want-7, you-6)
xsubj(see-9, you-6)
ccomp(want-5, want-7)
aux(see-9, to-8)
xcomp(want-7, see-9)

