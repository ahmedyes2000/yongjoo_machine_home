(ROOT
  (S
    (ADVP (RB Well))
    (NP (DT the) (JJ same) (NN thing))
    (VP (VBZ is)
      (ADJP (JJ true)
        (NP (NN today))
        (PP (IN in)
          (NP
            (NP (DT the) (NNP United) (NNPS States))
            (PP (IN of)
              (NP (NNP America)))))))
    (. .)))

advmod(true-6, Well-1)
det(thing-4, the-2)
amod(thing-4, same-3)
nsubj(true-6, thing-4)
cop(true-6, is-5)
root(ROOT-0, true-6)
tmod(true-6, today-7)
det(States-11, the-9)
nn(States-11, United-10)
prep_in(true-6, States-11)
prep_of(States-11, America-13)

