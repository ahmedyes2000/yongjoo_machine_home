(ROOT
  (S
    (NP (PRP We))
    (VP (MD can) (RB all)
      (VP (VB agree)
        (SBAR (IN that)
          (S
            (NP (DT a) (VBN fertilized) (NN egg))
            (VP (VBZ is)
              (ADJP (JJ alive)))))))
    (. .)))

nsubj(agree-4, We-1)
aux(agree-4, can-2)
advmod(agree-4, all-3)
root(ROOT-0, agree-4)
complm(alive-10, that-5)
det(egg-8, a-6)
amod(egg-8, fertilized-7)
nsubj(alive-10, egg-8)
cop(alive-10, is-9)
ccomp(agree-4, alive-10)

