(ROOT
  (S
    (NP (DT This))
    (VP (VBZ is)
      (SBAR
        (WHADVP (WRB why))
        (S
          (NP (PRP$ your) (NN argument))
          (VP (VBZ is)
            (NP (NN nonsense))))))
    (. .)))

nsubj(is-2, This-1)
root(ROOT-0, is-2)
advmod(nonsense-7, why-3)
poss(argument-5, your-4)
nsubj(nonsense-7, argument-5)
cop(nonsense-7, is-6)
advcl(is-2, nonsense-7)

