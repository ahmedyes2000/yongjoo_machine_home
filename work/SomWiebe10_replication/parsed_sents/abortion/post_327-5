(ROOT
  (S
    (NP (PRP They))
    (VP (VBP 're)
      (VP (VBG doing)
        (SBAR
          (WHNP (WP what))
          (S
            (NP (PRP they))
            (VP (VBP want)
              (PP (TO to)
                (NP (PRP$ their) (NN hand))))))))
    (: ...)))

nsubj(doing-3, They-1)
aux(doing-3, 're-2)
root(ROOT-0, doing-3)
dobj(want-6, what-4)
nsubj(want-6, they-5)
ccomp(doing-3, want-6)
poss(hand-9, their-8)
prep_to(want-6, hand-9)

