(ROOT
  (S
    (NP (PRP You))
    (VP (MD should) (RB n't)
      (VP (VB judge)
        (NP
          (NP (NN someone))
          (, ,)
          (NP (NN period)))))
    (. .)))

nsubj(judge-4, You-1)
aux(judge-4, should-2)
neg(judge-4, n't-3)
root(ROOT-0, judge-4)
dobj(judge-4, someone-5)
appos(someone-5, period-7)

