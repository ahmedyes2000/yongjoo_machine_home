  (SBARQ
    (WHNP
      (WHNP (WP What))
      (PP (IN about)
        (NP
          (NP (NN someone))
          (PP (IN with)
            (NP (NNP Alzheimer))))))
    (SQ
      (VP (VBZ 's)))
    (. ?)))

