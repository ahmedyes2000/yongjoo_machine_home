(ROOT
  (S
    (VP (VB Take)
      (SBAR (IN that)
        (S
          (PP (IN into)
            (NP (JJ acount) (JJ next) (NN time)))
          (NP (PRP you))
          (VP (VBP think)
            (PP (IN about)
              (NP (DT that)))))))
    (. .)))

root(ROOT-0, Take-1)
complm(think-8, that-2)
amod(time-6, acount-4)
amod(time-6, next-5)
prep_into(think-8, time-6)
nsubj(think-8, you-7)
ccomp(Take-1, think-8)
prep_about(think-8, that-10)

