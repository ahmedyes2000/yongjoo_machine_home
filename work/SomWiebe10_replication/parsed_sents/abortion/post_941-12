(ROOT
  (S
    (NP (DT Both) (NNS groups))
    (VP (VBP have)
      (NP
        (NP (JJ absolute) (NN belief))
        (PP (IN in)
          (NP (PRP$ their) (NNS sources)))))
    (. .)))

det(groups-2, Both-1)
nsubj(have-3, groups-2)
root(ROOT-0, have-3)
amod(belief-5, absolute-4)
dobj(have-3, belief-5)
poss(sources-8, their-7)
prep_in(belief-5, sources-8)

