(ROOT
  (S
    (S
      (NP (DT This))
      (VP (VBZ is)
        (ADVP (RB also))
        (NP (RB not) (DT a) (NN reason))))
    (, ,)
    (NP (PRP it))
    (VP (VBZ is)
      (NP (JJ pure) (NN opinion)))
    (. .)))

nsubj(reason-6, This-1)
cop(reason-6, is-2)
advmod(reason-6, also-3)
neg(reason-6, not-4)
det(reason-6, a-5)
ccomp(opinion-11, reason-6)
nsubj(opinion-11, it-8)
cop(opinion-11, is-9)
amod(opinion-11, pure-10)
root(ROOT-0, opinion-11)

