(ROOT
  (S
    (NP (PRP We))
    (VP (MD ca) (RB n't)
      (VP (VB say)
        (SBAR
          (S
            (NP (PRP it))
            (VP (VBZ is)
              (NP (DT the) (JJ sexual) (NN act))
              (SBAR (IN because)
                (S
                  (NP (DT that))
                  (ADVP (RB alone))
                  (VP (VBZ does) (RB n't)
                    (ADVP (RB always))
                    (VP (VB cause)
                      (NP (NNS problems)))))))))))
    (. .)))

nsubj(say-4, We-1)
aux(say-4, ca-2)
neg(say-4, n't-3)
root(ROOT-0, say-4)
nsubj(act-9, it-5)
cop(act-9, is-6)
det(act-9, the-7)
amod(act-9, sexual-8)
ccomp(say-4, act-9)
mark(cause-16, because-10)
nsubj(cause-16, that-11)
advmod(cause-16, alone-12)
aux(cause-16, does-13)
neg(cause-16, n't-14)
advmod(cause-16, always-15)
advcl(act-9, cause-16)
dobj(cause-16, problems-17)

