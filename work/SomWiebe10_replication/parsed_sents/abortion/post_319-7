(ROOT
  (S ('' '')
    (NP (NNP The))
    (VP (VBP claim)
      (SBAR (IN that)
        (S
          (NP (NNS inactions))
          (VP (VBP are) (RB not)
            (ADJP (RB as) (JJ harmful))
            (SBAR (IN as)
              (S
                (NP (NNS actions))
                (VP (VBZ is)
                  (NP (DT a) (NN fallacy)))))))))
    (. .)))

nsubj(claim-3, The-2)
root(ROOT-0, claim-3)
complm(harmful-9, that-4)
nsubj(harmful-9, inactions-5)
cop(harmful-9, are-6)
neg(harmful-9, not-7)
advmod(harmful-9, as-8)
ccomp(claim-3, harmful-9)
mark(fallacy-14, as-10)
nsubj(fallacy-14, actions-11)
cop(fallacy-14, is-12)
det(fallacy-14, a-13)
advcl(harmful-9, fallacy-14)

