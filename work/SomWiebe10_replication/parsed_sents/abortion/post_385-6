(ROOT
  (S
    (NP (PRP She))
    (VP (MD should)
      (VP (VB be)
        (ADJP (JJ free)
          (S
            (VP (TO to)
              (VP (VB live)
                (NP (PRP it))
                (SBAR (IN as)
                  (S
                    (NP (PRP she))
                    (VP (VBZ chooses))))))))))
    (. .)))

nsubj(free-4, She-1)
aux(free-4, should-2)
cop(free-4, be-3)
root(ROOT-0, free-4)
aux(live-6, to-5)
xcomp(free-4, live-6)
dobj(live-6, it-7)
mark(chooses-10, as-8)
nsubj(chooses-10, she-9)
advcl(live-6, chooses-10)

(ROOT
  (S
    (NP (NNS Abortions))
    (VP (MD should)
      (VP (VB be)
        (ADJP (JJ legal))))
    (. .)))

