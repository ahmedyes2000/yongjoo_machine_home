(ROOT
  (S
    (NP (PRP It) (POS 's))
    (VP (MD should)
      (VP (VB be)
        (NP (DT the) (JJ single) (NN person) (NN choice) (NN !!!))))))

nsubj(!!!-9, It-1)
possessive(It-1, 's-2)
aux(!!!-9, should-3)
cop(!!!-9, be-4)
det(!!!-9, the-5)
amod(!!!-9, single-6)
nn(!!!-9, person-7)
nn(!!!-9, choice-8)
root(ROOT-0, !!!-9)

