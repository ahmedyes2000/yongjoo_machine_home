(ROOT
  (S
    (NP
      (NP (NNS Others))
      (SBAR (IN because)
        (S
          (NP (PRP they))
          (VP (VBP live)
            (PP (IN in)
              (NP (JJ abusive) (NNS homes)))
            (SBAR (IN that)
              (S
                (NP (PRP they))
                (VP (VBP do) (RB n't))))))))
    (VP (VBP want)
      (S
        (VP (TO to)
          (VP (VB subject)
            (NP (DT a) (NN child))
            (PP (TO to))))))
    (. .)))

nsubj(want-12, Others-1)
xsubj(subject-14, Others-1)
mark(live-4, because-2)
nsubj(live-4, they-3)
dep(Others-1, live-4)
amod(homes-7, abusive-6)
prep_in(live-4, homes-7)
complm(do-10, that-8)
nsubj(do-10, they-9)
ccomp(live-4, do-10)
neg(do-10, n't-11)
root(ROOT-0, want-12)
aux(subject-14, to-13)
xcomp(want-12, subject-14)
det(child-16, a-15)
dobj(subject-14, child-16)
prep(subject-14, to-17)

