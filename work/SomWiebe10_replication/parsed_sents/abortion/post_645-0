(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is)
      (PP (IN for)
        (NP
          (NP (JJ legal) (NNS purposes))
          (, ,) (RB not)
          (NP (JJ moral) (NNS purposes)))))
    (. .)))

nsubj(is-2, It-1)
root(ROOT-0, is-2)
amod(purposes-5, legal-4)
prep_for(is-2, purposes-5)
neg(purposes-5, not-7)
amod(purposes-9, moral-8)
dep(purposes-5, purposes-9)

