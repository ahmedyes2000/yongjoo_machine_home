(ROOT
  (S
    (S
      (NP (PRP They))
      (VP (VBP 're) (RB not)
        (VP (VBG reproducing))))
    (CC and)
    (S
      (SBAR
        (WHADVP (WRB when))
        (S
          (NP (PRP they))
          (VP (VBP do))))
      (, ,)
      (NP (PRP they))
      (VP (VBP kill)
        (NP (PRP it))))
    (. .)))

nsubj(reproducing-4, They-1)
aux(reproducing-4, 're-2)
neg(reproducing-4, not-3)
root(ROOT-0, reproducing-4)
advmod(do-8, when-6)
nsubj(do-8, they-7)
advcl(kill-11, do-8)
nsubj(kill-11, they-10)
conj_and(reproducing-4, kill-11)
dobj(kill-11, it-12)

