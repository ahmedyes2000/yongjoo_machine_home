(ROOT
  (S ('' '')
    (NP (NNP Go))
    (VP (VBP get)
      (NP (PRP$ your) (NN Bible))
      ('' '')
      (SBAR (IN if)
        (S
          (NP (PRP you))
          (VP (VBP think)
            (SBAR
              (S
                (NP (PRP I))
                (VP (VBP am)
                  (ADJP (JJ wrong)))))))))
    (. .)))

nsubj(get-3, Go-2)
root(ROOT-0, get-3)
poss(Bible-5, your-4)
dobj(get-3, Bible-5)
mark(think-9, if-7)
nsubj(think-9, you-8)
advcl(get-3, think-9)
nsubj(wrong-12, I-10)
cop(wrong-12, am-11)
ccomp(think-9, wrong-12)

