(ROOT
  (S
    (ADVP (RB Therefore))
    (, ,)
    (NP (EX there))
    (VP (VBZ is)
      (NP
        (NP (DT a) (JJ fundamental) (NN inconsistency))
        (PP (IN in)
          (NP (DT this) (NN position)))))
    (. .)))

advmod(is-4, Therefore-1)
expl(is-4, there-3)
root(ROOT-0, is-4)
det(inconsistency-7, a-5)
amod(inconsistency-7, fundamental-6)
nsubj(is-4, inconsistency-7)
det(position-10, this-9)
prep_in(inconsistency-7, position-10)

