(ROOT
  (S
    (NP
      (NP (JJ Many) (NNS opponents))
      (PP (IN of)
        (NP (NN abortion))))
    (VP (VBP allow)
      (PP (IN for)
        (NP (NN abortion)))
      (PP (IN in)
        (NP
          (NP (NNS instances))
          (PP (IN of)
            (NP (NN rape))))))
    (. .)))

amod(opponents-2, Many-1)
nsubj(allow-5, opponents-2)
prep_of(opponents-2, abortion-4)
root(ROOT-0, allow-5)
prep_for(allow-5, abortion-7)
prep_in(allow-5, instances-9)
prep_of(instances-9, rape-11)

