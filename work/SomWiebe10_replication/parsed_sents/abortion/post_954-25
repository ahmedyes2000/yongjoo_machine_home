(ROOT
  (S
    (NP (NNP Abortion))
    (VP (VBZ takes)
      (NP
        (NP (DT the) (NN life))
        (PP (IN of)
          (NP (DT an) (JJ innocent) (JJ human) (NN being))))
      (PP (IN without)
        (NP (JJ proper) (NN justification))))
    (. .)))

nsubj(takes-2, Abortion-1)
root(ROOT-0, takes-2)
det(life-4, the-3)
dobj(takes-2, life-4)
det(being-9, an-6)
amod(being-9, innocent-7)
amod(being-9, human-8)
prep_of(life-4, being-9)
amod(justification-12, proper-11)
