(ROOT
  (S (CC Either)
    (S
      (NP (PRP she))
      (VP (VBZ has)
        (NP (DT the) (NN right)
          (S
            (VP (TO to)
              (VP (VB govern)
                (NP (PRP$ her) (JJ own) (NN body))))))))
    (CC or)
    (S
      (NP (PRP she))
      (VP (VBZ does) (RB not)))
    (. .)))

preconj(has-3, Either-1)
nsubj(has-3, she-2)
root(ROOT-0, has-3)
det(right-5, the-4)
dobj(has-3, right-5)
aux(govern-7, to-6)
infmod(right-5, govern-7)
poss(body-10, her-8)
amod(body-10, own-9)
dobj(govern-7, body-10)
nsubj(does-13, she-12)
conj_or(has-3, does-13)
