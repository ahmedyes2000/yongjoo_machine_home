(ROOT
  (S
    (NP (NN Everyone))
    (VP (VBZ has)
      (NP (DT the) (NN right)
        (S
          (VP (TO to)
            (VP (VB avoid)
              (NP (JJ unwanted) (NN pregnancy)))))))
    (. .)))

nsubj(has-2, Everyone-1)
root(ROOT-0, has-2)
det(right-4, the-3)
dobj(has-2, right-4)
aux(avoid-6, to-5)
infmod(right-4, avoid-6)
amod(pregnancy-8, unwanted-7)
dobj(avoid-6, pregnancy-8)

