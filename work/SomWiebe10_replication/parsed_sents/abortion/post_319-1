(ROOT
  (S
    (NP (PRP He))
    (VP (VBZ tells)
      (NP
        (NP (DT the) (NN story))
        (PP (IN of)
          (SBAR
            (WHADVP (WRB how))
            (S
              (NP (DT this) (NN girl))
              (VP (VBD was)
                (VP (VBN stabbed)
                  (PP (IN in)
                    (NP (JJ broad) (NN daylight))))))))))
    (: ...)))

nsubj(tells-2, He-1)
root(ROOT-0, tells-2)
det(story-4, the-3)
dobj(tells-2, story-4)
advmod(stabbed-10, how-6)
det(girl-8, this-7)
nsubjpass(stabbed-10, girl-8)
auxpass(stabbed-10, was-9)
prepc_of(story-4, stabbed-10)
amod(daylight-13, broad-12)
prep_in(stabbed-10, daylight-13)

