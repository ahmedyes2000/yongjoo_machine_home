(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (NN nothing))
        (VP (VBZ happens)
          (PP (TO to)
            (NP
              (NP (PRP him))
              (CC or)
              (NP (PRP her))))
          (, ,)
          (PP (IN in)
            (NP (CD nine) (NNS months))))))
    (NP (PRP they))
    (VP (MD will)
      (VP (VB be)
        (SBAR
          (WHNP (WP what))
          (S
            (NP (NN anyone))
            (VP (MD would)
              (VP (VB consider)
                (NP (DT a) (NN person))))))))
    (. .)))

