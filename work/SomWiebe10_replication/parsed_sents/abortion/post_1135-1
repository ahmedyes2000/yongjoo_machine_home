(ROOT
  (SQ (MD Would)
    (NP (PRP you))
    (VP (VB treat)
      (S
        (NP (DT this) (NN child))
        (ADJP (JJ different))
        (SBAR (IN after)
          (S
            (NP (PRP it))
            (VP (VBD was)
              (VP (VBN born)))))))
    (. ?)))

aux(treat-3, Would-1)
nsubj(treat-3, you-2)
root(ROOT-0, treat-3)
det(child-5, this-4)
nsubjpass(born-10, child-5)
dep(born-10, different-6)
mark(born-10, after-7)
nsubjpass(born-10, it-8)
auxpass(born-10, was-9)
xcomp(treat-3, born-10)

