(ROOT
  (S
    (ADVP (RB Well))
    (NP (PRP we))
    (VP (VBP 've)
      (VP (VBN covered)
        (NP
          (NP (DT a) (NN lot))
          (PP (IN of)
            (NP (NN ground))))))
    (. .)))

advmod(covered-4, Well-1)
nsubj(covered-4, we-2)
aux(covered-4, 've-3)
root(ROOT-0, covered-4)
det(lot-6, a-5)
dobj(covered-4, lot-6)
prep_of(lot-6, ground-8)

