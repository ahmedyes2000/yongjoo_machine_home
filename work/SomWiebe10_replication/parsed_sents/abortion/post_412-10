(ROOT
  (S
    (NP (EX There))
    (VP (MD can)
      (VP (VB be)
        (NP
          (NP (DT no) (NN evidence))
          (PP (IN for)
            (NP
              (NP (DT that))
              (SBAR
                (WHNP (WDT which))
                (S
                  (VP (VBZ does) (RB not)
                    (VP (VB exist))))))))))
    (. .)))

expl(evidence-5, There-1)
aux(evidence-5, can-2)
cop(evidence-5, be-3)
det(evidence-5, no-4)
root(ROOT-0, evidence-5)
prep_for(evidence-5, that-7)
nsubj(exist-11, that-7)
aux(exist-11, does-9)
neg(exist-11, not-10)
rcmod(that-7, exist-11)

