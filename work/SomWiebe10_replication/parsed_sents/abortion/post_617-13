(ROOT
  (S
    (PP (IN As)
      (NP
        (NP (IN in))
        (, ,) ('' '')
        (PP (IN at)
          (NP (DT this) (NN point)))))
    (NP (DT a) (JJ new) (NN life))
    (VP (VBZ is)
      (VP (VBN created)))
    (. .)))

prep(created-12, As-1)
pobj(As-1, in-2)
det(point-7, this-6)
prep_at(in-2, point-7)
det(life-10, a-8)
amod(life-10, new-9)
nsubjpass(created-12, life-10)
auxpass(created-12, is-11)
root(ROOT-0, created-12)

