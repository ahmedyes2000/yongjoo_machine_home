(ROOT
  (S
    (S
      (NP (DT That))
      (VP (VBZ is)
        (NP
          (NP (NN fine))
          (CC and)
          (NP (PRP$ your) (NN right)))))
    (, ,)
    (CC but)
    (S
      (NP (PRP we))
      (VP (VBP are)
        (VP (VBG straying)
          (ADJP (RB far) (JJ afield)
            (PP (IN from)
              (NP
                (NP (DT the) (NN purpose))
                (PP (IN of)
                  (NP (DT this) (NN discussion)))))))))
    (. .)))

nsubj(fine-3, That-1)
cop(fine-3, is-2)
root(ROOT-0, fine-3)
poss(right-6, your-5)
conj_and(fine-3, right-6)
