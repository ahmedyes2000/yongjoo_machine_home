(ROOT
  (S
    (NP
      (NP (NNP Sperm))
      (CC and)
      (NP (NNS eggs)))
    (VP (VBP are)
      (ADJP (JJ alive)))
    (. .)))

nsubj(alive-5, Sperm-1)
conj_and(Sperm-1, eggs-3)
nsubj(alive-5, eggs-3)
cop(alive-5, are-4)
root(ROOT-0, alive-5)

(ROOT
  (S
    (S
      (NP (PRP They))
      (VP (VBP do) (RB n't)
        (VP (VB have)
          (NP (DT an) (JJ independent) (NN life)))))
    (, ,)
    (CC but)
    (S
      (NP (DT neither))
      (VP (VBZ does)
        (NP
          (NP (DT any) (NN cell))
          (PP (IN of)
            (NP (PRP$ our) (NN body)))
          (, ,)
          (PP (VBG including)
            (NP (DT a) (VBN fertilized) (NN egg))))))
    (. .)))

nsubj(have-4, They-1)
aux(have-4, do-2)
neg(have-4, n't-3)
root(ROOT-0, have-4)
det(life-7, an-5)
amod(life-7, independent-6)
dobj(have-4, life-7)
nsubj(does-11, neither-10)
conj_but(have-4, does-11)
det(cell-13, any-12)
dobj(does-11, cell-13)
poss(body-16, our-15)
prep_of(cell-13, body-16)
det(egg-21, a-19)
amod(egg-21, fertilized-20)
prep_including(cell-13, egg-21)

(ROOT
  (S
    (NP (PRP You))
    (VP (VBP are)
      (ADJP (RB just) (JJ wrong)
        (PP (IN about)
          (NP (DT the) (JJ biological) (NN POV)))))
    (. .)))

nsubj(wrong-4, You-1)
cop(wrong-4, are-2)
advmod(wrong-4, just-3)
root(ROOT-0, wrong-4)
det(POV-8, the-6)
amod(POV-8, biological-7)
prep_about(wrong-4, POV-8)

