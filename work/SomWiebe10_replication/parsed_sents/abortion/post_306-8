(ROOT
  (S
    (S
      (LST (JJ First))
      (NP (PRP it))
      (VP (VBD was)
        (NP (NN body) (NN parts\/general))
        (ADVP (RB morphology))))
    (, ,) (RB then)
    (S
      (NP
        (NP (JJ random) (JJ physiological) (NNS characteristics))
        (PRN (, ,)
          (ADVP (RB then))
          (NP (NNP DNA))
