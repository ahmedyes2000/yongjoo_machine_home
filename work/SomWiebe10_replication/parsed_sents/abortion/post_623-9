(ROOT
  (S
    (NP (DT No) (VBG loving) (NN God))
    (VP (MD would)
      (VP (VB ask)
        (NP (PDT such) (DT a) (NN thing))))
    (. .)))

det(God-3, No-1)
amod(God-3, loving-2)
nsubj(ask-5, God-3)
aux(ask-5, would-4)
root(ROOT-0, ask-5)
predet(thing-8, such-6)
det(thing-8, a-7)
dobj(ask-5, thing-8)

