(ROOT
  (S
    (NP (PRP It))
    (ADVP (RB deliberately))
    (VP
      (VP (VBD recognized))
      (CC and)
      (VP (VBD secured)
        (NP
          (NP (JJ individual) (NNS rights))
          (, ,)
          (NP (DT a) (JJ unique) (JJ historical) (NN achievement)))))
    (. .)))

nsubj(recognized-3, It-1)
nsubj(secured-5, It-1)
advmod(recognized-3, deliberately-2)
root(ROOT-0, recognized-3)
conj_and(recognized-3, secured-5)
amod(rights-7, individual-6)
dobj(secured-5, rights-7)
det(achievement-12, a-9)
amod(achievement-12, unique-10)
amod(achievement-12, historical-11)
appos(rights-7, achievement-12)

