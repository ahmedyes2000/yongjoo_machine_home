(ROOT
  (S
    (NP
      (NP (JJ Formal) (NN cooperation))
      (PP (IN with)
        (NP (DT an) (JJ intrinsic) (NN evil))))
    (VP (VBZ is)
      (ADVP (RB always))
      (ADJP (RB morally) (JJ illicit)))
    (. .)))

amod(cooperation-2, Formal-1)
nsubj(illicit-10, cooperation-2)
det(evil-6, an-4)
amod(evil-6, intrinsic-5)
prep_with(cooperation-2, evil-6)
cop(illicit-10, is-7)
advmod(illicit-10, always-8)
advmod(illicit-10, morally-9)
root(ROOT-0, illicit-10)

