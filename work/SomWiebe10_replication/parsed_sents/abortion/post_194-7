(ROOT
  (S (CC And)
    (ADVP (RB certainly))
    (, ,)
    (VP (VBP do) (RB n't)
      (VP (VB let)
        (S
          (NP (DT the) (JJ current) (NN society) (NNS trends))
          (VP (VB be)
            (NP (PRP$ our) (NN guide))))))
    (. .)))

cc(let-6, And-1)
advmod(let-6, certainly-2)
aux(let-6, do-4)
neg(let-6, n't-5)
root(ROOT-0, let-6)
det(trends-10, the-7)
amod(trends-10, current-8)
nn(trends-10, society-9)
nsubj(guide-13, trends-10)
cop(guide-13, be-11)
poss(guide-13, our-12)
ccomp(let-6, guide-13)

