(ROOT
  (S
    (ADVP (RB Frankly))
    (, ,)
    (NP (DT those) (NNS citizens))
    (VP (VBP do) (RB not)
      (VP (VB have)
        (NP
          (NP (DT a) (NN right))
          (PP (TO to)
            (NP (DT any) (NN funding))))
        (ADVP (RB whatsoever))))
    (. .)))

advmod(have-7, Frankly-1)
det(citizens-4, those-3)
nsubj(have-7, citizens-4)
aux(have-7, do-5)
neg(have-7, not-6)
root(ROOT-0, have-7)
det(right-9, a-8)
