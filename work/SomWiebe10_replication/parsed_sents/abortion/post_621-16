(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is)
      (ADJP (RB much) (JJR easier))
      (SBAR
        (WHADVP (WRB when))
        (S
          (NP (EX there))
          (VP (VBP are) (RB not)))))
    (. .)))

nsubj(easier-4, It-1)
cop(easier-4, is-2)
advmod(easier-4, much-3)
root(ROOT-0, easier-4)
advmod(are-7, when-5)
expl(are-7, there-6)
advcl(easier-4, are-7)
neg(are-7, not-8)

