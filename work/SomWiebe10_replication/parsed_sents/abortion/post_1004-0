(ROOT
  (S
    (NP (PRP You))
    (VP (VBP 're)
      (ADJP (RB so) (JJ closed)
        (SBAR
          (S
            (VP (VBD minded)
              (SBAR
                (S
                  (NP (PRP it))
                  (VP (VBZ 's)
                    (ADJP (JJ ridiculous))))))))))
    (. .)))

nsubj(closed-4, You-1)
cop(closed-4, 're-2)
advmod(closed-4, so-3)
root(ROOT-0, closed-4)
dep(closed-4, minded-5)
nsubj(ridiculous-8, it-6)
cop(ridiculous-8, 's-7)
ccomp(minded-5, ridiculous-8)

