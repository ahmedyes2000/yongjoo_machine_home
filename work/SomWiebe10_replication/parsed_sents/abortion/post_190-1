(ROOT
  (S
    (NP (DT This) (NN declaration))
    (ADVP (RB expressly))
    (VP (VBZ leaves)
      (PRT (RP aside))
      (NP
        (NP (DT the) (NN question))
        (PP (IN of)
          (NP
            (NP (DT the) (NN moment))
            (SBAR
              (WHADVP (WRB when))
              (S
                (NP (DT the) (JJ spiritual) (NN soul))
                (VP (VBZ is)
                  (VP (VBN infused)))))))))
    (. .)))

det(declaration-2, This-1)
nsubj(leaves-4, declaration-2)
