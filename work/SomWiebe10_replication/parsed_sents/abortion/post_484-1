(ROOT
  (S
    (NP (DT This))
    (VP (VBZ is)
      (SBAR
        (WHADVP (WRB why))
        (S
          (NP (PRP I))
          (ADVP (RB always))
          (VP (VB defend)
            (NP (NN choice))))))
    (. .)))

nsubj(is-2, This-1)
root(ROOT-0, is-2)
advmod(defend-6, why-3)
nsubj(defend-6, I-4)
advmod(defend-6, always-5)
advcl(is-2, defend-6)
dobj(defend-6, choice-7)

