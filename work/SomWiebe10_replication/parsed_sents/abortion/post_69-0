(ROOT
  (S
    (NP (NNP Adoption))
    (VP (VBZ does) (RB not)
      (VP (VB spare)
        (S
          (NP (DT a) (NNS women))
          (NP
            (NP (DT the) (NNS pains\/risks))
            (PP (IN of)
              (NP (NN childbirth)))))))
    (. .)))

nsubj(spare-4, Adoption-1)
aux(spare-4, does-2)
neg(spare-4, not-3)
root(ROOT-0, spare-4)
det(women-6, a-5)
nsubj(pains\/risks-8, women-6)
det(pains\/risks-8, the-7)
xcomp(spare-4, pains\/risks-8)
prep_of(pains\/risks-8, childbirth-10)

