(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is)
      (NP (PRP$ my) (NN opinion))
      (SBAR (IN that)
        (S
          (NP (PRP it))
          (VP (VBZ is) (RB not)
            (ADJP (JJ acceptable)
              (S
                (VP (TO to)
                  (VP (VB destroy)
                    (NP (DT the) (NN unborn))
                    (PP (IN during)
                      (NP (DT those) (CD nine) (NNS months)))))))
            (SBAR
              (ADVP (RB simply))
              (IN because)
              (S
                (NP (PRP they))
                (VP (VBP have) (RB not)
                  (VP (VBN been)
                    (VP (VBN born)
                      (ADVP (RB yet)))))))))))
    (. .)))

nsubj(opinion-4, It-1)
cop(opinion-4, is-2)
poss(opinion-4, my-3)
root(ROOT-0, opinion-4)
complm(acceptable-9, that-5)
nsubj(acceptable-9, it-6)
cop(acceptable-9, is-7)
neg(acceptable-9, not-8)
ccomp(opinion-4, acceptable-9)
aux(destroy-11, to-10)
xcomp(acceptable-9, destroy-11)
det(unborn-13, the-12)
dobj(destroy-11, unborn-13)
det(months-17, those-15)
num(months-17, nine-16)
prep_during(destroy-11, months-17)
advmod(born-24, simply-18)
dep(born-24, because-19)
nsubjpass(born-24, they-20)
aux(born-24, have-21)
neg(born-24, not-22)
auxpass(born-24, been-23)
ccomp(acceptable-9, born-24)
