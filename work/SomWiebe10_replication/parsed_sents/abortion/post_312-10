(ROOT
  (S
    (NP
      (NP (DT A) (NN person))
      (, ,)
      (PP (IN by)
        (NP (JJ official) (, ,) (JJ authoritative) (NN definition)))
      (, ,))
    (VP (VBZ is)
      (NP
        (NP (DT a) (VBG living) (NN human))
        (VP (VBG being))))
    (. .)))

det(person-2, A-1)
nsubj(human-13, person-2)
amod(definition-8, official-5)
amod(definition-8, authoritative-7)
prep_by(person-2, definition-8)
cop(human-13, is-10)
det(human-13, a-11)
amod(human-13, living-12)
root(ROOT-0, human-13)
partmod(human-13, being-14)

