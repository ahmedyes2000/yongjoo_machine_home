(ROOT
  (S (`` ``)
    (NP (DT Those) (NNS people))
    (VP (VBP deserve)
      (NP (DT that) (NN choice))
      (SBAR (RB even) (IN if)
        (S
          (NP (PRP it))
          (VP (VBZ means)
            (SBAR (IN that)
              (S
                (NP (DT a) (NN life))
                (VP (MD wo) (RB n't)
                  (VP (VB enter)
                    (NP (DT this) (NN world))))))))))
    (. .)))

det(people-3, Those-2)
nsubj(deserve-4, people-3)
root(ROOT-0, deserve-4)
det(choice-6, that-5)
dobj(deserve-4, choice-6)
