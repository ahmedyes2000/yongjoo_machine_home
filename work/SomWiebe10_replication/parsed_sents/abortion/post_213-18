  (S
    (SBAR (IN If)
      (S
        (NP (PRP we))
        (VP (VBP do) (RB not)
          (VP (VB take)
            (S
              (NP (NNS steps))
              (VP (TO to)
                (VP
                  (ADVP (RB universally))
                  (VB guarantee)
                  (NP (PRP it)))))))))
    (, ,)
    (X
      (ADVP (RB then))
      (NP (DT the) (JJ only) (NN freedom)))
    (NP (PRP we))
    (VP (VBP have)
      (VP (VBZ is)
        (NP (JJ pure) (NN fantasy))))
    (. .)))

