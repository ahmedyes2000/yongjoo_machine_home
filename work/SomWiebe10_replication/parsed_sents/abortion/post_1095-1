(ROOT
  (S
    (NP (PRP We))
    (VP
      (VP (VBD set)
        (NP (JJ high) (NNS standards))
        (PP (IN for)
          (NP
            (NP (PRP$ our) (NNS children) (POS 's))
            (NN education))))
      (CC and)
      (VP (VBP hold)
        (NP (PRP them))
        (PP (TO to)
          (NP (PRP it)))))
    (. .)))

nsubj(set-2, We-1)
nsubj(hold-11, We-1)
root(ROOT-0, set-2)
amod(standards-4, high-3)
dobj(set-2, standards-4)
poss(children-7, our-6)
poss(education-9, children-7)
prep_for(set-2, education-9)
conj_and(set-2, hold-11)
dobj(hold-11, them-12)
prep_to(hold-11, it-14)

