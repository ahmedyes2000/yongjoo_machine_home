(ROOT
  (S
    (NP (EX There))
    (VP (VBP are)
      (NP
        (NP (NNS cases))
        (SBAR
          (WHADVP (WRB where))
          (S
            (NP (NN school) (NNS students))
            (VP (VBP are)
              (VP (VBN impregnated)
                (PP (IN through)
                  (NP (NN rape)))))))))
    (. .)))

expl(are-2, There-1)
root(ROOT-0, are-2)
nsubj(are-2, cases-3)
advmod(impregnated-8, where-4)
nn(students-6, school-5)
nsubjpass(impregnated-8, students-6)
auxpass(impregnated-8, are-7)
rcmod(cases-3, impregnated-8)
prep_through(impregnated-8, rape-10)

