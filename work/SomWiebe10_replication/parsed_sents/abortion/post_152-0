(ROOT
  (S
    (SBAR
      (WHADVP (WRB When))
      (S
        (NP (NN abortion))
        (VP (VBZ is)
          (ADJP (JJ illegal)))))
    (, ,)
    (NP (JJ many) (NNS women))
    (VP (VBP die)
      (CC or)
      (VBP suffer)
      (NP (JJ serious) (JJ medical) (NNS problems))
      (PP (IN from)
        (NP (DT either) (JJ self-induced)
          (CC or)
          (JJ illegal) (`` ``) (JJ back-alley) ('' '') (NNS abortions))))
    (: ...)))

advmod(illegal-4, When-1)
nsubj(illegal-4, abortion-2)
cop(illegal-4, is-3)
advcl(die-8, illegal-4)
amod(women-7, many-6)
nsubj(die-8, women-7)
nsubj(suffer-10, women-7)
root(ROOT-0, die-8)
conj_or(die-8, suffer-10)
amod(problems-13, serious-11)
amod(problems-13, medical-12)
dobj(die-8, problems-13)
preconj(self-induced-16, either-15)
prep_from(die-8, self-induced-16)
amod(abortions-22, illegal-18)
amod(abortions-22, back-alley-20)
prep_from(die-8, abortions-22)
conj_or(self-induced-16, abortions-22)

