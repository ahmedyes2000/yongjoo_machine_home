(ROOT
  (S
    (NP (JJ Many) (NNS families))
    (VP (VBP are)
      (VP (VBG looking)
        (S
          (VP (TO to)
            (VP (VB adopt))))))
    (. .)))

amod(families-2, Many-1)
nsubj(looking-4, families-2)
xsubj(adopt-6, families-2)
aux(looking-4, are-3)
root(ROOT-0, looking-4)
aux(adopt-6, to-5)
xcomp(looking-4, adopt-6)

