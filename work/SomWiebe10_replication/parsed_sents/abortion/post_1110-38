(ROOT
  (S
    (NP (EX There))
    (VP (VBP are)
      (ADVP
        (NP
          (QP (RB about) (CD 150,000))
          (NNS children))
        (RB up))
      (PP (IN for)
        (NP
          (NP (NN adoption))
          (PP (IN per)
            (NP (NN year))))))
    (. .)))

expl(are-2, There-1)
root(ROOT-0, are-2)
quantmod(150,000-4, about-3)
num(children-5, 150,000-4)
npadvmod(up-6, children-5)
advmod(are-2, up-6)
prep_for(are-2, adoption-8)
prep_per(adoption-8, year-10)

