(ROOT
  (S
    (NP (DT These))
    (VP (VBP are)
      (NP
        (NP (DT the) (JJ very) (NNS principles))
        (SBAR
          (S
            (NP (PRP$ our) (NN nation))
            (VP (VBD was)
              (VP (VBN founded)
                (PRT (RP on))))))))
    (. .)))

nsubj(principles-5, These-1)
cop(principles-5, are-2)
det(principles-5, the-3)
amod(principles-5, very-4)
root(ROOT-0, principles-5)
poss(nation-7, our-6)
nsubjpass(founded-9, nation-7)
auxpass(founded-9, was-8)
rcmod(principles-5, founded-9)
prt(founded-9, on-10)

