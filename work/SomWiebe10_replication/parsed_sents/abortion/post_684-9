(ROOT
  (S
    (NP (NN Conception))
    (VP (VBZ is)
      (NP
        (NP (DT another) (JJ later) (NN event))
        (SBAR
          (WHNP (WDT that))
          (S
            (VP (MD can)
              (VP (VB be)
                (VP (VBN described)
                  (NP (DT the) (JJ same) (NN way)))))))))
    (. .)))

nsubj(event-5, Conception-1)
cop(event-5, is-2)
det(event-5, another-3)
amod(event-5, later-4)
root(ROOT-0, event-5)
nsubjpass(described-9, event-5)
aux(described-9, can-7)
