(ROOT
  (S ('' '')
    (NP (PRP I))
    (VP (VBP 'm)
      (ADVP (RB still))
      (VP (VBG waiting)
        (PP (IN for)
          (NP (DT the) (NN evidence)))))
    (. .)))

nsubj(waiting-5, I-2)
aux(waiting-5, 'm-3)
advmod(waiting-5, still-4)
root(ROOT-0, waiting-5)
det(evidence-8, the-7)
prep_for(waiting-5, evidence-8)

