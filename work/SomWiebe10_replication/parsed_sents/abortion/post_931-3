(ROOT
  (S
    (PP (IN Because)
      (PP (IN with)
        (NP (DT that) (NN ruling))))
    (NP
      (NP (DT a) (JJ whole) (NN class))
      (PP (IN of)
        (NP (NNS people))))
    (VP
      (VP
        (ADVP (RB just))
        (VBD became)
        (NP (NN non) (NNS persons)))
      (CC and)
      (VP
        (ADVP (RB no) (RB longer))
        (JJ protected)
        (PP (IN under)
          (NP (DT the) (NNP Constitution)))))
    (. .)))

prep(persons-13, Because-1)
pcomp(Because-1, with-2)
det(ruling-4, that-3)
prep_with(Because-1, ruling-4)
det(class-7, a-5)
amod(class-7, whole-6)
nsubj(persons-13, class-7)
nsubj(protected-17, class-7)
prep_of(class-7, people-9)
advmod(persons-13, just-10)
cop(persons-13, became-11)
nn(persons-13, non-12)
root(ROOT-0, persons-13)
advmod(longer-16, no-15)
advmod(protected-17, longer-16)
conj_and(persons-13, protected-17)
det(Constitution-20, the-19)
prep_under(protected-17, Constitution-20)

