(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP (VBP are)
          (ADJP (JJ smart)))))
    (, ,)
    (NP (PRP you))
    (VP (MD will)
      (VP (VB know)
        (SBAR (IN that)
          (S
            (NP (NN birth) (NN control))
            (VP (VBZ fails))))))
    (. .)))

mark(smart-4, If-1)
nsubj(smart-4, you-2)
cop(smart-4, are-3)
advcl(know-8, smart-4)
nsubj(know-8, you-6)
aux(know-8, will-7)
root(ROOT-0, know-8)
complm(fails-12, that-9)
