  (S
    (NP (PRP We))
    (VP (MD should)
      (ADVP (RB never))
      (VP (VB take)
        (NP (DT the) (NN law))
        (PP (IN as)
          (NP (DT the) (CD one) (JJ ethical) (NN truth)))))
    (. .)))

