(ROOT
  (S
    (S
      (NP (PRP I))
      (VP (VBP have)
        (NP
          (NP (JJ personal) (NNS friends))
          (SBAR
            (WHNP (WP who))
            (S
              (VP (VBP have)
                (VP (VBN had)
                  (NP (DT an) (NN abortion)))))))))
    (CC and)
    (S
      (NP (PRP I))
      (VP (VBP love)
        (NP (PRP them))))
    (. .)))

nsubj(have-2, I-1)
root(ROOT-0, have-2)
amod(friends-4, personal-3)
dobj(have-2, friends-4)
nsubj(had-7, friends-4)
aux(had-7, have-6)
rcmod(friends-4, had-7)
det(abortion-9, an-8)
dobj(had-7, abortion-9)
nsubj(love-12, I-11)
conj_and(have-2, love-12)
dobj(love-12, them-13)

