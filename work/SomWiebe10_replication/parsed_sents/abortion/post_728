(ROOT
  (FRAG
    (ADJP (JJ thats))
    (SBAR
      (WHNP (WP what))
      (S
        (NP (DT the) (JJ other) (NN debater))
        (VP (VBD said))))
    (. .)))

root(ROOT-0, thats-1)
dobj(said-6, what-2)
det(debater-5, the-3)
amod(debater-5, other-4)
nsubj(said-6, debater-5)
dep(thats-1, said-6)

(ROOT
  (SBAR (IN unless)
    (S
      (S
        (NP (PRP it))
        (VP (VBD was)
          (NP (DT an) (NN accident))))
      (CC or)
      (S
        (NP (PRP$ her) (NN life))
        (VP (VBD was)
          (PP (IN in)
            (NP (NN dangerr))))))))

mark(accident-5, unless-1)
nsubj(accident-5, it-2)
cop(accident-5, was-3)
det(accident-5, an-4)
root(ROOT-0, accident-5)
poss(life-8, her-7)
nsubj(was-9, life-8)
conj_or(accident-5, was-9)
prep_in(was-9, dangerr-11)

