(ROOT
  (S
    (NP (DT The) (NN fact))
    (NP (PRP we))
    (VP (VBP know)
      (SBAR (IN that)
        (S
          (S
            (NP (NN life))
            (VP (VBZ is)
              (VP (VBN conceived))))
          (CC or)
          (S
            (NP (PRP you))
            (VP (MD would)
              (VP (VB say)
                (NP (NN potential))
                (PP (TO to)
                  (NP (NN life)))
                (PP (IN at)
                  (NP (NN conception)))))))))
    (. .)))

det(fact-2, The-1)
nsubj(know-4, fact-2)
nsubj(know-4, we-3)
root(ROOT-0, know-4)
complm(conceived-8, that-5)
nsubjpass(conceived-8, life-6)
auxpass(conceived-8, is-7)
ccomp(know-4, conceived-8)
nsubj(say-12, you-10)
aux(say-12, would-11)
ccomp(know-4, say-12)
conj_or(conceived-8, say-12)
dobj(say-12, potential-13)
prep_to(say-12, life-15)
prep_at(say-12, conception-17)

