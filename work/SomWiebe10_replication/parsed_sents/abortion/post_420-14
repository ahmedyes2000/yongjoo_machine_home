(ROOT
  (S
    (NP (DT That))
    (VP (VBZ is)
      (SBAR (IN because)
        (S
          (NP (EX there))
          (VP (VBZ is)
            (NP (DT no) (NN refutation))))))
    (. .)))

nsubj(is-2, That-1)
root(ROOT-0, is-2)
mark(is-5, because-3)
expl(is-5, there-4)
advcl(is-2, is-5)
det(refutation-7, no-6)
nsubj(is-5, refutation-7)

