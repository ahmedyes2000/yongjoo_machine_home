(ROOT
  (S
    (NP (DT The) (NN mother))
    (VP (VBZ is) (RB not)
      (ADVP (RB just)
        (NP (DT some) (NN adult)))
      (ADJP (JJ willing)
        (S
          (VP (TO to)
            (VP (VB take)
              (NP
                (NP (NN care))
                (PP (IN of)
                  (NP (DT the) (NN child)))))))))
    (. .)))

det(mother-2, The-1)
nsubj(willing-8, mother-2)
cop(willing-8, is-3)
neg(willing-8, not-4)
advmod(willing-8, just-5)
det(adult-7, some-6)
pobj(just-5, adult-7)
root(ROOT-0, willing-8)
aux(take-10, to-9)
xcomp(willing-8, take-10)
dobj(take-10, care-11)
det(child-14, the-13)
