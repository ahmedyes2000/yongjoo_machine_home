(ROOT
  (FRAG
    (SBAR
      (WHADVP (WRB when))
      (S
        (NP (PRP you))
        (VP (VBP have)
          (NP (DT the) (NN power)
            (S
              (VP (TO to)
                (VP (VB save)
                  (NP (DT that) (NN child)))))))))
    (. .)))

advmod(have-3, when-1)
nsubj(have-3, you-2)
root(ROOT-0, have-3)
det(power-5, the-4)
dobj(have-3, power-5)
aux(save-7, to-6)
infmod(power-5, save-7)
det(child-9, that-8)
dobj(save-7, child-9)

