  (S
    (S
      (NP (DT The) (NN question))
      (VP (VBZ is) (RB not)
        (ADJP (RB about) (JJ person-hood))))
    (, ,)
    (CC but)
    (S
      (NP
        (NP (NN humanity))
        (CC and)
        (NP (DT that)))
      (VP (VBZ is)
        (ADJP (RB genetically) (JJ verifiable))))
    (. .)))

det(question-2, The-1)
nsubj(person-hood-6, question-2)
cop(person-hood-6, is-3)
neg(person-hood-6, not-4)
