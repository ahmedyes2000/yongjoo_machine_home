(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (DT the) (NN question))
        (VP (VBD was)
          (VP
            (ADVP (RB precisely))
            (VBN defined)))))
    (NP (PRP we))
    (VP (MD might)
      (VP (VB come)
        (PP (TO to)
          (NP (NNS terms)))))
    (. .)))

mark(defined-6, If-1)
det(question-3, the-2)
nsubjpass(defined-6, question-3)
auxpass(defined-6, was-4)
advmod(defined-6, precisely-5)
advcl(come-9, defined-6)
nsubj(come-9, we-7)
aux(come-9, might-8)
root(ROOT-0, come-9)
prep_to(come-9, terms-11)

