    (NP (PRP you))
    (VP (VBP have)
      (ADVP (RB once) (RB again))
      (VP (VBD proclaimed)
        (PP (IN without)
          (NP (DT any) (NN justification)))
        (SBAR
          (SBAR (IN that)
            (S
              (NP (DT these) (NNS cases))
              (VP (MD will) (RB not)
                (VP (VB be)
                  (VP (VBN considered))))))
          (, ,)
          (CC but)
          (SBAR
            (WHNP (WDT that))
            (S
              (VP (VBZ 's) (RB not)
                (ADJP (JJ good) (RB enough))))))))
    (. .)))

