(ROOT
  (S
    (SBAR (IN Because)
      (S
        (NP (DT an) (JJ unwanted) (NN child))
        (VP (MD can)
          (VP (VB lead)
            (PP (TO to)
              (NP (DT an) (JJ abused) (NN child)))))))
    (, ,)
    (ADVP (RB therefore))
    (NP (PRP they))
    (VP (MD can)
      (VP (VB choose)
        (NP (NN adoption))))
    (. .)))

mark(lead-6, Because-1)
det(child-4, an-2)
amod(child-4, unwanted-3)
nsubj(lead-6, child-4)
aux(lead-6, can-5)
advcl(choose-15, lead-6)
det(child-10, an-8)
amod(child-10, abused-9)
prep_to(lead-6, child-10)
advmod(choose-15, therefore-12)
nsubj(choose-15, they-13)
aux(choose-15, can-14)
root(ROOT-0, choose-15)
dobj(choose-15, adoption-16)

