(ROOT
  (SQ (VBP Do)
    (NP (PRP I))
    (VP (VB risk)
      (NP (PRP$ my) (JJ own) (NN life))
      (PP (IN by)
        (S
          (VP (VBG undergoing)
            (NP (JJ major) (NN surgery))))))
    (. ?)))

aux(risk-3, Do-1)
nsubj(risk-3, I-2)
root(ROOT-0, risk-3)
poss(life-6, my-4)
amod(life-6, own-5)
dobj(risk-3, life-6)
prepc_by(risk-3, undergoing-8)
amod(surgery-10, major-9)
dobj(undergoing-8, surgery-10)

