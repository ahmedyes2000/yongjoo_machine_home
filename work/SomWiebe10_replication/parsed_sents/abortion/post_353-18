(ROOT
  (S
    (NP (NNS pro-lifers))
    (ADVP (RB just))
    (VP (VBP need)
      (NP
        (NP (JJR better) (NNS lawyers))
        (VP (VBG acting)
          (PP (IN in)
            (NP (DT these) (JJ small) (NNS circles))))))
    (. .)))

nsubj(need-3, pro-lifers-1)
advmod(need-3, just-2)
root(ROOT-0, need-3)
amod(lawyers-5, better-4)
dobj(need-3, lawyers-5)
partmod(lawyers-5, acting-6)
det(circles-10, these-8)
amod(circles-10, small-9)
prep_in(acting-6, circles-10)

