(ROOT
  (S
    (S
      (NP (PRP I))
      (VP (VBD was)
        (VP (VBN exposed)
          (PP (TO to)
            (NP (PRP it)))
          (PP (IN as)
            (NP (JJ little) (NN boy))))))
    (CC and)
    (S
      (NP (PRP it))
      (VP (VBD became)
        (NP
          (NP (DT a) (NN passion))
          (SBAR
            (WHNP (WDT that))
            (S
              (VP (VBD was)
                (ADVP (RB never))
                (ADJP (JJ satisfied))))))))
    (. .)))

nsubjpass(exposed-3, I-1)
auxpass(exposed-3, was-2)
root(ROOT-0, exposed-3)
prep_to(exposed-3, it-5)
amod(boy-8, little-7)
