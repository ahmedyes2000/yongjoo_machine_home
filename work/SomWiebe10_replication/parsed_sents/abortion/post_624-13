(ROOT
  (NP
    (NP (JJR More) (NNS people))
    (VP (VBG committing)
      (NP (NNS crimes) (, ,) (JJR more) (NN suffering))
      (SBAR (IN until)
        (S
          (ADVP (RB finally))
          (NP (PRP we))
          (VP (VBP destroy)
            (NP (DT each) (JJ other))))))
    (. ?)))

amod(people-2, More-1)
root(ROOT-0, people-2)
partmod(people-2, committing-3)
nn(suffering-7, crimes-4)
amod(suffering-7, more-6)
dobj(committing-3, suffering-7)
mark(destroy-11, until-8)
advmod(destroy-11, finally-9)
nsubj(destroy-11, we-10)
advcl(committing-3, destroy-11)
det(other-13, each-12)
dobj(destroy-11, other-13)

