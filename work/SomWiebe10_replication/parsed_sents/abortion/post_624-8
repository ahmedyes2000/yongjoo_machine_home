(ROOT
  (S
    (NP (EX There))
    (VP (MD will)
      (VP (VB be)
        (NP
          (NP (DT no) (JJR more) (NN death))
          (CC or)
          (NP
            (NP (VBG mourning)
              (CC or)
              (NN crying))
            (CC or)
            (NP (NN pain))))))
    (: ...)))

expl(death-6, There-1)
aux(death-6, will-2)
cop(death-6, be-3)
det(death-6, no-4)
amod(death-6, more-5)
root(ROOT-0, death-6)
conj_or(death-6, mourning-8)
conj_or(death-6, crying-10)
conj_or(mourning-8, crying-10)
conj_or(death-6, pain-12)
conj_or(mourning-8, pain-12)

