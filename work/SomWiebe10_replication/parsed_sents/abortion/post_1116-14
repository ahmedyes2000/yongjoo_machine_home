(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (DT this))
        (VP (VBZ causes)
          (NP
            (NP (DT a) (NN problem))
            (PRN (-LRB- -LRB-)
              (NP (NN abortion))
              (-RRB- -RRB-)))
          (SBAR (IN than)
            (S
              (NP (PRP we))
              (VP (MD must)
                (VP (VB say)
                  (NP (NN something)))))))))
    (VP (VBZ is)
      (ADJP (JJ dysfunctional)
        (CC or)
        (JJ unhealthy)))
    (. .)))

mark(causes-3, If-1)
nsubj(causes-3, this-2)
csubj(dysfunctional-15, causes-3)
csubj(unhealthy-17, causes-3)
det(problem-5, a-4)
dobj(causes-3, problem-5)
appos(problem-5, abortion-7)
mark(say-12, than-9)
nsubj(say-12, we-10)
aux(say-12, must-11)
advcl(causes-3, say-12)
dobj(say-12, something-13)
cop(dysfunctional-15, is-14)
root(ROOT-0, dysfunctional-15)
conj_or(dysfunctional-15, unhealthy-17)

