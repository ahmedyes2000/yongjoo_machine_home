(ROOT
  (S
    (NP
      (NP (NN None))
      (PP (IN of)
        (NP (DT those))))
    (VP (VBZ reasons)
      (S
        (NP (PRP you))
        (VP (VB give)
          (VP (VBN come)
            (ADVP (RB close)
              (PP (TO to)
                (NP (JJ proper) (NN justification))))
            (PP (IN for)
              (NP
                (NP (DT the) (NN taking))
                (PP (IN of)
                  (NP (JJ human) (NN life)))))))))
    (. .)))

nsubj(reasons-4, None-1)
prep_of(None-1, those-3)
root(ROOT-0, reasons-4)
nsubj(give-6, you-5)
ccomp(reasons-4, give-6)
dep(give-6, come-7)
amod(justification-11, proper-10)
prep_close_to(come-7, justification-11)
det(taking-14, the-13)
prep_for(come-7, taking-14)
amod(life-17, human-16)
prep_of(taking-14, life-17)

