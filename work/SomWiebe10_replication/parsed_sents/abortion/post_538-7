(ROOT
  (FRAG
    (ADJP
      (WHADVP (WRB How))
      (JJ unfortunate))
    (SBAR (IN that)
      (S
        (NP
          (NP (NNS decisions))
          (PP (IN in)
            (NP (NN life))))
        (VP (VBP are)
          (VP (VBN made)
            (PP (RB because) (IN of)
              (NP
                (NP (DT the) (JJ geographical) (NN location))
                (SBAR
                  (WHADVP (WRB where))
                  (S
                    (NP (PRP one))
                    (VP (VBZ is)
                      (VP (VBN born)))))))))))
    (. .)))

advmod(unfortunate-2, How-1)
root(ROOT-0, unfortunate-2)
dep(made-8, that-3)
nsubjpass(made-8, decisions-4)
prep_in(decisions-4, life-6)
auxpass(made-8, are-7)
dep(unfortunate-2, made-8)
