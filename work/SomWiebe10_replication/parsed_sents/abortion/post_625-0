(ROOT
  (SQ (VBZ Is)
    (NP (PRP it))
    (ADJP (JJ unethical)
      (CC and)
      (JJ immoral))
    (S
      (VP (TO to)
        (VP (VB impose)
          (NP (DT a) (NN fact))
          (PP (VBN based)
            (PP (IN on)
              (NP (JJ scientific) (NN evidence)))))))
    (. !)))

