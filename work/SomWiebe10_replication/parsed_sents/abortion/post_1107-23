(ROOT
  (S
    (NP (DT The) (JJ right) (NN thing)
      (S
        (VP (TO to)
          (VP (VB do)))))
    (VP (VBZ is)
      (VP (VB give)
        (NP (PRP them))
        (PP (TO to)
          (NP
            (NP (NN someone))
            (SBAR
              (WHNP (WP who))
              (S
                (VP (MD will))))))))
    (. .)))

det(thing-3, The-1)
amod(thing-3, right-2)
nsubj(give-7, thing-3)
aux(do-5, to-4)
infmod(thing-3, do-5)
aux(give-7, is-6)
root(ROOT-0, give-7)
dobj(give-7, them-8)
prep_to(give-7, someone-10)
nsubj(will-12, someone-10)
rcmod(someone-10, will-12)

