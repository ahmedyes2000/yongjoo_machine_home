(ROOT
  (S
    (PP (IN Because)
      (PP (IN without)
        (NP (NN faith))))
    (, ,)
    (NP (PRP you))
    (ADVP (RB really))
    (VP (VBP have)
      (NP (NN nothing)))
    (. .)))

prep(have-7, Because-1)
pcomp(Because-1, without-2)
prep_without(Because-1, faith-3)
nsubj(have-7, you-5)
advmod(have-7, really-6)
root(ROOT-0, have-7)
dobj(have-7, nothing-8)

