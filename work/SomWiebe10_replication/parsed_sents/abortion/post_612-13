(ROOT
  (S
    (NP (PRP We))
    (VP (VBP do) (RB not)
      (VP (VB have)
        (NP (DT a)
          (ADJP (JJ black)
            (CC and)
            (JJ white))
          (NN system))
        (SBAR
          (WHADVP (WRB where))
          (S
            (NP (DT every) (NN human))
            (VP (VBZ gets)
              (NP (DT the) (JJ same) (NN treatment)))))))
    (. .)))

nsubj(have-4, We-1)
aux(have-4, do-2)
neg(have-4, not-3)
root(ROOT-0, have-4)
det(system-9, a-5)
amod(system-9, black-6)
conj_and(black-6, white-8)
amod(system-9, white-8)
