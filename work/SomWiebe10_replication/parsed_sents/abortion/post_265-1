(ROOT
  (S
    (ADVP (RB Unfortunately))
    (, ,)
    (NP (PRP I))
    (VP (VBP believe)
      (SBAR (IN that)
        (S
          (NP (PRP you))
          (VP (VBP are)
            (NP (DT the) (NN exception))))))
    (. .)))

advmod(believe-4, Unfortunately-1)
nsubj(believe-4, I-3)
root(ROOT-0, believe-4)
complm(exception-9, that-5)
nsubj(exception-9, you-6)
cop(exception-9, are-7)
det(exception-9, the-8)
ccomp(believe-4, exception-9)

