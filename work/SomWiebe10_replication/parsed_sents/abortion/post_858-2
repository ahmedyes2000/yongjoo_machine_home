(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (NNS abortions))
        (VP (VBP are) (RB not)
          (ADJP (JJ legal)))))
    (, ,)
    (NP
      (NP (NNS women))
      (CC and)
      (NP (JJ young) (NNS girls)))
    (VP (MD will)
      (VP (VB be)
        (VP (VBG having)
          (NP
            (NP (NNS abortions))
            (PP (IN in)
              (NP
                (NP (NNS places))
                (PP (JJ such) (IN as)
                  (NP
                    (NP (NN motel) (NNS rooms))
                    (, ,)
                    (ADVP (RB back))
                    (NP (NN street) (NNS alleys))
                    (, ,) (FW etc.) (, ,)))))
            (SBAR
              (WHNP (WDT which))
              (S
                (VP (VBP are)
                  (ADJP (RB very) (JJ unsanitary)
                    (CC and)
                    (JJ dangerous)))))))))
    (. .)))

mark(legal-5, If-1)
nsubj(legal-5, abortions-2)
cop(legal-5, are-3)
neg(legal-5, not-4)
advcl(having-13, legal-5)
nsubj(having-13, women-7)
amod(girls-10, young-9)
conj_and(women-7, girls-10)
nsubj(having-13, girls-10)
aux(having-13, will-11)
aux(having-13, be-12)
root(ROOT-0, having-13)
dobj(having-13, abortions-14)
nsubj(unsanitary-31, abortions-14)
nsubj(dangerous-33, abortions-14)
prep_in(abortions-14, places-16)
nn(rooms-20, motel-19)
