(ROOT
  (S
    (NP (DT This))
    (VP (VBZ is)
      (NP
        (NP (DT an) (NN issue))
        (PP (IN of)
          (NP
            (NP (NN law)
              (CC and)
              (NN society))
            (, ,) (RB not)
            (NP (NN science))))))
    (. .)))

nsubj(issue-4, This-1)
cop(issue-4, is-2)
det(issue-4, an-3)
root(ROOT-0, issue-4)
prep_of(issue-4, law-6)
prep_of(issue-4, society-8)
conj_and(law-6, society-8)
neg(law-6, not-10)
dep(law-6, science-11)

