(ROOT
  (S
    (NP (PRP We))
    (VP (VBP have)
      (NP
        (NP
          (QP (RB nearly) (CD 500,000))
          (NNS children))
        (VP (VBG living)
          (PP (IN in)
            (NP (JJ foster) (NN care))))))
    (. .)))

nsubj(have-2, We-1)
root(ROOT-0, have-2)
quantmod(500,000-4, nearly-3)
num(children-5, 500,000-4)
dobj(have-2, children-5)
partmod(children-5, living-6)
amod(care-9, foster-8)
prep_in(living-6, care-9)

