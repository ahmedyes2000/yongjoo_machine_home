(ROOT
  (S
    (NP
      (NP (DT That) (NNP Bible) (NN verse))
      (, ,)
      (NP (NNP Jeremiah) (CD 1:5)))
    (VP (VBZ is)
      (VP (VBG saying)
        (SBAR (IN that)
          (S
            (NP (NNP God))
            (VP (VBD intended)
              (NP (DT a) (JJ unique) (JJ human) (NN being))
              (, ,)
              (PP (VBG starting)
                (PP (IN at)
                  (NP (NN conception)))))))))
    (. .)))

det(verse-3, That-1)
nn(verse-3, Bible-2)
nsubj(saying-8, verse-3)
appos(verse-3, Jeremiah-5)
num(Jeremiah-5, 1:5-6)
aux(saying-8, is-7)
root(ROOT-0, saying-8)
complm(intended-11, that-9)
nsubj(intended-11, God-10)
ccomp(saying-8, intended-11)
det(being-15, a-12)
amod(being-15, unique-13)
amod(being-15, human-14)
dobj(intended-11, being-15)
prep(intended-11, starting-17)
pcomp(starting-17, at-18)
prep_at(starting-17, conception-19)

