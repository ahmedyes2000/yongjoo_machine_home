(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP they))
        (VP (VBP are)
          (ADJP (JJ equal)))))
    (, ,)
    (NP
      (NP (DT the) (NNS desires))
      (PP (IN of)
        (NP (DT each))))
    (VP (MD must)
      (VP (VB be)
        (VP (VBN considered)
          (ADVP (RB equally)))))
    (. .)))

mark(equal-4, If-1)
nsubj(equal-4, they-2)
cop(equal-4, are-3)
advcl(considered-12, equal-4)
det(desires-7, the-6)
nsubjpass(considered-12, desires-7)
prep_of(desires-7, each-9)
aux(considered-12, must-10)
auxpass(considered-12, be-11)
root(ROOT-0, considered-12)
advmod(considered-12, equally-13)

