  (S
    (NP
      (NP (DT The) (JJ legal) (NNS consequences))
      (PP (IN of)
        (NP (PDT such) (DT an) (NN amendment))))
    (VP (MD would)
      (VP (VB be)
        (ADJP (JJ massive))))
    (. .)))

