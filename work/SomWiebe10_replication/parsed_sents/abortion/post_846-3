(ROOT
  (S
    (VP
      (VP (VB Review)
        (NP (PRP$ your) (NNS statements)))
      (CC and)
      (VP (VB make)
        (ADJP (JJ sure)
          (SBAR
            (S
              (NP (PRP they))
              (VP (VBP 're)
                (PP (IN on)
                  (NP
                    (NP (NN topic))
                    (, ,)
                    (NP (NNS guys))))))))))
    (. .)))

root(ROOT-0, Review-1)
poss(statements-3, your-2)
dobj(Review-1, statements-3)
conj_and(Review-1, make-5)
acomp(make-5, sure-6)
nsubj('re-8, they-7)
dep(sure-6, 're-8)
prep_on('re-8, topic-10)
appos(topic-10, guys-12)

