(ROOT
  (S
    (NP (PRP I))
    (VP (VBP agree)
      (PP (IN with)
        (NP (PRP you)))
      (PP (IN except) (IN on)
        (NP
          (NP (DT the) (NN issue))
          (PP (IN of)
            (NP (NN rape))))))
    (. .)))

nsubj(agree-2, I-1)
root(ROOT-0, agree-2)
prep_with(agree-2, you-4)
dep(agree-2, except-5)
det(issue-8, the-7)
prep_on(agree-2, issue-8)
prep_of(issue-8, rape-10)

(ROOT
  (SBARQ
    (WHADVP (WRB Why))
    (SQ (MD should)
      (NP (DT the) (NN child))
      (VP (VB be)
        (VP (VBN killed)
          (PP (IN for)
            (NP
              (NP (DT a) (NN crime))
              (SBAR
                (S
                  (NP (PRP they))
                  (VP (VBD did) (RB n't)
                    (VP (VB commit))))))))))
    (. ?)))

advmod(killed-6, Why-1)
aux(killed-6, should-2)
det(child-4, the-3)
nsubjpass(killed-6, child-4)
auxpass(killed-6, be-5)
root(ROOT-0, killed-6)
det(crime-9, a-8)
prep_for(killed-6, crime-9)
dobj(commit-13, crime-9)
nsubj(commit-13, they-10)
aux(commit-13, did-11)
neg(commit-13, n't-12)
rcmod(crime-9, commit-13)

