(ROOT
  (S
    (NP (DT The) (NN person))
    (, ,)
    (ADVP (RB therefore))
    (, ,)
    (VP (VBZ begins)
      (PP (IN at)
        (NP (NN conception))))
    (. .)))

det(person-2, The-1)
nsubj(begins-6, person-2)
advmod(begins-6, therefore-4)
root(ROOT-0, begins-6)
prep_at(begins-6, conception-8)

