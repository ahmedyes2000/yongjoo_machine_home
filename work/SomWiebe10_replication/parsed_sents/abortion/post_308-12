(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is) (RB not)
      (VP (VBN based)
        (PP (IN on)
          (NP (JJ moral) (NN relativism)))))
    (. .)))

nsubjpass(based-4, It-1)
auxpass(based-4, is-2)
neg(based-4, not-3)
root(ROOT-0, based-4)
amod(relativism-7, moral-6)
prep_on(based-4, relativism-7)

