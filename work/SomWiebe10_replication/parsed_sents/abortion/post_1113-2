(ROOT
  (S
    (NP (PRP It))
    (VP (MD can) (RB not)
      (VP (VB be)
        (VP (VBN disposed)
          (PP (IN of)
            (SBAR
              (WHADVP (WRB when))
              (S
                (NP (DT an) (NN individual))
                (VP (VBZ does) (RB n't)
                  (VP (VB want)
                    (NP (PRP it))))))))))
    (. .)))

nsubjpass(disposed-5, It-1)
aux(disposed-5, can-2)
neg(disposed-5, not-3)
auxpass(disposed-5, be-4)
root(ROOT-0, disposed-5)
advmod(want-12, when-7)
det(individual-9, an-8)
nsubj(want-12, individual-9)
aux(want-12, does-10)
neg(want-12, n't-11)
prepc_of(disposed-5, want-12)
dobj(want-12, it-13)

