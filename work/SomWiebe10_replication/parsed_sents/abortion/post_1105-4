(ROOT
  (S
    (NP (DT That))
    (VP (VBZ 's)
      (NP
        (NP (DT an) (VBN accepted) (JJ scientific) (NN fact))
        (SBAR
          (WHNP (WDT that))
          (S
            (VP (VBZ is) (RB not)
              (PP (IN in)
                (NP (NN question))))))))
    (. .)))

nsubj(fact-6, That-1)
cop(fact-6, 's-2)
det(fact-6, an-3)
amod(fact-6, accepted-4)
amod(fact-6, scientific-5)
root(ROOT-0, fact-6)
nsubj(is-8, fact-6)
rcmod(fact-6, is-8)
neg(is-8, not-9)
prep_in(is-8, question-11)

