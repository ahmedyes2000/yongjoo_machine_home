(ROOT
  (S
    (ADVP (RB However))
    (, ,)
    (NP (PRP we))
    (VP (MD 'll)
      (ADVP (RB never))
      (VP (VB know)
        (SBAR (IN as)
          (S
            (NP (PRP they))
            (VP (VBD did) (RB n't)
              (VP (VB get)
                (NP (DT a) (NN chance))))))))
    (. .)))

advmod(know-6, However-1)
nsubj(know-6, we-3)
aux(know-6, 'll-4)
neg(know-6, never-5)
root(ROOT-0, know-6)
mark(get-11, as-7)
nsubj(get-11, they-8)
aux(get-11, did-9)
neg(get-11, n't-10)
advcl(know-6, get-11)
det(chance-13, a-12)
dobj(get-11, chance-13)

