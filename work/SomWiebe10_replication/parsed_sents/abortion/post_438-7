(ROOT
  (S
    (S
      (VP (VBG Watch)
        (NP (DT this))))
    (: :)
    (S
      (NP (NNP Birds))
      (VP (VBP possess)
        (NP (DT a) (NN heartbeat))))
    (. .)))

root(ROOT-0, Watch-1)
dobj(Watch-1, this-2)
nsubj(possess-5, Birds-4)
parataxis(Watch-1, possess-5)
det(heartbeat-7, a-6)
dobj(possess-5, heartbeat-7)

