(ROOT
  (S (: ...)
    (ADJP (RB mr.) (JJ richardson))
    (, ,)
    (NP (FW i))
    (ADVP (RB never))
    (VP (VBD said)
      (SBAR
        (S
          (NP (PRP you))
          (VP (VBD said)
            (SBAR
              (S
                (NP (NN abortion))
                (VP (VBD was)
                  (ADJP (JJ unnatural)))))))))
    (. .)))

advmod(richardson-3, mr.-2)
dep(said-7, richardson-3)
nsubj(said-7, i-5)
neg(said-7, never-6)
root(ROOT-0, said-7)
nsubj(said-9, you-8)
ccomp(said-7, said-9)
nsubj(unnatural-12, abortion-10)
cop(unnatural-12, was-11)
ccomp(said-9, unnatural-12)

