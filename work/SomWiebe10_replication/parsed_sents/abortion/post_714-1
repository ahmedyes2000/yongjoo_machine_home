(ROOT
  (S (CC But)
    (NP (PRP I))
    (VP (VBP know)
      (SBAR
        (WHNP (WP what))
        (S
          (NP (PRP He))
          (VP (VBZ says)
            (PP (IN in)
              (NP (DT the) (NNP Bible)))))))
    (. .)))

cc(know-3, But-1)
nsubj(know-3, I-2)
root(ROOT-0, know-3)
dobj(says-6, what-4)
nsubj(says-6, He-5)
ccomp(know-3, says-6)
det(Bible-9, the-8)
prep_in(says-6, Bible-9)

