(ROOT
  (S
    (PP (IN about)
      (NP
        (NP (DT the) (NN reasoning))
        (PP (IN behind)
          (NP (DT the) (CD two) (NNS people)))))
    (NP (PRP you))
    (VP (VBP mention)
      (SBAR
        (S
          (NP (PRP I))
          (VP (MD will)
            (VP (VB stick)
              (PP (TO to)
                (NP (PRP$ my) (JJ own) (NNS thoughts))))))))
    (. .)))

det(reasoning-3, the-2)
prep_about(mention-9, reasoning-3)
det(people-7, the-5)
num(people-7, two-6)
prep_behind(reasoning-3, people-7)
nsubj(mention-9, you-8)
root(ROOT-0, mention-9)
nsubj(stick-12, I-10)
aux(stick-12, will-11)
ccomp(mention-9, stick-12)
poss(thoughts-16, my-14)
