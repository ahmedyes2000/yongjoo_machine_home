(ROOT
  (S
    (NP (DT These))
    (VP (VBP are)
      (NP
        (NP (NNS questions))
        (PP (IN about)
          (NP (NN value) (NNS determinations)))))
    (. .)))

nsubj(questions-3, These-1)
cop(questions-3, are-2)
root(ROOT-0, questions-3)
nn(determinations-6, value-5)
prep_about(questions-3, determinations-6)

