(ROOT
  (S
    (NP (NNP BUT))
    (, ,)
    (ADVP (RB then))
    (NP
      (NP (DT the) (NN issue))
      (PP (IN of)
        (NP (NN rape))))
    (VP (VBZ comes)
      (PP (IN into)
        (NP (NN play))))
    (. .)))

nsubj(comes-8, BUT-1)
advmod(comes-8, then-3)
det(issue-5, the-4)
nsubj(comes-8, issue-5)
prep_of(issue-5, rape-7)
root(ROOT-0, comes-8)
prep_into(comes-8, play-10)

