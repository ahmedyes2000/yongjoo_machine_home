(ROOT
  (S
    (NP (DT This) (NN woman))
    (VP (VBZ knows)
      (NP (JJ first) (NN hand))
      (SBAR
        (SBAR
          (WHNP (WP what))
          (S
            (NP (NN abortion))
            (VP (VBZ is))))
        (CC and)
        (SBAR
          (WHNP (WP what))
          (S
            (NP (PRP it))
            (VP (VBZ does))))))
    (. .)))

det(woman-2, This-1)
nsubj(knows-3, woman-2)
root(ROOT-0, knows-3)
amod(hand-5, first-4)
dobj(knows-3, hand-5)
dobj(is-8, what-6)
nsubj(is-8, abortion-7)
dep(knows-3, is-8)
dobj(does-12, what-10)
nsubj(does-12, it-11)
dep(knows-3, does-12)
conj_and(is-8, does-12)

