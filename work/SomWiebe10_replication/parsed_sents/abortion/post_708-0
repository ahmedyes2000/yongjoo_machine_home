(ROOT
  (S
    (NP
      (NP (VBG Passing) (NNS laws))
      (PP (IN on)
        (NP
          (NP (DT the) (NN basis))
          (PP (IN of)
            (NP
              (NP (CD one) (NN religion) (POS 's))
              (NNS beliefs))))))
    (VP (VBZ undercuts)
      (NP
        (NP (PRP$ our) (NN freedom))
        (PP (IN of)
          (NP (NN worship)))))
    (. .)))

amod(laws-2, Passing-1)
nsubj(undercuts-11, laws-2)
det(basis-5, the-4)
prep_on(laws-2, basis-5)
num(religion-8, one-7)
