(ROOT
  (SINV
    (PP (IN Until)
      (NP (RB then)))
    (VP (VBP please)
      (VP (VB take)
        (NP (PRP$ your) (NNS complaints))
        (PP (TO to)
          (NP (NNP Mother)))))
    (NP
      (NP (NN Nature)
        (CC or)
        (NN god))
      (, ,)
      (SBAR
        (WHNP (WDT whichever))
        (S
          (NP (PRP you))
          (VP (VBP believe)
            (S
              (VP (TO to)
                (VP (VB be)
                  (PP (IN at)
                    (NP (NN fault))))))))))
    (. .)))

prep(please-3, Until-1)
pobj(Until-1, then-2)
root(ROOT-0, please-3)
xcomp(please-3, take-4)
poss(complaints-6, your-5)
dobj(take-4, complaints-6)
prep_to(take-4, Mother-8)
nsubj(please-3, Nature-9)
nsubj(please-3, god-11)
conj_or(Nature-9, god-11)
rel(believe-15, whichever-13)
nsubj(believe-15, you-14)
xsubj(be-17, you-14)
rcmod(Nature-9, believe-15)
aux(be-17, to-16)
xcomp(believe-15, be-17)
prep_at(be-17, fault-19)

