(ROOT
  (S
    (NP
      (NP (PRP$ Our) (JJ modern) (JJ popular) (NN obsession))
      (PP (IN with)
        (NP
          (NP (DT the) (NN importance))
          (PP (IN of)
            (NP (NNP DNA))))))
    (VP (VBZ leads)
      (PP (TO to)
        (NP (DT the) (NN idea)))
      (SBAR (IN that)
        (S
          (NP (NN conception))
          (VP (VBZ is)
            (NP (DT the)
              (ADJP (RBS most) (JJ important))
              (NN benchmark))))))
    (. .)))

poss(obsession-4, Our-1)
amod(obsession-4, modern-2)
amod(obsession-4, popular-3)
nsubj(leads-10, obsession-4)
det(importance-7, the-6)
prep_with(obsession-4, importance-7)
prep_of(importance-7, DNA-9)
root(ROOT-0, leads-10)
det(idea-13, the-12)
prep_to(leads-10, idea-13)
