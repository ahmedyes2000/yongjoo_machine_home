(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP (VBP do) (RB not)
          (VP (VB want)
            (NP (DT a) (NN child))))))
    (, ,)
    (VP
      (VP (VB have)
        (NP (PRP it)))
      (CC and)
      (VP (VB put)
        (NP (PRP it))
        (PRT (RP up))
        (PP (IN for)
          (NP (NN adoption)))))
    (. .)))

mark(want-5, If-1)
nsubj(want-5, you-2)
aux(want-5, do-3)
neg(want-5, not-4)
advcl(have-9, want-5)
det(child-7, a-6)
dobj(want-5, child-7)
root(ROOT-0, have-9)
dobj(have-9, it-10)
conj_and(have-9, put-12)
dobj(put-12, it-13)
prt(put-12, up-14)
prep_for(put-12, adoption-16)

