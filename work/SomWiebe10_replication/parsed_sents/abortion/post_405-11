(ROOT
  (S
    (PP (IN In)
      (NP (NN fact)))
    (, ,)
    (NP
      (NP (PRP$ your) (NN argument))
      (PP (IN about)
        (NP (JJ medical) (NN technology))))
    (VP
      (VP
        (ADVP (RB only))
        (VBZ STRENGTHENS)
        (NP (PRP$ my) (NN argument)))
      (CC and)
      (VP (VBZ weakens)
        (NP (PRP$ your) (JJ own))))
    (. .)))

prep_in(STRENGTHENS-10, fact-2)
poss(argument-5, your-4)
nsubj(STRENGTHENS-10, argument-5)
nsubj(weakens-14, argument-5)
amod(technology-8, medical-7)
prep_about(argument-5, technology-8)
advmod(STRENGTHENS-10, only-9)
root(ROOT-0, STRENGTHENS-10)
poss(argument-12, my-11)
dobj(STRENGTHENS-10, argument-12)
conj_and(STRENGTHENS-10, weakens-14)
poss(own-16, your-15)
dobj(weakens-14, own-16)

