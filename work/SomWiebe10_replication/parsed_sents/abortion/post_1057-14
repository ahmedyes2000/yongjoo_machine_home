(ROOT
  (SBAR
    (WHADJP (WRB How) (JJ many))
    (S
      (NP (NNS doctors)
        (CC and)
        (NN scientist))
      (VP (VBD were)
        (VP (VBN exterminated))))))

advmod(many-2, How-1)
dep(exterminated-7, many-2)
nsubjpass(exterminated-7, doctors-3)
conj_and(doctors-3, scientist-5)
nsubjpass(exterminated-7, scientist-5)
auxpass(exterminated-7, were-6)
root(ROOT-0, exterminated-7)

