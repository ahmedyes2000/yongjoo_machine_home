(ROOT
  (S
    (NP (PRP I))
    (VP (VBP think)
      (SBAR (IN that)
        (S
          (VP (VBZ depends)
            (PRT (RP upon))
            (SBAR (IN whether)
              (S
                (NP (PRP it))
                (VP (VBD had)
                  (NP (DT a) (NN soul)))))))))
    (. .)))

nsubj(think-2, I-1)
root(ROOT-0, think-2)
complm(depends-4, that-3)
ccomp(think-2, depends-4)
prt(depends-4, upon-5)
complm(had-8, whether-6)
nsubj(had-8, it-7)
ccomp(depends-4, had-8)
det(soul-10, a-9)
dobj(had-8, soul-10)

