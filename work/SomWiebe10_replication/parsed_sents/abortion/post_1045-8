(ROOT
  (S
    (ADVP (RB Also))
    (, ,)
    (NP (PRP I))
    (VP (MD 'd)
      (VP (VB like)
        (S
          (VP (TO to)
            (VP (VB say)
              (SBAR (IN that)
                (S
                  (NP (PRP I))
                  (VP (VBP 'm) (RB not)
                    (ADJP (JJ sure)
                      (PP (IN about)
                        (NP (PRP$ my) (NN opinion))))
                    (PP (IN on)
                      (NP (NN abortion)))))))))))
    (: ...)))

advmod(like-5, Also-1)
nsubj(like-5, I-3)
xsubj(say-7, I-3)
aux(like-5, 'd-4)
root(ROOT-0, like-5)
aux(say-7, to-6)
xcomp(like-5, say-7)
complm(sure-12, that-8)
nsubj(sure-12, I-9)
cop(sure-12, 'm-10)
neg(sure-12, not-11)
ccomp(say-7, sure-12)
poss(opinion-15, my-14)
prep_about(sure-12, opinion-15)
prep_on(sure-12, abortion-17)

