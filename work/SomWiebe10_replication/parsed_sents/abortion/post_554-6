(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ 's)
      (NP
        (NP (DT a) (JJ simple) (NN concept))
        (: :)
        (S
          (SBAR (IN If)
            (S
              (NP (PRP you))
              (VP (VBP are)
                (PP (IN against)
                  (NP (NN abortion)))
                (, ,)
                (S
                  (ADJP (JJ fine))))))
          (, ,)
          (VP (VB do) (RB n't)
            (VP (VB have)
              (NP (CD one)))))))
    (. .)))

nsubj(concept-5, It-1)
cop(concept-5, 's-2)
det(concept-5, a-3)
amod(concept-5, simple-4)
root(ROOT-0, concept-5)
mark(are-9, If-7)
nsubj(are-9, you-8)
advcl(have-17, are-9)
prep_against(are-9, abortion-11)
dep(are-9, fine-13)
aux(have-17, do-15)
neg(have-17, n't-16)
dep(concept-5, have-17)
dobj(have-17, one-18)

