(ROOT
  (S
    (ADVP (RB Primarily))
    (NP
      (NP (PRP$ your) (NN statement))
      (NP (DT that) (JJ Christian) (NN morality)))
    (VP (VBZ applies)
      (ADVP (RB solely))
      (PP (TO to)
        (NP (NNS Christians))))
    (. .)))

advmod(applies-7, Primarily-1)
poss(statement-3, your-2)
nsubj(applies-7, statement-3)
det(morality-6, that-4)
amod(morality-6, Christian-5)
dep(statement-3, morality-6)
root(ROOT-0, applies-7)
advmod(applies-7, solely-8)
prep_to(applies-7, Christians-10)

