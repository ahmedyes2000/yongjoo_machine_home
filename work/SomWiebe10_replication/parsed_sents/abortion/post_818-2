(ROOT
  (S
    (NP (EX There))
    (VP (VBZ is)
      (ADVP (RB also))
      (NP
        (NP (DT a) (NN risk))
        (PP (IN of)
          (NP (NN hemorrhaging)))))
    (. .)))

expl(is-2, There-1)
root(ROOT-0, is-2)
advmod(is-2, also-3)
det(risk-5, a-4)
nsubj(is-2, risk-5)
prep_of(risk-5, hemorrhaging-7)

