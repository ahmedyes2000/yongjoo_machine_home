(ROOT
  (S
    (ADVP (RB Obviously))
    (NP (DT the) (JJ same))
    (VP (MD must)
      (VP (VB apply)
        (PP (IN in)
          (NP
            (NP (DT the) (NN case))
            (PP (IN of)
              (NP (JJ willful) (NN abortion)))))
        (SBAR (IN unless)
          (S
            (NP (EX there))
            (VP (VBP are)
              (NP (JJ medical) (NNS problems)))))))
    (. .)))

advmod(apply-5, Obviously-1)
det(same-3, the-2)
nsubj(apply-5, same-3)
aux(apply-5, must-4)
root(ROOT-0, apply-5)
det(case-8, the-7)
prep_in(apply-5, case-8)
