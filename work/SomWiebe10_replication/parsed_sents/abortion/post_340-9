(ROOT
  (S
    (SBAR (IN That)
      (S
        (NP (EX there))
        (VP (VBZ is)
          (NP
            (NP (DT a) (NN life))
            (VP (VBN involved)
              (PP (IN in)
                (NP (DT an) (NN abortion))))))))
    (VP (VBZ does) (RB n't)
      (VP (VB resolve)
        (NP (DT the) (NN fact))
        (SBAR (IN that)
          (S
            (NP (DT this))
            (VP (VBZ is)
              (NP
                (NP (DT a) (NN person) (POS 's))
                (NN body))
              (SBAR (IN that)
                (S
                  (NP (PRP you))
                  (VP (VBP are)
                    (VP (VBG allowing)
                      (NP (NN government) (NN discretion))
                      (PRT (RP over)))))))))))
    (. .)))

