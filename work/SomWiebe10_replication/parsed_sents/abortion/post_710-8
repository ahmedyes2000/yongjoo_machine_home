(ROOT
  (S
    (S
      (NP (NNP Life))
      (VP (VBZ is)
        (ADJP (JJ worth))
        (S
          (VP (VBG protecting)
            (PP (IN at)
              (NP (DT every) (NN point)))))))
    (, ,)
    (CC but)
    (FRAG (RB not)
      (ADJP (RB equally) (RB so)))
    (. .)))

nsubj(worth-3, Life-1)
cop(worth-3, is-2)
root(ROOT-0, worth-3)
xcomp(worth-3, protecting-4)
det(point-7, every-6)
prep_at(protecting-4, point-7)
conj_but(worth-3, not-10)
advmod(so-12, equally-11)
dep(not-10, so-12)

