(ROOT
  (S
    (NP (PRP I))
    (ADVP (RB just))
    (VP (VBP want)
      (S
        (VP (TO to)
          (VP (VB say)
            (SBAR
              (S
                (NP (PRP I))
                (VP (VBP am)
                  (NP
                    (NP (JJ pro) (NN life))
                    (PP (IN in)
                      (NP (DT these) (NNS matters)))))))))))
    (. .)))

nsubj(want-3, I-1)
xsubj(say-5, I-1)
advmod(want-3, just-2)
root(ROOT-0, want-3)
aux(say-5, to-4)
xcomp(want-3, say-5)
nsubj(life-9, I-6)
cop(life-9, am-7)
amod(life-9, pro-8)
ccomp(say-5, life-9)
det(matters-12, these-11)
prep_in(life-9, matters-12)

