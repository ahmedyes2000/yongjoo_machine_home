(ROOT
  (S
    (NP (JJ Late) (NN term) (NNS abortions))
    (VP (VBP are)
      (ADVP
        (NP (DT the) (NN exception))
        (RB not))
      (NP (DT the) (NN norm)))
    (. .)))

amod(abortions-3, Late-1)
nn(abortions-3, term-2)
nsubj(norm-9, abortions-3)
cop(norm-9, are-4)
det(exception-6, the-5)
npadvmod(not-7, exception-6)
neg(norm-9, not-7)
det(norm-9, the-8)
root(ROOT-0, norm-9)

