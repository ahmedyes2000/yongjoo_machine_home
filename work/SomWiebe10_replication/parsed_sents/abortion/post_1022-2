(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP
          (NP (DT a) (NN mother))
          (CC or)
          (NP (DT a) (NN couple)))
        (VP (VBZ does) (RB not)
          (VP (VB want)
            (NP (DT a) (NN child))))))
    (NP (EX there))
    (VP (VBZ is)
      (ADVP (RB always))
      (NP
        (NP (DT the) (NN option))
        (PP (IN of)
          (S
            (VP (VBG putting)
              (NP (DT the) (NN child))
              (PRT (RP up))
              (PP (IN for)
                (NP (NN adoption))))))
        (, ,)
        (ADVP (IN at) (JJS least))
        (SBAR
          (S
            (NP (PRP it))
            (VP (MD will)
              (VP (VB have)
                (NP
                  (NP (DT a) (NN chance))
                  (PP (IN at)
                    (NP
                      (NP (NN life))
                      (CC and)
                      (NP (DT a) (VBG loveing) (NN family)))))))))))
    (. .)))

mark(want-9, If-1)
det(mother-3, a-2)
nsubj(want-9, mother-3)
det(couple-6, a-5)
conj_or(mother-3, couple-6)
nsubj(want-9, couple-6)
aux(want-9, does-7)
neg(want-9, not-8)
advcl(is-13, want-9)
det(child-11, a-10)
dobj(want-9, child-11)
expl(is-13, there-12)
root(ROOT-0, is-13)
advmod(is-13, always-14)
det(option-16, the-15)
nsubj(is-13, option-16)
prepc_of(option-16, putting-18)
det(child-20, the-19)
dobj(putting-18, child-20)
prt(putting-18, up-21)
prep_for(putting-18, adoption-23)
advmod(option-16, at-25)
pobj(at-25, least-26)
nsubj(have-29, it-27)
aux(have-29, will-28)
rcmod(option-16, have-29)
det(chance-31, a-30)
