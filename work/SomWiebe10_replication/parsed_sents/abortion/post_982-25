(ROOT
  (S
    (NP (PRP I))
    (ADVP (RB also))
    (VP (VBD put)
      (PP (IN in) (IN in)
        (NP
          (NP (JJ bold))
          (PP (IN in)
            (NP (NN response)))))
      (PP (TO to)
        (NP (PRP$ your) (JJ other) (NN statement))))
    (. .)))

nsubj(put-3, I-1)
advmod(put-3, also-2)
root(ROOT-0, put-3)
dep(put-3, in-4)
prep_in(put-3, bold-6)
prep_in(bold-6, response-8)
poss(statement-12, your-10)
amod(statement-12, other-11)
prep_to(put-3, statement-12)

