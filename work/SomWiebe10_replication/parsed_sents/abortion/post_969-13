(ROOT
  (S
    (S
      (NP (DT Those))
      (VP (VBP are)
        (NP
          (NP (PDT all) (DT the) (NNS things))
          (SBAR
            (WHNP (WDT that))
            (S
              (VP (MD should)
                (VP (VB be)
                  (VP (VBN thought)
                    (PP (IN about))))))))))
    (, ,) (IN so)
    (S
      (NP (PRP I))
      (VP (MD will)
        (VP (VB be)
          (VP (VBG explaining)
            (NP (PRP them))
            (, ,)
            (S
              (VP (TO to)
                (VP (VB help)
                  (S
                    (NP (PRP you))
                    (VP (VB understand)
                      (NP (PRP them)))))))))))
    (. .)))

nsubj(things-5, Those-1)
cop(things-5, are-2)
predet(things-5, all-3)
det(things-5, the-4)
root(ROOT-0, things-5)
nsubjpass(thought-9, things-5)
aux(thought-9, should-7)
auxpass(thought-9, be-8)
rcmod(things-5, thought-9)
prep(thought-9, about-10)
dep(things-5, so-12)
nsubj(explaining-16, I-13)
aux(explaining-16, will-14)
aux(explaining-16, be-15)
ccomp(things-5, explaining-16)
dobj(explaining-16, them-17)
aux(help-20, to-19)
xcomp(explaining-16, help-20)
nsubj(understand-22, you-21)
ccomp(help-20, understand-22)
dobj(understand-22, them-23)

