(ROOT
  (S
    (PP (RB Rather) (IN than)
      (S
        (VP (VBG banning)
          (NP (PRP it)))))
    (, ,)
    (NP (PRP we))
    (VP (VBP need)
      (S
        (VP (TO to)
          (VP (VB be)
            (VP (VBG working)
              (PP (IN towards)
                (S
                  (VP (VBG making)
                    (NP (NN abortion))
                    (ADVP (RB pretty) (RB much))
                    (ADVP (RB unneccesary))))))))))
    (. .)))

advmod(need-7, Rather-1)
advcl(need-7, than-2)
prepc_than(need-7, banning-3)
dobj(banning-3, it-4)
nsubj(need-7, we-6)
xsubj(working-10, we-6)
root(ROOT-0, need-7)
aux(working-10, to-8)
aux(working-10, be-9)
xcomp(need-7, working-10)
prepc_towards(working-10, making-12)
dobj(making-12, abortion-13)
advmod(much-15, pretty-14)
advmod(making-12, much-15)
advmod(making-12, unneccesary-16)

