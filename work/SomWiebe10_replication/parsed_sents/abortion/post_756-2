(ROOT
  (S
    (NP (PRP I))
    (VP (VBP am)
      (NP
        (NP (JJ pro) (NN abstinence))
        (CC and)
        (NP (JJ pro) (NN life))))
    (. .)))

nsubj(abstinence-4, I-1)
cop(abstinence-4, am-2)
amod(abstinence-4, pro-3)
root(ROOT-0, abstinence-4)
amod(life-7, pro-6)
conj_and(abstinence-4, life-7)

