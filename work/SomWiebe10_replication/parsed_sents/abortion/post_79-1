(ROOT
  (S
    (ADVP (RB Yet))
    (, ,)
    (NP (PRP it))
    (VP (VBZ is)
      (ADJP (JJ easy)
        (S
          (VP (TO to)
            (VP (VB demonstrate)
              (SBAR (IN that)
                (S
                  (NP (DT this))
                  (VP (VBZ is) (RB not)
                    (NP (DT the) (NN case))))))))))
    (. .)))

advmod(easy-5, Yet-1)
nsubj(easy-5, it-3)
cop(easy-5, is-4)
root(ROOT-0, easy-5)
aux(demonstrate-7, to-6)
xcomp(easy-5, demonstrate-7)
complm(case-13, that-8)
nsubj(case-13, this-9)
cop(case-13, is-10)
neg(case-13, not-11)
det(case-13, the-12)
ccomp(demonstrate-7, case-13)

