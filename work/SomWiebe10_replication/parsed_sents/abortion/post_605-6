prep(up-9, until-10)
pcomp(until-10, about-11)

(ROOT
  (S
    (NP
      (NP (NN century))
      (ADVP (IN ago)))
    (, ,)
    (VP (VBD was)
      (NP
        (NP (DT the) (VBG leading) (NN cause))
        (PP (IN of)
          (NP
            (NP (NN mortality))
            (PP (IN among)
              (NP (NNS women)))))))
    (. .)))

nsubj(cause-7, century-1)
advmod(century-1, ago-2)
cop(cause-7, was-4)
