(ROOT
  (S
    (NP (NNP Pregnancy))
    (VP (VBZ is)
      (NP
        (NP (DT a) (NN body)
          (CC and)
          (NN life-changing) (NN event))
        (SBAR
          (WHNP (WDT which))))
      (ADVP (RB up)
        (PP (IN until)
          (ADVP (RB about)))))
    (. ?)))

nsubj(event-7, Pregnancy-1)
cop(event-7, is-2)
det(event-7, a-3)
nn(event-7, body-4)
