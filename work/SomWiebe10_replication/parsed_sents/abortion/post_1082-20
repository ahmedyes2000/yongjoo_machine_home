(ROOT
  (S
    (NP (PRP It))
    (VP
      (VP (VBD took)
        (NP
          (NP (DT a) (JJ tremendous) (NN amount))
          (PP (IN of)
            (NP (NN propaganda)))))
      (, ,)
      (VP (VBZ lies))
      (, ,)
      (CC and)
      (VP (VBD deceit)))
    (. .)))

nsubj(took-2, It-1)
nsubj(lies-9, It-1)
nsubj(deceit-12, It-1)
root(ROOT-0, took-2)
det(amount-5, a-3)
amod(amount-5, tremendous-4)
