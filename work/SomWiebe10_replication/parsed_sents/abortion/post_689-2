(ROOT
  (S
    (NP (PRP He))
    (VP (VBZ needs)
      (S
        (VP (TO to)
          (VP (CC either)
            (VP (VB deal)
              (ADVP (RB completely))
              (PP (IN with)
                (NP (DT the) (JJ legal) (NNS issues))))
            (, ,)
            (CC or)
            (ADVP (RB actually))
            (VP (VB tackle)
              (NP (DT the) (JJ philosophical) (NNS issues)))))))
    (. .)))

nsubj(needs-2, He-1)
xsubj(deal-5, He-1)
root(ROOT-0, needs-2)
aux(deal-5, to-3)
preconj(deal-5, either-4)
xcomp(needs-2, deal-5)
advmod(deal-5, completely-6)
det(issues-10, the-8)
amod(issues-10, legal-9)
