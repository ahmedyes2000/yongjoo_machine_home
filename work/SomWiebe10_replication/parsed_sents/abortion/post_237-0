(ROOT
  (SINV
    (VP
      (VP (VBN Quote)
        (PP (IN from)
          (NP
            (NP (JJ previous) (NNS commenters))
            (: :))))
      ('' '') (: ...)
      (PP (IN in)
        (NP (CD 2002))))
    (VP
      (ADVP (RB alone))
      (NP (RB there))
      (VP (VBD were)))
    (NP
      (NP (CD 357) (JJ maternal) (NNS deaths))
      (PP (IN in)
        (NP (DT the) (NNP USA))))
    (. .)))

root(ROOT-0, Quote-1)
amod(commenters-4, previous-3)
prep_from(Quote-1, commenters-4)
prep_in(Quote-1, 2002-9)
advmod(were-12, alone-10)
dobj(were-12, there-11)
dep(Quote-1, were-12)
num(deaths-15, 357-13)
amod(deaths-15, maternal-14)
nsubj(Quote-1, deaths-15)
det(USA-18, the-17)
prep_in(deaths-15, USA-18)

