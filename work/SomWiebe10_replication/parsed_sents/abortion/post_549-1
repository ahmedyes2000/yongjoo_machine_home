(ROOT
  (S
    (NP (DT That))
    (VP (VBZ is)
      (SBAR
        (WHADVP (WRB why))
        (S
          (NP (PRP we))
          (VP (VBP have)
            (NP (DT a) (NN court) (NN system))
            (, ,)
            (S
              (VP (TO to)
                (VP (VB uphold)
                  (NP (DT the) (NN constitution))
                  (ADVP (RB even))
                  (SBAR
                    (WHADVP (WRB when))
                    (S
                      (NP (DT the) (NN majority))
                      (VP (MD would)
                        (ADVP (RB rather) (RB not))))))))))))
    (. .)))

nsubj(is-2, That-1)
root(ROOT-0, is-2)
advmod(have-5, why-3)
nsubj(have-5, we-4)
xsubj(uphold-11, we-4)
advcl(is-2, have-5)
det(system-8, a-6)
nn(system-8, court-7)
dobj(have-5, system-8)
aux(uphold-11, to-10)
xcomp(have-5, uphold-11)
det(constitution-13, the-12)
dobj(uphold-11, constitution-13)
advmod(uphold-11, even-14)
advmod(would-18, when-15)
det(majority-17, the-16)
nsubj(would-18, majority-17)
advcl(uphold-11, would-18)
advmod(not-20, rather-19)
neg(would-18, not-20)

