(ROOT
  (S
    (ADVP (RB However))
    (, ,)
    (NP (NNS people))
    (VP (VBP need)
      (S
        (VP (TO to)
          (VP (VB give)
            (PRT (RP back))
            (PP (IN as)
              (S
                (NP (PRP they))
                (VP (MD can))))
            (PRN (-LRB- -LRB-)
              (FRAG (RB not)
                (NP (DT a) (NN welfare) (NN world)))
              (-RRB- -RRB-))))))
    (. .)))

advmod(need-4, However-1)
nsubj(need-4, people-3)
xsubj(give-6, people-3)
root(ROOT-0, need-4)
aux(give-6, to-5)
xcomp(need-4, give-6)
prt(give-6, back-7)
nsubj(can-10, they-9)
prepc_as(give-6, can-10)
dep(give-6, not-12)
det(world-15, a-13)
nn(world-15, welfare-14)
dep(not-12, world-15)

