(ROOT
  (S
    (SBAR
      (ADVP (RB Certainly))
      (WHADVP (WRB when))
      (S
        (NP
          (NP (DT the) (NN health))
          (PP (IN of)
            (NP (DT the) (NN mother))))
        (VP (VBZ is)
          (PP (IN at)
            (NP (NN risk))))))
    (VP (VBZ is)
      (NP
        (NP (CD one))
        (PP (IN of)
          (NP (PRP them)))))
    (. .)))

advmod(is-8, Certainly-1)
advmod(is-8, when-2)
det(health-4, the-3)
nsubj(is-8, health-4)
det(mother-7, the-6)
prep_of(health-4, mother-7)
csubj(one-12, is-8)
prep_at(is-8, risk-10)
cop(one-12, is-11)
root(ROOT-0, one-12)
prep_of(one-12, them-14)

