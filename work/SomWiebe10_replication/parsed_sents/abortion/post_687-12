  (SBARQ
    (WHADJP (WRB How)
      (ADJP (JJ extensive)))
    (SQ (MD can)
      (NP (DT these) (NNS rights))
      (VP (VB be)
        (SBAR
          (WHADVP (WRB when))
          (S
            (NP (PRP they))
            (VP (VBP conflict)
              (PP (IN with)
                (NP
                  (NP (DT the) (NNS wishes))
                  (PP (IN of)
                    (NP (DT the) (NN mother))))))))))
    (. ?)))

advmod(extensive-2, How-1)
dep(be-6, extensive-2)
aux(be-6, can-3)
det(rights-5, these-4)
