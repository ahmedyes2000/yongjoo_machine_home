(ROOT
  (FRAG
    (SBAR
      (WHADVP (WRB When))
      (S
        (NP (PRP we))
        (VP (VBP call)
          (ADJP (JJ evil))
          (, ,)
          (S
            (ADJP (JJ good))))))
    (. .)))

advmod(call-3, When-1)
nsubj(call-3, we-2)
root(ROOT-0, call-3)
acomp(call-3, evil-4)
dep(call-3, good-6)

