(ROOT
  (S
    (PP (IN For)
      (NP (NN example)))
    (, ,)
    (S
      (ADVP (RB suppose))
      (NP (PRP we))
      (VP (VBP are)
        (PP (IN in)
          (NP (DT the) (JJ same) (NN room)))))
    (CC and)
    (S
      (PP (IN in)
        (NP (NN silence)))
      (NP (PRP I))
      (VP (VBP point)
        (PP (TO to)
          (NP (DT a) (NN chair)))))
    (. .)))

prep_for(are-6, example-2)
advmod(are-6, suppose-4)
nsubj(are-6, we-5)
root(ROOT-0, are-6)
det(room-10, the-8)
amod(room-10, same-9)
prep_in(are-6, room-10)
prep_in(point-15, silence-13)
nsubj(point-15, I-14)
conj_and(are-6, point-15)
det(chair-18, a-17)
prep_to(point-15, chair-18)

