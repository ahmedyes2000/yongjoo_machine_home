(ROOT
  (S
    (NP (DT The) (JJ same))
    (VP (VBZ goes)
      (SBAR (IN if)
        (S
          (NP (FW i))
          (VP (VBD poked)
            (NP (NN someone))
            (ADVP (RB lightly))))))
    (. .)))

det(same-2, The-1)
nsubj(goes-3, same-2)
root(ROOT-0, goes-3)
mark(poked-6, if-4)
nsubj(poked-6, i-5)
advcl(goes-3, poked-6)
dobj(poked-6, someone-7)
advmod(poked-6, lightly-8)

