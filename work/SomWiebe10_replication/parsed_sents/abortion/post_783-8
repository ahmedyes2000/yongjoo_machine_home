(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (DT this) (NN family))
        (VP (VBD was) (RB not)
          (ADJP (RB financially) (JJ able)
            (S
              (VP (TO to)
                (VP (VB raise)
                  (NP (DT a) (NN child)))))))))
    (, ,)
    (NP (DT the) (NN public))
    (VP (VBZ ends)
      (PRT (RP up))
      (S
        (VP (VBG paying)
          (PP (IN for)
            (NP (PRP them)))
          (PP (IN through)
            (NP
              (NP (NN welfare) (NNS programs))
              (, ,)
              (NP (FW etc.) (. .)))))))
    (. .)))

