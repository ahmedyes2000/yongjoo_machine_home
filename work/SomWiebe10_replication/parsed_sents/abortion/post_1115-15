(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP (MD 'd)
          (VP (VB like)
            (S
              (VP (TO to)
                (VP (VB debate)
                  (NP (DT some) (JJ other) (NN topic)))))))))
    (, ,)
    (VP (VB let)
      (S
        (NP (POS 's))
        (VP (VB start)
          (NP (DT a) (JJ new) (NN debate)))))
    (. .)))

