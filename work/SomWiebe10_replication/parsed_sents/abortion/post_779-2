(ROOT
  (S
    (NP (PRP I))
    (VP (MD would)
      (VP (VB have)
        (VP (VBN hanged)
          (NP (PRP myself))
          (NP (DT a) (JJ long) (NN time) (RB ago)))))
    (. .)))

nsubj(hanged-4, I-1)
aux(hanged-4, would-2)
aux(hanged-4, have-3)
root(ROOT-0, hanged-4)
iobj(hanged-4, myself-5)
det(time-8, a-6)
amod(time-8, long-7)
dobj(hanged-4, time-8)
advmod(time-8, ago-9)

