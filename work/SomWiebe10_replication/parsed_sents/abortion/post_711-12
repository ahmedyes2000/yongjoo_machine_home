(ROOT
  (S
    (NP (DT No) (NN one))
    (VP (VBZ thinks)
      (SBAR
        (S
          (NP (NNS others))
          (VP (VBP have)
            (NP (DT the) (NN right)
              (S
                (VP (TO to)
                  (VP (VB murder)
                    (NP (PRP them))))))))))
    (. .)))

det(one-2, No-1)
nsubj(thinks-3, one-2)
root(ROOT-0, thinks-3)
nsubj(have-5, others-4)
ccomp(thinks-3, have-5)
det(right-7, the-6)
dobj(have-5, right-7)
aux(murder-9, to-8)
infmod(right-7, murder-9)
dobj(murder-9, them-10)

