  (S
    (S
      (NP (EX There))
      (VP (VBP are)
        (NP
          (NP (JJ many) (NNS circumstances))
          (SBAR
            (WHPP (IN in)
              (WHNP (WDT which)))
            (S
              (NP (JJ late-term) (NNS abortions))
              (VP (VBP are)
                (ADJP (RB quite) (JJ necessary))))))))
    (, ,)
    (CC but)
    (S
      (NP (PRP I))
      (VP (MD 'll)
        (ADVP (RB just))
        (VP (VB name)
          (NP (DT a) (JJ few) (JJ common) (NNS situations)))))
    (. .)))

