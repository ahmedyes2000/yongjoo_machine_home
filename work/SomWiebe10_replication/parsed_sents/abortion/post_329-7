(ROOT
  (S
    (ADVP (RB Too)
      (NP
        (QP (JJ many) (NNS times))))
    (NP (DT the) (NN father))
    (VP (VBD had)
      (S
        (VP (TO to)
          (VP (VB put)
            (NP (PRP$ his) (NNS children))
            (PP (IN into)
              (NP (NNS orphanages))))))
      (SBAR (IN because)
        (S
          (NP (PRP he))
          (VP (MD could) (RB n't)
            (VP (VB work)
              (CC and)
              (VB take)
              (NP
                (NP (NN care))
                (PP (IN of)
                  (NP (PRP them)))))))))
    (. .)))

advmod(had-6, Too-1)
quantmod(times-3, many-2)
pobj(Too-1, times-3)
det(father-5, the-4)
nsubj(had-6, father-5)
xsubj(put-8, father-5)
root(ROOT-0, had-6)
aux(put-8, to-7)
xcomp(had-6, put-8)
poss(children-10, his-9)
dobj(put-8, children-10)
prep_into(put-8, orphanages-12)
mark(work-17, because-13)
nsubj(work-17, he-14)
nsubj(take-19, he-14)
aux(work-17, could-15)
neg(work-17, n't-16)
advcl(had-6, work-17)
advcl(had-6, take-19)
conj_and(work-17, take-19)
dobj(work-17, care-20)
prep_of(care-20, them-22)

