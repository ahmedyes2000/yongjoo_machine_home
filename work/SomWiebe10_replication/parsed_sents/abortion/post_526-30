(ROOT
  (S
    (NP (PRP I))
    (ADVP (RB never))
    (VP
      (VP (VBD said)
        (WHNP (WDT that)))
      (, ,)
      (ADVP
        (ADVP (RB not) (RB once))
        (, ,)
        (CC or)
        (ADVP (RB even)))
      (VP (VBD implied)
        (WHNP (WDT that))))
    (. .)))

nsubj(said-3, I-1)
neg(said-3, never-2)
root(ROOT-0, said-3)
dobj(said-3, that-4)
dep(once-7, not-6)
advmod(said-3, once-7)
advmod(said-3, even-10)
conj_or(once-7, even-10)
dep(said-3, implied-11)
dobj(implied-11, that-12)

