(ROOT
  (S
    (NP (DT This))
    (VP (MD would)
      (VP (VB make)
        (NP (NN abortion) (JJ legal))
        (ADVP (RB only))
        (PP (IN before)
          (NP (DT the) (JJ 3rd) (NN month)))))
    (. .)))

nsubj(make-3, This-1)
aux(make-3, would-2)
root(ROOT-0, make-3)
dobj(make-3, abortion-4)
amod(abortion-4, legal-5)
advmod(make-3, only-6)
det(month-10, the-8)
amod(month-10, 3rd-9)
prep_before(make-3, month-10)

