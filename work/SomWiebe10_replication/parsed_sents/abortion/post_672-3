  (S (CC But)
    (S
      (ADVP (RB then))
      (NP (PRP you))
      (VP (VBP run)
        (PP (IN into)
          (NP (DT the) (JJ same) (NN issue)))))
    (CC and)
    (S
      (NP (PRP it))
      (VP (VBZ regresses)
        (ADVP (RB infinitely))))
    (. .)))

advmod(run-4, then-2)
nsubj(run-4, you-3)
root(ROOT-0, run-4)
