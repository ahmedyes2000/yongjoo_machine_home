(ROOT
  (S
    (ADVP (RB Scientifically))
    (ADVP (RB though))
    (, ,)
    (NP (PRP it))
    (VP (VBZ 's)
      (ADJP (RBR less) (JJ clear)))
    (. .)))

advmod(clear-7, Scientifically-1)
advmod(clear-7, though-2)
nsubj(clear-7, it-4)
cop(clear-7, 's-5)
advmod(clear-7, less-6)
root(ROOT-0, clear-7)

