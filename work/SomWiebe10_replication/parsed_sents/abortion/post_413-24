(ROOT
  (S
    (NP (DT Some))
    (VP (VBP have)
      (ADVP (RB even))
      (VP (VBD said)
        (SBAR
          (S
            (NP (PRP it))
            (VP (VBZ begins)
              (PP (IN at)
                (NP (NN toddlerhood))))))))
    (. .)))

nsubj(said-4, Some-1)
aux(said-4, have-2)
advmod(said-4, even-3)
root(ROOT-0, said-4)
nsubj(begins-6, it-5)
