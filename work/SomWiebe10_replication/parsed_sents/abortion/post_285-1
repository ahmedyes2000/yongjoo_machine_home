(ROOT
  (S
    (INTJ (WRB Why))
    (VP
      (VP (VB be)
        (ADJP (JJ selfish)))
      (CC and)
      (VP (VB take)
        (NP (DT a) (NN life))
        (SBAR (RB just) (IN so)
          (S
            (NP (PRP you))
            (VP (MD can)
              (VP (VB live)
                (S
                  (NP (PRP$ yours))
                  (ADJP (RB all) (JJ happy)
                    (PP (IN after))))))))))
    (. .)))

dep(selfish-3, Why-1)
cop(selfish-3, be-2)
root(ROOT-0, selfish-3)
conj_and(selfish-3, take-5)
det(life-7, a-6)
dobj(take-5, life-7)
advmod(live-12, just-8)
dep(live-12, so-9)
nsubj(live-12, you-10)
aux(live-12, can-11)
dep(take-5, live-12)
nsubj(happy-15, yours-13)
advmod(happy-15, all-14)
xcomp(live-12, happy-15)
prep(happy-15, after-16)

