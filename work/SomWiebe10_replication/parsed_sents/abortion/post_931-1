(ROOT
  (S
    (NP (DT The) (NN question))
    (VP (VBZ is)
      (SQ (MD should)
        (NP (NN abortion))
        (VP (VB be)
          (VP (VBN banned)))))
    (. .)))

det(question-2, The-1)
nsubj(is-3, question-2)
root(ROOT-0, is-3)
aux(banned-7, should-4)
nsubjpass(banned-7, abortion-5)
auxpass(banned-7, be-6)
dep(is-3, banned-7)

