(ROOT
  (FRAG
    (PP (RB Along) (IN with)
      (NP
        (NP (DT the) (NN health))
        (PP (IN of)
          (NP
            (NP (DT the) (NN mother))
            (PP (IN at)
              (NP (NN risk)))))))
    (. .)))

advmod(with-2, Along-1)
root(ROOT-0, with-2)
det(health-4, the-3)
pobj(with-2, health-4)
det(mother-7, the-6)
prep_of(health-4, mother-7)
prep_at(mother-7, risk-9)

