(ROOT
  (S
    (NP (PRP I))
    (ADJP
      (ADVP (RB never))
      (PP
        (ADVP (RB though))
        (IN about)
        (NP (PRP it))))
    (PP (IN as)
      (NP
        (NP (NN killing) (NNS millions))
        (PP (IN of)
          (NP (JJ little) (NNS lives)))))
    (: ...)))

dep(never-2, I-1)
root(ROOT-0, never-2)
advmod(never-2, though-3)
prep_about(never-2, it-5)
dep(never-2, as-6)
nn(millions-8, killing-7)
pobj(as-6, millions-8)
amod(lives-11, little-10)
prep_of(millions-8, lives-11)

(ROOT
  (FRAG
    (ADJP
      (WHADVP (WRB How))
      (JJ depressing))
    (. .)))

advmod(depressing-2, How-1)
root(ROOT-0, depressing-2)

