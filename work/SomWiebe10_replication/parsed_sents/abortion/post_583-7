(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is)
      (ADVP (RB sometimes))
      (NP
        (NP (DT the) (JJR lesser))
        (PP (IN of)
          (NP (CD two) (NNS evils)))))
    (. .)))

nsubj(lesser-5, It-1)
cop(lesser-5, is-2)
advmod(lesser-5, sometimes-3)
det(lesser-5, the-4)
root(ROOT-0, lesser-5)
