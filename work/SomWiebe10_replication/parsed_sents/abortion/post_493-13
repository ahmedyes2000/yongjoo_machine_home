(ROOT
  (S
    (NP (CC Neither)
      (NP (NN individual))
      (CC nor)
      (NP (JJ collective) (NNS rights)))
    (VP (VBP extend)
      (ADVP (RB far)))
    (. .)))

preconj(individual-2, Neither-1)
nsubj(extend-6, individual-2)
amod(rights-5, collective-4)
conj_nor(individual-2, rights-5)
nsubj(extend-6, rights-5)
root(ROOT-0, extend-6)
advmod(extend-6, far-7)

