(ROOT
  (S
    (S
      (NP (DT The) (NN conclusion))
      (VP (VBZ is)
        (ADJP (JJ clear))))
    (: :)
    (S
      (NP (EX There))
      (VP (VBZ is)
        (NP
          (NP (DT no) (NN room))
          (PP (IN in)
            (NP
              (NP (JJ Catholic) (NN theology))
              (PP (IN for)
                (NP
                  (NP (DT the) (JJ obvious) (NN disregard))
                  (PP (IN for)
                    (NP (JJ human) (NN life))))))))))
    (. .)))

det(conclusion-2, The-1)
nsubj(clear-4, conclusion-2)
cop(clear-4, is-3)
root(ROOT-0, clear-4)
expl(is-7, There-6)
parataxis(clear-4, is-7)
det(room-9, no-8)
nsubj(is-7, room-9)
amod(theology-12, Catholic-11)
prep_in(room-9, theology-12)
det(disregard-16, the-14)
amod(disregard-16, obvious-15)
prep_for(theology-12, disregard-16)
amod(life-19, human-18)
prep_for(disregard-16, life-19)

