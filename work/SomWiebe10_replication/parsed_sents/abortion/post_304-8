(ROOT
  (S
    (SBAR
      (WHNP (WP What))
      (S
        (VP (VBZ is)
          (ADJP (JJ fundamental)))))
    (VP (VBZ is)
      (NP
        (NP (DT the) (JJ actual) (NN nature))
        (PP (IN of)
          (NP
            (NP (DT the) (NN entity))
            (VP (VBN involved))))))
    (. .)))

nsubj(fundamental-3, What-1)
cop(fundamental-3, is-2)
csubj(nature-7, fundamental-3)
cop(nature-7, is-4)
det(nature-7, the-5)
amod(nature-7, actual-6)
root(ROOT-0, nature-7)
det(entity-10, the-9)
