(ROOT
  (S
    (NP (DT Some) (NN rape) (NNS victims))
    (VP (VBP are)
      (UCP
        (NP (NN sex) (NNS slaves))
        (CC or)
        (VP (VBN kidnapped)
          (PP (IN against)
            (NP (PRP$ their) (NN will))))))
    (. .)))

det(victims-3, Some-1)
nn(victims-3, rape-2)
nsubj(slaves-6, victims-3)
nsubj(kidnapped-8, victims-3)
cop(slaves-6, are-4)
nn(slaves-6, sex-5)
root(ROOT-0, slaves-6)
conj_or(slaves-6, kidnapped-8)
partmod(slaves-6, kidnapped-8)
poss(will-11, their-10)
prep_against(kidnapped-8, will-11)

