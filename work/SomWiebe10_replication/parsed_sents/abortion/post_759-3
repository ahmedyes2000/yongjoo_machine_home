(ROOT
  (S
    (NP (PRP It))
    (VP (MD would)
      (VP
        (VP (VB burn)
          (PP (IN into)
            (NP (PRP$ my) (JJ very) (NN heart))))
        (CC and)
        (VP (VB leave)
          (NP (DT a) (JJ big) (NN scar)))))
    (. .)))

nsubj(burn-3, It-1)
nsubj(leave-9, It-1)
aux(burn-3, would-2)
root(ROOT-0, burn-3)
poss(heart-7, my-5)
amod(heart-7, very-6)
prep_into(burn-3, heart-7)
conj_and(burn-3, leave-9)
det(scar-12, a-10)
amod(scar-12, big-11)
dobj(leave-9, scar-12)

