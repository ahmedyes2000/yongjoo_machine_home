(ROOT
  (S
    (NP (PRP You))
    (VP (VBP keep)
      (S
        (VP (VBG changing)
          (NP (PRP$ your) (NN criterion))
          (PP (IN for)
            (NP (NN personhood))))))
    (. .)))

nsubj(keep-2, You-1)
root(ROOT-0, keep-2)
xcomp(keep-2, changing-3)
poss(criterion-5, your-4)
dobj(changing-3, criterion-5)
prep_for(changing-3, personhood-7)

