(ROOT
  (S
    (NP (PRP We))
    (VP (VBP feel)
      (ADJP (JJ sorry)
        (PP (IN for)
          (NP
            (NP (DT the) (JJ innocent) (NNS lives))
            (VP (VBN lost))))))
    (. .)))

nsubj(feel-2, We-1)
root(ROOT-0, feel-2)
acomp(feel-2, sorry-3)
det(lives-7, the-5)
amod(lives-7, innocent-6)
prep_for(sorry-3, lives-7)
partmod(lives-7, lost-8)

