(ROOT
  (S (CC But)
    (VP (VB let)
      (S
        (NP (PRP me))
        (VP (VBG preface)
          (NP (PRP$ my) (NN argument))
          (PP (IN with)
            (NP (DT a) (NN disclaimer))))))
    (. .)))

cc(let-2, But-1)
root(ROOT-0, let-2)
nsubj(preface-4, me-3)
dep(let-2, preface-4)
poss(argument-6, my-5)
