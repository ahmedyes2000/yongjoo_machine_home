(ROOT
  (S
    (NP (DT That))
    (VP (VBZ 's)
      (ADVP (RB just))
      (NP
        (NP (NN nature))
        (VP (VBG taking)
          (NP
            (NP (PRP it) (POS 's))
            (NN course)))))
    (. .)))

nsubj(nature-4, That-1)
cop(nature-4, 's-2)
advmod(nature-4, just-3)
root(ROOT-0, nature-4)
partmod(nature-4, taking-5)
poss(course-8, it-6)
dobj(taking-5, course-8)

