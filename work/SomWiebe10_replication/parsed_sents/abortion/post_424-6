(ROOT
  (S
    (NP (DT A) (NN person))
    (VP (MD may)
      (VP (VB be)
        (ADJP (JJ alive)
          (CC or)
          (JJ dead))
        (SBAR (IN as)
          (S
            (NP (DT a) (JJ human) (NN being))
            (VP (MD may)
              (VP (VB be)
                (ADJP (JJ alive)
                  (CC or)
                  (JJ dead))))))))
    (. .)))

det(person-2, A-1)
nsubj(alive-5, person-2)
nsubj(dead-7, person-2)
aux(alive-5, may-3)
cop(alive-5, be-4)
root(ROOT-0, alive-5)
conj_or(alive-5, dead-7)
mark(alive-14, as-8)
det(being-11, a-9)
amod(being-11, human-10)
nsubj(alive-14, being-11)
nsubj(dead-16, being-11)
aux(alive-14, may-12)
cop(alive-14, be-13)
