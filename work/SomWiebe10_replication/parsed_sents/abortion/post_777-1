(ROOT
  (S
    (NP (PRP I))
    (VP (VBP 'm)
      (ADJP (RB just) (JJ glad)
        (SBAR (IN that)
          (S
            (NP (EX there))
            (VP (VBP are)
              (NP
                (NP
                  (QP (RB only) (DT a) (JJ few))
                  (NNS liberals))
                (PP (IN in)
                  (NP (PRP$ my) (NN school)))))))))
    (. .)))

nsubj(glad-4, I-1)
cop(glad-4, 'm-2)
advmod(glad-4, just-3)
root(ROOT-0, glad-4)
complm(are-7, that-5)
expl(are-7, there-6)
ccomp(glad-4, are-7)
quantmod(few-10, only-8)
quantmod(few-10, a-9)
num(liberals-11, few-10)
nsubj(are-7, liberals-11)
poss(school-14, my-13)
prep_in(liberals-11, school-14)

