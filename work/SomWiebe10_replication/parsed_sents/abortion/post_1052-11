(ROOT
  (S
    (NP (PRP They))
    (VP (VBP do) (RB n't)
      (VP (VB need)
        (NP (PRP$ my) (NN judgement))
        (SBAR (IN because)
          (S
            (NP (PRP they))
            (VP (VBP 're)
              (NP (DT the) (NNS ones)
                (S
                  (VP (VBG having)
                    (S
                      (VP (TO to)
                        (VP (VB live)
                          (PP (IN with)
                            (NP (DT that) (NN decision))))))))))))))
    (. .)))

nsubj(need-4, They-1)
aux(need-4, do-2)
neg(need-4, n't-3)
root(ROOT-0, need-4)
poss(judgement-6, my-5)
