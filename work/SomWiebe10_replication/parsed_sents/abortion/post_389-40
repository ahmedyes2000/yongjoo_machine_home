(ROOT
  (S
    (NP (DT The) (VBN born) (NN organism))
    (VP (MD can) (RB not)
      (ADVP (RB immediately))
      (VP (VB perceive)
        (S
          (NP (NN everything))
          (ADJP (JJ available)
            (PP (TO to)
              (NP (PRP him)))))))
    (. .)))

det(organism-3, The-1)
amod(organism-3, born-2)
nsubj(perceive-7, organism-3)
aux(perceive-7, can-4)
neg(perceive-7, not-5)
advmod(perceive-7, immediately-6)
root(ROOT-0, perceive-7)
nsubj(available-9, everything-8)
xcomp(perceive-7, available-9)
prep_to(available-9, him-11)

