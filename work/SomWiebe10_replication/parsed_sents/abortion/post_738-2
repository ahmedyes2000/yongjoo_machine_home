(ROOT
  (S
    (SBAR (IN as)
      (S
        (NP (FW i))
        (VP (VBD said))))
    (NP (PRP they))
    (VP (VBP shouldnt)
      (VP (VB be)
        (VP (VBG having)
          (NP (DT any) (JJ sexual) (NN activity)))))
    (. .)))

mark(said-3, as-1)
nsubj(said-3, i-2)
advcl(shouldnt-5, said-3)
nsubj(shouldnt-5, they-4)
root(ROOT-0, shouldnt-5)
aux(having-7, be-6)
dep(shouldnt-5, having-7)
det(activity-10, any-8)
amod(activity-10, sexual-9)
dobj(having-7, activity-10)

