(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ does)
      (VP (VB condemn)
        (S
          (VP (VBG judging)
            (ADVP (RB hypocritically))
            (ADVP (RB however))))))
    (. .)))

nsubj(condemn-3, It-1)
aux(condemn-3, does-2)
root(ROOT-0, condemn-3)
xcomp(condemn-3, judging-4)
advmod(judging-4, hypocritically-5)
advmod(judging-4, however-6)

