(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP (VB take)
          (PRT (RP away))
          (NP (CD one)))))
    (, ,)
    (NP (EX there))
    (VP (VBZ 's)
      (NP (DT no) (NN reason)
        (S
          (VP (TO to)
            (VP (VB follow)
              (NP
                (NP (DT any))
                (PP (IN of)
                  (NP (PRP them)))))))))
    (. .)))

mark(take-3, If-1)
nsubj(take-3, you-2)
advcl('s-8, take-3)
prt(take-3, away-4)
dobj(take-3, one-5)
expl('s-8, there-7)
root(ROOT-0, 's-8)
det(reason-10, no-9)
nsubj('s-8, reason-10)
aux(follow-12, to-11)
infmod(reason-10, follow-12)
dobj(follow-12, any-13)
prep_of(any-13, them-15)

