(ROOT
  (S
    (NP (NNP Nature))
    (VP (VBZ does) (RB n't)
      (VP (VB work)
        (ADVP (RB that) (RB well))
        (PP (IN for)
          (NP (PRP us)))
        (: ;)
        (SBAR
          (WHADVP (WRB when))
          (S
            (VP (VBN left)
              (PP (TO to)
                (NP (PRP itself))))))))
    (. .)))

nsubj(work-4, Nature-1)
aux(work-4, does-2)
neg(work-4, n't-3)
root(ROOT-0, work-4)
advmod(well-6, that-5)
advmod(work-4, well-6)
prep_for(work-4, us-8)
advmod(left-11, when-10)
advcl(work-4, left-11)
parataxis(work-4, left-11)
prep_to(left-11, itself-13)

