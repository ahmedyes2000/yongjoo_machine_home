(ROOT
  (S
    (NP (PRP They))
    (ADVP (RB still))
    (VP (VBP have) (RB n't)
      (VP (VBN disproved)
        (NP (PRP$ your) (NN argument))
        (ADVP (RB though))))
    (. .)))

nsubj(disproved-5, They-1)
advmod(disproved-5, still-2)
aux(disproved-5, have-3)
neg(disproved-5, n't-4)
root(ROOT-0, disproved-5)
poss(argument-7, your-6)
dobj(disproved-5, argument-7)
advmod(disproved-5, though-8)

