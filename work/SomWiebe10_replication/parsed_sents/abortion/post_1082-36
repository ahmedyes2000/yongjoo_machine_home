(ROOT
  (S
    (NP (PRP We))
    (ADVP (RB really))
    (VP (VBP do)
      (VP (VB have)
        (NP (DT the) (JJ political) (NN power)
          (S
            (VP (TO to)
              (VP (VB solve)
                (NP (DT this) (NN problem))))))))
    (. .)))

nsubj(have-4, We-1)
advmod(have-4, really-2)
aux(have-4, do-3)
root(ROOT-0, have-4)
det(power-7, the-5)
amod(power-7, political-6)
dobj(have-4, power-7)
aux(solve-9, to-8)
infmod(power-7, solve-9)
det(problem-11, this-10)
dobj(solve-9, problem-11)

