(ROOT
  (S
    (S
      (NP (PRP I))
      (VP (VBP do) (RB n't)))
    (CC and)
    (S
      (PP (IN by)
        (NP (DT the) (NN way)))
      (S
        (NP (PRP you))
        (VP (MD can)
          (ADVP (RB never))
          (VP (VB prove)
            (NP (PRP you)))))
      (CC or)
      (S
        (NP (DT any) (NN one))
        (VP (VBZ does)
          (VP (VB have)
            (NP (DT a) (NN soul))))))
    (. .)))

nsubj(do-2, I-1)
root(ROOT-0, do-2)
neg(do-2, n't-3)
det(way-7, the-6)
prep_by(prove-11, way-7)
nsubj(prove-11, you-8)
aux(prove-11, can-9)
neg(prove-11, never-10)
