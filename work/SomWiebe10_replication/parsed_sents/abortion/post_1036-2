(ROOT
  (S
    (NP (JJ Create) (NN Debate))
    (VP (VBZ is) (RB not)
      (NP
        (NP (DT a) (NN forum))
        (PP (IN for)
          (S
            (VP (VBG justifying)
              (NP (DT the) (NN unjustifiable)))))))
    (. .)))

amod(Debate-2, Create-1)
nsubj(forum-6, Debate-2)
cop(forum-6, is-3)
neg(forum-6, not-4)
det(forum-6, a-5)
root(ROOT-0, forum-6)
prepc_for(forum-6, justifying-8)
det(unjustifiable-10, the-9)
dobj(justifying-8, unjustifiable-10)

