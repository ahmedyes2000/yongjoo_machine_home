  (S
    (S
      (NP (NN No-one) (RB else))
      (VP (VBZ carries)
        (NP
          (NP (DT the) (NN child))
          (PP (IN for)
            (NP (PRP her))))))
    (: ;)
    (S
      (S
        (NP (PRP it))
        (VP (MD will)
          (VP (VB be)
            (NP (PRP$ her) (NN responsibility))
            (ADVP (RB alone)))))
      (, ,)
      (CC and)
      (S
        (ADVP (RB thus))
