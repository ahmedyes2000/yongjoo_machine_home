(ROOT
  (S
    (NP (NNP Abortion))
    (VP (VBZ is)
      (ADJP (JJ stupid)))
    (. .)))

nsubj(stupid-3, Abortion-1)
cop(stupid-3, is-2)
root(ROOT-0, stupid-3)

(ROOT
  (S
    (NP
      (NP (DT All) (JJ thes) (NNS chicks))
      (SBAR
        (WHNP (WDT that))
        (S
          (VP (VBP have)
            (NP (NNS abortions))))))
    (VP (VBP are)
      (ADJP (JJ stupid)))
    (. .)))

det(chicks-3, All-1)
amod(chicks-3, thes-2)
nsubj(have-5, chicks-3)
nsubj(stupid-8, chicks-3)
rcmod(chicks-3, have-5)
dobj(have-5, abortions-6)
cop(stupid-8, are-7)
root(ROOT-0, stupid-8)

