(ROOT
  (S
    (S
      (NP (DT That) (NN target))
      (VP (VBZ is)
        (ADJP (RB so) (JJ easy))
        (S
          (VP (TO to)
            (VP (VB hit))))))
    (, ,)
    (NP (PRP I))
    (VP (MD wo) (RB n't)
      (VP (VB bother)
        (S
          (VP (VBG shooting)
            (PP (IN at)
              (NP (PRP it)))))))
    (. .)))

det(target-2, That-1)
nsubj(easy-5, target-2)
xsubj(hit-7, target-2)
cop(easy-5, is-3)
advmod(easy-5, so-4)
ccomp(bother-12, easy-5)
aux(hit-7, to-6)
xcomp(easy-5, hit-7)
nsubj(bother-12, I-9)
aux(bother-12, wo-10)
neg(bother-12, n't-11)
root(ROOT-0, bother-12)
xcomp(bother-12, shooting-13)
prep_at(shooting-13, it-15)

