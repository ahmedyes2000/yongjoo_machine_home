(ROOT
  (NP
    (NP (DT those))
    (VP (VBG expressing)
      (NP (PRP themselves))
      (ADVP (RB sexually))
      (PP (IN outside)
        (NP
          (NP (DT the) (NN context))
          (PP (IN of)
            (NP (NN marriage))))))
    (. .)))

root(ROOT-0, those-1)
partmod(those-1, expressing-2)
dobj(expressing-2, themselves-3)
advmod(expressing-2, sexually-4)
det(context-7, the-6)
prep_outside(expressing-2, context-7)
prep_of(context-7, marriage-9)

