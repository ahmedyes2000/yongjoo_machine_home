(ROOT
  (S
    (NP (DT This) (NN ignorance))
    (VP (MD can)
      (ADVP (RB often))
      (VP (VB be)
        (VP (VBN imputed)
          (PP (TO to)
            (NP (JJ personal) (NN responsibility))))))
    (. .)))

det(ignorance-2, This-1)
nsubjpass(imputed-6, ignorance-2)
aux(imputed-6, can-3)
advmod(imputed-6, often-4)
auxpass(imputed-6, be-5)
root(ROOT-0, imputed-6)
amod(responsibility-9, personal-8)
prep_to(imputed-6, responsibility-9)

