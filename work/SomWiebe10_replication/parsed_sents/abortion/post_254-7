(ROOT
  (S
    (ADVP (RB Again))
    (, ,)
    (NP (DT a) (NN fetus))
    (VP (VBZ becomes)
      (NP
        (NP (DT a) (NN person))
        (PP (IN at)
          (NP (NN birth)))))
    (. .)))

advmod(person-7, Again-1)
det(fetus-4, a-3)
nsubj(person-7, fetus-4)
cop(person-7, becomes-5)
det(person-7, a-6)
root(ROOT-0, person-7)
prep_at(person-7, birth-9)

