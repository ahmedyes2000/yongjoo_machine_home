(ROOT
  (S
    (SBAR
      (WHADVP (WRB Where))
      (S
        (NP (EX there))
        (VP (VBZ is)
          (NP (DT no) (NN doubt)))))
    (, ,)
    (ADVP (RB then))
    (NP (EX there))
    (VP (VBZ is)
      (NP (DT no) (NN problem)))
    (. .)))

advmod(is-3, Where-1)
expl(is-3, there-2)
advcl(is-9, is-3)
det(doubt-5, no-4)
nsubj(is-3, doubt-5)
advmod(is-9, then-7)
expl(is-9, there-8)
root(ROOT-0, is-9)
det(problem-11, no-10)
nsubj(is-9, problem-11)

