(ROOT
  (S
    (ADVP (RB Right) (RB now))
    (, ,)
    (NP (DT the) (NNP Supreme) (NNP Court))
    (VP (VBZ has)
      (ADVP (RB essentially))
      (VP (VBN answered)
        (NP
          (NP (DT that))
          (: :)
          (NP
            (NP (DT each) (NN monther) (POS 's))
            (NN opinion)))))
    (. .)))

advmod(now-2, Right-1)
advmod(answered-9, now-2)
det(Court-6, the-4)
nn(Court-6, Supreme-5)
nsubj(answered-9, Court-6)
aux(answered-9, has-7)
advmod(answered-9, essentially-8)
root(ROOT-0, answered-9)
dobj(answered-9, that-10)
det(monther-13, each-12)
poss(opinion-15, monther-13)
dep(that-10, opinion-15)

