(ROOT
  (S
    (NP (PRP I))
    (VP (VBP 've)
      (ADVP (RB never))
      (VP (VBN asked)
        (PP (IN for)
          (NP (NN elaboration)))
        (PP (IN beyond)
          (NP (DT the) (JJ relevant)))))
    (. .)))

nsubj(asked-4, I-1)
aux(asked-4, 've-2)
neg(asked-4, never-3)
root(ROOT-0, asked-4)
prep_for(asked-4, elaboration-6)
det(relevant-9, the-8)
prep_beyond(asked-4, relevant-9)

