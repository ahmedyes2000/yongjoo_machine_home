(ROOT
  (S
    (NP (PRP It))
    (VP (MD would)
      (VP (VB appear)
        (SBAR (IN that)
          (S
            (NP (PRP you))
            (VP (VBP are) (RB n't)
              (ADJP
                (ADJP (RB as) (RB philosophically) (JJ inclined))
                (SBAR (IN as)
                  (S
                    (NP (PRP you))
                    (VP (MD would)
                      (VP (VB have)
                        (S
                          (NP (PRP me))
                          (VP (VB believe)))))))))))))
    (. .)))

nsubj(appear-3, It-1)
aux(appear-3, would-2)
root(ROOT-0, appear-3)
complm(inclined-10, that-4)
nsubj(inclined-10, you-5)
cop(inclined-10, are-6)
neg(inclined-10, n't-7)
