(ROOT
  (S
    (PP (IN Without)
      (NP (NN life)))
    (NP (DT no) (JJ other) (NNS rights))
    (VP (VBP are)
      (ADJP (JJ available)
        (PP (TO to)
          (NP (NN anyone)))))
    (. .)))

prep_without(available-7, life-2)
det(rights-5, no-3)
amod(rights-5, other-4)
nsubj(available-7, rights-5)
cop(available-7, are-6)
root(ROOT-0, available-7)
prep_to(available-7, anyone-9)

