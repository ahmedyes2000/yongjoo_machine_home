(ROOT
  (S
    (NP (DT The) (NN government))
    (VP (MD should)
      (VP (VB remain)
        (ADJP (JJ neutral))
        (PP (IN on)
          (NP (DT this) (NN issue)))))
    (. .)))

det(government-2, The-1)
nsubj(neutral-5, government-2)
aux(neutral-5, should-3)
cop(neutral-5, remain-4)
root(ROOT-0, neutral-5)
det(issue-8, this-7)
prep_on(neutral-5, issue-8)

