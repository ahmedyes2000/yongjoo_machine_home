(ROOT
  (FRAG
    (ADJP (RB Its) (RB so) (JJ immoral))
    (SBAR (IN that)
      (S
        (NP (PRP it))
        (VP (VBZ makes)
          (S
            (NP (PRP me))
            (ADJP (RB so) (JJ sick))))))
    (. !)))

advmod(immoral-3, Its-1)
advmod(immoral-3, so-2)
root(ROOT-0, immoral-3)
dep(makes-6, that-4)
nsubj(makes-6, it-5)
dep(immoral-3, makes-6)
nsubj(sick-9, me-7)
advmod(sick-9, so-8)
xcomp(makes-6, sick-9)

