(ROOT
  (S
    (NP
      (NP (NNP Morality))
      (CC and)
      (NP (NNS ethics)))
    (VP (VBP are)
      (ADVP
        (NP (CD two))
        (RB very))
      (NP (JJ different) (NNS things)))
    (. .)))

nsubj(things-8, Morality-1)
conj_and(Morality-1, ethics-3)
nsubj(things-8, ethics-3)
cop(things-8, are-4)
npadvmod(very-6, two-5)
advmod(things-8, very-6)
amod(things-8, different-7)
root(ROOT-0, things-8)

