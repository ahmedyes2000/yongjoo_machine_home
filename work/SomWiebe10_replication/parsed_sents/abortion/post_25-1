  (S
    (NP
      (NP (DT The) (NN potential))
      (PP (IN of)
        (NP (DT any) (VBN given) (NN child))))
    (VP (VBZ is)
      (ADJP (JJ unimportant)
        (PP (IN in)
          (NP
            (NP (DT the) (NN context))
            (PP (IN of)
              (NP (PRP it))))))
      (S
        (VP (VBG being)
          (ADJP (JJ possible))))
      (, ,)
      (PP (IN after)
        (NP (DT an) (NN abortion)))
      (, ,)
      (PP (IN for)
        (NP (DT a) (NN mother)
          (S
            (VP (TO to)
              (VP (VB reproduce)
                (NP (JJ many) (JJR more) (NNS children))))))))
    (. .)))

