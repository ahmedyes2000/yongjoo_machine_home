(ROOT
  (S
    (NP (PRP I))
    (VP (MD 'd)
      (VP (VB like)
        (S
          (VP (TO to)
            (VP (VB hear)
              (NP (CD one)))))))
    (. .)))

nsubj(like-3, I-1)
xsubj(hear-5, I-1)
aux(like-3, 'd-2)
root(ROOT-0, like-3)
aux(hear-5, to-4)
xcomp(like-3, hear-5)
dobj(hear-5, one-6)

