(ROOT
  (S
    (NP (PRP I))
    (ADVP (RBR more) (IN than) (RB that))
    (VP (VBP look)
      (PP (IN upon)
        (NP (DT the) (`` ``)
          (NP (JJ little) (NN one) (POS 's))
          ('' '') (NN right)))
      (PP (TO to)
        (NP (NN life))))
    (. .)))

nsubj(look-5, I-1)
advmod(than-3, more-2)
advmod(look-5, than-3)
advmod(than-3, that-4)
root(ROOT-0, look-5)
det(right-13, the-7)
amod(one-10, little-9)
