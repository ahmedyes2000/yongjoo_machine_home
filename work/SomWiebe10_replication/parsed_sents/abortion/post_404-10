      (VP (VB define)
        (NP
          (NP (DT the) (NN principle)
            (CC or)
            (NN category))
          (, ,) ('' '')
          (ADVP (RB then)))
        (SBAR
          (WHADVP (WRB why))
          (S
            (VP (MD can)
              (NP (PRP we))
              (S
                (VP (RB not) (VB outlaw)
                  (NP (JJ partial-birth) (NNS abortions)))))))))
    (. ?)))

