  (S
    (PP (IN In)
      (NP (DT any) (NN case)))
    (, ,)
    (S
      (SBAR (IN if)
        (S
          (NP (JJ social) (NNS problems))
          (VP (VBP are)
            (NP (DT the) (NN concern)))))
      (, ,)
      (NP (DT these) (NNS problems))
      (VP (MD should)
        (VP (VB be)
          (VP (VBN addressed)))))
    (: ;)
    (S
      (NP (NN abortion))
      (VP (VBZ is)
        (NP (DT a) (JJ terrible) (NN band-aid))))
    (. .)))

