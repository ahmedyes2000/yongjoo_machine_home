(ROOT
  (S
    (S
      (NP (PRP$ Our) (JJ cultural) (NNS enemies))
      (VP (VBP know)
        (NP (DT this))))
    (CC and)
    (S
      (NP (PRP they))
      (VP (VBP are)
        (ADJP (VBN scared))))
    (. .)))

poss(enemies-3, Our-1)
amod(enemies-3, cultural-2)
nsubj(know-4, enemies-3)
root(ROOT-0, know-4)
dobj(know-4, this-5)
nsubjpass(scared-9, they-7)
auxpass(scared-9, are-8)
conj_and(know-4, scared-9)

