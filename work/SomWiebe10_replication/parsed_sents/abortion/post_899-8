(ROOT
  (S
    (SBAR (IN Though)
      (S
        (NP (DT a) (NN mother))
        (VP (MD may) (RB not)
          (VP (VB want)
            (NP (PRP$ her) (NN baby))))))
    (, ,)
    (NP
      (NP (NN health))
      (PP (IN for)
        (NP
          (NP (DT both) (NNS remains))
          (CC and)
          (NP (NN adoption)))))
    (VP (VBZ remains)
      (NP (DT a) (JJ viable) (NN option)))
    (. .)))

mark(want-6, Though-1)
det(mother-3, a-2)
nsubj(want-6, mother-3)
aux(want-6, may-4)
neg(want-6, not-5)
advcl(option-19, want-6)
poss(baby-8, her-7)
dobj(want-6, baby-8)
nsubj(option-19, health-10)
det(remains-13, both-12)
prep_for(health-10, remains-13)
prep_for(health-10, adoption-15)
conj_and(remains-13, adoption-15)
cop(option-19, remains-16)
det(option-19, a-17)
amod(option-19, viable-18)
root(ROOT-0, option-19)

