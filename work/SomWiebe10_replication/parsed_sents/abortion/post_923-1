    (SBAR (IN If)
      (S
        (NP (PRP it))
        (VP (VBZ is)
          (NP (DT a) (NN human)
            (S
              (VP (VBG being)
                (ADVP (RB then))))))))
    (NP (PRP it))
    (VP (VBZ requires)
      (NP
        (NP (DT a)
          (ADJP (RB very) (JJ high))
          (NN level))
        (PP (IN of)
          (NP (NN justification))))
      (S
        (VP (TO to)
          (VP (VB take)
            (NP
              (NP (PRP it) (POS 's))
              (NN life))))))
    (. .)))

