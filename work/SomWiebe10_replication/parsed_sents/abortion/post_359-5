(ROOT
  (S
    (NP (NNS Abortions))
    (VP (VBP are)
      (ADJP
        (ADJP (RB far) (RBR more) (JJ expensive))
        (PP (IN than)
          (NP (NNS condoms)
            (CC and)
            (NNS pills)))))
    (. .)))

nsubj(expensive-5, Abortions-1)
cop(expensive-5, are-2)
advmod(expensive-5, far-3)
advmod(expensive-5, more-4)
root(ROOT-0, expensive-5)
prep_than(expensive-5, condoms-7)
prep_than(expensive-5, pills-9)
conj_and(condoms-7, pills-9)

