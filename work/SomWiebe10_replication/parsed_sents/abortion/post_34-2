(ROOT
  (S
    (PP
      (ADVP (RB So) (RB as))
      (IN in)
      (NP (NN abortion)))
    (, ,)
    (NP (PRP it))
    (VP (MD can)
      (VP (VB be)
        (ADJP (JJ justified)
          (S
            (VP (TO to)
              (VP (VB kill)
                (NP (DT a) (NN fetus))
                (PP (IN under)
                  (NP (JJ certain) (NNS circumstances)))))))))
    (. .)))

advmod(as-2, So-1)
advmod(justified-9, as-2)
prep_in(justified-9, abortion-4)
nsubj(justified-9, it-6)
aux(justified-9, can-7)
cop(justified-9, be-8)
root(ROOT-0, justified-9)
aux(kill-11, to-10)
xcomp(justified-9, kill-11)
det(fetus-13, a-12)
dobj(kill-11, fetus-13)
amod(circumstances-16, certain-15)
prep_under(kill-11, circumstances-16)

