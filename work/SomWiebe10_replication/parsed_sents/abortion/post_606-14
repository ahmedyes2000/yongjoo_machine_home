(ROOT
  (NP
    (NP (PRP$ My) (NNS stats))
    (: :)
    (NP
      (NP
        (ADJP (CD 20) (NN %))
        (NNP CERTAIN) (NN death))
      (PP (IN of)
        (NP (DT a) (NN human) (VBG being))))
    (. .)))

poss(stats-2, My-1)
root(ROOT-0, stats-2)
number(%-5, 20-4)
amod(death-7, %-5)
nn(death-7, CERTAIN-6)
dep(stats-2, death-7)
det(human-10, a-9)
prep_of(death-7, human-10)
amod(human-10, being-11)

