(ROOT
  (S
    (NP (PRP We))
    (VP (VBP carry)
      (NP
        (NP (DT some))
        (PP (IN of)
          (NP
            (NP (DT the) (`` ``) (NNS potencies) ('' ''))
            (PP (IN of)
              (NP (PDT all) (PRP$ our) (NNS descendants))))))
      (PP (IN with)
        (NP (PRP us)))
      (ADVP (RB right) (RB now)))
    (. .)))

nsubj(carry-2, We-1)
root(ROOT-0, carry-2)
dobj(carry-2, some-3)
det(potencies-7, the-5)
prep_of(some-3, potencies-7)
