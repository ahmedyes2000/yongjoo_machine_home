(ROOT
  (S
    (NP (NNP Granted))
    (, ,)
    (PP (IN at)
      (NP (DT that) (NN point)))
    (, ,)
    (NP (PRP it))
    (VP (VBZ is)
      (NP
        (NP (DT the) (NN doctor) (POS 's))
        (NN decision)))
    (. .)))

nsubj(decision-12, Granted-1)
det(point-5, that-4)
prep_at(decision-12, point-5)
nsubj(decision-12, it-7)
cop(decision-12, is-8)
det(doctor-10, the-9)
poss(decision-12, doctor-10)
root(ROOT-0, decision-12)

