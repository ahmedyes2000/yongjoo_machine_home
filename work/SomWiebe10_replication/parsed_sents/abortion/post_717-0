  (S
    (NP (PRP It))
    (VP (VBZ looks)
      (SBAR (IN like)
        (S
          (NP (PRP we))
          (VP (VBP agree)
            (SBAR (IN that)
              (S
                (NP
                  (NP (DT the) (NN emotion))
                  (PP (IN of)
                    (NP (DT the) (NN mother))))
                (VP (VBZ does) (RB not)
                  (VP (VB change)
                    (SBAR (IN whether)
                      (S
                        (NP
                          (NP (DT the) (NN act))
                          (PP (IN of)
                            (NP (NN abortion))))
                        (VP (VBZ is)
                          (ADJP (JJ right)
                            (CC or)
                            (JJ wrong)))))))))))))
    (. .)))

