(ROOT
  (S
    (NP (PRP We))
    (VP (VBP 've)
      (VP (VBN seen)
        (ADVP
          (NP (DT this) (NN play))
          (IN out))
        (PP (IN before)
          (CC and)
          (IN within)
          (NP (DT the) (NNP Catholic) (NNP Church)))
        (SBAR
          (WHADVP (WRB when))
          (S
            (NP (JJ certain) (NNS individuals))
            (VP (VBD decided)
              (S
                (VP (TO to)
                  (VP (VB place)
                    (NP (PRP themselves))
                    (PP (IN above)
                      (NP (NNP God)))))))))))
    (. .)))

nsubj(seen-3, We-1)
nsubj(seen-3', We-1)
aux(seen-3, 've-2)
root(ROOT-0, seen-3)
conj_and(seen-3, seen-3')
det(play-5, this-4)
