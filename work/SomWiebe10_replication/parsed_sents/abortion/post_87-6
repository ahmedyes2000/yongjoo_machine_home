(ROOT
  (S
    (ADVP (LS Second))
    (, ,)
    (NP (EX there))
    (VP (VBZ is)
      (NP
        (NP (DT no) (NN difference))
        (PP (IN in)
          (S
            (VP (VBG separating)
              (PP (IN from)
                (NP
                  (NP (DT an) (JJ unborn) (NN child))
                  (PRN (-LRB- -LRB-)
                    (NP (NN abortion))
                    (-RRB- -RRB-))))
              (PP (IN as))
              (PP (VBN compared)
                (PP (TO to)
                  (NP
                    (NP (DT a) (VBN born) (NN child))
                    (PRN (-LRB- -LRB-)
                      (NP (NN adoption))
                      (-RRB- -RRB-))))))))))
    (. .)))

advmod(is-4, Second-1)
expl(is-4, there-3)
root(ROOT-0, is-4)
det(difference-6, no-5)
nsubj(is-4, difference-6)
prepc_in(difference-6, separating-8)
det(child-12, an-10)
amod(child-12, unborn-11)
prep_from(separating-8, child-12)
appos(child-12, abortion-14)
prep(separating-8, as-16)
prepc_compared_to(separating-8, to-18)
det(child-21, a-19)
amod(child-21, born-20)
pobj(separating-8, child-21)
appos(child-21, adoption-23)

