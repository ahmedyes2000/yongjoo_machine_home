(ROOT
  (S
    (INTJ (WRB Why))
    (VP (VB wait)
      (PP (IN for)
        (NP (DT the) (NNP Supreme) (NNP Court)))
      (S
        (VP (TO to)
          (VP (VB rule)
            (ADVP (RB again))))))
    (. .)))

dep(wait-2, Why-1)
root(ROOT-0, wait-2)
det(Court-6, the-4)
nn(Court-6, Supreme-5)
prep_for(wait-2, Court-6)
aux(rule-8, to-7)
xcomp(wait-2, rule-8)
advmod(rule-8, again-9)

