(ROOT
  (S
    (NP (NNS Men))
    (VP
      (VP (VBP are)
        (ADJP (JJ dominant)
          (PP (IN in)
            (NP (PRP$ their) (NN ability)
              (S
                (VP (TO to)
                  (VP (VB impregnate)
                    (NP (DT a) (NN woman)))))))))
      (, ,)
      (CC but)
      (VP (VBP carry)
        (NP (DT no) (NNS responsibilities))
        (ADVP (RB afterward))))
    (. .)))

nsubj(dominant-3, Men-1)
nsubj(carry-13, Men-1)
cop(dominant-3, are-2)
root(ROOT-0, dominant-3)
poss(ability-6, their-5)
prep_in(dominant-3, ability-6)
aux(impregnate-8, to-7)
infmod(ability-6, impregnate-8)
det(woman-10, a-9)
