(ROOT
  (FRAG
    (NP (RB yes))
    (, ,)
    (CC but)
    (S
      (NP (PRP you))
      (VP (MD should)
        (VP (VB have)
          (S
            (VP (TO to)
              (VP (VB take)
                (NP
                  (NP (DT some) (NN sort))
                  (PP (IN of)
                    (NP (NN test)))))))
          (S
            (VP (VBG saying)
              (SBAR
                (WHADVP (WRB why))
                (S
                  (NP (PRP you))
                  (VP (VBP want)
                    (PP (TO to))))))))))
    (. .)))

root(ROOT-0, yes-1)
dep(yes-1, but-3)
nsubj(have-6, you-4)
xsubj(take-8, you-4)
aux(have-6, should-5)
dep(yes-1, have-6)
aux(take-8, to-7)
xcomp(have-6, take-8)
det(sort-10, some-9)
dobj(take-8, sort-10)
prep_of(sort-10, test-12)
xcomp(have-6, saying-13)
advmod(want-16, why-14)
nsubj(want-16, you-15)
advcl(saying-13, want-16)
prep(want-16, to-17)

