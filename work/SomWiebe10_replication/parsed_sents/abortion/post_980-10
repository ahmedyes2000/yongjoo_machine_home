(ROOT
  (S
    (NP (NNP God))
    (VP (VBZ looks)
      (PP (IN at)
        (NP
          (NP (DT every) (NN sin))
          (NP (DT the) (JJ same) (NN way))))
      (, ,)
      (PP (IN except) (IN for)
        (NP
          (NP (NN blasphemy))
          (PP (IN against)
            (NP (NNP HS))))))
    (. .)))

nsubj(looks-2, God-1)
root(ROOT-0, looks-2)
det(sin-5, every-4)
prep_at(looks-2, sin-5)
det(way-8, the-6)
amod(way-8, same-7)
dep(sin-5, way-8)
prep_except_for(looks-2, blasphemy-12)
prep_against(blasphemy-12, HS-14)

