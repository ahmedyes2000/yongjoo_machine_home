(ROOT
  (S
    (NP (FW i))
    (VP (VBP have)
      (NP (DT a) (NN choice)
        (S
          (VP (TO to)
            (VP (RB not) (VB bear)
              (NP (DT a) (NN child))))))
      (, ,)
      (ADVP (RB though)))
    (. .)))

nsubj(have-2, i-1)
root(ROOT-0, have-2)
det(choice-4, a-3)
dobj(have-2, choice-4)
aux(bear-7, to-5)
neg(bear-7, not-6)
infmod(choice-4, bear-7)
det(child-9, a-8)
dobj(bear-7, child-9)
advmod(have-2, though-11)

