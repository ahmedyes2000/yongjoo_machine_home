(ROOT
  (S
    (ADVP (RB Somewhere)
      (PP (IN along)
        (NP (DT the) (NN way))))
    (NP (DT the) (JJ potential) (NN child))
    (VP (VBZ starts)
      (S
        (VP (TO to)
          (VP (VB get)
            (S
              (NP (NNS rights))
              (ADJP (JJ independent)
                (PP (IN of)
                  (NP (DT the) (JJ potential) (NN parent)))))))))
    (. .)))

advmod(starts-8, Somewhere-1)
det(way-4, the-3)
prep_along(Somewhere-1, way-4)
det(child-7, the-5)
amod(child-7, potential-6)
