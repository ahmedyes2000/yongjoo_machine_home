(ROOT
  (S
    (NP (PRP I))
    (VP (VBP am)
      (ADJP (JJ sure)
        (SBAR (IN that)
          (S
            (NP
              (NP (PDT all) (DT the) (NNS girls))
              (SBAR
                (WHNP (WP who))
                (S
                  (VP (VBP are)
                    (PP (IN out)
                      (NP (RB there)))))))
            (VP (VBD were)
              (VP (VBG thinking)))))))
    (: ...)))

nsubj(sure-3, I-1)
cop(sure-3, am-2)
root(ROOT-0, sure-3)
complm(thinking-13, that-4)
predet(girls-7, all-5)
det(girls-7, the-6)
nsubj(are-9, girls-7)
nsubj(thinking-13, girls-7)
rcmod(girls-7, are-9)
prep(are-9, out-10)
pobj(out-10, there-11)
aux(thinking-13, were-12)
ccomp(sure-3, thinking-13)

