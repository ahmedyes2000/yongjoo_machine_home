(ROOT
  (S
    (PP (IN At)
      (NP (DT a) (NN minimum)))
    (, ,)
    (NP
      (NP (NN uncertainty))
      (PP (IN about)
        (NP (DT the) (`` ``) (NN truth) ('' ''))))
    (VP (MD should)
      (VP (VB cause)
        (S
          (NP (PRP us))
          (VP (TO to)
            (VP (VB place)
              (NP (DT a) (NN moratorium))
              (PP (IN on)
                (NP (NNS abortions)))
              (SBAR (IN until)
                (S
                  (NP (PRP we))
                  (VP (VBP figure)
                    (NP (PRP it))
                    (PRT (RP out))))))))))
    (. .)))

det(minimum-3, a-2)
prep_at(cause-12, minimum-3)
nsubj(cause-12, uncertainty-5)
det(truth-9, the-7)
prep_about(uncertainty-5, truth-9)
aux(cause-12, should-11)
root(ROOT-0, cause-12)
nsubj(place-15, us-13)
aux(place-15, to-14)
xcomp(cause-12, place-15)
det(moratorium-17, a-16)
dobj(place-15, moratorium-17)
prep_on(place-15, abortions-19)
mark(figure-22, until-20)
nsubj(figure-22, we-21)
advcl(place-15, figure-22)
dobj(figure-22, it-23)
prt(figure-22, out-24)

