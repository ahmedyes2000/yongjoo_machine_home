(ROOT
  (S
    (VP (VB Make)
      (ADJP (JJ sure)
        (SBAR
          (S
            (NP (PRP they))
            (VP (VBP have)
              (NP (JJ good) (NN food)
                (S
                  (VP (TO to)
                    (VP (VB eat)
                      (S
                        (NP
                          (NP (DT every) (NN day))
                          (CC and)
                          (NP (DT a) (JJ safe) (NN place)))
                        (VP (TO to)
                          (VP (VB live)))))))))))))
    (. .)))

root(ROOT-0, Make-1)
acomp(Make-1, sure-2)
nsubj(have-4, they-3)
dep(sure-2, have-4)
amod(food-6, good-5)
dobj(have-4, food-6)
aux(eat-8, to-7)
infmod(food-6, eat-8)
det(day-10, every-9)
nsubj(live-16, day-10)
det(place-14, a-12)
amod(place-14, safe-13)
conj_and(day-10, place-14)
nsubj(live-16, place-14)
aux(live-16, to-15)
xcomp(eat-8, live-16)

