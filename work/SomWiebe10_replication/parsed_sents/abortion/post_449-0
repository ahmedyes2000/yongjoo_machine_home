(ROOT
  (S
    (NP (PRP I))
    (VP (MD could) (RB not)
      (VP (VB have)
        (VP (VBD said)
          (S
            (NP (PRP it))
            (ADJP (JJR better))))))
    (: ...)))

nsubj(said-5, I-1)
aux(said-5, could-2)
neg(said-5, not-3)
aux(said-5, have-4)
root(ROOT-0, said-5)
nsubj(better-7, it-6)
xcomp(said-5, better-7)

