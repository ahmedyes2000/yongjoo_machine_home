(ROOT
  (S ('' '')
    (NP (PRP They))
    (VP (VBP follow)
      (NP
        (NP (DT the) (NNS rules))
        (PP (IN of)
          (NP (DT the) (NNP Vatican)))
        (SBAR
          (WHNP (WDT that))
          (S
            (VP (VBP are)
              (ADJP (JJ convenient))
              (PP (IN for)
                (NP (PRP them)))
              (, ,)
              (VP (VBG using)
                (NP
                  (NP (DT the) (NNS principles))
                  (PP (IN of)
                    (NP (NNP Christianity))))
                (PP (IN as)
                  (NP (NN fire) (NN insurance)))))))))
    (. .)))

nsubj(follow-3, They-2)
root(ROOT-0, follow-3)
det(rules-5, the-4)
dobj(follow-3, rules-5)
nsubj(using-15, rules-5)
det(Vatican-8, the-7)
prep_of(rules-5, Vatican-8)
cop(using-15, are-10)
acomp(using-15, convenient-11)
prep_for(using-15, them-13)
rcmod(rules-5, using-15)
det(principles-17, the-16)
dobj(using-15, principles-17)
prep_of(principles-17, Christianity-19)
nn(insurance-22, fire-21)
prep_as(using-15, insurance-22)

