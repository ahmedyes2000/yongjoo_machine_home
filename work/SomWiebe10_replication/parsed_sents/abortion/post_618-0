(ROOT
  (S
    (ADVP (RB Sorry))
    (NP (PRP I))
    (VP (VBD forgot)
      (SBAR (IN that)
        (S
          (NP (DT this))
          (VP (VBD was)
            (NP (JJ HTML) (NN format))))))
    (. ...)))

advmod(forgot-3, Sorry-1)
nsubj(forgot-3, I-2)
root(ROOT-0, forgot-3)
complm(format-8, that-4)
nsubj(format-8, this-5)
cop(format-8, was-6)
amod(format-8, HTML-7)
ccomp(forgot-3, format-8)

