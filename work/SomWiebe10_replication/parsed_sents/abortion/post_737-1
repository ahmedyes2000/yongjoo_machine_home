(ROOT
  (SBARQ
    (WHADVP (WRB When))
    (SQ (VBP do)
      (NP (PRP we))
      (VP (VB start)
        (S
          (NP (VBG holding) (NNS people))
          (ADJP (JJ accountable)
            (PP (IN for)
              (NP (PRP$ their) (NNS actions)))))))
    (. ?)))

advmod(start-4, When-1)
aux(start-4, do-2)
nsubj(start-4, we-3)
root(ROOT-0, start-4)
amod(people-6, holding-5)
nsubj(accountable-7, people-6)
