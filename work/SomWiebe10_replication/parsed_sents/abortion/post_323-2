(ROOT
  (S
    (ADVP (RB Also))
    (, ,)
    (NP
      (NP (NNS men) (POS 's))
      (NN body) (NNS parts))
    (VP (VBP are)
      (VP (VBN restricted)))
    (. .)))

advmod(restricted-8, Also-1)
poss(parts-6, men-3)
nn(parts-6, body-5)
nsubjpass(restricted-8, parts-6)
auxpass(restricted-8, are-7)
root(ROOT-0, restricted-8)

