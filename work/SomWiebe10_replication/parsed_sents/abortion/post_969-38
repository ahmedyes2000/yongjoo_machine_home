  (S
    (S
      (ADVP (RB Hopefully))
      (ADVP (RB then))
      (NP (DT the) (NN abortion) (NNS clinics))
      (VP (MD will)
        (VP (VB shut)
          (PRT (RP down)))))
    (, ,)
    (CC and)
    (S
      (NP (DT the) (NN law))
      (VP (MD will)
        (VP (VB have)
          (S
            (VP (TO to)
              (VP (VB change)))))))
    (. .)))

advmod(shut-7, Hopefully-1)
advmod(shut-7, then-2)
det(clinics-5, the-3)
nn(clinics-5, abortion-4)
nsubj(shut-7, clinics-5)
aux(shut-7, will-6)
root(ROOT-0, shut-7)
