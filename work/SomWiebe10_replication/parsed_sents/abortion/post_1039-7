(ROOT
  (S
    (ADVP (RB Therefore))
    (, ,)
    (NP (CD 1))
    (VP (VBZ is) (RB not)
      (NP (CD 4)))
    (. .)))

advmod(4-6, Therefore-1)
nsubj(4-6, 1-3)
cop(4-6, is-4)
neg(4-6, not-5)
root(ROOT-0, 4-6)

(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP (VBP have)
          (NP (PRP$ your) (NNS arguments))
          (PP (IN in)
            (NP (NN order))))))
    (, ,)
    (NP (DT every) (NN issue))
    (VP (VBZ is)
      (ADJP (JJ debatable)))
    (. .)))

mark(have-3, If-1)
nsubj(have-3, you-2)
advcl(debatable-12, have-3)
poss(arguments-5, your-4)
dobj(have-3, arguments-5)
prep_in(have-3, order-7)
det(issue-10, every-9)
nsubj(debatable-12, issue-10)
cop(debatable-12, is-11)
root(ROOT-0, debatable-12)

