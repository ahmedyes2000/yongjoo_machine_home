(ROOT
  (S
    (INTJ (VB Please))
    (VP (VB stop)
      (S
        (VP (VBG making)
          (NP (NNS appeals))
          (PP (TO to)
            (NP (NN emotion))))))
    (. .)))

dep(stop-2, Please-1)
root(ROOT-0, stop-2)
xcomp(stop-2, making-3)
dobj(making-3, appeals-4)
prep_to(making-3, emotion-6)

