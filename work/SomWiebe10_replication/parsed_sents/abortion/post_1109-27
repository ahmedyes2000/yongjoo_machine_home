(ROOT
  (S
    (NP
      (NP (DT The) (JJ vast) (NN majority))
      (PP (IN of)
        (NP (NNS abortions))))
    (VP (VBP are)
      (VP (VBN preformed)
        (PP (IN on)
          (NP (JJ healthy) (NNS women)
            (CC and)
            (NNS children)))))
    (. .)))

det(majority-3, The-1)
amod(majority-3, vast-2)
nsubjpass(preformed-7, majority-3)
prep_of(majority-3, abortions-5)
auxpass(preformed-7, are-6)
root(ROOT-0, preformed-7)
amod(women-10, healthy-9)
prep_on(preformed-7, women-10)
prep_on(preformed-7, children-12)
conj_and(women-10, children-12)

