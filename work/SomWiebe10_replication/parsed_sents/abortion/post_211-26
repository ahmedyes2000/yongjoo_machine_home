(ROOT
  (SINV
    (ADVP (RB Directly))
    (VP (VBG causing)
      (NP (PRP$ his) (NN death)))
    (VP (VBZ is)
      (ADJP
        (ADVP (RBS most) (RB certainly))
        (RBR more)))
    (NP
      (NP (DT a) (NN violation))
      (PP (IN of)
        (NP
          (NP (DT the) (NN child) (POS 's))
          (NNS rights)))
      (SBAR (IN than)
        (S
          (S
            (VP (VBG carrying)
              (NP (DT the) (NN baby))
              (PP (TO to)
                (NP (NN term)))))
          (VP (MD could)
            (ADVP (RB ever))
            (VP (VB be)
              (PP (IN for)
                (NP (PRP$ his) (NN mother))))))))
    (. .)))

advmod(causing-2, Directly-1)
root(ROOT-0, causing-2)
poss(death-4, his-3)
dobj(causing-2, death-4)
cop(more-8, is-5)
advmod(certainly-7, most-6)
