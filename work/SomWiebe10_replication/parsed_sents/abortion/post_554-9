(ROOT
  (S
    (VP (VB Remember)
      (SBAR
        (S
          (NP (PRP it))
          (VP (VBZ has)
            (VP (VBN been)
              (SBAR
                (NP
                  (QP (JJR less) (IN than) (CD 100))
                  (NNS years))
                (IN since)
                (S
                  (NP (PRP we))
                  (VP (VBD achieved)
                    (NP (NN status))
                    (PP (IN as)
                      (NP (JJ full) (NNS citizens)))
                    (PP (IN with)
                      (NP (DT the) (NN right)
                        (S
                          (VP (TO to)
                            (VP (VB vote))))))))))))))
    (. .)))

