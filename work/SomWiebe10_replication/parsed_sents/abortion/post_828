(ROOT
  (S
    (NP (PRP I))
    (VP (VBP believe)
      (SBAR (IN that)
        (S
          (NP (NN life))
          (VP (VBZ occurrs)
            (PP (IN at)
              (NP (NN conception)))))))
    (. .)))

nsubj(believe-2, I-1)
root(ROOT-0, believe-2)
complm(occurrs-5, that-3)
nsubj(occurrs-5, life-4)
ccomp(believe-2, occurrs-5)
prep_at(occurrs-5, conception-7)

(ROOT
  (S
    (NP (DT That))
    (VP (VBZ is)
      (SBAR
        (WHNP (WP what))
        (S
          (NP (PRP I))
          (VP (VBP believe)))))
    (. .)))

nsubj(is-2, That-1)
root(ROOT-0, is-2)
dobj(believe-5, what-3)
nsubj(believe-5, I-4)
ccomp(is-2, believe-5)

(ROOT
  (S
    (NP (PRP You))
    (VP
      (VP (MD can)
        (VP (VB believe)
          (S
            (NP (NN something))
            (ADJP (JJ different)))))
      (CC and)
      (VP (MD will)
        (VP (VB respect)
          (NP (DT that)))))
    (. .)))

nsubj(believe-3, You-1)
nsubj(respect-8, You-1)
aux(believe-3, can-2)
root(ROOT-0, believe-3)
nsubj(different-5, something-4)
xcomp(believe-3, different-5)
aux(respect-8, will-7)
conj_and(believe-3, respect-8)
dobj(respect-8, that-9)

