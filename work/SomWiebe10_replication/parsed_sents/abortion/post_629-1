(ROOT
  (S (: ...)
    (S
      (S
        (VP (VB mind)
          (NP (PRP you))))
      (NP (DT these) (NNS labels))
      (VP (VBP do) (RB not)
        (ADVP (RB truly))
        (VP (VB describe)
          (NP
            (NP (DT the) (NN majority))
            (PP (IN of)
              (NP (DT either) (NN party))))
          (SBAR (IN as)
            (S
              (NP (NNS labels))
              (VP (VBP are)
                (NP
                  (NP (DT the) (NNS exaggerations))
                  (PP (IN of)
                    (NP (DT the) (NNS extremes))))))))))
    (. ...)))

dep(describe-9, mind-2)
dobj(mind-2, you-3)
det(labels-5, these-4)
nsubj(describe-9, labels-5)
aux(describe-9, do-6)
neg(describe-9, not-7)
