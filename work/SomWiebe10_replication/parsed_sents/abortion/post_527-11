  (NP
    (NP
      (NP (NN life))
      (PP
        (PP (IN without)
          (NP (NN liberty)))
        (PRN (-LRB- -LRB-)
          (CC and)
          (VP (VBD screw)
            (NP (PDT all) (DT the) (JJ other) (NNS kinds))))))
    (: ...)))

