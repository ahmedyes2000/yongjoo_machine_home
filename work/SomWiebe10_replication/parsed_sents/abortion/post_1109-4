(ROOT
  (S
    (NP (NNS Laws))
    (VP (MD should)
      (VP (VB be)
        (VP (VBN based)
          (PP (IN on)
            (NP
              (NP (NNS ethics))
              (, ,) (RB not)
              (NP (NN morality)))))))
    (. .)))

nsubjpass(based-4, Laws-1)
aux(based-4, should-2)
auxpass(based-4, be-3)
root(ROOT-0, based-4)
prep_on(based-4, ethics-6)
neg(ethics-6, not-8)
dep(ethics-6, morality-9)

