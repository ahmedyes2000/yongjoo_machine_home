(ROOT
  (S
    (ADVP (RB Again))
    (, ,)
    (NP (PRP you))
    (VP (VBP do) (RB n't)
      (VP (VB believe)
        (SBAR (IN in)
          (S
            (NP (NN abortion))
            (VP (VBP do) (RB n't)
              (VP (VB have)
                (NP (CD one))))))))
    (. .)))

advmod(believe-6, Again-1)
nsubj(believe-6, you-3)
aux(believe-6, do-4)
neg(believe-6, n't-5)
root(ROOT-0, believe-6)
mark(have-11, in-7)
nsubj(have-11, abortion-8)
aux(have-11, do-9)
neg(have-11, n't-10)
advcl(believe-6, have-11)
dobj(have-11, one-12)

