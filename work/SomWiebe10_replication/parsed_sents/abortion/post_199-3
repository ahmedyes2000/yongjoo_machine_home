(ROOT
  (S
    (NP (PRP I))
    (VP (VBP think)
      (SBAR
        (S
          (NP (DT the) (NNP Church))
          (VP (VBZ has)
            (NP (NN access))
            (PP
              (PP (TO to)
                (NP
                  (NP (DT the) (NNS findings))
                  (PP (IN of)
                    (NP (NN science)))))
              (, ,) (RB just)
              (PP (IN like)
                (NP (NN everyone) (RB else))))))))
    (: ...)))

nsubj(think-2, I-1)
root(ROOT-0, think-2)
det(Church-4, the-3)
nsubj(has-5, Church-4)
ccomp(think-2, has-5)
dobj(has-5, access-6)
det(findings-9, the-8)
prep_to(has-5, findings-9)
prep_of(findings-9, science-11)
advmod(has-5, just-13)
prep_like(has-5, everyone-15)
advmod(everyone-15, else-16)

