  (S
    (NP (PRP I))
    (VP (VBP believe)
      (SBAR
        (S
          (NP (PRP we))
          (VP (MD must)
            (ADVP (RB consistently))
            (VP (VB define)
              (SBAR
                (SBAR
                  (WHNP (WP what))
                  (S
                    (NP (`` ``) (NN personhood) ('' ''))
                    (VP (VBZ is))))
                (CC and)
                (SBAR
                  (WHADVP (WRB when))
                  (S
                    (NP (PRP it))
                    (VP (VBZ begins))))))))))
    (. .)))

nsubj(believe-2, I-1)
root(ROOT-0, believe-2)
