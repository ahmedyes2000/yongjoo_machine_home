  (S
    (NP (NNP Sperm))
    (VP
      (VP (VBP are)
        (ADJP (JJ motile)
          (PP (IN for)
            (NP
              (NP (DT a) (NN period))
              (PP (IN of)
                (NP (NN time)))))))
      (, ,)
      (CC but)
      (ADVP (RB so))
      (VP (VBZ is)
        (NP
          (NP (DT a) (NN lizard) (POS 's))
          (NN tail))))
    (. .)))

nsubj(motile-3, Sperm-1)
cop(motile-3, are-2)
root(ROOT-0, motile-3)
