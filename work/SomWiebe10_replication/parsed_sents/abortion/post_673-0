  (S
    (S
      (NP (PRP You))
      (VP (VBP have) (RB n't)
        (VP (VBN given)
          (NP (DT any) (NN reason)
            (S
              (VP (TO to)
                (VP (VB rule)
                  (PRT (RP out))
                  (NP (NN conception))
                  (PP (IN as)
                    (S
                      (VP (VBG being)
                        (ADJP (JJ sufficient))))))))))))
    (, ,)
    (CC and)
    (S
      (NP (PRP you))
      (VP (VBP have)
        (VP (VBN allowed)
          (SBAR (IN that)
            (S
              (NP (PRP it))
              (VP (VBZ is)
                (ADJP (JJ necessary))))))))
    (. .)))

