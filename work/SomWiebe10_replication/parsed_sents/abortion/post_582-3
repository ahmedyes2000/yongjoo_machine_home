  (S
    (S
      (NP (DT The) (NN issue))
      (ADVP (RB here))
      (VP (VBZ is)
        (SBAR (IN whether)
          (S
            (NP (NN abortion))
            (VP (VBZ destroys)
              (NP (DT a) (NN life)))))))
    (, ,)
    (CC and)
    (S
      (NP (PRP it))
      (ADVP (RB clearly))
      (VP (VBZ does)))
    (. .)))

det(issue-2, The-1)
nsubj(is-4, issue-2)
advmod(is-4, here-3)
root(ROOT-0, is-4)
