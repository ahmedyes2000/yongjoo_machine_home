(ROOT
  (S
    (ADVP (RB Obviously))
    (, ,)
    (PP (IN as)
      (NP (DT a) (NN nation)))
    (NP (PRP we))
    (VP (VBP have)
      (VP (VBN failed)
        (NP (DT these) (NNS women))))
    (. .)))

advmod(failed-8, Obviously-1)
det(nation-5, a-4)
prep_as(failed-8, nation-5)
nsubj(failed-8, we-6)
aux(failed-8, have-7)
root(ROOT-0, failed-8)
det(women-10, these-9)
dobj(failed-8, women-10)

