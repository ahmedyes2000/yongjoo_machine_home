  (S
    (SBAR (IN If)
      (S
        (NP (EX there))
        (VP (VBD were))))
    (, ,)
    (NP (NNP Ted) (NNP Kennedy))
    (VP (MD would)
      (VP (VB be)
        (PP (IN in)
          (NP
            (NP (NN prison))
            (, ,) (RB not)
            (NP (DT the) (NNP Senate))))))
    (. .)))

