  (S
    (ADVP (RB However))
    (, ,)
    (NP (PRP they))
    (VP (MD will)
      (VP (VB tell)
        (NP (PRP you))
        (SBAR (IN that)
          (S
            (NP (DT the) (JJ same) (NN thing))
            (VP (VBZ is)
              (ADJP (JJ true)
                (PP (IN at)
                  (NP
                    (NP (DT the) (NN onset))
                    (PP (IN of)
                      (NP (DT a) (NN cancer)))))))))))
    (. .)))

