(ROOT
  (S
    (PP (IN By)
      (NP (DT the) (NN way)))
    (, ,)
    (NP (NNS fetuses))
    (VP (MD ca) (RB n't)
      (VP (VB feel)
        (NP (NN pain))
        (PP (IN until)
          (ADJP
            (NP (CD 12) (NNS weeks))
            (JJ old)))))
    (. .)))

det(way-3, the-2)
prep_by(feel-8, way-3)
nsubj(feel-8, fetuses-5)
aux(feel-8, ca-6)
neg(feel-8, n't-7)
root(ROOT-0, feel-8)
dobj(feel-8, pain-9)
num(weeks-12, 12-11)
npadvmod(old-13, weeks-12)
prep_until(feel-8, old-13)

