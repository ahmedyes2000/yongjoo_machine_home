(ROOT
  (S
    (NP (EX There))
    (VP (VBP are)
      (ADVP (RB even))
      (NP
        (NP (JJR more) (NNS reasons))
        (SBAR
          (S
            (NP (DT a) (NN man))
            (VP (MD may)
              (VP (VB choose)
                (S
                  (VP (TO to)
                    (VP (VB have)
                      (NP
                        (NP (DT a) (NN slave))
                        (SBAR
                          (WHNP (WDT that))
                          (S
                            (VP (VBP are) (RB not)
                              (ADJP (JJ good)))))))))))))))
    (. .)))

expl(are-2, There-1)
root(ROOT-0, are-2)
advmod(are-2, even-3)
amod(reasons-5, more-4)
nsubj(are-2, reasons-5)
dobj(choose-9, reasons-5)
det(man-7, a-6)
nsubj(choose-9, man-7)
xsubj(have-11, man-7)
aux(choose-9, may-8)
rcmod(reasons-5, choose-9)
aux(have-11, to-10)
xcomp(choose-9, have-11)
det(slave-13, a-12)
dobj(have-11, slave-13)
nsubj(good-17, slave-13)
cop(good-17, are-15)
neg(good-17, not-16)
rcmod(slave-13, good-17)

