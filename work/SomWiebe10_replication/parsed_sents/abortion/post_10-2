(ROOT
  (S
    (NP (NN Nobody))
    (VP (MD would)
      (ADVP (RB ever))
      (VP (VB say) (, ,) (`` ``)
        (S
          (NP (PRP I))
          (VP (VBP 'm)
            (ADJP (JJ pregnant)
              (PP (IN with)
                (NP (DT a) (NN fetus) ('' ''))))))))
    (. .)))

nsubj(say-4, Nobody-1)
aux(say-4, would-2)
advmod(say-4, ever-3)
root(ROOT-0, say-4)
nsubj(pregnant-9, I-7)
cop(pregnant-9, 'm-8)
ccomp(say-4, pregnant-9)
det(fetus-12, a-11)
prep_with(pregnant-9, fetus-12)

