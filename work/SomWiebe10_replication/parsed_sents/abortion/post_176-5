(ROOT
  (SBARQ
    (WHADVP (WRB How))
    (SQ (VBZ does)
      (NP (DT that))
      (VP (VB figure)
        (PP (IN into)
          (NP
            (NP (DT the) (NNS wonders))
            (PP (IN of)
              (NP (NN religion)))))))
    (. ?)))

advmod(figure-4, How-1)
aux(figure-4, does-2)
nsubj(figure-4, that-3)
root(ROOT-0, figure-4)
det(wonders-7, the-6)
prep_into(figure-4, wonders-7)
prep_of(wonders-7, religion-9)

