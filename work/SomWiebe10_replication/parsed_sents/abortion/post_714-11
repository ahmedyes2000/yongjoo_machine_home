(ROOT
  (S
    (NP (PRP I))
    (VP (VBP hope)
      (SBAR
        (S
          (NP (PRP I))
          (ADVP (RB satisfactorily))
          (VP (VBD answered)
            (NP (PRP$ your) (NN question))))))
    (. .)))

nsubj(hope-2, I-1)
root(ROOT-0, hope-2)
nsubj(answered-5, I-3)
advmod(answered-5, satisfactorily-4)
ccomp(hope-2, answered-5)
poss(question-7, your-6)
dobj(answered-5, question-7)

