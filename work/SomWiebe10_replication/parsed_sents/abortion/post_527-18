(ROOT
  (S
    (NP
      (NP (DT the) (JJ ultimate) (NN right))
      (PP (IN of)
        (NP
          (NP (NN choice))
          (PP (IN over)
            (NP (CD one))))))
    (VP (VBZ 's)
      (NP (JJ own) (NN body)))
    (. .)))

det(right-3, the-1)
amod(right-3, ultimate-2)
nsubj(body-10, right-3)
prep_of(right-3, choice-5)
prep_over(choice-5, one-7)
cop(body-10, 's-8)
amod(body-10, own-9)
root(ROOT-0, body-10)

