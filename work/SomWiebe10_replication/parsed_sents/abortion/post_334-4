(ROOT
  (S
    (NP (DT This) (NN action))
    (VP (VBZ causes)
      (NP (JJ significant) (NN energy)
        (S
          (VP (TO to)
            (VP (VB be)
              (VP (VBN spent)))))))
    (. .)))

det(action-2, This-1)
nsubj(causes-3, action-2)
root(ROOT-0, causes-3)
amod(energy-5, significant-4)
dobj(causes-3, energy-5)
aux(spent-8, to-6)
auxpass(spent-8, be-7)
infmod(energy-5, spent-8)

