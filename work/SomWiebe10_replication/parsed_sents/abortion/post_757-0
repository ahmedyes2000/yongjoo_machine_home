(ROOT
  (S
    (NP (RB Not) (NN everyone))
    (VP (VBZ wants)
      (S
        (VP (TO to)
          (VP (VB live)
            (NP (DT that) (NN way))))))
    (. .)))

neg(everyone-2, Not-1)
nsubj(wants-3, everyone-2)
xsubj(live-5, everyone-2)
root(ROOT-0, wants-3)
aux(live-5, to-4)
xcomp(wants-3, live-5)
det(way-7, that-6)
dobj(live-5, way-7)

