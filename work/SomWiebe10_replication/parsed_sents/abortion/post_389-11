(ROOT
  (S
    (PP (IN By)
      (NP (PRP$ its) (NN nature)))
    (, ,)
    (NP (PRP it))
    (VP (VBZ relies)
      (PP (IN on)
        (NP (PRP$ its) (NN host))))
    (. .)))

poss(nature-3, its-2)
prep_by(relies-6, nature-3)
nsubj(relies-6, it-5)
root(ROOT-0, relies-6)
poss(host-9, its-8)
prep_on(relies-6, host-9)

