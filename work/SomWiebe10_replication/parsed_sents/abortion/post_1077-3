  (S
    (NP (PRP You))
    (VP (VBP have)
      (NP (DT a) (NN right)
        (S
          (VP (TO to)
            (VP (VB choose)
              (SBAR
                (S
                  (VP (VBZ is)
                    (NP
                      (NP (PRP$ my) (NN stance))
                      (PP (IN on)
                        (NP (NN everything))))))))))))
    (. .)))

nsubj(have-2, You-1)
root(ROOT-0, have-2)
det(right-4, a-3)
