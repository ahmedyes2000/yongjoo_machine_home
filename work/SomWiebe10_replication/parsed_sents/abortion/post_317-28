(ROOT
  (S
    (PP (IN In)
      (NP (WDT which) (NN case)))
    (S
      (NP (PRP I)))
    (NP (PRP you))
    (VP (VBD had)
      (ADVP (RBR better))
      (VP (VB be)
        (VP (VBN registered)
          (CC and)
          (VBN prepared)
          (S
            (VP (TO to)
              (VP (VB donate)
                (NP
                  (NP (PRP$ your) (NN bone) (NN marrow))
                  (, ,)
                  (CC and)
                  (NP (PRP$ your) (NN kidney)))))))))
    (. .)))

dep(case-3, which-2)
prep_in(registered-9, case-3)
