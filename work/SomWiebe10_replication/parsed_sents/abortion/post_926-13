(ROOT
  (S (CC And)
    (SBAR (IN if)
      (S
        (NP (PRP you))
        (VP (VBP do))))
    (VP
      (VP (VBP have)
        (NP (DT an) (JJ unwanted) (NN pregnancy)))
      (PRN (, ,)
        (INTJ (VB Please)))
      (VP (VB consider)
        (NP (NN adoption))))
    (. .)))

cc(have-5, And-1)
mark(do-4, if-2)
nsubj(do-4, you-3)
csubj(have-5, do-4)
root(ROOT-0, have-5)
det(pregnancy-8, an-6)
amod(pregnancy-8, unwanted-7)
dobj(have-5, pregnancy-8)
dep(have-5, Please-10)
dep(have-5, consider-11)
dobj(consider-11, adoption-12)

