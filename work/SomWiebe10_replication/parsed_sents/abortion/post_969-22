(ROOT
  (S
    (NP (DT The) (NNP Bible))
    (VP (VBZ says)
      (SBAR (IN that)
        (S
          (NP (NN life))
          (VP (VBZ begins)
            (PP (IN at)
              (NP (NN conception)))
            (, ,)
            (S
              (VP (VBG meaning)
                (SBAR (IN that)
                  (S
                    (ADVP (RB right)
                      (PP (IN at)
                        (NP (NN conception))))
                    (NP (DT the) (NN baby))
                    (VP (VBZ becomes)
                      (NP
                        (NP (DT a) (VBG living) (NN human))
                        (VP (VBG being))))))))))))
    (. .)))

