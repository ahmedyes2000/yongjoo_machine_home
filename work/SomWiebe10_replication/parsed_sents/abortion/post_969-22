    (SBAR (IN Before)
      (S
        (NP (PRP I))
        (VP (VBD formed)
          (NP (PRP you))
          (PP (IN in)
            (NP (DT the) (NN womb))))))
    (, ,)
    (NP (PRP I))
    (VP (VBD knew)
      (NP (PRP you))
      (, ,)
      (SBAR (IN before)
