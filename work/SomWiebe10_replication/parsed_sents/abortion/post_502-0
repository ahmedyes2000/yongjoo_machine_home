(ROOT
  (S
    (NP (PRP I))
    (ADVP (RB personally))
    (VP (VBP do) (RB not)
      (VP (VB approve)
        (PP (IN of)
          (NP
            (NP (NN abortion))
            (SBAR
              (WHNP (WDT that))
              (S
                (VP (VBZ is)
                  (NP (PRP$ my) (NN opinion)))))))))
    (. .)))

nsubj(approve-5, I-1)
advmod(approve-5, personally-2)
aux(approve-5, do-3)
neg(approve-5, not-4)
root(ROOT-0, approve-5)
