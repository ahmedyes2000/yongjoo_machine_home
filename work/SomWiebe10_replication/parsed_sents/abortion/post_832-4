(ROOT
  (S
    (PP (VBN Given)
      (NP
        (NP (DT the) (NN choice))
        (PP (IN between)
          (S
            (VP (VBG saving)
              (NP
                (NP (DT a) (JJ young) (, ,) (JJ healthy) (NN child))
                (, ,)
                (CC and)
                (NP (DT an)
                  (ADJP (JJ old) (VBG dying))
                  (NN man))))))))
    (, ,)
    (NP (PRP I))
    (VP (MD would)
      (VP (VB have)
        (S
          (VP (TO to)
            (VP (VB choose)
              (NP (DT the) (NN child)))))))
    (. .)))

