(ROOT
  (S
    (S
      (VP (VB Do) (RB n't)))
    (NP (PRP they))
    (VP
      (ADVP (RB also))
      (VBP depend)
      (PRT (RP on))
      (NP (PRP$ his) (NN mother))
      (PP (IN for)
        (S
          (VP
            (VP (VBG sustaining))
            (-LRB- -LRB-)
            (SBAR (RB just) (IN as)
              (S
                (NP (DT the) (NN fetus))
                (VP (VBZ depends)
                  (PP (IN on)
                    (NP
                      (NP (DT the) (NN mother))
                      (PP (IN on)
                        (NP (DT this) (CD one) (NN thing))))))))
            (-RRB- -RRB-)))))
    (. .)))

