(ROOT
  (S (CC And)
    (ADVP (RB yet) (RB again))
    (, ,)
    (NP (RB not) (DT a) (JJ single) (NN abortion))
    (VP (MD would)
      (VP (VB have)
        (VP (VBN been)
          (VP (VBN stopped)))))
    (. .)))

cc(stopped-12, And-1)
advmod(again-3, yet-2)
advmod(stopped-12, again-3)
neg(abortion-8, not-5)
det(abortion-8, a-6)
amod(abortion-8, single-7)
nsubjpass(stopped-12, abortion-8)
aux(stopped-12, would-9)
aux(stopped-12, have-10)
auxpass(stopped-12, been-11)
root(ROOT-0, stopped-12)

