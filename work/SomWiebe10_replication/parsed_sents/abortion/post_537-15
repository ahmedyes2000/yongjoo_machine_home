(ROOT
  (S
    (NP (PRP I))
    (ADVP (RB also))
    (VP (VBP realize)
      (SBAR (IN that)
        (S
          (NP (PRP it))
          (VP (VBZ is)
            (ADJP (RB psychologically) (JJ satisfying))
            (S
              (VP (TO to)
                (VP (VB have)
                  (NP (JJ simple) (JJ black-and-white) (NNS answers))
                  (PP (TO to)
                    (NP (JJ important) (NNS issues))))))))))
    (. .)))

nsubj(realize-3, I-1)
advmod(realize-3, also-2)
root(ROOT-0, realize-3)
complm(satisfying-8, that-4)
nsubj(satisfying-8, it-5)
xsubj(have-10, it-5)
cop(satisfying-8, is-6)
advmod(satisfying-8, psychologically-7)
ccomp(realize-3, satisfying-8)
aux(have-10, to-9)
xcomp(satisfying-8, have-10)
amod(answers-13, simple-11)
amod(answers-13, black-and-white-12)
dobj(have-10, answers-13)
amod(issues-16, important-15)
prep_to(have-10, issues-16)

