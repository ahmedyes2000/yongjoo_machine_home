(ROOT
  (NP
    (NP (JJ Fourth) (NN paragraph))
    (: ;)
    (S
      (NP (EX there))
      (VP (VBZ is)
        (SBAR
          (SBAR
            (WHNP (WDT that))
            (S
              (VP (VBP question)
                (ADVP (RB again)))))
          (: ;)
          (SBAR
            (WHNP (WP what))
            (S
              (VP (VBZ is)
                (NP (PRP it))))))))
    (. ?)))

amod(paragraph-2, Fourth-1)
root(ROOT-0, paragraph-2)
expl(is-5, there-4)
dep(paragraph-2, is-5)
nsubj(question-7, that-6)
dep(is-5, question-7)
advmod(question-7, again-8)
nsubj(it-12, what-10)
cop(it-12, is-11)
dep(question-7, it-12)

