(ROOT
  (S
    (NP
      (NP (DT All))
      (SBAR
        (S
          (NP (PRP I))
          (VP (VBD said)
            (SBAR
              (S
                (VP (VBD was)
                  (NP (PRP you)))))))))
    (VP (VBD misapplied)
      (NP
        (NP (DT the) (NN concept))
        (PP (IN of)
          (NP (NN potentiality)))))
    (. .)))

nsubj(misapplied-6, All-1)
nsubj(said-3, I-2)
rcmod(All-1, said-3)
cop(you-5, was-4)
ccomp(said-3, you-5)
root(ROOT-0, misapplied-6)
det(concept-8, the-7)
