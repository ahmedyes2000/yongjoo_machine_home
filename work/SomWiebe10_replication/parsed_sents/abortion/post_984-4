(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP I))
        (VP (VBD did))))
    (, ,)
    (NP (PRP I))
    (VP (MD would) (RB n't)
      (VP (VB be)
        (NP (DT a) (NNP Christian))
        (SBAR (IN because)
          (S
            (NP (PRP I))
            (VP (MD would)
              (VP (VB be)
                (VP (VBG contradicting)
                  (NP
                    (NP (PRP myself))
                    (CC and)
                    (NP (PRP$ my) (NNS beliefs))))))))))
    (. .)))

mark(did-3, If-1)
nsubj(did-3, I-2)
advcl(Christian-10, did-3)
