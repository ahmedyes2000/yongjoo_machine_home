(ROOT
  (S
    (NP
      (NP (DT The) (JJ short) (NN life) (NN expectancy))
      (PP (IN of)
        (NP (DT a) (JJ disabled) (NN child))))
    (VP (VBZ does) (RB not)
      (VP (VB justify)
        (NP (PRP$ his)
          (ADJP (JJ deliberate))
          (NN killing))))
    (. !)))

det(expectancy-4, The-1)
amod(expectancy-4, short-2)
nn(expectancy-4, life-3)
nsubj(justify-11, expectancy-4)
det(child-8, a-6)
amod(child-8, disabled-7)
prep_of(expectancy-4, child-8)
aux(justify-11, does-9)
neg(justify-11, not-10)
root(ROOT-0, justify-11)
poss(killing-14, his-12)
amod(killing-14, deliberate-13)
dobj(justify-11, killing-14)

