(ROOT
  (S
    (S (IN So)
      (NP (PRP I))
      (VP (VBP say)))
    (, ,)
    (NP (PRP it))
    (VP (VBP ai) (RB n't)
      (NP (DT a) (NN chicken)))
    (. .)))

dep(say-3, So-1)
nsubj(say-3, I-2)
ccomp(ai-6, say-3)
nsubj(ai-6, it-5)
root(ROOT-0, ai-6)
neg(ai-6, n't-7)
det(chicken-9, a-8)
