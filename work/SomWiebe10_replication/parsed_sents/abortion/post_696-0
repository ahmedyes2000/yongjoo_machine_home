(ROOT
  (S
    (ADVP (RB Actually))
    (, ,)
    (NP (PRP you))
    (VP (VBP 're) (RB not)
      (VP (VBN allowed)
        (S
          (VP (TO to)
            (VP (VB make)
              (PRT (RP up))
              (NP
                (NP (NNS definitions))
                (PP (IN of)
                  (NP (NNS words))))
              (S
                (VP (TO to)
                  (VP (VB suit)
                    (NP
                      (NP (PRP$ your) (NN argument))
                      (CC or)
                      (NP (PRP$ your) (NN prejudice)))))))))))
    (. .)))

advmod(allowed-6, Actually-1)
nsubjpass(allowed-6, you-3)
xsubj(make-8, you-3)
auxpass(allowed-6, 're-4)
neg(allowed-6, not-5)
root(ROOT-0, allowed-6)
aux(make-8, to-7)
xcomp(allowed-6, make-8)
prt(make-8, up-9)
dobj(make-8, definitions-10)
prep_of(definitions-10, words-12)
aux(suit-14, to-13)
xcomp(make-8, suit-14)
poss(argument-16, your-15)
dobj(suit-14, argument-16)
poss(prejudice-19, your-18)
dobj(suit-14, prejudice-19)
conj_or(argument-16, prejudice-19)

