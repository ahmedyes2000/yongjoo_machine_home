(ROOT
  (S
    (VP (VB Thank)
      (NP (PRP you))
      (ADVP (RB once) (RB again))
      (PP (IN for)
        (S
          (VP (VBG making)
            (NP (PRP$ my) (NN case))))))
    (. .)))

root(ROOT-0, Thank-1)
dobj(Thank-1, you-2)
advmod(again-4, once-3)
advmod(Thank-1, again-4)
prepc_for(Thank-1, making-6)
poss(case-8, my-7)
dobj(making-6, case-8)

