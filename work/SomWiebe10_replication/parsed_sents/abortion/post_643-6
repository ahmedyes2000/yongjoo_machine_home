(ROOT
  (S
    (NP (DT Neither) (NN side))
    (VP (MD would)
      (VP (VB have)
        (NP (DT an) (NN advantage))
        (PP (IN beyond)
          (NP (DT the) (NN fact)))
        (SBAR (IN that)
          (S
            (NP (DT some) (NNS states))
            (ADVP (RB clearly))
            (VP (VB trend)
              (PP (IN towards)
                (NP
                  (NP (CD one) (NN position))
                  (CC or)
                  (NP (DT the) (JJ other)))))))))
    (. .)))

det(side-2, Neither-1)
nsubj(have-4, side-2)
aux(have-4, would-3)
root(ROOT-0, have-4)
det(advantage-6, an-5)
dobj(have-4, advantage-6)
det(fact-9, the-8)
prep_beyond(have-4, fact-9)
complm(trend-14, that-10)
det(states-12, some-11)
nsubj(trend-14, states-12)
advmod(trend-14, clearly-13)
ccomp(have-4, trend-14)
num(position-17, one-16)
prep_towards(trend-14, position-17)
det(other-20, the-19)
prep_towards(trend-14, other-20)
conj_or(position-17, other-20)

