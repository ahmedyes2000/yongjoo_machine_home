(ROOT
  (S
    (NP (PRP You))
    (VP (VBP seem)
      (S
        (VP (TO to)
          (VP (VB think)
            (SBAR (IN that)
              (S
                (S
                  (VP
                    (VP
                      (ADVP (RB just))
                      (VBG telling)
                      (S
                        (NP (PRP them))
                        (RB not)
                        (VP (TO to)
                          (VP (VB do)
                            (NP (PRP it))))))
                    (CC and)
                    (VP (VBG crossing)
                      (NP (PRP$ our) (NNS fingers)))))
                (VP (VBZ is)
                  (ADJP (JJS best)))))))))
    (. .)))

nsubj(seem-2, You-1)
xsubj(think-4, You-1)
root(ROOT-0, seem-2)
aux(think-4, to-3)
xcomp(seem-2, think-4)
