(ROOT
  (S
    (PP (IN For)
      (NP (DT these) (NNS reasons)))
    (, ,)
    (NP (PRP they))
    (VP (MD should) (RB not)
      (VP (VB be)
        (VP (VBN aborted))))
    (. .)))

det(reasons-3, these-2)
prep_for(aborted-9, reasons-3)
nsubjpass(aborted-9, they-5)
aux(aborted-9, should-6)
neg(aborted-9, not-7)
auxpass(aborted-9, be-8)
root(ROOT-0, aborted-9)

