(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ 's)
      (NP
        (NP (DT a) (NN degradation))
        (SBAR
          (WHADVP (WRB when))
          (S
            (NP (PRP it))
            (VP (VBZ deviates)
              (PP (IN from)
                (NP
                  (NP (DT the) (NN purpose))
                  (SBAR
                    (WHPP (IN for)
                      (WHNP (WDT which)))
                    (S
                      (NP (PRP it))
                      (VP (VBD was)
                        (VP (VBN designed))))))))))))
    (. .)))

nsubj(degradation-4, It-1)
cop(degradation-4, 's-2)
det(degradation-4, a-3)
root(ROOT-0, degradation-4)
tmod(deviates-7, degradation-4)
advmod(deviates-7, when-5)
nsubj(deviates-7, it-6)
dep(degradation-4, deviates-7)
det(purpose-10, the-9)
