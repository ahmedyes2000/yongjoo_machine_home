(ROOT
  (S
    (NP (PRP I))
    (ADVP (RB also))
    (VP (VBP think)
      (SBAR (IN that)
        (S
          (S
            (VP (VBG making)
              (S
                (NP (NN abortion))
                (ADJP (JJ illegal)))))
          (VP (VBZ is)
            (NP
              (NP (DT the) (JJS best) (NN solution))
              (SBAR (IN if)
                (S
                  (NP (PRP we))
                  (VP (VBP want)
                    (NP (NN abortion))
                    (S
                      (VP (TO to)
                        (VP (VB end))))))))))))
    (. .)))

nsubj(think-3, I-1)
advmod(think-3, also-2)
root(ROOT-0, think-3)
complm(solution-11, that-4)
csubj(solution-11, making-5)
nsubj(illegal-7, abortion-6)
xcomp(making-5, illegal-7)
cop(solution-11, is-8)
det(solution-11, the-9)
amod(solution-11, best-10)
ccomp(think-3, solution-11)
mark(want-14, if-12)
