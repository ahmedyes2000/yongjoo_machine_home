(ROOT
  (FRAG
    (ADVP (RB As)
      (PP (IN with)
        (NP (NN anything))))
    (SBAR
      (WHADVP (WRB when))
      (S
        (NP (PRP it))
        (VP (VBZ is) (RB not)
          (VP (VBN used)
            (ADVP (RB properly))))))
    (. .)))

root(ROOT-0, As-1)
prep_with(As-1, anything-3)
advmod(used-8, when-4)
nsubjpass(used-8, it-5)
auxpass(used-8, is-6)
neg(used-8, not-7)
dep(As-1, used-8)
advmod(used-8, properly-9)

