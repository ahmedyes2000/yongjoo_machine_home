(ROOT
  (S (CC But)
    (NP (PRP I))
    (ADVP (RB truly))
    (VP (VBP believe)
      (SBAR
        (S
          (NP (DT the) (NN answer))
          (VP (VBZ is)
            (NP
              (NP (CD one))
              (PP (IN of)
                (NP (NN compromise))))))))
    (. .)))

cc(believe-4, But-1)
nsubj(believe-4, I-2)
advmod(believe-4, truly-3)
root(ROOT-0, believe-4)
det(answer-6, the-5)
nsubj(one-8, answer-6)
cop(one-8, is-7)
ccomp(believe-4, one-8)
prep_of(one-8, compromise-10)

