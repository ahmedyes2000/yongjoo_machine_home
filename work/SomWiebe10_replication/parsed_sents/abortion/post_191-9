  (S
    (NP (PRP We))
    (VP (MD should)
      (VP (VB strive)
        (S
          (VP (TO to)
            (VP (VB educate)
              (NP (NNS people))
              (PP (IN about)
                (NP (NN sex)
                  (CC and)
                  (NN contraception)))
              (SBAR (RB so) (IN that)
                (S
                  (NP
                    (NP (DT those))
                    (SBAR
                      (WHNP (WP who))
                      (S
                        (VP (VBP want)
                          (S
                            (VP (TO to)
                              (VP (VB have)
                                (NP (DT a) (NN baby)))))))))
                  (VP (VBP have)
                    (NP (PRP them))
