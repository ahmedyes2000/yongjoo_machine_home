(ROOT
  (S
    (NP
      (NP (DT No) (JJ real) (NN scientist))
      (CC or)
      (NP (NN doctor)))
    (VP (MD would)
      (VP (VB deny)
        (NP (DT that))))
    (. .)))

det(scientist-3, No-1)
amod(scientist-3, real-2)
nsubj(deny-7, scientist-3)
conj_or(scientist-3, doctor-5)
nsubj(deny-7, doctor-5)
aux(deny-7, would-6)
root(ROOT-0, deny-7)
dobj(deny-7, that-8)

