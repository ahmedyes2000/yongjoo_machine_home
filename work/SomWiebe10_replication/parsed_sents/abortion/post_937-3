(ROOT
  (NP
    (NP (DT The) (CD two) (JJS youngest))
    (SBAR
      (S
        (NP (PRP I))
        (VP (VBP remember)
          (NP (PRP$ their) (NN life))
          (ADVP (RB clearly)))))
    (. .)))

det(two-2, The-1)
root(ROOT-0, two-2)
amod(two-2, youngest-3)
nsubj(remember-5, I-4)
rcmod(two-2, remember-5)
poss(life-7, their-6)
dobj(remember-5, life-7)
advmod(remember-5, clearly-8)

