(ROOT
  (S
    (NP (DT The) (JJ only) (NN one))
    (NP (PRP you))
    (VP (VBP have)
      (S
        (VP (TO to)
          (VP (VB answer)
            (PP (TO to)
              (NP (PRP$ his)))
            (NP (PRP him))))))
    (. .)))

det(one-3, The-1)
amod(one-3, only-2)
nsubj(have-5, one-3)
xsubj(answer-7, one-3)
nsubj(have-5, you-4)
xsubj(answer-7, you-4)
root(ROOT-0, have-5)
aux(answer-7, to-6)
xcomp(have-5, answer-7)
prep_to(answer-7, his-9)
dobj(answer-7, him-10)

