(ROOT
  (FRAG (RB Not)
    (NP
      (NP (DT a) (NN choice))
      (PP (IN in)
        (CC and)
        (IN of)
        (NP (PRP itself))))
    (. .)))

root(ROOT-0, Not-1)
det(choice-3, a-2)
dep(Not-1, choice-3)
dep(Not-1, choice-3')
conj_and(choice-3, choice-3')
prep_in(choice-3, itself-7)
prep_of(choice-3', itself-7)

