(ROOT
  (S
    (ADVP (RB Yeah) (RB right))
    (, ,)
    (VP (VB try)
      (NP (DT that) (NN defense))
      (PP (IN in)
        (NP
          (NP (DT a) (NN court))
          (PP (IN of)
            (NP (NN law))))))
    (. .)))

advmod(right-2, Yeah-1)
advmod(try-4, right-2)
root(ROOT-0, try-4)
det(defense-6, that-5)
dobj(try-4, defense-6)
det(court-9, a-8)
prep_in(try-4, court-9)
prep_of(court-9, law-11)

