(ROOT
  (S (IN For)
    (NP
      (NP (DT some) (NNS women))
      (VP (VBG having)
        (NP
          (NP (DT the) (NN rapist) (POS 's))
          (NN child))))
    (VP (MD would)
      (VP (VB be)
        (ADJP (RBR more) (JJ traumatizing)
          (PP (IN than)
            (NP (NN abortion))))))
    (. .)))

dep(traumatizing-12, For-1)
det(women-3, some-2)
nsubj(traumatizing-12, women-3)
partmod(women-3, having-4)
det(rapist-6, the-5)
poss(child-8, rapist-6)
dobj(having-4, child-8)
aux(traumatizing-12, would-9)
cop(traumatizing-12, be-10)
advmod(traumatizing-12, more-11)
root(ROOT-0, traumatizing-12)
prep_than(traumatizing-12, abortion-14)

(ROOT
  (S
    (NP (PRP You))
    (VP (VBP are)
      (NP (DT a)
        (ADJP (RB very) (JJ brave)
          (CC and)
          (JJ strong))
        (NN woman)))
    (. .)))

nsubj(woman-8, You-1)
cop(woman-8, are-2)
det(woman-8, a-3)
advmod(brave-5, very-4)
amod(woman-8, brave-5)
conj_and(brave-5, strong-7)
amod(woman-8, strong-7)
root(ROOT-0, woman-8)

