(ROOT
  (S
    (NP (PRP I))
    (VP (VBP support)
      (NP
        (NP (NNS groups))
        (SBAR
          (WHNP (WDT that))
          (S
            (VP (VBP help)
              (NP (NNS women))
              (SBAR
                (WHADVP (WRB when))
                (S
                  (NP (PRP they))
                  (VP (VBP are)
                    (PP (IN in)
                      (NP (NN crisis) (NN pregnancy) (NNS situations)))))))))))
    (. .)))

nsubj(support-2, I-1)
root(ROOT-0, support-2)
dobj(support-2, groups-3)
nsubj(help-5, groups-3)
rcmod(groups-3, help-5)
dobj(help-5, women-6)
advmod(are-9, when-7)
nsubj(are-9, they-8)
advcl(help-5, are-9)
nn(situations-13, crisis-11)
nn(situations-13, pregnancy-12)
prep_in(are-9, situations-13)

