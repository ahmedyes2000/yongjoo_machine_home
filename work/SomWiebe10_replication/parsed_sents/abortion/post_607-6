(ROOT
  (S
    (NP (DT This))
    (VP (VBZ is)
      (NP
        (NP (DT a) (NN situation))
        (SBAR
          (WHPP (IN in)
            (WHNP (WDT which)))
          (S
            (NP
              (NP (DT the) (NNS rights))
              (PP (IN of)
                (NP (CD one) (NN being))))
            (VP (MD will)
              (VP (VB HAVE)
                (S
                  (VP (TO to)
                    (VP (VB supercede)
                      (NP
                        (NP (DT the) (NNS rights))
                        (PP (IN of)
                          (NP (DT another)))))))))))))
    (. .)))

nsubj(situation-4, This-1)
cop(situation-4, is-2)
det(situation-4, a-3)
root(ROOT-0, situation-4)
rel(HAVE-13, which-6)
det(rights-8, the-7)
nsubj(HAVE-13, rights-8)
xsubj(supercede-15, rights-8)
num(being-11, one-10)
prep_of(rights-8, being-11)
aux(HAVE-13, will-12)
rcmod(situation-4, HAVE-13)
aux(supercede-15, to-14)
xcomp(HAVE-13, supercede-15)
det(rights-17, the-16)
dobj(supercede-15, rights-17)
prep_of(rights-17, another-19)

