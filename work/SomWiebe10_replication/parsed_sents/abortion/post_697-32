(ROOT
  (S (IN So)
    (NP (DT that))
    (VP (VBZ is)
      (NP
        (NP (DT a) (NN concept))
        (SBAR
          (S
            (NP (PRP we))
            (VP (MD should)
              (VP (VB stay)
                (PRT (RP away))
                (PP (IN from))))))))
    (. .)))

dep(concept-5, So-1)
nsubj(concept-5, that-2)
cop(concept-5, is-3)
det(concept-5, a-4)
root(ROOT-0, concept-5)
prep_away_from(stay-8, concept-5)
nsubj(stay-8, we-6)
aux(stay-8, should-7)
rcmod(concept-5, stay-8)

