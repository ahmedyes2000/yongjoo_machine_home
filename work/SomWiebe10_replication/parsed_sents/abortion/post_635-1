(ROOT
  (S
    (NP (PRP$ Our) (NNS laws))
    (VP (VBP are)
      (VP (VBN based)
        (PP (IN upon)
          (NP
            (NP (DT the) (JJ personal) (NNS beliefs))
            (PP (IN of)
              (NP
                (NP (DT the) (NNS members))
                (PP (IN of)
                  (NP (PRP$ our) (NN society)))
                (, ,)
                (ADJP (NN past)
                  (CC and)
                  (NN present))))))))
    (. .)))

poss(laws-2, Our-1)
nsubjpass(based-4, laws-2)
auxpass(based-4, are-3)
root(ROOT-0, based-4)
det(beliefs-8, the-6)
amod(beliefs-8, personal-7)
prep_upon(based-4, beliefs-8)
det(members-11, the-10)
prep_of(beliefs-8, members-11)
poss(society-14, our-13)
prep_of(members-11, society-14)
amod(members-11, past-16)
amod(members-11, present-18)
conj_and(past-16, present-18)

