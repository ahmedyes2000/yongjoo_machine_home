(ROOT
  (S
    (NP (JJ Many\/most) (NNS abortions))
    (VP (VBP are)
      (VP (VBN chosen)
        (PP (RB because) (IN of)
          (NP (JJ financial) (NNS reasons)))))
    (. .)))

amod(abortions-2, Many\/most-1)
nsubjpass(chosen-4, abortions-2)
auxpass(chosen-4, are-3)
root(ROOT-0, chosen-4)
amod(reasons-8, financial-7)
prep_because_of(chosen-4, reasons-8)

