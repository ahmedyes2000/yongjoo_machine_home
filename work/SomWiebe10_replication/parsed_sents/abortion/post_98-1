(ROOT
  (S
    (NP (DT This)
      (ADJP (JJ arrogant)
        (CC and)
        (JJ sinister))
      (NN presumption))
    (VP (VBZ is)
      (ADJP (JJ impossible)
        (S
          (VP (TO to)
            (VP (VB justify)))))
      (, ,)
      (PP (VBN given)
        (SBAR (IN that)
          (S
            (NP
              (NP (JJ many) (NNS people))
              (PP (IN with)
                (NP (NNS disabilities))))
            (VP (VBP lead)
              (S
                (VP (VBG fulfilling)
                  (NP (NNS lives)))))))))
    (. .)))

det(presumption-5, This-1)
amod(presumption-5, arrogant-2)
conj_and(arrogant-2, sinister-4)
amod(presumption-5, sinister-4)
