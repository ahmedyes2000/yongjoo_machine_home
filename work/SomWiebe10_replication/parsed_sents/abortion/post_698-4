(ROOT
  (S
    (NP (PRP You))
    (VP (MD should)
      (VP (VB apologize)
        (PP (IN for)
          (NP
            (NP (PRP$ your) (JJ bad) (NN choice))
            (PP (IN of)
              (NP (NN wording)))))))
    (. .)))

nsubj(apologize-3, You-1)
aux(apologize-3, should-2)
root(ROOT-0, apologize-3)
poss(choice-7, your-5)
amod(choice-7, bad-6)
prep_for(apologize-3, choice-7)
prep_of(choice-7, wording-9)

