(ROOT
  (S
    (SBAR (RB Just) (IN as)
      (S
        (NP (DT a) (NN side))
        (VP (VBP note))))
    (, ,)
    (NP
      (NP (DT a) (NN lot))
      (PP (IN of)
        (NP (DT this))))
    (VP (MD could)
      (VP (VB be)
        (VP (VBN said)
          (PP (IN about)
            (S
              (VP (VBG giving)
                (NP (NN birth))))))))
    (. .)))

advmod(note-5, Just-1)
dep(note-5, as-2)
det(side-4, a-3)
nsubj(note-5, side-4)
advcl(said-13, note-5)
det(lot-8, a-7)
nsubjpass(said-13, lot-8)
prep_of(lot-8, this-10)
aux(said-13, could-11)
auxpass(said-13, be-12)
root(ROOT-0, said-13)
prepc_about(said-13, giving-15)
dobj(giving-15, birth-16)

