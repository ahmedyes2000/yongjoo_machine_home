(ROOT
  (S
    (NP (PRP You))
    (VP (VBP are)
      (PP (IN in)
        (NP
          (NP (DT a) (NN beg))
          (NP (DT the) (NN question) (NN situation)))))
    (. .)))

nsubj(are-2, You-1)
root(ROOT-0, are-2)
det(beg-5, a-4)
prep_in(are-2, beg-5)
det(situation-8, the-6)
nn(situation-8, question-7)
dep(beg-5, situation-8)

