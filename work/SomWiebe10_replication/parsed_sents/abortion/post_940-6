(ROOT
  (S (CC But)
    (SBAR (RB even)
      (S
        (NP
          (NP (DT the) (NNS states))
          (SBAR
            (WHNP (WDT that))
            (S
              (VP (VBP do)))))
        (VP (VBP allow)
          (NP (PRP it))
          (PRT (RP up))
          (PP (TO to)
            (NP (NN birth))))))
    (, ,)
    (ADVP (RB again))
    (NP (PRP it))
    (VP (VBZ is)
      (ADVP (RB only))
      (PP (IN in)
        (NP (JJ extreme) (NNS circumstances))))
    (. .)))

cc(is-15, But-1)
advmod(allow-7, even-2)
det(states-4, the-3)
nsubj(do-6, states-4)
nsubj(allow-7, states-4)
rcmod(states-4, do-6)
dep(is-15, allow-7)
dobj(allow-7, it-8)
prt(allow-7, up-9)
prep_to(allow-7, birth-11)
advmod(is-15, again-13)
nsubj(is-15, it-14)
root(ROOT-0, is-15)
advmod(is-15, only-16)
amod(circumstances-19, extreme-18)
prep_in(is-15, circumstances-19)

