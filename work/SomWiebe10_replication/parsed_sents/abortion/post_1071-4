(ROOT
  (S
    (NP (DT The) (JJ whole) (NN argument))
    (VP (VBZ is)
      (SBAR (IN whether)
        (S
          (NP (PRP it))
          (VP (VBZ is)
            (NP (DT a) (NN person))))
        (CC or)
        (RB not)))
    (. .)))

det(argument-3, The-1)
amod(argument-3, whole-2)
nsubj(is-4, argument-3)
root(ROOT-0, is-4)
complm(person-9, whether-5)
nsubj(person-9, it-6)
cop(person-9, is-7)
det(person-9, a-8)
ccomp(is-4, person-9)
ccomp(is-4, not-11)
conj_or(person-9, not-11)

