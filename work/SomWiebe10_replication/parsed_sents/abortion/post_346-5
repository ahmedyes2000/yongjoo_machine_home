(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP we))
        (VP (VBP agree)
          (PP (IN with)
            (NP (DT this))))))
    (NP (PRP we))
    (VP (VBP have)
      (S
        (VP (TO to)
          (VP (VB give)
            (NP (DT all) (NNS humans))
            (NP (DT the) (JJ same) (NNS rights))))))
    (. .)))

mark(agree-3, If-1)
nsubj(agree-3, we-2)
advcl(have-7, agree-3)
prep_with(agree-3, this-5)
nsubj(have-7, we-6)
xsubj(give-9, we-6)
root(ROOT-0, have-7)
aux(give-9, to-8)
xcomp(have-7, give-9)
det(humans-11, all-10)
iobj(give-9, humans-11)
det(rights-14, the-12)
amod(rights-14, same-13)
dobj(give-9, rights-14)

