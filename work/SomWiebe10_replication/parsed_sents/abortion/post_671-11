(ROOT
  (S
    (PP (IN Except)
      (NP (DT that)))
    (NP (PRP it))
    (VP (VBZ has)
      (NP
        (NP (DT the) (NN potential))
        (PP (IN of)
          (S
            (VP (VBG developing)
              (PP (IN into)
                (NP (DT a) (NN person))))))))
    (. .)))

prep_except(has-4, that-2)
nsubj(has-4, it-3)
root(ROOT-0, has-4)
det(potential-6, the-5)
dobj(has-4, potential-6)
prepc_of(potential-6, developing-8)
det(person-11, a-10)
prep_into(developing-8, person-11)

