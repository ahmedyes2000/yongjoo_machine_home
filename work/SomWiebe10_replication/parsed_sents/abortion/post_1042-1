(ROOT
  (S
    (ADVP (RB However))
    (, ,)
    (PP (IN in)
      (NP (DT a) (NN sense)))
    (NP (PRP we))
    (VP (MD can)
      (ADVP (RB irrationally))
      (VP (VB argue)
        (NP (NN everything))))
    (. !)))

