(ROOT
  (S
    (NP (DT The) (NN sperm)
      (CC and)
      (NN egg))
    (VP (VBP have)
      (NP (NN DNA) (NNS patterns))
      (PP (IN from)
        (NP (DT the) (NNS parents))))
    (. .)))

det(sperm-2, The-1)
nsubj(have-5, sperm-2)
conj_and(sperm-2, egg-4)
nsubj(have-5, egg-4)
root(ROOT-0, have-5)
nn(patterns-7, DNA-6)
dobj(have-5, patterns-7)
det(parents-10, the-9)
prep_from(have-5, parents-10)

