(ROOT
  (S
    (PP (IN By)
      (NP (DT the) (NN way)))
    (, ,)
    (NP (PRP I))
    (VP (MD will)
      (VP (VB state)
        (NP (PRP$ my) (NNS axioms))
        (: :)
        (FRAG
          (NP
            (NP (NNP Existence))
            (, ,)
            (NP (NN identity))
            (, ,)
            (NP (NN consciousness))))))
    (. .)))

det(way-3, the-2)
prep_by(state-7, way-3)
nsubj(state-7, I-5)
aux(state-7, will-6)
root(ROOT-0, state-7)
poss(axioms-9, my-8)
dobj(state-7, axioms-9)
dep(state-7, Existence-11)
appos(Existence-11, identity-13)
appos(Existence-11, consciousness-15)

