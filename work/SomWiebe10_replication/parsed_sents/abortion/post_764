(ROOT
  (S
    (NP
      (NP (NNS People))
      (SBAR
        (WHNP (WP who))
        (S
          (VP (VBP want)
            (SBAR
              (S
                (NP (PRP$ their) (JJ pro-life) (NN position))
                (VP
                  (VP (VBD made)
                    (NP (NN law)))
                  (, ,)
                  (CC but)
                  (VP (VBP want)
                    (NP (DT the) (NN government))
                    (S
                      (VP (TO to)
                        (VP (VB stay)
                          (PRT (RP out))
                          (PP (IN of)
                            (NP (PRP$ their) (JJ personal) (NN life))))))))))))))
    (VP (VBP are)
      (ADJP (JJ hypocrits)))
    (. .)))

nsubj(want-3, People-1)
nsubj(hypocrits-22, People-1)
rcmod(People-1, want-3)
poss(position-6, their-4)
amod(position-6, pro-life-5)
nsubj(made-7, position-6)
nsubj(want-11, position-6)
ccomp(want-3, made-7)
dobj(made-7, law-8)
ccomp(want-3, want-11)
conj_but(made-7, want-11)
det(government-13, the-12)
dobj(want-11, government-13)
aux(stay-15, to-14)
xcomp(want-11, stay-15)
poss(life-20, their-18)
amod(life-20, personal-19)
prep_out_of(stay-15, life-20)
cop(hypocrits-22, are-21)
root(ROOT-0, hypocrits-22)

(ROOT
  (S
    (VP (VB Keep)
      (NP (PRP$ your) (JJ religious) (NNS opinions))
      (PP (TO to)
        (NP (PRP yourself)))
      (, ,)
      (NP
        (NP (RB especially) (VBG concerning) (NNS decisions))
        (SBAR
          (WHNP (WDT that))
          (S
            (VP (VBP do) (RB n't)
              (VP (VB effect)
                (NP (PRP you))))))))
    (. .)))

