(ROOT
  (S
    (NP
      (NP (DT The) (JJ defective) (NNS conditions))
      (PP (IN of)
        (NP (NN life))))
    (VP (VBP call)
      (ADVP (RB urgently))
      (PP (IN for)
        (NP (NN improvement))))
    (. .)))

det(conditions-3, The-1)
amod(conditions-3, defective-2)
nsubj(call-6, conditions-3)
prep_of(conditions-3, life-5)
root(ROOT-0, call-6)
advmod(call-6, urgently-7)
prep_for(call-6, improvement-9)

