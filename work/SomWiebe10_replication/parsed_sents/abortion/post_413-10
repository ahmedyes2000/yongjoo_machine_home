(ROOT
  (FRAG
    (NP
      (NP (NNP Life))
      (, ,)
      (NP (NN humanity))
      (, ,)
      (NP (NN person)))
    (: :)
    (SINV (VBP are)
      (NP
        (NP (DT these))
        (NP (DT the) (JJ same) (NN thing))))
    (. ?)))

root(ROOT-0, Life-1)
appos(Life-1, humanity-3)
appos(Life-1, person-5)
dep(these-8, are-7)
dep(Life-1, these-8)
det(thing-11, the-9)
amod(thing-11, same-10)
dep(these-8, thing-11)

