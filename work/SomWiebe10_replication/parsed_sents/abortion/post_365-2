(ROOT
  (S
    (NP (PRP I))
    (VP (VBP do) (RB n't)
      (VP (VB care)
        (SBAR (IN if)
          (S
            (NP (NNS trees)
              (CC or)
              (NNS chickens))
            (VP (VBP are)
              (VP (VBN aborted)
                (PP (IN by)
                  (NP (PRP$ their) (NNS mothers)))
                (PRN (-LRB- -LRB-)
                  (S
                    (VP (VB wich)
                      (PP (IN by)
                        (NP
                          (NP (DT the) (NN way))
                          (SBAR
                            (S
                              (NP (DT no) (NN animal) (NN specie))
                              (VP (VBP do))))))))
                  (-RRB- -RRB-))))))))
    (. .)))

