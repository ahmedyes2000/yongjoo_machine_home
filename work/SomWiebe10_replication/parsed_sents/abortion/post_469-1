(ROOT
  (S
    (NP
      (NP (DT A) (NN baby) (POS 's))
      (NN life))
    (VP (VBZ is)
      (PP (IN at)
        (NP
          (NP (NN stake))
          (, ,)
          (ADVP (RB here)))))
    (. .)))

det(baby-2, A-1)
poss(life-4, baby-2)
nsubj(is-5, life-4)
root(ROOT-0, is-5)
prep_at(is-5, stake-7)
advmod(stake-7, here-9)

