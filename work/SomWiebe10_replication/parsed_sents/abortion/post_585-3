(ROOT
  (SINV
    (S
      (NP
        (NP
          (NP (DT The) (NN person))
          (NP (DT each)))
        (PP (IN of)
          (NP (PRP us))))
      (VP (VBZ becomes)))
    (VP (VBZ is)
      (ADVP (RB certainly))
      (VP (VBN molded)
        (PP (IN by)
          (NP
            (QP (JJR more) (IN than) (RB just))))))
    (NP (PRP$ our) (NNS genes))
    (. .)))

det(person-2, The-1)
nsubj(becomes-6, person-2)
dep(person-2, each-3)
prep_of(person-2, us-5)
dep(molded-9, becomes-6)
