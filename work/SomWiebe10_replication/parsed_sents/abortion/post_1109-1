(ROOT
  (S
    (NP (PRP I))
    (ADVP (RB never))
    (VP (VBD said)
      (SBAR
        (S
          (NP (PRP it) (POS 's) (JJ sole) (NN purpose))
          (VP (VBD was)
            (NP (NN procreation))))))
    (. .)))

nsubj(said-3, I-1)
neg(said-3, never-2)
root(ROOT-0, said-3)
dep(purpose-7, it-4)
possessive(purpose-7, 's-5)
amod(purpose-7, sole-6)
nsubj(procreation-9, purpose-7)
cop(procreation-9, was-8)
ccomp(said-3, procreation-9)

