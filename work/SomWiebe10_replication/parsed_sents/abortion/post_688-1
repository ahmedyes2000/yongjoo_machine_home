(ROOT
  (SQ (VBP Do)
    (NP (PRP you))
    (VP (VB mean)
      (ADVP (RB really))
      (NP
        (NP (DT an) (NN atheist))
        (, ,)
        (CC or)
        (ADJP (RB pretty) (JJ sure))))
    (. .)))

aux(mean-3, Do-1)
nsubj(mean-3, you-2)
root(ROOT-0, mean-3)
advmod(mean-3, really-4)
det(atheist-6, an-5)
dobj(mean-3, atheist-6)
advmod(sure-10, pretty-9)
dobj(mean-3, sure-10)
conj_or(atheist-6, sure-10)

