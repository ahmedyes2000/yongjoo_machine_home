(ROOT
  (S (CC But)
    (NP (DT that))
    (VP (VBZ 's)
      (ADVP (RB just))
      (NP (PRP me)))
    (. .)))

cc(me-5, But-1)
nsubj(me-5, that-2)
cop(me-5, 's-3)
advmod(me-5, just-4)
root(ROOT-0, me-5)

(ROOT
  (S (CC Or)
    (NP (PRP I))
    (VP (MD would)
      (VP (VB 've)
        (VP (VBN gotten)
          (NP (NN shot))
          (PP (IN for)
            (S
              (VP (VBG voicing)
                (NP (PRP$ my) (NN opinion))
                (PP (TO to)
                  (NP (RB much)))))))))
    (. .)))

cc(gotten-5, Or-1)
nsubj(gotten-5, I-2)
aux(gotten-5, would-3)
aux(gotten-5, 've-4)
root(ROOT-0, gotten-5)
dobj(gotten-5, shot-6)
prepc_for(gotten-5, voicing-8)
poss(opinion-10, my-9)
dobj(voicing-8, opinion-10)
prep(voicing-8, to-11)
pobj(to-11, much-12)

