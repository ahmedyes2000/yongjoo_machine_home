  (S
    (NP (DT The) (NN woman))
    (VP (MD should) (RB not)
      (VP (VB punish)
        (NP
          (NP (DT the) (NN baby))
          (SBAR
            (S
              (NP (DT the) (NN rapist))
              (VP (MD should)
                (VP (VB be)
                  (VP (VBN held)
                    (S
                      (ADJP (RB financially) (JJ responsible)))))))))))
    (. .)))

det(woman-2, The-1)
nsubj(punish-5, woman-2)
aux(punish-5, should-3)
neg(punish-5, not-4)
root(ROOT-0, punish-5)
