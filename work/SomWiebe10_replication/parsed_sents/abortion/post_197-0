    (SBAR
      (WHADVP (WRB When))
      (S
        (NP (DT the) (NNP Pope))
        (VP (VBZ speaks)
          (ADVP (RB definitively))
          (PP (IN on)
            (NP (NN fiath)
              (CC and)
              (NNS morals))))))
    (, ,)
    (NP (PRP it))
    (VP (VBZ is)
      (ADJP (JJ infallible)))
    (. .)))

advmod(speaks-4, When-1)
det(Pope-3, the-2)
nsubj(speaks-4, Pope-3)
advcl(infallible-13, speaks-4)
