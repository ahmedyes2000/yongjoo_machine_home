(ROOT
  (S
    (NP (PRP You))
    (VP (VBP are)
      (VP (VBG confusing)
        (NP (PRP me))))
    (. .)))

nsubj(confusing-3, You-1)
aux(confusing-3, are-2)
root(ROOT-0, confusing-3)
dobj(confusing-3, me-4)

(ROOT
  (SQ (VBD Are)
    (NP (PRP you))
    (NP (NN saying))
    (SBAR (IN that)
      (S
        (NP (NNS men))
        (VP (VBP do) (RB n't)
          (VP (VB have)
            (NP (DT the) (NN right)
              (S
                (VP (TO to)
                  (VP (VB be)
                    (NP (JJ pro) (NN life))))))))))
    (. ?)))

cop(saying-3, Are-1)
nsubj(saying-3, you-2)
root(ROOT-0, saying-3)
complm(have-8, that-4)
nsubj(have-8, men-5)
aux(have-8, do-6)
neg(have-8, n't-7)
dep(saying-3, have-8)
det(right-10, the-9)
dobj(have-8, right-10)
aux(life-14, to-11)
cop(life-14, be-12)
amod(life-14, pro-13)
infmod(right-10, life-14)

