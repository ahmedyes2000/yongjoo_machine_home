(ROOT
  (S
    (NP (DT The) (NNS prophets))
    (ADVP (RB just))
    (VP (VBD thought)
      (PP (IN of)
        (NP (PRP them)))
      (ADVP (RB first)))
    (. .)))

det(prophets-2, The-1)
nsubj(thought-4, prophets-2)
advmod(thought-4, just-3)
root(ROOT-0, thought-4)
prep_of(thought-4, them-6)
advmod(thought-4, first-7)

