(ROOT
  (S
    (NP (DT These) (NNS rights))
    (VP (VBP are)
      (ADJP (JJ basic)
        (CC and)
        (JJ fundamental))
      (, ,)
      (PP (IN among)
        (S
          (NP (PRP them))
          (VP (VBP are) (: ;)
            (NP
              (NP (NNP LIFE))
              (, ,)
              (NP (NN liberty))
              (, ,)
              (NP (NN property))
              (, ,)
              (CC and)
              (NP
                (NP (DT the) (NN pursuit))
                (PP (IN of)
                  (NP (NN happiness)))))))))
    (. .)))

det(rights-2, These-1)
nsubj(basic-4, rights-2)
nsubj(fundamental-6, rights-2)
cop(basic-4, are-3)
root(ROOT-0, basic-4)
conj_and(basic-4, fundamental-6)
nsubj(LIFE-12, them-9)
cop(LIFE-12, are-10)
prepc_among(basic-4, LIFE-12)
prepc_among(basic-4, liberty-14)
conj_and(LIFE-12, liberty-14)
prepc_among(basic-4, property-16)
conj_and(LIFE-12, property-16)
