(ROOT
  (S
    (NP (JJ Many) (NNS pro-lifers))
    (VP (VBP go)
      (ADVP (RB so) (RB far) (RB as))
      (S
        (VP (TO to)
          (VP (VB assert)
            (SBAR (IN that)
              (S
                (NP
                  (NP (DT the) (NN morning))
                  (PP (IN after)
                    (NP (NN pill))))
                (VP (VBZ is)
                  (NP
                    (NP (NN murder))
                    (, ,)
                    (SBAR
                      (WHNP (WDT which))
                      (S
                        (VP (VBZ is)
                          (ADJP (JJ ludicrous)))))))))))))
    (. .)))

amod(pro-lifers-2, Many-1)
nsubj(go-3, pro-lifers-2)
