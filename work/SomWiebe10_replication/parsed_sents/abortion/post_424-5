(ROOT
  (S
    (S
      (NP (DT A) (NN person))
      (VP (VBZ is)
        (VP (VBN defined)
          (PP (IN as)
            (NP (DT a) (NN human) (VBG being))))))
    (, ,)
    (ADVP (RB therefore))
    (S
      (NP (DT any) (JJ human) (NN being))
      (VP (VBZ is)
        (NP (DT a) (NN person))))
    (. .)))

det(person-2, A-1)
nsubjpass(defined-4, person-2)
auxpass(defined-4, is-3)
root(ROOT-0, defined-4)
det(human-7, a-6)
prep_as(defined-4, human-7)
amod(human-7, being-8)
advmod(defined-4, therefore-10)
det(being-13, any-11)
amod(being-13, human-12)
nsubj(person-16, being-13)
cop(person-16, is-14)
det(person-16, a-15)
ccomp(defined-4, person-16)

