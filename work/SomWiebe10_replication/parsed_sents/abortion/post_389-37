  (S
    (NP (DT The) (NN transformation))
    (VP (VBZ involves)
      (NP (RB essentially)
        (NP (CD two) (NNS fundamentals))
        (: :)
        (NP (NN existence)
          (CC and)
          (NN development))))
    (. .)))

