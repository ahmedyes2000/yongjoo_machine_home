(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (NN abortion))
        (VP (VBD was)
          (ADJP (JJ illegal)))))
    (NP (PRP it))
    (VP (MD would)
      (ADVP (RB just))
      (VP (VB have)
        (S
          (NP (NNS women))
          (ADJP (JJ revert)
            (PP (TO to)
              (NP (DT the) (JJ old) (NNS ways))))
          (S
            (VP (VB IE)
              (NP (JJ close) (NNS hangers)))))))
    (. .)))

mark(illegal-4, If-1)
nsubj(illegal-4, abortion-2)
cop(illegal-4, was-3)
advcl(have-8, illegal-4)
nsubj(have-8, it-5)
aux(have-8, would-6)
advmod(have-8, just-7)
