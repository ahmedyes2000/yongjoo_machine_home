(ROOT
  (S
    (NP (EX There))
    (VP (MD will)
      (ADVP (RB always))
      (VP (VB be)
        (NP
          (NP (NNS cases))
          (SBAR
            (WHADVP (WRB where))
            (S
              (NP
                (NP (DT a) (NN woman) (POS 's))
                (NN health))
              (VP (VBZ requires)
                (S
                  (VP (VBG having)
                    (NP (DT an) (NN abortion))))))))))
    (. .)))

expl(cases-5, There-1)
aux(cases-5, will-2)
advmod(cases-5, always-3)
cop(cases-5, be-4)
root(ROOT-0, cases-5)
advmod(requires-11, where-6)
det(woman-8, a-7)
poss(health-10, woman-8)
nsubj(requires-11, health-10)
rcmod(cases-5, requires-11)
xcomp(requires-11, having-12)
det(abortion-14, an-13)
dobj(having-12, abortion-14)

