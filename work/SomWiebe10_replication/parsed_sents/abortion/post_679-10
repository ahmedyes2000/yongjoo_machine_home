(ROOT
  (S
    (NP (DT This))
    (VP (VBZ allows)
      (PP (IN for)
        (NP
          (NP
            (NP (DT a) (JJ compassionate) (NN comparison))
            (PP (IN between)
              (NP
                (NP (DT the) (JJ medical) (NNS needs)
                  (CC and)
                  (NNS rights))
                (PP (IN of)
                  (NP (DT a) (JJ pregnant) (NN woman))))))
          (CC and)
          (NP
            (NP (DT the) (JJ gradually) (VBG increasing) (NN realization))
            (PP (IN of)
              (NP
                (NP (DT the) (JJ human) (NN potential))
                (PP (IN of)
                  (NP (DT a) (NN fetus)))))))))
    (. .)))

nsubj(allows-2, This-1)
root(ROOT-0, allows-2)
det(comparison-6, a-4)
amod(comparison-6, compassionate-5)
prep_for(allows-2, comparison-6)
det(needs-10, the-8)
amod(needs-10, medical-9)
prep_between(comparison-6, needs-10)
prep_between(comparison-6, rights-12)
conj_and(needs-10, rights-12)
det(woman-16, a-14)
amod(woman-16, pregnant-15)
prep_of(needs-10, woman-16)
det(realization-21, the-18)
amod(realization-21, gradually-19)
amod(realization-21, increasing-20)
prep_for(allows-2, realization-21)
conj_and(comparison-6, realization-21)
det(potential-25, the-23)
amod(potential-25, human-24)
prep_of(realization-21, potential-25)
det(fetus-28, a-27)
prep_of(potential-25, fetus-28)

