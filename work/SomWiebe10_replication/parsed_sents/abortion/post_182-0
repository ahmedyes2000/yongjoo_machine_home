(ROOT
  (S
    (NP (EX There))
    (VP (VBZ is)
      (NP
        (NP (DT an) (JJ inherent) (JJ moral) (NN conflict))
        (PP (IN in)
          (NP (DT the) (NN abortion) (NN issue))))
      (, ,)
      (SBAR (RB even) (IN if)
        (S
          (NP (PRP you))
          (VP (VBP are)
            (NP (DT an) (NN atheist))))))
    (. .)))

expl(is-2, There-1)
root(ROOT-0, is-2)
det(conflict-6, an-3)
amod(conflict-6, inherent-4)
amod(conflict-6, moral-5)
nsubj(is-2, conflict-6)
det(issue-10, the-8)
nn(issue-10, abortion-9)
prep_in(conflict-6, issue-10)
