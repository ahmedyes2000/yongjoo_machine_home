(ROOT
  (S
    (S
      (NP (PRP You))
      (VP (VBD did) (RB n't)
        (VP (VB come)
          (PP (IN for)
            (NP (DT a) (NN debate))))))
    (, ,)
    (NP (PRP you))
    (VP (VBD came)
      (PP (IN for)
        (NP (DT a) (NN fight))))
    (. .)))

nsubj(come-4, You-1)
aux(come-4, did-2)
neg(come-4, n't-3)
ccomp(came-10, come-4)
det(debate-7, a-6)
prep_for(come-4, debate-7)
nsubj(came-10, you-9)
root(ROOT-0, came-10)
det(fight-13, a-12)
prep_for(came-10, fight-13)

