(ROOT
  (S
    (NP (DT This))
    (VP (MD can)
      (VP (VB affect)
        (NP (DT the) (NN debate))
        (PP (IN over)
          (NP
            (NP (DT the) (NNS ethics))
            (PP (IN of)
              (NP (JJ embryonic) (NN stem) (NN cell) (NN research)))))))
    (. .)))

nsubj(affect-3, This-1)
aux(affect-3, can-2)
root(ROOT-0, affect-3)
det(debate-5, the-4)
dobj(affect-3, debate-5)
