(ROOT
  (S
    (PP (IN on)
      (NP (DT the) (JJ other) (NN hand)))
    (, ,)
    (NP (FW i))
    (VP (VBP think)
      (SBAR (IN that)
        (S
          (NP (JJS most))
          (VP (MD would)
            (VP (VB agree)
              (SBAR (IN that)
                (S
                  (NP (EX there))
                  (VP (VBP are)
                    (VP (VBG extenuating)
                      (NP (NNS circumstances)))))))))))
    (. .)))

det(hand-4, the-2)
amod(hand-4, other-3)
prep_on(think-7, hand-4)
nsubj(think-7, i-6)
root(ROOT-0, think-7)
complm(agree-11, that-8)
nsubj(agree-11, most-9)
aux(agree-11, would-10)
ccomp(think-7, agree-11)
complm(extenuating-15, that-12)
expl(extenuating-15, there-13)
aux(extenuating-15, are-14)
ccomp(agree-11, extenuating-15)
dobj(extenuating-15, circumstances-16)
nsubj(extenuating-15, circumstances-16)

