  (S
    (NP (PRP We))
    (VP (MD can)
      (VP (VB have)
        (NP (JJ theological) (NNS debates))
        (PP (IN about)
          (SBAR
            (WHADVP (WRB how)
              (CC and)
              (WRB when))
            (S
              (NP (DT a) (NN soul))
              (VP (VBZ enters)
                (NP (DT the) (NN body))))))
        (, ,)
        (SBAR (IN whether)
          (S
            (NP (PRP we))
            (VP (VBP have)
              (NP (DT a) (NN soul))
              (PP (IN at)
                (NP
                  (NP (DT all))
                  (, ,)
                  (NP (FW etc.)))))))))
    (. .)))

