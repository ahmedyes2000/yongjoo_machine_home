(ROOT
  (S
    (NP (PRP It))
    (VP (MD may)
      (VP (VB take)
        (NP (NNS years))
        (SBAR (IN before)
          (S
            (VP (VBP are)
              (S
                (NP (NNS actions))
                (VP (VB bear)
                  (NP (NN fruit)))))))))
    (. .)))

nsubj(take-3, It-1)
aux(take-3, may-2)
root(ROOT-0, take-3)
tmod(take-3, years-4)
mark(are-6, before-5)
advcl(take-3, are-6)
nsubj(bear-8, actions-7)
ccomp(are-6, bear-8)
dobj(bear-8, fruit-9)

