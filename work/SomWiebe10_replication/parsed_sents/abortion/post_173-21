(ROOT
  (S
    (S
      (NP (DT A) (NN building))
      (VP (VBZ is)
        (PP (IN on)
          (NP (NN fire)))))
    (CC and)
    (S
      (NP (PRP you))
      (VP (VBP rush)
        (PRT (RP in))
        (S
          (VP (TO to)
            (VP (VB save)
              (NP (NN anyone))
              (ADVP (RB inside)))))))
    (. .)))

det(building-2, A-1)
nsubj(is-3, building-2)
root(ROOT-0, is-3)
prep_on(is-3, fire-5)
nsubj(rush-8, you-7)
xsubj(save-11, you-7)
conj_and(is-3, rush-8)
prt(rush-8, in-9)
aux(save-11, to-10)
xcomp(rush-8, save-11)
dobj(save-11, anyone-12)
advmod(save-11, inside-13)

