  (S
    (PP (IN In)
      (NP (NN fact)))
    (, ,)
    (PP (IN with)
      (NP (JJ complex) (NNS creations)))
    (, ,)
    (NP (EX there))
    (VP (VBZ is)
      (NP (DT no) (NN instant)
        (S
          (VP (TO to)
            (VP (VB glorify)
              (NP
                (NP (RB only) (DT a) (NN process))
                (PP (IN of)
                  (NP
                    (NP (JJ incremental) (NN accrual))
                    (PP (IN of)
                      (NP (NNS characteristics)))))))))))
    (. .)))

