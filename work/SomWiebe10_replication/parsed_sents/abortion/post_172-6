(ROOT
  (S ('' '')
    (NP (DT That))
    (VP (VBZ is) (RB not)
      (ADJP (JJ surprising)
        (PP (IN in)
          (NP
            (NP (NN light))
            (PP (IN of)
              (NP (DT the) (NN fact))))))
      (SBAR (IN that)
        (S
          (NP (DT no) (JJ such) (NN statement))
          (VP (VBD was)
            (VP
              (ADVP (RB ever))
              (VBN made))))))
    (. .)))

nsubj(surprising-5, That-2)
cop(surprising-5, is-3)
neg(surprising-5, not-4)
root(ROOT-0, surprising-5)
prep_in(surprising-5, light-7)
det(fact-10, the-9)
prep_of(light-7, fact-10)
complm(made-17, that-11)
det(statement-14, no-12)
amod(statement-14, such-13)
nsubjpass(made-17, statement-14)
auxpass(made-17, was-15)
advmod(made-17, ever-16)
ccomp(surprising-5, made-17)

