  (S
    (NP (JJ Legal) (NN abortion))
    (VP
      (VP (VBZ helps)
        (S
          (NP (NNS women))
          (VP (VB avert)
            (NP (DT these) (JJ unavoidable) (NNS risks))
            (PP (TO to)
              (NP (PRP$ their) (NN health))))))
      (CC and)
      (VP (VBZ lives)))
    (. .)))

