(ROOT
  (S (IN So)
    (NP (PRP I))
    (VP (VBP hope)
      (SBAR
        (S
          (NP (PRP you))
          (VP (MD can)
            (VP (VB see)
              (SBAR (IN that)
                (S
                  (NP (PRP I))
                  (VP (VBP 'm)
                    (ADVP (RB still))
                    (VP (VBG debating)
                      (NP (DT the) (JJ same) (NN issue)))))))))))
    (. .)))

dep(hope-3, So-1)
nsubj(hope-3, I-2)
root(ROOT-0, hope-3)
nsubj(see-6, you-4)
aux(see-6, can-5)
ccomp(hope-3, see-6)
complm(debating-11, that-7)
