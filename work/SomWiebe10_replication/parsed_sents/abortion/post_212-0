(ROOT
  (S
    (ADVP
      (ADVP (RB No) (RB not))
      (PP (IN for)
        (ADVP (RB now))))
    (NP (PRP it))
    (VP (VBZ 's)
      (NP (NN need)))
    (. .)))

advmod(not-2, No-1)
advmod(need-7, not-2)
prep(not-2, for-3)
pcomp(for-3, now-4)
nsubj(need-7, it-5)
cop(need-7, 's-6)
root(ROOT-0, need-7)

