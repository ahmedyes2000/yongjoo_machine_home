(ROOT
  (S
    (NP (NNP Life))
    (VP
      (VP (VBD began)
        (NP (NNS eons))
        (ADVP (IN ago)))
      (, ,)
      (CC and)
      (VP (VBZ continues)
        (ADJP (JJ uninterrupted)
          (PP (IN from)
            (NP (CD one) (NN generation))))
        (PP (TO to)
          (NP (DT the) (JJ next)))))
    (. .)))

nsubj(began-2, Life-1)
nsubj(continues-7, Life-1)
root(ROOT-0, began-2)
dobj(began-2, eons-3)
advmod(began-2, ago-4)
conj_and(began-2, continues-7)
acomp(continues-7, uninterrupted-8)
num(generation-11, one-10)
prep_from(uninterrupted-8, generation-11)
det(next-14, the-13)
prep_to(continues-7, next-14)

