(ROOT
  (S
    (NP (DT The) (NN issue))
    (VP (VBZ is)
      (SBAR (IN whether)
        (S
          (NP (NNS women))
          (VP (MD should)
            (VP (VB have)
              (NP (DT a) (NN right)
                (S
                  (VP (TO to)
                    (VP (VB abort)
                      (NP (DT a) (NN fetus)))))))))
        (CC or)
        (RB not)))
    (. .)))

det(issue-2, The-1)
nsubj(is-3, issue-2)
root(ROOT-0, is-3)
complm(have-7, whether-4)
nsubj(have-7, women-5)
aux(have-7, should-6)
ccomp(is-3, have-7)
det(right-9, a-8)
dobj(have-7, right-9)
aux(abort-11, to-10)
infmod(right-9, abort-11)
det(fetus-13, a-12)
dobj(abort-11, fetus-13)
ccomp(is-3, not-15)
conj_or(have-7, not-15)

