(ROOT
  (S
    (NP (PRP You))
    (VP (VBP want)
      (S
        (VP (TO to)
          (VP (VB justify)
            (S
              (VP (VBG killing)
                (NP (PRP it))))))))
    (. .)))

nsubj(want-2, You-1)
xsubj(justify-4, You-1)
root(ROOT-0, want-2)
aux(justify-4, to-3)
xcomp(want-2, justify-4)
xcomp(justify-4, killing-5)
dobj(killing-5, it-6)

