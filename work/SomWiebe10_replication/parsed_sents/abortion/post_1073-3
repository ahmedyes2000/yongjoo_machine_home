(ROOT
  (S
    (NP (EX There))
    (VP (VBZ is)
      (NP (DT no) (JJ good) (NN reason)
        (S
          (VP (TO to)
            (VP (VB have)
              (NP (DT an) (NN abortion) (NN !!!)))))))))

expl(is-2, There-1)
root(ROOT-0, is-2)
det(reason-5, no-3)
amod(reason-5, good-4)
nsubj(is-2, reason-5)
aux(have-7, to-6)
infmod(reason-5, have-7)
det(!!!-10, an-8)
nn(!!!-10, abortion-9)
dobj(have-7, !!!-10)

