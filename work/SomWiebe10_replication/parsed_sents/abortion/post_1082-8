(ROOT
  (S
    (ADVP (RB Everyday))
    (NP
      (NP (DT the) (NN number))
      (PP (IN of)
        (NP
          (NP (NNS babies))
          (VP (VBN killed)
            (PP (IN by)
              (NP (NNS abortionists) (RB approximately)))))))
    (VP (VBZ equals)
      (SBAR
        (S
          (NP (DT the) (CD 3,000))
          (VP (VBD killed)
            (PP (IN by)
              (NP (NNS terrorists)))
            (PP (IN on)
              (NP (NNP September) (CD 11) (, ,) (CD 2001)))))))
    (. .)))

advmod(equals-10, Everyday-1)
det(number-3, the-2)
