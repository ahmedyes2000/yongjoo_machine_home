num(September-17, 11-18)
num(September-17, 2001-20)

(ROOT
  (S
    (ADVP (RB Now))
    (NP (PRP I))
    (VP (VBP believe)
      (SBAR (IN that)
        (S
          (NP (PRP I))
          (VP (VBP do) (RB not)
            (VP (VB have)
              (S
                (VP (TO to)
                  (VP (VB convince)
                    (NP (NN anyone))
                    (ADVP (RB here))
                    (PP (IN in)
                      (NP
                        (NP (DT this) (NN church))
                        (PP (IN about)
                          (NP
                            (NP (DT the) (NN evilness)
                              (CC and)
                              (NN tragedy))
                            (PP (IN of)
                              (NP (NN abortion)))))))))))))))
    (. .)))

advmod(believe-3, Now-1)
nsubj(believe-3, I-2)
root(ROOT-0, believe-3)
complm(have-8, that-4)
nsubj(have-8, I-5)
