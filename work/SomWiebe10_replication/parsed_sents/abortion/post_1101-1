  (S (CC Or)
    (, ,)
    (S
      (S
        (VP (TO to)
          (VP (VB avoid)
            (NP (DT the) (JJ double) (NN negative)))))
      (, ,)
      (S
        (VP (VBG having)
          (NP (NN sex))
          (PP
            (ADVP (RB strictly))
            (IN with)
            (NP (PRP$ your) (NN spouse))))))
    (. .)))

cc(avoid-4, Or-1)
aux(avoid-4, to-3)
root(ROOT-0, avoid-4)
det(negative-7, the-5)
