(ROOT
  (S
    (NP
      (NP (DT The) (JJ central) (NN theme))
      (VP (VBG running)
        (PP (IN through)
          (NP (PRP$ your) (NN response)))))
    (VP (VBZ is)
      (NP (NN sex))
      (: ;)
      (S
        (NP (PDT all) (DT these) (NNS issues))
        (VP (VB revolve)
          (PRT (RP around))
          (NP (DT that) (JJ central) (NN theme)))))
    (. .)))

det(theme-3, The-1)
amod(theme-3, central-2)
nsubj(sex-9, theme-3)
partmod(theme-3, running-4)
poss(response-7, your-6)
prep_through(running-4, response-7)
cop(sex-9, is-8)
root(ROOT-0, sex-9)
predet(issues-13, all-11)
det(issues-13, these-12)
nsubj(revolve-14, issues-13)
parataxis(sex-9, revolve-14)
prt(revolve-14, around-15)
det(theme-18, that-16)
amod(theme-18, central-17)
dobj(revolve-14, theme-18)

