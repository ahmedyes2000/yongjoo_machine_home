(ROOT
  (S
    (S
      (VP (VBG Relying)
        (PP (IN on)
          (NP (DT a) (JJ formal) (NN authority)))))
    (VP (VBZ is)
      (NP
        (NP (DT the) (NN foundation))
        (PP (IN of)
          (NP
            (ADJP (JJ logical) (, ,) (RB even) (JJ rational))
            (NN argument)))))
    (. .)))

csubj(foundation-8, Relying-1)
det(authority-5, a-3)
amod(authority-5, formal-4)
prep_on(Relying-1, authority-5)
cop(foundation-8, is-6)
det(foundation-8, the-7)
root(ROOT-0, foundation-8)
amod(rational-13, logical-10)
advmod(rational-13, even-12)
amod(argument-14, rational-13)
prep_of(foundation-8, argument-14)

