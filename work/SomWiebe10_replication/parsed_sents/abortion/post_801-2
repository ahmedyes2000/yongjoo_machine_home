(ROOT
  (FRAG
    (NP (NNP EDIT))
    (: :)
    (S
      (NP (PRP I))
      (VP (VBP hope)
        (SBAR
          (S
            (NP (PRP you))
            (VP (VBD read)
              (NP (DT this) (NN argument))
              (SBAR
                (WHADVP (WRB when))
                (S
                  (NP (PRP$ my) (NN score))
                  (VP (VBZ is)
                    (ADVP (RB still))
                    (NP (CD 666))))))))))
    (. .)))

root(ROOT-0, EDIT-1)
nsubj(hope-4, I-3)
dep(EDIT-1, hope-4)
nsubj(read-6, you-5)
ccomp(hope-4, read-6)
det(argument-8, this-7)
dobj(read-6, argument-8)
advmod(666-14, when-9)
poss(score-11, my-10)
nsubj(666-14, score-11)
cop(666-14, is-12)
advmod(666-14, still-13)
advcl(read-6, 666-14)

