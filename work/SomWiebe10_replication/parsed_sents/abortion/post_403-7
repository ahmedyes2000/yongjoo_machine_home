(ROOT
  (S
    (NP (JJ Exotic) (, ,) (NN borderline) (, ,)
      (CC or)
      (JJ abnormal) (NNS cases))
    (VP (VBP do) (RB not)
      (VP (VB define)
        (NP (DT the) (NN principle)
          (CC or)
          (NN category))))
    (. .)))

amod(borderline-3, Exotic-1)
nsubj(define-10, borderline-3)
amod(cases-7, abnormal-6)
conj_or(borderline-3, cases-7)
nsubj(define-10, cases-7)
aux(define-10, do-8)
neg(define-10, not-9)
root(ROOT-0, define-10)
det(principle-12, the-11)
dobj(define-10, principle-12)
dobj(define-10, category-14)
conj_or(principle-12, category-14)

