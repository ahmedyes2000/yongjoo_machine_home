(ROOT
  (S
    (NP (NNP Human) (NNP DNA))
    (VP (VBZ is)
      (ADJP
        (ADJP (JJ unique))
        (CC and)
        (ADJP (RB quite) (JJ distinctive)))
      (: ;)
      (SBAR (IN as)
        (S
          (VP (VBZ is)
            (NP
              (ADJP
                (ADJP (RB generally) (JJ phenotypically))
                (PRN (-LRB- -LRB-)
                  (PP (VBG according)
                    (PP (TO to)
                      (NP (NNS looks))))
                  (-RRB- -RRB-)))
              (NNS obvious))))))
    (. .)))

nn(DNA-2, Human-1)
nsubj(unique-4, DNA-2)
nsubj(distinctive-7, DNA-2)
cop(unique-4, is-3)
root(ROOT-0, unique-4)
advmod(distinctive-7, quite-6)
conj_and(unique-4, distinctive-7)
mark(obvious-18, as-9)
cop(obvious-18, is-10)
advmod(phenotypically-12, generally-11)
amod(obvious-18, phenotypically-12)
prepc_according_to(phenotypically-12, to-15)
pobj(phenotypically-12, looks-16)
advcl(unique-4, obvious-18)
