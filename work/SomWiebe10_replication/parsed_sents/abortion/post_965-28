(ROOT
  (S
    (NP
      (NP (DT Every) (CD twenty) (NNS seconds))
      (NP (DT a) (NN child)))
    (VP (VBZ is)
      (VP (VBN aborted)))
    (. .)))

det(seconds-3, Every-1)
num(seconds-3, twenty-2)
nsubjpass(aborted-7, seconds-3)
det(child-5, a-4)
dep(seconds-3, child-5)
auxpass(aborted-7, is-6)
root(ROOT-0, aborted-7)

