(ROOT
  (S
    (NP (PRP You))
    (VP
      (VP (VBP make)
        (S
          (NP (PRP it))
          (VP (VB sound)
            (ADJP (RB as)
              (PP (IN if) ('' '')
                (NP
                  (NP (NNS Conservatives))
                  ('' '')
                  (PP (IN as)
                    (NP
                      (NP (DT this) (NN bastion))
                      (PP (IN of)
                        (NP
                          (NP (VBG gleaming) (NN solidarity))
                          (PP (IN in)
                            (NP (NNS beliefs)))))))
                  (: ...)))))))
      (CC and)
      (VP (VBP are) (RB not)
        (ADJP (JJ corrupt))))
    (. .)))

nsubj(make-2, You-1)
nsubj(corrupt-22, You-1)
root(ROOT-0, make-2)
nsubj(sound-4, it-3)
ccomp(make-2, sound-4)
acomp(sound-4, as-5)
prep_if(as-5, Conservatives-8)
det(bastion-12, this-11)
prep_as(Conservatives-8, bastion-12)
amod(solidarity-15, gleaming-14)
prep_of(bastion-12, solidarity-15)
prep_in(solidarity-15, beliefs-17)
cop(corrupt-22, are-20)
neg(corrupt-22, not-21)
conj_and(make-2, corrupt-22)

