(ROOT
  (S
    (NP (PRP You))
    (ADVP (RB never))
    (VP
      (VP (VBD said)
        (WHNP (WDT that)))
      (, ,)
      (CC or)
      (VP (VBD implied)
        (WHNP (WDT that))))
    (. .)))

nsubj(said-3, You-1)
nsubj(implied-7, You-1)
neg(said-3, never-2)
root(ROOT-0, said-3)
dobj(said-3, that-4)
conj_or(said-3, implied-7)
dobj(implied-7, that-8)

