(ROOT
  (S
    (NP (JJ Many) (NNS women))
    (VP (VBP turn)
      (PP (TO to)
        (NP (NNS drugs)
          (CC and)
          (NN alcohol)))
      (PP (IN as)
        (NP
          (NP (DT a) (NN way))
          (PP (IN of)
            (S
              (VP (VBG masking)
                (NP (DT the) (NN pain))))))))
    (. .)))

amod(women-2, Many-1)
nsubj(turn-3, women-2)
root(ROOT-0, turn-3)
prep_to(turn-3, drugs-5)
prep_to(turn-3, alcohol-7)
conj_and(drugs-5, alcohol-7)
det(way-10, a-9)
prep_as(turn-3, way-10)
prepc_of(way-10, masking-12)
det(pain-14, the-13)
dobj(masking-12, pain-14)

