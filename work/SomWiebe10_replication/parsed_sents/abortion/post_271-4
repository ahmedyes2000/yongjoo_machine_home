      (NP (DT the) (JJ same) (NN token)))
    (, ,)
    (NP (NNS parents))
    (VP (VBP do) (RB not)
      (VP (VB have)
        (NP (DT the) (NN right)
          (S
            (VP (TO to)
              (VP (VB beat)
                (NP (DT a) (NN child))
                (PP (IN into)
                  (NP (NN submission)))
                (, ,)
                (ADVP (RB either))))))))
    (. .)))

