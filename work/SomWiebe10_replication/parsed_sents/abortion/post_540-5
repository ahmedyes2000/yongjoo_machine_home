(ROOT
  (S
    (NP (PRP I))
    (VP (VBP have)
      (NP
        (NP (NNS responsibilies))
        (PP (IN like)
          (NP (PRP$ my) (NN daughter)
            (S
              (VP (TO to)
                (VP (VB take)
                  (NP
                    (NP
                      (NP (NN care))
                      (PP (IN of)))
                    (CONJP (RB as) (RB well) (IN as))
                    (NP (NN work)
                      (CC and)
                      (NN school))))))))))
    (. .)))

nsubj(have-2, I-1)
root(ROOT-0, have-2)
dobj(have-2, responsibilies-3)
poss(daughter-6, my-5)
prep_like(responsibilies-3, daughter-6)
aux(take-8, to-7)
infmod(daughter-6, take-8)
dobj(take-8, care-9)
prep(care-9, of-10)
dobj(take-8, work-14)
conj_and(care-9, work-14)
conj_and(care-9, school-16)
conj_and(work-14, school-16)

