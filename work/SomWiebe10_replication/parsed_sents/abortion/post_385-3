      (PP (IN under)
        (NP
          (NP (DT the) (NN dictates))
          (PP (IN of)
            (SBAR
              (WHNP (WP whoever))
              (S
                (VP (VBZ has)
                  (NP
                    (NP (DT the) (JJS most) (NN pull))
                    (CC and)
                    (NP (DT the) (JJS most) (NNS guns))))))))))
    (. .)))

prep_in(is-7, time-2)
det(nation-6, the-4)
