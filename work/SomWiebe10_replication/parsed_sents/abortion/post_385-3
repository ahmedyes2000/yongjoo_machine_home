(ROOT
  (S
    (SBAR
      (WHADVP (WRB When))
      (S
        (NP (DT that))
        (VP (VBZ 's)
          (NP (DT the) (NN case)))))
    (, ,)
    (NP (NNS males))
    (VP (VBP are)
      (ADVP (RB also))
      (VP (VBN affected)))
    (. .)))

advmod(case-5, When-1)
nsubj(case-5, that-2)
cop(case-5, 's-3)
det(case-5, the-4)
advcl(affected-10, case-5)
nsubjpass(affected-10, males-7)
auxpass(affected-10, are-8)
advmod(affected-10, also-9)
root(ROOT-0, affected-10)

(ROOT
  (S
    (PP (IN In)
      (NP (NN time)))
    (, ,)
