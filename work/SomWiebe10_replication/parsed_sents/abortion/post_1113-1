(ROOT
  (S
    (S
      (VP (VB Renders)
        (S
          (NP (DT the) (NN human))
          (NP (DT a) (NN non-human)))))
    (CC and)
    (S
      (ADVP (RB therefore))
      (NP (PRP it))
      (VP (MD can)
        (VP (VB be)
          (VP (VBN disposed)
            (PP (IN of)
              (SBAR
                (WHADVP (WRB when))
                (S
                  (NP (DT an) (NN individual))
                  (VP (VBZ does) (RB n't)
                    (VP (VB want)
                      (NP (PRP it)))))))))))
    (. .)))

root(ROOT-0, Renders-1)
det(human-3, the-2)
nsubj(non-human-5, human-3)
det(non-human-5, a-4)
xcomp(Renders-1, non-human-5)
