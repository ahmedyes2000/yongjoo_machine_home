(ROOT
  (S
    (NP (PRP They))
    (VP (VBP are)
      (VP (VBG paying)
        (S
          (VP (TO to)
            (VP (VB murder)
              (NP (DT an) (JJ innosent) (NN child)))))))
    (. .)))

nsubj(paying-3, They-1)
xsubj(murder-5, They-1)
aux(paying-3, are-2)
root(ROOT-0, paying-3)
aux(murder-5, to-4)
xcomp(paying-3, murder-5)
det(child-8, an-6)
amod(child-8, innosent-7)
dobj(murder-5, child-8)

