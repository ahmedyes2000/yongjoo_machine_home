(ROOT
  (S
    (NP (DT The) (NN baby))
    (VP (VBZ has)
      (NP
        (NP (DT the)
          (ADJP (JJ right)
            (PP (TO to)
              (NP (NN life))))
          (NN liberty))
        (CC and)
        (NP
          (NP (DT the) (NN pursuit))
          (PP (IN of)
            (NP (NN happiness))))))
    (. .)))

det(baby-2, The-1)
nsubj(has-3, baby-2)
root(ROOT-0, has-3)
det(liberty-8, the-4)
amod(liberty-8, right-5)
prep_to(right-5, life-7)
dobj(has-3, liberty-8)
det(pursuit-11, the-10)
dobj(has-3, pursuit-11)
conj_and(liberty-8, pursuit-11)
prep_of(pursuit-11, happiness-13)

