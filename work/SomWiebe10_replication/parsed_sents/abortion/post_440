(ROOT
  (S
    (NP (DT A) (NN soul))
    (VP (VBZ is)
      (NP
        (NP (DT a) (JJ religious) (NN concept))
        (, ,) (RB not)
        (NP (DT a) (JJ scientific) (NN one))))
    (. .)))

det(soul-2, A-1)
nsubj(concept-6, soul-2)
cop(concept-6, is-3)
det(concept-6, a-4)
amod(concept-6, religious-5)
root(ROOT-0, concept-6)
neg(concept-6, not-8)
det(one-11, a-9)
amod(one-11, scientific-10)
dep(concept-6, one-11)

(ROOT
  (S
    (NP (NNP Science))
    (VP (MD can) (RB not)
      (VP (VB discover)
        (CC or)
        (VB identify)
        (NP (DT a) (NN soul))))
    (. .)))

nsubj(discover-4, Science-1)
nsubj(identify-6, Science-1)
aux(discover-4, can-2)
neg(discover-4, not-3)
root(ROOT-0, discover-4)
conj_or(discover-4, identify-6)
det(soul-8, a-7)
dobj(discover-4, soul-8)

