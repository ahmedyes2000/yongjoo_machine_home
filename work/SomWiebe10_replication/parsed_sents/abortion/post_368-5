(ROOT
  (S
    (S
      (NP (PRP We))
      (ADVP (DT all))
      (VP (VBP make)
        (NP
          (NP (NNS choices))
          (PP (IN in)
            (NP (NN life))))))
    (, ,)
    (S
      (NP (PRP you))
      (VP (VBP are)
        (ADVP (RB only))
        (ADJP (JJ responsible)
          (PP (IN for)
            (NP (PRP$ your) (JJ own))))))
    (, ,)
    (S
      (NP (PRP it))
      (VP (VBZ 's)
        (VP (VBN called)
          (NP (NN accountability)))))
    (. .)))

nsubj(make-3, We-1)
advmod(make-3, all-2)
root(ROOT-0, make-3)
dobj(make-3, choices-4)
prep_in(choices-4, life-6)
nsubj(responsible-11, you-8)
cop(responsible-11, are-9)
advmod(responsible-11, only-10)
ccomp(make-3, responsible-11)
poss(own-14, your-13)
prep_for(responsible-11, own-14)
nsubjpass(called-18, it-16)
auxpass(called-18, 's-17)
ccomp(make-3, called-18)
dobj(called-18, accountability-19)

