(ROOT
  (S
    (NP (JJ Legal) (NN abortion))
    (VP (VBZ protects)
      (NP (NNS women))
      (PP (IN with)
        (NP
          (NP (JJ serious) (NNS illnesses))
          (SBAR
            (WHNP (WDT that))
            (S
              (VP (VBP are)
                (ADJP (JJ vulnerable))))))))
    (. .)))

amod(abortion-2, Legal-1)
nsubj(protects-3, abortion-2)
root(ROOT-0, protects-3)
dobj(protects-3, women-4)
amod(illnesses-7, serious-6)
prep_with(protects-3, illnesses-7)
nsubj(vulnerable-10, illnesses-7)
cop(vulnerable-10, are-9)
rcmod(illnesses-7, vulnerable-10)

