(ROOT
  (S
    (NP (JJ Many) (NNS women))
    (VP (VBP live)
      (PP (IN in)
        (NP
          (ADJP (JJ difficult) (, ,) (RB potentially) (JJ dangerous))
          (NNS situations))))
    (. .)))

amod(women-2, Many-1)
nsubj(live-3, women-2)
root(ROOT-0, live-3)
amod(dangerous-8, difficult-5)
advmod(dangerous-8, potentially-7)
amod(situations-9, dangerous-8)
prep_in(live-3, situations-9)

