dobj(banning-4, abortion-5)

(ROOT
  (NP
    (NP (NN Vote) (RB yes))
    (PP (TO to)
      (S
        (VP (VBG allowing)
          (NP (NN state) (NN funding))
          (S
            (VP (TO to)
              (VP (VB pay)
                (PP (IN for)
                  (NP (NNS abortions)))))))))
    (. .)))

root(ROOT-0, Vote-1)
advmod(Vote-1, yes-2)
