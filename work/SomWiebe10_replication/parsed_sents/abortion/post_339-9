(ROOT
  (S (CC Either)
    (S
      (NP (PRP we))
      (VP (VBP do) (RB n't)
        (VP (VB allow)
          (NP (NNS abortions))
          (ADVP (IN at) (DT all)))))
    (, ,)
    (CC or)
    (S
      (NP (PRP we))
      (VP (VBP allow)
        (NP (NN abortion))
        (PP (IN at)
          (NP
            (NP (DT any) (NN stage))
            (PP (IN of)
              (NP
                (NP (NN development))
                (PP (IN for)
                  (NP (DT any) (NN reason)))))))))
    (. .)))

preconj(allow-5, Either-1)
nsubj(allow-5, we-2)
aux(allow-5, do-3)
neg(allow-5, n't-4)
root(ROOT-0, allow-5)
dobj(allow-5, abortions-6)
advmod(allow-5, at-7)
pobj(at-7, all-8)
nsubj(allow-12, we-11)
conj_or(allow-5, allow-12)
dobj(allow-12, abortion-13)
det(stage-16, any-15)
