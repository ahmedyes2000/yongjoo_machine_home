(ROOT
  (S
    (NP (PRP It))
    (ADVP (RB absolutely))
    (VP (VBZ does)
      (VP (VB affect)
        (NP (NN everyone))
        (SBAR (IN if)
          (S
            (NP (NN abortion))
            (VP (VBZ is)
              (ADJP (JJ legal)))))))
    (. .)))

nsubj(affect-4, It-1)
advmod(affect-4, absolutely-2)
aux(affect-4, does-3)
root(ROOT-0, affect-4)
dobj(affect-4, everyone-5)
mark(legal-9, if-6)
nsubj(legal-9, abortion-7)
cop(legal-9, is-8)
advcl(affect-4, legal-9)

