(ROOT
  (S
    (PP (IN On)
      (NP (DT the) (JJ other)))
    (, ,)
    (NP (DT the) (NN second))
    (VP (VBZ has)
      (NP
        (NP (DT an)
          (ADJP (RB equally) (JJ inalienable))
          (NN right))
        (PP (TO to)
          (NP (NN his\/her) (NN liberty)))))
    (. .)))

det(other-3, the-2)
prep_on(has-7, other-3)
det(second-6, the-5)
nsubj(has-7, second-6)
root(ROOT-0, has-7)
det(right-11, an-8)
advmod(inalienable-10, equally-9)
amod(right-11, inalienable-10)
