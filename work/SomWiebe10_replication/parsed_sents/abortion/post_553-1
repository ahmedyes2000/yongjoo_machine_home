(ROOT
  (S
    (NP
      (NP (JJ Many) (NNS opponents))
      (PP (IN of)
        (NP (NN abortion))))
    (VP (VBP feel)
      (SBAR (IN that)
        (S
          (NP (DT the) (NNS justices))
          (ADVP (RB just))
          (VP (VBD used)
            (NP (NN privacy))
            (PP (IN as)
              (NP (DT a) (NN cover)))
            (PP (IN for)
              (NP (PRP$ their) (JJ personal) (NN decision)))))))
    (. .)))

amod(opponents-2, Many-1)
nsubj(feel-5, opponents-2)
prep_of(opponents-2, abortion-4)
root(ROOT-0, feel-5)
complm(used-10, that-6)
det(justices-8, the-7)
nsubj(used-10, justices-8)
advmod(used-10, just-9)
ccomp(feel-5, used-10)
dobj(used-10, privacy-11)
det(cover-14, a-13)
prep_as(used-10, cover-14)
poss(decision-18, their-16)
amod(decision-18, personal-17)
prep_for(used-10, decision-18)

