(ROOT
  (S
    (PP (IN From)
      (NP (DT a) (JJ religious) (NN context)))
    (, ,)
    (ADVP (RB sure))
    (, ,)
    (NP (PRP it))
    (VP (VBZ makes)
      (NP (JJ perfect) (NN sense))
      (PRN (, ,)
        (CC but)
        (ADVP (RB outside)))
      (PP (IN of)
        (SBAR (IN that)
          (S
            (NP (PRP it))
            (VP (VBZ does) (RB not))))))
    (. .)))

det(context-4, a-2)
amod(context-4, religious-3)
prep_from(makes-9, context-4)
advmod(makes-9, sure-6)
nsubj(makes-9, it-8)
root(ROOT-0, makes-9)
amod(sense-11, perfect-10)
dobj(makes-9, sense-11)
dep(makes-9, but-13)
complm(does-18, that-16)
nsubj(does-18, it-17)
prepc_outside_of(makes-9, does-18)
neg(does-18, not-19)

