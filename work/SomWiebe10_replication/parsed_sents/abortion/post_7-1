(ROOT
  (S
    (NP (PRP They))
    (VP (VBP are)
      (NP
        (NP (DT every) (NN human))
        (VP (VBG being)
          (VP (VBZ s)
            (NP
              (NP (NN entitlement))
              (PP (IN by)
                (NP
                  (NP (NN virtue))
                  (PP (IN of)
                    (NP (PRP$ his) (NN humanity))))))))))
    (. .)))

nsubj(human-4, They-1)
cop(human-4, are-2)
det(human-4, every-3)
root(ROOT-0, human-4)
aux(entitlement-7, being-5)
cop(entitlement-7, s-6)
partmod(human-4, entitlement-7)
agent(entitlement-7, virtue-9)
poss(humanity-12, his-11)
prep_of(virtue-9, humanity-12)

