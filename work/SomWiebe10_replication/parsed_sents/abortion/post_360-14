(ROOT
  (S
    (NP (PRP$ Your) (NN example))
    (VP (VBZ is) (RB not)
      (ADJP (JJ valid))
      (SBAR (IN because)
        (S
          (PP (IN in)
            (NP (DT a) (JJ broken) (NN leg)))
          (NP (NN situation) (NN surgery))
          (VP (VBZ has)
            (NP
              (NP (DT a) (NN purpose))
              (PP (IN of)
                (NP (NN healing)
                  (CC and)
                  (NN cure))))))))
    (. .)))

poss(example-2, Your-1)
nsubj(valid-5, example-2)
cop(valid-5, is-3)
neg(valid-5, not-4)
root(ROOT-0, valid-5)
mark(has-13, because-6)
