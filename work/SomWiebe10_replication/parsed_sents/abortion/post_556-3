(ROOT
  (SBARQ
    (WHADJP (WRB How)
      (ADJP (IN about)))
    (SQ
      (NP (DT each) (NN individual))
      (VP (VB decide)
        (PP (IN for)
          (NP (PRP themselves)))))
    (. ?)))

advmod(about-2, How-1)
dep(decide-5, about-2)
det(individual-4, each-3)
nsubj(decide-5, individual-4)
root(ROOT-0, decide-5)
prep_for(decide-5, themselves-7)

