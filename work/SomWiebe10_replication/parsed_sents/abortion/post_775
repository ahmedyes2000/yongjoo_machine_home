(ROOT
  (S
    (NP (PRP I))
    (VP (VBP love)
      (SBAR
        (WHADVP (WRB how))
        (S
          (NP (PRP you))
          (VP (VBP 're)
            (VP (VBG fighting)
              (NP (DT an) (JJ uphill) (NN battle)))))))
    (. .)))

nsubj(love-2, I-1)
root(ROOT-0, love-2)
advmod(fighting-6, how-3)
