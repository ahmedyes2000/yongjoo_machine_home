(ROOT
  (S
    (S
      (NP (PRP I))
      (VP (MD would)
        (VP (VB join)
          (PRT (RP in)))))
    (, ,)
    (CC but)
    (S
      (NP (PRP I))
      (VP (VBP do)
        (NP
          (NP (DT this))
          (PP (IN in)
            (NP (NN school) (NN way))))
        (ADVP (RB too) (RB often))))
    (. .)))

nsubj(join-3, I-1)
aux(join-3, would-2)
root(ROOT-0, join-3)
prt(join-3, in-4)
nsubj(do-8, I-7)
conj_but(join-3, do-8)
