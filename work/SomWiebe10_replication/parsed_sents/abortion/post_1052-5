  (S
    (S
      (PP (IN In)
        (NP (NN case)))
      (NP (PRP you))
      (VP (VBP do) (RB n't)
        (VP (VB know))))
    (, ,)
    (NP
      (NP (DT the) (NN business))
      (PP (IN of)
        (NP (NN adoption))))
    (VP (VBZ is)
      (PP (IN in)
        (NP
          (NP (DT the) (NNS billions))
          (PP (IN per)
            (NP (NN year))))))
    (. .)))

prep_in(know-6, case-2)
nsubj(know-6, you-3)
aux(know-6, do-4)
neg(know-6, n't-5)
ccomp(is-12, know-6)
det(business-9, the-8)
nsubj(is-12, business-9)
