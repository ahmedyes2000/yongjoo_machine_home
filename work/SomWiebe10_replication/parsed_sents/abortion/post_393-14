(ROOT
  (S
    (NP (JJ Wich) (NN choice))
    (VP (MD will)
      (VP (VB help)
        (S
          (NP (DT the) (NN mother))
          (VP (VB live)
            (ADVP (RBR better))
            (PP (IN after)
              (NP (DT this) (JJ terrible) (NN experience)))))))
    (. ?)))

amod(choice-2, Wich-1)
nsubj(help-4, choice-2)
aux(help-4, will-3)
root(ROOT-0, help-4)
det(mother-6, the-5)
nsubj(live-7, mother-6)
