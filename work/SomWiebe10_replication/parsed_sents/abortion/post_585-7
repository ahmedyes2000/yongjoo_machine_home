(ROOT
  (S
    (PP (IN In)
      (NP (DT an) (NN abortion)))
    (NP (DT an) (JJ innocent) (NN life))
    (VP (VBZ is)
      (ADVP (RB intentionally))
      (VP (VBN destroyed)))
    (. .)))

det(abortion-3, an-2)
prep_in(destroyed-9, abortion-3)
det(life-6, an-4)
amod(life-6, innocent-5)
nsubjpass(destroyed-9, life-6)
auxpass(destroyed-9, is-7)
advmod(destroyed-9, intentionally-8)
root(ROOT-0, destroyed-9)

