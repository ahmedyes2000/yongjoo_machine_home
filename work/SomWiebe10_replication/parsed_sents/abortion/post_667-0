(ROOT
  (SBARQ
    (WHADVP (WRB How))
    (SQ (VBP do)
      (NP (PRP you))
      (VP (VB justify)
        (NP
          (NP (VBG killing) (NNS animals))
          (PP (IN for)
            (NP (NN food)
              (CC or)
              (NN clothing))))))
    (. ?)))

advmod(justify-4, How-1)
aux(justify-4, do-2)
nsubj(justify-4, you-3)
root(ROOT-0, justify-4)
amod(animals-6, killing-5)
dobj(justify-4, animals-6)
prep_for(animals-6, food-8)
prep_for(animals-6, clothing-10)
conj_or(food-8, clothing-10)

