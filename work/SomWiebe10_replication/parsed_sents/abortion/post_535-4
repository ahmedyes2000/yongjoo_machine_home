(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (NN somebody))
        (VP (VBZ wants)
          (S
            (VP (TO to)
              (VP (VB have)
                (NP (DT a) (NN word))
                (SBAR (IN that)
                  (S
                    (NP (NNS rymes))
                    (VP (VBP whith))))))))))
    (VP (VBP shushmorshin) (RB then)
      (VP (VB let)
        (NP (PRP them))))
    (. .)))

mark(wants-3, If-1)
nsubj(wants-3, somebody-2)
xsubj(have-5, somebody-2)
csubj(shushmorshin-11, wants-3)
aux(have-5, to-4)
xcomp(wants-3, have-5)
det(word-7, a-6)
dobj(have-5, word-7)
complm(whith-10, that-8)
