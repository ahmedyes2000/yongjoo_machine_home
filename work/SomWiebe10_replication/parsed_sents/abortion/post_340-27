        (VP (VBG arguing)
          (PP (IN in)
            (NP
              (NP (NN favor))
              (PP (IN of)
                (NP
                  (NP (JJ forced) (NN sharing))
                  (PP (IN of)
                    (NP (NN body)))))))
          (SBAR (IN in) (NN order)
            (S
              (VP (TO to)
                (VP (VB save)
                  (NP (NNS lives)))))))))
    (, ,) (IN so)
    (S
      (NP (PRP I))
      (VP (VBD had)
        (S
          (VP (TO to)
            (VP (VB ask))))))
    (. .)))

