(ROOT
  (S
    (NP
      (NP (DT A) (NN woman) (POS 's))
      (NN egg))
    (VP (VBZ is)
      (NP
        (NP (NN life))
        (PP (IN in)
          (NP (NN potential)))))
    (. .)))

det(woman-2, A-1)
poss(egg-4, woman-2)
nsubj(life-6, egg-4)
cop(life-6, is-5)
root(ROOT-0, life-6)
prep_in(life-6, potential-8)

