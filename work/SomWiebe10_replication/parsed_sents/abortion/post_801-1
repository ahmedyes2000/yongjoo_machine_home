  (S
    (SBAR
      (WHADVP (WRB When))
      (S
        (NP (DT the) (NN alternative))
        (VP (VBZ is)
          (ADVP (RB possibly))
          (VP (VBG ruining)
            (NP
              (NP (DT the) (NN life))
              (PP (IN of)
                (NP (DT the) (NN mother))))))))
    (, ,)
    (NP (PRP$ her) (NN consciousness))
    (VP (VBZ takes)
      (NP (NN priority)))
    (. .)))

advmod(ruining-6, When-1)
det(alternative-3, the-2)
nsubj(ruining-6, alternative-3)
aux(ruining-6, is-4)
advmod(ruining-6, possibly-5)
advcl(takes-15, ruining-6)
det(life-8, the-7)
