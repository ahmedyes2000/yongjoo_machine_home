(ROOT
  (S
    (NP
      (NP (DT The) (NN one))
      (SBAR
        (WHNP (WP who))
        (S
          (VP (VBZ wants)
            (S
              (VP (TO to)
                (VP (VB go)
                  (ADVP (RB right)))))))))
    (VP (VBZ says)
      (SBAR
        (S
          (NP (PRP they))
          (DT both)
          (VP (VBP have)
            (S
              (VP (TO to)
                (VP (VB go)
                  (ADVP (RB right)))))))))
    (. .)))

det(one-2, The-1)
nsubj(wants-4, one-2)
nsubj(says-8, one-2)
rcmod(one-2, wants-4)
aux(go-6, to-5)
xcomp(wants-4, go-6)
advmod(go-6, right-7)
root(ROOT-0, says-8)
nsubj(have-11, they-9)
xsubj(go-13, they-9)
dep(have-11, both-10)
ccomp(says-8, have-11)
aux(go-13, to-12)
xcomp(have-11, go-13)
advmod(go-13, right-14)

