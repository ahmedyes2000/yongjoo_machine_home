(ROOT
  (S
    (NP (EX There))
    (VP (VBZ is)
      (NP
        (NP (DT no) (NN standard))
        (, ,)
        (NP
          (NP (DT no) (NN acceptance))
          (PP (IN of)
            (NP
              (NP (NNS facts))
              (, ,) (RB only)
              (NP (JJ pure) (NN opinion)))))))
    (. .)))

expl(is-2, There-1)
root(ROOT-0, is-2)
det(standard-4, no-3)
nsubj(is-2, standard-4)
det(acceptance-7, no-6)
appos(standard-4, acceptance-7)
prep_of(acceptance-7, facts-9)
advmod(facts-9, only-11)
amod(opinion-13, pure-12)
dep(facts-9, opinion-13)

