(ROOT
  (S
    (NP (PRP We))
    (VP (VBP do) (RB not)
      (VP (VB have)
        (NP (DT a) (NN right)
          (S
            (VP (TO to)
              (VP (VB procreate)))))))
    (. .)))

nsubj(have-4, We-1)
aux(have-4, do-2)
neg(have-4, not-3)
root(ROOT-0, have-4)
det(right-6, a-5)
dobj(have-4, right-6)
aux(procreate-8, to-7)
infmod(right-6, procreate-8)

