(ROOT
  (S
    (S
      (NP (NNP Again))
      (INTJ (VB please))
      (VP (VB respond)
        (PP (TO to)
          (NP (PRP$ my) (JJ other) (NN post)))))
    (NP (NNS hereI))
    (VP (VBP look)
      (ADVP (RB forward))
      (PP (TO to)
        (NP (PRP$ your) (NN response))))
    (. .)))

nsubj(respond-3, Again-1)
dep(respond-3, please-2)
dep(look-9, respond-3)
poss(post-7, my-5)
amod(post-7, other-6)
prep_to(respond-3, post-7)
nsubj(look-9, hereI-8)
root(ROOT-0, look-9)
