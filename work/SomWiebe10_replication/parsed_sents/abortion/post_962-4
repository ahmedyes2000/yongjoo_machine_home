(ROOT
  (S
    (NP (FW i))
    (VP (VBP do) (RB n't)
      (VP
        (VP (VB get)
          (SBAR
            (WHADVP (WRB how))
            (S
              (SBAR (IN if)
                (S
                  (NP (PRP you))
                  (VP (VBP kill)
                    (NP (NN somebody))
                    (ADVP (RB then)))))
              (NP (PRP you))
              (VP (VBP get)
                (VP (VBN charged)
                  (PP (IN for)
                    (NP (NN murder))))))))
        (, ,)
        (CC and)
        (VP (VB spend)
          (NP
            (NP (DT the) (NN rest))
            (PP (IN of)
              (NP (PRP$ your) (NN life))))
          (PP (IN in)
            (NP (NN prison))))))
    (: ...)))

nsubj(get-4, i-1)
nsubj(spend-18, i-1)
aux(get-4, do-2)
neg(get-4, n't-3)
root(ROOT-0, get-4)
