(ROOT
  (S
    (NP (EX There))
    (VP (VBP are)
      (NP
        (NP (JJ various) (JJ good) (NNS reasons))
        (PP (IN for)
          (S
            (VP (VBG having)
              (NP (NNS slaves)))))))
    (. .)))

expl(are-2, There-1)
root(ROOT-0, are-2)
amod(reasons-5, various-3)
amod(reasons-5, good-4)
nsubj(are-2, reasons-5)
prepc_for(reasons-5, having-7)
dobj(having-7, slaves-8)

