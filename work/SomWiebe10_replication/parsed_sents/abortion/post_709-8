(ROOT
  (S
    (NP (NNS People))
    (VP (VBD did) (RB n't)
      (VP (VB believe)
        (SBAR
          (S
            (NP (NNS blacks))
            (VP (VBD had)
              (NP (JJ human) (NNS rights)))))))
    (. .)))

nsubj(believe-4, People-1)
aux(believe-4, did-2)
neg(believe-4, n't-3)
root(ROOT-0, believe-4)
nsubj(had-6, blacks-5)
ccomp(believe-4, had-6)
amod(rights-8, human-7)
dobj(had-6, rights-8)

