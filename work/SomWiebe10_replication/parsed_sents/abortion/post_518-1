(ROOT
  (S
    (NP (DT The) (NN root))
    (VP (VBP question)
      (S
        (VP (VBZ is)))
      ('' '')
      (SBAR
        (WHADVP (WRB When))
        (S
          (VP (VBZ does)
            (S
              (NP (NN life))
              (VP (VB begin)))))))
    (. ?)))

det(root-2, The-1)
nsubj(question-3, root-2)
root(ROOT-0, question-3)
ccomp(question-3, is-4)
advmod(does-7, When-6)
advcl(question-3, does-7)
nsubj(begin-9, life-8)
ccomp(does-7, begin-9)

