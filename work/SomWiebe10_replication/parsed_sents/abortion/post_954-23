(ROOT
  (S
    (S (IN So)
      (NP (PRP I))
      (VP (VBD put)
        (ADVP
          (NP (DT this) (NN argument))
          (RB forth))
        (PP (IN for)
          (NP
            (NP (DT those))
            (SBAR
              (WHNP (WP who))
              (S
                (VP (VBP are)
                  (ADJP (RB truly) (JJ open-minded)))))))))
    (: ;)
    (S
      (PP (IN in)
        (NP (NNS hopes)))
      (NP (PRP you))
      (VP (MD will)
        (VP (VB be)
          (VP (VBN persuaded)))))
    (. .)))

