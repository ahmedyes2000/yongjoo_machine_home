(ROOT
  (S
    (NP (PRP You))
    (VP (VBP are) (RB not)
      (ADJP (JJ obliged)
        (S
          (VP (TO to)
            (VP (VB see)
              (NP (PRP it))
              (PP (TO to)
                (NP (NN term)))
              (SBAR (IN if)
                (S
                  (NP (PRP you))
                  (VP (VBP choose)
                    (S (RB not)
                      (VP (TO to)))))))))))
    (. .)))

nsubj(obliged-4, You-1)
cop(obliged-4, are-2)
neg(obliged-4, not-3)
root(ROOT-0, obliged-4)
aux(see-6, to-5)
