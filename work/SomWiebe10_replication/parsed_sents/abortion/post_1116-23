(ROOT
  (S
    (SBAR
      (WHADVP (WRB When))
      (S
        (NP (PRP you))
        (VP (NN plant)
          (NP
            (NP (DT a) (NN tree))
            (SBAR
              (S
                (NP (PRP you))
                (VP (VBP do) (RB n't)
                  (ADVP (RB immediately))
                  (VP (VB get)
                    (NP (NN fruit))))))))))
    (, ,)
    (NP (PRP it))
    (VP (VBZ takes)
      (NP (NN time)))
    (. .)))

advmod(plant-3, When-1)
nsubj(plant-3, you-2)
advcl(takes-14, plant-3)
det(tree-5, a-4)
dobj(plant-3, tree-5)
nsubj(get-10, you-6)
aux(get-10, do-7)
neg(get-10, n't-8)
advmod(get-10, immediately-9)
rcmod(tree-5, get-10)
dobj(get-10, fruit-11)
nsubj(takes-14, it-13)
root(ROOT-0, takes-14)
dobj(takes-14, time-15)

