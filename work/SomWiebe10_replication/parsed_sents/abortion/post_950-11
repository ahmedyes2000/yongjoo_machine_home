(ROOT
  (S
    (S
      (PP (IN With)
        (NP (NNP God)))
      (NP (EX there))
      (VP (VBZ is)
        (NP (NN forgiveness))))
    (, ,)
    (FRAG (CC but)
      (RB not)
      (PP (IN without)
        (NP (JJ great) (NN cost))))
    (. .)))

prep_with(is-4, God-2)
expl(is-4, there-3)
root(ROOT-0, is-4)
nsubj(is-4, forgiveness-5)
dep(not-8, but-7)
dep(is-4, not-8)
dep(not-8, without-9)
amod(cost-11, great-10)
pobj(without-9, cost-11)

