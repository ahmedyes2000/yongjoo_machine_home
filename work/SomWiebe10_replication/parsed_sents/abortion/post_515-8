(ROOT
  (S
    (NP (JJ Racial)
      (CC and)
      (JJ ethnic) (NNS disparities))
    (ADVP (RB clearly))
    (VP (VBP play)
      (NP (DT a) (NN role)))
    (. .)))

amod(disparities-4, Racial-1)
conj_and(Racial-1, ethnic-3)
amod(disparities-4, ethnic-3)
nsubj(play-6, disparities-4)
advmod(play-6, clearly-5)
root(ROOT-0, play-6)
det(role-8, a-7)
dobj(play-6, role-8)

