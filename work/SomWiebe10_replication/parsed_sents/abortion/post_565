(ROOT
  (S
    (PP (IN For)
      (NP (NN biology)))
    (, ,)
    (NP (NN life))
    (VP (VBZ is)
      (NP (DT a) (NN continuum)))
    (. .)))

prep_for(continuum-7, biology-2)
nsubj(continuum-7, life-4)
cop(continuum-7, is-5)
det(continuum-7, a-6)
root(ROOT-0, continuum-7)

(ROOT
  (S
    (ADVP (RB Obviously))
    (NP (EX there))
    (VP (MD could)
      (VP (VB be)
        (NP
          (NP (DT no) (NN conception))
          (PP (IN without)
            (NP
              (NP (DT the) (VBG living) (NN participation))
              (PP (IN of)
                (NP (DT the) (NN sperm)
                  (CC and)
                  (NN egg))))))))
    (. .)))

advmod(conception-6, Obviously-1)
expl(conception-6, there-2)
aux(conception-6, could-3)
cop(conception-6, be-4)
det(conception-6, no-5)
root(ROOT-0, conception-6)
det(participation-10, the-8)
amod(participation-10, living-9)
prep_without(conception-6, participation-10)
det(sperm-13, the-12)
prep_of(participation-10, sperm-13)
prep_of(participation-10, egg-15)
conj_and(sperm-13, egg-15)

