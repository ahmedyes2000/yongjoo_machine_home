(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (NN god)
          (CC or)
          (NN whoever))
        (VP (VBZ does) (RB n't)
          (VP (VB like)
            (NP (PRP it))))))
    (, ,)
    (ADVP (RB then))
    (NP (PRP he))
    (VP (MD can)
      (VP
        (VP (VB come)
          (PRT (RP down))
          (ADVP (RB here)))
        (CC and)
        (VP (VB tell)
          (NP (PRP her))
          (NP (PRP himself)))))
    (. .)))

mark(like-7, If-1)
nsubj(like-7, god-2)
conj_or(god-2, whoever-4)
nsubj(like-7, whoever-4)
aux(like-7, does-5)
neg(like-7, n't-6)
advcl(come-13, like-7)
dobj(like-7, it-8)
advmod(come-13, then-10)
nsubj(come-13, he-11)
nsubj(tell-17, he-11)
aux(come-13, can-12)
root(ROOT-0, come-13)
