(ROOT
  (S
    (NP (PRP They))
    (VP (VBP make)
      (NP
        (NP (DT the) (NN decision))
        (PP (IN for)
          (NP (DT the) (JJR greater) (NN good))))
      (: --)
      (NP (DT the) (NN family)
        (CC and)
        (NN society)))
    (. .)))

nsubj(make-2, They-1)
root(ROOT-0, make-2)
det(decision-4, the-3)
dobj(make-2, decision-4)
det(good-8, the-6)
amod(good-8, greater-7)
prep_for(decision-4, good-8)
det(family-11, the-10)
dobj(make-2, family-11)
dobj(make-2, society-13)
conj_and(family-11, society-13)

