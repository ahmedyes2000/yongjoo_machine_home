(ROOT
  (S
    (NP (DT That))
    (VP (VBZ 's)
      (SBAR
        (WHNP (WP what))
        (S
          (VP (VBZ makes)
            (NP (PRP it))
            (NP
              (NP (DT a) (JJ real) (JJ human) (NN being))
              (, ,)
              (CONJP (RB not) (RB just))
              (NP
                (NP (DT a) (NN part))
                (PP (IN of)
                  (NP (DT a) (NN human) (VBG being)))))))))
    (. .)))

nsubj('s-2, That-1)
root(ROOT-0, 's-2)
nsubj(makes-4, what-3)
ccomp('s-2, makes-4)
iobj(makes-4, it-5)
det(being-9, a-6)
amod(being-9, real-7)
amod(being-9, human-8)
dobj(makes-4, being-9)
neg(just-12, not-11)
dep(being-9, just-12)
det(part-14, a-13)
conj(being-9, part-14)
det(human-17, a-16)
prep_of(part-14, human-17)
amod(human-17, being-18)

