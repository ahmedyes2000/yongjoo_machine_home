(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (DT the) (NN baby))
        (VP (VBZ endangers)
          (NP
            (NP (DT the) (NN mother) (POS 's))
            (NN life)))))
    (, ,)
    (ADVP (RB then))
    (S
      (INTJ (UH yes))
      (, ,)
      (NP (DT a) (JJ late-term) (NN abortion))
      (VP (VBZ is)
        (ADJP (JJ justified))))
    (. .)))

mark(endangers-4, If-1)
det(baby-3, the-2)
nsubj(endangers-4, baby-3)
advcl(justified-17, endangers-4)
det(mother-6, the-5)
poss(life-8, mother-6)
dobj(endangers-4, life-8)
advmod(justified-17, then-10)
dep(justified-17, yes-11)
det(abortion-15, a-13)
amod(abortion-15, late-term-14)
nsubj(justified-17, abortion-15)
cop(justified-17, is-16)
root(ROOT-0, justified-17)

