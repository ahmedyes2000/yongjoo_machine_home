(ROOT
  (S
    (NP (PRP You))
    (VP (VBP are)
      (ADJP (JJ good)
        (PP (IN at)
          (S
            (VP (VBG making)
              (S
                (NP (PRP$ my) (NN point))
                (VP (VB thank)
                  (NP (PRP you)))))))))
    (. .)))

nsubj(good-3, You-1)
cop(good-3, are-2)
root(ROOT-0, good-3)
prepc_at(good-3, making-5)
poss(point-7, my-6)
nsubj(thank-8, point-7)
ccomp(making-5, thank-8)
dobj(thank-8, you-9)

