(ROOT
  (S
    (S
      (NP (PRP I))
      (VP (VBP understand)
        (NP (PRP$ your) (NN viewpoint))))
    (, ,)
    (CC but)
    (S
      (NP (PRP I))
      (VP (VBP disagree)
        (PP (IN with)
          (NP (PRP it)))))
    (. .)))

nsubj(understand-2, I-1)
root(ROOT-0, understand-2)
poss(viewpoint-4, your-3)
dobj(understand-2, viewpoint-4)
nsubj(disagree-8, I-7)
conj_but(understand-2, disagree-8)
prep_with(disagree-8, it-10)

