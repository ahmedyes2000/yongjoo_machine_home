(ROOT
  (S
    (NP (PRP I))
    (ADVP (RB also))
    (VP (VBP think)
      (S
        (NP (DT these) (NNS terms))
        (VP (VB extend)
          (PP (TO to)
            (NP (NNS animals))))))
    (. .)))

nsubj(think-3, I-1)
advmod(think-3, also-2)
root(ROOT-0, think-3)
det(terms-5, these-4)
nsubj(extend-6, terms-5)
ccomp(think-3, extend-6)
prep_to(extend-6, animals-8)

