(ROOT
  (S
    (PP (IN During)
      (NP (DT the) (JJ third) (NN trimester)))
    (NP (NN abortion))
    (VP (MD should)
      (ADVP (RB only))
      (VP (VB be)
        (VP (VBN allowed)
          (S
            (VP (TO to)
              (VP (VB protect)
                (NP
                  (NP (DT the) (NN health))
                  (PP (IN of)
                    (NP (DT the) (NN baby))))))))))
    (. .)))

det(trimester-4, the-2)
amod(trimester-4, third-3)
prep_during(allowed-9, trimester-4)
nsubjpass(allowed-9, abortion-5)
aux(allowed-9, should-6)
advmod(allowed-9, only-7)
auxpass(allowed-9, be-8)
root(ROOT-0, allowed-9)
aux(protect-11, to-10)
xcomp(allowed-9, protect-11)
det(health-13, the-12)
dobj(protect-11, health-13)
det(baby-16, the-15)
prep_of(health-13, baby-16)

