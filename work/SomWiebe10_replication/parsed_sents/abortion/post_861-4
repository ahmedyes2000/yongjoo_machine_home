(ROOT
  (S
    (NP (DT The) (JJ only) (NN event) (NN horizon))
    (VP (MD would)
      (VP (VB be)
        (NP
          (NP (NN life))
          (PP (IN at)
            (NP (NN conception))))))
    (. .)))

det(horizon-4, The-1)
amod(horizon-4, only-2)
nn(horizon-4, event-3)
nsubj(life-7, horizon-4)
aux(life-7, would-5)
cop(life-7, be-6)
