(ROOT
  (S
    (NP (PRP I))
    (VP (VBP 'm)
      (VP (VBN confused)
        (PP (IN with)
          (NP (PRP$ your) (NN argument)))))
    (. .)))

nsubjpass(confused-3, I-1)
auxpass(confused-3, 'm-2)
root(ROOT-0, confused-3)
poss(argument-6, your-5)
prep_with(confused-3, argument-6)

(ROOT
  (SQ (VBD Are)
    (NP (PRP you))
    (NP (NN saying))
    (SBAR (IN that)
      (S
        (NP (PRP it))
        (VP (MD should)
          (VP (VB be)
            (ADJP (JJ legal)
              (CC or)
              (JJ illegal))))))
    (. ?)))

cop(saying-3, Are-1)
nsubj(saying-3, you-2)
root(ROOT-0, saying-3)
complm(legal-8, that-4)
nsubj(legal-8, it-5)
nsubj(illegal-10, it-5)
aux(legal-8, should-6)
cop(legal-8, be-7)
dep(saying-3, legal-8)
dep(saying-3, illegal-10)
conj_or(legal-8, illegal-10)

