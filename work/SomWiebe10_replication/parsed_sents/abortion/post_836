nsubj(asking-10, man-8)
aux(asking-10, is-9)
ccomp(assume-4, asking-10)
nsubj(save-13, you-11)
aux(save-13, to-12)
xcomp(asking-10, save-13)
dobj(save-13, him-14)
det(child-18, the-17)
prep_instead_of(save-13, child-18)

(ROOT
  (S
    (S
      (ADVP (RB Clearly))
      (NP (DT the) (JJ old) (NN man))
      (VP (MD will)
        (VP (VB die)
          (PP (IN in)
            (NP (DT a) (JJ few) (NNS years))))))
    (, ,)
    (CC but)
    (S
      (NP (DT the) (NN boy))
      (VP (VBZ has)
        (NP
          (NP (PRP$ his) (JJ whole) (NN life))
          (PP (RB ahead) (IN of)
            (NP (PRP him))))))
    (. .)))

