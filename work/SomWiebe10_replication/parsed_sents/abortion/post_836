(ROOT
  (S
    (VP
      (VP (VB Go)
        (ADVP (RB ahead)))
      (CC and)
      (VP (VB assume)
        (SBAR (IN that)
          (S
            (NP (DT the) (JJ old) (NN man))
            (VP (VBZ is)
              (VP (VBG asking)
                (S
                  (NP (PRP you))
                  (VP (TO to)
                    (VP (VB save)
                      (NP (PRP him))
                      (PP (RB instead) (IN of)
                        (NP (DT the) (NN child))))))))))))
    (. .)))

root(ROOT-0, Go-1)
advmod(Go-1, ahead-2)
conj_and(Go-1, assume-4)
complm(asking-10, that-5)
det(man-8, the-6)
