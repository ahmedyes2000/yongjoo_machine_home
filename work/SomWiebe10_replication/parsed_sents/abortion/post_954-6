(ROOT
  (S (IN So)
    (NP (PRP$ my) (NN effort))
    (VP (VBZ is)
      (S
        (VP (TO to)
          (VP (VB make)
            (SBAR (IN that)
              (S
                (VP (VBP know)
                  (SBAR (IN as)
                    (S
                      (ADVP (RB best))
                      (NP (PRP I))
                      (VP (MD can)))))))))))
    (. .)))

dep(is-4, So-1)
poss(effort-3, my-2)
nsubj(is-4, effort-3)
xsubj(make-6, effort-3)
root(ROOT-0, is-4)
aux(make-6, to-5)
xcomp(is-4, make-6)
complm(know-8, that-7)
ccomp(make-6, know-8)
mark(can-12, as-9)
advmod(can-12, best-10)
nsubj(can-12, I-11)
advcl(know-8, can-12)

