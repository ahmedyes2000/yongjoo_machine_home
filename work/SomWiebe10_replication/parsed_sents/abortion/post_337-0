(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ has)
      (ADVP (RB always))
      (VP (VBN been)
        (ADJP (JJ convenient)
          (PP (IN for)
            (NP
              (NP (NNP King) (NNP Henry))
              (PP (IN of)
                (NP (NNP England)))))
          (S
            (VP (TO to)
              (VP (VB kill)
                (NP (PRP$ his)
                  (ADJP (RB newly) (JJ born))
                  (NNS daughters))))))
        (SBAR (IN because)
          (S
            (NP (PRP they))
            (VP (VBD were) (RB not)
              (NP (NNS males)))))))
    (. .)))

nsubj(convenient-5, It-1)
aux(convenient-5, has-2)
advmod(convenient-5, always-3)
cop(convenient-5, been-4)
root(ROOT-0, convenient-5)
nn(Henry-8, King-7)
prep_for(convenient-5, Henry-8)
prep_of(Henry-8, England-10)
aux(kill-12, to-11)
xcomp(convenient-5, kill-12)
poss(daughters-16, his-13)
advmod(born-15, newly-14)
amod(daughters-16, born-15)
dobj(kill-12, daughters-16)
mark(males-21, because-17)
nsubj(males-21, they-18)
cop(males-21, were-19)
neg(males-21, not-20)
advcl(convenient-5, males-21)

