(ROOT
  (S
    (ADVP (RB First))
    (, ,)
    (NP
      (NP (DT those))
      (SBAR
        (WHNP (WDT that))
        (S
          (VP (VBP are)
            (ADJP (JJ subject)
              (PP (TO to)
                (NP (NN capital) (NN punishment))))))))
    (VP (VBP are)
      (ADVP (RB often))
      (NP
        (NP (NNS murderers))
        (CC or)
        (NP (JJ extreme) (NNS offenders))))
    (. .)))

advmod(murderers-12, First-1)
nsubj(subject-6, those-3)
nsubj(murderers-12, those-3)
cop(subject-6, are-5)
rcmod(those-3, subject-6)
nn(punishment-9, capital-8)
prep_to(subject-6, punishment-9)
cop(murderers-12, are-10)
advmod(murderers-12, often-11)
root(ROOT-0, murderers-12)
amod(offenders-15, extreme-14)
