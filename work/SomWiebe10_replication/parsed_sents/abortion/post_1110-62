(ROOT
  (S
    (NP (PRP We))
    (VP (VBP 've)
      (VP (VBN got)
        (S
          (VP (TO to)
            (VP (VB take)
              (PP (IN into)
                (NP (NN account)))
              (NP
                (NP (DT those))
                (SBAR
                  (WHNP (WP who))
                  (S
                    (VP (MD will)
                      (VP (VB fail)
                        (PP (TO to)
                          (NP (NN abstain)))))))))))))
    (. .)))

nsubj(got-3, We-1)
xsubj(take-5, We-1)
aux(got-3, 've-2)
root(ROOT-0, got-3)
aux(take-5, to-4)
xcomp(got-3, take-5)
prep_into(take-5, account-7)
dobj(take-5, those-8)
nsubj(fail-11, those-8)
aux(fail-11, will-10)
rcmod(those-8, fail-11)
prep_to(fail-11, abstain-13)

