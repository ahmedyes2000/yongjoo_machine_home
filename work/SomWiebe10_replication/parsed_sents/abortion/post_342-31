(ROOT
  (S
    (S
      (NP (PRP$ Your) (NN body) (NNS parts))
      (VP (MD could)
        (VP (VB have)
          (VP (VBN prevented)
            (NP (PRP it))))))
    (CC and)
    (S
      (NP (PRP you))
      (ADVP (RB simply))
      (VP (VBD chose)
        (NP (DT the) (JJ other) (NN direction))))
    (. .)))

poss(parts-3, Your-1)
nn(parts-3, body-2)
nsubj(prevented-6, parts-3)
aux(prevented-6, could-4)
aux(prevented-6, have-5)
root(ROOT-0, prevented-6)
