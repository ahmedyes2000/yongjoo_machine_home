(ROOT
  (S
    (NP (NNP Women))
    (ADVP (RB rarely))
    (VP (VBP become)
      (ADJP (JJ pregnant)
        (SBAR (IN if)
          (S
            (NP (PRP they))
            (VP (VBP are)
              (VP (VBN raped)))))))
    (. .)))

nsubj(pregnant-4, Women-1)
advmod(pregnant-4, rarely-2)
cop(pregnant-4, become-3)
root(ROOT-0, pregnant-4)
mark(raped-8, if-5)
nsubjpass(raped-8, they-6)
auxpass(raped-8, are-7)
ccomp(pregnant-4, raped-8)

