(ROOT
  (S
    (S
      (NP (PRP I))
      (ADVP (RB personally))
      (VP (VBP hope)
        (SBAR
          (S
            (NP (PRP it))
            (VP (VBZ is) (RB n't)
              (NP (NN abortion)))))))
    (, ,)
    (CC but)
    (S
      (NP (PRP it))
      (VP (VBZ 's) (RB not)
        (NP (PRP$ our) (NN call))))
    (. .)))

nsubj(hope-3, I-1)
advmod(hope-3, personally-2)
root(ROOT-0, hope-3)
nsubj(abortion-7, it-4)
cop(abortion-7, is-5)
neg(abortion-7, n't-6)
ccomp(hope-3, abortion-7)
nsubj(call-14, it-10)
cop(call-14, 's-11)
neg(call-14, not-12)
poss(call-14, our-13)
conj_but(hope-3, call-14)

