(ROOT
  (S
    (NP (PRP He))
    (VP (MD should)
      (VP (VB have)
        (NP
          (NP (PRP$ his) (NN money))
          (VP (VBN allocated)
            (NP (DT the) (NN mother))))))
    (. .)))

nsubj(have-3, He-1)
aux(have-3, should-2)
root(ROOT-0, have-3)
poss(money-5, his-4)
dobj(have-3, money-5)
partmod(money-5, allocated-6)
det(mother-8, the-7)
dobj(allocated-6, mother-8)

