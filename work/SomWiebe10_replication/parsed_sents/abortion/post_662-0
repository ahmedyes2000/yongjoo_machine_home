(ROOT
  (S
    (NP (PRP I))
    (VP (VBP do) (RB n't)
      (VP (VB believe)
        (SBAR (IN that)
          (S
            (NP (PRP it))
            (VP (MD would)
              (VP (VB be)
                (ADJP (JJ easy)
                  (S
                    (VP (TO to)
                      (VP (VB end)
                        (PRT (RP up))
                        (PP (IN like)
                          (NP (NNPS Nazis)))))))))))))
    (. .)))

nsubj(believe-4, I-1)
aux(believe-4, do-2)
neg(believe-4, n't-3)
root(ROOT-0, believe-4)
complm(easy-9, that-5)
nsubj(easy-9, it-6)
aux(easy-9, would-7)
cop(easy-9, be-8)
ccomp(believe-4, easy-9)
aux(end-11, to-10)
