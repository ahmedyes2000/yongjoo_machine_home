(ROOT
  (S
    (PP (IN In)
      (NP (NN fact)))
    (NP (PRP they))
    (VP (VBP are)
      (NP
        (NP (NN life))
        (VP (VBG shattering)
          (PP (IN for)
            (NP (DT the) (NN woman))))))
    (. .)))

prep_in(life-5, fact-2)
nsubj(life-5, they-3)
cop(life-5, are-4)
root(ROOT-0, life-5)
partmod(life-5, shattering-6)
det(woman-9, the-8)
prep_for(shattering-6, woman-9)

