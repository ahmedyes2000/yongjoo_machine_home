(ROOT
  (S
    (NP (VBG Ionizing) (NN radiation))
    (VP (MD should)
      (VP (VB be)
        (VP (VBN avoided)
          (PP (IN during)
            (NP (NN pregnancy)))
          (FRAG
            (ADJP
              (WHADVP (WRB whenever))
              (JJ possible))))))
    (. .)))

amod(radiation-2, Ionizing-1)
nsubjpass(avoided-5, radiation-2)
aux(avoided-5, should-3)
auxpass(avoided-5, be-4)
root(ROOT-0, avoided-5)
prep_during(avoided-5, pregnancy-7)
advmod(possible-9, whenever-8)
dep(avoided-5, possible-9)

