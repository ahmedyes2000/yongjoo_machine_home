(ROOT
  (S
    (ADVP (RB However))
    (, ,)
    (NP (EX there))
    (VP (VBD was)
      (ADVP (RB never))
      (ADVP (DT any) (NN doubt)
        (SBAR (IN that)
          (S
            (NP
              (NP (DT the) (NN destruction))
              (PP (IN of)
                (NP
                  (NP (DT an) (JJ unborn) (NN child))
                  (PP (IN at)
                    (NP (DT any) (NN stage))))))
            (VP (VBD was)
              (NP
                (NP (DT an) (NN act))
                (PP (IN of)
                  (NP (NN evil)))))))))
    (. .)))

advmod(was-4, However-1)
expl(was-4, there-3)
root(ROOT-0, was-4)
neg(was-4, never-5)
dep(doubt-7, any-6)
advmod(was-4, doubt-7)
complm(act-20, that-8)
det(destruction-10, the-9)
nsubj(act-20, destruction-10)
det(child-14, an-12)
amod(child-14, unborn-13)
