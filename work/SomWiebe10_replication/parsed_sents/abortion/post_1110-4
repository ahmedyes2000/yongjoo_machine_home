  (S (CC And)
    (NP (PRP it))
    (VP (VBZ is)
      (PP (IN for)
        (NP (DT this) (NN reason)))
      (SBAR (IN that)
        (S
          (S
            (NP (PRP I))
            (ADVP (RB strongly))
            (VP (VBP believe)
              (PP (IN in)
                (S (RB not)
                  (VP (VBG banning)
                    (NP (NNS abortions)))))))
          (CC and)
          (S
            (NP (PRP I))
            (ADVP (RB strongly))
            (VP (VBP believe)
              (SBAR (IN that)
                (S
                  (NP (JJ abstinence) (JJ only) (NN education))
                  (VP (MD should)
                    (VP (VB be)
                      (VP (VBN replaced)
                        (PP (IN by)
                          (NP (JJ comprehensive) (JJ sexual) (NN education)))))))))))))
    (. .)))

