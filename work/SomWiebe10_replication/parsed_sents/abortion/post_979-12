(ROOT
  (S (CC But)
    (SBAR (IN if)
      (S
        (NP (PRP it))
        (VP (VBZ 's)
          (NP
            (NP (DT a) (NN human))
            (VP (VBG being)
              (VP (VBN killed)
                (PP (IN in)
                  (NP (DT an) (NN abortion)))))))))
    (ADVP (RB then))
    (NP (DT no) (NN justification))
    (VP (VBZ is)
      (ADJP (JJ adequate)))
    (. .)))

cc(adequate-16, But-1)
mark(human-6, if-2)
nsubj(human-6, it-3)
cop(human-6, 's-4)
det(human-6, a-5)
dep(adequate-16, human-6)
auxpass(killed-8, being-7)
