(ROOT
  (S
    (NP (DT A) (NN gamete))
    (VP (MD would)
      (ADVP (RB still))
      (VP (VB remain)
        (NP (DT a) (NN gamete))
        (SBAR (IN if)
          (S
            (NP (EX there))
            (VP (VBZ 's)
              (NP
                (NP (DT no) (NN union))
                (SBAR
                  (WHNP (WDT that))
                  (S
                    (VP (VBP occur))))))))))
    (. .)))

det(gamete-2, A-1)
nsubj(gamete-7, gamete-2)
aux(gamete-7, would-3)
advmod(gamete-7, still-4)
