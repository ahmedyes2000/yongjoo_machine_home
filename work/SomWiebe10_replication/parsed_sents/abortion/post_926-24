(ROOT
  (NP
    (NP (JJ Final) (NN thought))
    (: :)
    (SQ
      (VP (VBD Were))
      (NP (PRP you))
      (ADVP (RB ever))
      (NP (DT a) (NN fetus))
      (. ?))))

amod(thought-2, Final-1)
root(ROOT-0, thought-2)
dep(thought-2, Were-4)
dep(Were-4, you-5)
advmod(Were-4, ever-6)
det(fetus-8, a-7)
dep(Were-4, fetus-8)

