(ROOT
  (S ('' '')
    (ADVP (RB Well))
    (, ,)
    (NP (DT that))
    (VP (VBZ is)
      (ADJP
        (ADVP (NN kind) (IN of))
        (JJ funny)
        (PP (TO to)
          (NP
            (NP (PRP me))
            (PP (IN in)
              (NP (DT a) (NN way)))))))
    (. .)))

advmod(funny-8, Well-2)
nsubj(funny-8, that-4)
cop(funny-8, is-5)
dep(of-7, kind-6)
advmod(funny-8, of-7)
root(ROOT-0, funny-8)
prep_to(funny-8, me-10)
det(way-13, a-12)
prep_in(me-10, way-13)

