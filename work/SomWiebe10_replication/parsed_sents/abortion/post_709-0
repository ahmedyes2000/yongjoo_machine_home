(ROOT
  (S
    (NP (PRP You))
    (VP (VBP 're)
      (VP (VBG assuming)
        (SBAR (IN that)
          (S
            (NP (JJ universal) (NN truth))
            (VP (VBZ is)
              (NP
                (NP (DT a) (NN matter))
                (PP (IN of)
                  (NP (NN opinion)
                    (CC or)
                    (JJ personal) (NN belief)))))))))
    (. .)))

nsubj(assuming-3, You-1)
aux(assuming-3, 're-2)
root(ROOT-0, assuming-3)
complm(matter-9, that-4)
amod(truth-6, universal-5)
nsubj(matter-9, truth-6)
cop(matter-9, is-7)
det(matter-9, a-8)
