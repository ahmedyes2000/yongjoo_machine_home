(ROOT
  (S
    (NP (PRP I))
    (VP (VBP 'm)
      (VP (VBG choosing)
        (S
          (VP (TO to)
            (VP (VB kill)
              (NP (NN someone))
              (PP (IN through)
                (NP (PRP$ my) (NN inaction))))))))
    (. .)))

nsubj(choosing-3, I-1)
xsubj(kill-5, I-1)
aux(choosing-3, 'm-2)
root(ROOT-0, choosing-3)
aux(kill-5, to-4)
xcomp(choosing-3, kill-5)
dobj(kill-5, someone-6)
poss(inaction-9, my-8)
prep_through(kill-5, inaction-9)

