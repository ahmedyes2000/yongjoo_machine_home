(ROOT
  (S
    (ADVP (RB Perhaps))
    (NP (PRP I))
    (VP (MD should)
      (VP (VB have)
        (VP (VBN been)
          (ADJP (RBR more) (JJ specific))
          (PP (IN in)
            (NP (PRP$ my) (NN example))))))
    (. .)))

advmod(specific-7, Perhaps-1)
nsubj(specific-7, I-2)
aux(specific-7, should-3)
aux(specific-7, have-4)
cop(specific-7, been-5)
advmod(specific-7, more-6)
root(ROOT-0, specific-7)
poss(example-10, my-9)
prep_in(specific-7, example-10)

