(ROOT
  (S
    (NP
      (NP (NN Anyone))
      (SBAR
        (WHNP (WP who))
        (S
          (VP (VBZ looks)
            (PP (IN past)
              (NP (DT the) (NN rhetoric)))
            (PP (TO to)
              (NP
                (NP (NNP Obama) (POS 's))
                (JJ actual) (NN record)))))))
    (VP (VBZ knows)
      (SBAR (IN that)
        (S
          (VP (VBZ is)
            (NP (DT the) (NN truth))))))
    (. .)))

nsubj(looks-3, Anyone-1)
nsubj(knows-12, Anyone-1)
rcmod(Anyone-1, looks-3)
det(rhetoric-6, the-5)
prep_past(looks-3, rhetoric-6)
poss(record-11, Obama-8)
amod(record-11, actual-10)
prep_to(looks-3, record-11)
root(ROOT-0, knows-12)
complm(truth-16, that-13)
cop(truth-16, is-14)
det(truth-16, the-15)
ccomp(knows-12, truth-16)

