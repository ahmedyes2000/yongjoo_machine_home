  (S
    (NP (NNP Senator) (NNP Obama))
    (VP (VBZ has)
      (VP (VBN promised)
        (S
          (VP (TO to)
            (VP (VB sign)
              (NP
                (NP (DT the) (NN Freedom))
                (PP (IN of)
                  (NP (NNP Choice) (NNP Act))))
              (SBAR (IN if)
                (S
                  (VP (VBD elected)
                    (NP (NNP President))))))))))
    (. .)))

nn(Obama-2, Senator-1)
nsubj(promised-4, Obama-2)
xsubj(sign-6, Obama-2)
aux(promised-4, has-3)
root(ROOT-0, promised-4)
