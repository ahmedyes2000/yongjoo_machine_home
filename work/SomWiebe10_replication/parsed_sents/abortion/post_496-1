(ROOT
  (S
    (ADVP (RB Seriously))
    (NP (PRP it))
    (VP (VBZ is) (RB n't)
      (NP (DT that))
      (ADVP (RB hard))
      (S (RB not)
        (VP (TO to)
          (VP (VB spread)
            (NP (PRP$ your) (NNS legs))))))
    (. .)))

advmod(that-5, Seriously-1)
nsubj(that-5, it-2)
xsubj(spread-9, it-2)
cop(that-5, is-3)
neg(that-5, n't-4)
root(ROOT-0, that-5)
advmod(that-5, hard-6)
neg(spread-9, not-7)
aux(spread-9, to-8)
xcomp(that-5, spread-9)
poss(legs-11, your-10)
dobj(spread-9, legs-11)

