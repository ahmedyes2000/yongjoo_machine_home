(ROOT
  (S
    (ADVP (RB Again))
    (, ,)
    (SBAR
      (WHADVP (WRB when))
      (S
        (NP (JJ human) (NN life))
        (VP (VBZ begins))))
    (VP (VBZ is)
      (ADJP (JJ clear))
      (: :)
      (PP (IN at)
        (S
          (VP (VBG conception)))))
    (. .)))

advmod(clear-8, Again-1)
advmod(begins-6, when-3)
amod(life-5, human-4)
nsubj(begins-6, life-5)
advcl(clear-8, begins-6)
csubj(clear-8, begins-6)
cop(clear-8, is-7)
root(ROOT-0, clear-8)
prepc_at(clear-8, conception-11)

