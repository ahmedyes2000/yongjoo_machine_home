(ROOT
  (S
    (SBAR (IN Once)
      (S
        (NP (PRP they))
        (VP (VBP turn)
          (NP (CD 18)))))
    (NP (PRP they))
    (VP (VBP are)
      (PRT (RP out))
      (PP (IN of)
        (NP
          (NP (DT the) (NN system))
          (PP (IN except)
            (NP
              (NP (DT those))
              (PP (IN with)
                (NP (JJ medical) (NNS disabilities))))))))
    (. .)))

mark(turn-3, Once-1)
nsubj(turn-3, they-2)
advcl(are-6, turn-3)
dobj(turn-3, 18-4)
nsubj(are-6, they-5)
root(ROOT-0, are-6)
det(system-10, the-9)
prep_out_of(are-6, system-10)
prep_except(system-10, those-12)
amod(disabilities-15, medical-14)
prep_with(those-12, disabilities-15)

