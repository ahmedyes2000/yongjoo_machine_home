(ROOT
  (S
    (NP (PRP I))
    (, ,)
    (PP (IN for)
      (NP (CD one)))
    (, ,)
    (VP (MD would)
      (ADVP (RB never))
      (VP (VB ask)
        (NP (DT a) (NN rape) (NN victim))
        (S
          (VP (TO to)
            (VP (VB carry)
              (NP
                (NP (DT the) (NNS results))
                (PP (IN of)
                  (NP (DT that) (NN rape)))))))))
    (. .)))

nsubj(ask-8, I-1)
prep_for(ask-8, one-4)
aux(ask-8, would-6)
neg(ask-8, never-7)
root(ROOT-0, ask-8)
det(victim-11, a-9)
nn(victim-11, rape-10)
