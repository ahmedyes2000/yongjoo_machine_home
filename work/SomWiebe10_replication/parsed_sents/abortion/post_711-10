  (S
    (INTJ (DT That) (VBZ is))
    (, ,)
    (S
      (NP (PRP I))
      (VP (VBP have) (RB n't)
        (VP (VBN seen)
          (NP
            (NP (NNS people) (POS 's))
            (NNS attitudes))
          (PP (IN about)
            (SBAR
              (WHADVP (WRB how))
              (S
                (NP (PRP they))
                (VP (VBP think)
                  (SBAR
                    (S
                      (NP (PRP they))
                      (VP (MD should)
                        (VP (VB be)
                          (VP (VBN treated)
                            (NP (NN change))))))))))))))
    (. .)))

