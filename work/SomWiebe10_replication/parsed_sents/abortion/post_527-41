(ROOT
  (SBARQ
    (WHADVP (IN that))
    (SQ (VBZ is)
      (NP (DT the) (NN freedom) (FW i))
      (VP (VBN have)
        (PP (IN over)
          (NP (PRP$ my) (NN body)))
        (, ,)
        (SBAR (IN though)
          (S
            (NP (FW i))
            (VP (VBD chose) (RB not)
              (NP (DT the) (NN form)))))))
    (. .)))

advmod(have-6, that-1)
auxpass(have-6, is-2)
det(freedom-4, the-3)
nsubjpass(have-6, freedom-4)
dep(freedom-4, i-5)
root(ROOT-0, have-6)
poss(body-9, my-8)
prep_over(have-6, body-9)
mark(chose-13, though-11)
nsubj(chose-13, i-12)
advcl(have-6, chose-13)
neg(chose-13, not-14)
det(form-16, the-15)
dobj(chose-13, form-16)

