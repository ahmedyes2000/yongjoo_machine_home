(ROOT
  (S
    (PP (IN In)
      (NP
        (NP (DT the) (NN case))
        (PP (IN of)
          (NP (NN abortion)))))
    (NP (NN surgery))
    (VP (VBZ is)
      (VP (VBN used)
        (S
          (VP (TO to)
            (VP (VB terminate)
              (NP (DT a) (JJ human) (NN life)))))))
    (. .)))

det(case-3, the-2)
prep_in(used-8, case-3)
prep_of(case-3, abortion-5)
nsubjpass(used-8, surgery-6)
xsubj(terminate-10, surgery-6)
auxpass(used-8, is-7)
root(ROOT-0, used-8)
aux(terminate-10, to-9)
xcomp(used-8, terminate-10)
det(life-13, a-11)
amod(life-13, human-12)
dobj(terminate-10, life-13)

