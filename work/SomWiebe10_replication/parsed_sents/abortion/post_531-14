(ROOT
  (S
    (NP
      (NP (VBG Stripping) (JJ American) (NNS citizens))
      (PP (IN of)
        (NP (PRP$ their) (NNS rights))))
    (VP (VBZ is)
      (NP (DT a)
        (ADJP (RB very) (JJ dangerous))
        (NN game)))
    (. .)))

amod(citizens-3, Stripping-1)
amod(citizens-3, American-2)
nsubj(game-11, citizens-3)
poss(rights-6, their-5)
prep_of(citizens-3, rights-6)
cop(game-11, is-7)
det(game-11, a-8)
advmod(dangerous-10, very-9)
amod(game-11, dangerous-10)
root(ROOT-0, game-11)

