(ROOT
  (S (RB Yet) (, ,)
    (SBAR
      (WHADVP (WRB when))
      (S
        (NP (DT that) (NN child))
        (VP (VBZ is)
          (VP (VBN born)))))
    (NP (PRP you))
    (VP (VBP turn)
      (NP (PRP$ your) (NNS backs)))
    (. .)))

advmod(turn-9, Yet-1)
advmod(born-7, when-3)
det(child-5, that-4)
nsubjpass(born-7, child-5)
auxpass(born-7, is-6)
advcl(turn-9, born-7)
nsubj(turn-9, you-8)
root(ROOT-0, turn-9)
poss(backs-11, your-10)
dobj(turn-9, backs-11)

