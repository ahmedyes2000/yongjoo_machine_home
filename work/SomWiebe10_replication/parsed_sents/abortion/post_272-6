(ROOT
  (S
    (NP (PRP They))
    (ADVP (DT all))
    (VP (VBP seem)
      (PP (IN like)
        (NP
          (NP
            (ADJP (RB equally) (JJ momentous))
            (NNS events))
          (PP (IN in)
            (NP
              (NP (DT the) (NN creation))
              (PP (IN of)
                (NP (DT a) (JJ new) (JJ human) (NN being))))))))
    (. .)))

nsubj(seem-3, They-1)
advmod(seem-3, all-2)
root(ROOT-0, seem-3)
advmod(momentous-6, equally-5)
amod(events-7, momentous-6)
prep_like(seem-3, events-7)
det(creation-10, the-9)
prep_in(events-7, creation-10)
det(being-15, a-12)
amod(being-15, new-13)
amod(being-15, human-14)
prep_of(creation-10, being-15)

