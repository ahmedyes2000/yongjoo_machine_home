(ROOT
  (S
    (NP (PRP I))
    (VP (VBP am)
      (ADJP (JJ sorry)
        (S
          (VP (TO to)
            (VP (VB disagree)
              (PP (IN with)
                (NP (PRP you))))))))
    (. .)))

nsubj(sorry-3, I-1)
cop(sorry-3, am-2)
root(ROOT-0, sorry-3)
aux(disagree-5, to-4)
xcomp(sorry-3, disagree-5)
prep_with(disagree-5, you-7)

