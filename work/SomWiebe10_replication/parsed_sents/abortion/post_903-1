(ROOT
  (S
    (S
      (NP (EX There))
      (VP (VBP are)
        (NP
          (NP (JJ countless) (NNS women))
          (PP (IN in)
            (NP
              (NP (DT this) (NN world))
              (SBAR
                (WHNP (WDT that))
                (S
                  (VP
                    (VP (MD ca) (RB n't)
                      (VP (VB have)
                        (NP (NNS children))))
                    (CC or)
                    (VP (VBP are)
                      (VP (VBG struggling)
                        (S
                          (VP (TO to)
                            (VP (VB concieve))))))))))))))
    (: ...)
    (S
      (NP (NNS women))
      (VP (MD should)
        (VP
          (VP (VB deliver)
            (NP (DT the) (NNS babies)))
          (CC and)
          (VP (VB allow)
            (NP (DT the) (NN child)
              (S
                (VP (TO to)
                  (VP (VB live)
                    (NP (DT a) (NN life))))))))))
    (. .)))

expl(are-2, There-1)
root(ROOT-0, are-2)
amod(women-4, countless-3)
nsubj(are-2, women-4)
det(world-7, this-6)
prep_in(women-4, world-7)
nsubj(have-11, world-7)
nsubj(struggling-15, world-7)
aux(have-11, ca-9)
neg(have-11, n't-10)
rcmod(world-7, have-11)
dobj(have-11, children-12)
aux(struggling-15, are-14)
rcmod(world-7, struggling-15)
conj_or(have-11, struggling-15)
aux(concieve-17, to-16)
xcomp(struggling-15, concieve-17)
nsubj(deliver-21, women-19)
nsubj(allow-25, women-19)
aux(deliver-21, should-20)
parataxis(are-2, deliver-21)
det(babies-23, the-22)
dobj(deliver-21, babies-23)
parataxis(are-2, allow-25)
conj_and(deliver-21, allow-25)
det(child-27, the-26)
dobj(allow-25, child-27)
aux(live-29, to-28)
infmod(child-27, live-29)
det(life-31, a-30)
dobj(live-29, life-31)

