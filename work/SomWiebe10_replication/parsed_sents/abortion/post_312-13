(ROOT
  (S (`` ``)
    (ADVP (RB Now))
    (, ,)
    (NP (PRP I))
    (VP (VBP 've)
      (ADVP (RB clearly))
      (VP (VBN stated)
        (NP (PRP$ my) (NNS definitions))))
    (. .)))

advmod(stated-7, Now-2)
nsubj(stated-7, I-4)
aux(stated-7, 've-5)
advmod(stated-7, clearly-6)
root(ROOT-0, stated-7)
poss(definitions-9, my-8)
dobj(stated-7, definitions-9)

