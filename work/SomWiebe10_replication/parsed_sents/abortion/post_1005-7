  (S
    (NP (DT That))
    (VP (VBZ 's)
      (SBAR
        (WHADVP (WRB why))
        (S
          (NP (NN abortion))
          (VP (VBZ is)
            (ADVP (RB still))
            (NP
              (NP (DT a) (JJ hot) (NN topic) (NN 35yrs))
              (PP (IN after)
                (NP (NNP Roe) (NNP vs) (NNP Wade))))))))
    (. .)))

nsubj('s-2, That-1)
root(ROOT-0, 's-2)
