(ROOT
  (S
    (PP (IN As)
      (PP (IN in)
        (NP (DT the) (NN past))))
    (, ,)
    (NP (PRP you))
    (VP (VBP present)
      (NP (DT a) (JJ thoughtful) (NN question)))
    (. .)))

prep(present-7, As-1)
pcomp(As-1, in-2)
det(past-4, the-3)
prep_in(As-1, past-4)
nsubj(present-7, you-6)
root(ROOT-0, present-7)
det(question-10, a-8)
amod(question-10, thoughtful-9)
dobj(present-7, question-10)

