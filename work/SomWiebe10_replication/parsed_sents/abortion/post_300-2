(ROOT
  (S
    (PP (IN For)
      (NP (PRP us)))
    (, ,)
    (NP (PRP$ our) (NN family))
    (VP (MD would)
      (ADVP
        (ADJP (JJ simple) (NN cough))
        (ADVP (RB up)
          (NP (DT the) (NN money))
          (PP (IN for)
            (NP (DT a) (NN plane)))))
      (VP (VB ride)
        (PP (IN for)
          (NP (NN someplace)))
        (SBAR
          (WHADVP (WRB where))
          (S
            (NP (DT a)
              (ADJP (JJ legal)
                (CC and)
                (JJ safe))
              (NN abortion))
            (VP (MD could)
              (ADVP (RB still))
              (VP (VB be)
                (VP (VBN obtained))))))))
    (. .)))

prep_for(ride-15, us-2)
poss(family-5, our-4)
nsubj(ride-15, family-5)
aux(ride-15, would-6)
dep(cough-8, simple-7)
dep(up-9, cough-8)
advmod(ride-15, up-9)
det(money-11, the-10)
pobj(up-9, money-11)
det(plane-14, a-13)
