advmod(obtained-27, where-18)
det(abortion-23, a-19)
amod(abortion-23, legal-20)
conj_and(legal-20, safe-22)
amod(abortion-23, safe-22)
nsubjpass(obtained-27, abortion-23)
aux(obtained-27, could-24)
