(ROOT
  (S
    (VP (VBG Placing)
      (PP (DT both) (IN at)
        (NP
          (NP (DT the) (NN forefront))
          (PP (IN of)
            (NP (DT the) (NN battle)))))
      (PP (IN from)
        (SBAR
          (WHNP (WDT which))
          (S
            (VP (VB gain)
              (NP (NNS members)))))))
    (. .)))

root(ROOT-0, Placing-1)
dep(Placing-1, both-2)
det(forefront-5, the-4)
prep_at(Placing-1, forefront-5)
det(battle-8, the-7)
prep_of(forefront-5, battle-8)
nsubj(gain-11, which-10)
prepc_from(Placing-1, gain-11)
dobj(gain-11, members-12)

