(ROOT
  (S
    (NP (PRP I))
    (VP (VBP wish)
      (SBAR
        (S
          (NP (JJR more) (NNS people))
          (VP (VBD were)
            (ADJP (RB as) (JJ mature))
            (PP (IN as)
              (NP (PRP you)))))))
    (. .)))

nsubj(wish-2, I-1)
root(ROOT-0, wish-2)
amod(people-4, more-3)
nsubj(mature-7, people-4)
cop(mature-7, were-5)
advmod(mature-7, as-6)
ccomp(wish-2, mature-7)
prep_as(mature-7, you-9)

