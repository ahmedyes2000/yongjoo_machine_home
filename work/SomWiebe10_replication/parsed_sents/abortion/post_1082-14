(ROOT
  (S
    (NP (EX There))
    (ADVP (RB really))
    (VP (MD should)
      (VP (VB be)
        (NP
          (NP (DT no) (NN debate))
          (PP (IN at)
            (NP
              (NP (DT all))
              (PP (IN about)
                (NP (NN abortion))))))))
    (. .)))

expl(debate-6, There-1)
advmod(debate-6, really-2)
aux(debate-6, should-3)
cop(debate-6, be-4)
det(debate-6, no-5)
root(ROOT-0, debate-6)
prep_at(debate-6, all-8)
prep_about(all-8, abortion-10)

