(ROOT
  (S
    (NP (EX There))
    (VP (VBP are)
      (ADVP (RB certainly))
      (NP
        (NP (DT a) (NN number))
        (PP (IN of)
          (NP
            (NP (NNS drawbacks))
            (PP (TO to)
              (NP (DT the) (JJ sexual) (NN revolution)))))))
    (. .)))

expl(are-2, There-1)
root(ROOT-0, are-2)
advmod(are-2, certainly-3)
det(number-5, a-4)
nsubj(are-2, number-5)
prep_of(number-5, drawbacks-7)
det(revolution-11, the-9)
amod(revolution-11, sexual-10)
prep_to(drawbacks-7, revolution-11)

