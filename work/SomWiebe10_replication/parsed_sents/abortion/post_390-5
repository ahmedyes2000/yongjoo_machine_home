  (S
    (NP (DT A) (JJ human) (NN being))
    (VP (VBZ is)
      (NP
        (NP (DT a) (JJ human) (NN person))
        (SBAR
          (WHNP (WP who))
          (S
            (NP (NN hasn) (NN t))
            (ADVP (RB yet))
            (VP (VBD had)
              (NP (DT the) (NN chance)
                (S
                  (VP (TO to)
                    (VP (VB establish)
                      (NP (PRP themselves)))))))))))
    (. .)))

