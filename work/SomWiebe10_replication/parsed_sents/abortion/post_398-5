(ROOT
  (S
    (NP
      (NP (NNS Emotions))
      (CC and)
      (NP (JJ religious) (NNS doctrines)))
    (VP (VBP have)
      (ADVP (NN nothing))
      (S
        (VP (TO to)
          (VP (VB do)
            (PP (IN with)
              (NP (NN reality) (RB whatsoever)))))))
    (. .)))

nsubj(have-5, Emotions-1)
xsubj(do-8, Emotions-1)
amod(doctrines-4, religious-3)
conj_and(Emotions-1, doctrines-4)
nsubj(have-5, doctrines-4)
xsubj(do-8, doctrines-4)
root(ROOT-0, have-5)
advmod(have-5, nothing-6)
aux(do-8, to-7)
xcomp(have-5, do-8)
prep_with(do-8, reality-10)
advmod(reality-10, whatsoever-11)

