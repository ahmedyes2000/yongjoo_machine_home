(ROOT
  (S
    (NP (DT Both))
    (VP
      (VP (VBP kill)
        (NP
          (NP (NN something))
          (SBAR
            (WHNP (WDT that))
            (S
              (VP (VBZ is) (RB n't)
                (ADVP (RB yet))
                (NP (NN life)))))))
      (CC but)
      (VP (VBZ has)
        (NP (DT the) (NN potential)
          (S
            (VP (TO to)
              (VP (VB be)
                (NP (NN life))))))))
    (. .)))

nsubj(kill-2, Both-1)
nsubj(has-10, Both-1)
root(ROOT-0, kill-2)
dobj(kill-2, something-3)
nsubj(life-8, something-3)
cop(life-8, is-5)
neg(life-8, n't-6)
advmod(life-8, yet-7)
rcmod(something-3, life-8)
conj_but(kill-2, has-10)
det(potential-12, the-11)
dobj(has-10, potential-12)
aux(life-15, to-13)
cop(life-15, be-14)
infmod(potential-12, life-15)

