(ROOT
  (S
    (S
      (NP (PRP It))
      (VP (VBZ needs)
        (S
          (VP (TO to)
            (VP (VB meet)
              (NP (DT another) (NN gamete)))))))
    (CC and)
    (S
      (ADVP (RB only))
      (NP (RB then))
      (VP (MD can)
        (NP (CD two) (NNS gametes))
        (S
          (VP (VB create)
            (NP (NN life))))))
    (. .)))

nsubj(needs-2, It-1)
xsubj(meet-4, It-1)
root(ROOT-0, needs-2)
aux(meet-4, to-3)
xcomp(needs-2, meet-4)
det(gamete-6, another-5)
dobj(meet-4, gamete-6)
