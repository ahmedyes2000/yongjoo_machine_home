(ROOT
  (S
    (NP (DT This))
    (VP (VBZ has)
      (VP (VBN made)
        (S
          (NP (PRP it))
          (ADJP (JJ difficult)
            (PP (IN for)
              (NP
                (NP (DT the) (NNS Christians))
                (SBAR
                  (WHNP (WP who))
                  (S
                    (VP (VBP are)
                      (ADVP (RB traditionally))
                      (NP
                        (NP (NNS members))
                        (PP (IN of)
                          (NP (DT that) (NN party)))))))))))))
    (. .)))

nsubj(made-3, This-1)
aux(made-3, has-2)
root(ROOT-0, made-3)
nsubj(difficult-5, it-4)
xcomp(made-3, difficult-5)
det(Christians-8, the-7)
prep_for(difficult-5, Christians-8)
nsubj(members-12, Christians-8)
cop(members-12, are-10)
advmod(members-12, traditionally-11)
rcmod(Christians-8, members-12)
det(party-15, that-14)
prep_of(members-12, party-15)

