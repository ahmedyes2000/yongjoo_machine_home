(ROOT
  (S
    (NP (PRP It))
    (ADVP (RB really))
    (VP (VBZ is)
      (ADVP (RB just))
      (NP (DT a) (JJ religious) (NN issue)))
    (. .)))

nsubj(issue-7, It-1)
advmod(issue-7, really-2)
cop(issue-7, is-3)
advmod(issue-7, just-4)
det(issue-7, a-5)
amod(issue-7, religious-6)
root(ROOT-0, issue-7)

