(ROOT
  (S ('' '')
    (NP (NNS People))
    (VP (VBP seem)
      (S
        (VP (TO to)
          (VP (VB be)
            (ADJP (JJ clever)
              (PP (IN at)
                (S
                  (VP (VBG engaging)
                    (PP (IN in)
                      (NP (NN discussion)))
                    (PP (IN at)
                      (SBAR
                        (WHADVP (WRB when))
                        (S
                          (NP (NN life))
                          (VP (VBZ begins)))))))))))))
    (. .)))

nsubj(seem-3, People-2)
xsubj(clever-6, People-2)
root(ROOT-0, seem-3)
aux(clever-6, to-4)
cop(clever-6, be-5)
