  (S
    (NP (DT That))
    (VP (VBZ 's)
      (PP (IN like)
        (S
          (VP (VBG saying)
            (SBAR
              (S
                (NP
                  (NP (DT the) (NNS people))
                  (PP (IN in)
                    (NP (NNP Washington))))
                (VP (MD can)
