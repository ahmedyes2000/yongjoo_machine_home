(ROOT
  (S
    (NP (EX There))
    (VP (VBZ is)
      (NP
        (NP (DT no) (NN difference))
        (PP (IN in)
          (NP (DT those) (NNS choices)))))
    (. .)))

expl(is-2, There-1)
root(ROOT-0, is-2)
det(difference-4, no-3)
nsubj(is-2, difference-4)
det(choices-7, those-6)
prep_in(difference-4, choices-7)

