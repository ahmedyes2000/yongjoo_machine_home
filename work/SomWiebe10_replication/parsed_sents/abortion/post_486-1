(ROOT
  (S
    (NP (PRP It))
    (ADVP (RB just))
    (VP (VBZ alternates)
      (SBAR (IN as)
        (S
          (NP (DT the) (NNPS Republicans)
            (CC and)
            (NNPS Democrats))
          (VP (VBP exchange)
            (NP
              (NP (NN control))
              (PP (IN of)
                (NP (NN government))))))))
    (. .)))

nsubj(alternates-3, It-1)
advmod(alternates-3, just-2)
root(ROOT-0, alternates-3)
mark(exchange-9, as-4)
det(Republicans-6, the-5)
nsubj(exchange-9, Republicans-6)
conj_and(Republicans-6, Democrats-8)
nsubj(exchange-9, Democrats-8)
advcl(alternates-3, exchange-9)
dobj(exchange-9, control-10)
prep_of(control-10, government-12)

