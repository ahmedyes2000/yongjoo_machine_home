(ROOT
  (S
    (SBAR (IN Since)
      (S
        (NP (PRP you))
        (VP (VBP believe)
          (NP (DT that)))))
    (, ,)
    (NP (PRP you))
    (VP (VBP give)
      (NP (PRP it))
      (NP (NNS rights)))
    (. .)))

mark(believe-3, Since-1)
nsubj(believe-3, you-2)
advcl(give-7, believe-3)
dobj(believe-3, that-4)
nsubj(give-7, you-6)
root(ROOT-0, give-7)
iobj(give-7, it-8)
dobj(give-7, rights-9)

