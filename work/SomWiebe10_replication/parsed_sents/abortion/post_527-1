(ROOT
  (S (`` ``)
    (NP (EX there))
    (VP (VBZ 's)
      (NP
        (NP (DT a) (JJ dead) (NN man))
        (PP (IN in)
          (NP (PRP$ my) (NN tent)))))
    (. .)))

expl('s-3, there-2)
root(ROOT-0, 's-3)
det(man-6, a-4)
amod(man-6, dead-5)
