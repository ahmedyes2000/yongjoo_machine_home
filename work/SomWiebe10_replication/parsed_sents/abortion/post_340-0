(ROOT
  (S
    (NP (DT The) (JJ real) (NN issue))
    (VP (VBZ is) (RB n't)
      (SBAR
        (SBAR
          (WHADVP (WRB when))
          (S
            (NP (NN life))
            (VP (VBZ begins))))
        (, ,)
        (CC or)
        (SBAR
          (WHNP (WP what))
          (S
            (VP (VBZ constitutes)
              (NP (NN life)))))))
    (. .)))

det(issue-3, The-1)
amod(issue-3, real-2)
nsubj(is-4, issue-3)
root(ROOT-0, is-4)
neg(is-4, n't-5)
advmod(begins-8, when-6)
nsubj(begins-8, life-7)
dep(is-4, begins-8)
nsubj(constitutes-12, what-11)
dep(is-4, constitutes-12)
conj_or(begins-8, constitutes-12)
dobj(constitutes-12, life-13)

