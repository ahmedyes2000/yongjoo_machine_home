(ROOT
  (S
    (NP
      (NP
        (QP (DT Half)))
      (PP (IN of)
        (NP (DT those))))
    (VP (VBP are)
      (ADJP (JJ available)
        (PP (IN for)
          (NP (NN adoption)))))
    (. .)))

nsubj(available-5, Half-1)
prep_of(Half-1, those-3)
cop(available-5, are-4)
root(ROOT-0, available-5)
prep_for(available-5, adoption-7)

