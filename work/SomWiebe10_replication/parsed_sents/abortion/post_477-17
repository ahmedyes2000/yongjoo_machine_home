(ROOT
  (S
    (NP (PRP$ His) (JJ major) (NNS publications))
    (VP
      (VP (VBP include))
      (: :)
      (NP
        (NP (DT The) (NNP Moral) (NNP Question))
        (PP (IN of)
          (NP (NNP Abortion)))))
    (. .)))

poss(publications-3, His-1)
amod(publications-3, major-2)
nsubj(include-4, publications-3)
root(ROOT-0, include-4)
det(Question-8, The-6)
nn(Question-8, Moral-7)
dobj(include-4, Question-8)
prep_of(Question-8, Abortion-10)

