(ROOT
  (S (CC But)
    (NP (PRP I))
    (ADVP (RB absolutely))
    (VP (VBP do) (RB not)
      (VP (VB believe)
        (SBAR
          (S
            (NP (DT the) (NN government))
            (VP (MD should)
              (VP (VB ban)
                (NP (NN abortion))))))))
    (. .)))

cc(believe-6, But-1)
nsubj(believe-6, I-2)
advmod(believe-6, absolutely-3)
aux(believe-6, do-4)
neg(believe-6, not-5)
root(ROOT-0, believe-6)
det(government-8, the-7)
nsubj(ban-10, government-8)
aux(ban-10, should-9)
ccomp(believe-6, ban-10)
dobj(ban-10, abortion-11)

