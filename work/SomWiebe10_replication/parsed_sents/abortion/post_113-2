(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (NN abortion))
        (VP (VBZ is)
          (ADJP (JJ illegal)))))
    (, ,)
    (ADVP (RB therefore))
    (, ,)
    (NP (DT this) (JJ socio-economic) (NN group))
    (VP (MD will)
      (VP (VB be)
        (VP
          (ADVP (RB disproportionately))
          (VBN affected))))
    (. .)))

mark(illegal-4, If-1)
nsubj(illegal-4, abortion-2)
cop(illegal-4, is-3)
advcl(affected-14, illegal-4)
advmod(affected-14, therefore-6)
det(group-10, this-8)
