(ROOT
  (S ('' '')
    (PP (IN By)
      (NP
        (NP (DT the) (NN way))
        (, ,)
        (SBAR
          (WHADVP (WRB why))
          (S
            (VP (VBP do)
              (NP (NNS people)))))))
    (VP (VBP have)
      (NP (NNS kids)))
    (. ?)))

det(way-4, the-3)
prep_by(have-9, way-4)
advmod(do-7, why-6)
rcmod(way-4, do-7)
dobj(do-7, people-8)
root(ROOT-0, have-9)
dobj(have-9, kids-10)

