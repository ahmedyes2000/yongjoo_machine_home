(ROOT
  (S
    (NP (PRP$ Your) (JJ entire) (NN argument))
    (VP (VBZ is)
      (VP (VBN based)
        (PP (IN on)
          (NP (DT a) (JJ mistaken) (NN premise)))
        (SBAR (IN that)
          (S
            (NP (DT a) (NN fetus))
            (VP (VBZ is)
              (NP (DT a) (JJ human) (NN being)))))))
    (. .)))

poss(argument-3, Your-1)
amod(argument-3, entire-2)
nsubjpass(based-5, argument-3)
auxpass(based-5, is-4)
root(ROOT-0, based-5)
det(premise-9, a-7)
amod(premise-9, mistaken-8)
prep_on(based-5, premise-9)
complm(being-16, that-10)
det(fetus-12, a-11)
nsubj(being-16, fetus-12)
cop(being-16, is-13)
det(being-16, a-14)
amod(being-16, human-15)
ccomp(based-5, being-16)

