(ROOT
  (S (-LRB- -LRB-)
    (NP (PRP It))
    (VP (VBZ treats)
      (NP (JJ human) (NN life))
      (SBAR (RB as) (IN if)
        (S
          (NP (PRP it))
          (VP (VBD were)
            (NP
              (NP (DT the) (NN property))
              (PP (IN of)
                (NP (DT another))))))))
    (. .) (-RRB- -RRB-)))

nsubj(treats-3, It-2)
root(ROOT-0, treats-3)
amod(life-5, human-4)
dobj(treats-3, life-5)
advmod(property-11, as-6)
dep(property-11, if-7)
nsubj(property-11, it-8)
cop(property-11, were-9)
det(property-11, the-10)
dep(treats-3, property-11)
prep_of(property-11, another-13)

