(ROOT
  (S ('' '')
    (NP (PRP I))
    (VP (MD would)
      (ADVP (RB never))
      (VP (VB argue)
        (SBAR (IN that)
          (S
            (NP (NN abortion))
            (VP (VBZ is)
              (NP (DT a) (JJ good) (NN thing)))))))
    (. .)))

nsubj(argue-5, I-2)
aux(argue-5, would-3)
neg(argue-5, never-4)
root(ROOT-0, argue-5)
complm(thing-11, that-6)
nsubj(thing-11, abortion-7)
cop(thing-11, is-8)
det(thing-11, a-9)
amod(thing-11, good-10)
ccomp(argue-5, thing-11)

