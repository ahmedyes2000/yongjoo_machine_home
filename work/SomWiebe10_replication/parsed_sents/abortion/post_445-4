(ROOT
  (S
    (VP (VB Put)
      (NP (PDT all) (PRP$ your) (NN money))
      (PP (IN in)
        (NP
          (NP (DT a) (NN trust))
          (PP (IN for)
            (NP (DT the) (NN mom)
              (CC and)
              (NN baby))))))
    (. .)))

root(ROOT-0, Put-1)
predet(money-4, all-2)
poss(money-4, your-3)
dobj(Put-1, money-4)
det(trust-7, a-6)
prep_in(Put-1, trust-7)
det(mom-10, the-9)
prep_for(trust-7, mom-10)
prep_for(trust-7, baby-12)
conj_and(mom-10, baby-12)

