(ROOT
  (S
    (INTJ (UH Yes))
    (, ,)
    (NP (PRP we))
    (VP (VBP do)
      (NP (NN fight))
      (PP (IN against)
        (NP (NNP torture)))
      (SBAR (IN because)
        (S
          (NP (PRP it))
          (VP (VBZ is)
            (ADJP (JJ bad))))))
    (. .)))

dep(do-4, Yes-1)
nsubj(do-4, we-3)
root(ROOT-0, do-4)
dobj(do-4, fight-5)
prep_against(do-4, torture-7)
mark(bad-11, because-8)
nsubj(bad-11, it-9)
cop(bad-11, is-10)
advcl(do-4, bad-11)

