(ROOT
  (S (CC And)
    (VP (VB please)
      (VP (VB tell)
        (NP (PRP me))
        (SBAR
          (WHNP (WP what))
          (S
            (NP (DT that))
            (VP (VBZ is)
              (SBAR (IN if)
                (S
                  (NP (PRP it))
                  (VP (VBZ is) (RB n't)
                    (ADJP (JJ human))))))))))
    (. .)))

cc(please-2, And-1)
root(ROOT-0, please-2)
xcomp(please-2, tell-3)
dobj(tell-3, me-4)
dep(is-7, what-5)
dobj(human-12, what-5)
nsubj(is-7, that-6)
dep(tell-3, is-7)
mark(human-12, if-8)
nsubj(human-12, it-9)
cop(human-12, is-10)
neg(human-12, n't-11)
advcl(is-7, human-12)

