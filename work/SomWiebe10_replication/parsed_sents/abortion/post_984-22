(ROOT
  (S
    (NP (DT This))
    (VP (VBZ is)
      (NP
        (NP (DT the) (NN kind))
        (PP (IN of)
          (NP (JJ hypocritical) (NN judgment)))
        (SBAR
          (S
            (NP (NNP Jesus))
            (VP (VBD was)
              (VP (VBG talking) (IN about)
                (PP (IN in)
                  (NP (NNP Matthew) (CD 7)))))))))
    (. .)))

nsubj(kind-4, This-1)
cop(kind-4, is-2)
det(kind-4, the-3)
root(ROOT-0, kind-4)
dobj(talking-10, kind-4)
amod(judgment-7, hypocritical-6)
prep_of(kind-4, judgment-7)
nsubj(talking-10, Jesus-8)
aux(talking-10, was-9)
rcmod(kind-4, talking-10)
dep(talking-10, about-11)
prep_in(talking-10, Matthew-13)
