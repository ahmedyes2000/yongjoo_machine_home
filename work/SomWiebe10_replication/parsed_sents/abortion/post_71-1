(ROOT
  (S
    (NP (PRP I))
    (VP (VBP believe)
      (SBAR (IN that)
        (S
          (PP (IN in)
            (NP
              (NP (DT a) (NN society))
              (SBAR
                (WHNP (WDT that))
                (S
                  (VP (VBZ permits)
                    (S
                      (NP
                        (NP (DT the) (NN life))
                        (PP (IN of)
                          (NP (RB even) (CD one) (NN individual))))
                      (VP (TO to)
                        (VP (VB be)
                          (ADJP (JJ dependent)
                            (PP (IN on)))
                          (SBAR (IN whether)
                            (S
                              (NP (DT that) (NN life))
                              (VP (VBZ is)
                                (VP (VBN wanted))))
                            (CC or)
                            (RB not))))))))))
          (, ,)
          (NP (PDT all) (PRP$ its) (NNS citizens))
          (VP (VBP stand)
            (PP (IN in)
              (NP (NN danger)))))))
    (: ...)))

