(ROOT
  (SQ (VBP Have)
    (NP
      (NP (PRP you))
      (ADVP (RB ever)))
    (VP (VBN had)
      (NP (DT any) (NN success))
      (PP (IN with)
        (NP (NN something)))
      (SBAR (IN that)
        (S
          (NP (PRP you))
          (VP (VBD decided)
            (PP (IN from)
              (NP (DT the) (NN start)))
            (SBAR (IN that)
              (S
                (NP (PRP it))
                (VP (MD would)
                  (ADVP (RB never))
                  (VP (VB work)))))))))
    (. .)))

aux(had-4, Have-1)
nsubj(had-4, you-2)
advmod(you-2, ever-3)
root(ROOT-0, had-4)
det(success-6, any-5)
dobj(had-4, success-6)
prep_with(had-4, something-8)
complm(decided-11, that-9)
nsubj(decided-11, you-10)
ccomp(had-4, decided-11)
det(start-14, the-13)
prep_from(decided-11, start-14)
complm(work-19, that-15)
nsubj(work-19, it-16)
aux(work-19, would-17)
neg(work-19, never-18)
ccomp(decided-11, work-19)

