(ROOT
  (S
    (NP (DT No) (NN one))
    (VP (VBZ is)
      (ADVP (RB simply))
      (VP (VBG dismissing)
        (NP (NN anything))))
    (. .)))

det(one-2, No-1)
nsubj(dismissing-5, one-2)
aux(dismissing-5, is-3)
advmod(dismissing-5, simply-4)
root(ROOT-0, dismissing-5)
dobj(dismissing-5, anything-6)

