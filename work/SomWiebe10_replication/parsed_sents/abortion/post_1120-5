(ROOT
  (FRAG (CC And)
    (SBAR (IN that)
      (S
        (NP (DT these) (NNS people))
        (VP (VBP deserve)
          (NP (NNS facts))
          (PP (IN on)
            (S
              (VP (VBG having)
                (NP (JJ safe) (NN sex))))))))
    (. .)))

dep(deserve-5, And-1)
dep(deserve-5, that-2)
det(people-4, these-3)
nsubj(deserve-5, people-4)
root(ROOT-0, deserve-5)
dobj(deserve-5, facts-6)
prepc_on(deserve-5, having-8)
amod(sex-10, safe-9)
dobj(having-8, sex-10)

