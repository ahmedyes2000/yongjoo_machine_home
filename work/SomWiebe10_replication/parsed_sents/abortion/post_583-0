(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is)
      (ADVP (RB simply))
      (NP (PRP$ your) (NN opinion))
      (SBAR (IN that)
        (S
          (NP
            (NP (DT a) (JJ new) (NN member))
            (PP (IN of)
              (NP (DT the) (NN species))))
          (VP (VBZ is)
            (VP (VBN created)
              (PP (IN at)
                (NP (NN conception))))))))
    (. .)))

nsubj(opinion-5, It-1)
cop(opinion-5, is-2)
advmod(opinion-5, simply-3)
poss(opinion-5, your-4)
root(ROOT-0, opinion-5)
complm(created-14, that-6)
det(member-9, a-7)
