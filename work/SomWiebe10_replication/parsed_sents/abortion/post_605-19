(ROOT
  (S (CC But)
    (NP (EX there))
    (VP (VBP are)
      (NP
        (NP (DT some)
          (ADJP (RB very) (JJ valid))
          (NNS reasons))
        (PP (IN for)
          (NP (DT a) (NN woman)
            (S
              (VP (TO to)
                (VP (VB have)
                  (NP (CD one)))))))))
    (. .)))

cc(are-3, But-1)
expl(are-3, there-2)
root(ROOT-0, are-3)
det(reasons-7, some-4)
advmod(valid-6, very-5)
amod(reasons-7, valid-6)
nsubj(are-3, reasons-7)
det(woman-10, a-9)
prep_for(reasons-7, woman-10)
aux(have-12, to-11)
infmod(woman-10, have-12)
dobj(have-12, one-13)

