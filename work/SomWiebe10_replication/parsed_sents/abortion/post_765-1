(ROOT
  (S (`` ``)
    (VP (VB Keep)
      (NP (PRP$ your) (JJ religious) (NNS opinions))
      (PP (TO to)
        (S
          (NP (NNP yourself) ('' '') (NNP No))
          (`` ``)
          (ADVP (RB especially))
          (VP (VBG concerning)
            (NP
              (NP (NNS decisions))
              (SBAR
                (WHNP (WDT that))
                (S
                  (VP (VBP do) (RB n't)
                    (VP (VB effect)
                      (NP (PRP you)))))))))))
    (. .)))

root(ROOT-0, Keep-2)
poss(opinions-5, your-3)
amod(opinions-5, religious-4)
dobj(Keep-2, opinions-5)
nn(No-9, yourself-7)
nsubj(concerning-12, No-9)
advmod(concerning-12, especially-11)
prepc_to(Keep-2, concerning-12)
dobj(concerning-12, decisions-13)
nsubj(effect-17, decisions-13)
aux(effect-17, do-15)
neg(effect-17, n't-16)
rcmod(decisions-13, effect-17)
dobj(effect-17, you-18)

