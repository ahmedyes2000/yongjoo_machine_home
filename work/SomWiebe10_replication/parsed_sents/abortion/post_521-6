(ROOT
  (S
    (NP (DT The) (JJ same))
    (VP (MD can)
      (VP (VB be)
        (VP (VBN said)
          (PP (IN for)
            (NP (NN brain) (NN activity))))))
    (. .)))

det(same-2, The-1)
nsubjpass(said-5, same-2)
aux(said-5, can-3)
auxpass(said-5, be-4)
root(ROOT-0, said-5)
nn(activity-8, brain-7)
prep_for(said-5, activity-8)

