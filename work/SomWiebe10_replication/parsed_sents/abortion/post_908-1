(ROOT
  (S
    (S
      (NP (NNP God))
      (VP (VBZ is)
        (NP
          (NP (DT the) (NN creator))
          (PP (IN of)
            (NP (NN life))))))
    (CC and)
    (S
      (SBAR
        (WHADVP (WRB when))
        (S
          (NP (PRP you))
          (VP (VBP kill)
            (NP (JJ unborn) (NNS babies)))))
      (NP (PRP you))
      (VP (VBP are)
        (VP (VBG destroying)
          (NP (PRP$ his) (NNS creations)))))
    (. .)))

nsubj(creator-4, God-1)
cop(creator-4, is-2)
det(creator-4, the-3)
root(ROOT-0, creator-4)
prep_of(creator-4, life-6)
advmod(kill-10, when-8)
nsubj(kill-10, you-9)
advcl(destroying-15, kill-10)
amod(babies-12, unborn-11)
dobj(kill-10, babies-12)
nsubj(destroying-15, you-13)
aux(destroying-15, are-14)
conj_and(creator-4, destroying-15)
poss(creations-17, his-16)
dobj(destroying-15, creations-17)

