(ROOT
  (FRAG
    (SBAR (IN Because)
      (S
        (NP (EX there))
        (VP (VBZ 's)
          (ADVP (RB always))
          (NP
            (NP (NN abortion))
            (ADJP (JJ available)
              (PP (IN as)
                (NP (DT a) (NN solution))))))))
    (. .)))

mark('s-3, Because-1)
expl('s-3, there-2)
root(ROOT-0, 's-3)
advmod('s-3, always-4)
nsubj('s-3, abortion-5)
amod(abortion-5, available-6)
det(solution-9, a-8)
prep_as(available-6, solution-9)

