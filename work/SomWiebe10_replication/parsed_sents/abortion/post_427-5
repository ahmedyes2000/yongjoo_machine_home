(ROOT
  (S
    (NP (PRP I))
    (VP (MD could)
      (VP (VB have)
        (VP (VBN heard)
          (NP (PRP you))
          (PP (IN before)
            (SBAR (CC but)
              (S
                (NP (PRP it))
                (VP (MD would)
                  (VP (VB be)
                    (ADJP (RB very) (JJ difficult))
                    (ADVP (RB now))))))))))
    (. .)))

nsubj(heard-4, I-1)
aux(heard-4, could-2)
aux(heard-4, have-3)
root(ROOT-0, heard-4)
dobj(heard-4, you-5)
cc(difficult-12, but-7)
nsubj(difficult-12, it-8)
aux(difficult-12, would-9)
cop(difficult-12, be-10)
advmod(difficult-12, very-11)
prepc_before(heard-4, difficult-12)
advmod(difficult-12, now-13)

