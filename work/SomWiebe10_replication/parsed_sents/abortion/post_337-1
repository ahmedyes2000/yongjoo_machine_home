(ROOT
  (S
    (NP (PRP It))
    (VP (VBD was)
      (ADVP (RB always))
      (ADJP (JJ convenient)
        (PP (IN for)
          (NP (NNPS Germans))))
      (S
        (VP (TO to)
          (VP (VB kill)
            (NP (PRP$ their) (NN POW))
            (PP (IN during)
              (NP (NNP WWII)))))))
    (. .)))

nsubj(convenient-4, It-1)
xsubj(kill-8, It-1)
cop(convenient-4, was-2)
advmod(convenient-4, always-3)
root(ROOT-0, convenient-4)
prep_for(convenient-4, Germans-6)
aux(kill-8, to-7)
xcomp(convenient-4, kill-8)
poss(POW-10, their-9)
dobj(kill-8, POW-10)
prep_during(kill-8, WWII-12)

