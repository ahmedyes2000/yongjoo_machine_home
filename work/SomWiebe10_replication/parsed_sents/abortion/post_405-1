(ROOT
  (S
    (S
      (NP (PRP I))
      (VP (MD should)
        (VP (VB have)
          (VP (VBN stated))))
      (, ,) ('' ''))
    (: ...)
    (S
      (NP (DT the) (JJ whole) (NN organism))
      (VP (VBD becoming)
        (X
          (X (SYM \*))
          (ADJP (JJ capable)
            (PP (IN of)
              (S
                (VP (VBG being)
                  (ADJP (RB \*) (RB metaphysically) (JJ independent)))))))))
    (. .)))

nsubj(stated-4, I-1)
aux(stated-4, should-2)
aux(stated-4, have-3)
root(ROOT-0, stated-4)
det(organism-10, the-8)
amod(organism-10, whole-9)
nsubj(becoming-11, organism-10)
