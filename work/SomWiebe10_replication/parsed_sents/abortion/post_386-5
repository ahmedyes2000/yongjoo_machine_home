(ROOT
  (S
    (SBAR
      (WHNP (WP What))
      (S
        (VP (VBZ makes)
          (S
            (NP (DT this))
            (ADJP (RB even) (JJR worse)
              (SBAR
                (S
                  (VP (VBZ is)
                    (NP (PRP we))))))))))
    (NP (DT all))
    (VP (VBP have)
      (NP (DT a) (NN right)
        (S
          (VP (TO to)
            (VP (VB make)
              (NP (PRP$ our) (JJ own) (NNS choices)))))))
    (. .)))

nsubj(makes-2, What-1)
dep(have-9, makes-2)
nsubj(worse-5, this-3)
advmod(worse-5, even-4)
xcomp(makes-2, worse-5)
cop(we-7, is-6)
dep(worse-5, we-7)
nsubj(have-9, all-8)
root(ROOT-0, have-9)
det(right-11, a-10)
dobj(have-9, right-11)
aux(make-13, to-12)
infmod(right-11, make-13)
poss(choices-16, our-14)
amod(choices-16, own-15)
dobj(make-13, choices-16)

