(ROOT
  (NP
    (NP (JJ Premarital) (NN sex))
    (: :)
    (NP
      (NP (NN Sex))
      (SBAR
        (WHNP (WDT that))
        (S
          (VP (VBZ occurs)
            (PP (IN before)
              (NP (NN marriage)))))))
    (. .)))

amod(sex-2, Premarital-1)
root(ROOT-0, sex-2)
dep(sex-2, Sex-4)
nsubj(occurs-6, Sex-4)
rcmod(Sex-4, occurs-6)
prep_before(occurs-6, marriage-8)

