(ROOT
  (S
    (SBAR
      (WHNP (WP What))
      (S
        (NP (PRP I))
        (VP (VBP do) (RB n't)
          (VP (VB get)))))
    (VP (VBZ is)
      (NP
        (NP (NNS people))
        (SBAR
          (S
            (VP (VBP pay)
              (S
                (VP (TO to)
                  (VP (VB kill)
                    (NP (PRP$ their) (NNP OWN) (NNS children))))))))))
    (. .)))

