(ROOT
  (S
    (NP (PRP$ Your) (NN mother)
      (CC and)
      (NN father))
    (VP (VBD were)
      (ADJP (JJ alive))
      (SBAR
        (WHADVP (WRB when))
        (S
          (NP (PRP$ their) (NN living) (NN sperm)
            (CC and)
            (NN egg))
          (VP
            (VP (VBD met))
            (CC and)
            (VP (VBD made)
              (NP (PRP$ your) (VBG living) (NN embryo)))))))
    (. .)))

poss(mother-2, Your-1)
nsubj(alive-6, mother-2)
conj_and(mother-2, father-4)
nsubj(alive-6, father-4)
