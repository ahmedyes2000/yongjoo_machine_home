(ROOT
  (S (RB Then)
    (NP (PRP you))
    (VP (VBP wonder)
      (SBAR
        (WHNP (WDT which))
        (S
          (NP
            (NP (NN authority) (POS 's))
            (NN opinion))
          (VP (VBZ establishes)
            (NP (NN reality))))))
    (. .)))

advmod(wonder-3, Then-1)
nsubj(wonder-3, you-2)
root(ROOT-0, wonder-3)
dobj(establishes-8, which-4)
poss(opinion-7, authority-5)
nsubj(establishes-8, opinion-7)
ccomp(wonder-3, establishes-8)
dobj(establishes-8, reality-9)

