(ROOT
  (S
    (NP (WDT Which))
    (VP (VBZ is)
      (ADJP (DT a)
        (ADJP (JJ weird) (JJ right))
        (, ,)
        (CC but)
        (ADJP (JJ okay))))
    (. .)))

nsubj(right-5, Which-1)
nsubj(okay-8, Which-1)
cop(right-5, is-2)
dep(right-5, a-3)
amod(right-5, weird-4)
root(ROOT-0, right-5)
conj_but(right-5, okay-8)

