(ROOT
  (S
    (NP
      (NP (DT A) (NN woman) (POS 's))
      (NN abortion))
    (VP (VBZ has)
      (NP
        (NP (DT no) (JJ financial)
          (CC or)
          (JJ physical) (NN impact))
        (PP (IN on)
          (NP (PRP$ your) (NN life)))
        (S
          (ADVP (RB so))
          (VP
            (VP (VB leave)
              (NP (PRP them))
              (ADVP (RB alone)))
            (CC and)
            (VP (VB leave)
              (NP (PRP them))
              (PP (IN in)
                (NP (NN peace))))))))
    (. .)))

det(woman-2, A-1)
poss(abortion-4, woman-2)
nsubj(has-5, abortion-4)
root(ROOT-0, has-5)
det(impact-10, no-6)
amod(impact-10, financial-7)
conj_or(financial-7, physical-9)
amod(impact-10, physical-9)
dobj(has-5, impact-10)
poss(life-13, your-12)
prep_on(impact-10, life-13)
advmod(leave-15, so-14)
dep(impact-10, leave-15)
dobj(leave-15, them-16)
advmod(leave-15, alone-17)
dep(impact-10, leave-19)
conj_and(leave-15, leave-19)
dobj(leave-19, them-20)
prep_in(leave-19, peace-22)

