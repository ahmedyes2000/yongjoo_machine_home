(ROOT
  (S ('' '')
    (S
      (NP (DT No) (NN one))
      (VP (VBZ has)
        (NP
          (NP (NNS rights))
          (PP (TO to)
            (NP (PRP$ their) (NN body))))))
    (, ,)
    (VP (VBP remember))
    (. ?)))

det(one-3, No-2)
nsubj(has-4, one-3)
ccomp(remember-10, has-4)
dobj(has-4, rights-5)
poss(body-8, their-7)
prep_to(rights-5, body-8)
root(ROOT-0, remember-10)

