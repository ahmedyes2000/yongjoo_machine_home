(ROOT
  (S
    (SBAR
      (WHADVP (WRB When))
      (S
        (VP (VBG looking)
          (PP (IN at)
            (NP (NN evil))))))
    (NP (PRP I))
    (VP (VBP call)
      (S
        (NP (PRP it))
        (SBAR
          (SBAR (IN as)
            (S
              (NP (PRP it))
              (VP (VBZ is))))
          (: ;)
          (PRN
            (FRAG
              (ADJP (JJ evil)))))))
    (. .)))

advmod(looking-2, When-1)
advcl(call-6, looking-2)
prep_at(looking-2, evil-4)
nsubj(call-6, I-5)
root(ROOT-0, call-6)
dep(is-10, it-7)
mark(is-10, as-8)
nsubj(is-10, it-9)
dep(call-6, is-10)
dep(is-10, evil-12)

