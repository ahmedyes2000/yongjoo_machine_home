(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is)
      (NP
        (NP
          (NP (DT a) (NN person) (POS 's))
          (NN humanity))
        (, ,)
        (NP (RB not) (PRP$ his) (NN utility))
        (, ,)
        (SBAR
          (WHNP (WDT that))
          (S
            (VP (MD should)
              (VP (VB guarantee)
                (NP (NNS rights)
                  (CC and)
                  (NN protection))
                (PP (IN under)
                  (NP (DT the) (NN law)))))))))
    (. .)))

nsubj(humanity-6, It-1)
cop(humanity-6, is-2)
det(person-4, a-3)
poss(humanity-6, person-4)
root(ROOT-0, humanity-6)
neg(utility-10, not-8)
poss(utility-10, his-9)
appos(humanity-6, utility-10)
nsubj(guarantee-14, utility-10)
aux(guarantee-14, should-13)
rcmod(humanity-6, guarantee-14)
rcmod(utility-10, guarantee-14)
dobj(guarantee-14, rights-15)
dobj(guarantee-14, protection-17)
conj_and(rights-15, protection-17)
det(law-20, the-19)
prep_under(guarantee-14, law-20)

