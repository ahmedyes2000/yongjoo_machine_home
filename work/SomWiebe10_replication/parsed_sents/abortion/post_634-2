(ROOT
  (S
    (NP (NNP Whileeveryone) (NNP else) (POS 's))
    (VP (VBP are)
      (NP (JJ uncertain) (NNS ideas))
      (, ,)
      (PP (IN with)
        (NP
          (NP (DT no) (NN foundation))
          (PP (IN in)
            (NP (NN reality))))))
    (. .)))

nn(else-2, Whileeveryone-1)
nsubj(ideas-6, else-2)
possessive(else-2, 's-3)
cop(ideas-6, are-4)
amod(ideas-6, uncertain-5)
root(ROOT-0, ideas-6)
det(foundation-10, no-9)
