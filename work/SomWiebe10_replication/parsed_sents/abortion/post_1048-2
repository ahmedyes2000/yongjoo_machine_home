(ROOT
  (S
    (NP (DT This) (NN abortion))
    (VP (VBZ is)
      (ADVP (RB usually))
      (VP (VBN performed)
        (PP (IN in)
          (NP (DT the) (JJ third) (NN trimester)))))
    (. .)))

det(abortion-2, This-1)
nsubjpass(performed-5, abortion-2)
auxpass(performed-5, is-3)
advmod(performed-5, usually-4)
root(ROOT-0, performed-5)
det(trimester-9, the-7)
amod(trimester-9, third-8)
prep_in(performed-5, trimester-9)

