(ROOT
  (S
    (NP
      (NP (RB Nearly) (DT every) (NN child))
      (VP (VBN born)))
    (VP (VBZ is)
      (NP (DT an) (JJ unwelcome) (NN guest)))
    (. .)))

advmod(child-3, Nearly-1)
det(child-3, every-2)
nsubj(guest-8, child-3)
partmod(child-3, born-4)
cop(guest-8, is-5)
det(guest-8, an-6)
amod(guest-8, unwelcome-7)
root(ROOT-0, guest-8)

