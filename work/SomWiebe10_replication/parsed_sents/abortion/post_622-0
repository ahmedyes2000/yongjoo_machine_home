(ROOT
  (S
    (NP (PRP I))
    (VP (VBP am)
      (ADJP (RB very) (JJ sorry)
        (PP (IN for)
          (SBAR
            (WHNP (WP what))
            (S
              (VP (VBD happened)
                (PP (TO to)
                  (NP (PRP you)))))))))
    (. .)))

nsubj(sorry-4, I-1)
cop(sorry-4, am-2)
advmod(sorry-4, very-3)
root(ROOT-0, sorry-4)
nsubj(happened-7, what-6)
prepc_for(sorry-4, happened-7)
prep_to(happened-7, you-9)

