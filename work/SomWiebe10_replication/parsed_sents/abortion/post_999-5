(ROOT
  (S
    (S
      (NP (NNP Science))
      (VP (VBZ says)
        (SBAR
          (S
            (NP (PRP it))
            (VP (VBZ 's) (RB not)
              (NP (DT a) (NN baby)))))))
    (, ,)
    (CC and)
    (S
      (NP
        (NP (NN nothing))
        (SBAR
          (S
            (NP (PRP you))
            (VP (VBD said)))))
      (VP (MD will)
        (VP (VB stop)
          (NP (DT a) (JJ single) (NN abortion))
          (PP (IN from)
            (S
              (VP (VBG happening)))))))
    (. .)))

nsubj(says-2, Science-1)
root(ROOT-0, says-2)
nsubj(baby-7, it-3)
cop(baby-7, 's-4)
neg(baby-7, not-5)
det(baby-7, a-6)
ccomp(says-2, baby-7)
dobj(said-12, nothing-10)
nsubj(stop-14, nothing-10)
nsubj(said-12, you-11)
rcmod(nothing-10, said-12)
aux(stop-14, will-13)
