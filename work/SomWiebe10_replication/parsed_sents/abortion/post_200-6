(ROOT
  (S
    (NP (DT An) (JJ infallible) (NN example))
    (ADVP (RB here))
    (VP (MD would)
      (VP (VB be)
        (NP
          (NP (DT the) (NN reality))
          (PP (IN of)
            (NP (NNS angels)
              (CC and)
              (NNS demons))))
        (PP (IN as)
          (NP
            (NP (NNS creatures))
            (PP (IN of)
              (NP (NN intellect)
                (CC and)
                (NN will)))))))
    (. .)))

det(example-3, An-1)
amod(example-3, infallible-2)
nsubj(reality-8, example-3)
advmod(reality-8, here-4)
aux(reality-8, would-5)
cop(reality-8, be-6)
det(reality-8, the-7)
root(ROOT-0, reality-8)
prep_of(reality-8, angels-10)
prep_of(reality-8, demons-12)
conj_and(angels-10, demons-12)
prep_as(reality-8, creatures-14)
