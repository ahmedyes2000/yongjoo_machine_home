(ROOT
  (S
    (NP (DT The) (NNS wrongness))
    (VP (VBZ increases)
      (SBAR (IN as)
        (S
          (NP (PRP we))
          (VP (VB acquire)
            (NP
              (NP
                (NP (DT the) (NNS parts))
                (SBAR
                  (WHNP (WDT that))
                  (S
                    (VP (VBP make)
                      (S
                        (NP (PRP us))
                        (ADJP (JJ unique)))))))
              (, ,)
              (ADJP
                (ADJP (JJ complex))
                (, ,)
                (CC and)
                (ADJP
                  (ADVP (RB eventually))
                  (JJ sentient))))))))
    (. .)))

det(wrongness-2, The-1)
nsubj(increases-3, wrongness-2)
root(ROOT-0, increases-3)
mark(acquire-6, as-4)
nsubj(acquire-6, we-5)
advcl(increases-3, acquire-6)
det(parts-8, the-7)
dobj(acquire-6, parts-8)
nsubj(make-10, parts-8)
rcmod(parts-8, make-10)
nsubj(unique-12, us-11)
xcomp(make-10, unique-12)
amod(parts-8, complex-14)
advmod(sentient-18, eventually-17)
amod(parts-8, sentient-18)
conj_and(complex-14, sentient-18)

