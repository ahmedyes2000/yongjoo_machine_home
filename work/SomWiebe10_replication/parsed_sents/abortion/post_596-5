    (VP (VBP think)
      (SBAR
        (S
          (NP (PRP she))
          (VP (VBD decided)
            (PP (IN in)
              (NP (DT the) (NN end)))
            (S
              (VP (TO to)
                (VP (VB tell)
                  (NP (DT the) (NN daughter))
                  (SBAR (IN that)
                    (S
                      (NP
                        (NP (DT the) (JJS best) (NN thing))
                        (PP (IN in)
                          (NP (PRP$ her) (NN life))))
