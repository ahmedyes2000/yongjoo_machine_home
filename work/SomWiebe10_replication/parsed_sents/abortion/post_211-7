(ROOT
  (S
    (ADVP (RB Rather))
    (NP (PRP it))
    (VP (VBZ is)
      (ADJP (IN about))
      (SBAR (IN whether)
        (CC or)
        (RB not)
        (S
          (NP
            (NP (DT a) (JJ human) (NN life))
            (, ,)
            (NP (RB even) (DT an) (JJ unborn) (NN one))
            (, ,))
          (VP (VBZ is)
            (NP
              (NP (JJ deserving))
              (PP (IN of)
                (NP
                  (NP (NN protection))
                  (PP (IN as)
                    (NP
                      (NP (DT a) (NN person))
                      (PP (IN under)
                        (NP (DT the) (NN law))))))))))))
    (. .)))

advmod(about-4, Rather-1)
nsubj(about-4, it-2)
cop(about-4, is-3)
root(ROOT-0, about-4)
complm(deserving-18, whether-5)
ccomp(about-4, not-7)
conj_or(deserving-18, not-7)
det(life-10, a-8)
