nsubj(deserving-18, life-10)
advmod(one-15, even-12)
det(one-15, an-13)
amod(one-15, unborn-14)
appos(life-10, one-15)
cop(deserving-18, is-17)
ccomp(about-4, deserving-18)
prep_of(deserving-18, protection-20)
det(person-23, a-22)
prep_as(protection-20, person-23)
det(law-26, the-25)
prep_under(person-23, law-26)

