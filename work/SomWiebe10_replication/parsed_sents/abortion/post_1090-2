(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP (VBP do) (RB not)
          (VP (VB want)
            (S
              (NP (DT the) (NN child))
              (VP (VB give)
                (NP (PRP it))
                (ADVP (RB up)
                  (PP (IN for)
                    (NP (NN adoption))))))))))
    (, ,)
    (NP (EX there))
    (VP (VBZ is)
      (NP (DT no) (NN reason)
        (S
          (VP (TO to)
            (VP (VB kill)
              (NP (PRP it)))))))
    (. .)))

mark(want-5, If-1)
nsubj(want-5, you-2)
aux(want-5, do-3)
neg(want-5, not-4)
advcl(is-15, want-5)
det(child-7, the-6)
nsubj(give-8, child-7)
ccomp(want-5, give-8)
dobj(give-8, it-9)
advmod(give-8, up-10)
prep_for(up-10, adoption-12)
expl(is-15, there-14)
root(ROOT-0, is-15)
det(reason-17, no-16)
nsubj(is-15, reason-17)
aux(kill-19, to-18)
infmod(reason-17, kill-19)
dobj(kill-19, it-20)

