(ROOT
  (S
    (PP (VBG According)
      (PP (TO to)
        (NP (PRP you))))
    (, ,)
    (NP
      (NP (PRP$ my) (NN doctor))
      (, ,)
      (NP (PRP$ my) (NNS parents))
      (, ,)
      (CC and)
      (NP (PRP I)))
    (VP (VBP are)
      (NP (NNS murderers)))
    (. .)))

prepc_according_to(murderers-14, to-2)
pobj(murderers-14, you-3)
poss(doctor-6, my-5)
nsubj(murderers-14, doctor-6)
poss(parents-9, my-8)
conj_and(doctor-6, parents-9)
nsubj(murderers-14, parents-9)
conj_and(doctor-6, I-12)
nsubj(murderers-14, I-12)
