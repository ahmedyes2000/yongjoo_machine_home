(ROOT
  (S
    (NP (PRP I))
    (VP (VBP believe)
      (SBAR
        (S
          (NP (PRP it))
          (VP (VBZ is)
            (ADVP (RB never))
            (NP
              (NP (CD zero))
              (CC and)
              (NP
                (NP (NNS increases))
                (PP (IN throughout)
                  (NP (NN gestation)))))))))
    (. .)))

nsubj(believe-2, I-1)
root(ROOT-0, believe-2)
nsubj(zero-6, it-3)
cop(zero-6, is-4)
neg(zero-6, never-5)
ccomp(believe-2, zero-6)
