(ROOT
  (S
    (NP (PRP He))
    (VP (VBZ 's)
      (NP
        (NP (DT the) (JJ only) (NN one))
        (PP (IN with)
          (NP (DT the) (NN right)
            (S
              (VP (TO to)
                (VP (VB take)
                  (NP (PRP it))
                  (ADVP (RB away)))))))))
    (. .)))

nsubj(one-5, He-1)
cop(one-5, 's-2)
det(one-5, the-3)
amod(one-5, only-4)
root(ROOT-0, one-5)
det(right-8, the-7)
prep_with(one-5, right-8)
aux(take-10, to-9)
infmod(right-8, take-10)
dobj(take-10, it-11)
advmod(take-10, away-12)

