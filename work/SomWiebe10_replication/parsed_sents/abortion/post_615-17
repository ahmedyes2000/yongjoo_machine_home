(ROOT
  (S
    (NP (DT The) (JJ only) (NN dispute))
    (VP (VBZ regards)
      (NP
        (NP (DT the) (JJ hazy) (NN issue))
        (PP (IN of)
          (ADJP (`` ``) (JJ personhood) ('' '')))
        (SBAR
          (WHNP (WDT which))
          (S
            (VP (VBZ is)
              (NP
                (NP (DT a) (NN matter))
                (PP (IN of)
                  (NP (NN opinion))))
              (, ,)
              (S (RB not)
                (VP (TO to)
                  (VP (VB mention)
                    (NP
                      (NP (DT a) (NN distinction))
                      (SBAR
                        (WHNP (WDT that))
                        (S
                          (VP (MD may) (RB not)
                            (ADVP (RB even))
                            (VP (VB be)
                              (ADJP (JJ meaningful)))))))))))))))
    (. .)))

det(dispute-3, The-1)
amod(dispute-3, only-2)
nsubj(regards-4, dispute-3)
root(ROOT-0, regards-4)
det(issue-7, the-5)
