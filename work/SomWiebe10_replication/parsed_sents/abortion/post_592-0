(ROOT
  (S
    (NP (DT The) (NN sperm))
    (VP (VBZ is)
      (NP
        (NP (DT a) (NN part))
        (PP (IN of)
          (NP
            (NP (DT the) (NN father) (POS 's))
            (NN body)))))
    (. .)))

det(sperm-2, The-1)
nsubj(part-5, sperm-2)
cop(part-5, is-3)
det(part-5, a-4)
root(ROOT-0, part-5)
det(father-8, the-7)
poss(body-10, father-8)
prep_of(part-5, body-10)

