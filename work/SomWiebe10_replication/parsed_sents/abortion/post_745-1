(ROOT
  (S
    (NP (NNS thats))
    (SBAR
      (WHADVP (WRB when))
      (S
        (NP (NN abortion))
        (VP (VBZ is)
          (VP (VBN required)))))
    (. .)))

dep(required-5, thats-1)
advmod(required-5, when-2)
nsubjpass(required-5, abortion-3)
auxpass(required-5, is-4)
root(ROOT-0, required-5)

