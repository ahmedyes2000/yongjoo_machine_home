(ROOT
  (S
    (NP (PRP They))
    (VP (VBP respond)
      (S
        (VP (TO to)
          (VP (VB touch) (, ,)
            (S
              (VP
                (VP (TO to)
                  (VP (VB sound)))
                (CC and)
                (ADVP (RB even))
                (VP (TO to)
                  (VP (VB light)))))))))
    (. .)))

nsubj(respond-2, They-1)
xsubj(touch-4, They-1)
root(ROOT-0, respond-2)
aux(touch-4, to-3)
xcomp(respond-2, touch-4)
aux(sound-7, to-6)
ccomp(touch-4, sound-7)
ccomp(touch-4, even-9)
advmod(sound-7, even-9)
conj_and(sound-7, even-9)
aux(light-11, to-10)
ccomp(touch-4, light-11)
conj_and(sound-7, light-11)

