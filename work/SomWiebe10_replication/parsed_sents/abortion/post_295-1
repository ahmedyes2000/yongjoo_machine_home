(ROOT
  (S (CC and)
    (NP (DT the) (NNS babies))
    (VP (VBP look)
      (NP
        (ADJP (RB so) (JJ sad)
          (CC and)
          (JJ scary))
        (NN lol)))
    (. .)))

cc(look-4, and-1)
det(babies-3, the-2)
nsubj(look-4, babies-3)
root(ROOT-0, look-4)
advmod(sad-6, so-5)
amod(lol-9, sad-6)
conj_and(sad-6, scary-8)
amod(lol-9, scary-8)
dobj(look-4, lol-9)

