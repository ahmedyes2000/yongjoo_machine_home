(ROOT
  (S
    (NP (DT The) (NN fetus))
    (VP (MD can)
      (VP (VB feel)
        (NP (NN pain))
        (, ,)
        (S
          (ADVP (RB thus))
          (VP (VBG making)
            (NP
              (NP (DT the) (NNS processes))
              (VP (VBN used)
                (PP (IN in)
                  (NP (JJ partial) (NN birth) (NN abortion) (NNS barbarous)))))))))
    (. .)))

det(fetus-2, The-1)
nsubj(feel-4, fetus-2)
aux(feel-4, can-3)
root(ROOT-0, feel-4)
dobj(feel-4, pain-5)
advmod(making-8, thus-7)
partmod(feel-4, making-8)
xcomp(feel-4, making-8)
det(processes-10, the-9)
dobj(making-8, processes-10)
partmod(processes-10, used-11)
amod(barbarous-16, partial-13)
nn(barbarous-16, birth-14)
nn(barbarous-16, abortion-15)
prep_in(used-11, barbarous-16)

