(ROOT
  (S
    (S
      (NP (PRP I))
      (VP (VBP 'm)
        (ADJP (JJ pro-choice))))
    (CC but)
    (S
      (NP (PRP I))
      (VP (VBP am)
        (ADVP (RB also))
        (PP (IN for)
          (S
            (VP
              (VP (VBG taking)
                (NP
                  (NP (NN responsibility))
                  (PP (IN for)
                    (NP (PRP$ your) (NNS choices)))))
              (CC and)
              (VP (VBG accepting)
                (NP (DT the) (NNS consequences))))))))
    (. .)))

nsubj(pro-choice-3, I-1)
cop(pro-choice-3, 'm-2)
root(ROOT-0, pro-choice-3)
nsubj(am-6, I-5)
conj_but(pro-choice-3, am-6)
advmod(am-6, also-7)
prepc_for(am-6, taking-9)
