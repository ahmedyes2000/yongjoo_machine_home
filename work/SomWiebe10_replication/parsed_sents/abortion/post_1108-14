(ROOT
  (S (IN So)
    (NP (PRP you))
    (VP (MD should)
      (VP (VB be)
        (ADJP (JJ able)
          (S
            (VP (TO to)
              (VP (VB choose)
                (SBAR (IN whether)
                  (FRAG (CC or)
                    (ADVP (RB not)
                      (S
                        (VP (TO to)
                          (VP (VB play)
                            (NP (NN host))
                            (PP (IN for)
                              (NP (CD 9) (NNS months)))))))))))))))
    (. .)))

dep(able-5, So-1)
nsubj(able-5, you-2)
