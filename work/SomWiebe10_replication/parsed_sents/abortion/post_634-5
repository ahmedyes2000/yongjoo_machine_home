(ROOT
  (S
    (NP (NNP Science))
    (ADVP (RB even))
    (VP (VBZ admits)
      (PRN (, ,)
        (S
          (NP (JJ scientific) (NNS FACTS))
          (VP (VBP are) (RB n't)
            (ADJP (JJ like))))
        (, ,))
      (NP (DT every) (NN day) (NNS facts)))
    (. .)))

nsubj(admits-3, Science-1)
advmod(admits-3, even-2)
root(ROOT-0, admits-3)
amod(FACTS-6, scientific-5)
nsubj(like-9, FACTS-6)
cop(like-9, are-7)
neg(like-9, n't-8)
parataxis(admits-3, like-9)
det(facts-13, every-11)
nn(facts-13, day-12)
tmod(admits-3, facts-13)

