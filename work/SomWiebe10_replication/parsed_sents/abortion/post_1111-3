(ROOT
  (S
    (NP (DT That))
    (VP (VBZ 's)
      (NP
        (NP (NN kind))
        (PP (IN of)
          (PRN (, ,)
            (SBAR
              (WHADVP (WRB how))
              (S
                (VP (VBP do)
                  (SBAR
                    (S
                      (NP (PRP you))
                      (VP (VBP say)))))))
            (, ,))
          (NP
            (NP (NNS ah-yes))
            (, ,)
            (NP (NN bullshit))))))
    (. .)))

nsubj(kind-3, That-1)
cop(kind-3, 's-2)
root(ROOT-0, kind-3)
advmod(do-7, how-6)
dep(kind-3, do-7)
nsubj(say-9, you-8)
ccomp(do-7, say-9)
prep_of(kind-3, ah-yes-11)
appos(ah-yes-11, bullshit-13)

