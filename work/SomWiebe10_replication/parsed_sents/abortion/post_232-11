(ROOT
  (S
    (NP (PRP It))
    (VP (VBD was)
      (ADVP
        (NP
          (QP (JJR more) (IN than) (DT a))
          (NN decade))
        (RB ago)))
    (. .)))

nsubj(was-2, It-1)
root(ROOT-0, was-2)
mwe(than-4, more-3)
quantmod(a-5, than-4)
num(decade-6, a-5)
npadvmod(ago-7, decade-6)
advmod(was-2, ago-7)

