(ROOT
  (S
    (NP (DT The) (NN point))
    (VP (VBZ is)
      (SBAR (IN that)
        (S
          (NP (DT these) (NNS things))
          (VP (VBP are)
            (ADVP (RB never))
            (ADJP (JJ flawless))))))
    (. .)))

det(point-2, The-1)
nsubj(is-3, point-2)
root(ROOT-0, is-3)
complm(flawless-9, that-4)
det(things-6, these-5)
nsubj(flawless-9, things-6)
cop(flawless-9, are-7)
neg(flawless-9, never-8)
ccomp(is-3, flawless-9)

