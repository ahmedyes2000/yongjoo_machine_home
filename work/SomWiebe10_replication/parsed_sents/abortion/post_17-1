(ROOT
  (S
    (NP (DT This))
    (VP (VBZ is)
      (ADVP (RB partly))
      (SBAR
        (WHADVP (WRB why))
        (S
          (NP (NNS mothers))
          (ADVP (RB sometimes))
          (VP (VBP walk)
            (PRT (RP around))
            (PP (IN with)
              (NP
                (NP (NNS earphones))
                (PP (IN on)
                  (NP (PRP$ their) (NNS stomachs)))))))))
    (. .)))

nsubj(is-2, This-1)
root(ROOT-0, is-2)
advmod(is-2, partly-3)
advmod(walk-7, why-4)
nsubj(walk-7, mothers-5)
advmod(walk-7, sometimes-6)
advcl(is-2, walk-7)
prt(walk-7, around-8)
prep_with(walk-7, earphones-10)
poss(stomachs-13, their-12)
prep_on(earphones-10, stomachs-13)

