(ROOT
  (S
    (NP (PRP I))
    (ADVP (RB personally))
    (VP (VBP believe)
      (SBAR
        (S
          (NP (NN abortion))
          (VP (VBZ is)
            (NP (DT an) (JJ awful) (NN thing)
              (S
                (VP (TO to)
                  (VP (VB consider)
                    (PP (IN for)
                      (NP (DT a) (NN person)))))))))))
    (. .)))

nsubj(believe-3, I-1)
advmod(believe-3, personally-2)
root(ROOT-0, believe-3)
nsubj(thing-8, abortion-4)
cop(thing-8, is-5)
det(thing-8, an-6)
amod(thing-8, awful-7)
ccomp(believe-3, thing-8)
aux(consider-10, to-9)
infmod(thing-8, consider-10)
det(person-13, a-12)
prep_for(consider-10, person-13)

