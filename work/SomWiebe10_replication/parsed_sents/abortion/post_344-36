(ROOT
  (S ('' '')
    (NP (NNP Do) (NNP I))
    (VP (VBP risk)
      (NP (PRP$ my) (JJ own) (NN life))
      (PP (IN by)
        (S
          (VP (VBG undergoing)
            (NP (JJ major) (NN surgery))))))
    (. ?)))

nn(I-3, Do-2)
nsubj(risk-4, I-3)
root(ROOT-0, risk-4)
poss(life-7, my-5)
amod(life-7, own-6)
dobj(risk-4, life-7)
prepc_by(risk-4, undergoing-9)
amod(surgery-11, major-10)
dobj(undergoing-9, surgery-11)

