(ROOT
  (S
    (NP (PRP You))
    (ADVP (RB just))
    (VP (VBD compared)
      (NP
        (NP (DT the) (NN life))
        (PP (IN of)
          (NP (DT a) (JJ human) (NN fetus))))
      (PP (TO to)
        (NP (DT a) (NN snail))))
    (. .)))

nsubj(compared-3, You-1)
advmod(compared-3, just-2)
root(ROOT-0, compared-3)
det(life-5, the-4)
dobj(compared-3, life-5)
det(fetus-9, a-7)
amod(fetus-9, human-8)
prep_of(life-5, fetus-9)
det(snail-12, a-11)
prep_to(compared-3, snail-12)

