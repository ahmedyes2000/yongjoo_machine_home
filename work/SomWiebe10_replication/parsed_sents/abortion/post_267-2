(ROOT
  (S (CC And)
    (NP (EX there))
    (VP (VBP are)
      (NP
        (NP (JJ many) (NNS places))
        (SBAR
          (WHNP (WDT that))
          (S
            (VP (VBP refuse)
              (S
                (VP (TO to)
                  (VP (VB sell)
                    (NP (NN morning))
                    (PP (IN after)
                      (NP (NNS pills)))))))))))
    (. .)))

cc(are-3, And-1)
expl(are-3, there-2)
root(ROOT-0, are-3)
amod(places-5, many-4)
nsubj(are-3, places-5)
nsubj(refuse-7, places-5)
rcmod(places-5, refuse-7)
aux(sell-9, to-8)
xcomp(refuse-7, sell-9)
tmod(sell-9, morning-10)
prep_after(sell-9, pills-12)

