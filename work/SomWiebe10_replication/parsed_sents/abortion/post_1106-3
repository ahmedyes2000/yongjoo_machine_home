(ROOT
  (S
    (NP (PRP They))
    (VP (VBP are)
      (VP (VBG going)
        (S
          (VP (TO to)
            (VP (VB act)
              (PP (IN on)
                (NP (PRP it))))))))
    (. .)))

nsubj(going-3, They-1)
xsubj(act-5, They-1)
aux(going-3, are-2)
root(ROOT-0, going-3)
aux(act-5, to-4)
