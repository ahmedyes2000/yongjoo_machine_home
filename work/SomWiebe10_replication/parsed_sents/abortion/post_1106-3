(ROOT
  (S
    (S
      (NP (NNP Let) (POS 's))
      (VP (VBP face)
        (NP (PRP it))))
    (, ,)
    (NP (JJ unmarried) (NNS people))
    (VP (VBP have)
      (NP
        (NP
          (NP (NNS impulses))
          (, ,)
          (NP (NNS desires))
          (, ,))
        (X (NN etc.))))
    (. .)))

nsubj(face-3, Let-1)
possessive(Let-1, 's-2)
ccomp(have-8, face-3)
dobj(face-3, it-4)
amod(people-7, unmarried-6)
