  (S (CC And)
    (NP (DT these) (JJ homemade) (NNS abortions))
    (VP
      (VP (VBP are) (RB not)
        (ADJP (RB always) (JJ successful)))
      (CC and)
      (VP
        (ADVP (RB thus))
