(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP (VBP do) (RB n't)
          (VP (VB want)
            (NP (DT a) (NN child))))))
    (, ,)
    (VP (VB do) (RB n't)
      (VP (VB have)
        (NP (NN sex))))
    (. .)))

mark(want-5, If-1)
nsubj(want-5, you-2)
aux(want-5, do-3)
neg(want-5, n't-4)
advcl(have-11, want-5)
det(child-7, a-6)
dobj(want-5, child-7)
aux(have-11, do-9)
neg(have-11, n't-10)
root(ROOT-0, have-11)
dobj(have-11, sex-12)

