  (S
    (NP (PRP You))
    (VP (VBP claim)
      (SBAR (IN that)
        (S
          (NP (DT the) (NN difference))
          (VP (VBZ is)
            (SBAR (IN that)
              (S
                (NP (NNS women))
                (VP (VBP have)
                  (VP
                    (VP (VBN sex)
                      (PP (IN by)
                        (NP (NN choice))))
                    (CC and)
                    (VP (VBN become)
                      (S
                        (ADJP (JJ pregnant))))))))))))
    (. .)))

nsubj(claim-2, You-1)
root(ROOT-0, claim-2)
complm(is-6, that-3)
det(difference-5, the-4)
