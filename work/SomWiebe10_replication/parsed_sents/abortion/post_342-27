(ROOT
  (S
    (NP (DT No) (NN one))
    (VP (MD can)
      (VP (VB force)
        (S
          (NP (PRP you))
          (VP (TO to)
            (VP (VB do)
              (NP (DT those) (NNS things)))))))
    (. .)))

det(one-2, No-1)
nsubj(force-4, one-2)
aux(force-4, can-3)
root(ROOT-0, force-4)
nsubj(do-7, you-5)
aux(do-7, to-6)
xcomp(force-4, do-7)
det(things-9, those-8)
dobj(do-7, things-9)

