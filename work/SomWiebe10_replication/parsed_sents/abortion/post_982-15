(ROOT
  (S
    (S
      (NP (PRP You))
      (VP (MD should)
        (VP (VB tell)
          (NP (PRP them))
          (SBAR (IN that)
            (S
              (SBAR
                (WHNP (WP what))
                (S
                  (NP (PRP they))
                  (VP (VBP are)
                    (VP (VBG doing)))))
              (VP (VBZ is)
                (ADJP (JJ sinful)
                  (CC and)
                  (JJ pray))
                (PP (IN for)
                  (NP (PRP them)))))))))
    (, ,)
    (CC but)
    (S
      (NP (PRP I))
      (VP (VBP do) (RB n't)
        (VP (VB believe)
          (SBAR
            (S
              (NP (PRP you))
              (VP (MD should)
                (VP
                  (VP (VB harass)
                    (NP (PRP them)))
                  (CC and)
                  (VP (VB look)
                    (PRT (RP down))
                    (PP (IN upon)
                      (NP
                        (NP (PRP them))
                        (PP (IN for)
                          (SBAR
                            (WHNP (WP what))
                            (S
                              (NP (PRP they))
                              (VP (VBP have)
                                (VP (VBN done))))))))))))))))
    (. .)))

