(ROOT
  (NP ('' '')
    (CC Are)
    (NP
      (NP (PRP you))
      (VP (VBG saying)
        (SBAR (IN that)
          (S
            (SBAR (IN if)
              (S
                (NP (DT a) (NN fetus))
                (VP (VBZ is)
                  (VP (VBN terminated)
                    (PP (IN before)
                      (NP (CD 21) (NNS weeks)))))))
            (, ,)
            (NP (DT that))
            (VP (VBZ 's)
              (ADJP (JJ okay)))))))
    (. .)))

cc(you-3, Are-2)
root(ROOT-0, you-3)
partmod(you-3, saying-4)
complm(okay-17, that-5)
mark(terminated-10, if-6)
det(fetus-8, a-7)
nsubjpass(terminated-10, fetus-8)
auxpass(terminated-10, is-9)
advcl(okay-17, terminated-10)
num(weeks-13, 21-12)
prep_before(terminated-10, weeks-13)
nsubj(okay-17, that-15)
cop(okay-17, 's-16)
ccomp(saying-4, okay-17)

