(ROOT
  (S
    (NP (PRP$ Our) (JJ modern) (JJ popular) (NN obsession))
    (: ...)
    (VP (VBZ leads)
      (PP (TO to)
        (NP (DT the) (NN idea)))
      (SBAR (IN that)
        (S
          (NP (NN conception))
          (VP (VBZ is)
            (NP (DT the)
              (ADJP (RBS most) (JJ important))
              (NN benchmark))))))
    (. .)))

poss(obsession-4, Our-1)
amod(obsession-4, modern-2)
amod(obsession-4, popular-3)
nsubj(leads-6, obsession-4)
root(ROOT-0, leads-6)
det(idea-9, the-8)
prep_to(leads-6, idea-9)
complm(benchmark-16, that-10)
nsubj(benchmark-16, conception-11)
cop(benchmark-16, is-12)
det(benchmark-16, the-13)
advmod(important-15, most-14)
amod(benchmark-16, important-15)
ccomp(leads-6, benchmark-16)

