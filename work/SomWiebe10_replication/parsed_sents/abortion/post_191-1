(ROOT
  (S
    (NP
      (NP (NNS Depends))
      (PP (IN on)
        (NP (DT the) (NN way) (PRP we))))
    (VP (VBP feel)
      (PP (IN about)
        (NP
          (NP (DT the) (NN question))
          (VP (VBG being)
            (VP (VBN debated))))))
    (. .)))

nsubj(feel-6, Depends-1)
det(way-4, the-3)
prep_on(Depends-1, way-4)
dep(way-4, we-5)
root(ROOT-0, feel-6)
det(question-9, the-8)
prep_about(feel-6, question-9)
auxpass(debated-11, being-10)
partmod(question-9, debated-11)

