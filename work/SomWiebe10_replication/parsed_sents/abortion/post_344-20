(ROOT
  (S
    (NP (PRP You))
    (VP (VBP 're)
      (ADVP (RB now))
      (VP (VBG trying)
        (S
          (VP (TO to)
            (VP (VB change)
              (NP (PRP it)))))))
    (. .)))

nsubj(trying-4, You-1)
xsubj(change-6, You-1)
aux(trying-4, 're-2)
advmod(trying-4, now-3)
root(ROOT-0, trying-4)
aux(change-6, to-5)
xcomp(trying-4, change-6)
dobj(change-6, it-7)

