(ROOT
  (S
    (NP (PRP It))
    (VP (MD can)
      (VP (VB be)
        (VP (VBN found)
          (ADVP (RB clearly))
          (S
            (VP (VBN stated)
              (PP (IN in)
                (NP
                  (NP (DT the) (VBG opening) (NN paragraph))
                  (PP (IN of)
                    (NP
                      (NP (DT the) (NN Declaration))
                      (PP (IN of)
                        (NP (NN Independence))))))))))))
    (. .)))

nsubjpass(found-4, It-1)
aux(found-4, can-2)
auxpass(found-4, be-3)
root(ROOT-0, found-4)
advmod(found-4, clearly-5)
dep(found-4, stated-6)
det(paragraph-10, the-8)
amod(paragraph-10, opening-9)
prep_in(stated-6, paragraph-10)
det(Declaration-13, the-12)
prep_of(paragraph-10, Declaration-13)
prep_of(Declaration-13, Independence-15)

