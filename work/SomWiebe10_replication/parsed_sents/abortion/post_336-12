(ROOT
  (S
    (NP (WP What))
    (ADVP (RB then))
    (VP (VBZ is)
      (SBAR
        (S
          (NP (PRP it))
          (VP (VBD called)
            (SBAR
              (WHADVP (WRB when))
              (S
                (NP
                  (NP (DT the) (JJ innocent))
                  (, ,)
                  (SBAR
                    (WHNP (WP who))
                    (S
                      (VP (VBP doe) (RB n't)
                        (VP (VB have)
                          (NP (DT a) (NN voice)
                            (S
                              (VP (TO to)
                                (VP (VB defend)
                                  (NP (PRP$ their) (JJ own) (JJ inherent) (NNS rights)))))))))))
                (VP (VBP have)
                  (S
                    (NP (PRP them))
                    (VP (VBD violated))))))))))
    (: ...)))

nsubj(is-3, What-1)
advmod(is-3, then-2)
root(ROOT-0, is-3)
nsubj(called-5, it-4)
ccomp(is-3, called-5)
