(ROOT
  (S
    (NP (PRP You))
    (VP (MD 'll)
      (ADVP (RB never))
      (VP (VB find)
        (NP (JJ enough) (NNS people))
        (S
          (VP (TO to)
            (VP (VB adopt)
              (NP
                (NP
                  (QP (DT a) (CD million))
                  (NNS children))
                (PP (IN per)
                  (NP (NN year))))
              (NP (DT every) (NN year)))))))
    (. .)))

nsubj(find-4, You-1)
aux(find-4, 'll-2)
neg(find-4, never-3)
root(ROOT-0, find-4)
amod(people-6, enough-5)
dobj(find-4, people-6)
aux(adopt-8, to-7)
xcomp(find-4, adopt-8)
quantmod(million-10, a-9)
num(children-11, million-10)
dobj(adopt-8, children-11)
prep_per(children-11, year-13)
det(year-15, every-14)
tmod(adopt-8, year-15)

