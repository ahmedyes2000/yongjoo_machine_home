(ROOT
  (S
    (NP (NN Nobody))
    (VP (VBZ has)
      (NP (DT a) (NN right)
        (S
          (VP (TO to)
            (VP (VB take)
              (PRT (RP away))
              (NP (DT that) (NN gift)))))))
    (. .)))

nsubj(has-2, Nobody-1)
root(ROOT-0, has-2)
det(right-4, a-3)
dobj(has-2, right-4)
aux(take-6, to-5)
infmod(right-4, take-6)
prt(take-6, away-7)
det(gift-9, that-8)
dobj(take-6, gift-9)

