(ROOT
  (S
    (NP (DT The) (NN question))
    (VP (VBZ is) (RB not)
      (ADJP (IN about))
      (SBAR (IN whether)
        (S
          (NP (NNS abortions))
          (VP (MD should)
            (VP (VB be)
              (VP (VBN banned)))))
        (CC or)
        (RB not)))
    (. .)))

det(question-2, The-1)
nsubj(about-5, question-2)
cop(about-5, is-3)
neg(about-5, not-4)
root(ROOT-0, about-5)
complm(banned-10, whether-6)
nsubjpass(banned-10, abortions-7)
aux(banned-10, should-8)
auxpass(banned-10, be-9)
ccomp(about-5, banned-10)
ccomp(about-5, not-12)
conj_or(banned-10, not-12)

