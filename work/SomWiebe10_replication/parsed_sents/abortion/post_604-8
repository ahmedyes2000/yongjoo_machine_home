  (S
    (NP
      (QP (RB Just) (RB as))
      (NNS babies))
    (VP (VBP need)
      (NP
        (NP
          (NP (NN food))
          (PRN (-LRB- -LRB-)
            (NP
              (NP (NN mother) (POS 's))
              (NN milk))
            (-RRB- -RRB-)))
        (CC and)
        (NP (DT a) (NN place)
          (S
            (VP (TO to)
              (VP (VB stay))))))
      (PRN (-LRB- -LRB-)
        (S
          (NP (PRP it))
          (VP (VBZ 's)
            (NP
              (NP (NN mother) (POS 's))
              (NN home))))
        (-RRB- -RRB-)))
    (. .)))

