          (S
            (NP (PRP I))
            (VP (VBD thought)
              (SBAR
                (S
                  (NP (NN abortion))
                  (VP (VBD was)
                    (ADJP (JJ right)
                      (CC or)
                      (JJ good))))))))))
    (. .)))

