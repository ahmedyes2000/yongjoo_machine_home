(ROOT
  (S
    (NP (PRP I))
    (VP (VBP do) (RB not)
      (VP (VB see)
        (SBAR
          (WHADVP (WRB why))
          (S
            (NP (DT the) (NN child))
            (VP (MD should)
              (VP (VB have)
                (S
                  (VP (TO to)
                    (VP (VB suffer)
                      (PP (IN for)
                        (NP (PRP$ their) (NNS parents)))
                      (NP (JJ horrible) (NNS decisions)))))))
            (. .)))))
    (. .)))

