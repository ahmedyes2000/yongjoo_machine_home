(ROOT
  (S (RB So)
    (VP (VB do)
      (S
        (UCP
          (NP
            (NP (NNS rats))
            (CC and)
            (NP (NNS birds)
              (CC and)
              (NNS dolphins)))
          (CC and)
          (ADVP (RB so) (RB forth)))))
    (. .)))

advmod(do-2, So-1)
root(ROOT-0, do-2)
dep(do-2, rats-3)
dep(do-2, birds-5)
conj_and(rats-3, birds-5)
conj_and(rats-3, dolphins-7)
conj_and(birds-5, dolphins-7)
advmod(forth-10, so-9)
dep(do-2, forth-10)
advmod(rats-3, forth-10)
conj_and(rats-3, forth-10)

