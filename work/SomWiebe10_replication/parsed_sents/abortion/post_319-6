(ROOT
  (S (CC and)
    (NP (DT that))
    (VP (VBZ is)
      (NP
        (NP (DT the) (NN INDIFFERENCE))
        (PP (IN of)
          (NP (JJ good) (NNS men)))))
    (. .)))

cc(INDIFFERENCE-5, and-1)
nsubj(INDIFFERENCE-5, that-2)
cop(INDIFFERENCE-5, is-3)
det(INDIFFERENCE-5, the-4)
root(ROOT-0, INDIFFERENCE-5)
amod(men-8, good-7)
prep_of(INDIFFERENCE-5, men-8)

