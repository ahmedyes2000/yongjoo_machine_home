(ROOT
  (S
    (ADVP (RB Therefore))
    (, ,)
    (NP
      (NP (NN anyone))
      (PP (IN under)
        (NP
          (NP (DT the) (NN age))
          (PP (IN of)
            (NP (CD 18))))))
    (VP (MD should) (RB not)
      (VP (VB be)
        (VP (VBN held)
          (ADJP (RB entirely) (JJ responsible))
          (PP (IN for)
            (NP
              (NP (NNS actions))
              (PP (JJ such) (IN as)
                (NP (JJ unintended) (NN pregnancy))))))))
    (. .)))

advmod(held-12, Therefore-1)
nsubjpass(held-12, anyone-3)
det(age-6, the-5)
prep_under(anyone-3, age-6)
prep_of(age-6, 18-8)
aux(held-12, should-9)
neg(held-12, not-10)
auxpass(held-12, be-11)
root(ROOT-0, held-12)
advmod(responsible-14, entirely-13)
acomp(held-12, responsible-14)
prep_for(held-12, actions-16)
amod(pregnancy-20, unintended-19)
prep_such_as(actions-16, pregnancy-20)

