(ROOT
  (S
    (NP (PRP I))
    (VP (VBP have)
      (NP (DT a) (JJ little) (JJ wooden) (NN statue))
      (SBAR (IN that)
        (S
          (NP (PRP$ my) (NN brother))
          (VP (VBD gave)
            (NP (PRP me)))))
      (, ,)
      (SBAR
        (WHADVP (WRB where))
        (S
          (SBAR
            (WHADVP (WRB when))
            (S
              (NP (PRP you))
              (VP (VBP take)
                (NP (DT the) (JJ wooden) (NN barrel))
                (PRT (RP off))
                (PP (IN of)
                  (NP (PRP it))))))
          (, ,)
          (NP (PRP it))
          (VP (VBZ pops)
            (NP (DT an) (NN erection))))))
    (. .)))

nsubj(have-2, I-1)
root(ROOT-0, have-2)
det(statue-6, a-3)
amod(statue-6, little-4)
amod(statue-6, wooden-5)
dobj(have-2, statue-6)
complm(gave-10, that-7)
poss(brother-9, my-8)
nsubj(gave-10, brother-9)
ccomp(have-2, gave-10)
dobj(gave-10, me-11)
advmod(pops-25, where-13)
advmod(take-16, when-14)
nsubj(take-16, you-15)
advcl(pops-25, take-16)
det(barrel-19, the-17)
amod(barrel-19, wooden-18)
dobj(take-16, barrel-19)
prep_off_of(take-16, it-22)
nsubj(pops-25, it-24)
advcl(have-2, pops-25)
det(erection-27, an-26)
dobj(pops-25, erection-27)

