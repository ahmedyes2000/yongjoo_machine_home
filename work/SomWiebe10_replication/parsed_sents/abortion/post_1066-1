(ROOT
  (S
    (NP (DT The) (JJ sick)
      (CC and)
      (JJ handicapped))
    (VP (VBP are) (RB not)
      (PP (IN at)
        (NP (NN issue))))
    (. .)))

dep(sick-2, The-1)
nsubj(are-5, sick-2)
conj_and(sick-2, handicapped-4)
nsubj(are-5, handicapped-4)
root(ROOT-0, are-5)
neg(are-5, not-6)
prep_at(are-5, issue-8)

