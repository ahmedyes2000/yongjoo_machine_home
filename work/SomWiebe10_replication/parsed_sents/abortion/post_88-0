            (NP (NNS men))
            (-RRB- -RRB-)))
        (VP (VBP are)
          (ADVP (RB solely))
          (VP (VBN burdened)
            (PP (IN by)
              (NP (NN pregnancy)))))))
    (, ,)
    (NP (PRP they))
    (VP (MD must)
      (VP (VB have)
        (NP (DT a) (NN choice))))
    (. .)))

