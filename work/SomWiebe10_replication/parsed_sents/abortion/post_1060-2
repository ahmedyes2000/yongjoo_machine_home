  (S
    (NP (NNP Jstantall))
    (PRN (, ,)
      (S
        (NP (PRP you))
        (VP (VBP agree)
          (SBAR (IN that)
            (S
              (NP (NNS abortions))
              (VP (VBP are)
                (ADVP (RB sometimes))
                (ADJP (RB medically) (JJ necessary))))))))
    (VP (VBP correct)
      (NP (PRP me))
      (SBAR (IN if)
        (S
          (NP (PRP I))
          (VP (VBP am)
            (ADJP (JJ wrong))))))
    (. .)))

