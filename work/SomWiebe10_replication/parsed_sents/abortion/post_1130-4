(ROOT
  (S
    (ADVP (RB Again))
    (, ,)
    (VP (VB check)
      (PRT (RP out))
      (NP (DT the) (NNS stats))
      (PP (IN on)
        (NP (NN abortion) (NNS rates)))
      (PP (IN before)
        (CC and)
        (IN after)
        (NP (NNP Roe) (NNP vs) (NNP Wade))))
    (. .)))

advmod(check-3, Again-1)
root(ROOT-0, check-3)
conj_and(check-3, check-3')
prt(check-3, out-4)
det(stats-6, the-5)
dobj(check-3, stats-6)
nn(rates-9, abortion-8)
prep_on(check-3, rates-9)
nn(Wade-15, Roe-13)
nn(Wade-15, vs-14)
prep_after(check-3', Wade-15)
prep_before(check-3, Wade-15)

