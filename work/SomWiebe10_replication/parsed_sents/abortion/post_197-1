(ROOT
  (S
    (NP (JJ Many) (NNS things))
    (VP (VBP have)
      (VP (VBN been)
        (VP (VBN declared)
          (ADJP (JJ infallible))
          (PP (IN since)
            (NP (CD 1870))))))
    (. .)))

amod(things-2, Many-1)
nsubjpass(declared-5, things-2)
aux(declared-5, have-3)
auxpass(declared-5, been-4)
root(ROOT-0, declared-5)
acomp(declared-5, infallible-6)
prep_since(declared-5, 1870-8)

