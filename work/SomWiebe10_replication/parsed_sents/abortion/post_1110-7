(ROOT
  (S
    (S
      (VP (VBG Acting)
        (PP (IN upon)
          (NP (PRP them)))))
    (VP (VBZ is) (RB not)
      (NP
        (NP (DT a) (NN matter))
        (PP (IN of)
          (NP (NNS ethics)
            (CC or)
            (NN morality)))))
    (. .)))

csubj(matter-7, Acting-1)
prep_upon(Acting-1, them-3)
cop(matter-7, is-4)
neg(matter-7, not-5)
det(matter-7, a-6)
root(ROOT-0, matter-7)
prep_of(matter-7, ethics-9)
prep_of(matter-7, morality-11)
conj_or(ethics-9, morality-11)

