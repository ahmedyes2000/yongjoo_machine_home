(ROOT
  (S
    (NP (PRP We))
    (VP (MD can)
      (ADVP (RB certainly))
      (VP (DT both) (VB agree)
        (SBAR (IN that)
          (S
            (NP (PRP we))
            (VP (MD should)
              (VP (VB minimize)
                (NP
                  (NP (NNS numbers))
                  (PP (IN of)
                    (NP
                      (NP (NNS abortions))
                      (ADJP (RB as) (JJ possible)))))))))))
    (. .)))

nsubj(agree-5, We-1)
aux(agree-5, can-2)
advmod(agree-5, certainly-3)
dep(agree-5, both-4)
root(ROOT-0, agree-5)
complm(minimize-9, that-6)
