    (NP
      (NP (JJ definitive) (NNS statements))
      (PP (IN from)
        (NP
          (NP (DT the) (NNP Pope))
          (PP (IN in)
            (NP
              (NP (NN regard))
              (PP (TO to)
                (NP
                  (NP (NNS issues))
                  (PP (IN of)
                    (NP (NN faith)
                      (CC and)
                      (NNS morals))))))))))
    (VP
      (VP
        (ADVP
          (ADVP (RB also))
          (RB morally))
        (VBP obligate)
        (NP (DT all) (NNPS Catholics)))
      (CC and)
      (VP (VBP are) (DT both)
        (ADJP (JJ infallible)
          (CC and)
          (JJ unchangable))))
    (. .)))

amod(statements-3, definitive-2)
nsubj(obligate-17, statements-3)
nsubj(infallible-23, statements-3)
det(Pope-6, the-5)
