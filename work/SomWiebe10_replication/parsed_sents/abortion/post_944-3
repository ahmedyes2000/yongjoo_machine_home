(ROOT
  (S
    (NP (PRP You))
    (VP (VBP 're)
      (VP (VBG being)
        (VP
          (ADJP (JJ ridiculous))
          (, ,)
          (SBAR (RB just) (IN because)
            (S
              (NP (PRP you))
              (VP (VBP feel)
                (SBAR
                  (S
                    (NP (NN something))
                    (VP (VBZ does) (RB n't)
                      (VP (VB make)
                        (S
                          (NP (PRP it))
                          (ADJP (JJ true)))))))))))))
    (. .)))

nsubj(ridiculous-4, You-1)
aux(ridiculous-4, 're-2)
aux(ridiculous-4, being-3)
root(ROOT-0, ridiculous-4)
advmod(feel-9, just-6)
dep(feel-9, because-7)
nsubj(feel-9, you-8)
ccomp(ridiculous-4, feel-9)
nsubj(make-13, something-10)
aux(make-13, does-11)
neg(make-13, n't-12)
ccomp(feel-9, make-13)
nsubj(true-15, it-14)
xcomp(make-13, true-15)

