(ROOT
  (S (CC And)
    (PP (IN of)
      (NP (NN course)))
    (NP (JJ legal) (NN abortion))
    (VP (MD would)
      (VP (VB come)
        (PP (IN with)
          (NP (NNS regulations)))
        (S
          (VP (TO to)
            (VP (VB protect)
              (NP (PRP it))
              (PP (IN from)
                (NP (NN abuse))))))))
    (. .)))

cc(come-7, And-1)
prep_of(come-7, course-3)
amod(abortion-5, legal-4)
nsubj(come-7, abortion-5)
xsubj(protect-11, abortion-5)
aux(come-7, would-6)
root(ROOT-0, come-7)
prep_with(come-7, regulations-9)
aux(protect-11, to-10)
xcomp(come-7, protect-11)
dobj(protect-11, it-12)
prep_from(protect-11, abuse-14)

