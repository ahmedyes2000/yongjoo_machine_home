(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP (VBP know)
          (SBAR (IN that)
            (S
              (S
                (NP (DT a) (NN child))
                (VP (MD will)
                  (VP (VB die))))
              (CC and)
              (S
                (NP (PRP you))
                (VP (VBP do)
                  (NP (NN nothing))
                  (S
                    (VP (TO to)
                      (VP (VB prevent)
                        (NP (PRP it))))))))))))
    (, ,)
    (NP (PRP you))
    (VP (VBP have)
      (VP (VBN murdered)
        (NP (DT that) (NN child))))
    (. .)))

