(ROOT
  (S
    (SBAR (IN As)
      (S
        (NP (PRP you))
        (VP (MD can)
          (VP (VB see)))))
    (NP (EX there))
    (VP (VBP are)
      (NP
        (NP (NN multitude))
        (PP (IN of)
          (NP (JJ possible) (NNS situations)))
        (SBAR
          (WHPP (IN in)
            (WHNP (WDT which)))
          (S
            (NP (DT a) (JJ late-term) (NN abortion))
            (VP (VBZ is)
              (NP (DT the) (JJS best) (NN option)))))))
    (. .)))

