(ROOT
  (S
    (NP (PRP I))
    (ADVP (RB also))
    (VP (VBP have)
      (VP (VBN been)
        (ADJP (JJ careful)
          (S (RB not)
            (VP (TO to)
              (VP (VB argue)
                (PP (IN from)
                  (NP (JJ religious) (NNS reasons)))))))
        (, ,)
        (SBAR (IN although)
          (S
            (NP (PRP$ my) (NNS convictions))
            (VP (VB inform)
              (NP (PRP$ my) (NNS reasons)))))))
    (. .)))

nsubj(careful-5, I-1)
advmod(careful-5, also-2)
aux(careful-5, have-3)
cop(careful-5, been-4)
root(ROOT-0, careful-5)
neg(argue-8, not-6)
aux(argue-8, to-7)
dep(careful-5, argue-8)
amod(reasons-11, religious-10)
prep_from(argue-8, reasons-11)
mark(inform-16, although-13)
poss(convictions-15, my-14)
nsubj(inform-16, convictions-15)
advcl(careful-5, inform-16)
poss(reasons-18, my-17)
dobj(inform-16, reasons-18)

