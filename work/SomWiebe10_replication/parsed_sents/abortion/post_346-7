(ROOT
  (S
    (NP (DT This))
    (VP (VBZ is)
      (S
        (VP (TO to)
          (VP (VB save)
            (NP
              (NP (DT the) (JJ only) (NN life))
              (SBAR
                (WHNP (WP who))
                (S
                  (VP (MD would)
                    (VP (VB survive)
                      (SBAR (IN if)
                        (S
                          (NP (PRP we))
                          (VP (VBD did) (RB n't)
                            (VP (VB intervene))))))))))))))
    (. .)))

nsubj(is-2, This-1)
xsubj(save-4, This-1)
root(ROOT-0, is-2)
aux(save-4, to-3)
xcomp(is-2, save-4)
det(life-7, the-5)
amod(life-7, only-6)
dobj(save-4, life-7)
nsubj(survive-10, life-7)
aux(survive-10, would-9)
rcmod(life-7, survive-10)
mark(intervene-15, if-11)
