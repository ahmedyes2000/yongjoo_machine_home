(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP (VBP 're)
          (VP (VBG talking)
            (PP (IN about)
              (NP (JJ casual) (, ,) (JJ random) (NN sex)))))))
    (, ,)
    (ADVP (RB then))
    (NP (PRP$ your) (NN argument))
    (VP (MD may)
      (VP (VB hold)
        (NP (DT some) (NN weight))))
    (. .)))

mark(talking-4, If-1)
nsubj(talking-4, you-2)
aux(talking-4, 're-3)
advcl(hold-15, talking-4)
amod(sex-9, casual-6)
amod(sex-9, random-8)
prep_about(talking-4, sex-9)
advmod(hold-15, then-11)
poss(argument-13, your-12)
nsubj(hold-15, argument-13)
aux(hold-15, may-14)
root(ROOT-0, hold-15)
det(weight-17, some-16)
dobj(hold-15, weight-17)

