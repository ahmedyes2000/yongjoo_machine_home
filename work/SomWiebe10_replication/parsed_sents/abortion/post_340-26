(ROOT
  (S
    (NP
      (ADJP (JJ intact)
        (PP (IN through)))
      (NN abortion))
    (VP (VBZ is) (RB n't)
      (NP (DT a) (JJ separate) (NN issue)))
    (. .)))

amod(abortion-3, intact-1)
prep(intact-1, through-2)
nsubj(issue-8, abortion-3)
cop(issue-8, is-4)
neg(issue-8, n't-5)
det(issue-8, a-6)
amod(issue-8, separate-7)
root(ROOT-0, issue-8)

