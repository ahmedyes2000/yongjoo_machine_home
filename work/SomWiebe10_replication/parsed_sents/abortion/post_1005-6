(ROOT
  (S (CC And)
    (NP
      (NP (DT a) (NN question))
      (VP (RB not)
        (ADVP (RB properly))
        (VBN answered)))
    (VP (VBZ does) (RB n't)
      (VP (VB go)
        (PRT (RP away))))
    (. .)))

cc(go-9, And-1)
det(question-3, a-2)
nsubj(go-9, question-3)
neg(answered-6, not-4)
advmod(answered-6, properly-5)
partmod(question-3, answered-6)
aux(go-9, does-7)
neg(go-9, n't-8)
root(ROOT-0, go-9)
prt(go-9, away-10)

