(ROOT
  (NP
    (NP (DT The) (NN portrayal))
    (PP (IN of)
      (NP (NNS people)))
    (PP (IN as)
      (NP
        (NP (NNS objects))
        (PP (IN of)
          (NP (JJ sexual) (NN fulfillment)))))
    (. .)))

det(portrayal-2, The-1)
root(ROOT-0, portrayal-2)
prep_of(portrayal-2, people-4)
prep_as(portrayal-2, objects-6)
amod(fulfillment-9, sexual-8)
prep_of(objects-6, fulfillment-9)

