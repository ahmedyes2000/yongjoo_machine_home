(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is)
      (VP (VBN based)
        (PP (IN on)
          (SBAR
            (WHADVP (WRB when))
            (S
              (NP (NN consciousness))
              (VP (VBZ begins)))))))
    (. .)))

