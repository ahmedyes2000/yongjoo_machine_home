(ROOT
  (S
    (NP (PRP I))
    (ADVP (RB also))
    (VP (VBP love)
      (NP
        (NP (DT that) (JJ precious) (NN life))
        (SBAR
          (WHNP (WDT that))
          (S
            (VP (VBD was)
              (ADVP (RB inside))
              (PP (IN of)
                (NP (PRP you))))))))
    (. .)))

nsubj(love-3, I-1)
advmod(love-3, also-2)
root(ROOT-0, love-3)
det(life-6, that-4)
amod(life-6, precious-5)
dobj(love-3, life-6)
nsubj(was-8, life-6)
rcmod(life-6, was-8)
prep_inside_of(was-8, you-11)

