(ROOT
  (S
    (NP (DT These) (NNS issues))
    (VP (VBP are)
      (ADVP (RBR better))
      (VP (VBN left)
        (PP (IN between)
          (NP
            (NP (NNS women))
            (CC and)
            (NP (PRP$ their) (NNS physicians))))))
    (. .)))

det(issues-2, These-1)
nsubjpass(left-5, issues-2)
auxpass(left-5, are-3)
advmod(left-5, better-4)
root(ROOT-0, left-5)
prep_between(left-5, women-7)
poss(physicians-10, their-9)
prep_between(left-5, physicians-10)
conj_and(women-7, physicians-10)

