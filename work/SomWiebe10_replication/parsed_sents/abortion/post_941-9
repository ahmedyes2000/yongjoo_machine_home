            (WHADVP (WRB when))
            (S
              (NP (PRP$ your) (NN enemy))
              (VP (VBZ makes)
                (S
                  (NP (PRP you))
                  (ADJP (JJ angry)))))))))
    (: ...)
    (S
      (NP (PRP they))
