(ROOT
  (S
    (NP (PRP$ my) (NN son))
    (VP (VBZ is)
      (NP
        (NP (DT the) (NN world))
        (PP (TO to)
          (NP (PRP me)))))
    (. .)))

poss(son-2, my-1)
nsubj(world-5, son-2)
cop(world-5, is-3)
det(world-5, the-4)
root(ROOT-0, world-5)
prep_to(world-5, me-7)

(ROOT
  (S
    (NP (DT an) (NN abortion))
    (VP (MD would)
      (VP (VB be)
        (VP
          (ADVP (RB just) (RB as))
          (VBG tramatizing)
          (PP (IN as)
            (NP (DT a) (NN rape))))))))

det(abortion-2, an-1)
nsubj(tramatizing-7, abortion-2)
aux(tramatizing-7, would-3)
aux(tramatizing-7, be-4)
advmod(as-6, just-5)
advmod(tramatizing-7, as-6)
root(ROOT-0, tramatizing-7)
det(rape-10, a-9)
prep_as(tramatizing-7, rape-10)

