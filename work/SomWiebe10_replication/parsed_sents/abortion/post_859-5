(ROOT
  (S
    (NP (DT Every) (NN woman))
    (VP (MD should)
      (VP (VB have)
        (NP (DT the) (NN right)
          (S
            (VP (TO to)
              (VP (VB decide)
                (SBAR
                  (WHNP (WP what))
                  (S
                    (VP (VBZ 's)
                      (ADVP (RB bestfor))
                      (NP (PRP$ her) (NN health)))))))))))
    (. .)))

det(woman-2, Every-1)
nsubj(have-4, woman-2)
aux(have-4, should-3)
root(ROOT-0, have-4)
det(right-6, the-5)
dobj(have-4, right-6)
aux(decide-8, to-7)
infmod(right-6, decide-8)
nsubj(health-13, what-9)
cop(health-13, 's-10)
advmod(health-13, bestfor-11)
poss(health-13, her-12)
ccomp(decide-8, health-13)

