(ROOT
  (S
    (NP (PRP You))
    (VP (VBP make)
      (NP (DT an) (NN assertion))
      (PP (IN without)
        (S
          (VP (VBG substantiating)
            (NP (PRP it))))))
    (. .)))

nsubj(make-2, You-1)
root(ROOT-0, make-2)
det(assertion-4, an-3)
dobj(make-2, assertion-4)
prepc_without(make-2, substantiating-6)
dobj(substantiating-6, it-7)

