            (NP (NNP CreateDebate)))
          (SBAR
            (S
              (NP (PRP I))
              (VP (VBP suppose)))))))
    (. .)))

cc(ways-7, But-1)
advmod(ways-7, alas-2)
nsubj(ways-7, such-4)
cop(ways-7, are-5)
det(ways-7, the-6)
root(ROOT-0, ways-7)
