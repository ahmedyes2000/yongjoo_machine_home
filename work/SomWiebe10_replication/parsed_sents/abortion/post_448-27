(ROOT
  (S
    (VP (VB Make)
      (S
        (NP (DT the) (NN world))
        (NP
          (NP (DT a) (JJ welcome) (NN place))
          (PP (IN for)
            (NP (DT a) (NN child))))))
    (. .)))

root(ROOT-0, Make-1)
det(world-3, the-2)
nsubj(place-6, world-3)
det(place-6, a-4)
amod(place-6, welcome-5)
xcomp(Make-1, place-6)
det(child-9, a-8)
prep_for(place-6, child-9)

