(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is)
      (VP
        (VP (VBN consistant)
          (PP (IN with)
            (NP
              (NP (NN half))
              (PP (IN of)
                (NP (DT the) (NN country))))))
        (CC and)
        (VP (VBN opposed)
          (PP (IN by)
            (NP (NN half))))))
    (. .)))

nsubjpass(consistant-3, It-1)
nsubjpass(opposed-10, It-1)
auxpass(consistant-3, is-2)
root(ROOT-0, consistant-3)
prep_with(consistant-3, half-5)
det(country-8, the-7)
prep_of(half-5, country-8)
conj_and(consistant-3, opposed-10)
prep_by(opposed-10, half-12)

