(ROOT
  (S
    (ADVP (LS Third))
    (, ,)
    (NP (PRP you))
    (VP (VBP are)
      (VP (VBG putting)
        (NP
          (NP (NNS words))
          (PP (IN in)
            (NP (PRP$ my) (NN mouth))))))
    (. .)))

advmod(putting-5, Third-1)
nsubj(putting-5, you-3)
aux(putting-5, are-4)
root(ROOT-0, putting-5)
dobj(putting-5, words-6)
poss(mouth-9, my-8)
prep_in(words-6, mouth-9)

