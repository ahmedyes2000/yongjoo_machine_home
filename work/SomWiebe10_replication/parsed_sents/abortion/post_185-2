(ROOT
  (S (IN For)
    (NP (PRP it))
    (VP (VBZ is)
      (ADJP (JJ he\/she))
      (SBAR
        (WHNP (WP who))
        (S
          (VP (VBZ knows)
            (NP
              (NP (DT the) (NN truth))
              (PP (IN of)
                (NP (DT the) (NN matter))))))))
    (. .)))

dep(he\/she-4, For-1)
nsubj(he\/she-4, it-2)
cop(he\/she-4, is-3)
root(ROOT-0, he\/she-4)
nsubj(knows-6, who-5)
ccomp(he\/she-4, knows-6)
det(truth-8, the-7)
