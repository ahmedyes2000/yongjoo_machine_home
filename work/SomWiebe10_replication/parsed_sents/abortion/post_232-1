(ROOT
  (S
    (NP (PRP You))
    (VP (VBP are)
      (VP (VBG using)
        (NP
          (NP (DT a)
            (ADJP (RB pretty) (JJ sophisticated))
            (JJ philosophical) (NN argument))
          (VP
            (ADVP (RB first))
            (VBN made)
            (NP (NNS years))
            (ADVP (RB ago)
              (PP (IN in)
                (NP (NN print))))
            (, ,)
            (PP (IN by)
              (NP (DT a) (JJ female) (NN philosopher)))))))
    (. .)))

