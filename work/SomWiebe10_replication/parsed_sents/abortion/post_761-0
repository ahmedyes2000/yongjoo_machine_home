(ROOT
  (S
    (ADVP (RB Once) (RB again))
    (, ,)
    (NP (DT that))
    (VP (VBZ 's) (RB not)
      (NP (PRP$ my) (NN decision)))
    (. .)))

advmod(again-2, Once-1)
advmod(decision-8, again-2)
nsubj(decision-8, that-4)
cop(decision-8, 's-5)
neg(decision-8, not-6)
poss(decision-8, my-7)
root(ROOT-0, decision-8)

