(ROOT
  (S
    (NP (PRP You))
    (VP (VBP 're)
      (VP (VBG taking)
        (NP (DT the) (NN standpoint))
        (SBAR (IN that)
          (S
            (NP (PRP we))
            (VP (VBP create)
              (NP (DT each) (JJ other)))))))
    (. .)))

