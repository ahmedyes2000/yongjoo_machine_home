(ROOT
  (S (CC But)
    (NP (DT the) (NN unborn))
    (VP (VBP deserve)
      (S
        (NP (DT a) (NN chance))
        (VP (TO to)
          (VP (VB live)))))
    (. .)))

cc(deserve-4, But-1)
det(unborn-3, the-2)
nsubj(deserve-4, unborn-3)
root(ROOT-0, deserve-4)
det(chance-6, a-5)
nsubj(live-8, chance-6)
aux(live-8, to-7)
xcomp(deserve-4, live-8)

