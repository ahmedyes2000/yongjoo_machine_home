  (S ('' '')
    (S
      (SBAR (IN If)
        (S
          (NP (PRP you))
          (VP (VBP believe)
            (NP (DT that) (NN life)))))
      (VP (VBZ begins)
        (PP (IN at)
          (NP
            (NP (NN conception))
