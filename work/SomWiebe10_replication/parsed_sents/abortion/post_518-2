              (S
                (NP (PRP I))
                (VP (VBP do) (RB not)))
              (-RRB- -RRB-))))))
    (, ,) (RB then)
    (S
      (NP (EX there))
      (VP (MD can)
        (VP (VB be)
          (NP
            (NP (DT no) (NN justification))
            (PP (IN for)
              (NP (NN abortion))))
          (, ,)
          (SBAR (RB just) (IN as)
            (S
              (NP (EX there))
              (VP (MD can)
                (VP (VB be)
                  (NP
                    (NP (DT no) (NN justification))
                    (PP (IN for)
                      (NP (NN murder)))))))))))
    (. .)))

