(ROOT
  (S
    (SBAR
      (WHADVP (WRB When))
      (S
        (NP (PRP you))
        (VP (VBP do)
          (NP (DT these) (NNS things)))))
    (, ,)
    (NP (PRP you))
    (VP (VBP violate)
      (NP
        (NP (DT a) (NN woman) (POS 's))
        (JJ civil) (NNS rights)))
    (. .)))

advmod(do-3, When-1)
nsubj(do-3, you-2)
advcl(violate-8, do-3)
det(things-5, these-4)
dobj(do-3, things-5)
nsubj(violate-8, you-7)
root(ROOT-0, violate-8)
det(woman-10, a-9)
poss(rights-13, woman-10)
amod(rights-13, civil-12)
dobj(violate-8, rights-13)

