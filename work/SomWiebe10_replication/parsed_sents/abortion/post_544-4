(ROOT
  (S
    (NP (PRP You))
    (VP (MD might)
      (ADVP (RB as) (RB well))
      (VP (VB say)
        (SBAR (IN that)
          (S
            (NP
              (NP (DT the) (NN solution))
              (PP (IN for)
                (NP (PRP$ our) (NN health) (NN care) (NN system))))
            (VP (VBZ is)
              (PP (IN for)
                (NP (NNS people)))
              (S
                (VP (TO to)
                  (VP
                    (ADVP (RB just))
                    (VB stop)
                    (VP (VBG getting)
                      (ADJP (JJ sick)))))))))))
    (. .)))

