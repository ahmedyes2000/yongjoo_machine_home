(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is)
      (ADJP (RB as) (JJ close)
        (PP (TO to)
          (NP (JJ proven))))
      (SBAR (IN as)
        (S
          (NP (NN science))
          (VP (VBZ is)
            (ADJP (JJ able)
              (S
                (VP (TO to)
                  (VP (VB get)))))))))
    (. .)))

nsubj(close-4, It-1)
cop(close-4, is-2)
advmod(close-4, as-3)
root(ROOT-0, close-4)
prep_to(close-4, proven-6)
mark(able-10, as-7)
nsubj(able-10, science-8)
cop(able-10, is-9)
advcl(close-4, able-10)
aux(get-12, to-11)
xcomp(able-10, get-12)

