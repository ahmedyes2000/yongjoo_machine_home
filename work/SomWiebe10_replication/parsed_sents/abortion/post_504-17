(ROOT
  (S
    (VP (VB Do) (RB n't)
      (VP (VB be)
        (PP (IN like)
          (NP (DT the) (NN slave) (NNS sellers) (, ,) (NN slave) (NNS owners)
            (CC or)
            (NN slave) (NN ship) (NNS captains)))))
    (. .)))

aux(be-3, Do-1)
neg(be-3, n't-2)
root(ROOT-0, be-3)
det(owners-10, the-5)
nn(owners-10, slave-6)
nn(owners-10, sellers-7)
dep(owners-10, slave-9)
prep_like(be-3, owners-10)
nn(captains-14, slave-12)
nn(captains-14, ship-13)
prep_like(be-3, captains-14)
conj_or(owners-10, captains-14)

