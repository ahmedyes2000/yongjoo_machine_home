(ROOT
  (S
    (NP (PRP You))
    (ADVP (RB just))
    (VP (VBP make)
      (S
        (NP (PRP it))
        (ADJP (RB too) (JJ easy)
          (PP (IN for)
            (NP (PRP me))))))
    (. .)))

