(ROOT
  (S
    (S
      (S
        (NP (PRP You))
        (VP (VBP see)
          (S
            (NP (PRP them))
            (VP (VBG committing)
              (NP (DT a) (NN sin))))))
      (CC and)
      (S
        (NP (NNP loudly))
        (VP (VBP call)
          (NP (PRP them))
          (PRT (RP out)))))
    (, ,)
    (CC but)
    (S
      (NP (PRP you))
      (VP (VBP are)
        (VP (VBG being)
          (ADJP (JJ hypocritical)
            (SBAR (IN because)
              (S
                (NP (PRP you))
                (ADVP (RB also))
                (VP (VBZ sin))))))))
    (. .)))

nsubj(see-2, You-1)
root(ROOT-0, see-2)
nsubj(committing-4, them-3)
dep(see-2, committing-4)
det(sin-6, a-5)
dobj(committing-4, sin-6)
nsubj(call-9, loudly-8)
conj_and(see-2, call-9)
dobj(call-9, them-10)
prt(call-9, out-11)
nsubj(hypocritical-17, you-14)
aux(hypocritical-17, are-15)
cop(hypocritical-17, being-16)
conj_but(see-2, hypocritical-17)
mark(sin-21, because-18)
nsubj(sin-21, you-19)
advmod(sin-21, also-20)
ccomp(hypocritical-17, sin-21)

