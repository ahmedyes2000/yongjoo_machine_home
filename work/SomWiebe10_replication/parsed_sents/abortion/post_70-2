(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is) (, ,)
      (ADVP (RB therefore))
      (, ,)
      (FRAG (RB not)
        (ADVP (RB necessarily))
        (ADJP (JJR easier)
          (PP (IN on)
            (NP (DT the) (NN mother))))))
    (. .)))

nsubj(is-2, It-1)
root(ROOT-0, is-2)
advmod(is-2, therefore-4)
dep(is-2, not-6)
dep(not-6, necessarily-7)
dep(not-6, easier-8)
det(mother-11, the-10)
prep_on(easier-8, mother-11)

