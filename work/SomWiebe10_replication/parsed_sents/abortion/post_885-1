(ROOT
  (S
    (NP (DT No) (NN one))
    (VP (MD should)
      (VP (VB be)
        (VP
          (ADVP (RB willfully))
          (VBN killed)))
      (, ,)
      (VP (VB be)
        (NP (PRP it))
        (PP (IN at)
          (NP
            (NP (CD 1) (NN week))
            (CC or)
            (NP (CD 100) (NNS years))))))
    (. .)))

det(one-2, No-1)
nsubjpass(killed-6, one-2)
aux(killed-6, should-3)
auxpass(killed-6, be-4)
advmod(killed-6, willfully-5)
root(ROOT-0, killed-6)
cop(it-9, be-8)
dep(killed-6, it-9)
num(week-12, 1-11)
prep_at(it-9, week-12)
num(years-15, 100-14)
prep_at(it-9, years-15)
conj_or(week-12, years-15)

