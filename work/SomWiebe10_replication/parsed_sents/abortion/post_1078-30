(ROOT
  (S
    (PP (IN For)
      (NP (NN example)))
    (, ,)
    (S
      (NP (DT the) (NN heart))
      (VP (VBD beat)
        (NP (NNS starts))
        (ADVP (RB gradually))
        (, ,)
        (PP (IN over)
          (NP
            (NP (DT a) (NN period))
            (PP (IN of)
              (NP (NN time)))))))
    (IN so)
    (S
      (NP (NN conception))
      (VP (VBZ is)
        (NP
          (NP (DT the) (RB only) (JJ exact) (JJ starting) (NN point))
          (PP (IN of)
            (NP (NN life))))))
    (. .)))

prep_for(beat-6, example-2)
det(heart-5, the-4)
nsubj(beat-6, heart-5)
root(ROOT-0, beat-6)
dobj(beat-6, starts-7)
advmod(beat-6, gradually-8)
det(period-12, a-11)
prep_over(beat-6, period-12)
prep_of(period-12, time-14)
dep(beat-6, so-15)
nsubj(point-22, conception-16)
cop(point-22, is-17)
det(point-22, the-18)
advmod(point-22, only-19)
amod(point-22, exact-20)
amod(point-22, starting-21)
ccomp(beat-6, point-22)
prep_of(point-22, life-24)

