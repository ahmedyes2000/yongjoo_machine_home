(ROOT
  (S
    (ADVP (RB Instead))
    (NP (PRP they))
    (VP (VBP keep)
      (NP (PRP them))
      (PP (IN in)
        (NP
          (ADJP (RB extremely) (JJ dangerous)
            (CC and)
            (JJ abusive))
          (NN home) (NNS situations))))
    (. .)))

advmod(keep-3, Instead-1)
nsubj(keep-3, they-2)
root(ROOT-0, keep-3)
dobj(keep-3, them-4)
advmod(dangerous-7, extremely-6)
amod(situations-11, dangerous-7)
conj_and(dangerous-7, abusive-9)
amod(situations-11, abusive-9)
nn(situations-11, home-10)
prep_in(keep-3, situations-11)

