(ROOT
  (S
    (NP (DT That))
    (VP (VBZ is)
      (ADVP (RB basically))
      (NP
        (NP (CD one) (NN murder))
        (VP (VBG happening)
          (NP (DT every) (CD 20) (NNS seconds)))))
    (. .)))

nsubj(murder-5, That-1)
cop(murder-5, is-2)
advmod(murder-5, basically-3)
num(murder-5, one-4)
root(ROOT-0, murder-5)
partmod(murder-5, happening-6)
det(seconds-9, every-7)
num(seconds-9, 20-8)
dobj(happening-6, seconds-9)

