(ROOT
  (S
    (VP (MD Can) (RB not)
      (VP (VB be)
        (VP (VBN committed)
          (PP (IN against)
            (NP (DT a) (JJ dead) (NN person))))))
    (. .)))

aux(committed-4, Can-1)
neg(committed-4, not-2)
auxpass(committed-4, be-3)
root(ROOT-0, committed-4)
det(person-8, a-6)
amod(person-8, dead-7)
prep_against(committed-4, person-8)

