(ROOT
  (S
    (SBAR (IN Although)
      (S
        (NP (NN abortion))
        (VP (VBZ is)
          (NP
            (NP (PDT such) (DT a) (VBG sadening) (NN thing))
            (SBAR
              (WHNP (WDT that))
              (S
                (VP (VBZ hurts)
                  (S
                    (NP (PRP me))
                    (ADJP (RB i) (JJ dont))))))))))
    (VP (VBP feel)
      (SBAR (IN that)
        (S
          (NP (PRP it))
          (VP (MD should)
            (VP (VB be)
              (VP (VBN banned)))))))
    (. .)))

