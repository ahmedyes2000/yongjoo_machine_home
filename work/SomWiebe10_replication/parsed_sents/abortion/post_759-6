(ROOT
  (S
    (S
      (NP (PRP They))
      (VP (VBP 're)
        (ADVP (RB just))
        (VP (VBG hurting)
          (NP (PRP themselves)))))
    (, ,)
    (CC but)
    (S
      (S
        (NP (PRP it))
        (VP (VBZ 's) (RB not)
          (NP (PRP$ my) (NN choice))))
      (CC and)
      (S
        (NP (PRP I))
        (VP (MD wo) (RB n't)
          (VP (VB stop)
            (NP (PRP them))))))
    (. .)))

nsubj(hurting-4, They-1)
aux(hurting-4, 're-2)
advmod(hurting-4, just-3)
root(ROOT-0, hurting-4)
dobj(hurting-4, themselves-5)
nsubj(choice-12, it-8)
cop(choice-12, 's-9)
neg(choice-12, not-10)
poss(choice-12, my-11)
conj_but(hurting-4, choice-12)
nsubj(stop-17, I-14)
aux(stop-17, wo-15)
neg(stop-17, n't-16)
conj_but(hurting-4, stop-17)
conj_and(choice-12, stop-17)
dobj(stop-17, them-18)

