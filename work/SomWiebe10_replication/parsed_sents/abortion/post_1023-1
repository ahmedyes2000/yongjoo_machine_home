(ROOT
  (S
    (NP (PRP I))
    (VP (VBP 'm)
      (ADJP (RB so) (JJ glad)
        (SBAR
          (S
            (NP (PRP you))
            (VP
              (VP (VBD spoke)
                (PRT (RP up)))
              (CC and)
              (VP (MD would)
                (ADVP (RB strongly))
                (VP (VB encourage)
                  (S
                    (NP (PRP you))
                    (VP (TO to)
                      (VP (VB do)
                        (NP (PRP it))
                        (ADVP (RBR more) (RB often))))))))))))
    (. .)))

nsubj(glad-4, I-1)
cop(glad-4, 'm-2)
advmod(glad-4, so-3)
root(ROOT-0, glad-4)
nsubj(spoke-6, you-5)
nsubj(encourage-11, you-5)
dep(glad-4, spoke-6)
prt(spoke-6, up-7)
aux(encourage-11, would-9)
advmod(encourage-11, strongly-10)
dep(glad-4, encourage-11)
conj_and(spoke-6, encourage-11)
nsubj(do-14, you-12)
aux(do-14, to-13)
xcomp(encourage-11, do-14)
dobj(do-14, it-15)
advmod(often-17, more-16)
advmod(do-14, often-17)

