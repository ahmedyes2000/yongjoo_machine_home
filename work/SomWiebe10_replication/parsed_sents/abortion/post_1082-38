(ROOT
  (S
    (NP (PRP They))
    (ADVP (RB desperately))
    (VP (VBP work)
      (S
        (VP (TO to)
          (VP (VB demonize)
            (CC and)
            (VB marginalize)
            (NP (PRP us))))))
    (. .)))

nsubj(work-3, They-1)
xsubj(demonize-5, They-1)
advmod(work-3, desperately-2)
root(ROOT-0, work-3)
aux(demonize-5, to-4)
