(ROOT
  (S (CC But)
    (NP
      (NP (RB basically) (NN everything) (RB else))
      (PP (TO to)
        (NP (PRP me))))
    (VP (MD should)
      (VP (VB be)
        (VP (VBN banned))))
    (. .)))

cc(banned-9, But-1)
