(ROOT
  (UCP
    (FRAG
      (NP
        (NP (NN ie))
        (: :)
        (NP (NN rape))
        (, ,)))
    (CC or)
    (SBAR (IN if)
      (FRAG
        (NP
          (NP (DT the) (NN birth))
          (SBAR
            (S
              (NP (PRP itself))
              (VP (MD would)
                (VP (VB place)
                  (NP
                    (NP (DT the) (NN mother))
                    (PP (IN in)
                      (NP (NN danger))))))))
          (. .))))))

root(ROOT-0, ie-1)
dep(ie-1, rape-3)
mark(birth-8, if-6)
det(birth-8, the-7)
conj_or(ie-1, birth-8)
nsubj(place-11, itself-9)
aux(place-11, would-10)
rcmod(birth-8, place-11)
det(mother-13, the-12)
