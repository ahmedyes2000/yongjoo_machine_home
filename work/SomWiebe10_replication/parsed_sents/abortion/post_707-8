(ROOT
  (S
    (NP (PRP We))
    (VP (VBP 're) (RB all)
      (VP (VBG going)
        (S
          (VP (TO to)
            (VP (VB die)
              (PP (IN of)
                (NP
                  (NP (JJ old) (NN age))
                  (CC or)
                  (NP
                    (NP (DT a) (NN car) (NN wreck))
                    (CC or)
                    (NP (DT a) (NN miscarriage))))))))))
    (. .)))

nsubj(going-4, We-1)
xsubj(die-6, We-1)
aux(going-4, 're-2)
advmod(going-4, all-3)
root(ROOT-0, going-4)
aux(die-6, to-5)
xcomp(going-4, die-6)
amod(age-9, old-8)
prep_of(die-6, age-9)
det(wreck-13, a-11)
nn(wreck-13, car-12)
prep_of(die-6, wreck-13)
conj_or(age-9, wreck-13)
det(miscarriage-16, a-15)
conj_or(age-9, miscarriage-16)
conj_or(wreck-13, miscarriage-16)

