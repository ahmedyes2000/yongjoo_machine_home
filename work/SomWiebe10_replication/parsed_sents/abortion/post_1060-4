(ROOT
  (S
    (ADVP (RB Again))
    (VP (VB correct)
      (NP (PRP me))
      (SBAR (IN if)
        (S
          (NP (PRP I))
          (VP (VBP am)
            (ADJP (JJ wrong))))))
    (. .)))

advmod(correct-2, Again-1)
root(ROOT-0, correct-2)
dobj(correct-2, me-3)
mark(wrong-7, if-4)
nsubj(wrong-7, I-5)
cop(wrong-7, am-6)
advcl(correct-2, wrong-7)

