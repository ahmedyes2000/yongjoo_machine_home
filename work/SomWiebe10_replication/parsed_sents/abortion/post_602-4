(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP (VBP look)
          (PP (IN in)
            (NP (DT any) (NN dictionary))))))
    (NP
      (NP (DT the) (NN definition))
      (PP (IN of)
        (NP (NN personhood))))
    (VP
      (VP (VBZ is))
      (: :)
      (NP
        (NP (DT a) (NN human))
        (VP (VBG being))))
    (. .)))

mark(look-3, If-1)
nsubj(look-3, you-2)
advcl(is-11, look-3)
det(dictionary-6, any-5)
prep_in(look-3, dictionary-6)
det(definition-8, the-7)
nsubj(is-11, definition-8)
prep_of(definition-8, personhood-10)
root(ROOT-0, is-11)
det(human-14, a-13)
dobj(is-11, human-14)
partmod(human-14, being-15)

