(ROOT
  (S
    (NP (EX There))
    (VP (VBP are)
      (NP
        (NP (JJ certain) (JJ exceptional) (NNS cases))
        (SBAR
          (WHNP (WDT which))
          (S
            (VP (VBP have)
              (VP (VBN been)
                (VP (VBN cited)
                  (S
                    (ADJP (RB as) (RB circumstantially) (JJ appropriate)
                      (PP (IN for)
                        (NP (DT a) (JJ partial) (NN birth) (NN abortion))))))))))))
    (. .)))

expl(are-2, There-1)
root(ROOT-0, are-2)
amod(cases-5, certain-3)
amod(cases-5, exceptional-4)
nsubj(are-2, cases-5)
nsubjpass(cited-9, cases-5)
aux(cited-9, have-7)
auxpass(cited-9, been-8)
rcmod(cases-5, cited-9)
advmod(appropriate-12, as-10)
advmod(appropriate-12, circumstantially-11)
acomp(cited-9, appropriate-12)
det(abortion-17, a-14)
amod(abortion-17, partial-15)
nn(abortion-17, birth-16)
prep_for(appropriate-12, abortion-17)

