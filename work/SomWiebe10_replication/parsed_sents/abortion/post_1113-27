(ROOT
  (S
    (ADVP (RB Alas))
    (, ,)
    (NP (PRP I))
    (VP (VBP think)
      (SBAR
        (S
          (NP (PRP I))
          (VP (VBP 'm)
            (VP
              (NP (DT a) (JJ little))
              (VBN spent)
              (PP (IN on)
                (NP (DT this) (NN topic))))))))
    (. .)))

advmod(think-4, Alas-1)
nsubj(think-4, I-3)
root(ROOT-0, think-4)
nsubjpass(spent-9, I-5)
auxpass(spent-9, 'm-6)
det(little-8, a-7)
dobj(spent-9, little-8)
ccomp(think-4, spent-9)
det(topic-12, this-11)
prep_on(spent-9, topic-12)

