(ROOT
  (S (CC But)
    (NP (DT the) (RBS most) (JJ heinous) (NN crime))
    (VP (VBZ is)
      (SBAR
        (WHADVP (WRB when))
        (S
          (NP (PRP we))
          (VP (VBP make)
            (S
              (NP (NNS laws))
              (VP (TO to)
                (VP (VB exonerate)
                  (NP (PRP ourselves)))))))))
    (. .)))

cc(is-6, But-1)
det(crime-5, the-2)
advmod(crime-5, most-3)
amod(crime-5, heinous-4)
nsubj(is-6, crime-5)
root(ROOT-0, is-6)
advmod(make-9, when-7)
nsubj(make-9, we-8)
advcl(is-6, make-9)
nsubj(exonerate-12, laws-10)
aux(exonerate-12, to-11)
xcomp(make-9, exonerate-12)
dobj(exonerate-12, ourselves-13)

