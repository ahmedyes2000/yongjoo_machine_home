(ROOT
  (S
    (NP (PRP We))
    (VP (VBP provide)
      (NP (NNS funds))
      (PP (IN as)
        (NP
          (NP (DT an) (NN act))
          (PP (IN of)
            (NP (NN charity))))))
    (. .)))

nsubj(provide-2, We-1)
root(ROOT-0, provide-2)
dobj(provide-2, funds-3)
det(act-6, an-5)
prep_as(provide-2, act-6)
prep_of(act-6, charity-8)

