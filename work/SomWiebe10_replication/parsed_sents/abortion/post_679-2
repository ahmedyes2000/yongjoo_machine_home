  (S
    (NP
      (NP (DT The) (NN issue))
      (PP (IN of)
        (S
          (VP (VBG aborting)
            (NP (DT a) (NN pregnancy))))))
    (VP (VBZ involves)
      (NP
        (NP (JJ painful) (NNS issues))
        (PP (IN of)
          (NP (JJ moral) (NN uncertainty)))))
    (. .)))

