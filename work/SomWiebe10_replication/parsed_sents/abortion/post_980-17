(ROOT
  (S
    (S
      (NP (PRP I))
      (VP (VBP have)
        (VP (VBD said)
          (NP (DT this))
          (PP (IN throughout)
            (NP (PRP$ my) (JJ past) (NNS statements))))))
    (, ,)
    (CC yet)
    (S
      (NP (PRP you))
      (VP (VBP want)
        (S
          (VP (TO to)
            (VP (VB keep)
              (S
                (VP (VBG diverting)
                  (NP (DT the) (NN topic))
                  (PP (TO to)
                    (NP (NN something)))
                  (SBAR
                    (S
                      (NP (PRP you))
                      (VP (VBP feel)
                        (ADJP (JJ comfortable)
                          (PP (IN with)))))))))))))
    (. .)))

nsubj(said-3, I-1)
aux(said-3, have-2)
root(ROOT-0, said-3)
dobj(said-3, this-4)
poss(statements-8, my-6)
amod(statements-8, past-7)
prep_throughout(said-3, statements-8)
nsubj(want-12, you-11)
xsubj(keep-14, you-11)
conj_yet(said-3, want-12)
aux(keep-14, to-13)
xcomp(want-12, keep-14)
xcomp(keep-14, diverting-15)
det(topic-17, the-16)
dobj(diverting-15, topic-17)
prep_to(diverting-15, something-19)
nsubj(feel-21, you-20)
dep(diverting-15, feel-21)
acomp(feel-21, comfortable-22)
prep(comfortable-22, with-23)

