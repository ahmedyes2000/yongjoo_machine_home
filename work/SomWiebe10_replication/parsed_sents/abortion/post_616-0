(ROOT
  (S
    (NP (PRP You))
    (VP (VBP make)
      (NP
        (NP (JJ many) (JJ definitive) (NNS statements))
        (PP (IN about)
          (SBAR
            (WHNP (WP what))
            (S
              (VP (VBZ is)
                (ADJP (JJ true))))))))
    (. .)))

nsubj(make-2, You-1)
root(ROOT-0, make-2)
amod(statements-5, many-3)
amod(statements-5, definitive-4)
dobj(make-2, statements-5)
nsubj(true-9, what-7)
cop(true-9, is-8)
prepc_about(statements-5, true-9)

