(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is)
      (ADJP (NN contradictory)
        (S
          (VP (TO to)
            (VP (VB oppose)
              (NP (NN abortion))
              (PP (IN while)
                (S
                  (VP (VBG supporting)
                    (NP (NN capital) (NN punishment))))))))))
    (. .)))

nsubj(contradictory-3, It-1)
cop(contradictory-3, is-2)
root(ROOT-0, contradictory-3)
aux(oppose-5, to-4)
xcomp(contradictory-3, oppose-5)
dobj(oppose-5, abortion-6)
prepc_while(oppose-5, supporting-8)
nn(punishment-10, capital-9)
dobj(supporting-8, punishment-10)

