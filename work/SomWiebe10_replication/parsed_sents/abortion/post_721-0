(ROOT
  (S
    (ADVP (RB sigh))
    (NP (PRP I))
    (VP (VBP want)
      (S
        (VP (TO to)
          (VP (VB say)
            (INTJ (UH yes))
            (PP (IN for)
              (NP (DT the) (JJS most) (NN part)))))))
    (. .)))

advmod(want-3, sigh-1)
nsubj(want-3, I-2)
xsubj(say-5, I-2)
root(ROOT-0, want-3)
aux(say-5, to-4)
xcomp(want-3, say-5)
dep(say-5, yes-6)
det(part-10, the-8)
amod(part-10, most-9)
prep_for(say-5, part-10)

