(ROOT
  (S
    (NP (NNP America))
    (VP (VBD was)
      (VP (VBN founded)
        (PP (IN upon)
          (NP
            (NP (DT the) (NNS principles))
            (PP (IN of)
              (NP
                (NP (NN freedom))
                (PP (IN of)
                  (NP (NN religion)))))))))
    (. .)))

nsubjpass(founded-3, America-1)
auxpass(founded-3, was-2)
root(ROOT-0, founded-3)
det(principles-6, the-5)
prep_upon(founded-3, principles-6)
prep_of(principles-6, freedom-8)
prep_of(freedom-8, religion-10)

