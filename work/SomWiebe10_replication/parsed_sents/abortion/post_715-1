(ROOT
  (S
    (S
      (NP (NNP Emotion))
      (VP (MD may)
        (VP (VB inform)
          (NP (PRP$ our) (NN thinking)))))
    (, ,)
    (CC but)
    (S
      (ADVP (RB ultimately))
      (NP (DT any) (JJ real) (NN discussion))
      (VP (MD must)
        (VP (VB come)
          (PRT (RP down))
          (PP (TO to)
            (NP (NN logic))))))
    (. .)))

nsubj(inform-3, Emotion-1)
aux(inform-3, may-2)
root(ROOT-0, inform-3)
poss(thinking-5, our-4)
dobj(inform-3, thinking-5)
advmod(come-13, ultimately-8)
det(discussion-11, any-9)
amod(discussion-11, real-10)
nsubj(come-13, discussion-11)
aux(come-13, must-12)
conj_but(inform-3, come-13)
prt(come-13, down-14)
prep_to(come-13, logic-16)

