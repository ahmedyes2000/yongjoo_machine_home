(ROOT
  (S
    (NP (PRP We))
    (VP (VBP are)
      (ADJP (RB sufficiently) (JJ advanced)
        (PP (IN in)
          (NP (PRP$ our) (NN technology)
            (CC and)
            (NN knowledge))))
      (PP (JJ such))
      (SBAR (IN that)
        (S
          (NP (PRP it))
          (VP (VBZ is) (RB not)
            (ADJP (JJ necessary)
              (PP (IN for)
                (NP (NN survival))))))))
    (. .)))

nsubj(advanced-4, We-1)
cop(advanced-4, are-2)
advmod(advanced-4, sufficiently-3)
root(ROOT-0, advanced-4)
poss(technology-7, our-6)
prep_in(advanced-4, technology-7)
prep_in(advanced-4, knowledge-9)
conj_and(technology-7, knowledge-9)
prep(advanced-4, such-10)
complm(necessary-15, that-11)
nsubj(necessary-15, it-12)
cop(necessary-15, is-13)
neg(necessary-15, not-14)
ccomp(advanced-4, necessary-15)
prep_for(necessary-15, survival-17)

