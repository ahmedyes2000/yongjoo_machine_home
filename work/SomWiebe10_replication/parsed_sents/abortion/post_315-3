(ROOT
  (S
    (NP (DT The) (NN issue))
    (VP (VBZ has)
      (S
        (VP (TO to)
          (VP (VB do)
            (PP (IN with)
              (NP
                (NP (NN right))
                (PP (IN of)
                  (NP (DT the) (NN individual))))))))
      (S
        (VP (TO to)
          (VP (VB retain)
            (NP (DT the) (NN right))
            (PP (TO to)
              (NP (PRP$ her) (NN body)))))))
    (. .)))

det(issue-2, The-1)
nsubj(has-3, issue-2)
xsubj(do-5, issue-2)
xsubj(retain-12, issue-2)
