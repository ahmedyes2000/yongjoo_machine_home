(ROOT
  (S
    (NP (PRP You))
    (VP (VBP are)
      (VP (VBG arguing)
        (PP (IN for)
          (NP (DT the) (NN right)))
        (S
          (VP (TO to)
            (VP (VB terminate)
              (NP
                (NP (NN someone) (RB else) (POS 's))
                (NN life)))))))
    (. .)))

nsubj(arguing-3, You-1)
xsubj(terminate-8, You-1)
aux(arguing-3, are-2)
root(ROOT-0, arguing-3)
det(right-6, the-5)
prep_for(arguing-3, right-6)
aux(terminate-8, to-7)
xcomp(arguing-3, terminate-8)
poss(life-12, someone-9)
advmod(someone-9, else-10)
dobj(terminate-8, life-12)

