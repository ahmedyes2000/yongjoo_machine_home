(ROOT
  (S
    (ADVP (RB ''))
    (: ...)
    (NP (EX there))
    (VP (VBP are)
      (NP
        (NP (DT some)
          (ADJP (RB very) (JJ valid))
          (NNS reasons))
        (PP (IN for)
          (NP (DT a) (NN woman)
            (S
              (VP (TO to)
                (VP (VB have)
                  (NP (CD one)))))))))
    (. .)))

expl(are-4, there-3)
root(ROOT-0, are-4)
det(reasons-8, some-5)
advmod(valid-7, very-6)
amod(reasons-8, valid-7)
nsubj(are-4, reasons-8)
det(woman-11, a-10)
prep_for(reasons-8, woman-11)
aux(have-13, to-12)
infmod(woman-11, have-13)
dobj(have-13, one-14)

