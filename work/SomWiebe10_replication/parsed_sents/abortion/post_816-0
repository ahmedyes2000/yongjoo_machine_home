(ROOT
  (S
    (NP (PRP You))
    (VP (MD can) (RB not)
      (VP (VB get)
        (VP (VBN fired)
          (PP (IN for)
            (S
              (VP (VBG being)
                (ADJP (JJ pregnant))))))))
    (. .)))

nsubjpass(fired-5, You-1)
aux(fired-5, can-2)
neg(fired-5, not-3)
auxpass(fired-5, get-4)
root(ROOT-0, fired-5)
cop(pregnant-8, being-7)
prepc_for(fired-5, pregnant-8)

