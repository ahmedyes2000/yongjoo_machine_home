(ROOT
  (S
    (NP (PRP You))
    (VP (VBP are)
      (ADJP (RB absolutely) (JJ correct)
        (SBAR (IN that)
          (S
            (S
              (VP (VBG making)
                (S
                  (NP (NN abortion))
                  (ADJP (JJ illegal)))))
            (VP (VBZ is) (RB not)
              (NP (DT the) (JJ only) (NN option)))))))
    (. .)))

nsubj(correct-4, You-1)
cop(correct-4, are-2)
advmod(correct-4, absolutely-3)
root(ROOT-0, correct-4)
complm(option-13, that-5)
csubj(option-13, making-6)
nsubj(illegal-8, abortion-7)
xcomp(making-6, illegal-8)
cop(option-13, is-9)
neg(option-13, not-10)
det(option-13, the-11)
amod(option-13, only-12)
ccomp(correct-4, option-13)

