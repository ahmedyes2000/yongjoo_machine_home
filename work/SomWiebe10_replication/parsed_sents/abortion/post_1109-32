(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (NN perfection))
        (VP (VBZ is)
          (NP (PRP$ your) (NN standard))
          (ADVP (RB then)))))
    (NP (RB yes) (NN prohibition))
    (VP (VBZ does) (RB n't)
      (VP (VB work)))
    (. .)))

mark(standard-5, If-1)
nsubj(standard-5, perfection-2)
cop(standard-5, is-3)
poss(standard-5, your-4)
advcl(work-11, standard-5)
advmod(standard-5, then-6)
advmod(prohibition-8, yes-7)
nsubj(work-11, prohibition-8)
aux(work-11, does-9)
neg(work-11, n't-10)
root(ROOT-0, work-11)

