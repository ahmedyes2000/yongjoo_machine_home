(ROOT
  (S
    (SBAR (IN Since)
      (S
        (NP (PRP they))
        (VP (VB happen)
          (NP (PDT all) (DT the) (NN time)))))
    (NP (PRP we))
    (VP (VBP need) (RB not)
      (VP (VB make)
        (NP (DT a) (JJ special) (NN case))))
    (. .)))

mark(happen-3, Since-1)
nsubj(happen-3, they-2)
advcl(need-8, happen-3)
predet(time-6, all-4)
det(time-6, the-5)
dobj(happen-3, time-6)
nsubj(need-8, we-7)
root(ROOT-0, need-8)
neg(need-8, not-9)
dep(need-8, make-10)
det(case-13, a-11)
amod(case-13, special-12)
dobj(make-10, case-13)

