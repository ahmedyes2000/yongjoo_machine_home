(ROOT
  (S
    (NP (DT No) (NN reason))
    (VP (MD can)
      (VP (VB be)
        (VP (VBN put)
          (ADVP (RB forth))
          (S
            (VP (TO to)
              (VP (VB justify)
                (NP (NN murder))
                (PP (IN except)
                  (NP (NN self) (NN defense)))))))))
    (. .)))

det(reason-2, No-1)
nsubjpass(put-5, reason-2)
aux(put-5, can-3)
auxpass(put-5, be-4)
root(ROOT-0, put-5)
advmod(put-5, forth-6)
aux(justify-8, to-7)
xcomp(put-5, justify-8)
dobj(justify-8, murder-9)
nn(defense-12, self-11)
prep_except(justify-8, defense-12)

