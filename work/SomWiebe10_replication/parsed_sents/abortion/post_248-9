(ROOT
  (S
    (S
      (NP (JJ i) (NN dont))
      (VP (VBP think)
        (SBAR
          (S
            (NP (JJ pro) (NN choice))
            (VP (VBZ is)
              (VP (VBG PROMTING)
                (NP (NN abortion))))))))
    (: ...)
    (CC but)
    (S
      (NP (PRP it))
      (VP (VBZ IS)
        (VP (VBG promoting)
          (NP (DT the) (NN right)
            (S
              (VP (TO to)
                (VP (VB choose))))))))
    (. .)))

amod(dont-2, i-1)
nsubj(think-3, dont-2)
root(ROOT-0, think-3)
amod(choice-5, pro-4)
nsubj(PROMTING-7, choice-5)
aux(PROMTING-7, is-6)
ccomp(think-3, PROMTING-7)
dobj(PROMTING-7, abortion-8)
nsubj(promoting-13, it-11)
aux(promoting-13, IS-12)
conj_but(think-3, promoting-13)
det(right-15, the-14)
dobj(promoting-13, right-15)
aux(choose-17, to-16)
infmod(right-15, choose-17)

