(ROOT
  (S
    (NP (DT A) (NN woman))
    (VP (MD should)
      (VP (VB have)
        (NP (DT the) (NN right)
          (S
            (VP (TO to)
              (VP (VB choose)))))))
    (. .)))

det(woman-2, A-1)
nsubj(have-4, woman-2)
aux(have-4, should-3)
root(ROOT-0, have-4)
det(right-6, the-5)
dobj(have-4, right-6)
aux(choose-8, to-7)
infmod(right-6, choose-8)

(ROOT
  (NP (FW Period) (. .)))

root(ROOT-0, Period-1)

