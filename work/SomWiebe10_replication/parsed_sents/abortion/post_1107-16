(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is)
      (NP (DT a)
        (ADJP (JJ unique)
          (CC and)
          (JJ separate))
        (JJ human) (NN being))
      (SBAR (IN that)
        (S
          (NP (PRP you))
          (VP (VBP happen)
            (S
              (VP (TO to)
                (VP (VB be)
                  (VP (VBG hosting)))))))))
    (. .)))

nsubj(being-8, It-1)
cop(being-8, is-2)
det(being-8, a-3)
amod(being-8, unique-4)
conj_and(unique-4, separate-6)
amod(being-8, separate-6)
amod(being-8, human-7)
root(ROOT-0, being-8)
complm(happen-11, that-9)
nsubj(happen-11, you-10)
xsubj(hosting-14, you-10)
ccomp(being-8, happen-11)
aux(hosting-14, to-12)
aux(hosting-14, be-13)
xcomp(happen-11, hosting-14)

