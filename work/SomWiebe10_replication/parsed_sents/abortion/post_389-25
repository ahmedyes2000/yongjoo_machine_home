(ROOT
  (S
    (NP (DT That) (JJ specific) (NN kind))
    (VP (VBZ is)
      (VP (VBN determined)
        (PP (IN by)
          (NP (PRP$ its) (NN DNA)))))
    (. .)))

det(kind-3, That-1)
amod(kind-3, specific-2)
nsubjpass(determined-5, kind-3)
auxpass(determined-5, is-4)
root(ROOT-0, determined-5)
poss(DNA-8, its-7)
agent(determined-5, DNA-8)

