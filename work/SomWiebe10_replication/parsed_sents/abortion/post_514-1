(ROOT
  (S
    (S
      (NP (PRP They))
      (VP (VBP do) (RB n't)
        (VP (VB do)
          (NP (NN research)))))
    (: ;)
    (S
      (NP (PRP they))
      (VP (VBP preach)
        (SBAR
          (S
            (VP (VBZ lies))))))
    (. .)))

nsubj(do-4, They-1)
aux(do-4, do-2)
neg(do-4, n't-3)
root(ROOT-0, do-4)
dobj(do-4, research-5)
nsubj(preach-8, they-7)
parataxis(do-4, preach-8)
ccomp(preach-8, lies-9)

