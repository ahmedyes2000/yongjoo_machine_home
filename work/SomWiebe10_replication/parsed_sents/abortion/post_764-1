(ROOT
  (S
    (VP (VB Keep)
      (NP (PRP$ your) (JJ religious) (NNS opinions))
      (PP (TO to)
        (NP (PRP yourself)))
      (, ,)
      (NP
        (NP (RB especially) (VBG concerning) (NNS decisions))
        (SBAR
          (WHNP (WDT that))
          (S
            (VP (VBP do) (RB n't)
              (VP (VB effect)
                (NP (PRP you))))))))
    (. .)))

root(ROOT-0, Keep-1)
poss(opinions-4, your-2)
amod(opinions-4, religious-3)
dobj(Keep-1, opinions-4)
prep_to(Keep-1, yourself-6)
advmod(decisions-10, especially-8)
amod(decisions-10, concerning-9)
dobj(Keep-1, decisions-10)
nsubj(effect-14, decisions-10)
aux(effect-14, do-12)
neg(effect-14, n't-13)
rcmod(decisions-10, effect-14)
dobj(effect-14, you-15)

