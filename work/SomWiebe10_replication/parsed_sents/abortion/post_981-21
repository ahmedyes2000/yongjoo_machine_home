(ROOT
  (S
    (NP (DT that))
    (VP (VBZ is)
      (NP
        (NP (DT the) (JJ polar) (NN opposite))
        (PP (IN of)
          (SBAR
            (WHNP (WP what))
            (S
              (NP (PRP you))
              (VP (VBP say)))))))
    (. .)))

nsubj(opposite-5, that-1)
cop(opposite-5, is-2)
det(opposite-5, the-3)
amod(opposite-5, polar-4)
root(ROOT-0, opposite-5)
dobj(say-9, what-7)
nsubj(say-9, you-8)
prepc_of(opposite-5, say-9)

