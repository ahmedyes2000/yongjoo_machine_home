(ROOT
  (S (CC But)
    (ADVP (RB so))
    (ADVP (RB also))
    (VP (VBZ is)
      (NP
        (NP (DT an) (JJ unfertilized) (NN egg))
        (CC or)
        (NP (DT a) (NN sperm))))
    (. .)))

advmod(egg-7, so-2)
advmod(egg-7, also-3)
cop(egg-7, is-4)
det(egg-7, an-5)
amod(egg-7, unfertilized-6)
root(ROOT-0, egg-7)
det(sperm-10, a-9)
conj_or(egg-7, sperm-10)

