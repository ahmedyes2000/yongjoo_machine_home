(ROOT
  (S
    (ADVP (RB Therefore))
    (NP (EX there))
    (VP (VBZ is)
      (NP
        (NP (DT a) (NN way))
        (SBAR
          (WHPP (IN in)
            (WHNP (WDT which)))
          (S
            (NP (NN mankind))
            (VP (MD ought)
              (S
                (VP (TO to)
                  (VP (VB function)))))))))
    (. .)))

advmod(is-3, Therefore-1)
expl(is-3, there-2)
root(ROOT-0, is-3)
det(way-5, a-4)
nsubj(is-3, way-5)
rel(ought-9, which-7)
nsubj(ought-9, mankind-8)
xsubj(function-11, mankind-8)
rcmod(way-5, ought-9)
aux(function-11, to-10)
xcomp(ought-9, function-11)

