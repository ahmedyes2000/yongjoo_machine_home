(ROOT
  (S (CC And)
    (PP
      (ADVP (RB just))
      (IN for)
      (NP (DT the) (NN record)))
    (, ,)
    (NP (JJS most) (NNS abortions))
    (VP (VBP are)
      (VP (VBN performed)
        (PP (IN at)
          (NP
            (QP (RB about) (CD 10))
            (NNS weeks)))))
    (. .)))

cc(performed-10, And-1)
advmod(performed-10, just-2)
det(record-5, the-4)
prep_for(performed-10, record-5)
amod(abortions-8, most-7)
nsubjpass(performed-10, abortions-8)
auxpass(performed-10, are-9)
root(ROOT-0, performed-10)
quantmod(10-13, about-12)
num(weeks-14, 10-13)
prep_at(performed-10, weeks-14)

