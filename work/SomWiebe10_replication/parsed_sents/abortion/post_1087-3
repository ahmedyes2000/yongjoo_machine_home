(ROOT
  (S
    (S
      (NP (PRP You))
      (VP (VBP have)
        (NP (PRP$ your) (JJ own) (NNS choices))))
    (, ,)
    (NP (NN abortion))
    (VP (MD can)
      (VP (VB be)
        (NP
          (NP (CD one))
          (PP (IN of)
            (NP (PRP them))))))
    (. .)))

nsubj(have-2, You-1)
ccomp(one-10, have-2)
poss(choices-5, your-3)
amod(choices-5, own-4)
dobj(have-2, choices-5)
nsubj(one-10, abortion-7)
aux(one-10, can-8)
cop(one-10, be-9)
root(ROOT-0, one-10)
prep_of(one-10, them-12)

