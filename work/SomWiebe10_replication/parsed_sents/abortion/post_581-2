(ROOT
  (S
    (NP (DT The) (JJ unique) (JJ genetic) (NNS events))
    (VP (VBP happen)
      (ADVP (RBR earlier))
      (, ,)
      (SBAR
        (WHADVP (WRB when))
        (S
          (NP (DT the) (NNS gametes))
          (VP (VBP form)))))
    (. .)))

det(events-4, The-1)
amod(events-4, unique-2)
amod(events-4, genetic-3)
nsubj(happen-5, events-4)
root(ROOT-0, happen-5)
advmod(happen-5, earlier-6)
advmod(form-11, when-8)
det(gametes-10, the-9)
nsubj(form-11, gametes-10)
advcl(happen-5, form-11)

