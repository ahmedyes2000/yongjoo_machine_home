(ROOT
  (S
    (ADVP (RB clearly))
    (NP
      (NP (PRP$ your) (NN statement))
      (PP (IN about)
        (NP (NN quickening))))
    (VP (VBZ reveals)
      (SBAR (IN that)
        (S
          (NP (DT the) (NNP Church))
          (VP (VBD saw)
            (NP (NN life))
            (PP (IN before)
              (S
                (VP (VBG quickening)
                  (PP (IN as)
                    (NP
                      (QP (JJR less) (IN than)
                        (NP (NN life)))))
                  (PP (IN after)
                    (S
                      (VP (VBG quickening)))))))))))
    (. !)))

advmod(reveals-6, clearly-1)
poss(statement-3, your-2)
nsubj(reveals-6, statement-3)
prep_about(statement-3, quickening-5)
root(ROOT-0, reveals-6)
complm(saw-10, that-7)
det(Church-9, the-8)
nsubj(saw-10, Church-9)
ccomp(reveals-6, saw-10)
dobj(saw-10, life-11)
prepc_before(saw-10, quickening-13)
prep(quickening-13, as-14)
quantmod(than-16, less-15)
pobj(as-14, than-16)
dep(than-16, life-17)
prepc_after(quickening-13, quickening-19)

