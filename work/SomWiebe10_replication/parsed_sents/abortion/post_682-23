  (S
    (S
      (NP (DT This))
      (VP (VBZ illustrates)
        (NP
          (NP (DT the) (NN nature))
          (PP (IN of)
            (NP (DT a) (NN cycle))))))
    (, ,)
    (NP (EX there))
    (VP (VBZ is)
      (ADVP (RB always))
      (NP
        (NP (NN something))
        (ADVP (IN before))))
    (. .)))

