(ROOT
  (S
    (NP
      (NP (DT That))
      (NP (DT the) (NNP Bible)))
    (VP (VBZ says)
      (SBAR (IN that)
        (S
          (NP (NN life))
          (VP (VBZ begins)
            (PP (IN upon)
              (NP (JJ first) (NN breath)))))))
    (. .)))

nsubj(says-4, That-1)
det(Bible-3, the-2)
dep(That-1, Bible-3)
root(ROOT-0, says-4)
complm(begins-7, that-5)
nsubj(begins-7, life-6)
ccomp(says-4, begins-7)
amod(breath-10, first-9)
prep_upon(begins-7, breath-10)

