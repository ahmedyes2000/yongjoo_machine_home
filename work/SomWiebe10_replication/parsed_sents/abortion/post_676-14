(ROOT
  (S
    (S
      (NP (PRP They))
      (VP (MD can)
        (VP (VB theoretically))))
    (, ,)
    (NP (VBN given) (NN today))
    (VP (VBZ 's)
      (VP (VBG developing)
        (S
          (VP (VBG cloning)
            (S
              (NP (NN technology))
              (VP (VB generate)
                (NP (JJ new) (NN life))))))))
    (. .)))

nsubj(theoretically-3, They-1)
aux(theoretically-3, can-2)
ccomp(developing-8, theoretically-3)
amod(today-6, given-5)
nsubj(developing-8, today-6)
aux(developing-8, 's-7)
root(ROOT-0, developing-8)
xcomp(developing-8, cloning-9)
nsubj(generate-11, technology-10)
ccomp(cloning-9, generate-11)
amod(life-13, new-12)
dobj(generate-11, life-13)

