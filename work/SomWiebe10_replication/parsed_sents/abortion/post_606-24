(ROOT
  (S
    (NP (DT No) (JJ severe) (NN damage))
    (VP (VBZ is)
      (ADJP
        (ADJP (JJR worse))
        (PP (IN than)
          (NP (NN death)))))
    (. .)))

det(damage-3, No-1)
amod(damage-3, severe-2)
nsubj(worse-5, damage-3)
cop(worse-5, is-4)
root(ROOT-0, worse-5)
prep_than(worse-5, death-7)

