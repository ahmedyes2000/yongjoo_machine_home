(ROOT
  (S (RB So)
    (PP (IN in)
      (NP (NN reality)))
    (NP (PRP$ your) (JJ central) (NN theme))
    (VP (VBZ seems)
      (S
        (VP (TO to)
          (VP (VB be)
            (NP (NN sex))))))
    (. .)))

advmod(seems-7, So-1)
prep_in(seems-7, reality-3)
poss(theme-6, your-4)
amod(theme-6, central-5)
nsubj(seems-7, theme-6)
xsubj(sex-10, theme-6)
root(ROOT-0, seems-7)
aux(sex-10, to-8)
cop(sex-10, be-9)
xcomp(seems-7, sex-10)

