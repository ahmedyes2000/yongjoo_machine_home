(ROOT
  (S
    (NP
      (NP (DT That))
      (NP (DT the) (NNP Bible)))
    (VP (VBZ does) (RB n't)
      (VP (VB mention)
        (NP (NN abortion))))
    (. .)))

nsubj(mention-6, That-1)
det(Bible-3, the-2)
dep(That-1, Bible-3)
aux(mention-6, does-4)
neg(mention-6, n't-5)
root(ROOT-0, mention-6)
dobj(mention-6, abortion-7)

