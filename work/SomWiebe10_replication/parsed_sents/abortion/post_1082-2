(ROOT
  (S
    (ADVP (RB Perhaps))
    (NP (PRP you))
    (VP (VBP have)
      (VP (VBN noticed)
        (NP (DT the) (JJ pro-life) (NN sign))
        (PP (IN in)
          (NP
            (NP (NN front))
            (PP (IN of)
              (NP (PRP$ our) (NN church)))))))
    (. .)))

advmod(noticed-4, Perhaps-1)
nsubj(noticed-4, you-2)
aux(noticed-4, have-3)
root(ROOT-0, noticed-4)
det(sign-7, the-5)
amod(sign-7, pro-life-6)
dobj(noticed-4, sign-7)
poss(church-12, our-11)
prep_in_front_of(noticed-4, church-12)

