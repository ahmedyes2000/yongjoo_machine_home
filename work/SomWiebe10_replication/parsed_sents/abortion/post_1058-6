(ROOT
  (S
    (PP (IN In)
      (S
        (VP (VBG regards)
          (NP
            (NP (DT the) (NN taking))
            (PP (IN of)
              (NP (NN life)))))))
    (: ;)
    (NP (PRP I))
    (VP (VBP agree)
      (UCP
        (PP (IN with)
          (NP (PRP you)))
        (, ,)
        (CC but)
        (SBAR
          (WHADVP (RB not) (WRB when))
          (S
            (NP (PRP it))
            (VP (VBZ comes)
              (PP (TO to)
                (NP (NN abortion))))))))
    (. .)))

prepc_in(agree-9, regards-2)
det(taking-4, the-3)
dobj(regards-2, taking-4)
prep_of(taking-4, life-6)
nsubj(agree-9, I-8)
root(ROOT-0, agree-9)
dep(agree-9, with-10)
pobj(with-10, you-11)
dep(when-15, not-14)
advmod(comes-17, when-15)
nsubj(comes-17, it-16)
dep(agree-9, comes-17)
conj_but(with-10, comes-17)
prep_to(comes-17, abortion-19)

