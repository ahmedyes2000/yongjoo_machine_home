(ROOT
  (S
    (ADVP (RB First) (RB off))
    (, ,)
    (NP (PRP you))
    (VP (MD 'll)
      (ADVP (RB never))
      (VP (VB stop)
        (NP
          (NP (NNS abortions))
          (, ,)
          (ADJP (JJ legal)
            (CC or)
            (RB not)))))
    (. .)))

advmod(off-2, First-1)
advmod(stop-7, off-2)
nsubj(stop-7, you-4)
aux(stop-7, 'll-5)
neg(stop-7, never-6)
root(ROOT-0, stop-7)
dobj(stop-7, abortions-8)
amod(abortions-8, legal-10)
amod(abortions-8, not-12)
conj_or(legal-10, not-12)
neg(legal-10, not-12)

