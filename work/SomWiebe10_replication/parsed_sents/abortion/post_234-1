(ROOT
  (S
    (NP (PRP He))
    (VP
      (VP (VBZ 's)
        (VP (VBN retired)
          (PP (IN from)
            (NP (NNP URI)))))
      (CC and)
      (VP (VBZ is)
        (VP (VBG working)
          (PP (IN at)
            (NP (DT another) (JJ educational) (NN institution))))))
    (. .)))

nsubjpass(retired-3, He-1)
nsubjpass(working-8, He-1)
auxpass(retired-3, 's-2)
root(ROOT-0, retired-3)
prep_from(retired-3, URI-5)
aux(working-8, is-7)
conj_and(retired-3, working-8)
det(institution-12, another-10)
amod(institution-12, educational-11)
prep_at(working-8, institution-12)

