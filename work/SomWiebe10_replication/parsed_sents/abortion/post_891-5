(ROOT
  (FRAG
    (ADVP (RB Only))
    (SBAR (IN if)
      (S
        (NP (PRP she))
        (VP (VBD dropped)
          (NP (DT the) (NN pill))
          (PP (IN into)
            (NP (DT the) (NN drink))))))
    (. .)))

root(ROOT-0, Only-1)
mark(dropped-4, if-2)
nsubj(dropped-4, she-3)
dep(Only-1, dropped-4)
det(pill-6, the-5)
dobj(dropped-4, pill-6)
det(drink-9, the-8)
prep_into(dropped-4, drink-9)

