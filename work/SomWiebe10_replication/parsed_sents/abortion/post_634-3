(ROOT
  (S
    (NP (NNP Belief))
    (VP (VBZ is)
      (NP
        (NP (DT all))
        (SBAR
          (S
            (NP (PRP we))
            (VP (VBD got)
              (NP (NNP Jon)))))))
    (. .)))

nsubj(all-3, Belief-1)
cop(all-3, is-2)
root(ROOT-0, all-3)
nsubj(got-5, we-4)
rcmod(all-3, got-5)
dobj(got-5, Jon-6)

