(ROOT
  (S
    (NP (DT This) (NN principle))
    (VP (MD can)
      (VP (VB be)
        (VP (VBN applied)
          (PP (TO to)
            (NP (NN abortion))))))
    (. .)))

det(principle-2, This-1)
