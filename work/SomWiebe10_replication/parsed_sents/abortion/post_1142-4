(ROOT
  (S
    (ADVP (RB Ultimately))
    (NP (DT the) (NN choice))
    (VP (VBZ lies)
      (PP (IN within)
        (NP (DT the) (NNS women))))
    (. .)))

advmod(lies-4, Ultimately-1)
det(choice-3, the-2)
nsubj(lies-4, choice-3)
root(ROOT-0, lies-4)
det(women-7, the-6)
prep_within(lies-4, women-7)

