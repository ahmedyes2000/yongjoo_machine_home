(ROOT
  (S
    (NP (EX There))
    (VP (VBP are) (RB n't)
      (NP (JJ enough) (NNS children))
      (S
        (VP (TO to)
          (VP (VB be)
            (VP (VBN adopted)
              (ADVP (RB here))
              (PP (RB because) (IN of)
                (NP (NN abortion))))))))
    (. .)))

expl(are-2, There-1)
root(ROOT-0, are-2)
neg(are-2, n't-3)
amod(children-5, enough-4)
nsubj(are-2, children-5)
aux(adopted-8, to-6)
auxpass(adopted-8, be-7)
xcomp(are-2, adopted-8)
advmod(adopted-8, here-9)
prep_because_of(adopted-8, abortion-12)

