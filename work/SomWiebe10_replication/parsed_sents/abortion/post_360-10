(ROOT
  (S
    (NP (PRP I))
    (ADVP (RB just))
    (VP (VBP think)
      (SBAR (IN that)
        (S
          (SBAR (IN if)
            (S
              (NP (PRP we))
              (VP (VB address)
                (NP (DT the) (NN problem))
                (PP (IN at)
                  (NP (DT the) (NN source))))))
          (NP (RB then) (JJR less) (NN abortion))
          (VP (MD will)
            (VP (VB follow)
              (SBAR
                (S
                  (NP (NN wich))
                  (VP (VBZ is)
                    (SBAR
                      (WHNP (WP what))
                      (S
                        (NP (PRP we))
                        (VP (MD should) (RB all)
                          (VP (VB agree)
                            (PP (IN with))))))))))))))
    (. .)))

nsubj(think-3, I-1)
advmod(think-3, just-2)
root(ROOT-0, think-3)
complm(follow-17, that-4)
mark(address-7, if-5)
nsubj(address-7, we-6)
advcl(follow-17, address-7)
det(problem-9, the-8)
dobj(address-7, problem-9)
det(source-12, the-11)
prep_at(address-7, source-12)
advmod(abortion-15, then-13)
amod(abortion-15, less-14)
nsubj(follow-17, abortion-15)
aux(follow-17, will-16)
ccomp(think-3, follow-17)
nsubj(is-19, wich-18)
ccomp(follow-17, is-19)
dep(agree-24, what-20)
nsubj(agree-24, we-21)
aux(agree-24, should-22)
advmod(agree-24, all-23)
ccomp(is-19, agree-24)
prep(agree-24, with-25)

