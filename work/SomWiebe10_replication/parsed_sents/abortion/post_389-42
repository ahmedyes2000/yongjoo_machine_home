(ROOT
  (S
    (PP (IN Upon)
      (NP (NN birth)))
    (NP (PRP it))
    (VP (VBZ is)
      (ADVP (RB still))
      (PP (IN on)
        (NP (DT the) (JJ pleasure-pain) (NN principle))))
    (. .)))

prep_upon(is-4, birth-2)
nsubj(is-4, it-3)
root(ROOT-0, is-4)
advmod(is-4, still-5)
det(principle-9, the-7)
amod(principle-9, pleasure-pain-8)
