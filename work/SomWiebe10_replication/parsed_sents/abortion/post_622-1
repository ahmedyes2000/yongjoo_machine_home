(ROOT
  (S
    (NP (PRP You))
    (VP (VBP are)
      (ADJP (JJ wrong)
        (PP (IN in)
          (S
            (VP (VBG assuming)
              (SBAR (IN that) (IN because)
                (S
                  (NP (PRP I))
                  (VP (VBP know)
                    (SBAR (IN that)
                      (S
                        (NP (NN abortion))
                        (VP (VBZ kills)
                          (NP (DT a) (NN child))
                          (SBAR (IN that)
                            (S
                              (NP (PRP I))
                              (VP (MD must) (RB not)
                                (VP (VB care)
                                  (PP (IN about)
                                    (NP (DT the) (NN mother))))))))))))))))))
    (. .)))

nsubj(wrong-3, You-1)
cop(wrong-3, are-2)
root(ROOT-0, wrong-3)
prepc_in(wrong-3, assuming-5)
complm(know-9, that-6)
dep(know-9, because-7)
nsubj(know-9, I-8)
ccomp(assuming-5, know-9)
complm(kills-12, that-10)
nsubj(kills-12, abortion-11)
ccomp(know-9, kills-12)
det(child-14, a-13)
dobj(kills-12, child-14)
