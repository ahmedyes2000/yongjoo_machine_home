(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP (VBP do) (RB n't)
          (VP (VB like)
            (NP (DT a) (NN democracy))))))
    (, ,)
    (VP (VB go)
      (PP (TO to)
        (NP (NNP Iran))))
    (. .)))

