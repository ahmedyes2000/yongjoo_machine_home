  (S
    (NP (JJ Legal) (NNS abortions))
    (VP (VBP are)
      (ADJP
        (ADJP (RB much) (JJR safer))
        (PP (IN than)
          (NP (NN black-market) (NNS abortions)))))
    (. .)))

