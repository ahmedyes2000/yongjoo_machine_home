(ROOT
  (S
    (ADVP (RB However))
    (, ,)
    (NP (DT this))
    (ADVP (RB alone))
    (VP (VBZ is)
      (ADVP (RB also))
      (RB not)
      (ADJP (JJ enough)))
    (. .)))

advmod(enough-8, However-1)
nsubj(enough-8, this-3)
advmod(enough-8, alone-4)
cop(enough-8, is-5)
advmod(enough-8, also-6)
neg(enough-8, not-7)
root(ROOT-0, enough-8)

