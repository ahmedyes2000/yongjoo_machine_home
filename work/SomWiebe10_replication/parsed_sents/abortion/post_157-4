(ROOT
  (S ('' '')
    (NP (DT This) (NN argument))
    (VP (MD may)
      (ADVP (RB equally))
      (VP (VB apply)
        (PP (TO to)
          (NP (JJ partial) (NN birth) (NN abortion)))))
    (. .)))

det(argument-3, This-2)
nsubj(apply-6, argument-3)
aux(apply-6, may-4)
advmod(apply-6, equally-5)
root(ROOT-0, apply-6)
amod(abortion-10, partial-8)
nn(abortion-10, birth-9)
prep_to(apply-6, abortion-10)

