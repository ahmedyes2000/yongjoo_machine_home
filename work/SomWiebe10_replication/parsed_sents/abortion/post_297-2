(ROOT
  (S
    (ADVP (IN Besides))
    (, ,)
    (NP (DT these) (NNS pictures))
    (VP (VBP have)
      (VP (VBN been)
        (ADJP (IN around)
          (PP (IN for)
            (NP (NNS decades))))
        (SBAR (IN so)
          (S
            (NP (PRP they))
            (VP (VBD were)
              (ADVP (RB probably))
              (VP (VBN done)
                (NP (NNS ages))
                (ADVP (IN ago))))))))
    (. .)))

advmod(around-7, Besides-1)
det(pictures-4, these-3)
nsubj(around-7, pictures-4)
aux(around-7, have-5)
cop(around-7, been-6)
root(ROOT-0, around-7)
prep_for(around-7, decades-9)
mark(done-14, so-10)
nsubjpass(done-14, they-11)
auxpass(done-14, were-12)
advmod(done-14, probably-13)
advcl(around-7, done-14)
dobj(done-14, ages-15)
advmod(done-14, ago-16)

