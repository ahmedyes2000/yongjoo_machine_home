(ROOT
  (S
    (ADVP (RB Genetically))
    (, ,)
    (NP (PRP we))
    (VP (MD could)
      (VP (VB say)
        (SBAR (IN that)
          (S
            (NP (DT a) (JJ new) (NN individual))
            (VP (VBZ begins)
              (SBAR
                (WHADVP (WRB when))
                (S
                  (NP
                    (NP (DT a) (JJ new) (NN combination))
                    (PP (IN of)
                      (NP (NNS genes))))
                  (VP (VBZ emerges)))))))))
    (. .)))

advmod(say-5, Genetically-1)
nsubj(say-5, we-3)
aux(say-5, could-4)
root(ROOT-0, say-5)
complm(begins-10, that-6)
det(individual-9, a-7)
amod(individual-9, new-8)
nsubj(begins-10, individual-9)
ccomp(say-5, begins-10)
advmod(emerges-17, when-11)
det(combination-14, a-12)
