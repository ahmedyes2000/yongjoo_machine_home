(ROOT
  (S
    (S
      (NP (PRP I))
      (VP (VBP 've)
        (VP
          (ADVP (RB never))
          (VBN been)
          (PP (IN in)
            (NP (DT that) (NN situation)))
          (ADVP (RB before)))))
    (, ,)
    (CC but)
    (S
      (NP (NNS accidents))
      (VP (VBP happen)
        (PP (TO to)
          (NP (NN everyone)))))
    (. .)))

nsubj(been-4, I-1)
aux(been-4, 've-2)
neg(been-4, never-3)
root(ROOT-0, been-4)
det(situation-7, that-6)
prep_in(been-4, situation-7)
advmod(been-4, before-8)
nsubj(happen-12, accidents-11)
conj_but(been-4, happen-12)
prep_to(happen-12, everyone-14)

