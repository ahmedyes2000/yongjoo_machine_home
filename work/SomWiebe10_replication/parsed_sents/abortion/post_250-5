(ROOT
  (S
    (NP
      (NP (JJ Negative) (NNS feelings))
      (PP (IN about)
        (NP
          (NP (DT the)
            (ADJP (RB metaphysically) (JJ independent))
            (NN status))
          (PP (IN of)
            (NP (NNS women))))))
    (VP (MD should)
      (VP (VB have)
        (NP
          (NP (DT no) (NN bearing))
          (PP (IN on)
            (NP (DT the) (NN discussion))))))
    (. .)))

amod(feelings-2, Negative-1)
nsubj(have-11, feelings-2)
det(status-7, the-4)
advmod(independent-6, metaphysically-5)
amod(status-7, independent-6)
