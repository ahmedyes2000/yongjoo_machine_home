(ROOT
  (S
    (ADVP (RB As) (RB far)
      (SBAR (IN as)
        (S
          (NP (NN abortion))
          (VP (VBZ goes)))))
    (, ,)
    (NP (EX there))
    (VP (VBP are)
      (ADVP (RB clearly))
      (VP (VBG differing)
        (NP (NNS opinions))
        (PP (IN on)
          (SBAR
            (WHADVP (WRB when))
            (S
              (NP (NN life))
              (VP (VBZ begins)))))))
    (. .)))

advmod(far-2, As-1)
advmod(differing-10, far-2)
mark(goes-5, as-3)
nsubj(goes-5, abortion-4)
ccomp(far-2, goes-5)
expl(differing-10, there-7)
aux(differing-10, are-8)
advmod(differing-10, clearly-9)
root(ROOT-0, differing-10)
dobj(differing-10, opinions-11)
nsubj(differing-10, opinions-11)
advmod(begins-15, when-13)
nsubj(begins-15, life-14)
prepc_on(differing-10, begins-15)

