(ROOT
  (S
    (NP (DT That))
    (VP (VBZ 's)
      (NP (DT the) (NN lifestyle))
      (SBAR (IN that)
        (S
          (NP (PRP I))
          (VP (VBP choose)
            (S
              (VP (TO to)
                (VP (VB live))))))))
    (. .)))

nsubj(lifestyle-4, That-1)
cop(lifestyle-4, 's-2)
det(lifestyle-4, the-3)
root(ROOT-0, lifestyle-4)
complm(choose-7, that-5)
nsubj(choose-7, I-6)
xsubj(live-9, I-6)
ccomp(lifestyle-4, choose-7)
aux(live-9, to-8)
xcomp(choose-7, live-9)

