  (S
    (S
      (NP (PRP You))
      (VP (VBP do) (RB n't)
        (VP (VB know)
          (NP (DT the) (NNS circumstances)))))
    (, ,)
    (CC and)
    (S
      (NP (PRP you))
      (VP (MD ca) (RB n't)
        (VP (VB make)
          (NP (NNS judgements))
          (PP (IN like)
            (NP
              (NP (DT that))
              (PP (IN without)
                (NP
