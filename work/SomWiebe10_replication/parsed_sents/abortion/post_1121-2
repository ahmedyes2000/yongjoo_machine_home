  (S
    (PP (IN About)
      (NP (NN desire)))
    (, ,)
    (NP (PRP we))
    (ADVP (DT all))
    (VP (VBD got)
      (SBAR
        (S
          (NP (PRP them)
            (CC and)
            (PRP we))
          (ADVP (DT all))
          (VP (VBP have)
            (NP
              (NP (DT an) (NN assortment))
              (PP (IN of)
                (NP (PRP them))))))))
    (. .)))

prep_about(got-6, desire-2)
nsubj(got-6, we-4)
advmod(got-6, all-5)
root(ROOT-0, got-6)
