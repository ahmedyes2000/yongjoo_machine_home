(ROOT
  (S
    (SBAR (IN That)
      (S
        (NP (DT the) (NN fetus))
        (VP (MD may)
          (VP (VB do)
            (NP (NN harm))
            (PP (TO to)
              (NP (DT the) (NN mother)))))))
    (VP (MD can) (RB not)
      (VP (VB justify)
        (S
          (VP (VBG destroying)
            (NP (PRP it))))))
    (. .)))

complm(do-5, That-1)
det(fetus-3, the-2)
nsubj(do-5, fetus-3)
aux(do-5, may-4)
csubj(justify-12, do-5)
dobj(do-5, harm-6)
det(mother-9, the-8)
prep_to(do-5, mother-9)
aux(justify-12, can-10)
neg(justify-12, not-11)
root(ROOT-0, justify-12)
xcomp(justify-12, destroying-13)
dobj(destroying-13, it-14)

