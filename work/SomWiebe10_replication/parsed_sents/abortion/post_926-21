(ROOT
  (S
    (NP (PRP You))
    (VP (VBD noted)
      (ADVP (RB well))
      (SBAR (IN that)
        (S
          (NP (PRP I))
          (VP (VBP see)
            (S
              (NP (DT this) (NN issue))
              (ADJP (RB as) (JJ black)
                (CC and)
                (JJ white)))))))
    (. .)))

nsubj(noted-2, You-1)
root(ROOT-0, noted-2)
advmod(noted-2, well-3)
complm(see-6, that-4)
nsubj(see-6, I-5)
ccomp(noted-2, see-6)
det(issue-8, this-7)
nsubj(black-10, issue-8)
nsubj(white-12, issue-8)
advmod(black-10, as-9)
xcomp(see-6, black-10)
xcomp(see-6, white-12)
conj_and(black-10, white-12)

