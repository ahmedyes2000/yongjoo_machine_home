  (S
    (ADVP (RB So))
    (, ,)
    (S
      (ADVP (RB sure))
      (, ,)
      (NP (PRP you))
      (VP (MD can)
        (VP (VB call)
          (NP (NN abortion) (NN murder))
          (ADVP
            (ADVP (RB as) (RB much))
            (SBAR (IN as)
              (S
                (NP (PRP you))
                (ADVP (DT all))
                (VP (VBP want))))))))
    (, ,)
    (CC but)
    (S
      (NP (PRP it))
      (VP (VBZ 's)
        (VP (VBG getting)
          (ADVP (RB really))
          (PP (RB out) (IN of)
            (NP (NN hand))))))
    (. .)))

