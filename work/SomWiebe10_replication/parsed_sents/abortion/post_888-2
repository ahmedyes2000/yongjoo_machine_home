(ROOT
  (S
    (VP (VB Picture)
      (SBAR (IN that)
        (S
          (NP (PRP$ your) (JJ loving) (NN boy))
          (VP (VBD was)
            (NP
              (NP (DT the) (NN product))
              (PP (IN of)
                (NP (NN hate))))))))
    (. .)))

root(ROOT-0, Picture-1)
complm(product-8, that-2)
poss(boy-5, your-3)
amod(boy-5, loving-4)
nsubj(product-8, boy-5)
cop(product-8, was-6)
det(product-8, the-7)
