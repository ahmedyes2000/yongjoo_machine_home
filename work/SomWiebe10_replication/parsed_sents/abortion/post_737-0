(ROOT
  (FRAG
    (WHNP (WDT What))
    (PP (IN about)
      (NP
        (NP (DT the) (NN child))
        (SBAR
          (WHNP (WP who))
          (S
            (VP (VBZ has)
              (NP (DT no) (NN say))))))
      (. ?))))

root(ROOT-0, What-1)
dep(What-1, about-2)
det(child-4, the-3)
pobj(about-2, child-4)
nsubj(has-6, child-4)
rcmod(child-4, has-6)
det(say-8, no-7)
dobj(has-6, say-8)

