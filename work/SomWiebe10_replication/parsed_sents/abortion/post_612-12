(ROOT
  (S
    (NP (PRP We))
    (ADVP (RB constantly))
    (VP (VB balance)
      (NP
        (NP
          (NP (CD one) (NN person) (POS 's))
          (NNS rights))
        (PP (IN against)
          (NP (DT another) (NNS 's)))))
    (. .)))

nsubj(balance-3, We-1)
advmod(balance-3, constantly-2)
root(ROOT-0, balance-3)
num(person-5, one-4)
poss(rights-7, person-5)
dobj(balance-3, rights-7)
det('s-10, another-9)
prep_against(rights-7, 's-10)

