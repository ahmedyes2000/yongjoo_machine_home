  (S
    (NP
      (NP (NN None))
      (PP (IN of)
        (NP (PRP them))))
    (VP (VBP have)
      (VP (VBN hooked)
        (NP (PRP themselves))
        (ADVP (RB up)
          (PP (TO to)
            (NP
              (NP (DT the) (NNS bodies))
              (PP (IN of)
                (NP
                  (ADJP (JJ autonomous))
                  (NNS adults)))
              (SBAR (WP$ whose)
                (S
                  (NP (JJ physiological) (NNS functions))
                  (VP (VBP are)
                    (ADVP (RB then))
                    (VP (VBN used)
                      (S
                        (VP (TO to)
                          (VP (VB regulate)
                            (NP (PRP$ their) (JJ own))))))))))))))
    (. .)))

