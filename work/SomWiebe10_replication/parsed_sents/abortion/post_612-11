(ROOT
  (S
    (NP (DT The) (NN truth))
    (VP (VBZ is)
      (SBAR (IN that)
        (S
          (NP (PRP we))
          (VP (VB assign) (, ,)
            (PP (IN as)
              (NP (DT a) (NN society)))
            (, ,)
            (NP
              (NP (DT the) (NNS rights))
              (SBAR
                (S
                  (NP (PRP we))
                  (VP (VBP deem)
                    (ADJP (JJ appropriate)
                      (PP (TO to)
                        (NP (DT the) (NNS circumstances))))))))))))
    (. .)))

det(truth-2, The-1)
nsubj(is-3, truth-2)
root(ROOT-0, is-3)
complm(assign-6, that-4)
nsubj(assign-6, we-5)
ccomp(is-3, assign-6)
det(society-10, a-9)
prep_as(assign-6, society-10)
det(rights-13, the-12)
