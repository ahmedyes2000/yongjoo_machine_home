(ROOT
  (S
    (NP (PRP It))
    (VP (MD wo) (RB n't)
      (VP (VB hurt)
        (NP (NN anyone) (JJR older))
        (S
          (VP (TO to)
            (VP (VB carry)
              (NP (PRP$ their) (NN child)))))))
    (. .)))

nsubj(hurt-4, It-1)
aux(hurt-4, wo-2)
neg(hurt-4, n't-3)
root(ROOT-0, hurt-4)
dep(older-6, anyone-5)
dobj(hurt-4, older-6)
aux(carry-8, to-7)
xcomp(hurt-4, carry-8)
poss(child-10, their-9)
dobj(carry-8, child-10)

