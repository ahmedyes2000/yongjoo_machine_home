(ROOT
  (S
    (NP (RB since) (NN life))
    (VP (VBZ is)
      (ADJP (RB so) (JJ precious))
      (SBAR (IN as)
        (S
          (VP (TO to)
            (VP (VB choose)
              (NP (PRP it))
              (PP (IN over)
                (NP (NN liberty))))))))
    (. .)))

advmod(life-2, since-1)
nsubj(precious-5, life-2)
cop(precious-5, is-3)
advmod(precious-5, so-4)
root(ROOT-0, precious-5)
mark(choose-8, as-6)
aux(choose-8, to-7)
advcl(precious-5, choose-8)
xcomp(precious-5, choose-8)
dobj(choose-8, it-9)
prep_over(choose-8, liberty-11)

