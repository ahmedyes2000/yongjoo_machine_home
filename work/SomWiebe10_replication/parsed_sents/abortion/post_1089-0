(ROOT
  (NP
    (NP (DT some) (JJ ignorant) (NNS people))
    (PRN (-LRB- -LRB-)
      (NP
        (NP (RB mostly) (NNS men))
        (, ,)
        (PP (IN of)
          (NP (NN course))))
      (-RRB- -RRB-))
    (. .)))

det(people-3, some-1)
