  (S
    (ADVP (RB However))
    (, ,)
    (NP (DT the) (NNP Church))
    (ADVP (RB always))
    (VP (VBD viewed)
      (NP (NN abortion))
      (PP (IN as)
        (NP (DT a) (JJ great) (NN evil)))
      (PP (IN at)
        (NP
          (NP (DT any) (NN stage))
          (PP (IN of)
            (NP (NN pregnancy))))))
    (. .)))

advmod(viewed-6, However-1)
det(Church-4, the-3)
nsubj(viewed-6, Church-4)
advmod(viewed-6, always-5)
