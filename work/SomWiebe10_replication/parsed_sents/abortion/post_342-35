aux(do-15, do-13)
neg(do-15, not-14)
ccomp(easier-4, do-15)
dep(doing-20, but-17)
aux(doing-20, could-18)
aux(doing-20, be-19)
dep(do-15, doing-20)
amod(lives-24, saves-23)
