(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ 's)
      (ADVP (RB simply))
      (ADJP (JJR easier))
      (S
        (VP (TO to) (RB not)
          (VP (VB think)
            (PP (IN about)
              (NP (DT the) (NNS things))))))
      (SBAR (IN that)
        (S
          (NP (PRP we))
          (VP (VBP do) (RB not)
            (VP (VB do)
              (PRN (-LRB- -LRB-)
                (FRAG (CC but)
                  (S
                    (VP (MD could)
                      (VP (VB be)
                        (VP (VBG doing))))))
                (-RRB- -RRB-))
              (PP (TO to)
                (NP (JJ saves) (NNS lives))))))))
    (. .)))

nsubj(easier-4, It-1)
xsubj(think-7, It-1)
cop(easier-4, 's-2)
advmod(easier-4, simply-3)
root(ROOT-0, easier-4)
aux(think-7, to-5)
neg(think-7, not-6)
xcomp(easier-4, think-7)
det(things-10, the-9)
