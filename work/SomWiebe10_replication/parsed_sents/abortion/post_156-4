(ROOT
  (S
    (NP (DT Another) (NN birth))
    (VP (MD will)
      (VP (VB draw)
        (NP (DT the) (NN family))
        (ADVP (RBR deeper))
        (PP (IN into)
          (NP (NN poverty)))))
    (. .)))

det(birth-2, Another-1)
nsubj(draw-4, birth-2)
aux(draw-4, will-3)
root(ROOT-0, draw-4)
det(family-6, the-5)
dobj(draw-4, family-6)
