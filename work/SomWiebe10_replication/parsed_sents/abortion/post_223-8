(ROOT
  (S (CC and)
    (VP (TO to)
      (VP (VB give)
        (PRT (RP away))
        (SBAR
          (WHNP (WDT whatever))
          (S
            (NP (PRP they))
            (VP (VBD did) (RB n't)
              (VP (VB need)))))))
    (. .)))

cc(give-3, and-1)
aux(give-3, to-2)
root(ROOT-0, give-3)
prt(give-3, away-4)
dobj(need-9, whatever-5)
nsubj(need-9, they-6)
aux(need-9, did-7)
neg(need-9, n't-8)
ccomp(give-3, need-9)

