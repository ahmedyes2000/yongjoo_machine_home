(ROOT
  (S
    (NP (PRP I))
    (VP (VBP have)
      (VP (VBN spent)
        (NP
          (NP (CD three) (NNS years))
          (VP (VBG trying)
            (S
              (VP (TO to)
                (VP (VB answer)
                  (NP (DT that) (NN question)))))))))
    (. .)))

nsubj(spent-3, I-1)
aux(spent-3, have-2)
root(ROOT-0, spent-3)
num(years-5, three-4)
dobj(spent-3, years-5)
xsubj(answer-8, years-5)
partmod(years-5, trying-6)
aux(answer-8, to-7)
xcomp(trying-6, answer-8)
det(question-10, that-9)
dobj(answer-8, question-10)

