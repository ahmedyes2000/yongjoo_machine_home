(ROOT
  (S
    (NP (JJ Such) (NN cooperation))
    (VP (MD can)
      (ADVP (RB only))
      (VP (VB be)
        (VP (VBN tolerated)
          (SBAR (IN if)
            (S
              (NP
                (NP (DT all) (CD four))
                (PP (IN of)
                  (NP (DT the) (JJ following) (NNS conditions))))
              (VP (VBP exist)
                (NP (CD :1))))))))
    (. .)))

amod(cooperation-2, Such-1)
nsubjpass(tolerated-6, cooperation-2)
aux(tolerated-6, can-3)
advmod(tolerated-6, only-4)
auxpass(tolerated-6, be-5)
root(ROOT-0, tolerated-6)
mark(exist-14, if-7)
det(four-9, all-8)
nsubj(exist-14, four-9)
det(conditions-13, the-11)
amod(conditions-13, following-12)
prep_of(four-9, conditions-13)
advcl(tolerated-6, exist-14)
dobj(exist-14, :1-15)

