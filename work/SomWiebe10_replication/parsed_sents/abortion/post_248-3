(ROOT
  (S
    (NP (PRP you))
    (VP (VBP dont)
      (VP (VB have)
        (S
          (VP (TO to)
            (VP (VB be)
              (PP
                (PP (IN for)
                  (NP (PRP it)))
                (: ...)
                (CC or)
                (PP (IN against)
                  (NP (PRP it)))))))))
    (: ...)))

nsubj(dont-2, you-1)
xsubj(be-5, you-1)
root(ROOT-0, dont-2)
dep(dont-2, have-3)
aux(be-5, to-4)
xcomp(have-3, be-5)
xcomp(have-3, be-5')
conj_or(be-5, be-5')
prep_for(be-5, it-7)
prep_against(be-5', it-11)

