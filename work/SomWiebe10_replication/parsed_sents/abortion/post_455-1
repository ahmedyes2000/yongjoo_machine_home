(ROOT
  (S
    (NP (DT The) (NN issue))
    (VP (VBZ is) (RB not)
      (NP (NNS communities)
        (CC or)
        (NNS societies)))
    (. .)))

det(issue-2, The-1)
nsubj(communities-5, issue-2)
cop(communities-5, is-3)
neg(communities-5, not-4)
root(ROOT-0, communities-5)
conj_or(communities-5, societies-7)

