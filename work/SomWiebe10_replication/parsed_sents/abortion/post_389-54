(ROOT
  (S
    (NP (PRP You))
    (VP (VBD wrote) (: :) (`` ``)
      (S
        (S
          (NP (PRP$ Your) (NN argument))
          (VP (VBZ is)
            (VP (VBN based)
              (PP (IN on)
                (NP
                  (ADJP (RB very) (JJ simplistic))
                  (NNS terms))))))
        (: ...)
        (CC but)
        (S
          (NP (PRP it))
          (VP (VBZ does)
            (NP (NN nothing))
            (S
              (VP (TO to)
                (VP (VB change)
                  (NP (DT the) (NNS facts))
                  (SBAR (IN that)
                    (S
                      (NP
                        (NP (DT any) (NN abortion))
