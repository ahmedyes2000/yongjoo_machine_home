(ROOT
  (S
    (S
      (NP (EX There))
      (VP (VBZ is)
        (NP (DT no) (JJ simple) (NN answer))))
    (CC and)
    (S
      (NP (PRP I))
      (VP (VBP understand)
        (NP (PRP$ your) (NN concern))))
    (. .)))

expl(is-2, There-1)
root(ROOT-0, is-2)
det(answer-5, no-3)
amod(answer-5, simple-4)
nsubj(is-2, answer-5)
nsubj(understand-8, I-7)
conj_and(is-2, understand-8)
poss(concern-10, your-9)
dobj(understand-8, concern-10)

