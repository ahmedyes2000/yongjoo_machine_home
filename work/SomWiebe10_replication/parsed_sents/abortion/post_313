(ROOT
  (S
    (NP (PRP I))
    (VP (VBP love)
      (SBAR
        (WHADVP (WRB how))
        (S
          (S
            (PP (IN for)
              (NP
                (ADJP (RB so) (JJ many))
                (NNS years)))
            (NP (NNS women))
            (VP (VBP have)
              (VP (VBN fought)
                (PP (IN for)
                  (NP
                    (NP (NNS women) (POS 's))
                    (NNS rights))))))
          (CC and)
          (S
            (NP (PRP we))
            (VP (VBP use)
              (S
                (NP (PRP it))
                (VP (TO to)
                  (VP (VB kill)
                    (NP (PRP$ our) (JJ own) (NNS children))))))))))
    (. .)))

nsubj(love-2, I-1)
root(ROOT-0, love-2)
advmod(fought-10, how-3)
advmod(many-6, so-5)
amod(years-7, many-6)
prep_for(fought-10, years-7)
nsubj(fought-10, women-8)
aux(fought-10, have-9)
dep(love-2, fought-10)
poss(rights-14, women-12)
prep_for(fought-10, rights-14)
nsubj(use-17, we-16)
dep(love-2, use-17)
conj_and(fought-10, use-17)
nsubj(kill-20, it-18)
aux(kill-20, to-19)
xcomp(use-17, kill-20)
poss(children-23, our-21)
amod(children-23, own-22)
dobj(kill-20, children-23)

(ROOT
  (S
    (NP (NNP Pretty))
    (ADJP (JJ sad))
    (. .)))

dep(sad-2, Pretty-1)
root(ROOT-0, sad-2)

