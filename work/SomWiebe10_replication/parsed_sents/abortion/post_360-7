(ROOT
  (S
    (NP (DT The) (NNP AGI))
    (ADVP (RB also))
    (VP (VBZ reports)
      (SBAR (IN that)
        (S
          (NP (CD 1) (NN %))
          (VP (VBD had)
            (VP (VBN been)
              (VP (VBN forced)
                (S
                  (VP (TO to)
                    (VP (VB have)
                      (NP (NN sex)))))))))))
    (. .)))

det(AGI-2, The-1)
nsubj(reports-4, AGI-2)
advmod(reports-4, also-3)
root(ROOT-0, reports-4)
complm(forced-10, that-5)
num(%-7, 1-6)
nsubjpass(forced-10, %-7)
xsubj(have-12, %-7)
aux(forced-10, had-8)
auxpass(forced-10, been-9)
ccomp(reports-4, forced-10)
aux(have-12, to-11)
xcomp(forced-10, have-12)
dobj(have-12, sex-13)

