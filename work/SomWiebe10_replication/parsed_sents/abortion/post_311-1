(ROOT
  (S
    (NP (NNP Faith))
    (VP (VBZ is)
      (NP
        (NP (DT a) (NN means))
        (PP (IN of)
          (NP
            (NP (NN knowledge) (JJ other))
            (PP (IN than)
              (NP (NN reason)))))))
    (. .)))

nsubj(means-4, Faith-1)
cop(means-4, is-2)
det(means-4, a-3)
root(ROOT-0, means-4)
prep_of(means-4, knowledge-6)
amod(knowledge-6, other-7)
prep_than(knowledge-6, reason-9)

