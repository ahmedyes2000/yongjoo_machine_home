(ROOT
  (S
    (NP (NNP Women))
    (VP (VBP have)
      (NP (DT the) (NN right)
        (S
          (VP (TO to)
            (VP (VB choose) (, ,)
              (NP (NN period)))))))
    (. .)))

nsubj(have-2, Women-1)
root(ROOT-0, have-2)
det(right-4, the-3)
dobj(have-2, right-4)
