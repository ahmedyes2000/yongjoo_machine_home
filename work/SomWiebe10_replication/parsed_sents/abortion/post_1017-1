(ROOT
  (S
    (NP (JJ Many) (NNS countries))
    (VP (VBP face)
      (S
        (NP (DT a) (NN crisis))
        (ADJP (JJ due)
          (PP (TO to)
            (NP (DT this))))))
    (. .)))

amod(countries-2, Many-1)
nsubj(face-3, countries-2)
root(ROOT-0, face-3)
det(crisis-5, a-4)
nsubj(due-6, crisis-5)
xcomp(face-3, due-6)
prep_to(due-6, this-8)

