(ROOT
  (S
    (NP (PRP We))
    (ADVP (RB also))
    (VP (VBP know)
      (SBAR (IN that)
        (S
          (NP (JJ many) (, ,) (JJ many) (NNS people))
          (VP (VBP suffer)
            (PP (IN after)
              (S
                (VP (VBG having)
                  (NP
                    (NP (DT an) (NN abortion))
                    (, ,)
                    (NP (NNS men))
                    (CC and)
                    (NP (NNS women))))))))))
    (. .)))

nsubj(know-3, We-1)
advmod(know-3, also-2)
root(ROOT-0, know-3)
complm(suffer-9, that-4)
amod(people-8, many-5)
