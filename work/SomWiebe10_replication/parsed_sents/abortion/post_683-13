(ROOT
  (S
    (NP (PRP I))
    (VP (VBP 'm)
      (ADJP (JJ sure)
        (SBAR (IN that)
          (S
            (NP (PRP you))
            (VP (VBD said)
              (SBAR (IN that)
                (S
                  (NP (PRP you))
                  (VP (VBD were)
                    (NP (DT a) (NN scientist))))))))))
    (. .)))

nsubj(sure-3, I-1)
cop(sure-3, 'm-2)
root(ROOT-0, sure-3)
complm(said-6, that-4)
nsubj(said-6, you-5)
ccomp(sure-3, said-6)
complm(scientist-11, that-7)
nsubj(scientist-11, you-8)
cop(scientist-11, were-9)
det(scientist-11, a-10)
ccomp(said-6, scientist-11)

