(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is)
      (ADJP (RB morally) (JJ wrong))
      (SBAR (IN as)
        (S
          (NP (PRP we))
          (ADVP (DT all))
          (VP (VBP know)))))
    (. .)))

nsubj(wrong-4, It-1)
cop(wrong-4, is-2)
advmod(wrong-4, morally-3)
root(ROOT-0, wrong-4)
mark(know-8, as-5)
nsubj(know-8, we-6)
advmod(know-8, all-7)
advcl(wrong-4, know-8)

