(ROOT
  (S
    (NP (PRP$ Your) (JJ religious) (NNS beliefs))
    (VP
      (VP (VBZ guide)
        (NP (PRP$ your) (JJ own) (NNS actions)))
      (, ,)
      (CC but)
      (VP (MD can) (RB not)
        (VP (VB be)
          (NP
            (NP (DT the) (NN basis))
            (PP (IN for)
              (S
                (VP (VBG regulating)
                  (CC or)
                  (VBG judging)
                  (NP (NNS others)))))))))
    (. .)))

poss(beliefs-3, Your-1)
amod(beliefs-3, religious-2)
nsubj(guide-4, beliefs-3)
root(ROOT-0, guide-4)
poss(actions-7, your-5)
amod(actions-7, own-6)
dobj(guide-4, actions-7)
aux(basis-14, can-10)
neg(basis-14, not-11)
cop(basis-14, be-12)
det(basis-14, the-13)
conj_but(guide-4, basis-14)
prepc_for(basis-14, regulating-16)
prepc_for(basis-14, judging-18)
conj_or(regulating-16, judging-18)
dobj(regulating-16, others-19)

