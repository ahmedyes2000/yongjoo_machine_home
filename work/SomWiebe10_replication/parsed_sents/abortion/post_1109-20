(ROOT
  (S
    (PP (IN About)
      (NP (DT the) (NN choice)))
    (, ,)
    (NP (PRP you))
    (VP (VBD made)
      (NP (PRP it))
      (SBAR
        (WHADVP (WRB when))
        (S
          (NP (PRP you))
          (VP (VBD decided)
            (S
              (VP (TO to)
                (VP (VB have)
                  (NP (NN sex)))))))))
    (. .)))

det(choice-3, the-2)
prep_about(made-6, choice-3)
nsubj(made-6, you-5)
root(ROOT-0, made-6)
dobj(made-6, it-7)
advmod(decided-10, when-8)
nsubj(decided-10, you-9)
xsubj(have-12, you-9)
advcl(made-6, decided-10)
aux(have-12, to-11)
xcomp(decided-10, have-12)
dobj(have-12, sex-13)

