(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ does)
      (NP (PRP me)))
    (. .)))

nsubj(does-2, It-1)
root(ROOT-0, does-2)
dobj(does-2, me-3)

(ROOT
  (S
    (NP (DT That))
    (VP (VBZ 's)
      (SBAR
        (WHADVP (WRB why))
        (S
          (NP (PRP I))
          (VP (VBP oppose)
            (NP (PDT all) (DT these) (NNS things))))))
    (. .)))

nsubj('s-2, That-1)
root(ROOT-0, 's-2)
advmod(oppose-5, why-3)
nsubj(oppose-5, I-4)
advcl('s-2, oppose-5)
predet(things-8, all-6)
det(things-8, these-7)
dobj(oppose-5, things-8)

