(ROOT
  (S
    (NP (WDT Which))
    (VP (VBZ says)
      (NP (DT a) (NN lot))
      (PP (IN about)
        (NP
          (NP (JJR more))
          (PP (IN about)
            (NP (NN tolerance)))))
      (SBAR (IN than)
        (S
          (NP (PRP it))
          (VP (VBZ does)
            (PP (IN about)
              (NP (NN lifestyle)))))))
    (. .)))

nsubj(says-2, Which-1)
root(ROOT-0, says-2)
det(lot-4, a-3)
dobj(says-2, lot-4)
prep_about(says-2, more-6)
prep_about(more-6, tolerance-8)
mark(does-11, than-9)
nsubj(does-11, it-10)
advcl(says-2, does-11)
prep_about(does-11, lifestyle-13)

