(ROOT
  (S
    (ADVP (RB Maybe))
    (NP (DT that) (NN baby))
    (VP (MD would)
      (VP (VB have)
        (VP (VBN been)
          (NP (PRP$ my) (NN friend)))))
    (. .)))

advmod(friend-8, Maybe-1)
det(baby-3, that-2)
nsubj(friend-8, baby-3)
aux(friend-8, would-4)
aux(friend-8, have-5)
cop(friend-8, been-6)
poss(friend-8, my-7)
root(ROOT-0, friend-8)

