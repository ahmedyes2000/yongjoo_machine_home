(ROOT
  (S
    (S
      (VP (VBG Making)
        (S
          (NP (PRP it))
          (ADJP (JJ illegal)))))
    (VP (MD will) (RB not)
      (VP (VB prevent)
        (NP (NN abortion))))
    (. .)))

csubj(prevent-6, Making-1)
nsubj(illegal-3, it-2)
xcomp(Making-1, illegal-3)
aux(prevent-6, will-4)
neg(prevent-6, not-5)
root(ROOT-0, prevent-6)
dobj(prevent-6, abortion-7)

