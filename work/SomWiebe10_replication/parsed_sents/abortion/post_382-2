(ROOT
  (S
    (PP (IN As)
      (ADVP (RB far) (RB as)))
    (NP (PRP I))
    (VP (VBP am)
      (ADJP (JJ concerned)
        (S
          (VP (TO to)
            (VP (DT each)
              (VP (VB therer)
                (NP (JJ own)))
              (CC and)
              (VP (VB leave)
                (NP (PRP it))
                (NP (DT that) (NN way))))))))
    (. .)))

prep(concerned-6, As-1)
advmod(as-3, far-2)
pcomp(As-1, as-3)
nsubj(concerned-6, I-4)
cop(concerned-6, am-5)
root(ROOT-0, concerned-6)
aux(therer-9, to-7)
dep(therer-9, each-8)
xcomp(concerned-6, therer-9)
dobj(therer-9, own-10)
xcomp(concerned-6, leave-12)
conj_and(therer-9, leave-12)
iobj(leave-12, it-13)
det(way-15, that-14)
dobj(leave-12, way-15)

