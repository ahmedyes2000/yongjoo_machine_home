(ROOT
  (S
    (NP (DT This))
    (VP
      (VP (VBZ is)
        (ADJP (JJ wrong)))
      (, ,)
      (CC and)
      (VP (VBZ undermines)
        (NP
          (NP (DT the) (NN case))
          (PP (IN against)
            (NP (NN abortion))))))
    (. .)))

nsubj(wrong-3, This-1)
nsubj(undermines-6, This-1)
cop(wrong-3, is-2)
root(ROOT-0, wrong-3)
conj_and(wrong-3, undermines-6)
det(case-8, the-7)
dobj(undermines-6, case-8)
