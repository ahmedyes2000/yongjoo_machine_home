(ROOT
  (S
    (NP (DT This))
    (VP (VBZ is)
      (ADVP (RB certainly))
      (NP
        (NP (RB not) (DT the) (NN view))
        (PP (IN of)
          (ADJP (JJ right)
            (CC and)
            (JJ wrong))))
      (SBAR (IN that)
        (S
          (NP
            (NP (NNP Jesus))
            (PP (IN of)
              (NP (NNP Nazareth))))
          (VP (VBD held)))))
    (. .)))

nsubj(view-6, This-1)
cop(view-6, is-2)
advmod(view-6, certainly-3)
neg(view-6, not-4)
det(view-6, the-5)
root(ROOT-0, view-6)
prep_of(view-6, right-8)
prep_of(view-6, wrong-10)
conj_and(right-8, wrong-10)
complm(held-15, that-11)
nsubj(held-15, Jesus-12)
prep_of(Jesus-12, Nazareth-14)
ccomp(view-6, held-15)

