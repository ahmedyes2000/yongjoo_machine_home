(ROOT
  (S
    (NP (DT That))
    (VP (VBZ has)
      (ADVP
        (ADVP (RB never))
        (PRN (, ,)
          (PP (IN in)
            (NP
              (NP (DT the) (NN history))
              (PP (IN of)
                (NP (PRP$ our) (NN country)))))
          (, ,)))
      (VP (VBN been)
        (NP
          (NP (DT an) (JJ adequate) (NN reason))
          (PP (IN for)
            (S
              (VP (VBG banning)
                (NP (NN something))))))))
    (. .)))

nsubj(reason-15, That-1)
aux(reason-15, has-2)
advmod(reason-15, never-3)
dep(never-3, in-5)
det(history-7, the-6)
pobj(in-5, history-7)
poss(country-10, our-9)
prep_of(history-7, country-10)
cop(reason-15, been-12)
det(reason-15, an-13)
amod(reason-15, adequate-14)
root(ROOT-0, reason-15)
prepc_for(reason-15, banning-17)
dobj(banning-17, something-18)

