(ROOT
  (S
    (NP (DT A) (NN husband))
    (VP (VBZ has)
      (NP (DT no) (JJ enforceable) (NN right)
        (S
          (VP (TO to)
            (VP (VB require)
              (NP (DT a) (NN wife)
                (S
                  (VP (TO to)
                    (VP (VB advise)
                      (NP (PRP him))
                      (SBAR (IN before)
                        (S
                          (NP (PRP she))
                          (VP (VBZ exercises)
                            (NP (PRP$ her) (JJ personal) (NNS choices))))))))))))))
    (. .)))

det(husband-2, A-1)
nsubj(has-3, husband-2)
root(ROOT-0, has-3)
det(right-6, no-4)
amod(right-6, enforceable-5)
dobj(has-3, right-6)
aux(require-8, to-7)
infmod(right-6, require-8)
det(wife-10, a-9)
dobj(require-8, wife-10)
aux(advise-12, to-11)
infmod(wife-10, advise-12)
dobj(advise-12, him-13)
mark(exercises-16, before-14)
nsubj(exercises-16, she-15)
advcl(advise-12, exercises-16)
poss(choices-19, her-17)
amod(choices-19, personal-18)
dobj(exercises-16, choices-19)

