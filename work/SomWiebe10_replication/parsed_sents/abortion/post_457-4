(ROOT
  (S (CC And)
    (, ,)
    (PP (IN by)
      (NP (DT the) (NN way)))
    (, ,)
    (NP (PRP you))
    (ADVP (RB only))
    (VP (VBP get)
      (NP
        (NP (CD 2) (NNS years))
        (PP (IN of)
          (NP (NN welfare))))
      (SBAR (IN because)
        (S
          (NP (PRP we))
          (VP (VBP do) (RB n't)
            (VP (VB want)
              (S
                (VP (TO to)
                  (VP (VB pay)
                    (PP (IN for)
                      (NP (PRP$ your) (NN mistake)))))))))))
    (. .)))

cc(get-9, And-1)
det(way-5, the-4)
prep_by(get-9, way-5)
nsubj(get-9, you-7)
