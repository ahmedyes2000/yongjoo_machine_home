(ROOT
  (S
    (NP
      (NP (DT The) (NN majority))
      (PP (IN of)
        (NP (NNS people))))
    (VP (VBP allow)
      (NP
        (ADJP (RB much) (JJR more))
        (NN wiggle) (NN room)))
    (. .)))

det(majority-2, The-1)
nsubj(allow-5, majority-2)
prep_of(majority-2, people-4)
root(ROOT-0, allow-5)
advmod(more-7, much-6)
amod(room-9, more-7)
nn(room-9, wiggle-8)
dobj(allow-5, room-9)

