(ROOT
  (S
    (ADVP (RB Furthermore))
    (, ,)
    (NP (PRP you))
    (ADVP (RB entirely))
    (VP (VBD ignored)
      (NP (PRP$ my) (NN desire)
        (S
          (VP (TO to)
            (VP (VB quit)
              (S
                (VP (VBG debating)
                  (NP (JJ non-abortion) (JJ related) (NNS issues))
                  (PP (IN within)
                    (NP (DT this) (NN debate))))))))))
    (. .)))

advmod(ignored-5, Furthermore-1)
nsubj(ignored-5, you-3)
advmod(ignored-5, entirely-4)
root(ROOT-0, ignored-5)
poss(desire-7, my-6)
dobj(ignored-5, desire-7)
aux(quit-9, to-8)
infmod(desire-7, quit-9)
xcomp(quit-9, debating-10)
amod(issues-13, non-abortion-11)
amod(issues-13, related-12)
dobj(debating-10, issues-13)
det(debate-16, this-15)
prep_within(debating-10, debate-16)

