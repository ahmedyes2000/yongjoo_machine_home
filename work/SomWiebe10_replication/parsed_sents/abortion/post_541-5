(ROOT
  (S
    (S
      (NP (DT That))
      (VP (VBZ is)
        (ADJP (RB almost) (RB completely) (JJ irrelevant))))
    (, ,)
    (NP (DT the) (NN debate))
    (ADVP (RB here))
    (VP (VBZ is)
      (ADJP (IN about))
      (SBAR (IN whether)
        (S
          (NP (NN abortion))
          (VP (MD should)
            (VP (VB be)
              (ADJP (JJ legal)))))
        (CC or)
        (RB not)))
    (. .)))

nsubj(irrelevant-5, That-1)
cop(irrelevant-5, is-2)
advmod(irrelevant-5, almost-3)
advmod(irrelevant-5, completely-4)
ccomp(about-11, irrelevant-5)
det(debate-8, the-7)
nsubj(about-11, debate-8)
advmod(about-11, here-9)
cop(about-11, is-10)
root(ROOT-0, about-11)
complm(legal-16, whether-12)
nsubj(legal-16, abortion-13)
aux(legal-16, should-14)
cop(legal-16, be-15)
ccomp(about-11, legal-16)
ccomp(about-11, not-18)
conj_or(legal-16, not-18)

