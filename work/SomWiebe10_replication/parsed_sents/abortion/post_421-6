(ROOT
  (S
    (NP (PRP You))
    (VP (VBP have)
      (VP (VBN offered)
        (NP
          (NP (DT no) (NN evidence))
          (PP (IN for)
            (NP (PRP$ your) (NNS claims))))))
    (. .)))

nsubj(offered-3, You-1)
aux(offered-3, have-2)
root(ROOT-0, offered-3)
det(evidence-5, no-4)
dobj(offered-3, evidence-5)
poss(claims-8, your-7)
prep_for(evidence-5, claims-8)

