(ROOT
  (S
    (NP (DT Both))
    (VP (VBD trusted)
      (NP
        (NP (NNP God))
        (PP (IN in)
          (NP
            (ADJP (RB seemingly) (JJ impossible))
            (NNS situations)))))
    (. .)))

nsubj(trusted-2, Both-1)
root(ROOT-0, trusted-2)
dobj(trusted-2, God-3)
advmod(impossible-6, seemingly-5)
amod(situations-7, impossible-6)
prep_in(God-3, situations-7)

