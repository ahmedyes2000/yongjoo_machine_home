(ROOT
  (S
    (S
      (VP (VBG Deciding)
        (SBAR
          (WHADVP (WRB when))
          (S
            (NP (JJ human) (NN life))
            (VP (VBZ begins))))))
    (VP (VBZ is)
      (NP
        (NP (DT a) (JJ scientific) (NN question))
        (SBAR
          (WHNP (WDT that))
          (S
            (VP (VBZ is)
              (ADJP (JJ capable)
                (PP (IN of)
                  (NP (NN determination)))))))))
    (. .)))

csubj(question-9, Deciding-1)
advmod(begins-5, when-2)
amod(life-4, human-3)
nsubj(begins-5, life-4)
advcl(Deciding-1, begins-5)
cop(question-9, is-6)
det(question-9, a-7)
amod(question-9, scientific-8)
root(ROOT-0, question-9)
nsubj(capable-12, question-9)
cop(capable-12, is-11)
rcmod(question-9, capable-12)
prep_of(capable-12, determination-14)

