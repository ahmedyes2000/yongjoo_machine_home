(ROOT
  (S
    (NP (PRP I))
    (VP
      (VP (VBP have)
        (ADVP (RB now))
        (VP (VBN scoured)
          (NP (DT the) (NN site))))
      (CC and)
      (VP (VBD read)
        (NP
          (NP (DT each))
          (PP (IN of)
            (NP (PRP$ your) (NNS comments))))
        (NP (JJ several) (NNS times))))
    (. .)))

nsubj(scoured-4, I-1)
nsubj(read-8, I-1)
aux(scoured-4, have-2)
advmod(scoured-4, now-3)
root(ROOT-0, scoured-4)
det(site-6, the-5)
dobj(scoured-4, site-6)
conj_and(scoured-4, read-8)
iobj(read-8, each-9)
poss(comments-12, your-11)
