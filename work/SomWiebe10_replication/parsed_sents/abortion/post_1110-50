(ROOT
  (S
    (VP (VB Let)
      (S
        (NP (PRP me))
        (VP (VB start)
          (S
            (NP
              (NP (DT a) (JJ new) (NN debate))
              (PP (IN for)
                (NP (DT this))))
            (ADJP (RB alone))))))
    (. .)))

root(ROOT-0, Let-1)
nsubj(start-3, me-2)
ccomp(Let-1, start-3)
det(debate-6, a-4)
amod(debate-6, new-5)
nsubj(alone-9, debate-6)
prep_for(debate-6, this-8)
xcomp(start-3, alone-9)

