  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP (VBP 're)
          (PP (IN against)
            (NP (NNP Roe)
              (CC v.)
              (NNP Wade))))))
    (, ,)
    (NP (PRP you))
    (VP (VBP 're)
      (NP (JJ pro-dead) (NNS mothers))
      (, ,)
      (ADVP (RB not)
        (PP (IN against)
          (NP (NN abortion)))))
    (. .)))

mark('re-3, If-1)
nsubj('re-3, you-2)
advcl(mothers-12, 're-3)
