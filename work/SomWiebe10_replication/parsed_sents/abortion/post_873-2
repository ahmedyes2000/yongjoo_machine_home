  (S
    (NP (NNP Government))
    (VP (MD should) (RB not)
      (VP (VB place)
        (NP (NNS restrictions))
        (PP (IN on)
          (SBAR
            (WHNP (WP what))
            (S
              (NP (DT a) (NN female))
              (VP (MD can)
                (VP (VB do)
                  (PP
                    (PP (IN with)
                      (PP (TO to)
                        (NP (PRP$ her) (NN body))))
                    (, ,) (RB because)
                    (PP (IN of)
                      (NP (DT a) (NN chance)))))))))))
    (. .)))

