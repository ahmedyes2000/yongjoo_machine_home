(ROOT
  (S
    (NP (JJ Pro-life) (JJ militant) (NNS groups))
    (VP (VBP act)
      (ADVP (RB violently))
      (PP (IN in)
        (NP (NN opposition)))
      (PP (TO to)
        (NP (NN abortion))))
    (. .)))

amod(groups-3, Pro-life-1)
amod(groups-3, militant-2)
nsubj(act-4, groups-3)
root(ROOT-0, act-4)
advmod(act-4, violently-5)
prep_in(act-4, opposition-7)
prep_to(act-4, abortion-9)

