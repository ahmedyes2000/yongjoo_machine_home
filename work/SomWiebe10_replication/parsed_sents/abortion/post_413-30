(ROOT
  (S
    (NP (PRP I))
    (VP (VBP believe)
      (S
        (NP (PRP we))
        (DT both)
        (VP (VB agree)
          (SBAR (IN that)
            (S
              (NP
                (NP (NN reality))
                (PRN (-LRB- -LRB-)
                  (NP (NN fact))
                  (-RRB- -RRB-)))
              (VP (MD must)
                (VP (VB rule)
                  (NP (DT the) (NN argument) (CD \/) (NN debate) (CD \/) (NN question) (NN \/) (NN decision)))))))))
    (. .)))

nsubj(believe-2, I-1)
root(ROOT-0, believe-2)
nsubj(agree-5, we-3)
dep(agree-5, both-4)
ccomp(believe-2, agree-5)
complm(rule-12, that-6)
nsubj(rule-12, reality-7)
appos(reality-7, fact-9)
aux(rule-12, must-11)
ccomp(agree-5, rule-12)
det(decision-20, the-13)
