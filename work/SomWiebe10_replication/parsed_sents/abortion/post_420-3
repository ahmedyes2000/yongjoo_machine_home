(ROOT
  (S
    (NP (DT That))
    (VP (VBZ is) (, ,)
      (ADVP (IN after) (DT all))
      (, ,)
      (NP (DT a) (JJ formal) (JJ logical) (NN error)))
    (. .)))

nsubj(error-10, That-1)
cop(error-10, is-2)
advmod(error-10, after-4)
dep(after-4, all-5)
det(error-10, a-7)
amod(error-10, formal-8)
amod(error-10, logical-9)
root(ROOT-0, error-10)

