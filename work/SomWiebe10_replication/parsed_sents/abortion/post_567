      (VP (VBZ means))
      (, ,)
      (VP
        (ADVP (RB fully))
        (VBD formed))
      (, ,)
      (NP
        (NP (PDT all) (DT the) (JJ functional) (NNS organs))
        (PP (IN in)
          (NP (NN operation)))))
    (. .)))

