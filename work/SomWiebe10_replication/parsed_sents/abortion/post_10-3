(ROOT
  (S
    (ADVP (RB Therefore))
    (, ,)
    (NP (NNS fetuses))
    (VP (MD should)
      (VP (VB be)
        (VP (VBN considered)
          (NP (`` ``)
            (NP (JJ unborn) (NNS children))
            ('' '')
            (PP (IN with)
              (NP (VBG correlating) (NNS rights)))))))
    (. .)))

advmod(considered-6, Therefore-1)
nsubjpass(considered-6, fetuses-3)
aux(considered-6, should-4)
auxpass(considered-6, be-5)
root(ROOT-0, considered-6)
amod(children-9, unborn-8)
dobj(considered-6, children-9)
amod(rights-13, correlating-12)
prep_with(children-9, rights-13)

