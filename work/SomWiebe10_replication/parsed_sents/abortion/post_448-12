(ROOT
  (S
    (NP (PRP We))
    (VP (VBP have)
      (ADVP (RB approximately)
        (NP
          (QP (CD 1.2) (CD million))
          (NNS abortions)))
      (VP (VBN performed)
        (PP (IN in)
          (NP (DT the) (NNP U.S.)))
        (NP (DT every) (NN year))))
    (. .)))

nsubj(performed-7, We-1)
aux(performed-7, have-2)
advmod(performed-7, approximately-3)
number(million-5, 1.2-4)
num(abortions-6, million-5)
pobj(approximately-3, abortions-6)
root(ROOT-0, performed-7)
det(U.S.-10, the-9)
prep_in(performed-7, U.S.-10)
det(year-12, every-11)
tmod(performed-7, year-12)

