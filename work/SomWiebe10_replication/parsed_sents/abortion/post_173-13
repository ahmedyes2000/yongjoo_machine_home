(ROOT
  (S
    (NP (PRP You))
    (VP (MD 'd)
      (VP (VB have)
        (S
          (VP (TO to)
            (VP (VB account)
              (PP (IN for)
                (NP
                  (NP
                    (NP (CD 35) (NNS years))
                    (PP (IN of)
                      (NP (NN war))))
                  (CC and)
                  (NP
                    (NP
                      (QP (JJR more) (IN than) (RB just))
                      (DT the) (NNS deaths))
                    (PP (IN of)
                      (NP (DT a) (NN war)))))))))))
    (. .)))

nsubj(have-3, You-1)
xsubj(account-5, You-1)
aux(have-3, 'd-2)
root(ROOT-0, have-3)
aux(account-5, to-4)
xcomp(have-3, account-5)
num(years-8, 35-7)
prep_for(account-5, years-8)
prep_of(years-8, war-10)
quantmod(than-13, more-12)
num(deaths-16, than-13)
quantmod(than-13, just-14)
det(deaths-16, the-15)
prep_for(account-5, deaths-16)
conj_and(years-8, deaths-16)
det(war-19, a-18)
prep_of(deaths-16, war-19)

