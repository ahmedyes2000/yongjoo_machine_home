(ROOT
  (S
    (ADVP (RB No)
      (SBAR
        (WHADVP (WRB where))
        (S
          (VP (VBP have)))))
    (NP (PRP you))
    (VP (VBD refuted)
      (NP (PRP$ my) (NNS explications)))
    (. .)))

advmod(refuted-5, No-1)
advmod(have-3, where-2)
dep(No-1, have-3)
nsubj(refuted-5, you-4)
root(ROOT-0, refuted-5)
poss(explications-7, my-6)
dobj(refuted-5, explications-7)

