(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP (VBP believe)
          (NP (DT that)))))
    (NP (DT the) (NNP Big) (NNP Bang))
    (ADVP (RB even))
    (VP (VBD banged)
      (ADVP (IN at) (DT all)))
    (. .)))

mark(believe-3, If-1)
nsubj(believe-3, you-2)
advcl(banged-9, believe-3)
dobj(believe-3, that-4)
det(Bang-7, the-5)
nn(Bang-7, Big-6)
nsubj(banged-9, Bang-7)
advmod(banged-9, even-8)
root(ROOT-0, banged-9)
advmod(banged-9, at-10)
pobj(at-10, all-11)

