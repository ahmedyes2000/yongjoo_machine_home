(ROOT
  (S
    (S
      (ADVP (RB Again))
      (NP (PRP I))
      (VP (VBP say)))
    (: ;)
    (S
      (SBAR (IN If)
        (S
          (NP (DT a) (NN government))
          (VP (VBZ has)
            (ADVP (RB not) (RB right))
            (S
              (VP (TO to)
                (VP (VB protect)
                  (NP (NN life))))))))
      (, ,)
      (NP (WP what) (NN good))
      (VP (VBZ is)
        (NP (PRP it))))
    (. .)))

advmod(say-3, Again-1)
nsubj(say-3, I-2)
root(ROOT-0, say-3)
mark(has-8, If-5)
det(government-7, a-6)
nsubj(has-8, government-7)
xsubj(protect-12, government-7)
advcl(it-18, has-8)
dep(right-10, not-9)
advmod(has-8, right-10)
aux(protect-12, to-11)
xcomp(has-8, protect-12)
dobj(protect-12, life-13)
dep(good-16, what-15)
nsubj(it-18, good-16)
cop(it-18, is-17)
parataxis(say-3, it-18)

