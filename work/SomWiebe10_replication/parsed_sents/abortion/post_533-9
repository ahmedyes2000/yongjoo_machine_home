(ROOT
  (S
    (SBAR
      (WHNP (WP What))
      (S
        (NP (PRP he))
        (VP (VBZ fails)
          (S
            (VP (TO to)
              (VP (VB take)
                (PP (IN into)
                  (NP (NN account)))))))))
    (VP (VBZ is)
      (NP (NNP Federalism)))
    (. .)))

dep(fails-3, What-1)
nsubj(fails-3, he-2)
xsubj(take-5, he-2)
csubj(Federalism-9, fails-3)
aux(take-5, to-4)
xcomp(fails-3, take-5)
prep_into(take-5, account-7)
cop(Federalism-9, is-8)
root(ROOT-0, Federalism-9)

