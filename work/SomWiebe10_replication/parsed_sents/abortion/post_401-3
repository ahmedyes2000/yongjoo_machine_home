(ROOT
  (S
    (NP (PRP I))
    (VP (VBP 'm)
      (ADVP (RB only))
      (VP (VBG responding)
        (PP (TO to)
          (NP
            (NP (DT this) (NN comment))
            (CC and)
            (NP (DT this) (`` ``) (NN argument) ('' ''))))
        (SBAR (IN as)
          (S
            (VP (VBN defined)
              (PP (IN by)
                (NP (NNP OpposingViews))))))))
    (. .)))

nsubj(responding-4, I-1)
aux(responding-4, 'm-2)
advmod(responding-4, only-3)
root(ROOT-0, responding-4)
det(comment-7, this-6)
