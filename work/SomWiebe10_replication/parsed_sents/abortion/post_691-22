(ROOT
  (NP
    (NP (NNS changes))
    (PP (IN in)
      (NP (DT the) (NNS laws)))
    (PP (IN of)
      (NP (NN descent)
        (CC and)
        (NN distributionc)))
    (. .)))

root(ROOT-0, changes-1)
det(laws-4, the-3)
prep_in(changes-1, laws-4)
prep_of(changes-1, descent-6)
prep_of(changes-1, distributionc-8)
conj_and(descent-6, distributionc-8)

