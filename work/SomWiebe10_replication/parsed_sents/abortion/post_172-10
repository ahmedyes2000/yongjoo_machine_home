(ROOT
  (S
    (ADVP (RB However))
    (, ,)
    (NP (DT the) (NNP Church))
    (VP (VBZ has)
      (ADVP (RB never))
      (VP (VBN stated)
        (PP (IN as)
          (NP
            (NP (DT a) (NN matter))
            (PP (IN of)
              (NP (NN doctrine)))))
        (SBAR (IN that)
          (S
            (NP
              (NP (DT the) (NN War))
              (PP (IN in)
                (NP (NNP Iraq))))
            (VP (VBD was) (RB not)
              (NP (DT a)
                (ADVP (RB Just))
                (NN War)))))))
    (. .)))

advmod(stated-7, However-1)
det(Church-4, the-3)
nsubj(stated-7, Church-4)
aux(stated-7, has-5)
neg(stated-7, never-6)
root(ROOT-0, stated-7)
det(matter-10, a-9)
prep_as(stated-7, matter-10)
prep_of(matter-10, doctrine-12)
complm(War-22, that-13)
det(War-15, the-14)
nsubj(War-22, War-15)
prep_in(War-15, Iraq-17)
cop(War-22, was-18)
neg(War-22, not-19)
det(War-22, a-20)
advmod(War-22, Just-21)
ccomp(stated-7, War-22)

