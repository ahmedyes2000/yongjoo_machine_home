(ROOT
  (S
    (NP
      (NP (DT Those))
      (SBAR
        (WHNP (WP who))
        (S
          (VP (VBP wish)
            (S
              (VP (TO to)
                (VP (VB argue)
                  (NP (DT this)))))))))
    (VP (MD can)
      (ADVP (RB simply))
      (VP (VB research)
        (NP (PRP it))
        (PP (IN for)
          (NP (PRP themselves)))))
    (. .)))

nsubj(wish-3, Those-1)
nsubj(research-9, Those-1)
rcmod(Those-1, wish-3)
aux(argue-5, to-4)
xcomp(wish-3, argue-5)
dobj(argue-5, this-6)
aux(research-9, can-7)
advmod(research-9, simply-8)
root(ROOT-0, research-9)
dobj(research-9, it-10)
prep_for(research-9, themselves-12)

