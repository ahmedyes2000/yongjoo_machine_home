  (SINV
    (VP
      (ADVP (RB Merely))
      (VBG having)
      (NP (JJ human) (NN DNA)))
    (VP (VBZ does) (RB not)
      (VP (VB make)
        (NP (DT a) (NN fetus))))
    (NP (DT a) (NN person))
    (. .)))

