  (S
    (NP (DT A) (NN scientist))
    (VP (MD will)
      (ADVP (RB merely))
      (VP (VB shrug)
        (CC and)
        (VB tell)
        (NP (PRP you))
        (SBAR (IN that)
          (S
            (NP (WDT what))
            (VP (VBZ defines)
              (SBAR
                (S
                  (NP (DT a) (NNS species))
                  (VP (VBZ is)
                    (NP (JJ reproductive) (NN isolation))
                    (PP (RB not)
                      (NP (NNP DNA)))))))))))
    (. .)))

