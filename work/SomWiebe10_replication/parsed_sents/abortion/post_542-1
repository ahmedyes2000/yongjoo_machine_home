(ROOT
  (S
    (VP (VB Check)
      (PP (IN into)
        (NP
          (NP (DT the) (NN amount))
          (PP (IN of)
            (NP
              (NP (NNS couples))
              (SBAR
                (WHNP (WDT that))
                (S
                  (VP (VBP are)
                    (VP (VBG trying)
                      (S
                        (VP (TO to)
                          (VP (VB adopt)
                            (ADVP (RB sometime))))))))))))))
    (. .)))

root(ROOT-0, Check-1)
det(amount-4, the-3)
prep_into(Check-1, amount-4)
prep_of(amount-4, couples-6)
nsubj(trying-9, couples-6)
aux(trying-9, are-8)
rcmod(couples-6, trying-9)
aux(adopt-11, to-10)
xcomp(trying-9, adopt-11)
advmod(adopt-11, sometime-12)

