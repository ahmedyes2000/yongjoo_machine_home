(ROOT
  (S
    (ADVP (RB Furthermore))
    (, ,)
    (VP (VBG allowing)
      (NP (NNS women))
      (S
        (VP (TO to)
          (VP (VB choose)
            (SBAR (IN whether)
              (FRAG (CC or)
                (ADVP (RB not)
                  (S
                    (VP (TO to)
                      (VP (VB receive)
                        (NP (DT an) (NN abortion))))))))))))
    (VP (VBZ is)
      (PP (IN in)
        (NP
          (NP (DT no) (NN way))
          (VP (VBG forcing)
            (NP (DT a) (NN viewpoint))
            (PP (IN onto)
              (NP (DT the) (JJ entire) (NN populace)))))))
    (. .)))

advmod(allowing-3, Furthermore-1)
root(ROOT-0, allowing-3)
dobj(allowing-3, women-4)
aux(choose-6, to-5)
xcomp(allowing-3, choose-6)
complm(not-9, whether-7)
dep(not-9, or-8)
dep(choose-6, not-9)
aux(receive-11, to-10)
dep(not-9, receive-11)
det(abortion-13, an-12)
dobj(receive-11, abortion-13)
dep(allowing-3, is-14)
det(way-17, no-16)
prep_in(is-14, way-17)
partmod(way-17, forcing-18)
det(viewpoint-20, a-19)
dobj(forcing-18, viewpoint-20)
det(populace-24, the-22)
amod(populace-24, entire-23)
prep_onto(forcing-18, populace-24)

