(ROOT
  (S
    (NP
      (NP (DT the) (NN essence))
      (PP (IN of)
        (NP (NN life))))
    (VP (VBZ is) (RB not)
      (PP (IN in)
        (NP
          (SBAR
            (WHNP (WP who))
            (S
              (VP (VBZ gets)
                (S
                  (VP (TO to)
                    (VP (VB live)))))))
          (CC or)
          (SBAR
            (WHNP (WP who))
            (S
              (VP (VBZ gets)
                (S
                  (VP (TO to)
                    (VP (VB die)
                      (PP (CC but)
                        (NP
                          (NP (DT the) (NN worth))
                          (PP (IN of)
                            (NP
                              (NP (DT each) (NN life))
                              (CC both)
                              (NP (JJR older)
                                (CC and)
                                (JJR younger)))))))))))))))
    (. .)))

