(ROOT
  (S
    (S
      (NP (PRP I))
      (VP (MD would)
        (VP (VB carry)
          (NP (DT the) (JJ old) (NN man))
          (SBAR (IN while)
            (S
              (NP (DT the) (JJ old) (NN man))
              (VP (VBZ carries)
                (NP (DT the) (NN baby))))))))
    (CC and)
    (S
      (NP (PRP I))
      (VP (MD 'll)
        (VP (VB die)
          (S
            (VP (VBG trying)
              (S
                (VP (VBG saving)
                  (NP (PRP them))
                  (NP (DT both)))))))))
    (. .)))

