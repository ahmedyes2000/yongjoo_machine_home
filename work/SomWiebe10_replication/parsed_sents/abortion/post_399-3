(ROOT
  (S
    (NP (EX There))
    (VP (MD must)
      (VP (VB be)
        (NP
          (NP (DT some) (JJ scientific) (JJ \/) (JJ logical) (JJ \/) (JJ medical) (NN \/) (NNS criteria))
          (SBAR
            (S
              (NP (PRP we))
              (VP (MD can)
                (VP (VB use)
                  (S
                    (VP (TO to)
                      (VP (VB determine)
                        (NP (DT this))))))))))))
    (. .)))

expl(criteria-11, There-1)
aux(criteria-11, must-2)
cop(criteria-11, be-3)
det(criteria-11, some-4)
amod(criteria-11, scientific-5)
amod(criteria-11, \/-6)
amod(criteria-11, logical-7)
amod(criteria-11, \/-8)
amod(criteria-11, medical-9)
nn(criteria-11, \/-10)
root(ROOT-0, criteria-11)
dobj(use-14, criteria-11)
nsubj(use-14, we-12)
aux(use-14, can-13)
rcmod(criteria-11, use-14)
aux(determine-16, to-15)
