  (S (CC And)
    (PP (IN of)
      (NP (NN course)))
    (NP (PRP it))
    (VP (VBZ 's) (RB not)
      (ADJP (JJ ok)
        (S
          (VP (TO to)
            (VP (VB kill)
              (NP (PRP$ your) (NN neighbor)))))))
    (. .)))

