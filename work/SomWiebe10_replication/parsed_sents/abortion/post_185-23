(ROOT
  (S
    (ADVP (RBS Most) (RB recently))
    (PRN (, ,)
      (S
        (NP (DT this))
        (VP (VBD created)
          (NP (DT a) (NN break))
          (PP (IN in)
            (NP (DT the) (NNP Church)))
          (PP (IN with)
            (NP (JJ false) (NNS promises)))
          (PP (TO to)
            (NP
              (NP (DT those))
              (VP (VBG wanting)
                (NP (JJR more)))))))
      (, ,))
    (ADVP (RB so))
    (VP (VBD called)
      (NP (NN freedom))
      (PP (IN from)
        (NP
          (NP (DT the) (NNS teachings))
          (PP (IN of)
            (NP (DT the) (NNP Church))))))
    (. .)))

advmod(recently-2, Most-1)
advmod(called-20, recently-2)
nsubj(created-5, this-4)
parataxis(called-20, created-5)
det(break-7, a-6)
dobj(created-5, break-7)
det(Church-10, the-9)
prep_in(created-5, Church-10)
amod(promises-13, false-12)
prep_with(created-5, promises-13)
prep_to(created-5, those-15)
partmod(those-15, wanting-16)
dobj(wanting-16, more-17)
advmod(called-20, so-19)
root(ROOT-0, called-20)
dobj(called-20, freedom-21)
det(teachings-24, the-23)
prep_from(called-20, teachings-24)
det(Church-27, the-26)
prep_of(teachings-24, Church-27)

