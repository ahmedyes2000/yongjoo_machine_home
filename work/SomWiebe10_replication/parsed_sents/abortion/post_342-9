(ROOT
  (S
    (ADVP (RB Morality))
    (, ,)
    (ADVP (RB then))
    (, ,)
    (VP (MD can)
      (VP (VB be)
        (VP (VBN turned)
          (PRT (RP on)
            (CC and)
            (RP off))
          (PP (VBG according)
            (PP (TO to)
              (SBAR
                (WHNP (WDT whatever))
                (S
                  (VP (VBZ suits)
                    (NP (DT the) (NN mood))))))))))
    (. .)))

advmod(turned-7, Morality-1)
advmod(turned-7, then-3)
aux(turned-7, can-5)
auxpass(turned-7, be-6)
root(ROOT-0, turned-7)
prt(turned-7, on-8)
dep(on-8, and-9)
dep(on-8, off-10)
prepc_according_to(turned-7, to-12)
nsubj(suits-14, whatever-13)
pcomp(turned-7, suits-14)
det(mood-16, the-15)
dobj(suits-14, mood-16)

