(ROOT
  (S
    (S
      (NP (DT the) (NN man))
      (VP (VBZ wants)
        (NP (DT the) (NN child))))
    (, ,)
    (CC and)
    (S
      (NP (DT the) (NN woman))
      (VP (VBZ does) (RB n't)))
    (. .)))

det(man-2, the-1)
nsubj(wants-3, man-2)
root(ROOT-0, wants-3)
det(child-5, the-4)
dobj(wants-3, child-5)
det(woman-9, the-8)
nsubj(does-10, woman-9)
conj_and(wants-3, does-10)
neg(does-10, n't-11)

