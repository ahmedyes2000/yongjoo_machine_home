(ROOT
  (S
    (NP
      (NP (DT Any) (NN clarification))
      (SBAR
        (S
          (NP (PRP you))
          (VP (MD could)
            (VP (VB give))))))
    (VP (MD would)
      (VP (VB be)
        (VP (VBN appreciated))))
    (. .)))

det(clarification-2, Any-1)
dobj(give-5, clarification-2)
nsubjpass(appreciated-8, clarification-2)
nsubj(give-5, you-3)
aux(give-5, could-4)
rcmod(clarification-2, give-5)
aux(appreciated-8, would-6)
auxpass(appreciated-8, be-7)
root(ROOT-0, appreciated-8)

