  (S
    (SBAR
      (ADVP (RB Often))
      (WHADVP (WRB when))
      (S
        (NP (JJ young) (NNS girls))
        (VP (VBP are)
          (VP (VBN raped)))))
    (, ,)
    (NP (PRP they))
    (VP (MD will) (RB not)
      (VP (VB tell)
        (NP
          (NP
            (NP (NN anyone))
            (SBAR (IN because)
              (S
                (NP (PRP they))
                (VP (VBP are)
                  (ADJP (JJ afraid))))))
          (CC or)
          (NP
            (NP (DT a) (NN multitude))
            (PP (IN of)
              (NP (JJ other) (NNS reasons)))))))
    (. .)))

advmod(raped-6, Often-1)
advmod(raped-6, when-2)
amod(girls-4, young-3)
