(ROOT
  (S
    (SBAR (IN Unless)
      (S
        (NP (PRP it))
        (VP (VBZ 's)
          (NP
            (NP (PRP$ your) (NN wife))
            (, ,)
            (NP (NN daughter))
            (, ,)
            (CC or)
            (NP (DT another) (NN relative))))))
    (NP (PRP I))
    (ADVP (RB really))
    (VP (MD ca) (RB n't)
      (VP (VB see)
        (SBAR
          (WHADVP (WRB why))
          (S
            (NP (PRP you))
            (VP (VBP have)
              (NP (DT any) (NN right)
                (S
                  (VP (TO to)
                    (VP (VB interfere))))))))))
    (. .)))

mark(wife-5, Unless-1)
nsubj(wife-5, it-2)
cop(wife-5, 's-3)
poss(wife-5, your-4)
advcl(see-16, wife-5)
conj_or(wife-5, daughter-7)
advcl(see-16, daughter-7)
det(relative-11, another-10)
conj_or(wife-5, relative-11)
advcl(see-16, relative-11)
nsubj(see-16, I-12)
advmod(see-16, really-13)
aux(see-16, ca-14)
neg(see-16, n't-15)
root(ROOT-0, see-16)
advmod(have-19, why-17)
nsubj(have-19, you-18)
advcl(see-16, have-19)
det(right-21, any-20)
dobj(have-19, right-21)
aux(interfere-23, to-22)
infmod(right-21, interfere-23)

