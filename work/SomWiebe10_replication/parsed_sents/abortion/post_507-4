(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP
          (NP (NN half))
          (PP (IN of)
            (NP (DT the) (NN country))))
        (VP (MD would)
          (VP (VB oppose)
            (NP (DT this))))))
    (, ,)
    (ADVP (RB then))
    (NP (PRP it))
    (VP (MD can) (RB not)
      (VP (VB be)
        (ADJP (JJ consistent)
          (PP (IN with)
            (NP (PRP$ our) (NNS countries) (NNS values))))))
    (. .)))

mark(oppose-7, If-1)
nsubj(oppose-7, half-2)
det(country-5, the-4)
prep_of(half-2, country-5)
aux(oppose-7, would-6)
advcl(consistent-15, oppose-7)
dobj(oppose-7, this-8)
advmod(consistent-15, then-10)
nsubj(consistent-15, it-11)
aux(consistent-15, can-12)
neg(consistent-15, not-13)
cop(consistent-15, be-14)
root(ROOT-0, consistent-15)
poss(values-19, our-17)
nn(values-19, countries-18)
prep_with(consistent-15, values-19)

