(ROOT
  (S (-LRB- -LRB-)
    (S
      (NP (PRP We))
      (VP (VBP are)
        (VP (VBN programmed)
          (PP (TO to)
            (NP (NNP reproduced))))))
    (: ;)
    (S
      (NP (DT that))
      (VP (VBZ 's)
        (SBAR
          (WHADVP (WRB how))
          (S
            (NP (PRP$ our) (NN specie))
            (VP (VBD survived))))))
    (. .) (-RRB- -RRB-)))

nsubjpass(programmed-4, We-2)
auxpass(programmed-4, are-3)
root(ROOT-0, programmed-4)
prep_to(programmed-4, reproduced-6)
nsubj('s-9, that-8)
parataxis(programmed-4, 's-9)
advmod(survived-13, how-10)
poss(specie-12, our-11)
nsubj(survived-13, specie-12)
ccomp('s-9, survived-13)

