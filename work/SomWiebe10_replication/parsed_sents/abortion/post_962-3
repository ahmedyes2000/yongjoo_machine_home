  (S
    (VP (VB Do) (RB n't)
      (ADVP (RB just))
      (VP (VB look)
        (PP (IN for)
          (NP
            (NP (DT the) (JJ easy) (NN way))
            (PP (RB out) (RB because) (IN of)
              (NP (PRP$ your) (NN mistake)))))))
    (. !)))

