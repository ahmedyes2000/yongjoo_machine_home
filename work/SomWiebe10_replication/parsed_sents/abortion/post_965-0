(ROOT
  (S
    (S (IN For)
      (NP (PRP You))
      (VP (VBD created)
        (NP
          (NP (PRP$ my) (NN inmost))
          (VP (VBG being)))))
    (: ;)
    (S
      (NP (PRP You))
      (VP (VBD knit)
        (NP (PRP me))
        (ADVP (RB together))
        (PP (IN in)
          (NP
            (NP (PRP$ my) (NN mother) (POS 's))
            (NN womb)))))
    (. .)))

dep(created-3, For-1)
nsubj(created-3, You-2)
root(ROOT-0, created-3)
poss(inmost-5, my-4)
dobj(created-3, inmost-5)
partmod(inmost-5, being-6)
nsubj(knit-9, You-8)
parataxis(created-3, knit-9)
dobj(knit-9, me-10)
advmod(knit-9, together-11)
poss(mother-14, my-13)
poss(womb-16, mother-14)
prep_in(knit-9, womb-16)

