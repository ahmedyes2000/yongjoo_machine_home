(ROOT
  (S
    (NP (NNP Fruit) (NNS flies))
    (VP (VBP are)
      (ADJP (JJ hard)
        (S
          (VP (TO to)
            (VP (VB invest)
              (PP (IN with)
                (NP (JJ much) (NN importance))))))))
    (. .)))

nn(flies-2, Fruit-1)
