(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP it))
        (VP (VBZ is)
          (ADJP (JJ alive))
          (, ,)
          (PP (IN than)
            (S
              (VP (VBG killing)
                (NP (PRP it))))))))
    (VP (VBZ is)
      (NP (NN murder)))
    (. .)))

mark(alive-4, If-1)
nsubj(alive-4, it-2)
cop(alive-4, is-3)
csubj(murder-10, alive-4)
prepc_than(alive-4, killing-7)
dobj(killing-7, it-8)
cop(murder-10, is-9)
root(ROOT-0, murder-10)

