(ROOT
  (S
    (NP (DT No) (NN one))
    (VP (VBZ cries)
      (PP (IN for)
        (NP
          (NP (DT the) (NNS lives))
          (PP (IN of)
            (NP (DT the) (JJ other)
              (QP (CD billion) (CD sperm)))))))
    (. .)))

det(one-2, No-1)
nsubj(cries-3, one-2)
root(ROOT-0, cries-3)
det(lives-6, the-5)
prep_for(cries-3, lives-6)
det(sperm-11, the-8)
amod(sperm-11, other-9)
number(sperm-11, billion-10)
prep_of(lives-6, sperm-11)

