(ROOT
  (S
    (NP (PRP He))
    (VP (VBZ is)
      (ADVP (RB still))
      (ADJP (RB legally) (JJ liable)
        (PP (TO to)
          (NP (DT this) (NN child)))))
    (. .)))

nsubj(liable-5, He-1)
cop(liable-5, is-2)
advmod(liable-5, still-3)
advmod(liable-5, legally-4)
root(ROOT-0, liable-5)
det(child-8, this-7)
prep_to(liable-5, child-8)

