(ROOT
  (S
    (NP (PRP He))
    (VP (VBD received)
      (NP (DT an) (NN Award))
      (PP (IN for)
        (NP (NNP Excellence)))
      (PP (IN in)
        (NP
          (NP (NNP Undergraduate) (NNP Teaching))
          (PP (IN in)
            (NP (CD 1971))))))
    (. .)))

nsubj(received-2, He-1)
root(ROOT-0, received-2)
det(Award-4, an-3)
dobj(received-2, Award-4)
prep_for(received-2, Excellence-6)
nn(Teaching-9, Undergraduate-8)
prep_in(received-2, Teaching-9)
