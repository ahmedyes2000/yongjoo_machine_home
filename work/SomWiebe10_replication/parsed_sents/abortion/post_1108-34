(ROOT
  (S
    (S
      (ADVP (RB Tricky))
      (NP
        (NP (NN thing))
        (PP (IN about)
          (NP (NNS morals)))))
    (, ,)
    (NP (PRP they))
    (VP (VBP 're) (RB all)
      (ADJP (JJ relative)))
    (. .)))

advmod(thing-2, Tricky-1)
ccomp(relative-9, thing-2)
prep_about(thing-2, morals-4)
nsubj(relative-9, they-6)
cop(relative-9, 're-7)
advmod(relative-9, all-8)
root(ROOT-0, relative-9)

