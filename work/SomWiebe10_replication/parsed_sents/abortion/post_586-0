(ROOT
  (S
    (NP (PRP You))
    (ADVP (RB simply))
    (VP (VB restate)
      (NP (PRP$ your) (NN opinion))
      (SBAR (IN that)
        (S
          (NP
            (NP (DT a) (JJ new) (NN member))
            (PP (IN of)
              (NP (DT the) (NN species))))
          (VP (VBZ is)
            (VP (VBN created)
              (PP (IN at)
                (NP (NN conception))))))))
    (. .)))

nsubj(restate-3, You-1)
advmod(restate-3, simply-2)
root(ROOT-0, restate-3)
poss(opinion-5, your-4)
dobj(restate-3, opinion-5)
complm(created-14, that-6)
det(member-9, a-7)
amod(member-9, new-8)
nsubjpass(created-14, member-9)
det(species-12, the-11)
prep_of(member-9, species-12)
auxpass(created-14, is-13)
ccomp(restate-3, created-14)
prep_at(created-14, conception-16)

