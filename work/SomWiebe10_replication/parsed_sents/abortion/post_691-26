(ROOT
  (NP
    (NP (DT all) (NNS sorts))
    (PP (IN of)
      (NP (NNS issues)))
    (PP (IN in)
      (NP (NN automobile) (NN negligence)
        (CC and)
        (JJ other) (NN tort) (NNS lawsuits)))
    (. .)))

det(sorts-2, all-1)
root(ROOT-0, sorts-2)
prep_of(sorts-2, issues-4)
nn(negligence-7, automobile-6)
prep_in(sorts-2, negligence-7)
amod(lawsuits-11, other-9)
nn(lawsuits-11, tort-10)
prep_in(sorts-2, lawsuits-11)
conj_and(negligence-7, lawsuits-11)

