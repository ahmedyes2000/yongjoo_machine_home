(ROOT
  (S
    (SBAR
      (WHADVP (WRB When))
      (S
        (NP (PRP I))
        (VP (VBD was)
          (VP (VBN woven)
            (ADVP (RB together))
            (PP (IN in)
              (NP
                (NP (DT the) (NNS depths))
                (PP (IN of)
                  (NP (DT the) (NN earth)))))))))
    (, ,)
    (NP (PRP$ Your) (NNS eyes))
    (VP (VBD saw)
      (NP (PRP$ my) (JJ unformed) (NN body)))
    (. .)))

advmod(woven-4, When-1)
nsubjpass(woven-4, I-2)
auxpass(woven-4, was-3)
advcl(saw-15, woven-4)
advmod(woven-4, together-5)
det(depths-8, the-7)
prep_in(woven-4, depths-8)
det(earth-11, the-10)
prep_of(depths-8, earth-11)
poss(eyes-14, Your-13)
nsubj(saw-15, eyes-14)
root(ROOT-0, saw-15)
poss(body-18, my-16)
amod(body-18, unformed-17)
