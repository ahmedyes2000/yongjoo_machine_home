(ROOT
  (S
    (NP (EX There))
    (VP (VBP are)
      (NP
        (NP (JJ many) (NNS Americans))
        (, ,)
        (NP
          (NP (DT all))
          (PP (IN with)
            (NP (JJ different) (NNS beliefs))))
        (, ,)
        (NP (NNS customs))
        (, ,)
        (CC and)
        (NP (NNS traditions))))
    (. .)))

