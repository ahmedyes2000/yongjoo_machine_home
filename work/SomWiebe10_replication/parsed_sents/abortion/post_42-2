(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (NN infanticide))
        (VP (VBZ is)
          (NP (NN murder)))))
    (, ,)
    (ADVP (RB so) (RB too))
    (VP (MD must)
      (VP (VB be)
        (NP (NN abortion))))
    (. .)))

mark(murder-4, If-1)
nsubj(murder-4, infanticide-2)
cop(murder-4, is-3)
advcl(abortion-10, murder-4)
advmod(too-7, so-6)
advmod(abortion-10, too-7)
aux(abortion-10, must-8)
cop(abortion-10, be-9)
root(ROOT-0, abortion-10)

