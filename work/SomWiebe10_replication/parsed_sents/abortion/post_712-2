(ROOT
  (S
    (NP (NNP God))
    (VP (VBZ says)
      (PP (IN before)
        (NP
          (NP (DT the) (NN creation))
          (PP (IN of)
            (NP
              (NP (DT the) (NN world))
              (SBAR
                (S
                  (NP (PRP He))
                  (VP (VBD knew)
                    (NP (PRP me))))))))))
    (. .)))

nsubj(says-2, God-1)
root(ROOT-0, says-2)
det(creation-5, the-4)
prep_before(says-2, creation-5)
det(world-8, the-7)
prep_of(creation-5, world-8)
nsubj(knew-10, He-9)
rcmod(world-8, knew-10)
dobj(knew-10, me-11)

(ROOT
  (S
    (NP (NNP Life))
    (VP (VBZ begins)
      (SBAR
        (WHADVP (WRB when))
        (S
          (NP (NNP God))
          (VP (VBD planned)
            (NP (PRP$ my) (NN life))))))
    (. .)))

nsubj(begins-2, Life-1)
root(ROOT-0, begins-2)
advmod(planned-5, when-3)
nsubj(planned-5, God-4)
advcl(begins-2, planned-5)
poss(life-7, my-6)
dobj(planned-5, life-7)

