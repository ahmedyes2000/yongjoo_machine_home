(ROOT
  (S
    (NP (PRP I))
    (ADVP (RB just))
    (VP (VBD recommended)
      (SBAR
        (S
          (NP (PRP you)
            (CC and)
            (PRP I))
          (ADVP (RB totally))
          (VP (VBD did) (RB n't)
            (VP (VB mean)
              (PP (TO to)))))))
    (. .)))

nsubj(recommended-3, I-1)
advmod(recommended-3, just-2)
root(ROOT-0, recommended-3)
nsubj(mean-10, you-4)
conj_and(you-4, I-6)
nsubj(mean-10, I-6)
advmod(mean-10, totally-7)
aux(mean-10, did-8)
neg(mean-10, n't-9)
ccomp(recommended-3, mean-10)
prep(mean-10, to-11)

