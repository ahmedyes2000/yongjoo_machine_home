(ROOT
  (S ('' '')
    (NP (DT This))
    (ADVP (RB war)
      (PP (IN against)
        (NP (DT the) (NN child))))
    (VP (VBZ is)
      (NP (NN abortion)))
    (. .)))

nsubj(abortion-8, This-2)
advmod(abortion-8, war-3)
det(child-6, the-5)
prep_against(war-3, child-6)
cop(abortion-8, is-7)
root(ROOT-0, abortion-8)

