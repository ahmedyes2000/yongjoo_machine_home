(ROOT
  (S
    (NP (PRP I))
    (ADVP (RB also))
    (VP (VBP recognize)
      (NP (DT a) (JJ newborn) (NN baby))
      (PP (IN as)
        (NP (DT a) (NN person)))
      (, ,)
      (SBAR (RB even) (IN though)
        (S
          (NP (PRP it))
          (VP (VBZ does) (RB n't)
            (ADVP (RB yet))
            (VP (VB speak)
              (NP
                (NP (DT a) (NN language))
                (SBAR
                  (S
                    (NP (PRP I))
                    (VP (VBP know))))))))))
    (. .)))

nsubj(recognize-3, I-1)
advmod(recognize-3, also-2)
root(ROOT-0, recognize-3)
det(baby-6, a-4)
amod(baby-6, newborn-5)
dobj(recognize-3, baby-6)
det(person-9, a-8)
prep_as(recognize-3, person-9)
advmod(speak-17, even-11)
dep(speak-17, though-12)
nsubj(speak-17, it-13)
aux(speak-17, does-14)
neg(speak-17, n't-15)
advmod(speak-17, yet-16)
dep(recognize-3, speak-17)
det(language-19, a-18)
dobj(speak-17, language-19)
dobj(know-21, language-19)
nsubj(know-21, I-20)
rcmod(language-19, know-21)

