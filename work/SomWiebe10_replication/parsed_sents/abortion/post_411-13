(ROOT
  (S
    (NP (DT Each) (NN mother))
    (VP (VBZ gets)
      (S
        (VP (TO to)
          (VP (VB decide)
            (SBAR
              (WHADVP (WRB when))
              (S
                (NP (PRP$ her) (NN pregnancy))
                (VP (VBZ changes)
                  (PP (IN from)
                    (NP (DT a) (NN non-human)))
                  (PP (TO to)
                    (NP (DT a) (NN human))))))))))
    (. ?)))

det(mother-2, Each-1)
nsubj(gets-3, mother-2)
xsubj(decide-5, mother-2)
root(ROOT-0, gets-3)
aux(decide-5, to-4)
