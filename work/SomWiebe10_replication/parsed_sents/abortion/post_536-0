(ROOT
  (S
    (NP (PRP I))
    (VP (VBP 'm)
      (ADJP (RB just) (JJ thankful)
        (SBAR
          (S
            (NP (PRP$ my) (NN mother))
            (VP (VBD did) (RB n't)
              (VP (VB take)
                (NP (PRP$ my) (NN life))
                (ADVP (RB away))))))))
    (. .)))

nsubj(thankful-4, I-1)
cop(thankful-4, 'm-2)
advmod(thankful-4, just-3)
root(ROOT-0, thankful-4)
poss(mother-6, my-5)
nsubj(take-9, mother-6)
aux(take-9, did-7)
neg(take-9, n't-8)
dep(thankful-4, take-9)
poss(life-11, my-10)
dobj(take-9, life-11)
advmod(take-9, away-12)

