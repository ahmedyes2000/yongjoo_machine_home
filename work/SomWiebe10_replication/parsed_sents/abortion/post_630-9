(ROOT
  (S (: ...)
    (NP (WDT which))
    (VP (VBZ is)
      (SBAR
        (WHADVP (WRB why))
        (S
          (NP (PRP I))
          (VP (VBD chose)
            (NP (PRP it))))))
    (. .)))

nsubj(is-3, which-2)
root(ROOT-0, is-3)
advmod(chose-6, why-4)
nsubj(chose-6, I-5)
advcl(is-3, chose-6)
dobj(chose-6, it-7)

