(ROOT
  (S
    (NP (NNS People))
    (VP (VBP are)
      (VP (VBN viewed)
        (PP (IN as)
          (NP (NNS objects)))
        (S
          (VP (TO to)
            (VP (VB be)
              (VP
                (VP (VBN used)
                  (PP (IN for)
                    (NP (JJ personal) (NN pleasure))))
                (CC and)
                (VP
                  (ADVP (RB then))
                  (VBN discarded)
                  (SBAR
                    (WHADVP (WRB when))
                    (S
                      (NP (PRP you))
                      (VP (VBP are)
                        (VP (VBN done)
                          (PP (IN with)
                            (NP (PRP them))))))))))))))
    (. .)))

nsubjpass(viewed-3, People-1)
xsubj(used-8, People-1)
auxpass(viewed-3, are-2)
root(ROOT-0, viewed-3)
prep_as(viewed-3, objects-5)
aux(used-8, to-6)
auxpass(used-8, be-7)
xcomp(viewed-3, used-8)
amod(pleasure-11, personal-10)
prep_for(used-8, pleasure-11)
advmod(discarded-14, then-13)
xcomp(viewed-3, discarded-14)
conj_and(used-8, discarded-14)
advmod(done-18, when-15)
nsubjpass(done-18, you-16)
auxpass(done-18, are-17)
advcl(discarded-14, done-18)
prep_with(done-18, them-20)

