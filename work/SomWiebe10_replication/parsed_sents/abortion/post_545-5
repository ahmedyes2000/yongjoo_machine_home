(ROOT
  (S
    (ADVP (JJ Fourth))
    (, ,)
    (NP (PRP we))
    (VP (VBP have)
      (NP (NN responsibility))
      (SBAR (RB only) (IN after)
        (S
          (NP (PRP we))
          (VP (VBP have)
            (VP (VBN made)
              (NP
                (NP (DT the) (JJ personal) (NN choice))
                (PP (IN of)
                  (S
                    (VP (VBG keeping)
                      (NP (DT a) (VBN fertilized) (NN egg)))))))))))
    (. .)))

advmod(have-4, Fourth-1)
nsubj(have-4, we-3)
root(ROOT-0, have-4)
dobj(have-4, responsibility-5)
advmod(made-10, only-6)
dep(made-10, after-7)
nsubj(made-10, we-8)
aux(made-10, have-9)
dep(have-4, made-10)
det(choice-13, the-11)
