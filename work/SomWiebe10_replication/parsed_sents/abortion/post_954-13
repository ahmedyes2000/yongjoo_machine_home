  (S (CC And)
    (PP (IN through)
      (NP (PRP$ His) (NN resurrection)))
    (NP (PRP you))
    (VP (VBP are)
      (VP (VBN raised)
        (PP (TO to)
          (NP
            (NP (NNS newness))
            (PP (IN of)
              (NP (NN life)))))))
    (. .)))

