(ROOT
  (S
    (NP (NNP Abortion))
    (VP (VBZ is)
      (NP
        (NP (DT a) (JJ difficult) (NN decision))
        (, ,)
        (NP
          (NP (DT no) (NN matter))
          (NP
            (NP (DT the) (NN individual) (POS 's))
            (NNS circumstances)
            (CC or)
            (NNS beliefs)))))
    (. .)))

nsubj(decision-5, Abortion-1)
cop(decision-5, is-2)
det(decision-5, a-3)
amod(decision-5, difficult-4)
root(ROOT-0, decision-5)
det(matter-8, no-7)
appos(decision-5, matter-8)
det(individual-10, the-9)
poss(circumstances-12, individual-10)
dep(matter-8, circumstances-12)
dep(matter-8, beliefs-14)
conj_or(circumstances-12, beliefs-14)

