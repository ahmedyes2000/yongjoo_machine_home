(ROOT
  (S
    (ADVP (RB However))
    (: ;)
    (NP (DT the) (NN argument))
    (VP (VBZ is)
      (ADJP (RB purely) (JJ logical)))
    (. .)))

advmod(logical-7, However-1)
det(argument-4, the-3)
nsubj(logical-7, argument-4)
cop(logical-7, is-5)
advmod(logical-7, purely-6)
root(ROOT-0, logical-7)

