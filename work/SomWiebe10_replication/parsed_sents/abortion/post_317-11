(ROOT
  (S
    (NP (PRP You))
    (VP (VBD nor)
      (SBAR
        (S
          (NP (PRP I))
          (VP (VBP are)
            (ADJP (JJ dependent)
              (PP (IN for)
                (NP (NN life))))
            (PP (IN upon)
              (NP (NN anyone) (RB else)))))))
    (. .)))

nsubj(nor-2, You-1)
root(ROOT-0, nor-2)
nsubj(dependent-5, I-3)
cop(dependent-5, are-4)
ccomp(nor-2, dependent-5)
prep_for(dependent-5, life-7)
prep_upon(dependent-5, anyone-9)
advmod(anyone-9, else-10)

