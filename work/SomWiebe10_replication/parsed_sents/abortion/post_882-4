(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is)
      (NP (DT a) (NN fact))
      (SBAR (IN that)
        (S
          (NP (JJ unplanned\/unwanted) (NNS children))
          (VP (VBP are)
            (ADJP (RBR more) (JJ likely)
              (S
                (VP (TO to)
                  (VP (VB be)
                    (VP (VBN subjected)
                      (PP (TO to)
                        (NP (NN abuse)
                          (CC and)
                          (NN neglect))))))))
            (: --)
            (S
              (VP (VBG continuing)
                (NP (DT the) (NN cycle))))))))
    (. .)))

nsubj(fact-4, It-1)
cop(fact-4, is-2)
det(fact-4, a-3)
root(ROOT-0, fact-4)
complm(likely-10, that-5)
amod(children-7, unplanned\/unwanted-6)
nsubj(likely-10, children-7)
cop(likely-10, are-8)
advmod(likely-10, more-9)
ccomp(fact-4, likely-10)
aux(subjected-13, to-11)
auxpass(subjected-13, be-12)
xcomp(likely-10, subjected-13)
prep_to(subjected-13, abuse-15)
prep_to(subjected-13, neglect-17)
conj_and(abuse-15, neglect-17)
parataxis(likely-10, continuing-19)
xcomp(likely-10, continuing-19)
det(cycle-21, the-20)
dobj(continuing-19, cycle-21)

