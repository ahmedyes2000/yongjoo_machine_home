(ROOT
  (S
    (NP (DT Another) (NN situation))
    (VP (VBZ is)
      (ADVP (RB sometimes))
      (NP
        (NP (NNS mothers))
        (SBAR
          (S
            (VP (VBP are)
              (VP (VBN diagnosed)
                (PP (IN with)
                  (NP
                    (NP (NN cancer) (RB late))
                    (PP (IN in)
                      (NP (PRP$ their) (NNS pregnancies)))))
                (PP (IN beyond)
                  (NP
                    (NP (DT the) (NN date))
                    (PP (IN of)
                      (NP (JJ normal) (NNS abortions)))))))))))
    (. .)))

det(situation-2, Another-1)
nsubj(mothers-5, situation-2)
cop(mothers-5, is-3)
advmod(mothers-5, sometimes-4)
root(ROOT-0, mothers-5)
auxpass(diagnosed-7, are-6)
rcmod(mothers-5, diagnosed-7)
prep_with(diagnosed-7, cancer-9)
advmod(cancer-9, late-10)
poss(pregnancies-13, their-12)
prep_in(cancer-9, pregnancies-13)
det(date-16, the-15)
prep_beyond(diagnosed-7, date-16)
amod(abortions-19, normal-18)
prep_of(date-16, abortions-19)

