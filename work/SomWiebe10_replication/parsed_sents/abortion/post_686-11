(ROOT
  (S
    (NP (PRP I))
    (VP (VBP know)
      (SBAR (IN that)
        (S
          (NP (NN sperm))
          (VP (VBP are)
            (VP (VBN affected)
              (PP (IN by)
                (NP
                  (NP (JJ outside) (NNS chemicals))
                  (SBAR
                    (WHNP (WDT which))
                    (S
                      (VP (MD can)
                        (VP (VB cause)
                          (S
