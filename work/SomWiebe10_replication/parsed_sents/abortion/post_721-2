(ROOT
  (S
    (NP (PRP It))
    (VP (MD should) (RB not)
      (VP (VB be)
        (VP (VBN used)
          (PP (IN as)
            (NP
              (NP (DT a) (NN form))
              (PP (IN of)
                (NP (NN birth) (NN control))))))))
    (. .)))

nsubjpass(used-5, It-1)
aux(used-5, should-2)
neg(used-5, not-3)
auxpass(used-5, be-4)
root(ROOT-0, used-5)
det(form-8, a-7)
prep_as(used-5, form-8)
nn(control-11, birth-10)
prep_of(form-8, control-11)

