(ROOT
  (S (IN For)
    (NP (NNS others))
    (VP (MD will)
      (VP (VB treat)
        (NP (PRP you))
        (SBAR (IN as)
          (S
            (NP (PRP you))
            (VP (VBP treat)
              (NP (PRP them)))))))
    (. .)))

dep(treat-4, For-1)
nsubj(treat-4, others-2)
aux(treat-4, will-3)
root(ROOT-0, treat-4)
dobj(treat-4, you-5)
mark(treat-8, as-6)
nsubj(treat-8, you-7)
advcl(treat-4, treat-8)
dobj(treat-8, them-9)

