(ROOT
  (S
    (NP (PRP it))
    (ADVP (RB simply))
    (VP (VBZ means)
      (NP
        (NP (DT a) (NN change))
        (PP (IN in)
          (NP (NN procedure)))))
    (. .)))

nsubj(means-3, it-1)
advmod(means-3, simply-2)
root(ROOT-0, means-3)
det(change-5, a-4)
dobj(means-3, change-5)
prep_in(change-5, procedure-7)

