(ROOT
  (S (CC And)
    (SBAR (RB just)
      (S
        (VP (TO to)
          (VP (VB be)
            (ADJP (JJ clear))))))
    (, ,)
    (NP (DT an) (JJ unjustified) (NN action))
    (VP (VBZ is)
      (ADJP (JJ unjust)))
    (. .)))

cc(unjust-11, And-1)
advmod(clear-5, just-2)
aux(clear-5, to-3)
cop(clear-5, be-4)
dep(unjust-11, clear-5)
det(action-9, an-7)
amod(action-9, unjustified-8)
nsubj(unjust-11, action-9)
cop(unjust-11, is-10)
root(ROOT-0, unjust-11)

