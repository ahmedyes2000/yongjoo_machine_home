(ROOT
  (S
    (ADVP (RB However))
    (, ,)
    (NP (DT the) (NN issue))
    (VP (VBZ is) (RB not)
      (SBAR (IN whether)
        (S
          (NP (NNS women))
          (VP (VBP suffer)))))
    (. .)))

advmod(is-5, However-1)
det(issue-4, the-3)
nsubj(is-5, issue-4)
root(ROOT-0, is-5)
neg(is-5, not-6)
complm(suffer-9, whether-7)
nsubj(suffer-9, women-8)
ccomp(is-5, suffer-9)

