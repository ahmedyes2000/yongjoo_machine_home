(ROOT
  (S (CC And)
    (VP (MD will)
      (ADVP (RB daily))
      (VP
        (VP (VB seek)
          (NP (NN expiation)))
        (, ,)
        (CC or)
        (VP (VB seek)
          (S
            (VP (TO to)
              (VP (VB justify)
                (NP (PRP herself))))))))
    (. .)))

aux(seek-4, will-2)
advmod(seek-4, daily-3)
root(ROOT-0, seek-4)
dobj(seek-4, expiation-5)
conj_or(seek-4, seek-8)
aux(justify-10, to-9)
xcomp(seek-8, justify-10)
dobj(justify-10, herself-11)

