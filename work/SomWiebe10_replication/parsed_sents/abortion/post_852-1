(ROOT
  (S
    (S
      (NP (NN Sex))
      (VP (VBZ is)
        (ADJP (JJ pleasurable))))
    (, ,)
    (CC but)
    (S
      (NP (PRP it))
      (VP (VBZ is)
        (ADVP (RB still))
        (NP (NN procreation))))
    (. .)))

nsubj(pleasurable-3, Sex-1)
cop(pleasurable-3, is-2)
root(ROOT-0, pleasurable-3)
nsubj(procreation-9, it-6)
cop(procreation-9, is-7)
advmod(procreation-9, still-8)
conj_but(pleasurable-3, procreation-9)

