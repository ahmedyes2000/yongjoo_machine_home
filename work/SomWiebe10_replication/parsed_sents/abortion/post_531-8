(ROOT
  (S
    (NP
      (NP (DT The) (NN issue))
      (PP (IN of)
        (S
          (VP (VBG living)
            (PP (IN under)
              (NP
                (NP (NNS laws))
                (SBAR
                  (WHNP (WDT which))
                  (S
                    (NP (PRP one))
                    (VP (VBZ disagrees)
                      (PP (IN with)))))))))))
    (VP (VBZ is)
      (PP (IN in)
        (NP
          (NP (PRP$ my) (NN opinion))
          (NP (DT an) (JJ erroneous) (NN one)))))
    (. .)))

det(issue-2, The-1)
nsubj(is-11, issue-2)
prepc_of(issue-2, living-4)
prep_under(living-4, laws-6)
rel(disagrees-9, which-7)
nsubj(disagrees-9, one-8)
rcmod(laws-6, disagrees-9)
prep(disagrees-9, with-10)
root(ROOT-0, is-11)
poss(opinion-14, my-13)
prep_in(is-11, opinion-14)
det(one-17, an-15)
amod(one-17, erroneous-16)
dep(opinion-14, one-17)

