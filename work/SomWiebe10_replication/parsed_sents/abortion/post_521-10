(ROOT
  (S
    (NP (PRP It))
    (VP (MD must)
      (VP (VB be)
        (NP
          (NP (DT a) (NN brain))
          (PP (IN with)
            (NP
              (NP (NN consciousness))
              (, ,)
              (NP
                (NP (NN awareness))
                (, ,)
                (NP (NNS person-ness))))))))
    (. .)))

nsubj(brain-5, It-1)
aux(brain-5, must-2)
cop(brain-5, be-3)
det(brain-5, a-4)
root(ROOT-0, brain-5)
prep_with(brain-5, consciousness-7)
appos(consciousness-7, awareness-9)
appos(awareness-9, person-ness-11)

