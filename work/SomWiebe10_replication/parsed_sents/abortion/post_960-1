(ROOT
  (S
    (NP (JJ Many) (NN childern))
    (VP (VBP have)
      (VP (VBN been)
        (VP (VBN killed)
          (S
            (ADJP (JJ due)
              (PP (TO to)
                (NP (NN abortion))))))))
    (. .)))

amod(childern-2, Many-1)
nsubjpass(killed-5, childern-2)
aux(killed-5, have-3)
auxpass(killed-5, been-4)
root(ROOT-0, killed-5)
acomp(killed-5, due-6)
prep_to(due-6, abortion-8)

