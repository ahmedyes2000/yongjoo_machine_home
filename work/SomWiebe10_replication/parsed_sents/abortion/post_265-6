(ROOT
  (S
    (PP (IN For)
      (NP (NN instance)))
    (, ,)
    (NP (PRP I))
    (VP (MD would)
      (VP (VB be)
        (ADJP (RB just)
          (ADVP (RB as) (RB strongly))
          (VBN opposed)
          (PP (TO to)
            (NP (NN government) (VBN mandated) (NNS abortions))))
        (SBAR (IN as)
          (S
            (NP (PRP I))
            (VP (VBP am)
              (PP (IN towards)
                (S
                  (VP (VBG banning)
                    (NP (NN abortion))
                    (ADVP (DT all) (RB together))))))))))
    (. .)))

prep_for(opposed-10, instance-2)
nsubjpass(opposed-10, I-4)
aux(opposed-10, would-5)
auxpass(opposed-10, be-6)
advmod(opposed-10, just-7)
advmod(strongly-9, as-8)
advmod(opposed-10, strongly-9)
root(ROOT-0, opposed-10)
nn(abortions-14, government-12)
