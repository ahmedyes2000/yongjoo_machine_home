  (S (CC But)
    (S
      (NP
        (NP (DT the) (NN issue))
        (PP (IN of)
          (NP
            (NP (NN double-murder))
            (PP (IN for)
              (NP (JJ pregnant) (NNS women))))))
      (VP (VBZ is)
        (ADJP (JJ important))))
    (, ,) (IN so)
    (S
      (NP (PRP I))
      (VP (MD will)
        (VP (VB address)
          (NP (DT that))
          (ADVP (RB here)))))
    (. .)))

