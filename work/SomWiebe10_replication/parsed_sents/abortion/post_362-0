(ROOT
  (S
    (NP (JJ Religious) (NN dogma))
    (VP (VBZ is)
      (ADJP (JJ impervious)
        (PP (TO to)
          (NP (JJ logical) (NNS arguments)))))
    (. .)))

amod(dogma-2, Religious-1)
nsubj(impervious-4, dogma-2)
cop(impervious-4, is-3)
root(ROOT-0, impervious-4)
amod(arguments-7, logical-6)
prep_to(impervious-4, arguments-7)

