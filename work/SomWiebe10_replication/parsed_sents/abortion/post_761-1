(ROOT
  (S
    (NP (DT The) (NN choice))
    (ADVP (RB however))
    (VP (MD should)
      (VP (VB be)
        (ADVP (RB there)
          (PP (IN for)
            (NP
              (NP (NNS people))
              (SBAR
                (WHNP (WP who))
                (S
                  (VP (VBP demand)
                    (NP (PRP it))))))))))
    (. .)))

det(choice-2, The-1)
nsubj(be-5, choice-2)
advmod(be-5, however-3)
aux(be-5, should-4)
root(ROOT-0, be-5)
advmod(be-5, there-6)
prep_for(there-6, people-8)
nsubj(demand-10, people-8)
rcmod(people-8, demand-10)
dobj(demand-10, it-11)

