(ROOT
  (S
    (NP (DT This) (NN reality))
    (VP (VBZ has)
      (VP (VBN weighed)
        (ADVP (RB heavily))
        (PP (IN on)
          (NP (PRP$ my) (NN conscience)))))
    (. .)))

det(reality-2, This-1)
nsubj(weighed-4, reality-2)
aux(weighed-4, has-3)
root(ROOT-0, weighed-4)
advmod(weighed-4, heavily-5)
poss(conscience-8, my-7)
prep_on(weighed-4, conscience-8)

