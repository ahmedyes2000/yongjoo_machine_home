(ROOT
  (S ('' '')
    (NP (DT The) (JJ biblical) (NN concept))
    (VP (VBZ is)
      (ADJP (JJ human))
      (S
        (VP (VBG being)
          (VP (VBZ becomes)
            (ADJP (JJ alive)))))
      (SBAR
        (WHADVP (WRB when))
        (S
          (NP (DT the) (NN soul))
          (VP (VBZ enters)
            (NP
              (NP
                (NP (DT the) (NN body))
                (PP (IN upon)
                  (NP (NN birth))))
              (CC and)
              (NP
                (NP (DT the) (NN taking))
                (PP (IN of)
                  (NP (DT the) (JJ first) (NN breath)))))))))
    (. .)))

det(concept-4, The-2)
amod(concept-4, biblical-3)
nsubj(human-6, concept-4)
cop(human-6, is-5)
root(ROOT-0, human-6)
aux(alive-9, being-7)
cop(alive-9, becomes-8)
xcomp(human-6, alive-9)
advmod(enters-13, when-10)
det(soul-12, the-11)
nsubj(enters-13, soul-12)
advcl(human-6, enters-13)
det(body-15, the-14)
dobj(enters-13, body-15)
prep_upon(body-15, birth-17)
det(taking-20, the-19)
dobj(enters-13, taking-20)
conj_and(body-15, taking-20)
det(breath-24, the-22)
amod(breath-24, first-23)
prep_of(taking-20, breath-24)

