(ROOT
  (S
    (NP (NNP Everything))
    (VP (VBZ becomes)
      (ADJP (JJ justified)
        (PP (IN through)
          (NP (JJ individual) (NN belief)))))
    (. .)))

nsubj(justified-3, Everything-1)
cop(justified-3, becomes-2)
root(ROOT-0, justified-3)
amod(belief-6, individual-5)
prep_through(justified-3, belief-6)

