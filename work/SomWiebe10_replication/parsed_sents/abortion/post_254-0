(ROOT
  (S
    (NP (NNP Animal))
    (ADVP (RB cruelty))
    (VP (VBZ is)
      (ADVP (RB NOT))
      (NP (DT a) (JJ political) (NN problem))
      (, ,)
      (SBAR (IN unless)
        (S
          (NP (PRP you))
          (VP (VBP are)
            (NP (DT a) (NN collectivist))))))
    (. .)))

nsubj(problem-7, Animal-1)
advmod(problem-7, cruelty-2)
cop(problem-7, is-3)
neg(problem-7, NOT-4)
det(problem-7, a-5)
amod(problem-7, political-6)
root(ROOT-0, problem-7)
