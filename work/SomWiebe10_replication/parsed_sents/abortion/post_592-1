(ROOT
  (S
    (NP (DT The) (JJ unfertilized) (NN egg))
    (VP (VBZ is)
      (NP
        (NP (DT a) (NN part))
        (PP (IN of)
          (NP
            (NP (DT the) (NN mother) (POS 's))
            (NN body)))))
    (. .)))

det(egg-3, The-1)
amod(egg-3, unfertilized-2)
nsubj(part-6, egg-3)
cop(part-6, is-4)
det(part-6, a-5)
root(ROOT-0, part-6)
det(mother-9, the-8)
poss(body-11, mother-9)
