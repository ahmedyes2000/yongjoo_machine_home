(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is)
      (ADJP
        (ADJP
          (ADJP (RB very) (JJ difficult))
          (PRN (, ,)
            (SBAR (IN if)
              (FRAG (RB not)
                (ADJP (JJ impossible))))
            (, ,)))
        (PRN (-LRB- -LRB-)
          (SBAR (IN as)
            (S
              (NP (PRP it))
              (VP (MD should)
                (VP (VB be)))))
          (-RRB- -RRB-))
        (SBAR (IN for)
          (S
            (NP (DT the) (NN government))
            (VP (TO to)
              (VP (VB require)
                (NP (DT a) (NN person)
                  (S
                    (VP (TO to)
                      (VP (VB take)
                        (NP (NN action))
                        (S
                          (VP (TO to)
                            (VP (VB save)
                              (NP (DT a) (NN life)))))))))))))))
    (. .)))

nsubj(difficult-4, It-1)
cop(difficult-4, is-2)
advmod(difficult-4, very-3)
root(ROOT-0, difficult-4)
mark(not-7, if-6)
dep(difficult-4, not-7)
dep(not-7, impossible-8)
mark(be-14, as-11)
