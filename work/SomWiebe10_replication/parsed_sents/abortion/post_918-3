(ROOT
  (S
    (NP (PRP They))
    (ADVP (RB knowingly))
    (VP (VBP risk)
      (NP (PRP$ their) (NNS lives))
      (S
        (VP (TO to)
          (VP (VB help)
            (VP (VB ensure)
              (NP
                (NP (NNS women))
                (SBAR
                  (WHNP (WP who))
                  (S
                    (VP (VBP need)
                      (SBAR
                        (S
                          (NP (JJ late-term) (NNS abortions))
                          (VP (MD can)
                            (VP (VB get)
                              (NP (PRP them)))))))))))))))
    (. .)))

nsubj(risk-3, They-1)
advmod(risk-3, knowingly-2)
root(ROOT-0, risk-3)
poss(lives-5, their-4)
dobj(risk-3, lives-5)
aux(help-7, to-6)
xcomp(risk-3, help-7)
xcomp(help-7, ensure-8)
dobj(ensure-8, women-9)
nsubj(need-11, women-9)
rcmod(women-9, need-11)
amod(abortions-13, late-term-12)
nsubj(get-15, abortions-13)
aux(get-15, can-14)
ccomp(need-11, get-15)
dobj(get-15, them-16)

