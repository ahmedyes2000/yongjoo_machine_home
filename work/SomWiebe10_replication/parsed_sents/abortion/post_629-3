(ROOT
  (S
    (PP (IN As)
      (PP (IN in)))
    (, ,)
    (NP
      (NP (DT the) (JJ Christian) (NNS values))
      (SBAR
        (S
          (NP (PRP they))
          (VP (VBP profess)))))
    (VP (VBP are)
      (NP (JJ actual) (NN campaign) (NNS tactics)))
    (. .)))

prep(tactics-12, As-1)
pcomp(As-1, in-2)
prep(As-1, in-2)
det(values-6, the-4)
amod(values-6, Christian-5)
dobj(profess-8, values-6)
nsubj(tactics-12, values-6)
nsubj(profess-8, they-7)
rcmod(values-6, profess-8)
cop(tactics-12, are-9)
amod(tactics-12, actual-10)
nn(tactics-12, campaign-11)
root(ROOT-0, tactics-12)

