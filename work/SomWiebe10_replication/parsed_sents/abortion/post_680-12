(ROOT
  (S
    (NP (NNP Science))
    (VP (VBZ is)
      (VP (VBN supposed)
        (S
          (VP (TO to)
            (VP (VB be)
              (NP (NN empirical)
                (CC and)
                (NN objective)))))))
    (. .)))

nsubjpass(supposed-3, Science-1)
xsubj(empirical-6, Science-1)
auxpass(supposed-3, is-2)
root(ROOT-0, supposed-3)
aux(empirical-6, to-4)
cop(empirical-6, be-5)
