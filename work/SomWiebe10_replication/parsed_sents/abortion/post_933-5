  (S
    (NP (DT The) (NN world))
    (VP (VBZ has)
      (VP
        (VP (VBN been)
          (ADVP (IN through) (DT this))
          (NP
            (QP (DT a) (NN dozen))
            (NNS times)))
        (, ,)
        (CC and)
        (VP
          (PP (IN for)
            (NP
              (NP (NNS hundreds))
              (PP (IN of)
                (NP (NNS years))))))))
    (. .)))

