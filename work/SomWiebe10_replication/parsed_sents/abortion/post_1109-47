(ROOT
  (S
    (PP (IN About)
      (NP
        (NP (DT the) (NN parallel))
        (PP (IN with)
          (NP (NN fire)))))
    (, ,)
    (SBAR (IN in) (NN case)
      (S
        (NP (PRP you))
        (VP (VBD did) (RB n't)
          (VP (VB know)))))
    (, ,)
    (NP (NN passion))
    (VP (VBZ burns))
    (. .)))

det(parallel-3, the-2)
prep_about(burns-15, parallel-3)
prep_with(parallel-3, fire-5)
mark(know-12, in-7)
dep(know-12, case-8)
nsubj(know-12, you-9)
aux(know-12, did-10)
neg(know-12, n't-11)
advcl(burns-15, know-12)
nsubj(burns-15, passion-14)
root(ROOT-0, burns-15)

