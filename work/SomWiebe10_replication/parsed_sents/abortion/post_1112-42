(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP it))
        (VP (VBZ is)
          (ADJP (JJ human)
            (PP (IN than)
              (NP (DT no) (NN justification)))))))
    (VP (VBZ is)
      (ADJP (JJ adequate)))
    (. .)))

mark(human-4, If-1)
nsubj(human-4, it-2)
cop(human-4, is-3)
csubj(adequate-9, human-4)
det(justification-7, no-6)
prep_than(human-4, justification-7)
cop(adequate-9, is-8)
root(ROOT-0, adequate-9)

