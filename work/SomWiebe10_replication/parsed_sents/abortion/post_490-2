(ROOT
  (S
    (S
      (NP (DT Those) (NNS places))
      (VP (VBP are) (RB n't)
        (VP (VBG performing)
          (NP
            (QP (CD abortions) (CD 24\/7))))))
    (, ,)
    (NP (PRP you))
    (VP (VBP know))
    (. .)))

det(places-2, Those-1)
nsubj(performing-5, places-2)
aux(performing-5, are-3)
neg(performing-5, n't-4)
ccomp(know-10, performing-5)
number(24\/7-7, abortions-6)
dobj(performing-5, 24\/7-7)
nsubj(know-10, you-9)
root(ROOT-0, know-10)

