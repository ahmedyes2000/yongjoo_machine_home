  (S
    (NP (DT The) (NN penalty))
    (VP (MD would)
      (VP (VB be)
        (ADJP
          (ADJP (RBR more) (JJ severe))
          (SBAR (IN if)
            (S
              (NP (DT the) (NN quickening))
              (VP (VBD had)
                (VP (VBN taken)
                  (NP (NN place)))))))))
    (. .)))

