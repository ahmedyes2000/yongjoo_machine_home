(ROOT
  (S
    (NP (PRP I))
    (VP (VBP have)
      (NP
        (NP (NN respect))
        (PP (IN for)
          (NP (NNP ALL) (NN life) (NNS forms)))
        (, ,)
        (ADJP (JJ human)
          (CC and)
          (JJ non-human))))
    (. .)))

nsubj(have-2, I-1)
root(ROOT-0, have-2)
dobj(have-2, respect-3)
nn(forms-7, ALL-5)
nn(forms-7, life-6)
prep_for(respect-3, forms-7)
amod(respect-3, human-9)
amod(respect-3, non-human-11)
conj_and(human-9, non-human-11)

