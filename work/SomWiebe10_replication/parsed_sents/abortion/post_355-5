(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP (VB remove)
          (NP (DT the) (JJ life-support) (NN system)))))
    (, ,)
    (NP (DT the) (JJ human) (NN being))
    (VP (MD will)
      (VP (VB die)))
    (. .)))

mark(remove-3, If-1)
nsubj(remove-3, you-2)
advcl(die-12, remove-3)
det(system-6, the-4)
amod(system-6, life-support-5)
dobj(remove-3, system-6)
det(being-10, the-8)
amod(being-10, human-9)
nsubj(die-12, being-10)
aux(die-12, will-11)
root(ROOT-0, die-12)

