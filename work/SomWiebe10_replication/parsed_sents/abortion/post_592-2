(ROOT
  (S
    (SBAR
      (WHADVP (WRB When))
      (S
        (NP (PRP they))
        (VP (VBP are)
          (VP (VBN combined)))))
    (, ,)
    (NP (PRP they))
    (VP (VBP create)
      (NP (DT a) (JJ new) (NN life))
      (, ,)
      (PP (IN with)
        (NP
          (NP (PRP$ its) (JJ own) (NN DNA) (NN pattern))
          (CC and)
          (NP (JJ individual) (NN existence)))))
    (. .)))

advmod(combined-4, When-1)
nsubjpass(combined-4, they-2)
auxpass(combined-4, are-3)
advcl(create-7, combined-4)
nsubj(create-7, they-6)
root(ROOT-0, create-7)
det(life-10, a-8)
amod(life-10, new-9)
dobj(create-7, life-10)
poss(pattern-16, its-13)
