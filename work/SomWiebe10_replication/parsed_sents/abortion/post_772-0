(ROOT
  (S
    (NP (PRP I))
    (VP (VBP am)
      (VP (VBG saying)
        (SBAR (IN that)
          (S
            (NP (NNS men))
            (VP (MD should) (RB not)
              (VP (VB have)
                (NP
                  (NP (DT the) (VBG deciding) (NN vote))
                  (PP (IN on)
                    (NP (DT the) (NN matter)))
                  (SBAR
                    (WHNP (WDT which))
                    (S
                      (ADVP (RB really))
                      (NP (PRP they))
                      (VP (VBP do)))))))))))
    (. .)))

nsubj(saying-3, I-1)
aux(saying-3, am-2)
root(ROOT-0, saying-3)
complm(have-8, that-4)
nsubj(have-8, men-5)
aux(have-8, should-6)
neg(have-8, not-7)
ccomp(saying-3, have-8)
det(vote-11, the-9)
