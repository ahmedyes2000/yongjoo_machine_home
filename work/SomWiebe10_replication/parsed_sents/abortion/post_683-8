(ROOT
  (S
    (ADVP (RB Nonetheless))
    (, ,)
    (NP
      (NP (DT a) (JJ scientific) (NN determination))
      (PP (IN of)
        (SBAR
          (WHADVP (WRB when))
          (S
            (NP (NN something))
            (VP (VBZ begins))))))
    (VP (MD should) (RB n't)
      (VP (VB deal)
        (PP (IN with)
          (SBAR
            (WHNP (WP what))
            (S
              (VP (VBZ is)
                (ADJP (JJ important))))))))
    (. .)))

advmod(deal-12, Nonetheless-1)
det(determination-5, a-3)
amod(determination-5, scientific-4)
nsubj(deal-12, determination-5)
advmod(begins-9, when-7)
nsubj(begins-9, something-8)
prepc_of(determination-5, begins-9)
aux(deal-12, should-10)
neg(deal-12, n't-11)
root(ROOT-0, deal-12)
nsubj(important-16, what-14)
cop(important-16, is-15)
prepc_with(deal-12, important-16)

