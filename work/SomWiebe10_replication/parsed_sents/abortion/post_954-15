(ROOT
  (S
    (VP
      (VP (VB Confess)
        (NP (PRP$ your) (NNS sins)))
      (CC and)
      (VP (VB trust)
        (S
          (NP (NNP God))
          (VP (TO to)
            (VP (VB do)
              (PP (IN for)
                (NP (PRP you)))
              (SBAR
                (WHNP (WP what))
                (S
                  (NP (PRP he))
                  (VP (VBD said)
                    (SBAR
                      (S
                        (NP (PRP He))
                        (VP (MD would)
                          (PP (IN in)
                            (NP (NNP Christ))))))))))))))
    (. .)))

root(ROOT-0, Confess-1)
poss(sins-3, your-2)
dobj(Confess-1, sins-3)
conj_and(Confess-1, trust-5)
nsubj(do-8, God-6)
aux(do-8, to-7)
xcomp(trust-5, do-8)
prep_for(do-8, you-10)
dep(said-13, what-11)
dobj(would-15, what-11)
nsubj(said-13, he-12)
ccomp(do-8, said-13)
nsubj(would-15, He-14)
ccomp(said-13, would-15)
prep_in(would-15, Christ-17)

