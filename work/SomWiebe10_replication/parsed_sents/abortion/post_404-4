(ROOT
  (S ('' '')
    (NP (DT Both))
    (VP (VBP need)
      (S
        (VP (TO to)
          (VP (VB be)
            (VP (VBN pulled)
              (PP (IN in)
                (NP
                  (NP (DT the) (JJ final) (NNS minutes))
                  (PP (IN of)
                    (NP (NN labor))))))))))
    (. .)))

nsubj(need-3, Both-2)
xsubj(pulled-6, Both-2)
root(ROOT-0, need-3)
aux(pulled-6, to-4)
auxpass(pulled-6, be-5)
xcomp(need-3, pulled-6)
det(minutes-10, the-8)
amod(minutes-10, final-9)
prep_in(pulled-6, minutes-10)
prep_of(minutes-10, labor-12)

