(ROOT
  (S
    (S
      (NP (PRP I))
      (VP
        (VP (VBP agree) (, ,) (VBP go)
          (ADVP (RB back)))
        (CC and)
        (VP (VBP look)
          (PP (IN at)
            (NP
              (NP (DT the) (NN record))
              (PP (IN of)
                (NP (NNP Congress)))))
          (PP (IN under)
            (NP (NNP Bush))))))
    (: ;)
    (S
      (NP
        (NP (PRP it) (POS 's))
        (NN record))
      (VP (VBZ is)
        (ADJP (JJ shameful))))
    (. .)))

nsubj(agree-2, I-1)
nsubj(look-7, I-1)
root(ROOT-0, agree-2)
dep(agree-2, go-4)
advmod(agree-2, back-5)
conj_and(agree-2, look-7)
det(record-10, the-9)
