(ROOT
  (S
    (NP
      (NP (DT The) (NN degree))
      (PP (IN of)
        (NP
          (NP (NN protection))
          (SBAR (IN that)
            (S
              (NP (NN life))
              (VP (VBZ deserves)))))))
    (VP (VBZ is)
      (SBAR
        (WHNP (WP what))
        (S
          (VP (MD can) (RB not)
            (VP (VB be)
              (VP (VBN determined)
                (PP (IN by)
                  (NP (NN science)))))))))
    (. .)))

det(degree-2, The-1)
nsubj(is-8, degree-2)
