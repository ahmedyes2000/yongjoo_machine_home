(ROOT
  (S
    (NP (DT A) (JJ new) (NN life))
    (VP (VBZ has)
      (VP (VBN been)
        (VP (VBN created)
          (PP (IN as)
            (NP
              (NP (DT a) (NN matter))
              (PP (IN of)
                (NP (NN science))))))))
    (. .)))

det(life-3, A-1)
amod(life-3, new-2)
nsubjpass(created-6, life-3)
aux(created-6, has-4)
auxpass(created-6, been-5)
root(ROOT-0, created-6)
det(matter-9, a-8)
