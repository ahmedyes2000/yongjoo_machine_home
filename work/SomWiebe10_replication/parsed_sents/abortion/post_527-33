  (S
    (NP (PRP I))
    (VP (VBP have)
      (ADVP (RB no) (RB right))
      (S
        (VP (TO to)
          (VP (VB do)
            (SBAR (IN as)
              (S
                (NP (PRP I))
                (VP (VBP please)
                  (S
                    (VP
                      (PRN (-LRB- -LRB-)
                        (PP (IN with)
                          (NP (PRP$ my) (NN body)))
                        (-RRB- -RRB-))
                      (PP (IN for)
                        (NP
                          (NP (DT the) (NN sake))
                          (PP (IN of)
                            (NP (PRP$ my) (NN womb)))))))
                  (SBAR (IN because)
                    (S
                      (NP (NN nature))
