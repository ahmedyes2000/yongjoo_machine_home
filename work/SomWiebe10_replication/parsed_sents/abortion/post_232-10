  (S
    (S
      (NP (PRP I))
      (VP (VBP 'm)
        (ADJP
          (NP (DT a) (RB little))
          (JJ old))
        (, ,)
        (ADVP (RB now))))
    (: ;)
    (CC and)
    (S
      (NP (PRP I))
      (VP (MD ca) (RB n't)
        (VP (VB remember)
          (NP
            (NP (DT the) (NN title))
            (PP (IN of)
              (NP (PRP$ his) (NN book))))
          (ADVP (RB just) (RB now)))))
    (. .)))

