(ROOT
  (S
    (ADVP (RB Hope))
    (NP (DT that))
    (VP (VBZ clears)
      (NP (NNS things))
      (PRT (RP up))
      (PP (IN for)
        (NP (PRP you))))
    (. .)))

advmod(clears-3, Hope-1)
nsubj(clears-3, that-2)
root(ROOT-0, clears-3)
dobj(clears-3, things-4)
prt(clears-3, up-5)
prep_for(clears-3, you-7)

