(ROOT
  (S
    (ADVP (RB Homosexuality))
    (: :)
    (NP (NNP Sexual))
    (VP (VBP desire)
      (PP (IN for)
        (NP
          (NP (DT those))
          (PP (IN of)
            (NP (DT the) (JJ same) (NN sex))))))
    (. .)))

advmod(desire-4, Homosexuality-1)
nsubj(desire-4, Sexual-3)
root(ROOT-0, desire-4)
prep_for(desire-4, those-6)
det(sex-10, the-8)
amod(sex-10, same-9)
prep_of(those-6, sex-10)

