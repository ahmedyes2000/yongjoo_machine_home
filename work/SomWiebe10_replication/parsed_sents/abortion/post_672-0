(ROOT
  (S
    (NP
      (NP (DT The) (NN claim))
      (SBAR (IN that)
        (S
          (NP (DT this))
          (VP (VBZ is)
            (NP (DT the) (JJ only) (`` ``) (JJ logical) (NN answer) ('' ''))))))
    (VP (VBZ is)
      (ADVP (RB positively))
      (ADJP (JJ absurd)))
    (. .)))

det(claim-2, The-1)
nsubj(absurd-14, claim-2)
complm(answer-10, that-3)
nsubj(answer-10, this-4)
cop(answer-10, is-5)
det(answer-10, the-6)
amod(answer-10, only-7)
amod(answer-10, logical-9)
dep(claim-2, answer-10)
cop(absurd-14, is-12)
