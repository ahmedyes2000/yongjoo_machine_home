  (S
    (ADVP (RB Well))
    (, ,)
    (SBAR (IN once)
      (S
        (NP (PRP it))
        (VP (VBZ is)
          (NP (DT a) (NN child)))))
    (, ,)
    (NP (PRP it))
    (ADVP (RB already))
    (VP (VBZ is)
      (ADJP (JJ illegal)
        (S
          (VP (TO to)
            (VP (VB kill)
              (NP (PRP it)))))))
    (. .)))

