(ROOT
  (S
    (ADVP (RB Perhaps))
    (NP (PRP you))
    (VP (MD might)
      (ADVP (RB actually))
      (VP (VB know)
        (NP
          (NP (PRP$ my) (NN friend))
          (: :)
          (NP (NNP Professor) (NNP Steve) (NNP Schwartz)))))
    (. .)))

advmod(know-5, Perhaps-1)
nsubj(know-5, you-2)
aux(know-5, might-3)
advmod(know-5, actually-4)
root(ROOT-0, know-5)
poss(friend-7, my-6)
dobj(know-5, friend-7)
