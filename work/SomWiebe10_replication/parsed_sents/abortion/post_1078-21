(ROOT
  (S
    (NP
      (NP (DT The) (NN fact))
      (SBAR (IN that)
        (S
          (NP (NN life))
          (VP (VBZ begins)
            (PP (IN at)
              (NP (NN conception)))))))
    (VP (VBZ is)
      (ADVP (RB spiritually) (RB right)))
    (. .)))

det(fact-2, The-1)
dobj(begins-5, fact-2)
nsubj(is-8, fact-2)
complm(begins-5, that-3)
nsubj(begins-5, life-4)
dep(fact-2, begins-5)
prep_at(begins-5, conception-7)
root(ROOT-0, is-8)
advmod(right-10, spiritually-9)
advmod(is-8, right-10)

