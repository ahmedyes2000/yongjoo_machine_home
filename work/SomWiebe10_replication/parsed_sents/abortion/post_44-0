  (S
    (NP (`` ``) (NNP Back) (NNP alley) ('' '') (NNS abortions))
    (VP (VBP are)
      (ADJP
        (ADJP (RBR more) (JJ frequent))
        (SBAR
          (WHADVP (WRB when))
          (S
            (NP (NN abortion))
            (VP (VBZ is)
              (ADJP (JJ illegal)))))))
    (. .)))

nn(abortions-5, Back-2)
nn(abortions-5, alley-3)
nsubj(frequent-8, abortions-5)
