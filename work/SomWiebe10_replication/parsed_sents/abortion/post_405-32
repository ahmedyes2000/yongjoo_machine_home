(ROOT
  (S
    (VP
      (ADVP (RB Still))
      (S
        (VP (VBG waiting)
          (PP (IN for)
            (NP (DT that) (NN evidence)))
          (SBAR (IN that)
            (S
              (NP (DT a) (VBN fertilized) (NN egg))
              (VP (VBZ has)
                (NP (DT a) (NN soul))))))))
    (. .)))

root(ROOT-0, Still-1)
xcomp(Still-1, waiting-2)
det(evidence-5, that-4)
prep_for(waiting-2, evidence-5)
complm(has-10, that-6)
det(egg-9, a-7)
amod(egg-9, fertilized-8)
nsubj(has-10, egg-9)
ccomp(waiting-2, has-10)
det(soul-12, a-11)
dobj(has-10, soul-12)

