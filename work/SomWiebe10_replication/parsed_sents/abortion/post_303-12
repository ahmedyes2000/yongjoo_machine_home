(ROOT
  (SBARQ
    (SBAR (IN As)
      (FRAG
        (PP (IN for)
          (NP (DT the) (NN practice)))))
    (, ,)
    (WHADVP (WRB where)
      (ADJP (RB else)))
    (SQ (VBP do)
      (NP (PRP you))
      (VP (VB get)
        (NP (NNS laws))
        (PP (IN against)
          (NP
            (NP (NN murder) (, ,) (NN rape) (, ,)
              (CC and)
              (NN theft) (, ,))
            (PP (IN except)
              (PP (IN from)
                (NP (NN religion))))))))
    (. ?)))

mark(for-2, As-1)
advcl(get-10, for-2)
det(practice-4, the-3)
pobj(for-2, practice-4)
advmod(get-10, where-6)
dep(where-6, else-7)
aux(get-10, do-8)
