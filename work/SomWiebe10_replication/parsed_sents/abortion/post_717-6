(ROOT
  (S
    (NP (DT This))
    (VP (VBZ is)
      (ADVP (RB indeed))
      (NP
        (NP (DT the) (NN sort))
        (PP (IN of)
          (NP (NN discussion)))
        (SBAR
          (S
            (NP (PRP I))
            (VP (VBP think)
              (SBAR
                (S
                  (NP (PRP we))
                  (ADVP (DT all))
                  (VP (MD should)
                    (VP (VB be)
                      (ADJP (JJ having)))))))))))
    (. .)))

nsubj(sort-5, This-1)
cop(sort-5, is-2)
advmod(sort-5, indeed-3)
det(sort-5, the-4)
root(ROOT-0, sort-5)
prep_of(sort-5, discussion-7)
nsubj(think-9, I-8)
rcmod(sort-5, think-9)
nsubj(having-14, we-10)
advmod(having-14, all-11)
aux(having-14, should-12)
cop(having-14, be-13)
ccomp(think-9, having-14)

