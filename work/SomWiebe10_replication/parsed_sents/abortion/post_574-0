(ROOT
  (S
    (NP (RB Almost) (DT no) (NN one))
    (ADVP (RB really))
    (VP (VBZ believes)
      (SBAR
        (S
          (NP (DT a) (VBN fertilized) (NN egg))
          (VP (VBZ is)
            (NP
              (NP (DT a) (JJ human) (NN being))
              (PP (IN with)
                (NP (PDT all) (DT the) (NNS rights)
                  (SBAR
                    (WHNP (WDT that))
                    (S
                      (VP (VBZ entails)))))))))))
    (. .)))

advmod(one-3, Almost-1)
det(one-3, no-2)
nsubj(believes-5, one-3)
advmod(believes-5, really-4)
root(ROOT-0, believes-5)
det(egg-8, a-6)
amod(egg-8, fertilized-7)
nsubj(being-12, egg-8)
cop(being-12, is-9)
det(being-12, a-10)
amod(being-12, human-11)
ccomp(believes-5, being-12)
predet(rights-16, all-14)
det(rights-16, the-15)
prep_with(being-12, rights-16)
nsubj(entails-18, that-17)
ccomp(rights-16, entails-18)

