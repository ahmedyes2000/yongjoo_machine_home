  (S
    (NP (EX There))
    (VP (VBP are)
      (NP
        (NP (NNS thousands))
        (PP (IN of)
          (NP
            (NP (NNS children))
            (SBAR
              (WHNP (WP who))
              (S
                (VP (VBP have)
                  (NP
                    (NP (DT a) (NN right))
                    (PP (TO to)
                      (NP
                        (NP (NN life))
                        (VP (VBG dying)
                          (PP (IN at)
                            (NP (DT this) (JJ very) (NN moment)))
                          (PP (IN from)
