(ROOT
  (S
    (SBAR (IN After)
      (S
        (NP (DT a) (NN woman))
        (VP (VBZ has)
          (NP (DT an) (NN abortion)))))
    (, ,)
    (NP
      (NP (PRP$ her) (JJ initial) (NN response))
      (PP (IN in)
        (NP (JJS most) (NNS cases))))
    (VP (VBZ is)
      (NP
        (NP (DT a) (NN feeling))
        (PP (IN of)
          (NP (NN relief)))))
    (. .)))

mark(has-4, After-1)
det(woman-3, a-2)
nsubj(has-4, woman-3)
advcl(feeling-16, has-4)
det(abortion-6, an-5)
dobj(has-4, abortion-6)
poss(response-10, her-8)
amod(response-10, initial-9)
nsubj(feeling-16, response-10)
amod(cases-13, most-12)
prep_in(response-10, cases-13)
cop(feeling-16, is-14)
det(feeling-16, a-15)
root(ROOT-0, feeling-16)
prep_of(feeling-16, relief-18)

