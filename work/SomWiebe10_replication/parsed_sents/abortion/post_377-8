(ROOT
  (S
    (NP (DT The) (NN man))
    (VP (VBZ is)
      (ADJP (JJ irrelevant)
        (PP (IN in)
          (NP (DT this) (NN situation)))))
    (. .)))

det(man-2, The-1)
nsubj(irrelevant-4, man-2)
cop(irrelevant-4, is-3)
root(ROOT-0, irrelevant-4)
det(situation-7, this-6)
prep_in(irrelevant-4, situation-7)

