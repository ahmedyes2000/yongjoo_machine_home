(ROOT
  (S
    (NP (EX There))
    (VP (VBZ is)
      (ADJP
        (ADJP (NN nothing) (JJ reasonable))
        (PP (IN about)
          (S
            (VP (VBG raising)
              (NP
                (NP (DT a) (JJ constant) (NN reminder))
                (PP (IN of)
                  (NP (DT a) (JJ cruel) (NN crime)))))))))
    (. .)))

expl(is-2, There-1)
root(ROOT-0, is-2)
dep(reasonable-4, nothing-3)
acomp(is-2, reasonable-4)
prepc_about(reasonable-4, raising-6)
det(reminder-9, a-7)
amod(reminder-9, constant-8)
dobj(raising-6, reminder-9)
det(crime-13, a-11)
amod(crime-13, cruel-12)
