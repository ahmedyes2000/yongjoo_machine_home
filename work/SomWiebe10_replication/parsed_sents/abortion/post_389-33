(ROOT
  (S
    (INTJ (VB Please))
    (VP (VB provide)
      (NP (DT the) (NN evidence)
        (S
          (VP (TO to)
            (VP (VB support)
              (NP (DT that) (NN comment)))))))
    (. .)))

dep(provide-2, Please-1)
root(ROOT-0, provide-2)
det(evidence-4, the-3)
dobj(provide-2, evidence-4)
aux(support-6, to-5)
infmod(evidence-4, support-6)
det(comment-8, that-7)
dobj(support-6, comment-8)

