(ROOT
  (S
    (NP (PRP They))
    (DT both)
    (VP (VBP want)
      (S
        (VP (TO to)
          (VP (VB create)
            (NP (DT the) (JJ binary) (NN situation))
            (SBAR (IN that)
              (S
                (NP (PRP I))
                (VP (VBP argue)
                  (PP (IN against)
                    (NP (DT the) (JJ worthless\/important) (NN dichotomy)))
                  (SBAR (IN as)
                    (S
                      (NP (PRP you))
                      (VP (MD aptly)
                        (VP (VB describe)
                          (NP (PRP it)))))))))))))
    (. .)))

nsubj(want-3, They-1)
xsubj(create-5, They-1)
dep(want-3, both-2)
root(ROOT-0, want-3)
aux(create-5, to-4)
xcomp(want-3, create-5)
det(situation-8, the-6)
amod(situation-8, binary-7)
dobj(create-5, situation-8)
complm(argue-11, that-9)
nsubj(argue-11, I-10)
ccomp(create-5, argue-11)
det(dichotomy-15, the-13)
amod(dichotomy-15, worthless\/important-14)
prep_against(argue-11, dichotomy-15)
mark(aptly-18, as-16)
nsubj(aptly-18, you-17)
advcl(argue-11, aptly-18)
dep(aptly-18, describe-19)
dobj(describe-19, it-20)

