(ROOT
  (S
    (NP (DT Some) (NNS women))
    (VP (VBP are)
      (VP (VBG caring)
        (PP (IN for)
          (NP (JJ several) (NNS infants)
            (CC and)
            (NNS toddlers)))
        (ADVP (RB already))
        (, ,)
        (SBAR
          (WHNP (WP who))
          (S
            (VP
              (VP (VBP rely)
                (PP (IN on)
                  (NP (PRP them))))
              (, ,)
              (CC and)
              (VP (MD can) (RB not)
                (VP (VB undergo)
                  (NP
                    (NP (DT the) (NN stress))
                    (PP (IN of)
                      (NP
                        (NP (DT another) (NN pregnancy))
                        (, ,)
                        (ADVP (VB let) (JJ alone))
                        (NP (NN care)))))
                  (PP (IN for)
                    (NP
                      (NP (PRP$ their) (VBG existing) (NNS children))
                      (PP (IN during)
                        (NP (DT a) (JJ difficult) (NN pregnancy))))))))))))
    (. .)))

det(women-2, Some-1)
nsubj(caring-4, women-2)
aux(caring-4, are-3)
root(ROOT-0, caring-4)
amod(infants-7, several-6)
prep_for(caring-4, infants-7)
prep_for(caring-4, toddlers-9)
conj_and(infants-7, toddlers-9)
advmod(caring-4, already-10)
nsubj(rely-13, who-12)
nsubj(undergo-20, who-12)
ccomp(caring-4, rely-13)
prep_on(rely-13, them-15)
aux(undergo-20, can-18)
neg(undergo-20, not-19)
ccomp(caring-4, undergo-20)
conj_and(rely-13, undergo-20)
det(stress-22, the-21)
dobj(undergo-20, stress-22)
det(pregnancy-25, another-24)
prep_of(stress-22, pregnancy-25)
dep(alone-28, let-27)
advmod(pregnancy-25, alone-28)
dep(pregnancy-25, care-29)
poss(children-33, their-31)
amod(children-33, existing-32)
prep_for(undergo-20, children-33)
det(pregnancy-37, a-35)
amod(pregnancy-37, difficult-36)
prep_during(children-33, pregnancy-37)

