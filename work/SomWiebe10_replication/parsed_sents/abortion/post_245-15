(ROOT
  (S
    (NP (DT The) (JJ whole) (NN thing))
    (VP (VBZ is) (RB not)
      (VP (VBG wanting)
        (S
          (VP (TO to)
            (VP (VB be)
              (ADJP (JJ pregnant))
              (: ...)
              (PP (IN for)
                (NP (WDT whatever) (NN reason))))))))
    (. .)))

det(thing-3, The-1)
amod(thing-3, whole-2)
nsubj(wanting-6, thing-3)
xsubj(pregnant-9, thing-3)
aux(wanting-6, is-4)
neg(wanting-6, not-5)
root(ROOT-0, wanting-6)
aux(pregnant-9, to-7)
cop(pregnant-9, be-8)
xcomp(wanting-6, pregnant-9)
dep(reason-13, whatever-12)
prep_for(pregnant-9, reason-13)

