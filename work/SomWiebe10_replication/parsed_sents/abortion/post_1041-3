  (S
    (S
      (NP (PRP We))
      (VP (VBD made)
        (NP (NNS sciences))
        (PRT (RP up))))
    (, ,)
    (CC and)
    (S
      (NP (PRP it))
      (VP (VBZ 's)
        (ADVP (RB up)
          (PP (TO to)
            (NP (PRP us))))
        (S
          (VP (TO to)
            (VP (VB upgrade)
              (NP (PRP them)))))
        (SBAR (IN in) (NN order)
          (S
            (VP (TO to)
              (VP (VB improve)
                (NP (PRP$ our) (NN knowledge))
                (PP (IN about)
                  (NP (DT the) (NN world)))))))))
    (. .)))

