(ROOT
  (S
    (S
      (NP (DT This))
      (VP (VBZ is)
        (NP (DT a) (NN debate))))
    (, ,)
    (CC and)
    (S
      (NP (PRP you))
      (VP (MD must)
        (VP (VB give)
          (NP (NNS counter-arguments))
          (PP (IN against)
            (NP
              (NP (NNS arguments))
              (VP (VBN brought)
                (S
                  (VP (TO to)
                    (VP
                      (VP (VB bear))
                      (, ,)
                      (CC or)
                      (VP (VB admit)
                        (NP (NN defeat))))))))))))
    (. .)))

nsubj(debate-4, This-1)
cop(debate-4, is-2)
det(debate-4, a-3)
root(ROOT-0, debate-4)
nsubj(give-9, you-7)
aux(give-9, must-8)
conj_and(debate-4, give-9)
dobj(give-9, counter-arguments-10)
prep_against(give-9, arguments-12)
xsubj(bear-15, arguments-12)
partmod(arguments-12, brought-13)
aux(bear-15, to-14)
xcomp(brought-13, bear-15)
xcomp(brought-13, admit-18)
conj_or(bear-15, admit-18)
dobj(admit-18, defeat-19)

