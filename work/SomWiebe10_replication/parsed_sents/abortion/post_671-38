(ROOT
  (S
    (S
      (INTJ (UH OK))
      (, ,)
      (VP (VB let)))
    (VP (VBZ 's)
      (VP (VBN come)
        (PRT (RP back))
        (PP (TO to)
          (NP (DT the) (JJ real) (NN issue)))))
    (. .)))

dep(let-3, OK-1)
csubjpass(come-5, let-3)
auxpass(come-5, 's-4)
root(ROOT-0, come-5)
prt(come-5, back-6)
det(issue-10, the-8)
