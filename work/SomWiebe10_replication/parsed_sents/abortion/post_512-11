  (S
    (NP (PRP It))
    (ADVP (RB probably))
    (VP (MD would) (RB not)
      (VP (VB be)
        (NP
          (NP (DT a) (JJ good) (NN use))
          (PP (IN of)
            (NP
              (NP (NN tax) (NNS dollars))
              (ADVP (RB though)))))))
    (. .)))

