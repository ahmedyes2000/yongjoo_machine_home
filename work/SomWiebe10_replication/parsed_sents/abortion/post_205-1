(ROOT
  (S
    (NP
      (NP (RB Just) (RB as))
      (PP (IN in)
        (NP (DT the) (JJ abstract) (, ,) (JJ capital) (NN punishment))))
    (VP (VBZ is)
      (ADJP (RB morally) (JJ permissible))
      (SBAR
        (WHADVP (WRB when))
        (S
          (NP (EX there))
          (VP (VBZ exists)
            (NP (DT no) (JJ other) (NN option)
              (S
                (VP (TO to)
                  (VP (VB protect)
                    (NP (NN society))))))))))
    (. .)))

advmod(as-2, Just-1)
nsubj(permissible-11, as-2)
det(punishment-8, the-4)
amod(punishment-8, abstract-5)
amod(punishment-8, capital-7)
prep_in(as-2, punishment-8)
cop(permissible-11, is-9)
advmod(permissible-11, morally-10)
root(ROOT-0, permissible-11)
advmod(exists-14, when-12)
expl(exists-14, there-13)
advcl(permissible-11, exists-14)
det(option-17, no-15)
amod(option-17, other-16)
dobj(exists-14, option-17)
nsubj(exists-14, option-17)
aux(protect-19, to-18)
infmod(option-17, protect-19)
dobj(protect-19, society-20)

