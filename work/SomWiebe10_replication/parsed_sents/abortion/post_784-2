(ROOT
  (S (CC But)
    (NP (PRP I))
    (VP (VBP am)
      (ADVP (RB never))
      (NP
        (NP (FW pro))
        (PP (IN of)
          (NP (NN abortion)))
        (SBAR
          (S
            (NP (DT this))
            (VP (VBZ is)
              (ADVP (NN way))
              (ADJP (RB too) (JJ sensitive)))))))
    (. .)))

cc(pro-5, But-1)
nsubj(pro-5, I-2)
cop(pro-5, am-3)
neg(pro-5, never-4)
root(ROOT-0, pro-5)
prep_of(pro-5, abortion-7)
nsubj(sensitive-12, this-8)
cop(sensitive-12, is-9)
advmod(sensitive-12, way-10)
advmod(sensitive-12, too-11)
rcmod(pro-5, sensitive-12)

