(ROOT
  (S (IN So)
    (ADVP (RB really))
    (NP (DT the) (JJ only) (JJ safe) (NN sex))
    (VP (VBZ is)
      (S
        (VP (TO to)
          (VP (VB take)
            (NP
              (NP (DT a) (NN vow))
              (PP (IN of)
                (NP (NN abstinence))))))))
    (. .)))

dep(is-7, So-1)
advmod(is-7, really-2)
det(sex-6, the-3)
amod(sex-6, only-4)
amod(sex-6, safe-5)
nsubj(is-7, sex-6)
