(ROOT
  (S (-LRB- -LRB-)
    (NP
      (NP (PRP$ Your) (NN suggestion))
      (PP (IN of)
        (NP (JJ haploid) (NNS gametes))))
    (VP (VBZ is)
      (ADJP (RB just) (JJ silly)))
    (. .) (-RRB- -RRB-)))

poss(suggestion-3, Your-2)
nsubj(silly-9, suggestion-3)
amod(gametes-6, haploid-5)
prep_of(suggestion-3, gametes-6)
cop(silly-9, is-7)
