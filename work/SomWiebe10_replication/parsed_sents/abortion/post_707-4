(ROOT
  (S
    (NP (PRP You))
    (VP (VBD saw)
      (NP (PRP me))
      (SBAR (IN before)
        (S
          (NP (PRP I))
          (VP (VBD was)
            (VP (VBN born))))))
    (. .)))

nsubj(saw-2, You-1)
root(ROOT-0, saw-2)
dobj(saw-2, me-3)
mark(born-7, before-4)
nsubjpass(born-7, I-5)
auxpass(born-7, was-6)
advcl(saw-2, born-7)

