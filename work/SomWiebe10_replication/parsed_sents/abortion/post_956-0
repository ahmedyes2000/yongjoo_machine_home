(ROOT
  (S
    (NP (PRP I))
    (VP (MD will)
      (VP (VB take)
        (NP
          (NP (DT the) (NN bait))
          (PP (IN for)
            (NP (NN fun))))))
    (. .)))

nsubj(take-3, I-1)
aux(take-3, will-2)
root(ROOT-0, take-3)
det(bait-5, the-4)
dobj(take-3, bait-5)
prep_for(bait-5, fun-7)

