(ROOT
  (S
    (PP (IN By)
      (NP (DT the) (NN way)))
    (, ,)
    (PP (IN in)
      (NP (DT this) (JJ entire) (NN post)))
    (NP (PRP you))
    (VP (VBP make)
      (NP (DT this) (NN assumption))
      (: :)
      (SBAR (IN that)
        (S
          (NP (DT the) (NN fetus))
          (VP (VBZ is)
            (ADVP (RB just))
            (NP
              (NP (NN part))
              (PP (IN of)
                (NP
                  (NP (DT the) (NN woman) (POS 's))
                  (NN body))))))))
    (. .)))

det(way-3, the-2)
prep_by(make-10, way-3)
det(post-8, this-6)
amod(post-8, entire-7)
prep_in(make-10, post-8)
nsubj(make-10, you-9)
root(ROOT-0, make-10)
det(assumption-12, this-11)
dobj(make-10, assumption-12)
complm(part-19, that-14)
det(fetus-16, the-15)
nsubj(part-19, fetus-16)
cop(part-19, is-17)
advmod(part-19, just-18)
ccomp(make-10, part-19)
parataxis(make-10, part-19)
det(woman-22, the-21)
poss(body-24, woman-22)
prep_of(part-19, body-24)

