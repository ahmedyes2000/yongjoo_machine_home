(ROOT
  (NP
    (NP (NNP Pray))
    (PP (IN for)
      (NP (PDT all)
        (NP (PRP$ our) (NNS brothers))
        (CC and)
        (NP (NNS sisters))))
    (PP (IN of)
      (NP (JJ other) (NNS faiths)))
    (. .)))

root(ROOT-0, Pray-1)
dep(brothers-5, all-3)
poss(brothers-5, our-4)
prep_for(Pray-1, brothers-5)
prep_for(Pray-1, sisters-7)
conj_and(brothers-5, sisters-7)
amod(faiths-10, other-9)
prep_of(Pray-1, faiths-10)

