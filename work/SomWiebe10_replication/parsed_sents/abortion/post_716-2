(ROOT
  (S
    (NP (PRP You))
    (VP (VBP make)
      (NP (DT the) (NN statement))
      (, ,) (`` ``)
      (SBAR
        (WHADVP (WRB When))
        (S
          (NP (DT a) (NN woman))
          (VP
            (VP (VBZ chooses)
              (SBAR (IN whether)
                (S
                  (VP (TO to)
                    (VP (VB have)
                      (NP
                        (NP (DT an) (NN abortion))
                        (SBAR
                          (S
                            (NP (PRP they))
                            (VP (MD may)
                              (VP (VB feel)
                                (ADJP (JJ horrible))
                                (PP (IN about)
                                  (NP (PRP it)))))))))))))
            (CC but)
            (VP (VBP know)
              (SBAR (IN that)
                (S
                  (NP (PRP it))
                  (VP (VBZ is)
                    (NP (DT the) (JJS best) (NN decision))))))))))
    (. .)))

