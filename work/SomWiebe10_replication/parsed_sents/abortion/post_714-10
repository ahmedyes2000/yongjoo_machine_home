(ROOT
  (S
    (NP (DT No) (JJ intellectual) (NN arguement))
    (VP (VBZ is)
      (VP (VBG going)
        (S
          (VP (TO to)
            (VP (VB argue)
              (NP (PRP$ their) (NN grief))
              (ADVP (RB away)))))))
    (. .)))

det(arguement-3, No-1)
amod(arguement-3, intellectual-2)
nsubj(going-5, arguement-3)
xsubj(argue-7, arguement-3)
aux(going-5, is-4)
root(ROOT-0, going-5)
aux(argue-7, to-6)
xcomp(going-5, argue-7)
poss(grief-9, their-8)
dobj(argue-7, grief-9)
advmod(argue-7, away-10)

