(ROOT
  (S
    (PP (IN In)
      (NP (DT any) (JJ other) (NN situation)))
    (, ,)
    (NP (EX there))
    (VP (VBP are)
      (NP
        (NP (RB plenty))
        (PP (IN of)
          (NP
            (NP (JJ other) (NNS options))
            (PP (JJ such) (IN as)
              (NP (NN adoption)))))))
    (. .)))

det(situation-4, any-2)
amod(situation-4, other-3)
prep_in(are-7, situation-4)
expl(are-7, there-6)
root(ROOT-0, are-7)
nsubj(are-7, plenty-8)
amod(options-11, other-10)
prep_of(plenty-8, options-11)
prep_such_as(options-11, adoption-14)

