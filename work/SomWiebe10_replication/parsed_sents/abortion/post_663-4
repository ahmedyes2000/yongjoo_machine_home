(ROOT
  (S
    (ADVP (RB However))
    (, ,)
    (NP (PRP I))
    (VP (VBP think)
      (SBAR
        (S
          (NP (PRP it))
          (VP (VBZ is)
            (ADJP (JJ critical))
            (SBAR (IN that)
              (S
                (NP (PRP we))
                (VP (VB stamp)
                  (PRT (RP out))
                  (NP
                    (NP (NNS elements))
                    (PP (IN of)
                      (NP (PRP$ their) (NN ideology)))
                    (SBAR
                      (WHADVP (WRB whenever))
                      (S
                        (NP (PRP it))
                        (VP (VBZ raises)
                          (NP (PRP$ its) (JJ ugly) (NN head)))))))))))))
    (. .)))

advmod(think-4, However-1)
nsubj(think-4, I-3)
root(ROOT-0, think-4)
nsubj(critical-7, it-5)
cop(critical-7, is-6)
ccomp(think-4, critical-7)
complm(stamp-10, that-8)
nsubj(stamp-10, we-9)
ccomp(critical-7, stamp-10)
prt(stamp-10, out-11)
dobj(stamp-10, elements-12)
poss(ideology-15, their-14)
prep_of(elements-12, ideology-15)
advmod(raises-18, whenever-16)
nsubj(raises-18, it-17)
dep(elements-12, raises-18)
poss(head-21, its-19)
amod(head-21, ugly-20)
dobj(raises-18, head-21)

