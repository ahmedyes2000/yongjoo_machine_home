(ROOT
  (X (CC But)
    (S
      (VP (VB let)
        (S
          (NP (PRP me))
          (VP (VB ask)
            (NP
              (NP (PRP you))
              (: ;)
              (SQ
                (VP (VBD were))
                (NP (PRP you))
                (ADVP (RB ever))
                (NP (DT an) (JJ unborn) (NN child))
                (. ?)))))))))

dep(let-2, But-1)
root(ROOT-0, let-2)
nsubj(ask-4, me-3)
ccomp(let-2, ask-4)
dobj(ask-4, you-5)
dep(you-5, were-7)
dep(were-7, you-8)
advmod(were-7, ever-9)
det(child-12, an-10)
amod(child-12, unborn-11)
dep(were-7, child-12)

