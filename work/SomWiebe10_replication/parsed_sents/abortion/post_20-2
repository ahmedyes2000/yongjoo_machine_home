(ROOT
  (S
    (NP (PRP They))
    (VP (MD must)
      (VP (VB tolerate)
        (NP
          (NP (DT a) (NN woman) (POS 's))
          (NN right)
          (S
            (VP (TO to)
              (VP (VB have)
                (NP (DT an) (NN abortion))
                (, ,)
                (SBAR (RB even) (IN if)
                  (S
                    (NP (PRP they))
                    (VP (VBP believe)
                      (S
                        (NP (DT the) (NN act))
                        (VP (TO to)
                          (VP (VB be)
                            (ADJP (RB morally) (JJ wrong))))))))))))))
    (. .)))

nsubj(tolerate-3, They-1)
aux(tolerate-3, must-2)
root(ROOT-0, tolerate-3)
det(woman-5, a-4)
poss(right-7, woman-5)
dobj(tolerate-3, right-7)
aux(have-9, to-8)
infmod(right-7, have-9)
det(abortion-11, an-10)
dobj(have-9, abortion-11)
advmod(believe-16, even-13)
dep(believe-16, if-14)
nsubj(believe-16, they-15)
dep(have-9, believe-16)
det(act-18, the-17)
nsubj(wrong-22, act-18)
aux(wrong-22, to-19)
cop(wrong-22, be-20)
advmod(wrong-22, morally-21)
xcomp(believe-16, wrong-22)

