(ROOT
  (S
    (NP (PRP You))
    (VP (NN might)
      (CONJP (RB as) (RB well))
      (VB ban)
      (NP
        (NP (JJ ugly) (NNS ties))
        (CC and)
        (NP (NN disco) (NN music))))
    (. .)))

nsubj(might-2, You-1)
nsubj(ban-5, You-1)
root(ROOT-0, might-2)
conj_and(might-2, ban-5)
amod(ties-7, ugly-6)
dobj(might-2, ties-7)
nn(music-10, disco-9)
dobj(might-2, music-10)
conj_and(ties-7, music-10)

