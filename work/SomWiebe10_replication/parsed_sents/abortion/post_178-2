(ROOT
  (S (CC But)
    (NP (DT that))
    (VP (MD would)
      (VP (VB require)
        (S
          (NP (PRP us))
          (VP (TO to)
            (VP (VB teach)
              (NP (PRP$ our) (NNS children))
              (PP (IN about)
                (NP (NN sex)
                  (CC and)
                  (NNS contraceptives))))))))
    (. .)))

cc(require-4, But-1)
nsubj(require-4, that-2)
aux(require-4, would-3)
root(ROOT-0, require-4)
nsubj(teach-7, us-5)
aux(teach-7, to-6)
