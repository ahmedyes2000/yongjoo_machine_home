(ROOT
  (S
    (VP (NN Note) (: :)
      (S
        (NP (DT some))
        (NP
          (NP (NNS bans))
          (PP (IN on)
            (NP (JJ partial) (NN birth) (NNS abortions))))
        (, ,)
        (S
          (VP (VB allow)
            (PP (IN for)
              (NP (PRP them)))
            (SBAR
              (WHADVP (WRB when))
              (S
                (NP (PRP it))
                (VP (MD would)
                  (VP (VB save)
                    (NP
                      (NP (DT the) (NN life))
                      (PP (IN of)
                        (NP (DT the) (NN mother))))))))))))
    (. .)))

