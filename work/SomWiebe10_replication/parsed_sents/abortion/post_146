  (S
    (NP
      (NP (DT A) (NN blanket) (NN ban))
      (PP (IN on)
        (NP (JJ partial) (NN birth) (NNS abortions))))
    (VP (MD may)
      (VP (VB make)
        (S
          (NP (PRP it))
          (ADJP (JJ impossible)
            (S
              (VP (TO to)
                (VP (VB save)
                  (NP
                    (NP
                      (NP (DT a) (NN woman) (POS 's))
                      (NN life))
                    (PP (IN in)
                      (NP (JJ certain) (JJ medical) (NNS circumstances)))))))))))
    (. .)))

