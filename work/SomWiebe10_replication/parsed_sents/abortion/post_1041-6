(ROOT
  (S
    (NP (DT This))
    (VP (VBZ is)
      (ADJP (RB specially) (JJ valid)
        (PP (IN for)
          (NP
            (NP (DT the) (NN question))
            (PP (IN of)
              (NP (NN abortion))))))
      (, ,)
      (SBAR (IN since)
        (S
          (NP
            (NP (PRP$ our) (JJ old) (NNS conceptions))
            (PP (IN about)
              (SBAR
                (WHADVP (WRB when))
                (S
                  (NP (NN life))
                  (VP (VBZ begins))))))
          (VP (MD might)
            (VP (VB be)
              (ADJP (JJ wrong)))))))
    (. .)))

nsubj(valid-4, This-1)
cop(valid-4, is-2)
advmod(valid-4, specially-3)
root(ROOT-0, valid-4)
det(question-7, the-6)
prep_for(valid-4, question-7)
prep_of(question-7, abortion-9)
mark(wrong-21, since-11)
poss(conceptions-14, our-12)
amod(conceptions-14, old-13)
nsubj(wrong-21, conceptions-14)
advmod(begins-18, when-16)
nsubj(begins-18, life-17)
prepc_about(conceptions-14, begins-18)
aux(wrong-21, might-19)
cop(wrong-21, be-20)
advcl(valid-4, wrong-21)

