(ROOT
  (S
    (ADVP (RB Again))
    (, ,)
    (VP
      (VP (VB stop)
        (NP (NN cop-ing))
        (PRT (RP out)))
      (CC and)
      (VP (VB keep)
        (NP (PRP it))
        (PP (TO to)
          (NP (DT the) (NN topic)))))
    (. .)))

advmod(stop-3, Again-1)
root(ROOT-0, stop-3)
dobj(stop-3, cop-ing-4)
prt(stop-3, out-5)
conj_and(stop-3, keep-7)
dobj(keep-7, it-8)
det(topic-11, the-10)
