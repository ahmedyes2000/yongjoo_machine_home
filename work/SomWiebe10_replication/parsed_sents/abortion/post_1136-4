(ROOT
  (S
    (NP (PRP You))
    (VP (VBZ Kamranw)
      (VP (VB forget)
        (NP
          (NP (DT the) (NN situation))
          (PP (IN of)
            (NP
              (NP (DT the) (NN mother) (POS 's))
              (NN death))))))
    (. .)))

nsubj(Kamranw-2, You-1)
root(ROOT-0, Kamranw-2)
xcomp(Kamranw-2, forget-3)
det(situation-5, the-4)
dobj(forget-3, situation-5)
det(mother-8, the-7)
poss(death-10, mother-8)
prep_of(situation-5, death-10)

