(ROOT
  (S
    (NP (PRP You))
    (VP (VBP are)
      (VP (VBG justifying)
        (NP (PRP$ your) (NN intent))))
    (. !)))

nsubj(justifying-3, You-1)
aux(justifying-3, are-2)
root(ROOT-0, justifying-3)
poss(intent-5, your-4)
dobj(justifying-3, intent-5)

(ROOT
  (S
    (VP (VBG Justifying)
      (NP (DT the) (NN undebatable))
      (PP (IN with)
        (NP (NN debate))))
    (. !)))

root(ROOT-0, Justifying-1)
det(undebatable-3, the-2)
dobj(Justifying-1, undebatable-3)
prep_with(Justifying-1, debate-5)

