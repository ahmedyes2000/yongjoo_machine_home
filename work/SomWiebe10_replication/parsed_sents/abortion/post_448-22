(ROOT
  (SBARQ
    (WHADVP (WRB Where))
    (SQ (VBP are)
      (NP (PRP you))
      (NP (NNS people))
      (PP (IN in)
        (S
          (VP (VBG regards)
            (PP (TO to)
              (NP (PRP them)))))))
    (. ?)))

