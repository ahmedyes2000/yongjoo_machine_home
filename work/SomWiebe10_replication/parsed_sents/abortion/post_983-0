(ROOT
  (S
    (NP (PRP You))
    (VP (VBP are)
      (ADJP (JJ correct)
        (PP (IN in)
          (NP
            (NP (PRP$ your) (NN assessment))
            (PP (IN of)
              (NP (PRP me)))))))
    (. .)))

nsubj(correct-3, You-1)
cop(correct-3, are-2)
root(ROOT-0, correct-3)
poss(assessment-6, your-5)
prep_in(correct-3, assessment-6)
prep_of(assessment-6, me-8)

(ROOT
  (S
    (ADVP (RB Doctrinally))
    (NP (PRP I))
    (VP (VBP consider)
      (NP (PRP myself))
      (NP
        (NP (DT a) (NNP Reformed) (NNP Baptist))
        (, ,)
        (CC but)
        (RB primarily)
        (NP
          (NP (DT a) (NN follower))
          (PP (IN of)
            (NP
              (NP (DT the) (JJ historical) (NN Jesus))
              (PP (IN of)
                (NP (NNP Nazareth))))))))
    (. .)))

advmod(consider-3, Doctrinally-1)
nsubj(consider-3, I-2)
root(ROOT-0, consider-3)
iobj(consider-3, myself-4)
det(Baptist-7, a-5)
nn(Baptist-7, Reformed-6)
dobj(consider-3, Baptist-7)
dobj(consider-3, primarily-10)
advmod(Baptist-7, primarily-10)
conj_but(Baptist-7, primarily-10)
det(follower-12, a-11)
dep(Baptist-7, follower-12)
det(Jesus-16, the-14)
amod(Jesus-16, historical-15)
prep_of(follower-12, Jesus-16)
prep_of(Jesus-16, Nazareth-18)

