(ROOT
  (S ('' '')
    (S
      (PP (IN As)
        (ADVP (RB long) (RB as)))
      (NP (PRP I))
      (VP (VBP ask)
        (PP (IN for)
          (NP (NN forgiveness)))))
    (, ,)
    (NP (PRP I))
    (VP (MD will)
      (VP (VB be)
        (VP (VBN saved))))
    (. .)))

prep(ask-6, As-2)
advmod(as-4, long-3)
pcomp(As-2, as-4)
nsubj(ask-6, I-5)
ccomp(saved-13, ask-6)
prep_for(ask-6, forgiveness-8)
