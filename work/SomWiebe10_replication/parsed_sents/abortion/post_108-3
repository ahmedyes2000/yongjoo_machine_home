  (S
    (NP (DT The) (JJ real) (NN inconsistency))
    (VP (VBZ is)
      (PP (IN in)
        (S
          (VP (VBG supporting)
            (NP (NN abortion))
            (PP (IN while)
              (S
                (VP (VBG opposing)
                  (NP (NN capital) (NN punishment)))))))))
    (. .)))

