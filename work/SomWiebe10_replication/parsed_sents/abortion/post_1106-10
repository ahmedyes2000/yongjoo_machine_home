(ROOT
  (S
    (NP (DT This))
    (VP (VBZ is)
      (SBAR
        (WHADVP (WRB why))
        (S
          (NP
            (NP (DT a) (NN ban))
            (PP (IN on)
              (NP (NN abortion))))
          (VP (MD would)
            (VP (VB fail))))))
    (. .)))

nsubj(is-2, This-1)
root(ROOT-0, is-2)
advmod(fail-9, why-3)
det(ban-5, a-4)
nsubj(fail-9, ban-5)
prep_on(ban-5, abortion-7)
aux(fail-9, would-8)
advcl(is-2, fail-9)

