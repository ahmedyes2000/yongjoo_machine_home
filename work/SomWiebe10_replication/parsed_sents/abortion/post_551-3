(ROOT
  (S
    (NP (DT That) (JJ damn) (NN government))
    (ADVP (RB also))
    (VP (VBZ keeps)
      (NP (PRP me))
      (PP (IN from)
        (S
          (VP (VBG murdering)
            (NP (NNS others))))))
    (. !)))

det(government-3, That-1)
amod(government-3, damn-2)
nsubj(keeps-5, government-3)
advmod(keeps-5, also-4)
root(ROOT-0, keeps-5)
dobj(keeps-5, me-6)
prepc_from(keeps-5, murdering-8)
dobj(murdering-8, others-9)

