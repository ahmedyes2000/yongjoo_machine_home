(ROOT
  (S (-LRB- -LRB-)
    (NP (PRP I))
    (VP (VBP qualify)
      (NP (DT that) (NN statement))
      (SBAR (IN because)
        (S
          (NP (NN science))
          (VP (MD ca) (RB n't)
            (ADVP (RB even))
            (VP (VB prove)
              (PRN
                (S
                  (NP (PRP we))
                  (VP (VBP 're) (RB all)
                    (ADVP (RB here))))
                (-RRB- -RRB-))
              (SBAR
                (S
                  (NP (PRP We))
                  (VP (VBP do) (RB not)
                    (ADVP (RB intentionally))
                    (VP (VB kill)
                      (NP (JJ innocent) (NNS civilians))
                      (PP (IN in)
                        (NP (NNP Iraq)
                          (CC or)
                          (NNP Afghanistan))))))))))))
    (. .)))

nsubj(qualify-3, I-2)
root(ROOT-0, qualify-3)
det(statement-5, that-4)
dobj(qualify-3, statement-5)
mark(prove-11, because-6)
nsubj(prove-11, science-7)
aux(prove-11, ca-8)
neg(prove-11, n't-9)
advmod(prove-11, even-10)
advcl(qualify-3, prove-11)
nsubj('re-13, we-12)
parataxis(prove-11, 're-13)
advmod('re-13, all-14)
advmod('re-13, here-15)
nsubj(kill-21, We-17)
aux(kill-21, do-18)
neg(kill-21, not-19)
advmod(kill-21, intentionally-20)
ccomp(prove-11, kill-21)
amod(civilians-23, innocent-22)
dobj(kill-21, civilians-23)
prep_in(kill-21, Iraq-25)
prep_in(kill-21, Afghanistan-27)
conj_or(Iraq-25, Afghanistan-27)

