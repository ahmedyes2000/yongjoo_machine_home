(ROOT
  (S
    (PP (IN At)
      (NP
        (NP (DT the) (NN moment))
        (PP (IN of)
          (NP (NN conception)))))
    (NP (DT a) (JJ human) (NN life))
    (VP (VBZ has)
      (VP (VBN been)
        (VP (VBN created))))
    (. .)))

det(moment-3, the-2)
prep_at(created-11, moment-3)
prep_of(moment-3, conception-5)
det(life-8, a-6)
amod(life-8, human-7)
nsubjpass(created-11, life-8)
aux(created-11, has-9)
auxpass(created-11, been-10)
root(ROOT-0, created-11)

