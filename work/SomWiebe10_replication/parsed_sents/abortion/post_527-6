(ROOT
  (S
    (S
      (NP (PRP it))
      (VP (VBD was)
        (NP
          (NP (DT the) (JJ last) (NN thing))
          (SBAR
            (S
              (NP (FW i))
              (VP (VBD said) (, ,)
                (S
                  (VP (VBG k.r.)
                    (SBAR
                      (S
                        (NP (EX there))
                        (VP (VBP are)
                          (NP
                            (NP (DT some) (NNS people))
                            (PP (IN on)
                              (NP (RB here)))
                            (SBAR
                              (WHNP (WDT that))
                              (S
                                (VP (VBP think)
                                  (SBAR (IN that) (IN because)
                                    (S
                                      (NP (FW i))
                                      (VP (MD will)
                                        (VP
                                          (ADVP (RB never))
                                          (PP (IN under)
                                            (NP (DT any) (NNS circumstances))))))))))))))))))))))
    (: ...)
    (CC and)
    (S
      (NP (FW i))
      (VP (VBP do) (RB n't)
        (VP (VB care)
          (SBAR (IN if)
            (S
              (NP (PRP it))
              (VP (VBD meant)
                (SBAR (IN that)
                  (S
                    (NP
                      (QP (CD ten) (CD million))
                      (NNS babies))
                    (VP (MD would)
                      (VP (VB die)
                        (VP (VB support)
                          (NP
                            (NP (DT the) (JJ forced) (NN use))
                            (PP (IN of)
                              (NP
                                (NP
                                  (NP
                                    (NP (CD one) (NN person) (POS 's))
                                    (NN body))
                                  (PP (IN for)
                                    (NP (DT the) (NNS needs)))
                                  (, ,)
                                  (VP (VBZ wants))
                                  (, ,))
                                (CC or)
                                (NP
                                  (NP (NNS expectations))
                                  (PP (IN of)
                                    (NP (DT another))))))))))))))))))
    (. .)))

