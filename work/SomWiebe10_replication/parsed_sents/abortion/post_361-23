(ROOT
  (S
    (NP (DT The) (NN mother))
    (VP (VBZ does) (RB not)
      (VP (VB want)
        (NP (DT the) (JJ unwelcome) (NN guest))))
    (. .)))

det(mother-2, The-1)
nsubj(want-5, mother-2)
aux(want-5, does-3)
neg(want-5, not-4)
root(ROOT-0, want-5)
det(guest-8, the-6)
amod(guest-8, unwelcome-7)
dobj(want-5, guest-8)

