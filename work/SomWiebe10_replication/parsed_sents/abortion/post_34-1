(ROOT
  (S
    (NP (DT This))
    (VP (VBZ is)
      (NP
        (NP (DT the) (NN case))
        (PP (IN in)
          (S
            (VP (VBG sending)
              (NP (NNS soldiers))
              (PP (TO to)
                (NP (NN war))))))))
    (. .)))

nsubj(case-4, This-1)
cop(case-4, is-2)
det(case-4, the-3)
root(ROOT-0, case-4)
prepc_in(case-4, sending-6)
dobj(sending-6, soldiers-7)
prep_to(sending-6, war-9)

