(ROOT
  (S
    (VP (VBN Usually)
      (S
        (ADJP
          (ADVP (RB somewhere) (RB far))
          (RBR less) (JJ safe)))
      (PP (IN than)
        (NP (DT a) (NN hospital))))
    (. .)))

root(ROOT-0, Usually-1)
advmod(far-3, somewhere-2)
advmod(safe-5, far-3)
advmod(safe-5, less-4)
acomp(Usually-1, safe-5)
det(hospital-8, a-7)
prep_than(Usually-1, hospital-8)

