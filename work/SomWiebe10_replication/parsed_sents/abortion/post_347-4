(ROOT
  (S
    (NP (DT This))
    (ADVP (RB also))
    (VP (VBZ applies)
      (PP (IN in)
        (NP
          (NP (NNS cases))
          (PP (IN of)
            (NP (NN incest))))))
    (. .)))

nsubj(applies-3, This-1)
advmod(applies-3, also-2)
root(ROOT-0, applies-3)
prep_in(applies-3, cases-5)
prep_of(cases-5, incest-7)

