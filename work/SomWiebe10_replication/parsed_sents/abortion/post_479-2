(ROOT
  (S
    (NP (PRP We))
    (VP (VBP are)
      (VP (VBG moving)
        (PRT (RP away))
        (PP (IN from)
          (S
            (VP (VBG being)
              (NP
                (NP (CD one))
                (NP (DT every) (NN day))))))))
    (. .)))

nsubj(moving-3, We-1)
aux(moving-3, are-2)
root(ROOT-0, moving-3)
cop(one-7, being-6)
prepc_away_from(moving-3, one-7)
