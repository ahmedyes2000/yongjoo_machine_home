(ROOT
  (S (CC And)
    (NP (PRP you))
    (VP (VBP wonder)
      (SBAR
        (WHADVP (WRB why))
        (S
          (NP (DT no) (NN one))
          (VP (VBZ likes)
            (S
              (VP (VBG talking)
                (NP (NN hardcore) (NNS Christians))
                (PP (IN about)
                  (NP
                    (NP (DT this) (NN kind))
                    (PP (IN of)
                      (NP (NN thing) (CD .3)))))))))))
    (. .)))

cc(wonder-3, And-1)
nsubj(wonder-3, you-2)
root(ROOT-0, wonder-3)
advmod(likes-7, why-4)
det(one-6, no-5)
