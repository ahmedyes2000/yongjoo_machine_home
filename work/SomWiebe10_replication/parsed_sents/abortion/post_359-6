(ROOT
  (S (CC And)
    (NP (NNS condoms))
    (VP (VBP are)
      (ADVP (RB always))
      (ADJP (RBR more) (JJ effective)
        (PP (IN than)
          (NP (JJ unprotected) (NN sex)))))
    (. .)))

cc(effective-6, And-1)
nsubj(effective-6, condoms-2)
cop(effective-6, are-3)
advmod(effective-6, always-4)
advmod(effective-6, more-5)
root(ROOT-0, effective-6)
amod(sex-9, unprotected-8)
prep_than(effective-6, sex-9)

