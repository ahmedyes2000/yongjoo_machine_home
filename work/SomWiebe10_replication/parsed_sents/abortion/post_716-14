(ROOT
  (S
    (NP (PRP I))
    (VP (VBP 'm)
      (VP (VBG basing)
        (NP (PRP it))
        (PP (IN on)
          (SBAR
            (WHNP (WP what))
            (S
              (NP (NNP God))
              (VP (VBZ has)
                (VP (VBN revealed)
                  (PP (TO to)
                    (NP (NN mankind)))
                  (PP (IN through)
                    (NP (DT the) (NNP Bible))))))))))
    (. .)))

nsubj(basing-3, I-1)
aux(basing-3, 'm-2)
root(ROOT-0, basing-3)
dobj(basing-3, it-4)
dobj(revealed-9, what-6)
nsubj(revealed-9, God-7)
aux(revealed-9, has-8)
prepc_on(basing-3, revealed-9)
prep_to(revealed-9, mankind-11)
det(Bible-14, the-13)
prep_through(revealed-9, Bible-14)

