(ROOT
  (S
    (NP
      (NP (DT Any) (NN argument))
      (PP (TO to)
        (NP (DT the) (NN contrary))))
    (VP (VBZ is)
      (ADJP (JJ delusional)))
    (. .)))

det(argument-2, Any-1)
nsubj(delusional-7, argument-2)
det(contrary-5, the-4)
prep_to(argument-2, contrary-5)
cop(delusional-7, is-6)
root(ROOT-0, delusional-7)

