(ROOT
  (S
    (NP (NNS Humans))
    (VP (VBP are)
      (ADVP (RB always))
      (VP (VBG increasing)
        (PP (IN in)
          (NP (PRP$ their) (NN independence)
            (CC and)
            (NN ability)))
        (S
          (VP (TO to)
            (VP (VB survive))))))
    (. .)))

nsubj(increasing-4, Humans-1)
xsubj(survive-11, Humans-1)
aux(increasing-4, are-2)
advmod(increasing-4, always-3)
root(ROOT-0, increasing-4)
poss(independence-7, their-6)
prep_in(increasing-4, independence-7)
prep_in(increasing-4, ability-9)
conj_and(independence-7, ability-9)
aux(survive-11, to-10)
xcomp(increasing-4, survive-11)

