(ROOT
  (S
    (ADVP (RB Therefore))
    (NP (DT the) (NN church))
    (VP (VBZ has)
      (VP (VBN spoken)
        (ADVP (RB definitely))
        (PP (IN on)
          (NP (DT the) (NN matter)))))
    (. .)))

