(ROOT
  (S
    (NP (EX There))
    (VP (VBZ 's)
      (ADVP (RB just)))
    (. .) (. .)))

expl('s-2, There-1)
root(ROOT-0, 's-2)
advmod('s-2, just-3)

(ROOT
  (NP
    (NP (DT a) (NN line))
    (PP (IN in)
      (NP (DT a) (NN textbook)))
    (. .)))

det(line-2, a-1)
root(ROOT-0, line-2)
det(textbook-5, a-4)
prep_in(line-2, textbook-5)

