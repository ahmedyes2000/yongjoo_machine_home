(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (DT this))
        (VP (VBZ is)
          (NP (DT the) (NN case)))))
    (, ,)
    (NP (DT that) (NN woman))
    (VP (MD should) (RB not)
      (ADVP (RB even))
      (VP (VB be)
        (VP (VBG trying)
          (S
            (VP (TO to)
              (VP (VB have)
                (NP (DT a) (NN child))
                (SBAR (IN because)
                  (S
                    (NP (PRP she))
                    (VP (VBZ is)
                      (NP (DT an)
                        (ADJP (JJ immoral)
                          (CC and)
                          (JJ selfish))
                        (NN person)))))))))))
    (. .)))

mark(case-5, If-1)
nsubj(case-5, this-2)
cop(case-5, is-3)
det(case-5, the-4)
advcl(trying-13, case-5)
det(woman-8, that-7)
nsubj(trying-13, woman-8)
aux(trying-13, should-9)
neg(trying-13, not-10)
advmod(trying-13, even-11)
aux(trying-13, be-12)
root(ROOT-0, trying-13)
aux(have-15, to-14)
xcomp(trying-13, have-15)
det(child-17, a-16)
dobj(have-15, child-17)
mark(person-25, because-18)
nsubj(person-25, she-19)
cop(person-25, is-20)
det(person-25, an-21)
amod(person-25, immoral-22)
conj_and(immoral-22, selfish-24)
amod(person-25, selfish-24)
advcl(have-15, person-25)

