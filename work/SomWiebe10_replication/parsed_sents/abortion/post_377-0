(ROOT
  (S
    (NP (DT The) (NN man))
    (VP (VBZ has)
      (NP
        (NP (DT no) (NN responsibility))
        (SBAR (IN until)
          (S
            (NP (DT the) (NN child))
            (VP (VBZ is)
              (VP (VBN born)))))))
    (. .)))

det(man-2, The-1)
nsubj(has-3, man-2)
root(ROOT-0, has-3)
det(responsibility-5, no-4)
dobj(has-3, responsibility-5)
mark(born-10, until-6)
det(child-8, the-7)
nsubjpass(born-10, child-8)
auxpass(born-10, is-9)
dep(responsibility-5, born-10)

