(ROOT
  (S
    (PP (IN After)
      (NP (DT all)))
    (, ,)
    (NP (DT that) (NN viewpoint))
    (VP (VBZ has)
      (VP (VBN been)
        (VP (VBN forced)
          (PP (IN on)
            (NP (PRP me)))
          (, ,)
          (PP (IN against)
            (NP (PRP$ my) (NNS wishes))))))
    (. .)))

prep_after(forced-8, all-2)
det(viewpoint-5, that-4)
nsubjpass(forced-8, viewpoint-5)
aux(forced-8, has-6)
auxpass(forced-8, been-7)
root(ROOT-0, forced-8)
prep_on(forced-8, me-10)
poss(wishes-14, my-13)
prep_against(forced-8, wishes-14)

