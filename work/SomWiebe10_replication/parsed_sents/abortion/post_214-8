(ROOT
  (S
    (SBAR
      (ADVP (RB Long))
      (IN after)
      (S
        (NP (NNP Obama))
        (VP (VBZ leaves)
          (NP (NN office)))))
    (, ,)
    (NP (DT those) (NNS justices))
    (VP (MD will)
      (VP (VB be)
        (VP (VBG striking)
          (PRT (RP down))
          (NP (JJ pro-life) (NNS initiatives)))))
    (. .)))

advmod(leaves-4, Long-1)
dep(leaves-4, after-2)
nsubj(leaves-4, Obama-3)
advcl(striking-11, leaves-4)
dobj(leaves-4, office-5)
det(justices-8, those-7)
nsubj(striking-11, justices-8)
aux(striking-11, will-9)
aux(striking-11, be-10)
root(ROOT-0, striking-11)
prt(striking-11, down-12)
amod(initiatives-14, pro-life-13)
dobj(striking-11, initiatives-14)

