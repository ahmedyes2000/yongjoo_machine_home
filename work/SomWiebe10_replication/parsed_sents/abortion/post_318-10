(ROOT
  (S
    (S
      (VP (VBG Doing)
        (NP (NN nothing))))
    (VP (MD would)
      (VP (VB allow)
        (NP (DT the) (NN child)
          (S
            (VP (TO to)
              (VP (VB live)))))))
    (. .)))

csubj(allow-4, Doing-1)
dobj(Doing-1, nothing-2)
aux(allow-4, would-3)
root(ROOT-0, allow-4)
det(child-6, the-5)
dobj(allow-4, child-6)
aux(live-8, to-7)
infmod(child-6, live-8)

