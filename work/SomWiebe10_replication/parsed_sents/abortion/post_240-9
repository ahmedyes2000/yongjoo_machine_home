(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP (VBP do) (RB n't)
          (VP (VB believe)
            (PP (IN in)
              (NP (NN abortion)))))))
    (ADVP (RB then))
    (VP (VBP do) (RB n't)
      (VP (VB have)
        (NP (CD one))))
    (. .)))

mark(believe-5, If-1)
nsubj(believe-5, you-2)
aux(believe-5, do-3)
neg(believe-5, n't-4)
advcl(have-11, believe-5)
csubj(have-11, believe-5)
prep_in(believe-5, abortion-7)
advmod(have-11, then-8)
aux(have-11, do-9)
neg(have-11, n't-10)
root(ROOT-0, have-11)
dobj(have-11, one-12)

