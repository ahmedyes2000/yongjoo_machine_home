(ROOT
  (S
    (NP (PRP It))
    (VP (VBD was)
      (PP (IN about)
        (NP
          (NP (DT the) (NN government) (POS 's))
          (NN ability)
          (S
            (VP (TO to)
              (VP (VB make)
                (NP (NNS laws))))))))
    (. .)))

nsubj(was-2, It-1)
root(ROOT-0, was-2)
det(government-5, the-4)
poss(ability-7, government-5)
prep_about(was-2, ability-7)
aux(make-9, to-8)
infmod(ability-7, make-9)
dobj(make-9, laws-10)

