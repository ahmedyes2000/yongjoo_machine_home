(ROOT
  (S
    (PP (IN By)
      (NP (PRP$ your) (NN logic)))
    (NP (NN nothing))
    (VP (MD should)
      (VP (VB be)
        (ADJP (JJ illegal))
        (SBAR (IN because)
          (S
            (NP (NNS people))
            (VP (VBP do)
              (NP (PRP it))
              (ADVP (RB anyway)))))))
    (. .)))

poss(logic-3, your-2)
prep_by(illegal-7, logic-3)
nsubj(illegal-7, nothing-4)
aux(illegal-7, should-5)
cop(illegal-7, be-6)
root(ROOT-0, illegal-7)
mark(do-10, because-8)
nsubj(do-10, people-9)
advcl(illegal-7, do-10)
dobj(do-10, it-11)
advmod(do-10, anyway-12)

