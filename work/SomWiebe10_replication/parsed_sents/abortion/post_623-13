(ROOT
  (SBARQ
    (WHADVP (WRB Why)
      (ADJP (RB else)))
    (SQ (MD would)
      (NP (PRP he))
      (VP (VB allow)
        (NP (PDT such) (DT a) (NN thing))))
    (. ?)))

advmod(allow-5, Why-1)
dep(Why-1, else-2)
aux(allow-5, would-3)
nsubj(allow-5, he-4)
root(ROOT-0, allow-5)
predet(thing-8, such-6)
