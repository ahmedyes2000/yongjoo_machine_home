(ROOT
  (S
    (NP
      (NP (DT A) (NN sperm))
      (CC and)
      (NP (DT an) (NN egg)))
    (VP (VBP are)
      (ADJP (JJ analogous)
        (PP (TO to)
          (NP
            (NP (DT the) (NN chlorine))
            (CC and)
            (NP (DT the) (NN sodium))))))
    (. .)))

det(sperm-2, A-1)
nsubj(analogous-7, sperm-2)
det(egg-5, an-4)
conj_and(sperm-2, egg-5)
nsubj(analogous-7, egg-5)
cop(analogous-7, are-6)
root(ROOT-0, analogous-7)
det(chlorine-10, the-9)
