(ROOT
  (S
    (ADVP (RB Personally))
    (, ,)
    (NP (PRP I))
    (VP (VBP do) (RB n't)
      (VP (VB think)
        (SBAR
          (S
            (NP (NN abortion))
            (VP (MD should)
              (VP (VB be)
                (VP (VBN allowed)
                  (ADVP (RB anywhere)))))))))
    (. .)))

advmod(think-6, Personally-1)
nsubj(think-6, I-3)
aux(think-6, do-4)
neg(think-6, n't-5)
root(ROOT-0, think-6)
nsubjpass(allowed-10, abortion-7)
aux(allowed-10, should-8)
auxpass(allowed-10, be-9)
ccomp(think-6, allowed-10)
advmod(allowed-10, anywhere-11)

