(ROOT
  (S (RB Then)
    (VP (VB suppose)
      (SBAR
        (S
          (NP (PRP I))
          (VP (VBD stated) (, ,) (`` ``)
            (S
              (NP (PRP I))
              (VP (VB need)
                (NP (JJR more) (NN furniture))))))))
    (. .)))

advmod(suppose-2, Then-1)
root(ROOT-0, suppose-2)
nsubj(stated-4, I-3)
ccomp(suppose-2, stated-4)
nsubj(need-8, I-7)
ccomp(stated-4, need-8)
amod(furniture-10, more-9)
dobj(need-8, furniture-10)

