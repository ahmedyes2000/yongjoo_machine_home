(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP (VBP want)
          (S
            (VP (TO to)
              (VP (VB protect)
                (NP (DT the) (JJ precious) (JJ unborn) (NNS babies))))))))
    (, ,)
    (VP (VB offer)
      (NP (DT some) (NN support))
      (PP (TO to)
        (NP
          (NP (NNS mothers))
          (PP (IN with)
            (NP (JJ unwed) (NNS pregnancies))))))
    (. .)))

mark(want-3, If-1)
nsubj(want-3, you-2)
xsubj(protect-5, you-2)
advcl(offer-11, want-3)
aux(protect-5, to-4)
xcomp(want-3, protect-5)
det(babies-9, the-6)
amod(babies-9, precious-7)
amod(babies-9, unborn-8)
dobj(protect-5, babies-9)
root(ROOT-0, offer-11)
det(support-13, some-12)
dobj(offer-11, support-13)
prep_to(offer-11, mothers-15)
amod(pregnancies-18, unwed-17)
prep_with(mothers-15, pregnancies-18)

