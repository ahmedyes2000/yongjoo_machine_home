(ROOT
  (S
    (NP (JJ Late) (NN term) (NNS abortions))
    (VP (VBP are)
      (ADVP (RB only))
      (VP (VBN done)
        (SBAR (IN if)
          (S
            (NP
              (NP (DT the) (NN mother) (POS 's))
              (NN health))
            (VP (VBZ is)
              (PP (IN at)
                (NP (NN stake))))))))
    (. .)))

amod(abortions-3, Late-1)
nn(abortions-3, term-2)
nsubjpass(done-6, abortions-3)
auxpass(done-6, are-4)
advmod(done-6, only-5)
root(ROOT-0, done-6)
mark(is-12, if-7)
det(mother-9, the-8)
poss(health-11, mother-9)
nsubj(is-12, health-11)
advcl(done-6, is-12)
prep_at(is-12, stake-14)

