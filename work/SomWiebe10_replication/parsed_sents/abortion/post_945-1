(ROOT
  (S
    (NP (PRP I))
    (ADVP (RB never))
    (VP (VBD mentioned)
      (NP (NNS videos)
        (CC or)
        (NNS movies)))
    (. !)))

nsubj(mentioned-3, I-1)
neg(mentioned-3, never-2)
root(ROOT-0, mentioned-3)
dobj(mentioned-3, videos-4)
dobj(mentioned-3, movies-6)
conj_or(videos-4, movies-6)

