(ROOT
  (S
    (SBAR
      (WHADVP (WRB Where))
      (, ,)
      (S
        (NP (RB as) (PRP it))
        (VP (VBZ says)
          (PP (IN in)
            (NP
              (NP (DT the) (JJ last) (NN book))
              (PP (IN of)
                (NP (DT the) (NNP Bible) ('' ''))))))))
    (NP (PRP He))
    (VP (MD will)
      (VP (VB wipe)
        (PRT (RP away))
        (NP
          (NP (DT every) (NN tear))
          (PP (IN from)
            (NP (PRP$ their) (NNS eyes))))))
    (. .)))

advmod(says-5, Where-1)
advmod(it-4, as-3)
nsubj(says-5, it-4)
advcl(wipe-16, says-5)
det(book-9, the-7)
amod(book-9, last-8)
prep_in(says-5, book-9)
det(Bible-12, the-11)
prep_of(book-9, Bible-12)
nsubj(wipe-16, He-14)
aux(wipe-16, will-15)
root(ROOT-0, wipe-16)
prt(wipe-16, away-17)
det(tear-19, every-18)
dobj(wipe-16, tear-19)
poss(eyes-22, their-21)
prep_from(tear-19, eyes-22)

