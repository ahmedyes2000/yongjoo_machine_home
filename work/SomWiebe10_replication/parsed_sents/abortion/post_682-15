  (S
    (NP (PRP We))
    (VP (VBP have)
      (NP
        (NP (DT a) (JJ brief) (JJ haploid) (NN period))
        (PP (IN in)
          (NP
            (NP (PRP$ our) (NN life) (NN cycle))
            (, ,)
            (SBAR
              (WHNP (WDT which))
              (S
                (VP (VBZ occurs)
                  (PP (IN at)
                    (NP
                      (NP (DT the) (JJ very) (NN beginning))
                      (PP (IN of)
                        (NP (JJ human) (NN reproduction))))))))))))
    (. .)))

nsubj(have-2, We-1)
root(ROOT-0, have-2)
det(period-6, a-3)
amod(period-6, brief-4)
amod(period-6, haploid-5)
