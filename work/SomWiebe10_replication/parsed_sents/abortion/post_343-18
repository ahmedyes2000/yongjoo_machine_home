(ROOT
  (S
    (PP (IN After)
      (NP (DT all)))
    (, ,)
    (NP (PRP it))
    (VP (VBZ 's)
      (ADVP (RB just))
      (NP
        (NP (NN nature))
        (VP (VBG taking)
          (NP (PRP$ its) (NN course)))))
    (. .)))

prep_after(nature-7, all-2)
nsubj(nature-7, it-4)
cop(nature-7, 's-5)
advmod(nature-7, just-6)
root(ROOT-0, nature-7)
partmod(nature-7, taking-8)
poss(course-10, its-9)
dobj(taking-8, course-10)

