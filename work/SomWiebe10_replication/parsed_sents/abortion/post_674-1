(ROOT
  (S
    (INTJ (DT That) (VBZ is))
    (, ,)
    (S
      (S
        (VP (VBG assuming)
          (NP (PRP we))))
      (VP (VBP have)
        (NP
          (NP (DT the) (JJ same) (NN definition))
          (PP (IN of)
            (NP (NN conception))))))
    (. .)))

dep(have-6, That-1)
dep(That-1, is-2)
csubj(have-6, assuming-4)
dobj(assuming-4, we-5)
root(ROOT-0, have-6)
det(definition-9, the-7)
