(ROOT
  (S
    (NP (DT That))
    (VP (VBZ 's)
      (SBAR (IN because)
        (S
          (NP (PRP they))
          (VP (VBP are)
            (VP (VBG looking)
              (PP (TO to)
                (NP
                  (NP (NNS adults))
                  (PP (IN for)
                    (NP (NN guidance))))))))))
    (. .)))

nsubj('s-2, That-1)
root(ROOT-0, 's-2)
mark(looking-6, because-3)
nsubj(looking-6, they-4)
aux(looking-6, are-5)
advcl('s-2, looking-6)
prep_to(looking-6, adults-8)
prep_for(adults-8, guidance-10)

