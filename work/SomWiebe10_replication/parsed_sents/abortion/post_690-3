(ROOT
  (S
    (NP
      (NP (RB Almost) (DT every) (NN cell))
      (PP (IN in)
        (NP (PRP$ my) (NN body))))
    (VP (VBZ is)
      (ADJP (JJ alive)))
    (. .)))

advmod(cell-3, Almost-1)
det(cell-3, every-2)
nsubj(alive-8, cell-3)
poss(body-6, my-5)
prep_in(cell-3, body-6)
cop(alive-8, is-7)
root(ROOT-0, alive-8)

