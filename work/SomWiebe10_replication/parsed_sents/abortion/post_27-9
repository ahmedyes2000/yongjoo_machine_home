(ROOT
  (S
    (NP (DT These))
    (VP (VBP are)
      (NP
        (NP (JJ critical) (NNS differences))
        (SBAR
          (WHNP (WDT that))
          (S
            (VP (VBP invalidate)
              (NP
                (NP (DT a) (JJ classic) (, ,) (JJ central) (NN argument))
                (PP (IN for)
                  (NP (NN abortion)))))))))
    (. .)))

nsubj(differences-4, These-1)
cop(differences-4, are-2)
amod(differences-4, critical-3)
root(ROOT-0, differences-4)
nsubj(invalidate-6, differences-4)
rcmod(differences-4, invalidate-6)
det(argument-11, a-7)
amod(argument-11, classic-8)
amod(argument-11, central-10)
dobj(invalidate-6, argument-11)
prep_for(argument-11, abortion-13)

