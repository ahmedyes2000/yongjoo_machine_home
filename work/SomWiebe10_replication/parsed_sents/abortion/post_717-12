  (S
    (NP (PRP I))
    (VP (MD would)
      (VP (VB say)
        (SBAR (IN that)
          (S
            (NP
              (NP (DT a) (NN person))
              (PP (IN with)
                (NP
                  (NP (JJ advanced) (NNP Alzheimer) (POS 's))
                  (NNP Disease))))
            (VP (VBZ has)
              (NP
                (NP (JJR less) (NN value))
                (PP (IN than)
                  (NP (NN someone)))
                (SBAR
                  (WHNP (WP who))
                  (S
                    (VP (VBZ does) (RB not)
                      (VP (VB have)
                        (NP (DT the) (NN disease)))))))
              (, ,)
              (ADVP (RB all) (RB else)
                (S
                  (VP (VBG being)
                    (ADJP (JJ equal))))))))))
    (. .)))

