(ROOT
  (S
    (NP
      (NP (JJ Supernatural) (NNS entities))
      (PP (IN as)
        (NP
          (NP (DT the) (NN basis))
          (PP (IN of)
            (NP (JJ individual) (NNS rights))))))
    (VP (VBZ is)
      (NP (DT an) (JJ irrational) (NN concept)))
    (. .)))

amod(entities-2, Supernatural-1)
nsubj(concept-12, entities-2)
det(basis-5, the-4)
prep_as(entities-2, basis-5)
amod(rights-8, individual-7)
prep_of(basis-5, rights-8)
cop(concept-12, is-9)
det(concept-12, an-10)
amod(concept-12, irrational-11)
root(ROOT-0, concept-12)

