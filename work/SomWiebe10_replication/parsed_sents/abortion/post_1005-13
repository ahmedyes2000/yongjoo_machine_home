(ROOT
  (S
    (S
      (NP (NNS Facts))
      (VP (VBP are)
        (NP
          (NP (JJ annoying) (NNS things))
          (SBAR
            (WHADVP (WRB when))
            (S
              (NP (PRP they))
              (VP (VB contradict)
                (NP (PRP you))))))))
    (CC and)
    (S
      (NP (PRP you))
      (VP (MD ca) (RB n't)
        (VP (VB make)
          (S
            (NP (PRP them))
            (VP (VB go)
              (ADVP (RB away))
              (PP (IN by)
                (S
                  (VP (VBG changing)
                    (NP (PRP them))))))))))
    (. .)))

nsubj(things-4, Facts-1)
cop(things-4, are-2)
amod(things-4, annoying-3)
root(ROOT-0, things-4)
tmod(contradict-7, things-4)
advmod(contradict-7, when-5)
nsubj(contradict-7, they-6)
dep(things-4, contradict-7)
dobj(contradict-7, you-8)
nsubj(make-13, you-10)
aux(make-13, ca-11)
neg(make-13, n't-12)
conj_and(things-4, make-13)
tmod(contradict-7, make-13)
nsubj(go-15, them-14)
ccomp(make-13, go-15)
advmod(go-15, away-16)
prepc_by(go-15, changing-18)
dobj(changing-18, them-19)

