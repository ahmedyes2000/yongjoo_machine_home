(ROOT
  (S
    (ADVP (RB However))
    (, ,)
    (NP
      (NP (DT an) (JJ unborn) (NN child))
      (SBAR
        (WHNP (WP who))
        (S
          (VP (VBZ is)
            (VP (VBN aborted)
              (ADVP (RB never)))))))
    (VP (VBZ gets)
      (NP (DT a) (NN chance)
        (S
          (VP (TO to)
            (VP (VB become)
              (NP (NN anything)))))))
    (. .)))

advmod(gets-10, However-1)
det(child-5, an-3)
amod(child-5, unborn-4)
nsubjpass(aborted-8, child-5)
nsubj(gets-10, child-5)
auxpass(aborted-8, is-7)
rcmod(child-5, aborted-8)
neg(aborted-8, never-9)
root(ROOT-0, gets-10)
det(chance-12, a-11)
dobj(gets-10, chance-12)
aux(anything-15, to-13)
cop(anything-15, become-14)
infmod(chance-12, anything-15)

