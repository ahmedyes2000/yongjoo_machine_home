(ROOT
  (S
    (NP (DT This))
    (VP (VBZ is)
      (SBAR
        (WHNP (WP what))
        (S
          (NP (PRP I))
          (VP (VBP 'm)
            (VP (VBG talking) (IN about))))))
    (. .)))

nsubj(is-2, This-1)
root(ROOT-0, is-2)
dobj(talking-6, what-3)
nsubj(talking-6, I-4)
aux(talking-6, 'm-5)
ccomp(is-2, talking-6)
dep(talking-6, about-7)

