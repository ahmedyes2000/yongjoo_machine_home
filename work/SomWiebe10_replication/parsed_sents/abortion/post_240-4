(ROOT
  (S
    (NP (PRP I))
    (VP (VBP believe)
      (SBAR (IN that)
        (S
          (NP (DT the) (NN woman))
          (VP (VBZ is)
            (ADVP (RB here)
              (CC and)
              (RB now))))))
    (. .)))

nsubj(believe-2, I-1)
root(ROOT-0, believe-2)
complm(is-6, that-3)
det(woman-5, the-4)
nsubj(is-6, woman-5)
ccomp(believe-2, is-6)
advmod(is-6, here-7)
advmod(is-6, now-9)
conj_and(here-7, now-9)

