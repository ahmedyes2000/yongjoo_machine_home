(ROOT
  (S
    (ADVP (RB However))
    (, ,)
    (NP
      (NP (DT the) (NN issue))
      (PP (IN of)
        (SBAR
          (WHADVP (WRB when))
          (S
            (NP (JJ human) (NN life))
            (VP (VBZ begins))))))
    (VP (VBZ is)
      (NP (DT a) (JJ scientific) (NN one)))
    (. .)))

advmod(one-13, However-1)
det(issue-4, the-3)
nsubj(one-13, issue-4)
advmod(begins-9, when-6)
amod(life-8, human-7)
nsubj(begins-9, life-8)
prepc_of(issue-4, begins-9)
cop(one-13, is-10)
det(one-13, a-11)
amod(one-13, scientific-12)
root(ROOT-0, one-13)

