(ROOT
  (S
    (NP (PRP I))
    (VP (VBP am)
      (VP (VBG saying)
        (SBAR
          (S
            (NP (DT the) (NN money) (NN shold))
            (VP (VBP have)
              (VP (VBN been)
                (VP (VBN used)
                  (S
                    (NP (DT some) (NN place))
                    (ADJP (RB else))))))))))
    (. .)))

nsubj(saying-3, I-1)
aux(saying-3, am-2)
root(ROOT-0, saying-3)
det(shold-6, the-4)
nn(shold-6, money-5)
nsubjpass(used-9, shold-6)
aux(used-9, have-7)
auxpass(used-9, been-8)
ccomp(saying-3, used-9)
det(place-11, some-10)
nsubj(else-12, place-11)
xcomp(used-9, else-12)

