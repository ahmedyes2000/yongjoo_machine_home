  (S
    (NP
      (NP (NN Everyone))
      (SBAR
        (WHNP (WP who))
        (S
          (VP (VBZ is)
            (PP (IN for)
              (NP (NN abortion)))))))
    (VP (VBZ seems)
      (S
        (VP (TO to)
          (VP (VB insist)
            (SBAR
              (S
                (NP (DT this))
