(ROOT
  (S
    (NP (PRP We))
    (VP
      (VP (VBP have)
        (NP (PDT all) (DT these) (NNS rights)))
      (CC and)
      (VP (VBP do) (RB n't)
        (VP (VB want)
          (NP
            (NP (DT the) (NN responsibility))
            (SBAR
              (WHNP (WDT that))
              (S
                (VP (VBZ comes)
                  (PP (IN with)
                    (S
                      (VP (VBG exercising)
                        (NP (PRP them))))))))))))
    (. .)))

nsubj(have-2, We-1)
nsubj(want-9, We-1)
root(ROOT-0, have-2)
predet(rights-5, all-3)
det(rights-5, these-4)
dobj(have-2, rights-5)
aux(want-9, do-7)
neg(want-9, n't-8)
conj_and(have-2, want-9)
det(responsibility-11, the-10)
dobj(want-9, responsibility-11)
nsubj(comes-13, responsibility-11)
