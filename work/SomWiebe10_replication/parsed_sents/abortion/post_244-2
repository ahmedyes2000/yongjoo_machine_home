(ROOT
  (FRAG
    (WHADVP (WRB Why))
    (RB not)
    (VP (VB give)
      (NP (PRP them))
      (NP (DT the) (NN chance)
        (S
          (VP (TO to)
            (VP (VB live))))))
    (. !)))

dep(not-2, Why-1)
root(ROOT-0, not-2)
dep(not-2, give-3)
iobj(give-3, them-4)
det(chance-6, the-5)
dobj(give-3, chance-6)
aux(live-8, to-7)
infmod(chance-6, live-8)

