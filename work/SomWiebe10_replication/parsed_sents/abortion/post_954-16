(ROOT
  (S
    (NP (DT This) (NN faith))
    (VP (VBZ is) (RB not)
      (NP
        (NP (DT a) (NN leap))
        (PP (IN in)
          (NP (DT the) (NN dark))))
      (, ,)
      (PP (IN like)
        (S
          (VP (VBG crossing)
            (NP (PRP$ your) (NNS fingers))))))
    (. .)))

det(faith-2, This-1)
nsubj(leap-6, faith-2)
cop(leap-6, is-3)
neg(leap-6, not-4)
det(leap-6, a-5)
root(ROOT-0, leap-6)
det(dark-9, the-8)
