(ROOT
  (S
    (NP (NNP Women))
    (VP (MD should)
      (VP (VB be)
        (VP (VBN held)
          (ADJP (JJ responsible))
          (PP (IN for)
            (NP
              (NP (NN behavior) (VBG leading))
              (PP (TO to)
                (NP (NN impregnation))))))))
    (. .)))

nsubjpass(held-4, Women-1)
aux(held-4, should-2)
auxpass(held-4, be-3)
root(ROOT-0, held-4)
acomp(held-4, responsible-5)
prep_for(held-4, behavior-7)
amod(behavior-7, leading-8)
prep_to(behavior-7, impregnation-10)

