  (S
    (ADVP (RB Ultimately))
    (, ,)
    (NP (PRP you))
    (VP (VBP have)
      (S
        (VP (TO to)
          (VP (VB decide)
            (SBAR
              (WHNP (WDT which))
              (S
                (VP (VBZ is)
                  (ADJP (RBR more) (JJ important))
                  (NP
                    (NP
                      (NP (PRP$ your) (NN right))
                      (PP (TO to)
                        (NP (PRP$ your) (NN body))))
                    (CC or)
                    (NP
                      (NP (DT the) (NN right))
                      (PP (IN for)
                        (NP (NN someone) (RB else)))))))))))
      (S
        (VP (TO to)
          (VP (VB live)))))
    (. .)))

