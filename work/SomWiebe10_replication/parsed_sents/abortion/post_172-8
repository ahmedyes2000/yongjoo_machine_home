  (S
    (NP (DT The) (NNP Church))
    (ADVP (PRP itself))
    (VP
      (ADVP (RB long) (RB ago))
      (VBD developed)
      (NP (DT the) (RB Just) (JJ War) (NN theory))
      (SBAR (IN because)
        (S
          (NP (PRP it))
          (VP (VBD acknowledged)
            (SBAR (IN that)
              (S
                (PP (IN under)
                  (NP (JJ certain) (NNS circumstances)))
                (NP (NNS wars))
                (VP (MD might)
                  (VP (VB have)
                    (S
                      (VP (TO to)
                        (VP (VB be)
                          (VP (VBN fought)))))))))))))
    (. .)))

