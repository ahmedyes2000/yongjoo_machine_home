(ROOT
  (S
    (PP (IN For)
      (NP (DT a) (JJ long) (NN time)))
    (NP (PRP it))
    (VP (VBZ is)
      (ADVP (RB essentially))
      (NP
        (NP (RB just) (DT a) (NN ball))
        (PP (IN of)
          (NP (NNS cells)))))
    (. .)))

det(time-4, a-2)
amod(time-4, long-3)
prep_for(ball-10, time-4)
nsubj(ball-10, it-5)
cop(ball-10, is-6)
advmod(ball-10, essentially-7)
advmod(ball-10, just-8)
det(ball-10, a-9)
root(ROOT-0, ball-10)
prep_of(ball-10, cells-12)

