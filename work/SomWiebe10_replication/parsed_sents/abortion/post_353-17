(ROOT
  (S
    (NP
      (NP (NNP Destroying) (NNP Roe) (POS 's))
      (JJ legal) (NN credibility))
    (VP (VBZ is) (RB not)
      (NP (DT a) (JJ big) (NN problem)))
    (. .)))

nn(Roe-2, Destroying-1)
poss(credibility-5, Roe-2)
amod(credibility-5, legal-4)
nsubj(problem-10, credibility-5)
cop(problem-10, is-6)
neg(problem-10, not-7)
det(problem-10, a-8)
amod(problem-10, big-9)
root(ROOT-0, problem-10)

