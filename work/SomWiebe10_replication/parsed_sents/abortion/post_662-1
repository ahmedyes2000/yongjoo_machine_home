(ROOT
  (S
    (PP (IN For)
      (NP (CD one) (NN thing)))
    (, ,)
    (NP (PRP we))
    (VP (VBP have)
      (NP (PRP$ their) (JJ cautionary) (NN example)))
    (. .)))

num(thing-3, one-2)
prep_for(have-6, thing-3)
nsubj(have-6, we-5)
root(ROOT-0, have-6)
poss(example-9, their-7)
amod(example-9, cautionary-8)
dobj(have-6, example-9)

