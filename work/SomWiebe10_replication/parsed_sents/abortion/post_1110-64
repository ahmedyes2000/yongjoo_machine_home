(ROOT
  (S
    (ADVP (RB As) (RB far)
      (SBAR (IN as)
        (S
          (NP (DT the) (NNP Ted) (NNP Bundy) (NN bit))
          (VP (VBZ goes)))))
    (, ,)
    (NP (PRP I))
    (ADVP (RB really))
    (VP (VB see)
      (NP (DT no) (NN bearing))
      (ADVP (RB here)))
    (. .)))

advmod(far-2, As-1)
advmod(see-12, far-2)
mark(goes-8, as-3)
det(bit-7, the-4)
nn(bit-7, Ted-5)
nn(bit-7, Bundy-6)
nsubj(goes-8, bit-7)
ccomp(far-2, goes-8)
