(ROOT
  (S
    (NP (EX There))
    (ADVP (RB really))
    (VP (VBZ needs)
      (S
        (VP (TO to)
          (VP (VB be)
            (NP
              (NP (JJR more) (NN truth))
              (PP (IN in)
                (NP
                  (NP (NN advertising))
                  (PP (IN for)
                    (NP (DT the) (JJ sexual) (NN revolution))))))))))
    (. .)))

expl(needs-3, There-1)
xsubj(truth-7, There-1)
advmod(needs-3, really-2)
root(ROOT-0, needs-3)
aux(truth-7, to-4)
cop(truth-7, be-5)
amod(truth-7, more-6)
xcomp(needs-3, truth-7)
prep_in(truth-7, advertising-9)
det(revolution-13, the-11)
