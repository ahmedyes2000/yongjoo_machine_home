(ROOT
  (S
    (NP (PRP We))
    (ADVP (RB just))
    (VP (VBP need)
      (S
        (VP (TO to)
          (VP (VB stop)
            (S
              (VP (VBG joking)
                (NP (PRP ourselves))))))))
    (. .)))

nsubj(need-3, We-1)
xsubj(stop-5, We-1)
advmod(need-3, just-2)
root(ROOT-0, need-3)
aux(stop-5, to-4)
xcomp(need-3, stop-5)
xcomp(stop-5, joking-6)
dobj(joking-6, ourselves-7)

