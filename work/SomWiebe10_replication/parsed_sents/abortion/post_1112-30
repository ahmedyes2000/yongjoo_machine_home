(ROOT
  (S
    (PP (IN On)
      (NP
        (NP (JJ moral) (NN relativism))
        (: ;)
        (SBAR (IN if)
          (S
            (NP (DT that))
            (VP (VBZ 's)
              (ADJP (JJ true))
              (ADVP (RB then))
              (SBAR
                (SBAR
                  (S
                    (NP (PRP it))
                    (VP (VBZ 's)
                      (ADVP (RB just))
                      (NP (PRP$ your) (JJ personal) (NN preference)))))
                (CC and)
                (SBAR
                  (WHADVP (WRB why))
                  (S
                    (VP (MD should))))))))))
    (NP (PRP I))
    (VP (VBP like)
      (SBAR
        (WHNP (WP what))
        (S
          (NP (PRP you))
          (VP (VBP do)))))
    (. .)))

amod(relativism-3, moral-2)
prep_on(like-20, relativism-3)
mark(true-8, if-5)
nsubj(true-8, that-6)
cop(true-8, 's-7)
dep(relativism-3, true-8)
advmod(true-8, then-9)
nsubj(preference-15, it-10)
cop(preference-15, 's-11)
advmod(preference-15, just-12)
poss(preference-15, your-13)
amod(preference-15, personal-14)
dep(true-8, preference-15)
advmod(should-18, why-17)
dep(true-8, should-18)
conj_and(preference-15, should-18)
nsubj(like-20, I-19)
root(ROOT-0, like-20)
dobj(do-23, what-21)
nsubj(do-23, you-22)
ccomp(like-20, do-23)

