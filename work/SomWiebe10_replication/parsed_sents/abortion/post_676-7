(ROOT
  (S
    (NP (DT A) (NN zygote))
    (VP (MD can) (RB not)
      (VP (VB survive)
        (NP
          (NP (RB outside))
          (PP (IN of)
            (NP (DT the) (NN mother))))))
    (. .)))

det(zygote-2, A-1)
nsubj(survive-5, zygote-2)
aux(survive-5, can-3)
neg(survive-5, not-4)
root(ROOT-0, survive-5)
dobj(survive-5, outside-6)
det(mother-9, the-8)
prep_of(outside-6, mother-9)

