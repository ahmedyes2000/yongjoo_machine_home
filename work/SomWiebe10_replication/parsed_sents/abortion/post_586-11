(ROOT
  (S
    (NP
      (NP (NNS Rules))
      (PP (IN of)
        (NP (NN war))))
    (ADVP (RB just))
    (VP (VBP require)
      (SBAR (IN that)
        (S
          (NP (PRP we))
          (VP (VBP try)
            (S
              (VP (TO to)
                (VP (VB limit)
                  (NP
                    (NP (DT the) (NNS deaths))
                    (PP (IN of)
                      (NP (NNS innocents)))))))))))
    (. .)))

nsubj(require-5, Rules-1)
prep_of(Rules-1, war-3)
advmod(require-5, just-4)
root(ROOT-0, require-5)
