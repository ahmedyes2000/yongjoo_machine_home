(ROOT
  (S
    (S
      (ADVP (RB Now))
      (S
        (VP (TO to)
          (VP (VB answer)
            (NP (PRP$ your) (NN question))
            (, ,)
            (SBAR (RB even) (IN as)
              (S
                (NP (PRP you)))))))
      (VP (VBD put)
        (NP (PRP it))))
    (: ;)
    (S
      (NP (NNP Truth)
        (CC and)
        (NNP Fact))
      (NP (PRP themselves)))
    (. .)))

advmod(put-10, Now-1)
aux(answer-3, to-2)
csubj(put-10, answer-3)
poss(question-5, your-4)
dobj(answer-3, question-5)
advmod(you-9, even-7)
dep(you-9, as-8)
dep(answer-3, you-9)
root(ROOT-0, put-10)
dobj(put-10, it-11)
dep(themselves-16, Truth-13)
conj_and(Truth-13, Fact-15)
dep(themselves-16, Fact-15)
parataxis(put-10, themselves-16)

