  (S
    (S
      (VP (VBZ Is)
        (NP (PRP it))
        (SBAR (IN because)
          (S
            (NP (PRP you))
            (VP (VBP think)
              (SBAR
                (S
                  (NP (EX there))
                  (VP (VBZ is)
                    (NP (DT an) (JJ objective) (NN standard))
                    (SBAR (IN that)
                      (S
                        (NP (NNS people))
                        (VP (MD ought)
                          (S
                            (VP (TO to)
                              (VP (VB follow)))))))))))))))
    (CC and)
    (S
      (NP (DT a) (NN way) (NNS things))
      (VP (MD ought)
        (S
          (VP (TO to)
            (VP (VB be))))))
    (. .)))

