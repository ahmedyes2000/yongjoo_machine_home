(ROOT
  (S
    (NP (PRP it))
    (VP
      (VP (VBZ 's)
        (PP (IN IN)
          (NP (PRP$ her) (NN body))))
      (CC and)
      (VP (VBZ is)
        (ADVP (RB essentially))
        (VP
          (VP (VBG sucking)
            (NP (PRP$ her) (NN life)))
          (CC and)
          (VP (VBG using)
            (NP
              (NP (NNP HER))
              (PP (IN as)
                (NP
                  (NP (DT a) (NN means))
                  (PP (IN of)
                    (NP (NN living))))))))))
    (. .)))

nsubj('s-2, it-1)
nsubj(sucking-9, it-1)
root(ROOT-0, 's-2)
poss(body-5, her-4)
prep_in('s-2, body-5)
aux(sucking-9, is-7)
