(ROOT
  (X
    (ADVP (RB Also))
    (, ,)
    (NP
      (NP (WP what))
      (PP
        (PP (IN about)
          (NP
            (NP (DT the) (NN mother) (POS 's))
            (NN career)))
        (CC or)
        (PP (IN in)
          (NP (DT some) (NNS cases)))
        (NP
          (NP (DT the) (NN mother) (POS 's))
          (NN education))))
    (. .)))

dep(what-3, Also-1)
root(ROOT-0, what-3)
conj_or(what-3, what-3')
det(mother-6, the-5)
poss(career-8, mother-6)
prep_about(what-3, career-8)
det(cases-12, some-11)
prep_in(what-3', cases-12)
det(mother-14, the-13)
poss(education-16, mother-14)
dep(what-3, education-16)

