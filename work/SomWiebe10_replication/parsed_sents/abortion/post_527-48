(ROOT
  (SBAR (IN because)
    (S
      (NP (EX there))
      (VP (VBP are)
        (NP
          (NP (NNS choices))
          (PP (IN in)
            (NP (DT the) (NN choice))))))
    (: ...)))

mark(are-3, because-1)
expl(are-3, there-2)
root(ROOT-0, are-3)
nsubj(are-3, choices-4)
det(choice-7, the-6)
prep_in(choices-4, choice-7)

