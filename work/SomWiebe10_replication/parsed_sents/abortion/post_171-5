(ROOT
  (S
    (PP (IN After)
      (NP (DT all)))
    (, ,)
    (NP (PRP it))
    (VP (VBZ 's) (RB all)
      (VP (VBN based)
        (PP (IN on)
          (NP (DT the) (NN passage)))
        (SBAR (IN that)
          (S
            (NP (NNP God))
            (VP (VBD knew)
              (NP (PRP us))
              (SBAR (IN before)
                (S
                  (NP (PRP we))
                  (VP (VBD were)
                    (VP (VBN born))))))))))
    (. .)))

prep_after(based-7, all-2)
nsubjpass(based-7, it-4)
auxpass(based-7, 's-5)
advmod(based-7, all-6)
root(ROOT-0, based-7)
det(passage-10, the-9)
prep_on(based-7, passage-10)
complm(knew-13, that-11)
nsubj(knew-13, God-12)
ccomp(based-7, knew-13)
dobj(knew-13, us-14)
mark(born-18, before-15)
nsubjpass(born-18, we-16)
auxpass(born-18, were-17)
advcl(knew-13, born-18)

