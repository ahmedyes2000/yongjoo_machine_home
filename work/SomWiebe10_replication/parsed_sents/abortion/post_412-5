(ROOT
  (S (CC But)
    (NP
      (NP (DT no) (NN one) (POS 's))
      (NN opinion))
    (VP (VBZ establishes)
      (NP (NN reality)))
    (. .)))

cc(establishes-6, But-1)
det(one-3, no-2)
poss(opinion-5, one-3)
nsubj(establishes-6, opinion-5)
root(ROOT-0, establishes-6)
dobj(establishes-6, reality-7)

