  (S
    (NP
      (NP (NNP Human) (NNP DNA))
      (CC or)
      (NP (DT a) (NN heartbeat)
        (CC or)
        (NN brain) (NN activity)))
    (VP (VBP are) (RB not)
      (VP (VBG defining)
        (NP
          (NP (NN characteristic))
          (PP (IN of)
            (NP (DT a) (NN human) (VBG being))))))
    (. .)))

