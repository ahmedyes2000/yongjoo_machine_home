(ROOT
  (FRAG
    (ADVP (RB Answer))
    (: :)
    (SBAR (IN Because)
      (S
        (NP
          (NP (PRP$ your) (NN mother))
          (, ,)
          (CC and)
          (ADVP (RB probably))
          (NP (PRP$ your) (NN father))
          (, ,))
        (VP (VBD loved)
          (S
            (NP (PRP you))
            (ADJP (JJR more)
              (SBAR (IN than)
                (S
                  (NP (PRP they))
                  (VP (VBD loved)
                    (S
                      (NP (PRP themselves))
                      (NP (NNP !!!!!!!!!!!!!!!!!!!) (NNP Hypocrite)))))))))))
    (. !)))

root(ROOT-0, Answer-1)
mark(loved-12, Because-3)
poss(mother-5, your-4)
nsubj(loved-12, mother-5)
advmod(mother-5, probably-8)
conj_and(mother-5, probably-8)
nsubj(loved-12, probably-8)
poss(father-10, your-9)
conj_and(mother-5, father-10)
nsubj(loved-12, father-10)
dep(Answer-1, loved-12)
nsubj(more-14, you-13)
xcomp(loved-12, more-14)
