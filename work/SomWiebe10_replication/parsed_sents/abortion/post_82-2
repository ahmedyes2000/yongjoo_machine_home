(ROOT
  (S
    (NP (DT The) (NN distinction))
    (VP
      (VP (VBZ is)
        (ADJP (JJ important)))
      (, ,)
      (CC and)
      (VP (VBZ is)
        (NP (DT a) (JJ good) (NN reason))
        (S
          (VP (TO to)
            (VP (VB oppose)
              (NP (NN abortion))
              (ADVP (RB even))
              (PP (IN during)
                (NP (JJ special) (NNS emergencies))))))))
    (. .)))

det(distinction-2, The-1)
nsubj(important-4, distinction-2)
cop(important-4, is-3)
root(ROOT-0, important-4)
cop(reason-10, is-7)
det(reason-10, a-8)
amod(reason-10, good-9)
conj_and(important-4, reason-10)
aux(oppose-12, to-11)
xcomp(reason-10, oppose-12)
dobj(oppose-12, abortion-13)
advmod(oppose-12, even-14)
amod(emergencies-17, special-16)
prep_during(oppose-12, emergencies-17)

