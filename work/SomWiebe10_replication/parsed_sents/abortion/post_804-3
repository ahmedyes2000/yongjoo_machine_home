(ROOT
  (S
    (PP (IN In)
      (NP (DT that) (NN case)))
    (NP (PRP we))
    (VP (VBP 're)
      (S
        (NP (DT all))
        (VP (VB be)
          (NP
            (NP (NNS murderers))
            (PP (IN of)
              (NP (JJ little) (NN life)))))))
    (. .)))

det(case-3, that-2)
prep_in('re-5, case-3)
nsubj('re-5, we-4)
root(ROOT-0, 're-5)
nsubj(murderers-8, all-6)
cop(murderers-8, be-7)
ccomp('re-5, murderers-8)
amod(life-11, little-10)
prep_of(murderers-8, life-11)

