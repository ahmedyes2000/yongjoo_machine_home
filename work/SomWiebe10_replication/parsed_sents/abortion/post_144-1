(ROOT
  (S
    (NP (DT The) (NNS options))
    (VP (VBP include) (, ,)
      (NP (NN abstinence) (, ,) (NN condom)
        (CC and)
        (NNS profalactics))
      (, ,)
      (S
        (NP (JJ birth-control) (NNS pills) (, ,) (NN morning-after)
          (CC or)
          (NN plan) (NN B) (NNS pills))))
    (. .)))

det(options-2, The-1)
nsubj(include-3, options-2)
root(ROOT-0, include-3)
dobj(include-3, abstinence-5)
dobj(include-3, condom-7)
conj_and(abstinence-5, condom-7)
dobj(include-3, profalactics-9)
conj_and(abstinence-5, profalactics-9)
amod(pills-18, birth-control-11)
nn(pills-18, pills-12)
conj_or(pills-12, morning-after-14)
nn(pills-18, morning-after-14)
conj_or(pills-12, plan-16)
nn(pills-18, plan-16)
nn(pills-18, B-17)
dep(include-3, pills-18)

