(ROOT
  (S
    (NP (PRP I))
    (VP (VBP feel)
      (ADJP (JJ sorry))
      (PP (IN for)
        (NP (PRP you))))
    (. .)))

nsubj(feel-2, I-1)
root(ROOT-0, feel-2)
acomp(feel-2, sorry-3)
prep_for(feel-2, you-5)

(ROOT
  (S
    (NP (PRP I))
    (VP (VBP hope)
      (SBAR
        (S
          (NP (PRP you))
          (VP (MD will)
            (VP (VB realize)
              (NP
                (NP (DT the) (NNS evilness))
                (PP (IN of)
                  (NP (PRP$ your) (NNS abortions)))))))))
    (. .)))

nsubj(hope-2, I-1)
root(ROOT-0, hope-2)
nsubj(realize-5, you-3)
aux(realize-5, will-4)
ccomp(hope-2, realize-5)
det(evilness-7, the-6)
dobj(realize-5, evilness-7)
poss(abortions-10, your-9)
prep_of(evilness-7, abortions-10)

