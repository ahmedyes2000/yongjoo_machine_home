(ROOT
  (S
    (NP
      (NP
        (NP (DT The) (NNP CDC) (POS 's))
        (JJS latest) (NNS estimates))
      (PP (IN for)
        (NP (JJ international) (NNS rankings))))
    (VP (VBP are)
      (VP (VBN based)
        (PP (IN on)
          (NP (CD 2004) (NNS data)))))
    (. .)))

det(CDC-2, The-1)
poss(estimates-5, CDC-2)
amod(estimates-5, latest-4)
nsubjpass(based-10, estimates-5)
amod(rankings-8, international-7)
prep_for(estimates-5, rankings-8)
auxpass(based-10, are-9)
root(ROOT-0, based-10)
