(ROOT
  (S (CC And)
    (NP (DT some) (JJ human) (NNS lives))
    (VP (VBP are)
      (ADJP (RBR more) (JJ important)
        (PP (IN than)
          (NP (NNS others)))))
    (. .)))

cc(important-7, And-1)
det(lives-4, some-2)
amod(lives-4, human-3)
nsubj(important-7, lives-4)
cop(important-7, are-5)
advmod(important-7, more-6)
root(ROOT-0, important-7)
prep_than(important-7, others-9)

