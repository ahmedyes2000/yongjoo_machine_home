(ROOT
  (S
    (NP (DT The) (NNP fetus) ('' '') (NN choice))
    (VP (VBZ is)
      (VP (VBN made)
        (PP (IN for)
          (NP (PRP them)))))
    (. .)))

det(choice-4, The-1)
nn(choice-4, fetus-2)
nsubjpass(made-6, choice-4)
auxpass(made-6, is-5)
root(ROOT-0, made-6)
prep_for(made-6, them-8)

