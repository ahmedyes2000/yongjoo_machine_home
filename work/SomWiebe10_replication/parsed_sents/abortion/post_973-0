(ROOT
  (S
    (NP (PRP I))
    (VP (VBP agree)
      (PP (IN with)
        (NP (PRP you)))
      (PP (IN except) (IN on)
        (NP
          (NP (DT the) (NN issue))
          (PP (IN of)
            (NP (NN rape))))))
    (. .)))

nsubj(agree-2, I-1)
root(ROOT-0, agree-2)
prep_with(agree-2, you-4)
dep(agree-2, except-5)
det(issue-8, the-7)
prep_on(agree-2, issue-8)
prep_of(issue-8, rape-10)

