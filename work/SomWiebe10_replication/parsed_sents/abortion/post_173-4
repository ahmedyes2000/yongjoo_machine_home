(ROOT
  (S ('' '')
    (PP (VBG Any)
      (NP
        (NP (JJ honest) (NN reading))
        (PP (IN of)
          (NP (DT the) (RB Just) (NNP War) (NN theory)))))
    (VP (VBZ makes)
      (S
        (NP (PRP it))
        (ADJP (JJ clear)))
      (SBAR (IN that)
        (S
          (NP (NNP Iraq))
          (VP (VBZ is)
            (NP (DT an) (JJ unjust) (NN war))))))
    (. .)))

amod(reading-4, honest-3)
prep_any(makes-10, reading-4)
det(theory-9, the-6)
advmod(theory-9, Just-7)
nn(theory-9, War-8)
prep_of(reading-4, theory-9)
root(ROOT-0, makes-10)
nsubj(clear-12, it-11)
xcomp(makes-10, clear-12)
complm(war-18, that-13)
nsubj(war-18, Iraq-14)
cop(war-18, is-15)
det(war-18, an-16)
amod(war-18, unjust-17)
ccomp(makes-10, war-18)

