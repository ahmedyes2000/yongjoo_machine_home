(ROOT
  (S
    (NP (PRP You))
    (VP (VBP make)
      (S
        (NP (PRP it))
        (VP (VB sound)
          (SBAR (IN as)
            (S
              (SBAR (IN though)
                (S
                  (NP (EX there))
                  (VP (VBZ is)
                    (NP
                      (NP (DT some) (NN kind))
                      (PP (IN of)
                        (NP (NN conspiracy)))
                      (S
                        (VP (TO to)
                          (VP (VB abort)
                            (NP
