(ROOT
  (S
    (S (CC But)
      (NP (DT that))
      (VP (VBZ is) (RB not)
        (NP (DT the) (NN issue))))
    (: ;)
    (S
      (NP (DT the) (NN issue))
      (VP (VBZ concerns)
        (S
          (NP (DT the) (NN taking))
          (NP
            (NP (DT the) (NN life))
            (PP (IN of)
              (NP (DT an) (JJ innocent) (JJ human) (NN being)))))))
    (. .)))

cc(issue-6, But-1)
nsubj(issue-6, that-2)
cop(issue-6, is-3)
neg(issue-6, not-4)
det(issue-6, the-5)
root(ROOT-0, issue-6)
det(issue-9, the-8)
nsubj(concerns-10, issue-9)
parataxis(issue-6, concerns-10)
det(taking-12, the-11)
nsubj(life-14, taking-12)
det(life-14, the-13)
xcomp(concerns-10, life-14)
det(being-19, an-16)
amod(being-19, innocent-17)
amod(being-19, human-18)
prep_of(life-14, being-19)

