(ROOT
  (S
    (PP (IN On)
      (NP (DT the) (NN premise)))
    (NP (DT that) (NN abortion))
    (VP (VBZ is) (RB n't)
      (NP (NN murder)))
    (. ?)))

det(premise-3, the-2)
prep_on(murder-8, premise-3)
det(abortion-5, that-4)
nsubj(murder-8, abortion-5)
cop(murder-8, is-6)
neg(murder-8, n't-7)
root(ROOT-0, murder-8)

