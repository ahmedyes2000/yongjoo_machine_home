(ROOT
  (S
    (NP (DT that))
    (VP (VBZ 's)
      (SBAR
        (WHADVP (WRB why))
        (S
          (NP (JJS most) (NNS couples))
          (VP (VBP adopt)
            (PP (IN from)
              (NP (RB overseas)))))))
    (. .)))

nsubj('s-2, that-1)
root(ROOT-0, 's-2)
advmod(adopt-6, why-3)
amod(couples-5, most-4)
nsubj(adopt-6, couples-5)
advcl('s-2, adopt-6)
prep(adopt-6, from-7)
pobj(from-7, overseas-8)

