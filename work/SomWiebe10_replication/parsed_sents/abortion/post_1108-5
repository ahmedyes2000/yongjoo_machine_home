  (S
    (NP (NNS Laws))
    (VP (MD should)
      (VP (VB be)
        (VP (VBN based)
          (PP (IN on)
            (S
              (VP (VBG protecting)
                (NP (JJ other) (NNS people))
                (, ,)
                (PP (RB not) (IN on)
                  (NP (NN morality)))))))))
    (. .)))

