(ROOT
  (S
    (NP
      (NP (DT Both) (NNS sides))
      (PP (IN of)
        (NP (DT the) (NN argument))))
    (VP (MD must)
      (ADVP (RB equally))
      (VP (VB provide)
        (NP (NN support))
        (PP (IN for)
          (NP (PRP$ their) (NN position)))))
    (. .)))

det(sides-2, Both-1)
nsubj(provide-8, sides-2)
det(argument-5, the-4)
prep_of(sides-2, argument-5)
aux(provide-8, must-6)
advmod(provide-8, equally-7)
root(ROOT-0, provide-8)
dobj(provide-8, support-9)
poss(position-12, their-11)
prep_for(provide-8, position-12)

