(ROOT
  (S
    (S
      (VP (VBG Killing)
        (NP (DT a) (NN human))))
    (VP (VBZ is)
      (NP (NN murder)))
    (. .)))

csubj(murder-5, Killing-1)
det(human-3, a-2)
dobj(Killing-1, human-3)
cop(murder-5, is-4)
root(ROOT-0, murder-5)

(ROOT
  (S
    (NP (NN Murder))
    (VP (VBZ is)
      (ADJP (JJ illegal)))
    (. .)))

nsubj(illegal-3, Murder-1)
cop(illegal-3, is-2)
root(ROOT-0, illegal-3)

(ROOT
  (S
    (ADVP (RB Therefore))
    (, ,)
    (SBAR (IN because)
      (S
        (NP (NN abortion))
        (VP (VBZ is)
          (VP (VBG killing)
            (NP (DT a) (NN human))))))
    (, ,)
    (NP (NN abortion))
    (VP (MD should)
      (VP (VB be)
        (ADJP (JJ illegal))))
    (. .)))

advmod(illegal-13, Therefore-1)
mark(killing-6, because-3)
nsubj(killing-6, abortion-4)
aux(killing-6, is-5)
advcl(illegal-13, killing-6)
det(human-8, a-7)
dobj(killing-6, human-8)
nsubj(illegal-13, abortion-10)
aux(illegal-13, should-11)
cop(illegal-13, be-12)
root(ROOT-0, illegal-13)

