(ROOT
  (S
    (NP
      (NP (RB About) (NN Abortion) (NN legislation))
      (, ,)
      (SBAR
        (WHNP (WDT which))
        (S
          (NP (DT this) (NN debate)))))
    (VP (VBZ is) (RB all)
      (ADJP (IN about)))
    (. .)))

advmod(legislation-3, About-1)
nn(legislation-3, Abortion-2)
dep(debate-7, legislation-3)
nsubj(about-10, legislation-3)
det(debate-7, this-6)
rcmod(legislation-3, debate-7)
cop(about-10, is-8)
advmod(about-10, all-9)
root(ROOT-0, about-10)

