        (WHNP (WDT that))
        (S
          (VP (VBZ forces)
            (NP (DT the) (NN decision)
              (S
                (VP (TO to)
                  (VP (VB be)
                    (VP
                      (VP
                        (ADVP (RB carefully))
                        (VBN reviewed))
                      (, ,)
                      (CC but)
                      (VP
                        (ADVP (RB still))))))))))))
    (VP (VBP allow)
      (PP (IN for)
        (NP (PRP it))))
    (-RRB- -RRB-) (. .)))

nsubj(forces-3, something-1)
nsubj(allow-13, something-1)
rcmod(something-1, forces-3)
