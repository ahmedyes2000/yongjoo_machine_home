  (S
    (SBAR (IN if)
      (S
        (NP (PRP you))
        (VP (VB keep)
          (NP (NN abortion) (JJ legal))
          (PP (IN than)
            (NP (PRP you))))))
    (VP (MD should)
      (VP (VB make)
        (NP (NN murder) (JJ legal))))
    (. .)))

mark(keep-3, if-1)
nsubj(keep-3, you-2)
csubj(make-9, keep-3)
dobj(keep-3, abortion-4)
amod(abortion-4, legal-5)
