(ROOT
  (S
    (NP (PRP It))
    (VP (VBD was)
      (ADVP (RB definitively))
      (VP (VBN declared)
        (PP (IN by)
          (NP
            (NP (NNP Pope) (NNP Paul) (NNP VI))
            (PP (IN in)
              (NP
                (NP (DT the) (JJ encyclical) (NNP Humanae) (NNP Vitae))
                (, ,)
                (SBAR
                  (WHNP (WDT which))
                  (S
                    (VP (VBD was)
                      (VP (VBN written)
                        (ADVP (RB specifically))
                        (PP (IN for)
                          (NP (DT that) (NN purpose)))))))))))))
    (. .)))

