(ROOT
  (S
    (ADVP (RB Once) (RB again))
    (, ,)
    (NP (PRP I))
    (VP (VBP am)
      (ADJP (JJ sorry)
        (PP (IN for)
          (NP (PRP$ your) (JJ personal) (NN experience)))))
    (. .)))

advmod(again-2, Once-1)
advmod(sorry-6, again-2)
nsubj(sorry-6, I-4)
cop(sorry-6, am-5)
root(ROOT-0, sorry-6)
poss(experience-10, your-8)
amod(experience-10, personal-9)
prep_for(sorry-6, experience-10)

