(ROOT
  (S
    (SBAR (RB Just) (IN because)
      (S
        (NP (NN abortion))
        (VP (VBZ is)
          (ADJP (JJ legal)))))
    (VP (VBZ does) (RB n't)
      (VP (VB make)
        (S
          (NP (PRP it))
          (ADJP (RB right)))))
    (. .)))

advmod(legal-5, Just-1)
dep(legal-5, because-2)
nsubj(legal-5, abortion-3)
cop(legal-5, is-4)
advcl(make-8, legal-5)
csubj(make-8, legal-5)
aux(make-8, does-6)
neg(make-8, n't-7)
root(ROOT-0, make-8)
nsubj(right-10, it-9)
xcomp(make-8, right-10)

