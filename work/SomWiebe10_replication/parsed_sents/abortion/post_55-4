(ROOT
  (S
    (NP (NNP Abortion))
    (VP (VBZ is)
      (NP
        (NP (DT the) (NN choice))
        (PP (IN of)
          (NP
            (NP (NNS evils))
            (PP (IN for)
              (NP (JJ such) (NNS women)))))))
    (. .)))

nsubj(choice-4, Abortion-1)
cop(choice-4, is-2)
det(choice-4, the-3)
root(ROOT-0, choice-4)
