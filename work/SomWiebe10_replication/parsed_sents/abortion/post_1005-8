(ROOT
  (S
    (NP (EX There))
    (VP (VBP are)
      (NP
        (NP (DT a) (NN lot))
        (PP (IN of)
          (NP
            (NP (NNS people))
            (PP (IN for)
              (SBAR
                (SBAR
                  (WHNP (WP whom))
                  (S
                    (NP (NN something))
                    (VP (VBZ seems)
                      (ADJP (RB dreadfully) (JJ wrong)))))
                (CC and)
                (SBAR (IN for)
                  (S
                    (NP
                      (NP (DT those))
                      (PP (IN of)
                        (NP (PRP you)))
                      (SBAR
                        (WHNP (WP who))
                        (S
                          (VP (VBP support)
                            (NP (NN abortion))
                            (S
                              (VP (TO to)
                                (VP (VB dismiss)
                                  (NP (PRP us))
                                  (PP (IN with)
                                    (NP (DT some) (JJ fancy) (JJ mental) (NNS gymnastics))))))))))
                    (VP (VBZ is)
                      (ADVP (RB simply))
                      (ADJP (JJ inadequate)))))))))))
    (. .)))

expl(are-2, There-1)
root(ROOT-0, are-2)
det(lot-4, a-3)
nsubj(are-2, lot-4)
prep_of(lot-4, people-6)
dobj(wrong-12, whom-8)
