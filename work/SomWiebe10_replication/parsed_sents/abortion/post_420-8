  (S
    (NP (NN Nothing))
    (VP (VBZ is)
      (ADJP (`` ``) (JJ safe) ('' '')
        (PP (IN from)
          (NP
            (NP (DT the) (NN power))
            (PP (IN of)
              (NP (JJ human) (NN reason)))))))
    (. .)))

