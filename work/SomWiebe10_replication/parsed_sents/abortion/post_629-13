(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is)
      (VP (VBN meant)
        (S
          (VP (TO to)
            (VP (VB keep)
              (NP (DT the) (JJ poor))
              (PP (IN as)
                (NP (RB exactly) (DT that) (JJ poor))))))))
    (. .)))

nsubjpass(meant-3, It-1)
xsubj(keep-5, It-1)
auxpass(meant-3, is-2)
root(ROOT-0, meant-3)
aux(keep-5, to-4)
xcomp(meant-3, keep-5)
det(poor-7, the-6)
dobj(keep-5, poor-7)
advmod(poor-11, exactly-9)
det(poor-11, that-10)
prep_as(keep-5, poor-11)

