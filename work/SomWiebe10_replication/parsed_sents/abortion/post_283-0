    (SBAR (RB even) (IN if)
      (S
        (NP (PRP it))
        (VP (VBD were))))
    (, ,)
    (NP (PRP you))
    (VP (MD would)
      (VP (VB have)
        (NP (DT a)
          (ADJP (RB very) (JJ poor))
          (NN one))))
    (. .)))

