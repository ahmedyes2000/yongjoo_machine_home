(ROOT
  (S
    (NP (DT The) (NN question))
    (VP (VBZ is) (RB n't)
      (SBAR
        (WHADVP (WRB when))
        (S
          (NP (DT an) (NN abortion))
          (VP (MD should)
            (VP (VB be)
              (ADJP (JJ legal)))))))
    (. .)))

det(question-2, The-1)
nsubj(is-3, question-2)
root(ROOT-0, is-3)
neg(is-3, n't-4)
advmod(legal-10, when-5)
det(abortion-7, an-6)
nsubj(legal-10, abortion-7)
aux(legal-10, should-8)
cop(legal-10, be-9)
advcl(is-3, legal-10)

