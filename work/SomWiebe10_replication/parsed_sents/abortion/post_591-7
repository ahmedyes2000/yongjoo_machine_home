(ROOT
  (S
    (SBAR (IN Whether)
      (S
        (S
          (NP (PRP it))
          (VP (VBZ is)
            (NP (DT a) (NN person))))
        (CC and)
        (S
          (ADVP (RB thus))
          (VP (VBN entitled)
            (PP (TO to)
              (NP (NNS rights)))))))
    (VP (VBZ is)
      (ADVP (RB up))
      (PP (IN for)
        (NP (NN debate))))
    (. .)))

complm(person-5, Whether-1)
nsubj(person-5, it-2)
nsubj(entitled-8, it-2)
cop(person-5, is-3)
det(person-5, a-4)
csubj(is-11, person-5)
advmod(entitled-8, thus-7)
conj_and(person-5, entitled-8)
csubj(is-11, entitled-8)
prep_to(entitled-8, rights-10)
root(ROOT-0, is-11)
advmod(is-11, up-12)
prep_for(is-11, debate-14)

