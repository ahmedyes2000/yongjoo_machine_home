(ROOT
  (S
    (NP (EX There))
    (VP (VBP are)
      (NP
        (NP (NNS thousands))
        (PP (IN of)
          (NP
            (NP (NNS children))
            (VP (VBG dying)
              (S
                (VP (TO to)
                  (VP (VB be)
                    (VP (VBN adopted))))))))))
    (. .)))

expl(are-2, There-1)
root(ROOT-0, are-2)
nsubj(are-2, thousands-3)
prep_of(thousands-3, children-5)
xsubj(adopted-9, children-5)
partmod(children-5, dying-6)
aux(adopted-9, to-7)
auxpass(adopted-9, be-8)
xcomp(dying-6, adopted-9)

