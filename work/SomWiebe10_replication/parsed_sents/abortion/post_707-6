(ROOT
  (S
    (NP (PRP You))
    (VP (VBD investigated)
      (NP (PDT all) (PRP$ my) (NNS actions))
      (SBAR (IN before)
        (S
          (NP (DT the) (NN world))
          (VP (VBD was)
            (VP (VBN created))))))
    (. .)))

nsubj(investigated-2, You-1)
root(ROOT-0, investigated-2)
predet(actions-5, all-3)
poss(actions-5, my-4)
dobj(investigated-2, actions-5)
mark(created-10, before-6)
det(world-8, the-7)
nsubjpass(created-10, world-8)
auxpass(created-10, was-9)
advcl(investigated-2, created-10)

