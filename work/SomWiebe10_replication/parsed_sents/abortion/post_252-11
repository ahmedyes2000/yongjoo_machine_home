(ROOT
  (S
    (S
      (NP (NNP Reason))
      (VP (VBZ is)
        (ADJP (JJ clear)
          (PP (IN on)
            (NP (DT this))))))
    (: :)
    (S
      (NP (DT A) (NN fetus))
      (VP (VBZ is) (RB not)
        (NP (DT a) (NN person))))
    (. .)))

nsubj(clear-3, Reason-1)
cop(clear-3, is-2)
root(ROOT-0, clear-3)
prep_on(clear-3, this-5)
det(fetus-8, A-7)
nsubj(person-12, fetus-8)
cop(person-12, is-9)
neg(person-12, not-10)
det(person-12, a-11)
parataxis(clear-3, person-12)

