(ROOT
  (S
    (NP (NNS Things))
    (VP (VBD were)
      (VP (VBN done)
        (SBAR (IN that)
          (S
            (VP (MD might) (RB not)
              (VP (VB have)
                (VP (VBN been)
                  (VP (VBN done)
                    (PP (IN in)
                      (NP (JJR less) (JJ dire) (NNS circumstances)))))))))))
    (. .)))

nsubjpass(done-3, Things-1)
auxpass(done-3, were-2)
root(ROOT-0, done-3)
