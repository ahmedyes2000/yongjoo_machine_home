    (VP (VBN Envision)
      (NP (PRP yourself))
      (PP (IN at)
        (S
          (NP
            (NP (DT the) (NN door))
            (PP (IN of)
              (NP (DT an) (NNP ER))))
          (VP (VBG watching)
            (S
              (NP (DT a) (NN scenario))
