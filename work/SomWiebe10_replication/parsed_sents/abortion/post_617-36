(ROOT
  (S
    (NP (NNP Sure))
    (, ,)
    (PP (IN in)
      (NP (DT the) (JJ latter) (NN case)))
    (NP (PRP you))
    (VP (VBP have)
      (NP (DT a)
        (ADJP (JJ complete) (JJ living))
        (NN cancer) (NN cell)))
    (. .)))

