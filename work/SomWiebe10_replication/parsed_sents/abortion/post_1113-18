(ROOT
  (S
    (NP (NNP Hustler))
    (VP (VBZ is)
      (VP (VBN viewed)
        (PP (IN as)
          (NP
            (NP (DT an) (NN object))
            (PP (IN for)
              (NP (JJ personal) (NN pleasure)))))
        (S
          (VP (TO to)
            (VP (VB be)
              (VP (VBN discarded)))))))
    (. .)))

nsubjpass(viewed-3, Hustler-1)
xsubj(discarded-12, Hustler-1)
auxpass(viewed-3, is-2)
root(ROOT-0, viewed-3)
det(object-6, an-5)
prep_as(viewed-3, object-6)
amod(pleasure-9, personal-8)
prep_for(object-6, pleasure-9)
aux(discarded-12, to-10)
auxpass(discarded-12, be-11)
xcomp(viewed-3, discarded-12)

