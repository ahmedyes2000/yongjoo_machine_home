(ROOT
  (S
    (NP
      (NP (DT That) (NN scenario))
      (PP (IN of)
        (NP (DT the) (JJ endangered) (NN mother))))
    (VP (VBZ is)
      (ADJP (JJ rare)))
    (. .)))

det(scenario-2, That-1)
nsubj(rare-8, scenario-2)
det(mother-6, the-4)
amod(mother-6, endangered-5)
prep_of(scenario-2, mother-6)
cop(rare-8, is-7)
root(ROOT-0, rare-8)

