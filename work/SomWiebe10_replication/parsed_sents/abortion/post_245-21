(ROOT
  (S
    (NP
      (NP (DT The) (NN ego))
      (SBAR
        (WHNP (WDT that))
        (S
          (VP (VBZ says)
            (SBAR
              (S
                (NP
                  (NP (DT the) (NN life))
                  (PP (IN of)
                    (NP (CD one))))
                (VP (VBZ is)
                  (ADJP (RBR more) (JJ valuable)))))))))
    (ADVP (RB probably))
    (VP (VBZ depends)
      (PP (IN on)
        (SBAR
          (WHADVP (WRB where))
          (S
            (NP (PRP you))
            (VP (VBP 're)
              (VP (VBG standing)))))))
    (. .)))

