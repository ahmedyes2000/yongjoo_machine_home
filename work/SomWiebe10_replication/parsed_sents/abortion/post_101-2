(ROOT
  (S
    (PP (IN In)
      (NP (NN modernity)))
    (, ,)
    (NP (NNS children))
    (VP (MD can) (RB not)
      (VP (VB survive)
        (PP (IN on)
          (NP (PRP$ their) (JJ own)))
        (PP (IN into)
          (NP (PRP$ their) (NNS teens)))))
    (. .)))

prep_in(survive-7, modernity-2)
nsubj(survive-7, children-4)
aux(survive-7, can-5)
neg(survive-7, not-6)
root(ROOT-0, survive-7)
poss(own-10, their-9)
prep_on(survive-7, own-10)
poss(teens-13, their-12)
prep_into(survive-7, teens-13)

