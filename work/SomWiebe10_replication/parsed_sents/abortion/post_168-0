(ROOT
  (S
    (NP
      (NP (DT The) (NN majority))
      (PP (IN of)
        (NP (NNPS Americans))))
    (VP (VBP oppose)
      (NP (JJ late) (NN term) (NNS abortions)))
    (. .)))

det(majority-2, The-1)
nsubj(oppose-5, majority-2)
prep_of(majority-2, Americans-4)
root(ROOT-0, oppose-5)
amod(abortions-8, late-6)
