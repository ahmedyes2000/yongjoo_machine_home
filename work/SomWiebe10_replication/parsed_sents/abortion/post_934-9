(ROOT
  (S
    (VP (VB Consider)
      (SBAR
        (S
          (NP (NNP Europe))
          (, ,)
          (PP (IN within)
            (NP (DT a) (CD hundred) (NNS years)))
          (, ,)
          (SBAR (IN if)
            (FRAG (RB not)
              (ADVP (RBR sooner))))
          (, ,)
          (NP (NNPS Europeans))
          (VP (MD will)
            (VP (VB cease)
              (S
                (VP (TO to)
                  (VP (VB exist)
                    (PP (IN as)
                      (NP (DT a) (JJ unique) (NNS people) (NN group)))))))))))
    (. .)))

root(ROOT-0, Consider-1)
nsubj(cease-15, Europe-2)
xsubj(exist-17, Europe-2)
det(years-7, a-5)
num(years-7, hundred-6)
