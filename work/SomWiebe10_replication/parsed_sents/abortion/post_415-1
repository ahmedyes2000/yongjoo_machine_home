(ROOT
  (S
    (NP (NN Today))
    (NP (PRP I))
    (VP (VBD restated)
      (SBAR
        (WHADVP (WRB why))
        (S
          (NP (NN birth))
          (VP (VBZ is)
            (NP
              (NP (DT the) (NN objective))
              (, ,)
              (NP (JJ rational) (, ,) (JJ absolute) (NNS criteria))
              (, ,)
              (SBAR (IN so)
                (S
                  (NP (PRP I))
                  (VP (MD wo) (RB n't)
                    (VP (VB repost)
                      (NP (PRP it))
                      (ADVP (RB here)))))))))))
    (. .)))

tmod(restated-3, Today-1)
nsubj(restated-3, I-2)
root(ROOT-0, restated-3)
advmod(objective-8, why-4)
nsubj(objective-8, birth-5)
cop(objective-8, is-6)
det(objective-8, the-7)
advcl(restated-3, objective-8)
amod(criteria-13, rational-10)
amod(criteria-13, absolute-12)
appos(objective-8, criteria-13)
mark(repost-19, so-15)
nsubj(repost-19, I-16)
aux(repost-19, wo-17)
neg(repost-19, n't-18)
dep(objective-8, repost-19)
dobj(repost-19, it-20)
advmod(repost-19, here-21)

