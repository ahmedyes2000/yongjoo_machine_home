(ROOT
  (S
    (S
      (NP (DT Each))
      (VP (VBZ is)
        (ADJP (JJ necessary))))
    (, ,)
    (CC but)
    (S
      (NP (NN none))
      (VP (VBP are)
        (ADVP (RB individually))
        (ADJP (JJ sufficient))))
    (. .)))

nsubj(necessary-3, Each-1)
cop(necessary-3, is-2)
root(ROOT-0, necessary-3)
nsubj(sufficient-9, none-6)
cop(sufficient-9, are-7)
advmod(sufficient-9, individually-8)
conj_but(necessary-3, sufficient-9)

