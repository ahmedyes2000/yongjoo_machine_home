(ROOT
  (S
    (NP (DT These) (NNS conditions))
    (VP (VBP are)
      (NP
        (NP (DT the) (JJ same))
        (PP (IN for)
          (NP
            (NP (DT every) (JJ other) (NN tissue))
            (PP (IN in)
              (NP
                (NP (DT the) (NN mother) (POS 's))
                (NN body)))))))
    (. .)))

det(conditions-2, These-1)
nsubj(same-5, conditions-2)
cop(same-5, are-3)
det(same-5, the-4)
root(ROOT-0, same-5)
det(tissue-9, every-7)
amod(tissue-9, other-8)
prep_for(same-5, tissue-9)
det(mother-12, the-11)
poss(body-14, mother-12)
prep_in(tissue-9, body-14)

