(ROOT
  (S
    (VP (VBZ is) (RB not)
      (NP (DT the) (NN right)
        (S
          (VP (TO to)
            (VP (VB exist)
              (PP (IN under)
                (NP
                  (NP
                    (ADJP (DT any)
                      (CC and)
                      (DT all))
                    (NNS sets))
                  (PP (IN of)
                    (NP (NNS circumstances))))))))))
    (. .)))

cop(right-4, is-1)
neg(right-4, not-2)
det(right-4, the-3)
