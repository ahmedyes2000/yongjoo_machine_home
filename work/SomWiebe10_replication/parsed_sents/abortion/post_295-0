  (S
    (NP
      (NP (JJ thats) (JJ true) (NNS i))
      (VP (VBN looked)
        (PP (IN at)
          (NP
            (NP (DT the) (NNS pictures))
            (PP (IN of)
              (NP (DT the) (NNS fetuses)))))))
    (VP (VBZ thats)
      (NP (RB just) (DT a) (JJ crying) (NN shame)))
    (. .)))

