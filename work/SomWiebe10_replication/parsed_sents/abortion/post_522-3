  (S
    (NP (DT This))
    (VP (VBZ is)
      (NP (DT the) (JJ same) (NN scenario))
      (SBAR (RB as) (IN if)
        (S
          (NP (DT an) (NN abortion))
          (VP (VBD occured)
            (S
              (VP (TO to)
                (VP (VB save)
                  (NP
                    (NP (DT the) (NN life))
                    (PP (IN of)
                      (NP (DT the) (NN mother)))))))))))
    (. .)))

