(ROOT
  (S
    (PP (IN Unlike)
      (NP (NN transubstantiation)))
    (, ,)
    (NP
      (NP (DT the) (NN ending))
      (PP (IN of)
        (NP (DT a) (JJ human) (NN life))))
    (VP (VBZ is) (RB not)
      (NP
        (NP (DT a) (JJ theological) (NN nicety))
        (SBAR
          (S
            (NP (PRP it))
            (VP (VBZ is)
              (NP (DT an) (JJ important) (JJ moral) (NN controversy)))))))
    (. .)))

prep_unlike(nicety-14, transubstantiation-2)
det(ending-5, the-4)
nsubj(nicety-14, ending-5)
det(life-9, a-7)
amod(life-9, human-8)
prep_of(ending-5, life-9)
cop(nicety-14, is-10)
neg(nicety-14, not-11)
det(nicety-14, a-12)
amod(nicety-14, theological-13)
root(ROOT-0, nicety-14)
nsubj(controversy-20, it-15)
cop(controversy-20, is-16)
det(controversy-20, an-17)
amod(controversy-20, important-18)
amod(controversy-20, moral-19)
rcmod(nicety-14, controversy-20)

