(ROOT
  (S ('' '')
    (S
      (S
        (VP
          (PRN (-LRB- -LRB-)
            (NP (CD 4))
            (-RRB- -RRB-))
          (PP (IN In)
            (NP (JJ other) (NNS words)))))
      (, ,)
      (NP (DT the) (NN unborn))
      (VP (MD would) (RB all)
        (VP (VB be)
          (PP (IN against)
            (NP (NN abortion))))))
    (, ,)
    (CC but)
    (S (, ,)
      (PP (IN of)
        (NP (NN course)))
      (, ,)
      (NP (PRP they))
      (VP (MD ca) (RB n't)
        (VP (VB express)
          (NP (PRP$ their) (NN opinion))
          (PP (IN as)
            (NP (JJ for\/against) (NN abortion))))))
    (. .)))

