(ROOT
  (S
    (NP (DT This))
    (VP (VBZ is)
      (SBAR
        (WHADVP (WRB why))
        (S
          (NP (PRP it))
          (VP (MD should) (RB not)
            (VP (VB be)
              (ADJP (JJ legal)))))))
    (. .)))

nsubj(is-2, This-1)
root(ROOT-0, is-2)
advmod(legal-8, why-3)
nsubj(legal-8, it-4)
aux(legal-8, should-5)
neg(legal-8, not-6)
cop(legal-8, be-7)
advcl(is-2, legal-8)

