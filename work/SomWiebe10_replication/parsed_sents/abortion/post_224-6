(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP
          (VP (VBP call)
            (S
              (NP (PRP yourself))
              (ADJP (NNP Catholic))))
          (CC and)
          (VP (VBP believe)
            (SBAR
              (S
                (NP (PRP you))
                (VP (MD can)
                  (VP (VB be)
                    (ADJP (JJ Pro-Choice))))))))))
    (, ,)
    (NP (PRP you))
    (VP (VBP 're)
      (ADJP (JJ delusional)))
    (. .)))

mark(call-3, If-1)
nsubj(call-3, you-2)
nsubj(believe-7, you-2)
advcl(delusional-15, call-3)
nsubj(Catholic-5, yourself-4)
xcomp(call-3, Catholic-5)
conj_and(call-3, believe-7)
advcl(delusional-15, believe-7)
nsubj(Pro-Choice-11, you-8)
aux(Pro-Choice-11, can-9)
cop(Pro-Choice-11, be-10)
ccomp(believe-7, Pro-Choice-11)
nsubj(delusional-15, you-13)
cop(delusional-15, 're-14)
root(ROOT-0, delusional-15)

