(ROOT
  (S
    (NP (PRP You))
    (VP (MD can)
      (VP
        (VP (VB go)
          (CC and)
          (VB hear)
          (NP
            (NP (DT all))
            (PP (IN of)
              (NP (PRP$ their) (NNS stories)))))
        (CC and)
        (VP (VB decide)
          (NP
            (SBAR
              (WHNP (WP who))
              (S
                (VP (MD can))))
            (CC and)
            (SBAR
              (WHNP (WP who))
              (S
                (VP (MD ca) (RB n't)
                  (VP (VB get)
                    (NP (NNS abortions))))))))))
    (. .)))

nsubj(go-3, You-1)
nsubj(hear-5, You-1)
nsubj(decide-11, You-1)
aux(go-3, can-2)
root(ROOT-0, go-3)
