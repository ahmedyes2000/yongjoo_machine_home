(ROOT
  (S
    (NP (PRP I))
    (VP (MD must)
      (VP (VB tell)
        (NP (PRP you))
        (SBAR (IN that)
          (S
            (NP (PRP I))
            (VP (VBP am)
              (ADVP (RB totally))
              (VP (VBN amazed)))))))
    (. .)))

nsubj(tell-3, I-1)
aux(tell-3, must-2)
root(ROOT-0, tell-3)
dobj(tell-3, you-4)
complm(amazed-9, that-5)
nsubjpass(amazed-9, I-6)
auxpass(amazed-9, am-7)
advmod(amazed-9, totally-8)
ccomp(tell-3, amazed-9)

