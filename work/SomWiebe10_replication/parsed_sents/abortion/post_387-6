(ROOT
  (S
    (NP (PRP It))
    (VP (MD can) (RB not)
      (VP (VB think)
        (CC or)
        (VB act)
        (PP (IN without)
          (NP (PRP$ its) (NN host)))))
    (. .)))

nsubj(think-4, It-1)
nsubj(act-6, It-1)
aux(think-4, can-2)
neg(think-4, not-3)
root(ROOT-0, think-4)
conj_or(think-4, act-6)
poss(host-9, its-8)
prep_without(think-4, host-9)

