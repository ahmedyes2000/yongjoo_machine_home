  (S
    (INTJ (UH No))
    (, ,)
    (NP (PRP she))
    (VP (MD should)
      (VP
        (VP (VB give)
          (NP (PRP it))
          (PRT (RP up))
          (PP (IN for)
            (NP (NN adoption))))
        (CC and)
        (VP (VB be)
          (ADJP (JJ proud)
            (SBAR
              (S
                (NP (PRP she))
                (VP (VBD gave)
                  (NP (DT a) (JJ loving) (NN home))
                  (NP
                    (NP (DT a) (NN child))
                    (SBAR
                      (S
                        (NP (PRP they))
                        (VP (MD would) (RB not)
                          (VP (VB have)
                            (VP (VBN had)))))))))))
          (ADVP (RB otherwise)))))
    (. .)))

