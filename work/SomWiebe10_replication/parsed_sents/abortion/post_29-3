(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is)
      (NP
        (NP (DT a) (JJ serious) (NN matter))
        (PP (IN of)
          (NP (NN reproduction)
            (CC and)
            (NN life)))))
    (. .)))

nsubj(matter-5, It-1)
cop(matter-5, is-2)
det(matter-5, a-3)
amod(matter-5, serious-4)
root(ROOT-0, matter-5)
prep_of(matter-5, reproduction-7)
prep_of(matter-5, life-9)
conj_and(reproduction-7, life-9)

