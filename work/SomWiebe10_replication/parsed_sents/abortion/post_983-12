(ROOT
  (S (CC And)
    (NP (DT that))
    (VP (VBZ means)
      (SBAR (IN that)
        (S
          (NP (NNP Jesus))
          (VP (VBD died)
            (PP (IN in)
              (NP (JJ vain)))))))
    (. .)))

cc(means-3, And-1)
nsubj(means-3, that-2)
root(ROOT-0, means-3)
complm(died-6, that-4)
nsubj(died-6, Jesus-5)
ccomp(means-3, died-6)
prep_in(died-6, vain-8)

