(ROOT
  (S
    (NP (PRP I))
    (VP (MD would)
      (VP (VB say)
        (SBAR
          (SBAR
            (S
              (NP (NN man))
              (VP (VBZ has)
                (NP (DT a) (NN nature)))))
          (CC and)
          (SBAR (IN that)
            (S
              (NP (PRP it))
              (VP (VBZ is)
                (VP (VBN fixed))))))))
    (. .)))

nsubj(say-3, I-1)
aux(say-3, would-2)
root(ROOT-0, say-3)
nsubj(has-5, man-4)
ccomp(say-3, has-5)
det(nature-7, a-6)
dobj(has-5, nature-7)
complm(fixed-12, that-9)
nsubjpass(fixed-12, it-10)
auxpass(fixed-12, is-11)
ccomp(say-3, fixed-12)
conj_and(has-5, fixed-12)

