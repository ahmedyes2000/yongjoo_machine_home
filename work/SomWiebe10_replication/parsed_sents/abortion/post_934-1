(ROOT
  (S
    (S
      (SBAR (IN If)
        (S
          (NP (NN perfection))
          (VP (VBZ is)
            (NP
              (NP (PRP$ your) (NN standard))
              (PP (IN for)
                (NP (NN law)))))))
      (ADVP (RB then))
      (NP (PRP you))
      (VP (VBP are)
        (ADJP (JJ correct))))
    (, ,)
    (ADVP (RB however))
    (S
      (NP (NNS laws))
      (VP (VBD were)
        (VP (VBN meant)
          (S
            (VP (TO to)
              (VP (VB restrain)))))))
    (. .)))

mark(standard-5, If-1)
nsubj(standard-5, perfection-2)
cop(standard-5, is-3)
poss(standard-5, your-4)
advcl(correct-11, standard-5)
prep_for(standard-5, law-7)
advmod(correct-11, then-8)
nsubj(correct-11, you-9)
cop(correct-11, are-10)
root(ROOT-0, correct-11)
advmod(correct-11, however-13)
nsubjpass(meant-16, laws-14)
xsubj(restrain-18, laws-14)
auxpass(meant-16, were-15)
ccomp(correct-11, meant-16)
aux(restrain-18, to-17)
xcomp(meant-16, restrain-18)

