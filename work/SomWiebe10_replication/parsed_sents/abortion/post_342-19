(ROOT
  (S
    (NP (DT That))
    (VP (VBZ 's)
      (NP
        (NP (NN nature))
        (VP (VBG taking)
          (NP (PRP$ its) (NN course)))))
    (. .)))

nsubj(nature-3, That-1)
cop(nature-3, 's-2)
root(ROOT-0, nature-3)
partmod(nature-3, taking-4)
poss(course-6, its-5)
dobj(taking-4, course-6)

