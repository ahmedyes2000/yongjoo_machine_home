(ROOT
  (S
    (NP (DT The) (NN DNA))
    (VP (VBZ is)
      (VP (VBN set)
        (PP (IN for)
          (NP (DT that) (NN person)))
        (ADVP (RB forever))))
    (. .)))

det(DNA-2, The-1)
nsubjpass(set-4, DNA-2)
auxpass(set-4, is-3)
root(ROOT-0, set-4)
det(person-7, that-6)
prep_for(set-4, person-7)
advmod(set-4, forever-8)

