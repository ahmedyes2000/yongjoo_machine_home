(ROOT
  (S
    (NP (PRP I))
    (VP (VBP have) (RB not)
      (VP (VBN scoured)
        (NP
          (NP (DT this) (NN site))
          (VP (VBG looking)
            (PP (IN for)
              (NP
                (NP (DT all))
                (PP (IN of)
                  (NP (PRP$ your) (NNS comments)))))))))
    (. .)))

nsubj(scoured-4, I-1)
aux(scoured-4, have-2)
neg(scoured-4, not-3)
root(ROOT-0, scoured-4)
det(site-6, this-5)
dobj(scoured-4, site-6)
partmod(site-6, looking-7)
prep_for(looking-7, all-9)
poss(comments-12, your-11)
prep_of(all-9, comments-12)

