(ROOT
  (S
    (NP
      (NP (JJS Most) (NNS women))
      (SBAR
        (WHNP (WP who))
        (S
          (VP (VBP have)
            (NP (PRP them))))))
    (VP (VBP regret)
      (NP (PRP$ their) (NN decision)))
    (. .)))

amod(women-2, Most-1)
nsubj(have-4, women-2)
nsubj(regret-6, women-2)
rcmod(women-2, have-4)
dobj(have-4, them-5)
root(ROOT-0, regret-6)
poss(decision-8, their-7)
dobj(regret-6, decision-8)

