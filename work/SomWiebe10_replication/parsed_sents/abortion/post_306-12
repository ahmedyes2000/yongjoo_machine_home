(ROOT
  (S
    (NP (DT The) (JJ only) (NN way)
      (S
        (VP (TO to)
          (VP (VB know)
            (SBAR
              (WHNP (WP what))
              (S
                (VP (VBZ is)
                  (NP (DT a) (JJ human) (NN being)))))))))
    (VP (VBZ is)
      (PP (IN by)
        (S
          (VP (VBG looking)
            (PP (IN at)
              (NP (NN reality))))))
      (, ,)
      (FRAG (RB not)
        (ADJP (JJ inward)
          (PP (IN into)
            (NP (PRP$ your) (NN consciousness))))))
    (. .)))

det(way-3, The-1)
amod(way-3, only-2)
nsubj(is-11, way-3)
aux(know-5, to-4)
infmod(way-3, know-5)
