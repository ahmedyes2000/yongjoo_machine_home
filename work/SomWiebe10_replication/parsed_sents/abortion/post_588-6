aux(trust-16, can-15)
root(ROOT-0, trust-16)
dobj(trust-16, me-17)

(ROOT
  (S
    (SBAR (IN While)
      (S
        (NP (PRP I))
        (VP (VBP have) (RB n't)
          (VP (VBN been)
            (PP (IN over)
              (NP
                (NP (RB there))
                (, ,)
                (PRN (-LRB- -LRB-)
                  (ADVP (RB yet))
                  (-RRB- -RRB-))))))))
    (NP (PRP I))
    (VP (VBP have)
      (NP
        (NP (NNS friends))
        (SBAR
          (WHNP (WP who))
          (S
            (VP (VBP have))))))
    (. .)))

