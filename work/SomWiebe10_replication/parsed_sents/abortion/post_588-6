(ROOT
  (S
    (SBAR (IN if)
      (S
        (NP (PRP you))
        (VP (VBP do) (RB n't)
          (VP (VB believe)
            (NP
              (NP (DT the) (NNS media)
                (CC or)
                (NNS whatever))
              (PP (IN for)
                (NP (WDT whatever) (NN reason))))))))
    (, ,)
    (NP (PRP you))
    (VP (MD can)
      (VP (VB trust)
        (NP (PRP me))))
    (. .)))

mark(believe-5, if-1)
