(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is)
      (VP (VBG taking)
        (NP
          (NP (DT the) (NN life))
          (PP (IN of)
            (NP (DT another) (NN human) (VBG being))))))
    (. .)))

nsubj(taking-3, It-1)
aux(taking-3, is-2)
root(ROOT-0, taking-3)
det(life-5, the-4)
dobj(taking-3, life-5)
det(human-8, another-7)
prep_of(life-5, human-8)
amod(human-8, being-9)

