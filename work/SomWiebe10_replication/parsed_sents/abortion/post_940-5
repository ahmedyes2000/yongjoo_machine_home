(ROOT
  (FRAG
    (NP (NNP True))
    (, ,)
    (CC and)
    (S
      (NP (PRP it))
      (VP (VBZ 's)
        (NP
          (NP (NN state))
          (PP (TO to)
            (NP (NN state))))))
    (. .)))

root(ROOT-0, True-1)
dep(True-1, and-3)
nsubj(state-6, it-4)
cop(state-6, 's-5)
dep(True-1, state-6)
prep_to(state-6, state-8)

