  (S
    (NP (PRP$ My) (NNS comments))
    (VP
      (VP (VBD were)
        (ADVP (RB strictly))
        (PP (IN in)
          (NP
            (NP (NN response))
            (NP (DT the) (`` ``) (NN argument) ('' '')))))
      (CC and)
