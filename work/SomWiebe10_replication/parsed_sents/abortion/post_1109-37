(ROOT
  (S
    (NP (NN Sex))
    (VP (VBZ is)
      (VP (VBN used)
        (S
          (VP (TO to)
            (VP (VB sell)
              (NP (NN everything))
              (NP (DT these) (NNS days)))))))
    (. .)))

nsubjpass(used-3, Sex-1)
xsubj(sell-5, Sex-1)
auxpass(used-3, is-2)
root(ROOT-0, used-3)
aux(sell-5, to-4)
xcomp(used-3, sell-5)
dobj(sell-5, everything-6)
det(days-8, these-7)
tmod(sell-5, days-8)

