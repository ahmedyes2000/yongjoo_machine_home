(ROOT
  (S
    (NP (PRP I))
    (VP (MD 'd)
      (VP (VB like)
        (VP (VB add)
          (NP (DT a) (JJ different) (NN perspective))
          (PP (TO to)
            (NP (PRP$ your) (NN argument))))))
    (. .)))

nsubj(like-3, I-1)
aux(like-3, 'd-2)
root(ROOT-0, like-3)
xcomp(like-3, add-4)
det(perspective-7, a-5)
amod(perspective-7, different-6)
dobj(add-4, perspective-7)
poss(argument-10, your-9)
prep_to(add-4, argument-10)

