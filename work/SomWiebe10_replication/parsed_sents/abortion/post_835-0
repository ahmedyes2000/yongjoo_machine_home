  (S (`` ``)
    (NP (PRP I))
    (VP (VBP understand)
      (SBAR
        (S
          (NP (PRP you))
          (VP (VBP think)
            (SBAR
              (S
                (NP (NN life))
                (VP (VBZ is)
                  (ADJP (RB really) (JJ important)))))))))
    (. .)))

nsubj(understand-3, I-2)
root(ROOT-0, understand-3)
