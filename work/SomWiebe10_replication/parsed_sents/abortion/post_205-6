(ROOT
  (S
    (S
      (VP (VB Let)
        (SBAR
          (S
            (NP (PRP me))
            (ADVP (RB first))
            (VP (VBP state)
              (NP (PRP$ my) (NNS premises)))))))
    (: :)
    (S
      (NP (NNP Abortion))
      (VP (VBZ is)
        (NP (DT an) (JJ intrinsic) (NN evil))))
    (. .)))

root(ROOT-0, Let-1)
nsubj(state-4, me-2)
advmod(state-4, first-3)
ccomp(Let-1, state-4)
poss(premises-6, my-5)
dobj(state-4, premises-6)
nsubj(evil-12, Abortion-8)
