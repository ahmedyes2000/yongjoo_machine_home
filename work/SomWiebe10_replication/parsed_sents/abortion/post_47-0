(ROOT
  (S
    (NP (NNP Abortion))
    (VP (VBZ is)
      (ADJP
        (ADJP
          (ADVP (RB no) (RBR more))
          (JJ risky)
          (CC or)
          (JJ harmful))
        (PP (IN than)
          (NP (JJ ordinary) (NN birth)))))
    (. .)))

nsubj(risky-5, Abortion-1)
nsubj(harmful-7, Abortion-1)
cop(risky-5, is-2)
advmod(more-4, no-3)
advmod(risky-5, more-4)
root(ROOT-0, risky-5)
conj_or(risky-5, harmful-7)
amod(birth-10, ordinary-9)
prep_than(risky-5, birth-10)

