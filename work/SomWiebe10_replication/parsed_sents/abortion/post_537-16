(ROOT
  (S (CC But)
    (NP (DT this))
    (VP (VBZ does) (RB not)
      (VP (VB justify)
        (NP (JJ sweeping) (, ,) (NN blanket) (NNS pronouncements))
        (PP (IN by)
          (NP
            (NP (DT either) (NN side))
            (PP (IN in)
              (NP
                (NP (DT the) (NN absence))
                (PP (IN of)
                  (NP (JJ supporting) (NNS facts)))))))))
    (. .)))

cc(justify-5, But-1)
nsubj(justify-5, this-2)
aux(justify-5, does-3)
neg(justify-5, not-4)
root(ROOT-0, justify-5)
amod(pronouncements-9, sweeping-6)
dep(pronouncements-9, blanket-8)
dobj(justify-5, pronouncements-9)
det(side-12, either-11)
prep_by(justify-5, side-12)
det(absence-15, the-14)
prep_in(side-12, absence-15)
amod(facts-18, supporting-17)
prep_of(absence-15, facts-18)

