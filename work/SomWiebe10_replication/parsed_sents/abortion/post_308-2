  (S
    (S
      (VP (VB Straight)
        (PP (IN from)
          (NP
            (NP (DT the) (NN horse) (POS 's))
            (NN mouth)))))
    (, ,)
    (NP (PRP it))
    (VP (VBZ does) (RB n't)
      (VP (VB get)
        (NP (DT any) (JJR clearer))
        (PP (IN than)
          (NP (DT that)))))
    (. .)))

ccomp(get-11, Straight-1)
det(horse-4, the-3)
poss(mouth-6, horse-4)
prep_from(Straight-1, mouth-6)
