  (S
    (ADVP (RB So))
    (, ,)
    (PP (IN from)
      (NP (DT a) (JJ scientific) (NN perspective)))
    (, ,)
    (NP (EX there))
    (ADVP (RB really))
    (VP (VBZ is)
      (NP
        (NP
          (ADJP (RB very) (JJ little))
          (NN difference))
        (PP (IN between)
          (NP (DT the) (CD two)))))
    (. .)))

advmod(is-10, So-1)
det(perspective-6, a-4)
amod(perspective-6, scientific-5)
prep_from(is-10, perspective-6)
expl(is-10, there-8)
advmod(is-10, really-9)
