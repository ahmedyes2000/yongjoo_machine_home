(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (NNS men))
        (VP (MD could)
          (VP (VB get)
            (ADJP (JJ pregnant))))))
    (, ,)
    (NP (NN abortion))
    (VP (MD would)
      (VP (VB be)
        (NP (DT a) (NN sacrament))))))

mark(pregnant-5, If-1)
nsubj(pregnant-5, men-2)
aux(pregnant-5, could-3)
dep(pregnant-5, get-4)
