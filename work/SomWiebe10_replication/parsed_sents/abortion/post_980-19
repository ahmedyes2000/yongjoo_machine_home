(ROOT
  (S
    (NP (DT The) (NN question))
    (VP (VBZ is) (RB not)
      (PP (IN about)
        (SBAR
          (WHNP (WP what))
          (S
            (VP (VBZ is)
              (VP (VBG being)
                (VP (VBN killed)
                  (PP (IN in)
                    (NP (DT an) (NN abortion))))))))))
    (. .)))

det(question-2, The-1)
nsubj(is-3, question-2)
root(ROOT-0, is-3)
neg(is-3, not-4)
nsubjpass(killed-9, what-6)
aux(killed-9, is-7)
auxpass(killed-9, being-8)
prepc_about(is-3, killed-9)
det(abortion-12, an-11)
prep_in(killed-9, abortion-12)

