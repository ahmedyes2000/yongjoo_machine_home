(ROOT
  (S
    (NP (DT These) (NNS women))
    (ADVP (RB often))
    (VP (VBP need)
      (NP (NN chemotherapy)
        (CC or)
        (NN radiation) (NN therapy)))
    (. .)))

det(women-2, These-1)
nsubj(need-4, women-2)
advmod(need-4, often-3)
root(ROOT-0, need-4)
nn(therapy-8, chemotherapy-5)
conj_or(chemotherapy-5, radiation-7)
nn(therapy-8, radiation-7)
dobj(need-4, therapy-8)

