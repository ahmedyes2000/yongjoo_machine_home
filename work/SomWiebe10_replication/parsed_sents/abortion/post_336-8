(ROOT
  (S
    (ADVP (RBR More) (RB importantly))
    (, ,)
    (NP (NN pregnancy))
    (VP (VBZ is)
      (NP
        (NP (DT a) (NN choice))
        (PP (IN in)
          (NP (PRP itself)))))
    (. .)))

advmod(importantly-2, More-1)
advmod(choice-7, importantly-2)
nsubj(choice-7, pregnancy-4)
cop(choice-7, is-5)
det(choice-7, a-6)
root(ROOT-0, choice-7)
prep_in(choice-7, itself-9)

