(ROOT
  (S
    (NP (JJ Collateral) (NN damage) (NN wil))
    (VP (VBP happen)
      (ADVP
        (ADVP (RB so) (RB long))
        (SBAR (IN as)
          (S
            (NP (EX there))
            (VP (VBZ is)
              (NP (NN war)))))))
    (. .)))

