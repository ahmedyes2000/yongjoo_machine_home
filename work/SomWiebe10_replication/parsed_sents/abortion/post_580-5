(ROOT
  (S
    (PP (IN At)
      (NP
        (NP (DT the) (NN moment))
        (PP (IN of)
          (NP (NN conception)))))
    (, ,)
    (NP (DT a) (JJ distinct) (JJ human) (NN life))
    (VP (VBZ is)
      (VP (VBN created)))
    (. .)))

det(moment-3, the-2)
prep_at(created-12, moment-3)
prep_of(moment-3, conception-5)
det(life-10, a-7)
amod(life-10, distinct-8)
amod(life-10, human-9)
nsubjpass(created-12, life-10)
auxpass(created-12, is-11)
root(ROOT-0, created-12)

