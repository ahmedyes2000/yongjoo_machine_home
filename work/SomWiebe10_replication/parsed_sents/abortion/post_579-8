(ROOT
  (S
    (PP (IN Of)
      (NP (NN course)))
    (, ,)
    (NP (DT this))
    (VP (VBZ is)
      (SBAR
        (WHNP (WP what))
        (S
          (ADVP (RB generally))
          (VP (VBZ happens)
            (PP (IN in)
              (NP (JJ such) (NNS cases)))
            (, ,)
            (ADVP (RB anyway))))))
    (. .)))

prep_of(is-5, course-2)
nsubj(is-5, this-4)
root(ROOT-0, is-5)
nsubj(happens-8, what-6)
advmod(happens-8, generally-7)
ccomp(is-5, happens-8)
amod(cases-11, such-10)
prep_in(happens-8, cases-11)
advmod(happens-8, anyway-13)

