(ROOT
  (S
    (NP (DT That))
    (VP (VBZ is)
      (SBAR
        (WHADVP (WRB when))
        (S
          (NP (DT the) (NN life) (NN cycle))
          (VP (VBZ begins)))))
    (. .)))

nsubj(is-2, That-1)
root(ROOT-0, is-2)
advmod(begins-7, when-3)
det(cycle-6, the-4)
nn(cycle-6, life-5)
nsubj(begins-7, cycle-6)
advcl(is-2, begins-7)

