(ROOT
  (S
    (NP
      (NP (RB Almost) (DT the) (NN entirity))
      (PP (IN of)
        (NP (PRP$ your) (JJ last) (NN post))))
    (VP (VBD was)
      (ADJP (JJ irrational)))
    (. .)))

advmod(entirity-3, Almost-1)
det(entirity-3, the-2)
nsubj(irrational-9, entirity-3)
poss(post-7, your-5)
amod(post-7, last-6)
prep_of(entirity-3, post-7)
cop(irrational-9, was-8)
root(ROOT-0, irrational-9)

