    (WHNP
      (WHNP (JJ Look))
      (PP (IN at)
        (NP
          (NP (PRP it))
          (NP (DT this) (NN way)))
        (: :)))
    (SQ (MD should)
      (NP (DT a) (JJ human) (NN life))
      (VP (VB suffer)
        (PP (RB because) (IN of)
          (NP
            (NP (DT the) (NNS mistakes))
            (PP (IN of)
              (NP (PRP$ their) (NNS parents)))))))
    (. ?)))

