(ROOT
  (S
    (NP (DT That))
    (VP (VBZ 's) (RB not)
      (NP (DT a)
        (ADJP (RB very) (JJ loving))
        (NN thing)
        (S
          (VP (TO to)
            (VP (VB do))))))
    (. .)))

nsubj(thing-7, That-1)
cop(thing-7, 's-2)
neg(thing-7, not-3)
det(thing-7, a-4)
advmod(loving-6, very-5)
amod(thing-7, loving-6)
root(ROOT-0, thing-7)
aux(do-9, to-8)
infmod(thing-7, do-9)

