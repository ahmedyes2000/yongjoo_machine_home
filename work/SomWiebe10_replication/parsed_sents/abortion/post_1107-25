(ROOT
  (S
    (NP (NN Trouble))
    (VP (VBZ is)
      (SBAR
        (S
          (NP (EX there))
          (VP (VBP are)
            (NP
              (NP (JJR more) (NNS voices))
              (VP (VBG telling)
                (NP (NNS kids))
                (S
                  (VP (TO to)
                    (VP (VB have)
                      (NP (NN sex))
                      (ADVP (IN than) (RB not)))))))))))
    (. .)))

nsubj(is-2, Trouble-1)
root(ROOT-0, is-2)
expl(are-4, there-3)
ccomp(is-2, are-4)
amod(voices-6, more-5)
nsubj(are-4, voices-6)
partmod(voices-6, telling-7)
dobj(telling-7, kids-8)
aux(have-10, to-9)
xcomp(telling-7, have-10)
dobj(have-10, sex-11)
neg(have-10, than-12)
dep(than-12, not-13)

