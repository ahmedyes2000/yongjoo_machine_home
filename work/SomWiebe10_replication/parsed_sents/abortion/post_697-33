(ROOT
  (S
    (ADVP (RB Anyhow))
    (, ,)
    (NP (PRP I))
    (VP (MD would)
      (VP (VB appreciate)
        (NP (PRP it))
        (SBAR (IN if)
          (S
            (NP (PRP you))
            (VP (MD would) (RB not)
              (VP
                (VP (VB take)
                  (NP (PRP$ my) (NNS statements))
                  (PRT (RP out))
                  (PP (IN of)
                    (NP (NN context))))
                (CC and)
                (VP (VB apply)
                  (NP (NNS meanings))
                  (PP (TO to)
                    (NP (PRP them)))
                  (SBAR (IN that)
                    (S
                      (NP (PRP I))
                      (ADVP (RB never))
                      (VP (VBD stated)))))))))))
    (. .)))

