(ROOT
  (S
    (NP (NNP Roe))
    (VP (VBZ tries)
      (S
        (VP (TO to)
          (VP (VB stop)
            (NP (DT the) (NNS people))
            (PP (IN from)
              (S
                (VP (VBG voting)
                  (ADVP (RB up)
                    (CC or)
                    (RB down))
                  (NP (NN abortion)))))))))
    (. .)))

nsubj(tries-2, Roe-1)
xsubj(stop-4, Roe-1)
root(ROOT-0, tries-2)
aux(stop-4, to-3)
