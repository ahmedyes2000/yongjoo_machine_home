(ROOT
  (S
    (NP
      (NP (DT The) (RB only) (JJ safe) (NN place))
      (PP (IN of)
        (NP
          (NP (NN expression))
          (PP (IN for)
            (NP (NN sex))))))
    (VP (VBZ is)
      (PP (IN within)
        (NP
          (NP (DT the) (NN context))
          (PP (IN of)
            (NP (DT a) (NN life) (JJ long) (NN heterosexual) (NN marriage))))))
    (. .)))

det(place-4, The-1)
advmod(place-4, only-2)
amod(place-4, safe-3)
nsubj(is-9, place-4)
prep_of(place-4, expression-6)
prep_for(expression-6, sex-8)
root(ROOT-0, is-9)
det(context-12, the-11)
prep_within(is-9, context-12)
det(marriage-18, a-14)
nn(marriage-18, life-15)
amod(marriage-18, long-16)
nn(marriage-18, heterosexual-17)
prep_of(context-12, marriage-18)

