(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (DT this))
        (VP (VBZ is) (RB not)
          (ADVP (RB so)))))
    (, ,)
    (ADVP (RB then))
    (NP (DT the) (NN mother))
    (VP (VBZ does) (RB not)
      (VP (VB have)
        (NP
          (NP (DT a) (JJ natural) (NN right))
          (PP (TO to)
            (NP (NN life))))
        (ADVP (RB either))))
    (. .)))

mark(is-3, If-1)
nsubj(is-3, this-2)
advcl(have-12, is-3)
neg(is-3, not-4)
advmod(is-3, so-5)
advmod(have-12, then-7)
det(mother-9, the-8)
nsubj(have-12, mother-9)
aux(have-12, does-10)
neg(have-12, not-11)
root(ROOT-0, have-12)
det(right-15, a-13)
amod(right-15, natural-14)
dobj(have-12, right-15)
prep_to(right-15, life-17)
advmod(have-12, either-18)

