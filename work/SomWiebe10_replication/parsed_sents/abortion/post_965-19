(ROOT
  (S
    (PP (IN For)
      (NP (NN abortion)))
    (NP (DT the) (JJ approximate) (NN death) (NN rate))
    (VP (VBZ is)
      (NP
        (NP
          (QP (CD 1.4) (CD million))
          (NNS babies))
        (NP (DT each) (NN year))))
    (. .)))

prep_for(babies-10, abortion-2)
det(rate-6, the-3)
amod(rate-6, approximate-4)
nn(rate-6, death-5)
nsubj(babies-10, rate-6)
cop(babies-10, is-7)
number(million-9, 1.4-8)
num(babies-10, million-9)
root(ROOT-0, babies-10)
det(year-12, each-11)
dep(babies-10, year-12)

