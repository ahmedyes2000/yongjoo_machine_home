  (S
    (NP (DT This))
    (VP (VBZ is)
      (SBAR
        (WHADVP (WRB why))
        (S
          (NP (PRP I))
          (VP (VBD responded)
            (SBAR (IN that)
              (S
                (NP (NN morality))
                (VP (MD ca) (RB n't)
                  (VP (VB be)
                    (VP (VBN turned)
                      (PRT (RP on)
                        (CC and)
