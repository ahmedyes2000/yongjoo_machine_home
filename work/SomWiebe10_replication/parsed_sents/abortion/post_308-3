(ROOT
  (S
    (NP (DT The) (NNS Founders))
    (ADVP (RB conscientiously))
    (VP (VBD derived)
      (NP (PRP$ their) (JJ secular) (JJ political) (NN philosophy))
      (PP (IN from)
        (NP (NNP Enlightenment) (NNS ideas))))
    (. .)))

det(Founders-2, The-1)
nsubj(derived-4, Founders-2)
advmod(derived-4, conscientiously-3)
root(ROOT-0, derived-4)
poss(philosophy-8, their-5)
amod(philosophy-8, secular-6)
amod(philosophy-8, political-7)
dobj(derived-4, philosophy-8)
nn(ideas-11, Enlightenment-10)
prep_from(derived-4, ideas-11)

