(ROOT
  (S
    (S
      (NP (DT A) (NN being))
      (VP (VBP need) (RB not)
        (VP (VB exhibit)
          (NP (DT every) (NN criterion)
            (S
              (VP (TO to)
                (VP (VB qualify)
                  (PP (IN as)
                    (NP (DT a) (NN person))))))))))
    (, ,)
    (CC but)
    (S
      (NP (NN failure)
        (S
          (VP (TO to)
            (VP (VB exhibit)
              (ADVP (RBS most))))))
      (VP (VBZ is)
        (VP (VBN proposed)
          (PP (IN as)
            (NP (NN disqualification))))))
    (. .)))

det(being-2, A-1)
nsubj(need-3, being-2)
root(ROOT-0, need-3)
neg(need-3, not-4)
dep(need-3, exhibit-5)
det(criterion-7, every-6)
dobj(exhibit-5, criterion-7)
aux(qualify-9, to-8)
infmod(criterion-7, qualify-9)
det(person-12, a-11)
prep_as(qualify-9, person-12)
nsubjpass(proposed-20, failure-15)
aux(exhibit-17, to-16)
infmod(failure-15, exhibit-17)
advmod(exhibit-17, most-18)
auxpass(proposed-20, is-19)
conj_but(need-3, proposed-20)
prep_as(proposed-20, disqualification-22)

