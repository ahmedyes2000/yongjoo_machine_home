  (S
    (S
      (NP (DT This) (NN ruling))
      (VP (VBZ goes)
        (PP (IN against)
          (NP (DT the) (JJ very) (NN Constitution) (PRP itself)))))
    (: ;)
    (S
      (NP (PRP It))
      (VP (VBZ is)
        (NP (DT a) (NN disgrace))))
    (. .)))

det(ruling-2, This-1)
nsubj(goes-3, ruling-2)
root(ROOT-0, goes-3)
det(Constitution-7, the-5)
