(ROOT
  (S ('' '')
    (PP (IN At)
      (NP
        (NP (CD one))
        (VP (VBP point)
          (NP (PRP you)))))
    (VP (VBD were)
      (NP (`` ``)
        (NP (DT a) (JJ tiny) (NN clump))
        (PP (IN of)
          (NP (NNS cells)))
        ('' ''))
      (ADVP (RB too)))
    (. .)))

prep_at(clump-10, one-3)
dep(one-3, point-4)
dobj(point-4, you-5)
cop(clump-10, were-6)
det(clump-10, a-8)
amod(clump-10, tiny-9)
root(ROOT-0, clump-10)
