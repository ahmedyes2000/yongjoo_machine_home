(ROOT
  (S ('' '')
    (NP (RB Now))
    (VP (VBP add)
      (NP
        (NP (DT the) (JJ symbolic) (NN essence))
        (PP (IN of)
          (NP
            (NP (DT the) (NN Sacrament))
            (PP (IN of)
              (NP (NNP Baptism)))))))
    (. .)))

nsubj(add-3, Now-2)
root(ROOT-0, add-3)
det(essence-6, the-4)
amod(essence-6, symbolic-5)
dobj(add-3, essence-6)
det(Sacrament-9, the-8)
prep_of(essence-6, Sacrament-9)
prep_of(Sacrament-9, Baptism-11)

