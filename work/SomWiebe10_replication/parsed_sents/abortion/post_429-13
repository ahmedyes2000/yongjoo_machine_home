  (FRAG ('' '')
    (NP (NNP Err))
    (PP (IN on)
      (NP
        (NP
          (NP (DT the) (NN side))
          (PP (IN of)
            (NP (NN justice) (, ,) (NN honor)
              (CC and)
              (NN mercy))))
        (, ,) (RB not) (IN on)
        (NP
          (NP (DT the) (NN side))
          (PP (IN of)
            (NP (JJ political) (NNS correctness))))))
    (. .)))

