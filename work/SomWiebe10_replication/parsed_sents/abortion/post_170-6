(ROOT
  (S
    (S
      (NP (DT That) (NN life))
      (VP (MD may)
        (VP (VB end)
          (PP (IN before)
            (NP (NN birth)))
          (PP (IN through)
            (NP (NN miscarriage)
              (CC or)
              (NN abortion))))))
    (, ,)
    (CC but)
    (S
      (NP (PRP it))
      (VP (VBD was)
        (NP (DT a) (JJ human) (NN life))))
    (. .)))

det(life-2, That-1)
nsubj(end-4, life-2)
aux(end-4, may-3)
root(ROOT-0, end-4)
prep_before(end-4, birth-6)
prep_through(end-4, miscarriage-8)
prep_through(end-4, abortion-10)
conj_or(miscarriage-8, abortion-10)
nsubj(life-17, it-13)
cop(life-17, was-14)
