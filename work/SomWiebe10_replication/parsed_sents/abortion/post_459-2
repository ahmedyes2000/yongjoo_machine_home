(ROOT
  (S
    (NP (PRP They))
    (ADVP (RB simply))
    (VP (VBP want)
      (NP (DT the) (NN option))
      (PP (IN off)
        (NP (DT the) (NN table))))
    (. .)))

nsubj(want-3, They-1)
advmod(want-3, simply-2)
root(ROOT-0, want-3)
det(option-5, the-4)
dobj(want-3, option-5)
det(table-8, the-7)
prep_off(want-3, table-8)

