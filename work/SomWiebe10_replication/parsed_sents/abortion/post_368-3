(ROOT
  (SQ (VBZ Is)
    (NP (NN abortion))
    (NP
      (NP (DT a) (JJ legal) (NN issue))
      (CC or)
      (NP (DT a) (JJ personal) (NN decision)))
    (. ?)))

cop(issue-5, Is-1)
nsubj(issue-5, abortion-2)
det(issue-5, a-3)
amod(issue-5, legal-4)
root(ROOT-0, issue-5)
det(decision-9, a-7)
amod(decision-9, personal-8)
conj_or(issue-5, decision-9)

