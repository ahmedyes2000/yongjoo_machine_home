(ROOT
  (S
    (S
      (NP (DT All) (CD three))
      (VP (VBP have)
        (NP
          (NP (DT the) (JJ same) (NN thing))
          (PP (IN in)
            (NP (JJ common))))))
    (, ,)
    (NP (NNS people))
    (VP (VBD thought)
      (SBAR
        (S
          (NP
            (NP (DT the) (NNS lives))
            (PP (IN of)
              (NP (DT the) (NN innoncent))))
          (VP (VBD were) (RB not)
            (ADJP (JJ worth)
              (S
                (VP (VBG saving))))))))
    (. .)))

det(three-2, All-1)
nsubj(have-3, three-2)
ccomp(thought-11, have-3)
det(thing-6, the-4)
amod(thing-6, same-5)
dobj(have-3, thing-6)
prep_in(thing-6, common-8)
nsubj(thought-11, people-10)
root(ROOT-0, thought-11)
det(lives-13, the-12)
nsubj(worth-19, lives-13)
det(innoncent-16, the-15)
prep_of(lives-13, innoncent-16)
cop(worth-19, were-17)
neg(worth-19, not-18)
ccomp(thought-11, worth-19)
dep(worth-19, saving-20)

