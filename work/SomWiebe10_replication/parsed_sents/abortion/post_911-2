(ROOT
  (S
    (NP (NNS Abortions))
    (VP (VBP are)
      (ADVP (RB very) (RB emotionally))
      (VP (VBG trying)
        (PP (TO to)
          (NP
            (NP (NNS mothers))
            (CC and)
            (NP (PRP$ their) (NNS families))))))
    (. .)))

nsubj(trying-5, Abortions-1)
aux(trying-5, are-2)
advmod(emotionally-4, very-3)
advmod(trying-5, emotionally-4)
root(ROOT-0, trying-5)
prep_to(trying-5, mothers-7)
poss(families-10, their-9)
prep_to(trying-5, families-10)
conj_and(mothers-7, families-10)

