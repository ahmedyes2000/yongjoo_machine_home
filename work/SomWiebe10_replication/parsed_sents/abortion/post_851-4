(ROOT
  (S
    (NP
      (NP (JJ Other) (NNS primates))
      (-LRB- -LRB-)
      (CC and)
      (NP (JJ many) (JJ other) (NNS species))
      (-RRB- -RRB-))
    (VP (VBP have)
      (NP
        (NP (NN sex))
        (PP (IN for)
          (NP
            (NP (DT the) (NN pleasure))
            (ADVP (RB alone))))))
    (. .)))

amod(primates-2, Other-1)
nsubj(have-9, primates-2)
amod(species-7, many-5)
amod(species-7, other-6)
conj_and(primates-2, species-7)
nsubj(have-9, species-7)
root(ROOT-0, have-9)
dobj(have-9, sex-10)
det(pleasure-13, the-12)
prep_for(sex-10, pleasure-13)
advmod(pleasure-13, alone-14)

