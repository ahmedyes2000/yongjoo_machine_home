(ROOT
  (S
    (NP (DT The) (NN time)
      (S
        (VP (TO to)
          (VP (VB stop)
            (NP (DT both))))))
    (VP (VBZ is)
      (ADVP (RB now)))
    (. .)))

det(time-2, The-1)
nsubj(is-6, time-2)
aux(stop-4, to-3)
infmod(time-2, stop-4)
dobj(stop-4, both-5)
root(ROOT-0, is-6)
advmod(is-6, now-7)

