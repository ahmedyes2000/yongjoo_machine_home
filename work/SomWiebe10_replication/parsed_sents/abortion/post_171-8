      (NP (DT a) (NN war))
      (SBAR (IN that)
        (S
          (NP (DT the) (NNP Pope))
          (VP (VBZ has)
            (VP (VBD said)
              (SBAR
                (S
                  (VP (VBZ is)
                    (UCP
                      (ADJP (JJ immoral))
                      (, ,)
                      (ADJP (JJ illegal))
                      (, ,)
                      (CC and)
                      (NP (DT a) (NN threat))))))))))
      (PP (TO to)
        (NP (NN humanity))))
    (. .)))

