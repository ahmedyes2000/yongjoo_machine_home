(ROOT
  (S
    (SBAR (IN Because)
      (S
        (NP
          (NP (JJ sexual) (NN expression))
          (PP (IN in)
            (NP (CD one) (NN way))))
        (VP (VBZ is)
          (PP
            (PP (IN without)
              (NP (NNS problems)))
            (CC and)
            (PP (IN in)
              (NP (DT another)))))))
    (NP (PRP it))
    (VP (VBZ causes)
      (NP (NNS problems)))
    (. .)))

mark(is-7, Because-1)
amod(expression-3, sexual-2)
nsubj(is-7, expression-3)
nsubj(is-7', expression-3)
num(way-6, one-5)
prep_in(expression-3, way-6)
conj_and(is-7, is-7')
advcl(causes-14, is-7)
advcl(causes-14, is-7')
prep_without(is-7, problems-9)
prep_in(is-7', another-12)
nsubj(causes-14, it-13)
root(ROOT-0, causes-14)
dobj(causes-14, problems-15)

