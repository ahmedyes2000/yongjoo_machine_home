(ROOT
  (S
    (NP (PRP we))
    (VP (VBP 're)
      (VP (VBG saying)
        (SBAR (IN that)
          (S
            (NP (DT the) (JJ pregnant) (NN person))
            (VP (MD should)
              (VP (VB have)
                (NP (DT the) (NN option)
                  (S
                    (VP (TO to)
                      (VP (VB abort)))))))))))
    (. .)))

nsubj(saying-3, we-1)
aux(saying-3, 're-2)
root(ROOT-0, saying-3)
complm(have-9, that-4)
det(person-7, the-5)
amod(person-7, pregnant-6)
nsubj(have-9, person-7)
aux(have-9, should-8)
ccomp(saying-3, have-9)
det(option-11, the-10)
dobj(have-9, option-11)
aux(abort-13, to-12)
infmod(option-11, abort-13)

