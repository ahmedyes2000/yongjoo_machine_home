(ROOT
  (S
    (NP (DT That))
    (VP (VBZ is)
      (NP
        (NP (DT the) (JJ main) (NN point))
        (PP (IN of)
          (NP (PRP$ my) (NN argument)))))
    (. .)))

nsubj(point-5, That-1)
cop(point-5, is-2)
det(point-5, the-3)
amod(point-5, main-4)
root(ROOT-0, point-5)
poss(argument-8, my-7)
prep_of(point-5, argument-8)

