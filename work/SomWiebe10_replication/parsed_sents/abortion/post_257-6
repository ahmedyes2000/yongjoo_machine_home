(ROOT
  (S
    (NP (PRP We))
    (VP (VBP have)
      (NP
        (NP (DT a) (NN life))
        (PP (IN of)
          (NP (JJ continuous) (NN development)))))
    (. .)))

nsubj(have-2, We-1)
root(ROOT-0, have-2)
det(life-4, a-3)
dobj(have-2, life-4)
amod(development-7, continuous-6)
prep_of(life-4, development-7)

