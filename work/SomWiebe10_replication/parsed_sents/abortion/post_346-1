(ROOT
  (S
    (ADVP (RB Seriously))
    (: :)
    (NP (FW i))
    (VP (VBP am)
      (PP (IN against)
        (NP
          (NP (DT all) (NN abortion))
          (PRN (-LRB- -LRB-)
            (CC and)
            (S
              (NP (DT this))
              (VP (VBZ is)
                (NP (PRP$ my) (NN opinion))))
            (-RRB- -RRB-))
          (SBAR (IN because)
            (S
              (NP (PRP it))
              (ADVP (RB simply))
              (VP (VBZ terminates)
                (NP
                  (NP (DT the) (NN life))
                  (PP (IN of)
                    (NP
                      (NP (DT a) (JJ human) (NN being) (POS 's))
                      (NN life) (NN cycle))))))))))
    (. .)))

advmod(am-4, Seriously-1)
nsubj(am-4, i-3)
root(ROOT-0, am-4)
det(abortion-7, all-6)
prep_against(am-4, abortion-7)
dep(opinion-13, and-9)
nsubj(opinion-13, this-10)
cop(opinion-13, is-11)
poss(opinion-13, my-12)
dep(abortion-7, opinion-13)
mark(terminates-18, because-15)
