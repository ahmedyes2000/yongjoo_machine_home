(ROOT
  (S
    (NP (EX There))
    (VP (VBP are)
      (ADJP
        (ADJP (JJR more)
          (SBAR
            (S
              (NP (JJ murdersMy) (NN position))
              (VP (VBZ is)
                (SBAR (IN that)
                  (S
                    (NP (NN abortion))
                    (VP
                      (VP (VBZ is)
                        (ADJP (JJ unconscionable)))
                      (CC and)
                      (VP (MD should)
                        (ADVP (RB forever))
                        (VP (VB be)
                          (VP (VBN banned)))))))))))
        (CC and)
        (ADJP (JJ illegal))))
    (. .)))

expl(are-2, There-1)
root(ROOT-0, are-2)
acomp(are-2, more-3)
amod(position-5, murdersMy-4)
nsubj(is-6, position-5)
dep(more-3, is-6)
complm(unconscionable-10, that-7)
nsubj(unconscionable-10, abortion-8)
nsubjpass(banned-15, abortion-8)
cop(unconscionable-10, is-9)
ccomp(is-6, unconscionable-10)
aux(banned-15, should-12)
advmod(banned-15, forever-13)
auxpass(banned-15, be-14)
ccomp(is-6, banned-15)
conj_and(unconscionable-10, banned-15)
acomp(are-2, illegal-17)
conj_and(more-3, illegal-17)

