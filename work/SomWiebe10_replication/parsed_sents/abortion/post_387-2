(ROOT
  (S
    (NP (JJ Many) (NNS things))
    (VP (VBP are)
      (ADJP (JJ alive))
      (, ,)
      (PP (VBG including)
        (NP (NNS insects) (, ,) (NNS weeds)
          (CC and)
          (NN algae))))
    (. .)))

amod(things-2, Many-1)
nsubj(alive-4, things-2)
cop(alive-4, are-3)
root(ROOT-0, alive-4)
prep_including(alive-4, insects-7)
prep_including(alive-4, weeds-9)
conj_and(insects-7, weeds-9)
prep_including(alive-4, algae-11)
conj_and(insects-7, algae-11)

