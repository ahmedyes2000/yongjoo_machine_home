(ROOT
  (S
    (VP (VB Take) (, ,)
      (PP (IN for)
        (NP (NN example)))
      (, ,)
      (FRAG
        (NP (DT the)
          (ADJP (RBS most) (JJ common))
          (NN argument))
        (. .)))))

root(ROOT-0, Take-1)
