(ROOT
  (S
    (S
      (NP (PRP They))
      (VP (VBP 're) (RB not)
        (ADJP (JJ sad)
          (PP (IN about)
            (S
              (VP (VBG being)
                (VP (VBN aborted))))))))
    (, ,)
    (S
      (NP (PRP it))
      (VP (VBZ does) (RB n't)
        (VP (VB hurt)
          (NP (PRP them)))))
    (, ,)
    (S
      (NP (PRP they))
      (VP (VBP do) (RB n't)
        (VP (VB have)
          (NP (DT these) (JJ human) (NNS emotions))
          (ADVP (RB yet)))))
    (. .)))

nsubj(sad-4, They-1)
cop(sad-4, 're-2)
neg(sad-4, not-3)
root(ROOT-0, sad-4)
auxpass(aborted-7, being-6)
prepc_about(sad-4, aborted-7)
