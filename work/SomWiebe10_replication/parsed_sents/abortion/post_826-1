(ROOT
  (S
    (NP (DT The) (NN baby))
    (VP (VBZ ceases)
      (S
        (VP (TO to)
          (VP (VB exsit)
            (SBAR (IN so)
              (S
                (NP (PRP it))
                (VP (VBZ is)
                  (VP (VBN murdered)))))))))
    (. .)))

det(baby-2, The-1)
nsubj(ceases-3, baby-2)
xsubj(exsit-5, baby-2)
root(ROOT-0, ceases-3)
aux(exsit-5, to-4)
xcomp(ceases-3, exsit-5)
mark(murdered-9, so-6)
nsubjpass(murdered-9, it-7)
auxpass(murdered-9, is-8)
advcl(exsit-5, murdered-9)

