(ROOT
  (S
    (NP (DT That))
    (VP (VBZ 's)
      (NP
        (NP (CD 56) (NNS days))
        (, ,)
        (SBAR
          (WHNP (WDT which))
          (S
            (VP (VBZ is)
              (ADJP (RB pretty) (JJ close)
                (PP (TO to)
                  (NP (PRP$ your) (CD 40) (NN day) (NN suggestion)))))))))
    (. .)))

nsubj(days-4, That-1)
cop(days-4, 's-2)
num(days-4, 56-3)
root(ROOT-0, days-4)
nsubj(close-9, days-4)
cop(close-9, is-7)
advmod(close-9, pretty-8)
rcmod(days-4, close-9)
poss(suggestion-14, your-11)
num(suggestion-14, 40-12)
nn(suggestion-14, day-13)
prep_to(close-9, suggestion-14)

