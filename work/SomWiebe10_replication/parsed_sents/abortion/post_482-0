(ROOT
  (S
    (PP (IN In)
      (NP
        (NP (JJS most))
        (PP (IN of)
          (NP (DT the) (NNS polls)))))
    (NP
      (NP (JJ pro) (NN life))
      (CC and)
      (NP (JJ pro) (NN choice)))
    (VP (VBP are)
      (ADJP (RB statistically) (JJ equal)))
    (. .)))

prep_in(equal-13, most-2)
det(polls-5, the-4)
prep_of(most-2, polls-5)
amod(life-7, pro-6)
nsubj(equal-13, life-7)
amod(choice-10, pro-9)
conj_and(life-7, choice-10)
nsubj(equal-13, choice-10)
cop(equal-13, are-11)
