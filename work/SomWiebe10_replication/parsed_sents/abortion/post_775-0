(ROOT
  (S
    (NP (PRP I))
    (VP (VBP love)
      (SBAR
        (WHADVP (WRB how))
        (S
          (NP (PRP you))
          (VP (VBP 're)
            (VP (VBG fighting)
              (NP (DT an) (JJ uphill) (NN battle)))))))
    (. .)))

nsubj(love-2, I-1)
root(ROOT-0, love-2)
advmod(fighting-6, how-3)
nsubj(fighting-6, you-4)
aux(fighting-6, 're-5)
ccomp(love-2, fighting-6)
det(battle-9, an-7)
amod(battle-9, uphill-8)
dobj(fighting-6, battle-9)

