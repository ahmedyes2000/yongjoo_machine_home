(ROOT
  (S
    (S
      (NP (NN Reality))
      (VP (VBZ exists)
        (ADVP (RB independently))
        (PP (IN of)
          (NP (JJ human) (NN opinion)))))
    (, ,)
    (CC and)
    (S
      (NP (NN reason))
      (VP (VBZ is)
        (NP (PRP$ our) (JJ only) (NN tool)
          (S
            (VP (TO to)
              (VP (VB understand)
                (NP (PRP it))))))))
    (. .)))

nsubj(exists-2, Reality-1)
root(ROOT-0, exists-2)
advmod(exists-2, independently-3)
amod(opinion-6, human-5)
prep_of(exists-2, opinion-6)
nsubj(tool-13, reason-9)
cop(tool-13, is-10)
poss(tool-13, our-11)
amod(tool-13, only-12)
conj_and(exists-2, tool-13)
aux(understand-15, to-14)
infmod(tool-13, understand-15)
