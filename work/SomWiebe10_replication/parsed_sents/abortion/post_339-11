(ROOT
  (S
    (PP (IN During)
      (NP (DT the) (JJ first) (NN trimester)))
    (NP (NN abortion))
    (VP (MD should)
      (VP (VB be)
        (VP
          (ADVP (RB universally))
          (VBN allowed))))
    (. .)))

det(trimester-4, the-2)
amod(trimester-4, first-3)
prep_during(allowed-9, trimester-4)
nsubjpass(allowed-9, abortion-5)
aux(allowed-9, should-6)
auxpass(allowed-9, be-7)
