(ROOT
  (S
    (NP (NNP God))
    (VP (VBD wrote)
      (NP (DT the) (NNP Bible))
      (PP (IN by)
        (S
          (VP (VBG manipulating)
            (NP
              (NP (DT the) (NNS hands))
              (PP (IN of)
                (NP (DT the) (NNS authors))))))))
    (. .)))

nsubj(wrote-2, God-1)
root(ROOT-0, wrote-2)
det(Bible-4, the-3)
