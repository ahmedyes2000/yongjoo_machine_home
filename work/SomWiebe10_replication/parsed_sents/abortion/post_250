(ROOT
  (S
    (NP
      (NP (JJ Negative) (NNS feelings))
      (PP (IN about)
        (NP
          (NP (DT the)
            (ADJP (RB metaphysically) (JJ independent))
            (NN status))
          (PP (IN of)
            (NP (NNS women))))))
    (VP (MD should)
      (VP (VB have)
        (NP
          (NP (DT no) (NN bearing))
          (PP (IN on)
            (NP (DT the) (NN discussion))))))
    (. .)))

