(ROOT
  (S
    (NP (DT A) (NN dog))
    (VP (VBZ is) (RB not)
      (NP (DT a) (NN person)))
    (. .)))

det(dog-2, A-1)
nsubj(person-6, dog-2)
cop(person-6, is-3)
neg(person-6, not-4)
det(person-6, a-5)
root(ROOT-0, person-6)

(ROOT
  (S
    (ADVP (RB Therefore))
    (, ,)
    (NP (PRP it))
    (VP (VBZ does) (RB not)
      (VP (VB have)
        (NP (NNS rights))))
    (. .)))

advmod(have-6, Therefore-1)
nsubj(have-6, it-3)
aux(have-6, does-4)
neg(have-6, not-5)
root(ROOT-0, have-6)
dobj(have-6, rights-7)

(ROOT
  (S
    (NP
      (NP (JJ Positive) (NNS feelings))
      (PP (IN about)
        (NP (NNS dogs))))
    (VP (MD should)
      (VP (VB have)
        (NP
          (NP (DT no) (NN bearing))
          (PP (IN on)
            (NP (DT the) (NN discussion))))))
    (. .)))

amod(feelings-2, Positive-1)
nsubj(have-6, feelings-2)
prep_about(feelings-2, dogs-4)
aux(have-6, should-5)
root(ROOT-0, have-6)
det(bearing-8, no-7)
dobj(have-6, bearing-8)
det(discussion-11, the-10)
prep_on(bearing-8, discussion-11)

