(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (DT the) (NN baby))
        (VP (VBZ dies))))
    (, ,)
    (NP (PRP it))
    (VP (VBZ is)
      (SBAR (RB only) (IN because)
        (S
          (NP (PRP it))
          (VP (VBD was) (RB not)
            (ADJP (JJ able)
              (S
                (VP (TO to)
                  (VP (VB survive)))))))))
    (. .)))

mark(dies-4, If-1)
det(baby-3, the-2)
nsubj(dies-4, baby-3)
advcl(is-7, dies-4)
nsubj(is-7, it-6)
root(ROOT-0, is-7)
