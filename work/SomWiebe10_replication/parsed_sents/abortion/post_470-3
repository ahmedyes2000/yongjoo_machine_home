(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ dances)
      (SBAR
        (WHADVP (WRB when))
        (S
          (NP (DT the) (NN music))
          (VP (VBZ plays)))))
    (. .)))

nsubj(dances-2, It-1)
root(ROOT-0, dances-2)
advmod(plays-6, when-3)
det(music-5, the-4)
nsubj(plays-6, music-5)
advcl(dances-2, plays-6)

