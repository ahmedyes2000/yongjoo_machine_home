(ROOT
  (S
    (ADVP (RB Yes))
    (NP (PRP I))
    (VP (VBP understand)
      (SBAR
        (S
          (NP (PRP you))
          (VP (VBP are)
            (NP
              (NP (DT a) (NN firm) (NN proponent))
              (PP (IN of)
                (NP (DT the) (NN death) (NN penalty))))))))
    (. ...)))

advmod(understand-3, Yes-1)
nsubj(understand-3, I-2)
root(ROOT-0, understand-3)
nsubj(proponent-8, you-4)
cop(proponent-8, are-5)
det(proponent-8, a-6)
nn(proponent-8, firm-7)
ccomp(understand-3, proponent-8)
det(penalty-12, the-10)
nn(penalty-12, death-11)
prep_of(proponent-8, penalty-12)

