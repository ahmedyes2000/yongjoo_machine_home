(ROOT
  (S
    (NP
      (NP (PRP$ Our) (NNS laws))
      (, ,)
      (PP (IN without)
        (NP
          (NP (DT the) (NN belief))
          (PP (IN in)
            (NP (NNP God)))
          (S
            (VP (TO to)
              (VP (VB back)
                (NP (PRP them))
                (PRT (RP up)))))))
      (, ,))
    (VP (VBP do) (RB n't)
      (VP (VB motivate)
        (NP (JJ many) (NNS people))
        (S
          (VP (TO to)
            (VP (VB follow)
              (NP (PRP them)))))))
    (. .)))

poss(laws-2, Our-1)
nsubj(motivate-16, laws-2)
det(belief-6, the-5)
prep_without(laws-2, belief-6)
prep_in(belief-6, God-8)
aux(back-10, to-9)
