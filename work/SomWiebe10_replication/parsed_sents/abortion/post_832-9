(ROOT
  (S
    (NP (DT That))
    (VP (VBZ 's)
      (NP
        (NP (PDT all) (DT this) (NN decision))
        (SBAR
          (S
            (VP (VBZ is) (, ,)
              (NP
                (NP (DT a) (NN balance))
                (PP (IN of)
                  (NP (NNS rights)))))))))
    (. .)))

nsubj(decision-5, That-1)
cop(decision-5, 's-2)
predet(decision-5, all-3)
det(decision-5, this-4)
root(ROOT-0, decision-5)
cop(balance-9, is-6)
det(balance-9, a-8)
rcmod(decision-5, balance-9)
prep_of(balance-9, rights-11)

