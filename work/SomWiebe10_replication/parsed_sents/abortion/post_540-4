(ROOT
  (S
    (NP (PRP I))
    (VP (MD could) (RB not)
      (VP
        (VP (VB sit)
          (PP (IN at)
            (NP
              (NP (NN home))
              (PP (IN in)
                (NP (DT a) (NN bubble))))))
        (CC and)
        (VP (VB be)
          (VP (VBN depressed)))))
    (. .)))

nsubj(sit-4, I-1)
nsubjpass(depressed-12, I-1)
aux(sit-4, could-2)
neg(sit-4, not-3)
root(ROOT-0, sit-4)
prep_at(sit-4, home-6)
det(bubble-9, a-8)
prep_in(home-6, bubble-9)
