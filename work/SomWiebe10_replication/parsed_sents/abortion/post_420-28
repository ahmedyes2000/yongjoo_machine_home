(ROOT
  (S
    (PP (IN By)
      (NP (DT the) (NN way)))
    (, ,)
    (NP (PRP I))
    (VP
      (VP (VBP refer)
        (PP (TO to)
          (NP (PRP$ my) (NNS posts))))
      (, ,) (RB not)
      (VP (VBD Recommend)
        (NP (PRP them))))
    (. .)))

det(way-3, the-2)
prep_by(refer-6, way-3)
nsubj(refer-6, I-5)
root(ROOT-0, refer-6)
poss(posts-9, my-8)
prep_to(refer-6, posts-9)
neg(refer-6, not-11)
dep(refer-6, Recommend-12)
dobj(Recommend-12, them-13)

