(ROOT
  (S
    (NP (PRP I))
    (VP (VBP 'm)
      (VP (VBG trying)
        (S
          (VP (TO to)
            (VP (VB understand)
              (NP
                (NP (DT the) (NN value))
                (PP (IN of)
                  (NP (NN life))))
              (PP (IN from)
                (NP (DT a) (JJ Catholic) (NN perspective))))))))
    (. .)))

nsubj(trying-3, I-1)
xsubj(understand-5, I-1)
aux(trying-3, 'm-2)
root(ROOT-0, trying-3)
aux(understand-5, to-4)
xcomp(trying-3, understand-5)
det(value-7, the-6)
dobj(understand-5, value-7)
prep_of(value-7, life-9)
det(perspective-13, a-11)
amod(perspective-13, Catholic-12)
prep_from(understand-5, perspective-13)

