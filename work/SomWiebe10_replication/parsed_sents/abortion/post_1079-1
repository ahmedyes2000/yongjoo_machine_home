(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is)
      (NP
        (NP (JJ natures) (NN way))
        (PP (IN of)
          (S
            (VP (VBG preparing)
              (NP
                (NP (DT a) (NN woman))
                (PP (IN for)
                  (NP (NN motherhood)))))))))
    (. .)))

nsubj(way-4, It-1)
cop(way-4, is-2)
amod(way-4, natures-3)
root(ROOT-0, way-4)
prepc_of(way-4, preparing-6)
det(woman-8, a-7)
dobj(preparing-6, woman-8)
prep_for(woman-8, motherhood-10)

