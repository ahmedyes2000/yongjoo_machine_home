(ROOT
  (S
    (NP (NNS People))
    (VP (VBP suffer)
      (ADJP (JJ physically)
        (CC and)
        (RB psychologically))
      (PP (IN after)
        (S
          (VP (VBG giving)
            (NP (NN birth))))))
    (. .)))

nsubj(suffer-2, People-1)
root(ROOT-0, suffer-2)
acomp(suffer-2, physically-3)
acomp(suffer-2, psychologically-5)
advmod(physically-3, psychologically-5)
conj_and(physically-3, psychologically-5)
prepc_after(suffer-2, giving-7)
dobj(giving-7, birth-8)

