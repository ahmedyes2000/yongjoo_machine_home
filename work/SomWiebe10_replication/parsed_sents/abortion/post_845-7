(ROOT
  (S
    (NP (PRP You))
    (VP (VBP 're)
      (ADVP (RB basically))
      (VP (VBG calling)
        (S
          (NP (PRP it))
          (NP (NNP Government) (NNP Property)))))
    (. .)))

nsubj(calling-4, You-1)
aux(calling-4, 're-2)
advmod(calling-4, basically-3)
root(ROOT-0, calling-4)
nsubj(Property-7, it-5)
nn(Property-7, Government-6)
xcomp(calling-4, Property-7)

