(ROOT
  (S
    (NP (PRP$ Your) (NN second))
    (, ,)
    (S
      (VP (VBG regarding)
        (NP (JJ gay) (NN marriage))))
    (, ,)
    (VP (VBZ is) (RB not))
    (. .)))

poss(second-2, Your-1)
nsubj(is-8, second-2)
partmod(is-8, regarding-4)
amod(marriage-6, gay-5)
dobj(regarding-4, marriage-6)
root(ROOT-0, is-8)
neg(is-8, not-9)

