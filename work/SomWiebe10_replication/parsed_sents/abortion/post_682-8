(ROOT
  (S
    (NP
      (NP (DT The) (NNS parts))
      (PP (IN of)
        (NP (PRP us)))
      (SBAR
        (WHNP (WDT that))
        (S
          (VP (VBP are)
            (ADJP (JJ new)
              (CC and)
              (JJ unique))))))
    (VP (VBP appear)
      (PP (IN at)
        (NP
          (NP (JJ several) (JJ different) (NNS places))
          (PP (IN in)
            (NP (DT the) (NN cycle))))))
    (. .)))

det(parts-2, The-1)
nsubj(new-7, parts-2)
nsubj(unique-9, parts-2)
nsubj(appear-10, parts-2)
prep_of(parts-2, us-4)
cop(new-7, are-6)
rcmod(parts-2, new-7)
rcmod(parts-2, unique-9)
conj_and(new-7, unique-9)
root(ROOT-0, appear-10)
amod(places-14, several-12)
amod(places-14, different-13)
prep_at(appear-10, places-14)
det(cycle-17, the-16)
