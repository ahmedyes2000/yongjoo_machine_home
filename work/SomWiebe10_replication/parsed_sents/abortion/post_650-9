(ROOT
  (S
    (PP (IN For)
      (NP (NN example)))
    (, ,)
    (NP (PRP we))
    (VP (MD could)
      (VP (VB go)
        (PP (TO to)
          (NP (DT the) (NNS neurologists)))))
    (. .)))

