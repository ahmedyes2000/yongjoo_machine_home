    (, ,)
    (NP
      (NP (WP what))
      (PP (IN about)
        (NP
          (NP (DT the) (NNS victims))
          (PP (IN of)
            (NP (NNS accidents)))))
      (SBAR
        (WHNP (WP who))
        (S
          (VP (VBP fall)
            (ADJP (JJ unconscious)))))
      (: :))
    (VP (VBP do)
      (SBAR
        (S
          (NP (PRP they))
          (ADVP (RB suddenly))
