(ROOT
  (S
    (S
      (NP (PRP You))
      (VP (VBP are)
        (ADJP (JJ correct)
          (PP (IN about)
            (NP (DT the) (JJ double) (NN homicide) (NN law))))))
    (: ;)
    (S
      (ADVP (RB however))
      (NP (DT the) (NN contradiction))
      (VP (VBZ is)
        (ADJP (JJ inherent)
          (PP (IN in)
            (NP
              (NP (DT the) (NN ruling))
              (ADVP (PRP itself)))))))
    (. .)))

nsubj(correct-3, You-1)
cop(correct-3, are-2)
root(ROOT-0, correct-3)
det(law-8, the-5)
amod(law-8, double-6)
nn(law-8, homicide-7)
prep_about(correct-3, law-8)
advmod(inherent-14, however-10)
det(contradiction-12, the-11)
nsubj(inherent-14, contradiction-12)
cop(inherent-14, is-13)
