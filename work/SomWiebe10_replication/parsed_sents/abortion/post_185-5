(ROOT
  (S
    (PP (IN Throughout)
      (NP (DT the) (NNS ages)))
    (NP (NN mankind))
    (VP (VBZ has)
      (VP (VBN fought)
        (PP (IN against)
          (ADJP (JJ good)
            (CC and)
            (JJ evil)))))
    (. .)))

det(ages-3, the-2)
prep_throughout(fought-6, ages-3)
nsubj(fought-6, mankind-4)
aux(fought-6, has-5)
root(ROOT-0, fought-6)
prep_against(fought-6, good-8)
prep_against(fought-6, evil-10)
conj_and(good-8, evil-10)

