(ROOT
  (S
    (NP (DT The) (NNPS Teachings))
    (VP (VBP have) (RB not)
      (VP (VBN changed) (, ,)
        (SBAR (IN though)
          (S
            (NP (DT the) (NNS people))
            (VP (VBP have))))))
    (. .)))

det(Teachings-2, The-1)
nsubj(changed-5, Teachings-2)
aux(changed-5, have-3)
neg(changed-5, not-4)
root(ROOT-0, changed-5)
mark(have-10, though-7)
det(people-9, the-8)
nsubj(have-10, people-9)
advcl(changed-5, have-10)

