  (S
    (NP (EX There))
    (VP (VBP are)
      (NP
        (NP (NNS thousands))
        (PP (IN of)
          (NP
            (NP (NNS people))
            (SBAR
              (WHNP (WP who))
              (S
                (VP (VBP want)
                  (S
                    (VP (TO to)
                      (VP (VB take)
                        (NP (DT these) (NNS children))
                        (SBAR (IN because)
                          (S
                            (NP (PRP they))
                            (VP (MD can) (RB not)
                              (VP (VB have)
                                (NP (PRP$ their) (JJ own))))))))))))))))
    (. .)))

expl(are-2, There-1)
root(ROOT-0, are-2)
nsubj(are-2, thousands-3)
prep_of(thousands-3, people-5)
