(ROOT
  (S
    (NP
      (NP (DT No) (NN need))
      (PP (IN for)
        (NP (NN anyone)
          (S
            (VP (TO to)
              (VP (VB worry)
                (NP (RB about) (DT the) (NNS others) ('' ''))))))))
    (VP (VBZ morals)
      (SBAR (IN because)
        (S
          (NP (DT the) (NN decision))
          (VP (VBZ is)
            (NP (DT an) (JJ individual) (NN decision))))))
    (. .)))

