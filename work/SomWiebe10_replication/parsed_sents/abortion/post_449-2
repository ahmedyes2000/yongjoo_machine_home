  (S
    (NP (PRP I))
    (ADVP (RB also))
    (VP (VBD had)
      (NP
        (NP (CD 1) (NN abortion))
        (FRAG
          (SBAR (IN after)
            (S
              (S
                (NP (PRP$ my) (NN husband))
                (VP (VBD had)
                  (NP (DT a) (NN vasectomy))))
              (CC and)
              (S
                (NP (PRP I))
                (ADVP (RB already))
                (VP (VBD had)
                  (NP (CD 6) (NNS children))))))
          (. .))))
    (. .)))

