(ROOT
  (S
    (NP (NNP Modern))
    (ADVP (RB embryology))
    (ADVP (RB now))
    (VP (VBZ confirms)
      (SBAR
        (WHNP (WP what))
        (S
          (NP (DT the) (NNP Church))
          (VP (VBD knew)
            (SBAR
              (S
                (NP
                  (NP (DT all))
                  (PP (IN along)
                    (NP (NN life))))
                (VP (VBZ begins)
                  (PP (IN at)
                    (NP (NN conception))))))))))
    (. .)))

nsubj(confirms-4, Modern-1)
advmod(confirms-4, embryology-2)
advmod(confirms-4, now-3)
root(ROOT-0, confirms-4)
