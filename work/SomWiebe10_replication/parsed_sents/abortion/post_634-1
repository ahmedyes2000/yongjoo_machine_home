(ROOT
  (S
    (PP (IN By)
      (S
        (VP (VBG declaring)
          (NP (DT that)))))
    (, ,) (`` ``)
    (NP (PRP$ their) (NNS beliefs))
    (VP (VBP are)
      (NP (NNS facts) ('' '')))
    (. .)))

prepc_by(facts-9, declaring-2)
dobj(declaring-2, that-3)
poss(beliefs-7, their-6)
nsubj(facts-9, beliefs-7)
cop(facts-9, are-8)
root(ROOT-0, facts-9)

