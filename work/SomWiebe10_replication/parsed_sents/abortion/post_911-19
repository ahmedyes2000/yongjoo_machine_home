(ROOT
  (S
    (PP (IN In)
      (NP (DT these) (NNS situations)))
    (NP
      (NP (DT the)
        (ADJP (RBS most) (JJ humane))
        (NN thing))
      (SBAR
        (S
          (VP (TO to)
            (VP (VB do))))))
    (VP (VBZ is)
      (S
        (VP (TO to)
          (VP
            (VP (VB terminate)
              (NP (DT the) (NN pregnancy)))
            (CC and)
            (VP (VB treat)
              (NP (DT the) (NN mother)))))))
    (. .)))

det(situations-3, these-2)
prep_in(is-10, situations-3)
det(thing-7, the-4)
advmod(humane-6, most-5)
amod(thing-7, humane-6)
nsubj(is-10, thing-7)
xsubj(terminate-12, thing-7)
aux(do-9, to-8)
infmod(thing-7, do-9)
root(ROOT-0, is-10)
aux(terminate-12, to-11)
xcomp(is-10, terminate-12)
det(pregnancy-14, the-13)
dobj(terminate-12, pregnancy-14)
xcomp(is-10, treat-16)
conj_and(terminate-12, treat-16)
det(mother-18, the-17)
dobj(treat-16, mother-18)

