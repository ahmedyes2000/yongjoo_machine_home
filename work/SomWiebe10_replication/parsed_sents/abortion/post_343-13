(ROOT
  (S
    (NP (PRP They))
    (VP
      (VP (VBP are) (DT both)
        (ADJP (JJ immoral)))
      (CC and)
      (VP
        (ADVP (RB thus))
        (JJ equal)))
    (. .)))

nsubj(immoral-4, They-1)
nsubj(equal-7, They-1)
cop(immoral-4, are-2)
dep(immoral-4, both-3)
root(ROOT-0, immoral-4)
advmod(equal-7, thus-6)
conj_and(immoral-4, equal-7)

