(ROOT
  (S ('' '')
    (NP (DT Some))
    (VP (VBZ define)
      (ADJP (JJ personhood))
      (PRN (-LRB- -LRB-)
        (S
          (VP (VBG qualifying)
            (PP (IN for)
              (NP (NNS rights)))))
        (-RRB- -RRB-))
      (PP (IN through)
        (NP
          (NP (DT a) (NN set))
          (PP (IN of)
            (NP (NNS criteria))))))
    (. .)))

nsubj(define-3, Some-2)
root(ROOT-0, define-3)
acomp(define-3, personhood-4)
parataxis(define-3, qualifying-6)
prep_for(qualifying-6, rights-8)
det(set-12, a-11)
prep_through(define-3, set-12)
prep_of(set-12, criteria-14)

