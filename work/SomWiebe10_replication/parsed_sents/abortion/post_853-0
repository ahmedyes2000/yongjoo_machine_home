(ROOT
  (S
    (NP
      (NP (DT The) (JJ frightening) (NN thing))
      (PP (IN about)
        (NP
          (NP (DT the) (NN abortion) (NN debate))
          (PP (IN in)
            (NP (DT this) (NN country))))))
    (VP (VBZ is)
      (SBAR (IN that)
        (S
          (NP (RB not) (JJ enough) (NN attention))
          (VP (VBZ is)
            (VP (VBN brought)
              (PP (TO to)
                (NP (DT the) (NN fact)))
              (SBAR (IN that)
                (S
                  (NP (JJ white) (NNS men))
                  (ADVP (RB often))
                  (VP (VBP have)
                    (NP
                      (NP (DT the) (JJS strongest) (NN opinion))
                      (PP (IN on)
                        (NP (DT the) (NN matter))))))))))))
    (. .)))

det(thing-3, The-1)
amod(thing-3, frightening-2)
nsubj(is-11, thing-3)
det(debate-7, the-5)
nn(debate-7, abortion-6)
