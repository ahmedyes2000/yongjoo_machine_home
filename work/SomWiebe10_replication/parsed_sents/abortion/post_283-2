(ROOT
  (S
    (NP (PRP It))
    (VP (MD can)
      (ADVP (RB clearly))
      (VP (VB be)
        (VP (VBN identified)
          (PP (IN as)
            (NP (JJ such))))))
    (. .)))

nsubjpass(identified-5, It-1)
aux(identified-5, can-2)
advmod(identified-5, clearly-3)
auxpass(identified-5, be-4)
root(ROOT-0, identified-5)
prep_such_as(identified-5, such-7)

