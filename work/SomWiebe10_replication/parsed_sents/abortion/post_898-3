(ROOT
  (S
    (NP (PRP I))
    (VP (VBP think)
      (SBAR (IN that)
        (S
          (PP (IN for)
            (NP
              (NP (DT those))
              (SBAR
                (WHNP (WP who))
                (S
                  (VP (VBP consider)
                    (NP (DT an) (NN abortion)))))))
          (, ,)
          (NP (PRP they))
          (VP (MD should) (RB n't)
            (VP (VB have)
              (VP (VBN opened)
                (NP (PRP$ their) (NNS legs))
                (PP (IN in)
                  (NP (DT the) (JJ first) (NN place)))
                (, ,)
                (SBAR (IN whether)
                  (S
                    (NP (DT the) (NN guy))
                    (VP (VBD was)
                      (VP (VBG wearing)
                        (NP (DT a) (NN glove)))))
                  (CC or)
                  (RB not))))))))
    (. .)))

nsubj(think-2, I-1)
root(ROOT-0, think-2)
complm(opened-15, that-3)
nsubj(consider-7, those-5)
prep_for(opened-15, those-5)
rcmod(those-5, consider-7)
det(abortion-9, an-8)
dobj(consider-7, abortion-9)
nsubj(opened-15, they-11)
aux(opened-15, should-12)
neg(opened-15, n't-13)
aux(opened-15, have-14)
ccomp(think-2, opened-15)
poss(legs-17, their-16)
dobj(opened-15, legs-17)
det(place-21, the-19)
amod(place-21, first-20)
prep_in(opened-15, place-21)
complm(wearing-27, whether-23)
det(guy-25, the-24)
nsubj(wearing-27, guy-25)
aux(wearing-27, was-26)
ccomp(opened-15, wearing-27)
det(glove-29, a-28)
dobj(wearing-27, glove-29)
ccomp(opened-15, not-31)
conj_or(wearing-27, not-31)

