(ROOT
  (S
    (ADVP (RB Just))
    (SBAR (IN because)
      (S
        (NP (DT the) (NN law))
        (VP (VBZ says)
          (NP (PRP it))
          (ADVP (RB not)))))
    (, ,)
    (VP (VBZ does) (RB not)
      (VP (VB make)
        (NP (DT that) (NN right))))
    (. .)))

advmod(make-11, Just-1)
mark(says-5, because-2)
det(law-4, the-3)
nsubj(says-5, law-4)
dep(make-11, says-5)
dobj(says-5, it-6)
neg(says-5, not-7)
aux(make-11, does-9)
neg(make-11, not-10)
root(ROOT-0, make-11)
det(right-13, that-12)
dobj(make-11, right-13)

