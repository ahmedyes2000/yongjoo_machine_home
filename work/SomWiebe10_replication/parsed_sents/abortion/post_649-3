(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is) (RB not)
      (VP (VBG going)
        (S
          (VP (TO to)
            (VP (VB develop)
              (PP (IN into)
                (NP
                  (NP (DT a) (NN puppy))
                  (, ,)
                  (CC or)
                  (NP (DT a) (NN pony)))))))))
    (. .)))

nsubj(going-4, It-1)
xsubj(develop-6, It-1)
aux(going-4, is-2)
neg(going-4, not-3)
root(ROOT-0, going-4)
aux(develop-6, to-5)
xcomp(going-4, develop-6)
det(puppy-9, a-8)
prep_into(develop-6, puppy-9)
det(pony-13, a-12)
prep_into(develop-6, pony-13)
conj_or(puppy-9, pony-13)

