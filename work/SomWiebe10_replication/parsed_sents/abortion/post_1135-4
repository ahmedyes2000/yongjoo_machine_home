(ROOT
  (S
    (NP (PRP They))
    (VP (MD can)
      (VP (VB put)
        (NP (DT the) (NN child))
        (PRT (RP up))
        (PP (IN for)
          (NP (NN adoption)))))
    (. .)))

nsubj(put-3, They-1)
aux(put-3, can-2)
root(ROOT-0, put-3)
det(child-5, the-4)
dobj(put-3, child-5)
prt(put-3, up-6)
prep_for(put-3, adoption-8)

