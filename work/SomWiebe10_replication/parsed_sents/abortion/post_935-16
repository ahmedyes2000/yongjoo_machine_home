(ROOT
  (S
    (S
      (NP (PRP I))
      (VP (VBP find)
        (NP
          (NP (DT this) (JJ entire) (NN post))
          (ADJP (RB incredibly) (JJ offensive)))))
    (, ,)
    (CC and)
    (S
      (NP (PRP you))
      (VP (VBP get)
        (NP (DT a) (JJ rare) (NN downvote))
        (PP (IN from)
          (NP (PRP me)))))
    (. .)))

nsubj(find-2, I-1)
root(ROOT-0, find-2)
det(post-5, this-3)
amod(post-5, entire-4)
dobj(find-2, post-5)
advmod(offensive-7, incredibly-6)
amod(post-5, offensive-7)
nsubj(get-11, you-10)
conj_and(find-2, get-11)
det(downvote-14, a-12)
amod(downvote-14, rare-13)
dobj(get-11, downvote-14)
prep_from(get-11, me-16)

