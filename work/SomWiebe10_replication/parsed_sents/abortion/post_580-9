(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is) (RB not)
      (VP (VBG going)
        (S
          (VP (TO to)
            (VP (VB be)
              (NP (DT a) (NN pony)))))))
    (. .)))

nsubj(going-4, It-1)
xsubj(pony-8, It-1)
aux(going-4, is-2)
neg(going-4, not-3)
root(ROOT-0, going-4)
aux(pony-8, to-5)
cop(pony-8, be-6)
det(pony-8, a-7)
xcomp(going-4, pony-8)

