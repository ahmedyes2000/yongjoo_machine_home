(ROOT
  (NP
    (NP (JJ Homeless) (NNS women))
    (: :)
    (S
      (NP (NNP Women))
      (VP (MD may)
        (VP (VB seek)
          (S
            (VP (TO to)
              (VP (VB abort)
                (S
                  (VP (VBG having)
                    (NP
                      (NP (DT the) (NN child))
                      (PP (IN of)
                        (NP (DT a) (NN batterer))))))))))))
    (. .)))

amod(women-2, Homeless-1)
root(ROOT-0, women-2)
nsubj(seek-6, Women-4)
xsubj(abort-8, Women-4)
aux(seek-6, may-5)
dep(women-2, seek-6)
aux(abort-8, to-7)
xcomp(seek-6, abort-8)
xcomp(abort-8, having-9)
det(child-11, the-10)
dobj(having-9, child-11)
det(batterer-14, a-13)
prep_of(child-11, batterer-14)

