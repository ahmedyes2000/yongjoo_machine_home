(ROOT
  (S
    (PP (IN As)
      (NP (DT a) (JJ pro-life) (NN atheist)))
    (, ,)
    (NP (PRP I))
    (VP (VBP have)
      (NP
        (NP (DT no) (NN patience))
        (PP (IN for)
          (NP (JJ such) (NNS tactics)))))
    (. .)))

det(atheist-4, a-2)
amod(atheist-4, pro-life-3)
prep_as(have-7, atheist-4)
nsubj(have-7, I-6)
root(ROOT-0, have-7)
det(patience-9, no-8)
dobj(have-7, patience-9)
amod(tactics-12, such-11)
prep_for(patience-9, tactics-12)

