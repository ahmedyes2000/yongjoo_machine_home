          (WHNP (WDT that))
          (S
            (VP (VBP want)
              (S
                (VP (TO to)
                  (VP (VB adopt)
                    (PP (IN of)
                      (NP (DT a) (JJ potential) (NN child)))))))))))
    (. .)))

