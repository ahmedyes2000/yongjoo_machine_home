  (S
    (NP (PRP We))
    (VP (MD can) (RB not)
      (VP (VB govern)
        (NP
          (NP (PRP$ your) (NN kidney))
          (CC or)
          (NP (PRP$ my) (NN bone) (NN marrow)))
        (PP (VBN based)
          (PP (IN on)
            (NP
              (NP (JJ emotional) (NNS appeals))
              (SBAR
                (WHNP (WDT that))
                (S
                  (VP (VBG doing)
                    (SBAR (IN so)
                      (S
                        (VP (MD will)
                          (VP (VB save)
                            (NP (NNS lives))))))))))))))
    (. .)))

