(ROOT
  (S
    (NP
      (NP (DT The) (NN purpose))
      (PP (IN of)
        (NP (DT this) (NN argument))))
    (VP (VBZ is)
      (S
        (VP (TO to)
          (VP (VB discuss)
            (SBAR (IN whether)
              (S
                (NP (DT a) (NNP Catholic))
                (VP (MD can)
                  (VP (VB vote)
                    (PP (IN for)
                      (NP (DT a) (JJ pro-choice) (NN candidate)))))))))))
    (. .)))

det(purpose-2, The-1)
nsubj(is-6, purpose-2)
xsubj(discuss-8, purpose-2)
det(argument-5, this-4)
prep_of(purpose-2, argument-5)
root(ROOT-0, is-6)
aux(discuss-8, to-7)
