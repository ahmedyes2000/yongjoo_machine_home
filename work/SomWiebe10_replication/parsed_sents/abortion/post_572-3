(ROOT
  (S
    (ADVP (RB However))
    (NP (JJ meaningful) (NN life))
    (VP (VBZ begins)
      (SBAR
        (WHADVP (WRB when))
        (S
          (NP (NNS people))
          (VP (VBP give)
            (NP (PRP it))
            (NP (NN value))))))
    (. .)))

advmod(begins-4, However-1)
amod(life-3, meaningful-2)
nsubj(begins-4, life-3)
root(ROOT-0, begins-4)
advmod(give-7, when-5)
nsubj(give-7, people-6)
advcl(begins-4, give-7)
iobj(give-7, it-8)
dobj(give-7, value-9)

