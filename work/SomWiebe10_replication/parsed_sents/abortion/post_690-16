(ROOT
  (S
    (NP (DT That))
    (VP (VBZ 's)
      (SBAR
        (WHADVP (WRB why))
        (S
          (S
            (VP (VBG killing)
              (NP (DT a) (JJ brainless) (JJ single) (NN cell))))
          (VP (VBZ is)
            (ADJP (JJ repulsive)
              (PP (TO to)
                (NP (PRP them))))))))
    (. .)))

nsubj('s-2, That-1)
root(ROOT-0, 's-2)
advmod(repulsive-10, why-3)
csubj(repulsive-10, killing-4)
det(cell-8, a-5)
amod(cell-8, brainless-6)
amod(cell-8, single-7)
dobj(killing-4, cell-8)
cop(repulsive-10, is-9)
advcl('s-2, repulsive-10)
prep_to(repulsive-10, them-12)

