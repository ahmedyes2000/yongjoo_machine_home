(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (DT the) (NN abortio))
        (VP (VBZ is) (RB not)
          (NP
            (NP (DT a) (JJ safe) (NN mode))
            (VP (VBN executed))))))
    (, ,)
    (ADVP (RB then))
    (NP (PRP it))
    (VP (MD can)
      (VP (VB affect)
        (NP
          (NP (DT the) (NN parent) (POS 's))
          (NN health))
        (ADVP (RB too))))
    (. .)))

mark(mode-8, If-1)
det(abortio-3, the-2)
nsubj(mode-8, abortio-3)
cop(mode-8, is-4)
neg(mode-8, not-5)
det(mode-8, a-6)
amod(mode-8, safe-7)
advcl(affect-14, mode-8)
partmod(mode-8, executed-9)
advmod(affect-14, then-11)
nsubj(affect-14, it-12)
aux(affect-14, can-13)
root(ROOT-0, affect-14)
det(parent-16, the-15)
poss(health-18, parent-16)
dobj(affect-14, health-18)
advmod(affect-14, too-19)

