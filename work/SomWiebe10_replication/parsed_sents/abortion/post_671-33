(ROOT
  (S
    (S
      (NP (DT The) (NNP Bible))
      (VP (VBZ says)
        (ADVP (RB so))))
    (, ,)
    (CC but)
    (S
      (NP (PRP it))
      (VP (VBD was)
        (VP (VBN written)
          (PP (IN by)
            (NP
              (NP
                (NP (NNS people))
                (SBAR
                  (WHNP (WP who))
                  (S
                    (VP (VBD thought)
                      (SBAR
                        (S
                          (NP (DT the) (NN earth))
                          (VP (VBD was)
                            (ADJP (JJ flat)))))))))
              (CC and)
              (NP
                (NP (DT the) (NN center))
                (PP (IN of)
                  (NP (DT the) (NN universe)))))))))
    (. .)))

det(Bible-2, The-1)
nsubj(says-3, Bible-2)
root(ROOT-0, says-3)
advmod(says-3, so-4)
