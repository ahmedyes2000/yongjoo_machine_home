(ROOT
  (S
    (NP (DT The) (NN thing))
    (VP (VBZ has)
      (VP (VBN been)
        (VP (VBG growning)
          (PP (IN by)
            (NP (CD 40) (NN mill)))
          (NP (DT every) (NN year))
          (SBAR (IN since)
            (S
              (NP (PRP it))
              (VP (VBZ has)
                (VP (VBN been)
                  (VP (VBN gagged)))))))))
    (. .)))

det(thing-2, The-1)
nsubj(growning-5, thing-2)
aux(growning-5, has-3)
aux(growning-5, been-4)
root(ROOT-0, growning-5)
num(mill-8, 40-7)
prep_by(growning-5, mill-8)
det(year-10, every-9)
tmod(growning-5, year-10)
mark(gagged-15, since-11)
nsubjpass(gagged-15, it-12)
aux(gagged-15, has-13)
auxpass(gagged-15, been-14)
advcl(growning-5, gagged-15)

