(ROOT
  (S
    (S
      (NP (PRP They))
      (ADVP (RB indeed))
      (VP (VBP have)))
    (CC and)
    (S
      (NP (DT the) (NNS results))
      (VP (VBP are)
        (ADVP (RB always))
        (ADJP (JJ devastating))))
    (. .)))

nsubj(have-3, They-1)
advmod(have-3, indeed-2)
root(ROOT-0, have-3)
det(results-6, the-5)
nsubj(devastating-9, results-6)
cop(devastating-9, are-7)
advmod(devastating-9, always-8)
conj_and(have-3, devastating-9)

