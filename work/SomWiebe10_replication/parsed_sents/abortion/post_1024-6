(ROOT
  (S
    (NP (NNP Reproduction))
    (VP (VBD permitted)
      (PP (IN for)
        (NP (JJ non-commercial) (NN use)))
      (ADVP (RB only)))
    (. .)))

nsubj(permitted-2, Reproduction-1)
root(ROOT-0, permitted-2)
amod(use-5, non-commercial-4)
prep_for(permitted-2, use-5)
advmod(permitted-2, only-6)

