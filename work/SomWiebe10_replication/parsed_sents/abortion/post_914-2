(ROOT
  (S
    (S
      (NP (PRP It))
      (VP (VBZ 's)
        (ADJP (JJ sad))
        (, ,)
        (INTJ (UH yes))))
    (, ,)
    (CC but)
    (S
      (NP (PRP it))
      (VP (VBZ 's) (RB not)
        (NP (PRP$ our) (NN responsibility)
          (S
            (VP (TO to)
              (VP (VB decide)
                (NP
                  (SBAR
                    (WHNP (WP who))
                    (S
                      (VP (VBZ has)
                        (S
                          (VP (TO to)
                            (VP (VB have)
                              (NP (NNS children))))))))
                  (CC and)
                  (SBAR
                    (WHNP (WP who))
                    (S
                      (VP (VBZ does) (RB n't)))))))))))
    (. .)))

