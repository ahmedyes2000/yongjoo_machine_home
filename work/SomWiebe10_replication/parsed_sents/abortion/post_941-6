  (S
    (NP (PRP I))
    (ADVP (RB somewhat))
    (VP (VBP agree)
      (PP (IN on)
        (NP
          (NP (DT this) (NN one))
          (SBAR
            (WHPP (IN in)
              (WHNP (WDT that)))
            (S
              (SBAR (IN if)
                (S
                  (NP (DT the) (NN birth))
                  (VP (VBZ risks)
                    (NP
                      (NP (DT the) (NN life))
                      (PP (IN of)
                        (NP
                          (NP (PDT both) (DT the) (NN mother))
                          (CC and)
                          (NP (DT the) (NN baby))))))))
              (ADVP (RB then))
              (NP (EX there))
              (VP (VBZ is)
                (NP (DT no) (NN choice))))))))
    (. .)))

