(ROOT
  (S
    (NP (PRP He))
    (VP (VBD recognized)
      (PP (CC both)
        (NP
          (NP (PRP$ my) (NN wife) (POS 's))
          (CC and)
          (NP (PRP$ my) (NN voice)
            (CC and)
            (NN music))))
      (SBAR (IN that)
        (S
          (NP (PRP he))
          (VP (VBD heard)
            (NP (NN while))
            (PP (IN in)
              (NP (DT the) (JJ womb) (CD .3)))))))
    (. .)))

nsubj(recognized-2, He-1)
root(ROOT-0, recognized-2)
prep(recognized-2, both-3)
poss(wife-5, my-4)
dep(both-3, wife-5)
possessive(wife-5, 's-6)
poss(voice-9, my-8)
dep(both-3, voice-9)
