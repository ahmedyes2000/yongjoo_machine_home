(ROOT
  (S
    (NP (NNP Jesus))
    (VP (VBD condensed)
      (NP (PRP them))
      (PP (IN into)
        (NP
          (NP (CD two) (NNS commandments))
          (PP (IN for)
            (NP (NNS Christians)))))
      (S
        (VP (TO to)
          (VP (VB follow)))))
    (. .)))

nsubj(condensed-2, Jesus-1)
xsubj(follow-10, Jesus-1)
root(ROOT-0, condensed-2)
dobj(condensed-2, them-3)
num(commandments-6, two-5)
prep_into(condensed-2, commandments-6)
prep_for(commandments-6, Christians-8)
aux(follow-10, to-9)
xcomp(condensed-2, follow-10)

