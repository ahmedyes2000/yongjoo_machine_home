(ROOT
  (S
    (NP (DT The) (JJ Jewish) (NN Holocaust))
    (VP (VBZ is)
      (VP (VBN considered)
        (NP (DT a) (NN life) (NN issue))))
    (. .)))

det(Holocaust-3, The-1)
amod(Holocaust-3, Jewish-2)
nsubjpass(considered-5, Holocaust-3)
auxpass(considered-5, is-4)
root(ROOT-0, considered-5)
det(issue-8, a-6)
nn(issue-8, life-7)
dobj(considered-5, issue-8)

