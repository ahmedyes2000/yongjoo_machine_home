(ROOT
  (S
    (ADVP (RB First))
    (, ,)
    (NP (DT this))
    (VP (VBZ is)
      (NP
        (NP (DT a) (NN debate))
        (, ,) (RB not)
        (NP
          (NP (DT a) (NN court))
          (PP (IN with)
            (NP (DT a) (NN prosecution)
              (CC and)
              (NN defense))))))
    (. .)))

advmod(debate-6, First-1)
nsubj(debate-6, this-3)
cop(debate-6, is-4)
det(debate-6, a-5)
root(ROOT-0, debate-6)
neg(debate-6, not-8)
det(court-10, a-9)
dep(debate-6, court-10)
det(prosecution-13, a-12)
prep_with(court-10, prosecution-13)
prep_with(court-10, defense-15)
conj_and(prosecution-13, defense-15)

