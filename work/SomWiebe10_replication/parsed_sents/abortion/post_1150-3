(ROOT
  (S
    (ADVP (RB Perhaps))
    (NP (PRP$ its) (NN time)
      (S
        (VP (TO to)
          (VP (VB switch)
            (NP (PRP$ your) (NN birth))))))
    (VP (VBP control))
    (. .)))

advmod(control-8, Perhaps-1)
poss(time-3, its-2)
nsubj(control-8, time-3)
aux(switch-5, to-4)
infmod(time-3, switch-5)
poss(birth-7, your-6)
dobj(switch-5, birth-7)
root(ROOT-0, control-8)

