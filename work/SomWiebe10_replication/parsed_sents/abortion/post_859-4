(ROOT
  (S
    (NP (NNP Women))
    (VP (VBP are)
      (ADJP (JJ responsible)
        (PP (IN for)
          (S
            (VP
              (VP (VB bringinglife)
                (PP (IN into)
                  (NP (DT this) (NN world)))
                (, ,)
                (SBAR
                  (ADVP (RB however))
                  (IN that)
                  (S
                    (NP (NN responsibility))
                    (VP (VBZ falls)
                      (PP (IN into)
                        (NP (DT each)))))))
              (CC and)
              (VP (VB everyoneof)
                (NP (PRP$ their) (NNS hands))))))))
    (. .)))

nsubj(responsible-3, Women-1)
cop(responsible-3, are-2)
root(ROOT-0, responsible-3)
prepc_for(responsible-3, bringinglife-5)
det(world-8, this-7)
prep_into(bringinglife-5, world-8)
advmod(falls-13, however-10)
dep(falls-13, that-11)
nsubj(falls-13, responsibility-12)
ccomp(bringinglife-5, falls-13)
prep_into(falls-13, each-15)
prepc_for(responsible-3, everyoneof-17)
conj_and(bringinglife-5, everyoneof-17)
poss(hands-19, their-18)
dobj(everyoneof-17, hands-19)

