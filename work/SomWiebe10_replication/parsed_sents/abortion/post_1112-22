(ROOT
  (S
    (NP (EX There))
    (VP (VBZ 's)
      (NP
        (NP (DT a) (NN way))
        (SBAR
          (S
            (NP (PRP you))
            (VP (VBP play)
              (SBAR (IN that)
                (S
                  (VP (VBZ makes)
                    (S
                      (NP (PRP it))
                      (NP
                        (NP (NN football))
                        (CC and)
                        (NP
                          (NP (DT a) (NN way))
                          (SBAR
                            (WHNP (WDT that))
                            (S
                              (VP (VBZ makes)
                                (S
                                  (NP (PRP it))
                                  (NP (DT no) (JJR longer) (NN football)))))))))))))))))
    (. .)))

expl('s-2, There-1)
root(ROOT-0, 's-2)
det(way-4, a-3)
nsubj('s-2, way-4)
nsubj(play-6, you-5)
rcmod(way-4, play-6)
complm(makes-8, that-7)
ccomp(play-6, makes-8)
nsubj(football-10, it-9)
xcomp(makes-8, football-10)
det(way-13, a-12)
xcomp(makes-8, way-13)
conj_and(football-10, way-13)
nsubj(makes-15, way-13)
rcmod(way-13, makes-15)
nsubj(football-19, it-16)
det(football-19, no-17)
amod(football-19, longer-18)
xcomp(makes-15, football-19)

