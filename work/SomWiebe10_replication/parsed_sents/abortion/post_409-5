  (S
    (S
      (SBAR
        (WHADVP (WRB When))
        (S
          (NP (DT an) (NN argument))
          (VP (VBZ is)
            (VP (VBN brought)
              (PRT (RP up))))))
      (, ,)
      (NP (PRP you))
      (VP (MD must)
        (VP (VB argue)
          (PP (IN against)
            (NP (PRP it))))))
    (, ,)
    (NP (PRP you))
    (VP (MD can) (RB not)
      (ADVP (RB simply))
      (VP (VB proclaim)
        (SBAR (IN that)
          (S
            (NP (PRP it))
            (VP (VBZ is) (RB not)
              (NP (NN evidence)))))))
    (. .)))

