(ROOT
  (S
    (NP
      (NP (DT The) (VBG deciding) (NN factor))
      (PP (IN in)
        (NP (DT each) (NN case))))
    (VP (VBZ is)
      (NP (PRP you)))
    (. .)))

det(factor-3, The-1)
amod(factor-3, deciding-2)
nsubj(you-8, factor-3)
det(case-6, each-5)
prep_in(factor-3, case-6)
cop(you-8, is-7)
