(ROOT
  (S
    (PP (IN Except)
      (ADVP (RB perhaps)))
    (NP (PRP you))
    (, ,)
    (ADVP (RB since))
    (NP (PRP you))
    (VP
      (ADVP (RB so) (RB obviously))
      (VBP find)
      (NP (NN offense))
      (PP (IN in)
        (NP
          (NP (DT the) (NN act))
          (PP (IN of)
            (NP (NN love))))))
    (. .)))

prep(find-9, Except-1)
pcomp(Except-1, perhaps-2)
nsubj(find-9, you-3)
advmod(find-9, since-5)
nsubj(find-9, you-6)
advmod(obviously-8, so-7)
advmod(find-9, obviously-8)
root(ROOT-0, find-9)
dobj(find-9, offense-10)
det(act-13, the-12)
prep_in(find-9, act-13)
prep_of(act-13, love-15)

