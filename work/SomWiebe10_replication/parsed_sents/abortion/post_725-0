(ROOT
  (SINV
    (PP (IN Because)
      (NP (JJ sexual) (NN activity)))
    (VP (VBZ is))
    (NP
      (NP (JJ awesome)
        (CC and)
        (DT a) (NN sign))
      (PP (IN of)
        (NP (JJ good) (NN health))))
    (. .)))

amod(activity-3, sexual-2)
prep_because(is-4, activity-3)
root(ROOT-0, is-4)
amod(sign-8, awesome-5)
conj_and(awesome-5, a-7)
amod(sign-8, a-7)
nsubj(is-4, sign-8)
amod(health-11, good-10)
prep_of(sign-8, health-11)

