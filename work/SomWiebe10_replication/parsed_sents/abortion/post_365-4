(ROOT
  (S
    (PP (IN Because)
      (NP (DT all)))
    (NP (PRP$ our) (NNS laws))
    (VP (VBP place)
      (NP
        (NP (DT the) (JJS highest) (NN value))
        (PP (IN on)
          (NP (JJ human) (NN life)))))
    (. .)))

prep_because(place-5, all-2)
poss(laws-4, our-3)
nsubj(place-5, laws-4)
root(ROOT-0, place-5)
det(value-8, the-6)
amod(value-8, highest-7)
dobj(place-5, value-8)
amod(life-11, human-10)
prep_on(value-8, life-11)

