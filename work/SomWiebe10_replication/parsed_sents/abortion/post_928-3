(ROOT
  (S
    (NP (DT that))
    (VP (VBZ 's)
      (NP
        (NP (DT the) (JJ only) (NN one))
        (SBAR
          (WHNP (WDT that))
          (S
            (VP (VBZ matters))))))
    (. .)))

nsubj(one-5, that-1)
cop(one-5, 's-2)
det(one-5, the-3)
amod(one-5, only-4)
root(ROOT-0, one-5)
nsubj(matters-7, one-5)
rcmod(one-5, matters-7)

