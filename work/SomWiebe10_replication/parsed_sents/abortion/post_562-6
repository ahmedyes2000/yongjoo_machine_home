(ROOT
  (S
    (S
      (NP (DT The) (JJR greater) (NN good))
      (ADVP (RB always))
      (VP (VBZ is)
        (NP (NN life))))
    (, ,)
    (NP (EX there))
    (VP (VBP are)
      (ADJP
        (ADVP (DT no) (JJR greater))
        (JJ good)
        (PP (IN than)
          (NP
            (NP (DT the) (NN life))
            (PP (IN of)
              (NP (DT a) (NN human) (VBG being)))))))
    (. .)))

det(good-3, The-1)
amod(good-3, greater-2)
nsubj(life-6, good-3)
