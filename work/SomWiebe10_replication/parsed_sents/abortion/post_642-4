(ROOT
  (S
    (PP (IN After)
      (NP
        (NP (CD 30) (NNS years))
        (PP (IN of)
          (NP (JJ bitter) (NN fighting)))))
    (, ,)
    (NP (PRP I))
    (VP (VBP do) (RB n't)
      (VP (VB think)
        (SBAR
          (S
            (NP (PRP we))
            (VP (MD would)
              (VP (VB get)
                (NP (JJ reasonable) (NNS discussions))
                (PP (IN like)
                  (NP (DT this) (NN one)))))))))
    (. .)))

num(years-3, 30-2)
prep_after(think-11, years-3)
amod(fighting-6, bitter-5)
prep_of(years-3, fighting-6)
nsubj(think-11, I-8)
aux(think-11, do-9)
neg(think-11, n't-10)
root(ROOT-0, think-11)
nsubj(get-14, we-12)
aux(get-14, would-13)
ccomp(think-11, get-14)
amod(discussions-16, reasonable-15)
dobj(get-14, discussions-16)
det(one-19, this-18)
prep_like(get-14, one-19)

