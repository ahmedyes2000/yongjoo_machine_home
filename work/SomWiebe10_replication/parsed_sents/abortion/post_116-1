(ROOT
  (S
    (NP (JJ Many) (JJ pro-life) (NNS activists))
    (VP (VBP act)
      (ADVP (RB violently))
      (PP (IN in)
        (NP (NN opposition)))
      (PP (TO to)
        (NP (NN abortion))))
    (. .)))

amod(activists-3, Many-1)
amod(activists-3, pro-life-2)
nsubj(act-4, activists-3)
root(ROOT-0, act-4)
advmod(act-4, violently-5)
prep_in(act-4, opposition-7)
prep_to(act-4, abortion-9)

