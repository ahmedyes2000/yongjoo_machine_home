(ROOT
  (S
    (NP (DT The) (JJ ancient) (NNS Greeks))
    (VP (VBD did) (RB n't)
      (VP (VB believe)
        (SBAR (IN that)
          (S
            (NP (NNS women))
            (VP (VBD had)
              (NP (NNS souls)))))))
    (. .)))

det(Greeks-3, The-1)
amod(Greeks-3, ancient-2)
nsubj(believe-6, Greeks-3)
aux(believe-6, did-4)
neg(believe-6, n't-5)
root(ROOT-0, believe-6)
complm(had-9, that-7)
nsubj(had-9, women-8)
ccomp(believe-6, had-9)
dobj(had-9, souls-10)

