(ROOT
  (S
    (VP (VB Let)
      (SBAR
        (S
          (NP (PRP me))
          (VP (VBP know)
            (SBAR
              (WHADVP (WRB when))
              (S
                (S
                  (NP (DT that))
                  (VP (VBZ happens)))
                (, ,)
                (CC and)
                (S
                  (NP (PRP I))
                  (VP (MD will)
                    (VP (VB be)
                      (ADJP (JJ glad)
                        (S
                          (VP (TO to)
                            (VP (VB revise)
                              (NP
                                (NP (PRP$ my) (NN concept))
                                (PP (IN of)
                                  (NP (NNS fetuses)))))))))))))))))
    (. .)))

root(ROOT-0, Let-1)
nsubj(know-3, me-2)
ccomp(Let-1, know-3)
advmod(happens-6, when-4)
nsubj(happens-6, that-5)
