(ROOT
  (S
    (NP (PRP I))
    (VP (VBD bet)
      (SBAR
        (S
          (NP (PRP it))
          (VP (MD 'll)
            (VP (VB taste)
              (PP (IN like)
                (NP (NN chicken))))))))
    (. .)))

nsubj(bet-2, I-1)
root(ROOT-0, bet-2)
nsubj(taste-5, it-3)
aux(taste-5, 'll-4)
ccomp(bet-2, taste-5)
prep_like(taste-5, chicken-7)

