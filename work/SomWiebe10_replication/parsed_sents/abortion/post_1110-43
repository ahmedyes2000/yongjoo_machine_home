(ROOT
  (S
    (NP (DT These))
    (VP (MD would)
      (VP (VB be)
        (NP
          (NP (NNS laws))
          (PP (VBG regarding)
            (NP (PRP$ our) (NNS bodies))))))
    (. .)))

nsubj(laws-4, These-1)
aux(laws-4, would-2)
cop(laws-4, be-3)
root(ROOT-0, laws-4)
poss(bodies-7, our-6)
prep_regarding(laws-4, bodies-7)

