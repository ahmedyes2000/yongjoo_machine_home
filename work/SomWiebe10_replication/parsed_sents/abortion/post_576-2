(ROOT
  (S
    (NP (EX There))
    (VP (VBZ is) (RB n't)
      (NP
        (NP (DT a) (JJR simpler) (NN question))
        (SBAR
          (S
            (NP (DT a) (NN life))
            (VP
              (VP (VBZ begins)
                (PP (IN at)
                  (NP (NN birth))))
              (CC and)
              (VP (VBZ has)
                (ADVP (RB almost) (RB always))
                (VP (VBN been)
                  (ADVP (RB thus)))))))))
    (. .)))

