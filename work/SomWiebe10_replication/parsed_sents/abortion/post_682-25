(ROOT
  (S
    (NP
      (NP (DT The) (NNS ideologues))
      (PP (IN on)
        (NP
          (NP (DT both) (NNS sides))
          (PP (IN of)
            (NP (DT the) (NN abortion) (NN issue))))))
    (VP (VBP want)
      (NP
        (NP (DT an) (NN answer))
        (SBAR
          (WHNP (WDT that))
          (S
            (VP (VBZ leads)
              (PP (TO to)
                (NP (DT a) (JJ legal) (NN result))))))))
    (. .)))

det(ideologues-2, The-1)
nsubj(want-10, ideologues-2)
det(sides-5, both-4)
prep_on(ideologues-2, sides-5)
det(issue-9, the-7)
nn(issue-9, abortion-8)
prep_of(sides-5, issue-9)
root(ROOT-0, want-10)
det(answer-12, an-11)
dobj(want-10, answer-12)
nsubj(leads-14, answer-12)
rcmod(answer-12, leads-14)
det(result-18, a-16)
