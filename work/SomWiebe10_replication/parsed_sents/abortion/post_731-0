(ROOT
  (S
    (NP (PRP I))
    (VP (VBP dont)
      (SBAR
        (S
          (VP (VBP think)
            (SBAR
              (S
                (NP (PRP you))
                (ADVP (RB really))
                (VP (VBP read)
                  (NP (PRP$ my) (NN comment)))))))))
    (. .)))

nsubj(dont-2, I-1)
root(ROOT-0, dont-2)
ccomp(dont-2, think-3)
nsubj(read-6, you-4)
advmod(read-6, really-5)
ccomp(think-3, read-6)
poss(comment-8, my-7)
dobj(read-6, comment-8)

