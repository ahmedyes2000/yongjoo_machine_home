(ROOT
  (S
    (S
      (VP (VBZ views)
        (NP (JJ other) (NNS people))
        (PP (IN as)
          (NP (NNS objects)))
        (S
          (VP (TO to)
            (VP (VB be)
              (VP (VBN used)
                (PP (IN for)
                  (NP
                    (NP (NNS ones))
                    (NP (JJ own) (JJ personal) (NN pleasure))))))))))
    (CC and)
    (S
      (ADVP (RB then))
      (VP (VBN discarded)
        (SBAR
          (WHADVP (WRB when))
          (S
            (ADVP (RB no) (RB longer))
            (VP (VBD wanted))))))
    (. .)))

root(ROOT-0, views-1)
amod(people-3, other-2)
dobj(views-1, people-3)
prep_as(views-1, objects-5)
aux(used-8, to-6)
auxpass(used-8, be-7)
xcomp(views-1, used-8)
prep_for(used-8, ones-10)
amod(pleasure-13, own-11)
amod(pleasure-13, personal-12)
dep(ones-10, pleasure-13)
advmod(discarded-16, then-15)
conj_and(views-1, discarded-16)
advmod(wanted-20, when-17)
advmod(longer-19, no-18)
advmod(wanted-20, longer-19)
advcl(discarded-16, wanted-20)

