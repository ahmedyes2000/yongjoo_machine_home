(ROOT
  (S
    (NP (PRP I))
    (VP (MD would)
      (VP (VB prefer)
        (S
          (NP (NNS adults))
          (VP (VB control)
            (NP (PRP$ their) (NNS desires))
            (PP (IN in)
              (NP (NN order)))))
        (SBAR (IN that)
          (S
            (NP (NN society))
            (VP (MD might)
              (VP (VB function)
                (PP (IN at)
                  (NP (PRP$ its) (JJS best)))))))))
    (. .)))

nsubj(prefer-3, I-1)
aux(prefer-3, would-2)
root(ROOT-0, prefer-3)
nsubj(control-5, adults-4)
ccomp(prefer-3, control-5)
poss(desires-7, their-6)
dobj(control-5, desires-7)
prep_in(control-5, order-9)
complm(function-13, that-10)
nsubj(function-13, society-11)
aux(function-13, might-12)
ccomp(prefer-3, function-13)
poss(best-16, its-15)
prep_at(function-13, best-16)

