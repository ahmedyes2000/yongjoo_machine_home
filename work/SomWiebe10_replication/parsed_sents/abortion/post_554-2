(ROOT
  (S
    (S
      (SBAR (IN If)
        (S
          (NP (EX there))
          (VP (VBD were)
            (NP
              (NP (JJ such) (NNS things))
              (PP (IN as)
                (NP (VBN forced) (NNS abortions)))))))
      (NP (PRP you))
      (VP (MD would)
        (VP (VB have)
          (NP (DT an) (NN argument)))))
    (, ,)
    (CC but)
    (S
      (NP (DT this))
      (VP (VBZ is) (RB not)
        (NP (DT the) (NN case))))
    (. .)))

mark(were-3, If-1)
expl(were-3, there-2)
advcl(have-11, were-3)
amod(things-5, such-4)
nsubj(were-3, things-5)
amod(abortions-8, forced-7)
prep_as(things-5, abortions-8)
nsubj(have-11, you-9)
aux(have-11, would-10)
root(ROOT-0, have-11)
det(argument-13, an-12)
dobj(have-11, argument-13)
nsubj(case-20, this-16)
cop(case-20, is-17)
neg(case-20, not-18)
det(case-20, the-19)
conj_but(have-11, case-20)

