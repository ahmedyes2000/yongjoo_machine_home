(ROOT
  (S
    (S
      (NP (PRP One))
      (VP (MD may)
        (VP (VB argue)
          (PP (IN about)
            (NP
              (NP (DT the) (JJ later) (NNS stages))
              (PP (IN of)
                (NP (DT a) (NN pregnancy))))))))
    (, ,)
    (CC but)
    (S
      (NP (DT the) (JJ essential) (NN issue))
      (VP (VBZ concerns)
        (PP (RB only)
          (NP (DT the) (JJ first) (CD three) (NNS months)))))
    (. .)))

