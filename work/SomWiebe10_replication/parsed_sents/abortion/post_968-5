(ROOT
  (S
    (S
      (SBAR (IN If)
        (S
          (NP (NN somebody))
          (VP (VBD did)
            (NP (DT this)))))
      (NP (PRP they))
      (VP (MD would)
        (VP (VB be)
          (VP (VBG breaking)
            (NP (DT the) (JJ fifth) (NN commandment))))))
    (: :)
    (S
      (NP (PRP You))
      (VP (MD shall) (RB not)
        (VP (VB murder))))
    (. .)))

mark(did-3, If-1)
nsubj(did-3, somebody-2)
advcl(breaking-8, did-3)
dobj(did-3, this-4)
nsubj(breaking-8, they-5)
aux(breaking-8, would-6)
aux(breaking-8, be-7)
root(ROOT-0, breaking-8)
det(commandment-11, the-9)
amod(commandment-11, fifth-10)
dobj(breaking-8, commandment-11)
nsubj(murder-16, You-13)
aux(murder-16, shall-14)
neg(murder-16, not-15)
parataxis(breaking-8, murder-16)

