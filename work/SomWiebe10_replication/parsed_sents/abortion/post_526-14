(ROOT
  (S
    (SBAR (IN As)
      (S
        (NP (PRP you))
        (ADVP (RB well))
        (VP (VBP know))))
    (, ,)
    (NP (DT this))
    (VP (VBD was) (RB not)
      (ADVP (RB always))
      (NP (DT the) (NN case)))
    (. .)))

mark(know-4, As-1)
nsubj(know-4, you-2)
advmod(know-4, well-3)
advcl(case-11, know-4)
nsubj(case-11, this-6)
cop(case-11, was-7)
neg(case-11, not-8)
advmod(case-11, always-9)
det(case-11, the-10)
root(ROOT-0, case-11)

