(ROOT
  (SQ
    (VP (VB suggestion) (-RRB- -RRB-)
      (NP (IN for) (DT the) (NN status))
      (PP (IN of)
        (NP
          (NP (DT the) (NN organism))
          (SBAR
            (WHNP (WDT that))
            (S
              (VP (VBZ is)
                (VP (VBN given)
                  (NP
                    (NP (NN value))
                    (CC and)
                    (NP (JJ legal) (NN protection))))))))))
    (. .)))

root(ROOT-0, suggestion-1)
amod(status-5, for-3)
det(status-5, the-4)
dobj(suggestion-1, status-5)
det(organism-8, the-7)
prep_of(suggestion-1, organism-8)
nsubjpass(given-11, organism-8)
auxpass(given-11, is-10)
rcmod(organism-8, given-11)
dobj(given-11, value-12)
amod(protection-15, legal-14)
dobj(given-11, protection-15)
conj_and(value-12, protection-15)

