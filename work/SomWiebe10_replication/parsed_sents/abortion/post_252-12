(ROOT
  (S (CC And)
    (NP (DT the) (JJ moral) (NN position))
    (VP (VBZ is)
      (SBAR (IN that)
        (S
          (NP (DT an) (JJ actual) (NN individual))
          (VP (MD should) (RB not)
            (VP (VB be)
              (VP (VBN treated)
                (PP (IN as)
                  (NP (DT a) (NNS means)))
                (PP (TO to)
                  (NP
                    (NP (DT the) (NNS ends))
                    (PP (IN of)
                      (NP (JJ other) (NNS persons)
                        (CC or)
                        (NNS non-persons)))))))))))
    (. .)))

cc(is-5, And-1)
det(position-4, the-2)
amod(position-4, moral-3)
nsubj(is-5, position-4)
root(ROOT-0, is-5)
complm(treated-13, that-6)
det(individual-9, an-7)
amod(individual-9, actual-8)
nsubjpass(treated-13, individual-9)
aux(treated-13, should-10)
neg(treated-13, not-11)
auxpass(treated-13, be-12)
ccomp(is-5, treated-13)
det(means-16, a-15)
prep_as(treated-13, means-16)
det(ends-19, the-18)
prep_to(treated-13, ends-19)
amod(persons-22, other-21)
prep_of(ends-19, persons-22)
prep_of(ends-19, non-persons-24)
conj_or(persons-22, non-persons-24)

