  (S
    (NP (PRP I))
    (VP (VBP have)
      (ADVP (RB never))
      (VP (VBN heard)
        (NP
          (NP (NN anyone) (NN call))
          (PP (IN for)
            (NP
              (NP (DT that))
              (, ,)
              (CC or)
              (NP
                (NP (DT any) (NN punishment) (RB whatsoever))
                (PP (IN for)
                  (NP (NNS women))))))
          (SBAR
            (WHNP (WP who))
            (S
              (VP (VBP abort)))))))
    (. .)))

nsubj(heard-4, I-1)
aux(heard-4, have-2)
neg(heard-4, never-3)
root(ROOT-0, heard-4)
nn(call-6, anyone-5)
dobj(heard-4, call-6)
