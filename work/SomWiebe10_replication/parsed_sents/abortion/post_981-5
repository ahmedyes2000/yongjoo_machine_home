(ROOT
  (S
    (S
      (NP (PRP I))
      (VP (VBD thought)
        (SBAR
          (S
            (NP (DT the) (NN question))
            (VP (VBD was))))))
    (: ;)
    (SQ (MD Should)
      (NP (NN abortion))
      (VP (VB be)
        (VP (VBN banned))))
    (. .)))

nsubj(thought-2, I-1)
root(ROOT-0, thought-2)
det(question-4, the-3)
nsubj(was-5, question-4)
ccomp(thought-2, was-5)
aux(banned-10, Should-7)
nsubjpass(banned-10, abortion-8)
auxpass(banned-10, be-9)
parataxis(thought-2, banned-10)

