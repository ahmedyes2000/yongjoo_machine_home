  (S
    (S
      (NP (PRP I))
      (VP (VBP mean)))
    (, ,)
    (S
      (SBAR (IN if)
        (S
          (NP (PRP you))
          (VP (VBP want)
            (S
              (VP (TO to)
                (VP
                  (VP (VB cop)
                    (PRT (RP out)))
                  (CC and)
                  (RB not)
                  (VP (VB answer)
                    (NP (PRP them)))))))))
      (, ,)
      (NP (PRP you))
      (VP (MD should) (RB n't)
        (VP (VB keep)
          (NP
            (NP (DT the) (NN debate))
            (VP (VBG going))))))
    (, ,)
    (SBAR (IN since)
      (S
        (NP (PRP you))
        (VP (VBD were)
          (NP
            (NP (DT the) (NN one))
            (SBAR
              (WHNP (WP who))
              (S
                (VP (VBD disputed)
                  (NP (PRP me)))))))))
    (. .)))

