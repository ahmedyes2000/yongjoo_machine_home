(ROOT
  (FRAG
    (NP (RB First) (DT a) (NN question))
    (: :)
    (S
      (NP (PRP You))
      (VP (VBD used)
        (S
          (NP (DT the) (NN term))
          (`` ``)
          (ADJP (JJ miraculous))
          ('' ''))))
    (. .)))

advmod(question-3, First-1)
det(question-3, a-2)
root(ROOT-0, question-3)
nsubj(used-6, You-5)
dep(question-3, used-6)
det(term-8, the-7)
dep(miraculous-10, term-8)
dep(used-6, miraculous-10)

