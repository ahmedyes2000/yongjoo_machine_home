(ROOT
  (S
    (NP (PRP I))
    (VP (MD will)
      (VP (VB be)
        (ADJP (JJ interested)
          (PP (IN in)
            (S
              (VP (VBG seeing)
                (SBAR
                  (WHADVP (WRB how))
                  (S
                    (NP
                      (NP (DT the) (NNS experts))
                      (PP (IN on)
                        (NP (DT that) (NN side))))
                    (VP (VBP explain)
                      (NP (DT the) (NN statement))
                      (ADVP (RB away)))))))))))
    (. .)))

nsubj(interested-4, I-1)
aux(interested-4, will-2)
cop(interested-4, be-3)
root(ROOT-0, interested-4)
prepc_in(interested-4, seeing-6)
advmod(explain-13, how-7)
det(experts-9, the-8)
nsubj(explain-13, experts-9)
det(side-12, that-11)
prep_on(experts-9, side-12)
ccomp(seeing-6, explain-13)
det(statement-15, the-14)
