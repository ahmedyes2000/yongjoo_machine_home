(ROOT
  (S
    (NP (PRP It))
    (VP (VBD took)
      (NP (JJ many) (JJ good) (NNS Christians))
      (PP (IN in)
        (NP (NNP Germany)))
      (S
        (VP
          (VP (VBG closing)
            (NP (PRP$ their) (NNS eyes)))
          (CC and)
          (VP (VBG remaining)
            (ADJP (JJ silent))))))
    (. .)))

nsubj(took-2, It-1)
root(ROOT-0, took-2)
amod(Christians-5, many-3)
amod(Christians-5, good-4)
dobj(took-2, Christians-5)
prep_in(took-2, Germany-7)
dep(took-2, closing-8)
poss(eyes-10, their-9)
dobj(closing-8, eyes-10)
dep(took-2, remaining-12)
conj_and(closing-8, remaining-12)
acomp(remaining-12, silent-13)

