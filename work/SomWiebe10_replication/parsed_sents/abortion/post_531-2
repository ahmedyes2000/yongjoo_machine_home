(ROOT
  (S
    (NP
      (NP (DT The) (NN issue))
      (PP (IN of)
        (NP (NN abortion))))
    (VP (VBZ is)
      (NP
        (NP (CD one))
        (SBAR
          (WHNP (WDT which))
          (S
            (VP (VBZ goes)
              (ADJP
                (ADJP (RB much) (JJR deeper))
                (PP (IN than)
                  (NP (NNP pro-life)
                    (CC v.)
                    (NNP pro-choice)))))))))
    (. .)))

det(issue-2, The-1)
nsubj(one-6, issue-2)
prep_of(issue-2, abortion-4)
