(ROOT
  (S
    (NP (NNP God))
    (VP
      (VP (VBZ cares)
        (PP (IN about)
          (NP (PRP you))))
      (CC and)
      (VP (VBZ has)
        (NP (NN healing)
          (CC and)
          (NNS blessings)))
      (S
        (VP (TO to)
          (VP (VB give)
            (NP (PRP you))
            (SBAR (IN if)
              (S
                (NP (PRP you))
                (VP (MD would)
                  (ADVP (RB only))
                  (VP (VB turn)
                    (PP (TO to)
                      (NP (PRP Him)))))))))))
    (. .)))

nsubj(cares-2, God-1)
nsubj(has-6, God-1)
xsubj(give-11, God-1)
root(ROOT-0, cares-2)
prep_about(cares-2, you-4)
conj_and(cares-2, has-6)
dobj(has-6, healing-7)
dobj(has-6, blessings-9)
conj_and(healing-7, blessings-9)
aux(give-11, to-10)
xcomp(cares-2, give-11)
dobj(give-11, you-12)
mark(turn-17, if-13)
nsubj(turn-17, you-14)
aux(turn-17, would-15)
advmod(turn-17, only-16)
advcl(give-11, turn-17)
prep_to(turn-17, Him-19)

