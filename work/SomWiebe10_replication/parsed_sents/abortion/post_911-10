(ROOT
  (S
    (ADVP (RB Sometimes))
    (NP (DT no) (NN one))
    (VP (VBZ knows)
      (SBAR
        (S
          (NP (DT these) (NNS girls))
          (VP (VBP are)
            (ADJP (JJ pregnant)
              (SBAR (IN until)
                (S
                  (NP (PRP they))
                  (VP (VBP start)
                    (S
                      (VP (VBG getting)
                        (NP
                          (NP (JJ big))
                          (PP (IN in)
                            (NP (PRP$ their) (JJ third) (NN trimester))))))))))))))
    (. .)))

advmod(knows-4, Sometimes-1)
det(one-3, no-2)
nsubj(knows-4, one-3)
root(ROOT-0, knows-4)
det(girls-6, these-5)
nsubj(pregnant-8, girls-6)
cop(pregnant-8, are-7)
ccomp(knows-4, pregnant-8)
mark(start-11, until-9)
nsubj(start-11, they-10)
ccomp(pregnant-8, start-11)
xcomp(start-11, getting-12)
dobj(getting-12, big-13)
poss(trimester-17, their-15)
amod(trimester-17, third-16)
prep_in(big-13, trimester-17)

