(ROOT
  (S
    (NP (PRP We))
    (ADVP (DT all))
    (VP (VBP know)
      (SBAR
        (S
          (NP (DT a) (JJ precious) (NN life))
          (VP (VBZ is)
            (ADVP (RB there) (RB regardless))
            (PP (IN of)
              (NP
                (NP
                  (ADJP (PRP$ his)
                    (CC or)
                    (PRP$ her))
                  (NN stage))
                (PP (IN of)
                  (NP (NN development)))))))))
    (. .)))

nsubj(know-3, We-1)
advmod(know-3, all-2)
root(ROOT-0, know-3)
det(life-6, a-4)
amod(life-6, precious-5)
nsubj(is-7, life-6)
ccomp(know-3, is-7)
advmod(is-7, there-8)
amod(stage-14, his-11)
conj_or(his-11, her-13)
poss(his-11, her-13)
amod(stage-14, her-13)
prep_regardless_of(is-7, stage-14)
prep_of(stage-14, development-16)

