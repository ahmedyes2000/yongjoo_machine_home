(ROOT
  (S
    (NP
      (NP (DT The) (NN question))
      (PP (IN of)
        (NP (NN abortion))))
    (VP (MD will)
      (ADVP (RB never))
      (VP (VB be)
        (VP (VBN settled))))
    (. .)))

det(question-2, The-1)
nsubjpass(settled-8, question-2)
prep_of(question-2, abortion-4)
aux(settled-8, will-5)
neg(settled-8, never-6)
auxpass(settled-8, be-7)
root(ROOT-0, settled-8)

