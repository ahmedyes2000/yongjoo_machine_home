(ROOT
  (S
    (S
      (NP (PRP It))
      (ADVP (RB simply))
      (VP (VBZ is) (RB n't)))
    (, ,)
    (CC and)
    (S
      (NP (DT that))
      (VP (VBZ is)
        (NP (DT a) (NN fact))))
    (. .)))

nsubj(is-3, It-1)
advmod(is-3, simply-2)
root(ROOT-0, is-3)
neg(is-3, n't-4)
nsubj(fact-10, that-7)
cop(fact-10, is-8)
det(fact-10, a-9)
conj_and(is-3, fact-10)

