(ROOT
  (S
    (ADVP (RB As) (RB far)
      (SBAR (IN as)
        (S
          (NP (PRP I))
          (VP (VBP know)))))
    (, ,)
    (NP (DT the) (NN prion) (NN form))
    (ADVP (RB always))
    (VP (VBZ loses)
      (NP (DT this) (NN quality)))
    (. .)))

advmod(far-2, As-1)
advmod(loses-11, far-2)
mark(know-5, as-3)
nsubj(know-5, I-4)
ccomp(far-2, know-5)
det(form-9, the-7)
nn(form-9, prion-8)
nsubj(loses-11, form-9)
advmod(loses-11, always-10)
root(ROOT-0, loses-11)
det(quality-13, this-12)
dobj(loses-11, quality-13)

