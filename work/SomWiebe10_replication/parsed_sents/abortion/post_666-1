(ROOT
  (S
    (NP (DT No) (NN one))
    (VP (VBZ wants)
      (S
        (VP (TO to)
          (VP (VB be)
            (VP
              (VP (VBN lied)
                (S
                  (VP (TO to))))
              (CC or)
              (VP (VBD cheated)
                (PRT (RP on)))
              (CC or)
              (VP (VBN stolen)
                (PP (IN from)
                  (NP (FW etc.)))))))))
    (. .)))

det(one-2, No-1)
nsubj(wants-3, one-2)
xsubj(lied-6, one-2)
root(ROOT-0, wants-3)
aux(lied-6, to-4)
auxpass(lied-6, be-5)
xcomp(wants-3, lied-6)
xcomp(lied-6, to-7)
xcomp(wants-3, cheated-9)
conj_or(lied-6, cheated-9)
prt(cheated-9, on-10)
xcomp(wants-3, stolen-12)
conj_or(lied-6, stolen-12)
prep_from(stolen-12, etc.-14)

(ROOT
  (S
    (NP (PRP You))
    (VP (VBP 're)
      (ADVP (RB still))
      (VP (VBG basing)
        (NP (PRP$ your) (JJ personal) (NNS beliefs))
        (PP (IN on)
          (NP (DT the) (NN premise)))
        (SBAR (IN that)
          (S
            (NP (EX there))
            (VP (VBP are)
              (NP (DT no) (NNS absolutes))
              (NP (RB just) (JJ different) (NNS perspectives)))))))
    (. .)))

nsubj(basing-4, You-1)
aux(basing-4, 're-2)
advmod(basing-4, still-3)
root(ROOT-0, basing-4)
poss(beliefs-7, your-5)
amod(beliefs-7, personal-6)
dobj(basing-4, beliefs-7)
det(premise-10, the-9)
prep_on(basing-4, premise-10)
complm(are-13, that-11)
expl(are-13, there-12)
ccomp(basing-4, are-13)
det(absolutes-15, no-14)
iobj(are-13, absolutes-15)
nsubj(are-13, absolutes-15)
advmod(perspectives-18, just-16)
amod(perspectives-18, different-17)
nsubj(are-13, perspectives-18)

