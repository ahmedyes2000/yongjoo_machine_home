(ROOT
  (S
    (SBAR (IN Until)
      (S
        (NP (PRP you))
        (VP (VBP get)
          (NP (DT both)))))
    (, ,)
    (NP (PRP you))
    (ADVP (RB only))
    (VP (VBP have)
      (NP (NNS pieces)))
    (. .)))

mark(get-3, Until-1)
nsubj(get-3, you-2)
advcl(have-8, get-3)
dobj(get-3, both-4)
nsubj(have-8, you-6)
advmod(have-8, only-7)
root(ROOT-0, have-8)
dobj(have-8, pieces-9)

