(ROOT
  (S
    (SBAR
      (WHNP (WP What))
      (S
        (VP (MD must)
          (ADVP (RB also))
          (VP (VB be)
            (VP (VBN taken)
              (PP (IN into)
                (NP (NN account))))))))
    (VP (VBZ is)
      (NP
        (NP (DT the) (NN amount))
        (PP (IN of)
          (NP
            (NP (NN liberty))
            (VP (VBG being)
              (VP (VBN taken)))))))
    (. .)))

nsubjpass(taken-5, What-1)
aux(taken-5, must-2)
advmod(taken-5, also-3)
auxpass(taken-5, be-4)
csubj(amount-10, taken-5)
prep_into(taken-5, account-7)
cop(amount-10, is-8)
det(amount-10, the-9)
root(ROOT-0, amount-10)
prep_of(amount-10, liberty-12)
auxpass(taken-14, being-13)
partmod(liberty-12, taken-14)

