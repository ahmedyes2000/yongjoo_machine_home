(ROOT
  (S (CC Or)
    (S
      (VP (TO to)
        (VP (VB put)
          (S
            (NP (PRP it))
            (NP (DT another) (NN way))))))
    (: ;)
    (S
      (S
        (NP (PRP one))
        (VP (VBZ is)
          (NP (DT a) (JJ healthy) (NN expression))))
      (CC and)
      (S
        (NP (DT the) (JJ other))
        (VP (VBZ is)
          (ADJP (JJ unhealthy)))))
    (. .)))

cc(put-3, Or-1)
aux(put-3, to-2)
root(ROOT-0, put-3)
nsubj(way-6, it-4)
det(way-6, another-5)
xcomp(put-3, way-6)
nsubj(expression-12, one-8)
cop(expression-12, is-9)
det(expression-12, a-10)
amod(expression-12, healthy-11)
parataxis(put-3, expression-12)
det(other-15, the-14)
nsubj(unhealthy-17, other-15)
cop(unhealthy-17, is-16)
parataxis(put-3, unhealthy-17)
conj_and(expression-12, unhealthy-17)

