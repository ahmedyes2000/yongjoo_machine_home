(ROOT
  (FRAG (CC But)
    (ADVP (RB better))
    (PP (TO to)
      (ADJP (JJ loose)))
    (PP
      (NP
        (QP (DT a) (CD 100)))
      (IN than)
      (NP (CD 10000)))
    (. .)))

dep(better-2, But-1)
root(ROOT-0, better-2)
dep(better-2, to-3)
pobj(to-3, loose-4)
quantmod(100-6, a-5)
npadvmod(than-7, 100-6)
dep(better-2, than-7)
pobj(than-7, 10000-8)

