(ROOT
  (S
    (ADVP (RB However))
    (, ,)
    (VP (VBP do) (RB n't)
      (VP (VB use)
        (NP (NN government) (NN power))
        (S
          (VP (TO to)
            (VP (VB force)
              (S
                (NP (NN everyone) (RB else))
                (VP (TO to)
                  (VP (VB live)
                    (PP (IN by)
                      (NP (PRP$ your) (JJ biblical) (NNS edicts)))))))))))
    (. .)))

advmod(use-5, However-1)
aux(use-5, do-3)
neg(use-5, n't-4)
root(ROOT-0, use-5)
nn(power-7, government-6)
dobj(use-5, power-7)
aux(force-9, to-8)
xcomp(use-5, force-9)
nsubj(live-13, everyone-10)
advmod(everyone-10, else-11)
aux(live-13, to-12)
xcomp(force-9, live-13)
poss(edicts-17, your-15)
amod(edicts-17, biblical-16)
prep_by(live-13, edicts-17)

