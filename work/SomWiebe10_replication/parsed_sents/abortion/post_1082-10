(ROOT
  (S
    (NP (PDT All) (DT the)
      (ADJP (JJ pro-choice) (JJ pro-abortion))
      (NN rhetoric))
    (VP (MD will)
      (ADVP (RB just))
      (VP (VB melt)
        (PRT (RP away))))
    (. .)))

predet(rhetoric-5, All-1)
det(rhetoric-5, the-2)
amod(pro-abortion-4, pro-choice-3)
amod(rhetoric-5, pro-abortion-4)
nsubj(melt-8, rhetoric-5)
aux(melt-8, will-6)
advmod(melt-8, just-7)
root(ROOT-0, melt-8)
prt(melt-8, away-9)

