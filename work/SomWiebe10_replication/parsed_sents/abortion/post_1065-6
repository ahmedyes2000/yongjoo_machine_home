  (S (CC And)
    (ADVP (RB further) (RBR more))
    (NP (PRP we))
    (VP (MD could)
      (VP (VB extend)
        (NP (DT this))
        (PP (TO to)
          (NP
            (NP (DT the) (NN mentally))
            (CC and)
            (NP (RB physically) (NNS handicapped))))))
    (. .)))

