(ROOT
  (S
    (NP (PRP I))
    (VP (VBP 'm) (RB not)
      (ADJP (JJ sure))
      (SBAR (IN if)
        (S
          (NP (DT that))
          (VP (MD would)
            (VP (VB be)
              (ADJP (JJ good))
              (PRN (-LRB- -LRB-)
                (SBAR (IN although)
                  (S
                    (NP (PRP I))
                    (VP (VBP do) (RB n't)
                      (VP (VB know)
                        (SBAR (IN if)
                          (S
                            (NP (PRP it))
                            (VP (MD would)
                              (VP (VB be)
                                (ADJP (JJ bad))
                                (ADVP (RB either))))))))))
                (-RRB- -RRB-)))))))
    (. .)))

nsubj(sure-4, I-1)
cop(sure-4, 'm-2)
neg(sure-4, not-3)
root(ROOT-0, sure-4)
mark(good-9, if-5)
nsubj(good-9, that-6)
aux(good-9, would-7)
cop(good-9, be-8)
advcl(sure-4, good-9)
mark(know-15, although-11)
nsubj(know-15, I-12)
aux(know-15, do-13)
neg(know-15, n't-14)
parataxis(good-9, know-15)
mark(bad-20, if-16)
nsubj(bad-20, it-17)
aux(bad-20, would-18)
cop(bad-20, be-19)
advcl(know-15, bad-20)
advmod(bad-20, either-21)

