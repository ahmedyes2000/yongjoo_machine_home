(ROOT
  (SBAR (IN If)
    (S
      (NP (PRP you))
      (VP (VBP are)
        (PP (IN for)
          (NP
            (NP (NN abortion))
            (ADVP (IN at) (JJS least))))
        (S
          (VP
            (VP (VB inform)
              (NP (PRP yourself)))
            (CC and)
            (VP (VB watch)
              (S
                (NP (DT the) (JJ video) (JJ silent) (NN screamand))
                (VP (VB make)
                  (NP
                    (NP (DT an) (VBN educated) (NN call))
                    (PP (IN about)
                      (NP (NN abortion)))))))))))))

mark(are-3, If-1)
nsubj(are-3, you-2)
root(ROOT-0, are-3)
prep_for(are-3, abortion-5)
advmod(abortion-5, at-6)
pobj(at-6, least-7)
ccomp(are-3, inform-8)
