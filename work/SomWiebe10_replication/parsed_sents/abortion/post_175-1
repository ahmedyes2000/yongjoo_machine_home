  (S
    (S
      (NP (PRP I))
      (VP (VB apologize)
        (SBAR (IN if)
          (S
            (NP (PRP$ my) (NNS questions))
            (VP (VBP are)
              (ADJP (JJ difficult)))))))
    (, ,)
    (CC but)
    (S
