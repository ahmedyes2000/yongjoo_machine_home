(ROOT
  (S
    (ADVP (UH OK))
    (NP (PRP I))
    (VP (VBP agree)
      (PP (IN about)
        (NP
          (NP (DT a) (JJ new) (NN debate))
          (: ;)
          (NP (DT any) (NNS ideas)))))
    (. ?)))

advmod(agree-3, OK-1)
