(ROOT
  (S
    (NP (PRP We))
    (VP (VBP do)
      (ADVP (RB intentionally))
      (VP (VB kill)
        (NP (JJ innocent) (NNS civilians))
        (, ,)
        (ADVP
          (ADVP (RB even) (RB now))
          (PP (IN in)
            (NP (NNP Afghanistan))))))
    (. .)))

nsubj(kill-4, We-1)
aux(kill-4, do-2)
advmod(kill-4, intentionally-3)
root(ROOT-0, kill-4)
amod(civilians-6, innocent-5)
dobj(kill-4, civilians-6)
advmod(now-9, even-8)
advmod(kill-4, now-9)
prep_in(now-9, Afghanistan-11)

