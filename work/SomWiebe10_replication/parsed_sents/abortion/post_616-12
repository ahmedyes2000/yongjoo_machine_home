        (NP
          (NP
            (NP (NNP e.g.))
            (NP (JJ organismal) (NNS biologists)))
          (, ,)
          (NP (NNS ecologists))
          (, ,) (FW etc.))
        (-RRB- -RRB-))
      (VP (VB use)
        (NP
          (NP (DT a) (NN definition))
          (SBAR
            (WHNP (WDT that))
            (S
              (VP (VBZ includes)
                (ADJP (`` ``) (JJ independent) ('' ''))
