(ROOT
  (S
    (NP (NNP Women))
    (VP (MD should)
      (VP (VB take)
        (NP (NN responsibility))
        (PP (IN for)
          (NP (PRP$ their) (NNS actions)))))
    (. .)))

nsubj(take-3, Women-1)
aux(take-3, should-2)
root(ROOT-0, take-3)
dobj(take-3, responsibility-4)
poss(actions-7, their-6)
prep_for(take-3, actions-7)

