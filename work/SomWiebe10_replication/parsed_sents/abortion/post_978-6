(ROOT
  (S
    (S
      (NP (PRP You))
      (VP (VBP stand)
        (PP (IN by))))
    (CC and)
    (S
      (NP (NN watch) (NNS people))
      (VP (VBP kill)
        (NP (DT each) (JJ other))
        (PP (IN during)
          (NP (NN war)))))
    (. .)))

nsubj(stand-2, You-1)
root(ROOT-0, stand-2)
prep(stand-2, by-3)
nn(people-6, watch-5)
nsubj(kill-7, people-6)
conj_and(stand-2, kill-7)
det(other-9, each-8)
dobj(kill-7, other-9)
prep_during(kill-7, war-11)

