(ROOT
  (S
    (ADVP (RB Additionally))
    (, ,)
    (NP (PRP$ your) (NN appendix))
    (VP (VBZ is)
      (NP
        (NP (DT a) (NN part))
        (PP (IN of)
          (NP (PRP$ your) (JJ person-hood))))
      (, ,)
      (SBAR (IN as)
        (S
          (NP (PRP it))
          (VP (VBZ contains)
            (NP (PRP$ your) (NN DNA))))))
    (. .)))

advmod(part-7, Additionally-1)
poss(appendix-4, your-3)
nsubj(part-7, appendix-4)
cop(part-7, is-5)
det(part-7, a-6)
root(ROOT-0, part-7)
poss(person-hood-10, your-9)
