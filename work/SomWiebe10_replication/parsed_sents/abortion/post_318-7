(ROOT
  (S
    (S
      (S
        (VP (VBG Doing)
          (NP (NN nothing))))
      (VP (MD would)
        (VP (VB cause)
          (NP (DT the) (NN child)
            (S
              (VP (TO to)
                (VP (VB die))))))))
    (, ,)
    (CC but)
    (S
      (NP (NN action))
      (VP (MD would)
        (VP (VB save)
          (NP (PRP him)))))
    (. .)))

csubj(cause-4, Doing-1)
dobj(Doing-1, nothing-2)
aux(cause-4, would-3)
root(ROOT-0, cause-4)
det(child-6, the-5)
dobj(cause-4, child-6)
aux(die-8, to-7)
infmod(child-6, die-8)
nsubj(save-13, action-11)
aux(save-13, would-12)
conj_but(cause-4, save-13)
dobj(save-13, him-14)

