(ROOT
  (S
    (NP (PRP You))
    (VP (MD will) (RB not)
      (VP (VB be)
        (ADJP (JJ able)
          (S
            (VP (TO to)
              (VP (VB argue)
                (PRT (RP away))
                (NP (PRP$ their) (NN grief))
                (PP (IN with))
                ('' '')))))
        (SBAR
          (WHADVP (WRB When))
          (S
            (VP (VBZ is)
              (NP (NN life) (NN viabile) ('' '') (NNS rhetoric)))))))
    (. .)))

nsubj(able-5, You-1)
aux(able-5, will-2)
neg(able-5, not-3)
cop(able-5, be-4)
root(ROOT-0, able-5)
aux(argue-7, to-6)
xcomp(able-5, argue-7)
prt(argue-7, away-8)
poss(grief-10, their-9)
dobj(argue-7, grief-10)
prep(argue-7, with-11)
advmod(rhetoric-18, When-13)
cop(rhetoric-18, is-14)
nn(rhetoric-18, life-15)
nn(rhetoric-18, viabile-16)
advcl(able-5, rhetoric-18)

