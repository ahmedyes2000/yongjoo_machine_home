(ROOT
  (S
    (S
      (ADVP (RB Clearly))
      (NP (DT the) (JJ old) (NN man))
      (VP (MD will)
        (VP (VB die)
          (PP (IN in)
            (NP (DT a) (JJ few) (NNS years))))))
    (, ,)
    (CC but)
    (S
      (NP (DT the) (NN boy))
      (VP (VBZ has)
        (NP
          (NP (PRP$ his) (JJ whole) (NN life))
          (PP (RB ahead) (IN of)
            (NP (PRP him))))))
    (. .)))

advmod(die-6, Clearly-1)
det(man-4, the-2)
amod(man-4, old-3)
nsubj(die-6, man-4)
aux(die-6, will-5)
root(ROOT-0, die-6)
det(years-10, a-8)
amod(years-10, few-9)
prep_in(die-6, years-10)
det(boy-14, the-13)
nsubj(has-15, boy-14)
conj_but(die-6, has-15)
poss(life-18, his-16)
amod(life-18, whole-17)
dobj(has-15, life-18)
prep_ahead_of(life-18, him-21)

