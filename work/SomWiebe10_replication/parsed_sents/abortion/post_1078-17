(ROOT
  (S
    (NP (DT Every) (NN year))
    (NP
      (QP (CD 1.5) (CD million))
      (NNS babies))
    (VP (VBP are)
      (VP (VBN killed)
        (PP (IN by)
          (NP (NN abortion)))))
    (. .)))

det(year-2, Every-1)
nsubjpass(killed-7, year-2)
tmod(killed-7, year-2)
number(million-4, 1.5-3)
num(babies-5, million-4)
nsubjpass(killed-7, babies-5)
auxpass(killed-7, are-6)
root(ROOT-0, killed-7)
agent(killed-7, abortion-9)

