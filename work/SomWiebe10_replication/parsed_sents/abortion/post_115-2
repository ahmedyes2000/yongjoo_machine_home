(ROOT
  (S
    (NP
      (NP (DT The) (NN fact))
      (SBAR (IN that)
        (S
          (NP (RB only) (JJ poor) (NNS women))
          (VP (VBP are)
            (VP (VBN denied)
              (NP (JJ reproductive) (NN freedom))
              (SBAR
                (WHADVP (WRB when))
                (S
                  (NP (NNS abortions))
                  (VP (VBP are)
                    (ADJP (JJ illegal))))))))))
    (VP (VBZ is)
      (ADJP (JJ unpersuasive)
        (PP (TO to)
          (NP
            (NP (DT those))
            (SBAR
              (WHNP (WP who))
              (S
                (VP (VBP oppose)
                  (NP
                    (NP (NN abortion))
                    (PP (IN on)
                      (NP (JJ moral) (NNS grounds)))))))))))
    (. .)))

det(fact-2, The-1)
nsubj(unpersuasive-16, fact-2)
complm(denied-8, that-3)
advmod(women-6, only-4)
amod(women-6, poor-5)
nsubjpass(denied-8, women-6)
auxpass(denied-8, are-7)
dep(fact-2, denied-8)
amod(freedom-10, reproductive-9)
dobj(denied-8, freedom-10)
advmod(illegal-14, when-11)
nsubj(illegal-14, abortions-12)
cop(illegal-14, are-13)
advcl(denied-8, illegal-14)
cop(unpersuasive-16, is-15)
root(ROOT-0, unpersuasive-16)
prep_to(unpersuasive-16, those-18)
nsubj(oppose-20, those-18)
rcmod(those-18, oppose-20)
dobj(oppose-20, abortion-21)
amod(grounds-24, moral-23)
prep_on(abortion-21, grounds-24)

