(ROOT
  (S
    (S
      (S
        (VP (TO To)
          (VP (VB say)
            (ADVP (RB otherwise)))))
      (VP (VBZ is)
        (S
          (VP (TO to)
            (VP (VB deny)
              (NP (DT that))
              (NP (PRP she))
              (, ,)
              (NP (PRP yourself)))))))
    (, ,)
    (CC or)
    (S
      (NP (NN anyone) (RB else))
      (VP (VBZ has)
        (NP (DT the) (NN right)
          (S
            (VP (TO to)
              (VP (VB govern)
                (NP (PRP$ their) (NNS bodies))))))))
    (. .)))

aux(say-2, To-1)
csubj(is-4, say-2)
advmod(say-2, otherwise-3)
root(ROOT-0, is-4)
aux(deny-6, to-5)
