(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP (VBD decided)
          (SBAR (IN that)
            (S
              (NP (PRP you))
              (VP (VBD wanted)
                (S
                  (VP (TO to)
                    (VP (VB get)
                      (NP (PRP$ your) (NN dog))
                      (ADVP
                        (NP (DT an) (NN abortion))
                        (RB that)))))))))))
    (VP (MD would)
      (VP (VB end)
        (S
          (NP (PRP you))
          (VP (VBG being)
            (VP (VBN put)
              (PP (IN in)
                (NP (NN Jail))))))))
    (. .)))

