(ROOT
  (S
    (S
      (VP (VB Read)
        (NP (DT the) (JJ entire) (NN paragraph))))
    (CC and)
    (S
      (NP (PRP you))
      (VP (MD will)
        (VP (VB see)
          (NP (PRP$ my) (NN response)))))
    (. .)))

root(ROOT-0, Read-1)
det(paragraph-4, the-2)
amod(paragraph-4, entire-3)
dobj(Read-1, paragraph-4)
nsubj(see-8, you-6)
aux(see-8, will-7)
conj_and(Read-1, see-8)
poss(response-10, my-9)
dobj(see-8, response-10)

