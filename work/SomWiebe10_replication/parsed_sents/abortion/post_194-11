(ROOT
  (S
    (VP
      (VP (VB Be)
        (ADJP (JJ thankful)
          (SBAR
            (S
              (NP (DT the) (NNP Church))
              (VP (VBZ stands)
                (NP
                  (NP (NN firm))
                  (PP (IN on)
                    (NP
                      (NP (NNP God) (POS 's))
                      (NNS laws)))))))))
      (CC and)
      (VP (VB obey)
        (NP (PRP them))))
    (. .)))

cop(thankful-2, Be-1)
root(ROOT-0, thankful-2)
det(Church-4, the-3)
nsubj(stands-5, Church-4)
dep(thankful-2, stands-5)
dobj(stands-5, firm-6)
poss(laws-10, God-8)
