(ROOT
  (S
    (NP (PRP I))
    (VP (VBP think)
      (SBAR
        (S
          (NP (PRP we))
          (VP (MD should)
            (VP (VB treat)
              (S
                (NP (NN life))
                (ADJP (RBR more) (JJ sacredly))))))))
    (. .)))

nsubj(think-2, I-1)
root(ROOT-0, think-2)
nsubj(treat-5, we-3)
aux(treat-5, should-4)
ccomp(think-2, treat-5)
nsubj(sacredly-8, life-6)
advmod(sacredly-8, more-7)
xcomp(treat-5, sacredly-8)

