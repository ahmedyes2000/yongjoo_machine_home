(ROOT
  (S
    (NP
      (NP (DT those))
      (SBAR
        (WHNP (WP who))
        (S
          (VP (VBP regret)
            (SBAR
              (S
                (NP (PRP it))
                (VP (VBP do)
                  (SBAR (RB so) (IN because)
                    (S
                      (NP (PRP they))
                      (VP (VBP know)
                        (NP (DT the))))))))))))
    (VP (VBP have)
      (VP (VBN killed)
        (NP (PRP$ their) (NN baby))))
    (. .)))

nsubj(regret-3, those-1)
nsubj(killed-12, those-1)
rcmod(those-1, regret-3)
nsubj(do-5, it-4)
ccomp(regret-3, do-5)
