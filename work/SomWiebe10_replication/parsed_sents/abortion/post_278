poss(baby-14, their-13)
dobj(killed-12, baby-14)

(ROOT
  (S
    (NP (PRP$ my) (NN mother))
    (VP (VBZ says)
      (NP (DT this))
      (PP (IN after)
        (NP (PRP$ her) (NN abortion))
        (ADVP
          (NP (CD 27) (NNS years))
          (RB ago))))))

