(ROOT
  (S
    (NP
      (NP (DT The) (NN number))
      (PP (IN of)
        (NP (NNS abortions))))
    (VP (MD will)
      (ADVP (RB obviously))
      (VP (VB increase)))
    (. .)))

det(number-2, The-1)
nsubj(increase-7, number-2)
prep_of(number-2, abortions-4)
aux(increase-7, will-5)
advmod(increase-7, obviously-6)
root(ROOT-0, increase-7)

