  (S
    (S
      (NP (PRP I))
      (VP (VBP wish)
        (PRN
          (S
            (NP (EX there))
            (VP (VBD were)
              (NP
                (NP (DT a) (NN preview))
                (SBAR
                  (S
                    (VP (VBP feature))
                    (: :))))))
          (-RRB- -RRB-))
        (NP (PRP I))
        ('' ')))
    (NP (NNP l))
    (VP (VBP try)
      (ADVP (RB again)))
    (. .)))

