(ROOT
  (S
    (NP (DT These) (NNS decisions))
    (VP (VBP are)
      (ADVP (RBS best))
      (VP (VBN left)
        (PRT (RP up))
        (PP (TO to)
          (NP
            (NP (NNS doctors))
            (CC and)
            (NP (PRP$ their) (NNS patients))))))
    (. .)))

det(decisions-2, These-1)
nsubjpass(left-5, decisions-2)
auxpass(left-5, are-3)
advmod(left-5, best-4)
root(ROOT-0, left-5)
prt(left-5, up-6)
prep_to(left-5, doctors-8)
poss(patients-11, their-10)
prep_to(left-5, patients-11)
conj_and(doctors-8, patients-11)

