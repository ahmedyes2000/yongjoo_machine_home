(ROOT
  (S
    (SBAR
      (WHADVP (WRB When))
      (S
        (NP (PRP one))
        (VP (VBZ trusts)
          (NP
            (NP (NNP God) (POS 's))
            (NN sovereignty)))))
    (NP (PRP we))
    (VP (VBP know)
      (SBAR (IN that)
        (S
          (NP
            (NP (RB even) (NN pregnancy))
            (PP (IN from)
              (NP (NN rape))))
          (VP (MD can)
            (VP (VB bring)
              (NP (NN hope)
                (CC and)
                (NN meaning)))))))
    (. .)))

advmod(trusts-3, When-1)
nsubj(trusts-3, one-2)
advcl(know-8, trusts-3)
poss(sovereignty-6, God-4)
dobj(trusts-3, sovereignty-6)
nsubj(know-8, we-7)
root(ROOT-0, know-8)
complm(bring-15, that-9)
advmod(pregnancy-11, even-10)
nsubj(bring-15, pregnancy-11)
