partmod(fetus-12, Forcing-13)
det(woman-15, a-14)
dobj(Forcing-13, woman-15)
aux(continue-17, to-16)
infmod(woman-15, continue-17)
det(pregnancy-20, an-18)
amod(pregnancy-20, unwanted-19)
nsubj(subjugates-21, pregnancy-20)
ccomp(continue-17, subjugates-21)
det(woman-23, a-22)
dobj(subjugates-21, woman-23)
det(fetus-26, the-25)
prep_to(subjugates-21, fetus-26)

