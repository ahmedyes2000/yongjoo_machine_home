(ROOT
  (S
    (NP (NNP Women))
    (VP (MD must)
      (VP (VB control)
        (NP (PRP$ their) (NNS bodies)
          (CC or)
          (NN risk))
        (S
          (VP (VBG becoming)
            (NP
              (NP (NNS servants))
              (PP (IN of)
                (NP
                  (NP (DT the) (NN fetus))
                  (VP (VBG Forcing)
                    (NP (DT a) (NN woman)
                      (S
                        (VP (TO to)
                          (VP (VB continue)
                            (SBAR
                              (S
                                (NP (DT an) (JJ unwanted) (NN pregnancy))
                                (VP (VBZ subjugates)
                                  (NP (DT a) (NN woman))
                                  (PP (TO to)
                                    (NP (DT the) (NN fetus))))))))))))))))))
    (. .)))

nsubj(control-3, Women-1)
aux(control-3, must-2)
root(ROOT-0, control-3)
poss(bodies-5, their-4)
dobj(control-3, bodies-5)
dobj(control-3, risk-7)
conj_or(bodies-5, risk-7)
xcomp(control-3, becoming-8)
attr(becoming-8, servants-9)
det(fetus-12, the-11)
