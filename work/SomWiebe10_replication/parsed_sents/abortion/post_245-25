(ROOT
  (S
    (NP (PRP We))
    (ADVP (DT all))
    (VP (VBP place)
      (NP (JJR greater) (NN value))
      (PP (IN upon)
        (NP
          (NP (DT those))
          (SBAR
            (S
              (NP (PRP we))
              (VP (VBP love)
                (CC and)
                (VBP care)
                (PP (IN about))))))))
    (. .)))

nsubj(place-3, We-1)
advmod(place-3, all-2)
root(ROOT-0, place-3)
amod(value-5, greater-4)
dobj(place-3, value-5)
prep_upon(place-3, those-7)
prep_about(love-9, those-7)
nsubj(love-9, we-8)
nsubj(care-11, we-8)
rcmod(those-7, love-9)
rcmod(those-7, care-11)
conj_and(love-9, care-11)

