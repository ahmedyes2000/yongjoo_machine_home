  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP (MD can)
          (VP (VB do)
            (NP
              (NP (DT either))
              (PP (IN of)
                (NP (DT these))))))))
    (, ,)
    (NP (PRP I))
    (VP (MD will)
      (VP (VB be)
        (ADJP (JJ astounded)
          (PP (IN about)
            (SBAR
              (WHADVP (WRB how))
              (S
                (VP (VBZ emotionless)
                  (SBAR
                    (S
                      (NP (PRP you))
                      (VP (VBP are)))))))))))
    (. .)))

