(ROOT
  (S
    (NP
      (NP (DT The) (NN value))
      (PP (IN of)
        (NP (DT a) (JJ new) (JJ human) (NN life))))
    (VP (VBZ is)
      (SBAR
        (WHADVP (WRB where))
        (S
          (NP (PRP we))
          (VP (VBP disagree)))))
    (. .)))

det(value-2, The-1)
nsubj(is-8, value-2)
det(life-7, a-4)
amod(life-7, new-5)
amod(life-7, human-6)
prep_of(value-2, life-7)
root(ROOT-0, is-8)
advmod(disagree-11, where-9)
nsubj(disagree-11, we-10)
advcl(is-8, disagree-11)

