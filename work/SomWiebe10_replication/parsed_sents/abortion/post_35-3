(ROOT
  (S
    (ADVP (RB Clearly))
    (, ,)
    (PP (IN in)
      (NP (NN war)))
    (, ,)
    (NP (NNS humans))
    (ADVP (RB frequently))
    (VP (VBZ justify)
      (S
        (VP (VBG killing)
          (NP (JJ other) (JJ human) (NNS beings)))))
    (. .)))

advmod(justify-8, Clearly-1)
prep_in(justify-8, war-4)
nsubj(justify-8, humans-6)
advmod(justify-8, frequently-7)
root(ROOT-0, justify-8)
xcomp(justify-8, killing-9)
amod(beings-12, other-10)
amod(beings-12, human-11)
dobj(killing-9, beings-12)

