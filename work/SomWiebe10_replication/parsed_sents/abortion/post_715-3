(ROOT
  (S
    (S
      (NP (PRP$ Our) (NNS emotions))
      (VP (VBP exist)
        (S
          (VP (TO to)
            (VP (VB aid)
              (NP (PRP us)))))))
    (, ,)
    (CC but)
    (S
      (NP (PRP they))
      (VP (VBP are) (RB not)
        (ADJP (JJ perfect))))
    (. .)))

poss(emotions-2, Our-1)
nsubj(exist-3, emotions-2)
xsubj(aid-5, emotions-2)
root(ROOT-0, exist-3)
aux(aid-5, to-4)
xcomp(exist-3, aid-5)
dobj(aid-5, us-6)
nsubj(perfect-12, they-9)
cop(perfect-12, are-10)
neg(perfect-12, not-11)
conj_but(exist-3, perfect-12)

