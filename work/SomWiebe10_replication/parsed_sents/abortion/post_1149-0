(ROOT
  (S
    (S
      (NP (PRP I))
      (VP (VBP do)
        (PP (TO to))))
    (: ...)
    (S
      (NP (PRP you))
      (VP (VBP need)
        (S
          (VP (TO to)
            (VP
              (VP (VB pray)
                (PP (TO to)
                  (NP (NNP God))))
              (CC and)
              (VP (VB realize)
                (SBAR (IN that)
                  (S
                    (NP (DT that))
                    (VP (VBZ is)
                      (ADJP (JJ wrong)))))))))))
    (. .)))

nsubj(do-2, I-1)
root(ROOT-0, do-2)
prep(do-2, to-3)
nsubj(need-6, you-5)
xsubj(pray-8, you-5)
parataxis(do-2, need-6)
aux(pray-8, to-7)
xcomp(need-6, pray-8)
prep_to(pray-8, God-10)
xcomp(need-6, realize-12)
conj_and(pray-8, realize-12)
complm(wrong-16, that-13)
nsubj(wrong-16, that-14)
cop(wrong-16, is-15)
ccomp(realize-12, wrong-16)

