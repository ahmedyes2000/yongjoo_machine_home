(ROOT
  (S
    (NP (DT This))
    (VP (VBZ is)
      (ADJP (RB only) (JJ possible)
        (PP (IN through)
          (NP (NN repentance)
            (CC and)
            (NN faith)))))
    (. .)))

nsubj(possible-4, This-1)
cop(possible-4, is-2)
advmod(possible-4, only-3)
root(ROOT-0, possible-4)
prep_through(possible-4, repentance-6)
prep_through(possible-4, faith-8)
conj_and(repentance-6, faith-8)

