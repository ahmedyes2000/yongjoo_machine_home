(ROOT
  (S
    (ADVP (RB Yet))
    (, ,)
    (NP
      (NP (DT the) (NN subject))
      (PP (IN of)
        (NP (NN abortion))))
    (VP (VBD was)
      (VP
        (ADVP (RB never))
        (VBN addressed)))
    (. .)))

advmod(addressed-9, Yet-1)
det(subject-4, the-3)
nsubjpass(addressed-9, subject-4)
prep_of(subject-4, abortion-6)
auxpass(addressed-9, was-7)
neg(addressed-9, never-8)
root(ROOT-0, addressed-9)

