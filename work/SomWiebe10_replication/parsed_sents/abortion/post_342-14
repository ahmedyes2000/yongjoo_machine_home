(ROOT
  (S ('' '')
    (S
      (NP (NN defense))
      (, ,)
      (ADVP (WRB how))
      (VP (VBP do)
        (S
          (NP (PRP we))
          (VP (VB respond)
            (PP (TO to)
              (NP (NN something)))))))
    (IN like)
    (S
      (NP (DT a) (NN heart))
      (VP (VBP attack)))
    (. .)))

nsubj(do-5, defense-2)
advmod(do-5, how-4)
root(ROOT-0, do-5)
nsubj(respond-7, we-6)
ccomp(do-5, respond-7)
prep_to(respond-7, something-9)
dep(do-5, like-10)
det(heart-12, a-11)
nsubj(attack-13, heart-12)
ccomp(do-5, attack-13)

