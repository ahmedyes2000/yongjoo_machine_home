(ROOT
  (S
    (S
      (NP (PRP You))
      (VP (MD can)
        (VP (VB prove)
          (SBAR (IN whether)
            (CC or)
            (RB not)
            (S
              (NP (PRP they))
              (VP (VBP 're)
                (ADJP (JJ human))))))))
    (: ;)
    (S
      (NP (PRP I))
      (VP (VBP have)))
    (. .)))

nsubj(prove-3, You-1)
aux(prove-3, can-2)
root(ROOT-0, prove-3)
