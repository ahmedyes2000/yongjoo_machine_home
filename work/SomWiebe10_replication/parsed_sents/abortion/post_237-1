(ROOT
  (S ('' '')
    (NP (PRP It))
    (VP (VBZ is)
      (ADJP (JJ true))
      (SBAR (IN that)
        (S
          (S
            (VP
              (ADVP (RB indeed))
              (VBG giving)
              (NP (NN birth))))
          (VP (VBZ is)
            (NP
              (NP (NNP NOT))
              (PP (IN without)
                (NP (PRP$ its) (NNS risks))))))))
    (. .)))

nsubj(true-4, It-2)
cop(true-4, is-3)
root(ROOT-0, true-4)
complm(NOT-10, that-5)
advmod(giving-7, indeed-6)
csubj(NOT-10, giving-7)
dobj(giving-7, birth-8)
cop(NOT-10, is-9)
ccomp(true-4, NOT-10)
poss(risks-13, its-12)
prep_without(NOT-10, risks-13)

