(ROOT
  (S
    (NP (PRP I))
    (VP (VBD 'd)
      (S
        (ADVP (RB rather))
        (NP (PRP we))
        (VP (VBD told)
          (NP (DT the) (NNS kids))))
      (NP
        (NP (DT the) (JJ true) (NN cost))
        (PP (IN of)
          (NP (JJ premarital) (NN sex)))))
    (. .)))

nsubj('d-2, I-1)
root(ROOT-0, 'd-2)
advmod(told-5, rather-3)
nsubj(told-5, we-4)
ccomp('d-2, told-5)
det(kids-7, the-6)
dobj(told-5, kids-7)
det(cost-10, the-8)
amod(cost-10, true-9)
dobj('d-2, cost-10)
amod(sex-13, premarital-12)
prep_of(cost-10, sex-13)

