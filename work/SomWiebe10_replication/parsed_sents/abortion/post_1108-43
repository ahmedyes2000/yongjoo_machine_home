  (S (CC But)
    (PP (IN for)
      (NP
        (NP (DT the) (NN sake))
        (PP (IN of)
          (NP (NN society)))))
    (, ,)
    (NP (NNP let) (POS 's))
    (VP (VBP teach)
      (NP (DT all) (NNS children))
      (PRN (-LRB- -LRB-)
        (PP (VBG including)
          (NP (NNP yours)))
        (-RRB- -RRB-))
      (SBAR
        (WHNP (WP what))
        (S
          (NP (PRP it))
          (VP (VBZ means)
            (S
              (VP (TO to)
                (VP (VB be)
                  (ADJP (JJ responsible)
                    (PP (IN with)
                      (S
                        (VP (VBG regards)
                          (PP (TO to)
                            (NP (NN sex))))))))))))))
    (. .)))

