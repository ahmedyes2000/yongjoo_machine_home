(ROOT
  (S
    (NP (JJ Pro-life) (NNS people))
    (VP (VBP have)
      (ADVP (RB already))
      (VP (VBN taken)
        (NP (JJ heroic) (NNS steps))
        (, ,)
        (ADVP (RB often)
          (PP (IN at)
            (NP (JJ great) (JJ personal) (NN sacrifice))))
        (, ,)
        (S
          (VP (TO to)
            (VP (VB provide)
              (PP (IN for)
                (NP (JJ unwed) (NNS mothers))))))))
    (. .)))

amod(people-2, Pro-life-1)
nsubj(taken-5, people-2)
xsubj(provide-16, people-2)
aux(taken-5, have-3)
advmod(taken-5, already-4)
root(ROOT-0, taken-5)
amod(steps-7, heroic-6)
dobj(taken-5, steps-7)
advmod(taken-5, often-9)
amod(sacrifice-13, great-11)
amod(sacrifice-13, personal-12)
prep_at(often-9, sacrifice-13)
aux(provide-16, to-15)
xcomp(taken-5, provide-16)
amod(mothers-19, unwed-18)
prep_for(provide-16, mothers-19)

