(ROOT
  (S
    (NP (PRP It))
    (ADVP (RB also))
    (VP (VBZ allows)
      (S
        (NP (NNS people))
        (VP (TO to)
          (VP (VB choose)
            (S
              (NP (NNP NOT))
              (VP (TO to)
                (VP (VB have)
                  (NP (DT an) (NN abortion)))))))))
    (. .)))

nsubj(allows-3, It-1)
advmod(allows-3, also-2)
root(ROOT-0, allows-3)
nsubj(choose-6, people-4)
aux(choose-6, to-5)
xcomp(allows-3, choose-6)
nsubj(have-9, NOT-7)
aux(have-9, to-8)
xcomp(choose-6, have-9)
det(abortion-11, an-10)
dobj(have-9, abortion-11)

