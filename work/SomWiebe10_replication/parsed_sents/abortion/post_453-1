(ROOT
  (S
    (ADVP (RB Heck))
    (, ,)
    (NP (NNS people))
    (VP (VBP suffer)
      (ADJP (JJ physically)
        (CC and)
        (RB psychologically))
      (PP (IN after)
        (NP (JJ open-heart) (NN surgery))))
    (. .)))

advmod(suffer-4, Heck-1)
nsubj(suffer-4, people-3)
root(ROOT-0, suffer-4)
acomp(suffer-4, physically-5)
acomp(suffer-4, psychologically-7)
advmod(physically-5, psychologically-7)
conj_and(physically-5, psychologically-7)
amod(surgery-10, open-heart-9)
prep_after(suffer-4, surgery-10)

