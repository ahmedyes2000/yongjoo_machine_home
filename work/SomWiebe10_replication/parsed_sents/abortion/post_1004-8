(ROOT
  (S
    (NP (DT A) (JJ self-aware) (NN human))
    (VP (MD should) (RB not)
      (VP (VB be)
        (VP (VBN aborted))))
    (. .)))

det(human-3, A-1)
amod(human-3, self-aware-2)
nsubjpass(aborted-7, human-3)
aux(aborted-7, should-4)
neg(aborted-7, not-5)
auxpass(aborted-7, be-6)
root(ROOT-0, aborted-7)

