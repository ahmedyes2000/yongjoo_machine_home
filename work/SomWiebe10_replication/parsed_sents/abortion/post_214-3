(ROOT
  (S
    (NP (EX There))
    (VP (MD will)
      (VP (VB be)
        (NP
          (NP (JJR more) (NNS abortions))
          (PP (IN in)
            (NP (DT this) (NN country))))
        (SBAR (IN if)
          (S
            (NP (NNP Barack) (NNP Obama))
            (VP (VBZ becomes)
              (NP (NNP President)))))))
    (. .)))

expl(abortions-5, There-1)
aux(abortions-5, will-2)
cop(abortions-5, be-3)
amod(abortions-5, more-4)
root(ROOT-0, abortions-5)
det(country-8, this-7)
