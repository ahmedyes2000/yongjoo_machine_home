(ROOT
  (S
    (NP (PRP He))
    (VP
      (VP (MD can)
        (VP (VB sense)
          (NP (NN discomfort)
            (CC and)
            (NN hunger))))
      (CC and)
      (VP (MD will)
        (VP (VB react))))
    (. .)))

nsubj(sense-3, He-1)
nsubj(react-9, He-1)
aux(sense-3, can-2)
root(ROOT-0, sense-3)
dobj(sense-3, discomfort-4)
dobj(sense-3, hunger-6)
conj_and(discomfort-4, hunger-6)
aux(react-9, will-8)
conj_and(sense-3, react-9)

