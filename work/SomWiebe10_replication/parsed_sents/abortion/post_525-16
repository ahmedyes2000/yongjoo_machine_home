  (FRAG (CC And)
    (SBAR (IN if)
      (S
        (NP (PRP you))
        (VP (VBP hold)
          (SBAR (IN that)
            (S
              (NP (PRP$ your) (NN life))
              (VP (VBZ is)
                (ADJP
                  (ADJP (NN worth) (JJR less))
                  (SBAR (IN than)
                    (S
                      (NP (NNS others))
                      (VP (VBZ lives)))))
                (, ,)
                (SBAR
                  (WHADVP (WRB why))
                  (S
                    (NP (RB then))
                    (VP (MD should)
                      (NP (PRP you))
                      (S
                        (VP (VB try)
                          (CC and)
                          (VB force)
                          (NP (PRP$ your) (NN belief))
                          (PP (IN upon)
                            (NP (PRP me))))))))))))))
    (. ?)))

