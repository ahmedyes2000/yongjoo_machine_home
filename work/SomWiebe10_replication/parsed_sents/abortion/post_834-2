(ROOT
  (S
    (NP (PRP I))
    (VP (VBP understand)
      (SBAR
        (S
          (NP (PRP you))
          (VP (VBP think)
            (SBAR
              (S
                (NP (NN life))
                (VP (VBZ is)
                  (ADJP (RB really) (JJ important)))))))))
    (. .)))

nsubj(understand-2, I-1)
root(ROOT-0, understand-2)
nsubj(think-4, you-3)
ccomp(understand-2, think-4)
nsubj(important-8, life-5)
cop(important-8, is-6)
advmod(important-8, really-7)
ccomp(think-4, important-8)

