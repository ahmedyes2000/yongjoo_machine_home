  (S
    (NP (PRP it))
    (VP (VBZ 's)
      (NP
        (NP (DT the) (NN freedom))
        (PP (IN of)
          (NP (JJ simple) (NNS BEING)))
        (SBAR
          (WHNP (WDT that))
          (PRN
            (S
              (NP (PRP we))
              (VP (VBP are)
                (VP (VBG talking) (IN about))))
            (-RRB- -RRB-))
          (S
            (VP (VBZ is)
              (ADJP (RB essentially) (JJ meaningless)))))))
    (. .)))

