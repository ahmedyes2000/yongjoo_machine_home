      (VP (VBZ seems)
        (S
          (VP (TO to)
            (VP (VB be)
              (VP (VBG lacking)
                (PP (IN in)
                  (NP (NN mine)))
                (, ,)
                (ADVP (RB however))))))))
    (, ,)
    (NP
      (NP (DT the) (JJ great) (NN thing))
      (PP (IN about)
        (NP (NN english))))
    (VP (VBZ is)
      (SBAR (IN that)
        (S
          (NP (PRP I))
          (VP (MD can)
