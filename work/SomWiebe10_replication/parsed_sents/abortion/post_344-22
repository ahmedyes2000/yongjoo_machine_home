(ROOT
  (INTJ (UH .)))


(ROOT
  (S
    (NP (PRP you))
    (VP (VBP 're)
      (VP (VBG changing)
        (NP (PRP$ your) (NNS words))
        (S
          (VP (TO to)
            (VP (VB suit)
              (NP (PRP yourself)))))))
    (. .)))

nsubj(changing-3, you-1)
aux(changing-3, 're-2)
root(ROOT-0, changing-3)
poss(words-5, your-4)
dobj(changing-3, words-5)
aux(suit-7, to-6)
xcomp(changing-3, suit-7)
dobj(suit-7, yourself-8)

