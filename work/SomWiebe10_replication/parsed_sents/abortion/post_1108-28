  (S
    (NP (PRP We))
    (VP (VBP 're)
      (VP (VBG providing)
        (NP (JJ sexual) (NN education))
        (SBAR (RB so) (IN that)
          (S
            (SBAR (IN if)
              (S
                (NP (PRP they))
                (VP (VBP make)
                  (NP (DT that) (NN choice)))))
            (, ,)
            (NP (PRP they))
            (VP (MD will)
              (VP (VB be)
                (VP (VBN informed)
                  (PP (IN on)
                    (SBAR
                      (WHADVP (WRB how))
                      (S
                        (VP (TO to)
                          (VP (VB be)
                            (ADJP (JJ healthy)
                              (CC and)
                              (JJ responsible))))))))))))))
    (. .)))

