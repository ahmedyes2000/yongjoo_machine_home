(ROOT
  (S
    (NP (DT That) (NN option))
    (VP (MD should)
      (VP (VB be)
        (ADJP (JJ available)
          (PP (TO to)
            (NP (DT all) (NNS women))))
        (, ,)
        (SBAR (IN whether)
          (CC or)
          (RB not)
          (S
            (NP (PRP you))
            (ADVP (RB personally))
            (VP (VBP agree)
              (PP (IN with)
                (NP (PRP$ their) (JJ ultimate) (NN decision))))))))
    (. .)))

det(option-2, That-1)
nsubj(available-5, option-2)
aux(available-5, should-3)
cop(available-5, be-4)
root(ROOT-0, available-5)
det(women-8, all-7)
prep_to(available-5, women-8)
complm(agree-15, whether-10)
ccomp(available-5, not-12)
conj_or(agree-15, not-12)
nsubj(agree-15, you-13)
advmod(agree-15, personally-14)
ccomp(available-5, agree-15)
poss(decision-19, their-17)
amod(decision-19, ultimate-18)
prep_with(agree-15, decision-19)

