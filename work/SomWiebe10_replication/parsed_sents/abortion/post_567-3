(ROOT
  (S
    (NP (DT That))
    (VP (VBZ is)
      (SBAR
        (WHNP (WP what))
        (S
          (VP (VBZ happens)
            (PP (IN in)
              (NP
                (NP (NNS courts))
                (PP (IN of)
                  (NP (NN law)))))))))
    (. .)))

nsubj(is-2, That-1)
root(ROOT-0, is-2)
nsubj(happens-4, what-3)
ccomp(is-2, happens-4)
prep_in(happens-4, courts-6)
prep_of(courts-6, law-8)

