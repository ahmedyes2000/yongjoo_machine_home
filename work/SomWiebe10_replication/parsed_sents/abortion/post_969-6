(ROOT
  (S
    (NP
      (NP (DT This) (NN story))
      (SBAR (IN that)
        (S
          (NP (PRP I))
          (ADVP (RB just))
          (VP (VBD told)
            (NP (PRP you))))))
    (VP (VBZ is)
      (ADJP (JJ true)))
    (. .)))

det(story-2, This-1)
nsubj(true-9, story-2)
complm(told-6, that-3)
nsubj(told-6, I-4)
advmod(told-6, just-5)
dep(story-2, told-6)
dobj(told-6, you-7)
cop(true-9, is-8)
root(ROOT-0, true-9)

