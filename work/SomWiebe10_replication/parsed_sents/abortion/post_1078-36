  (FRAG
    (ADVP (RB There))
    (NP
      (NP (JJ many) (NNS things))
      (SBAR (IN that)
        (S
          (NP (PRP we))
          (VP (MD can)
            (VP (VB do)
              (PP (IN about)
                (NP (NN abortion))))))))
    (. .)))

