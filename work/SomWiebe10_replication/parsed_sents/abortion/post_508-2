        (NP (DT the) (NNP MCP))
        (VP (VBD was)
          (VP (VBN legislated)))))
    (, ,)
    (NP (PRP it))
    (VP (VBD created)
      (NP (DT a) (JJ false) (NN majority))
      (PP (IN for)
        (NP (DT the) (JJ pro) (NN life) (NN side)))
      (PP (IN by)
        (S
          (VP (VBG having)
            (S
              (NP (PRP$ their) (NNS views))
              (VP (VB be)
                (NP
                  (NP (DT the) (NN default) (NN opinion))
                  (VP (VBG representing)
                    (NP
                      (NP (DT the) (NNS citizens))
                      (PP (IN of)
                        (NP (DT this) (NN country))))))))))))
    (. .)))

