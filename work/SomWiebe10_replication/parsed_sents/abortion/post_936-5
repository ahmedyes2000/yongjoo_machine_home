(ROOT
  (S
    (NP (DT The) (NNS numbers))
    (VP (MD would)
      (VP (VB DECREASE)
        (NP (NNP SIGNIFICANTLY))
        (SBAR (IN if)
          (S
            (NP (NN abortion))
            (VP (VBD was)
              (ADJP (JJ illegal)))))))
    (. .)))

det(numbers-2, The-1)
nsubj(DECREASE-4, numbers-2)
aux(DECREASE-4, would-3)
root(ROOT-0, DECREASE-4)
dobj(DECREASE-4, SIGNIFICANTLY-5)
mark(illegal-9, if-6)
nsubj(illegal-9, abortion-7)
cop(illegal-9, was-8)
advcl(DECREASE-4, illegal-9)

