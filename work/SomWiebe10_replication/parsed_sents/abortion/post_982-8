(ROOT
  (S
    (ADVP (RB Practically))
    (NP (DT all) (NNS Christians))
    (VP (VBP are)
      (PP (IN against)
        (NP (NNS abortions)))
      (, ,)
      (SBAR (IN because)
        (S
          (NP (PRP it))
          (VP (VBZ goes)
            (PP (IN against)
              (SBAR
                (WHNP (WP what))
                (S
                  (NP (DT the) (NNP Bible))
                  (VP (VBZ says)))))))))
    (. .)))

advmod(are-4, Practically-1)
det(Christians-3, all-2)
nsubj(are-4, Christians-3)
root(ROOT-0, are-4)
prep_against(are-4, abortions-6)
mark(goes-10, because-8)
nsubj(goes-10, it-9)
advcl(are-4, goes-10)
dobj(says-15, what-12)
det(Bible-14, the-13)
nsubj(says-15, Bible-14)
prepc_against(goes-10, says-15)

