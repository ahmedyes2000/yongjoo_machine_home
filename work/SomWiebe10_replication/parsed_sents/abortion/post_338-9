(ROOT
  (S
    (ADVP (RB Similarly))
    (, ,)
    (NP
      (NP (NNS thousands))
      (PP (IN of)
        (NP (NNS children))))
    (VP (VBP are)
      (PP (IN in)
        (NP
          (NP (NN need))
          (PP (IN of)
            (NP (NN bone) (NN marrow))))))
    (. .)))

advmod(are-6, Similarly-1)
nsubj(are-6, thousands-3)
prep_of(thousands-3, children-5)
root(ROOT-0, are-6)
prep_in(are-6, need-8)
nn(marrow-11, bone-10)
prep_of(need-8, marrow-11)

