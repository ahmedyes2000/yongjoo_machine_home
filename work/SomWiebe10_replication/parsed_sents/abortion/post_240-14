(ROOT
  (S
    (NP
      (NP (NNS Millions))
      (PP (IN of)
        (NP
          (NP (NNS children))
          (PP (IN in)
            (NP (DT this) (NN country))))))
    (VP (VBP go)
      (S
        (VP (TO to)
          (VP (VB bed)
            (S
              (NP (DT every) (NN night))
              (ADJP (JJ hungry)))))))
    (. .)))

nsubj(go-7, Millions-1)
xsubj(bed-9, Millions-1)
prep_of(Millions-1, children-3)
det(country-6, this-5)
prep_in(children-3, country-6)
root(ROOT-0, go-7)
aux(bed-9, to-8)
xcomp(go-7, bed-9)
det(night-11, every-10)
nsubj(hungry-12, night-11)
xcomp(bed-9, hungry-12)

