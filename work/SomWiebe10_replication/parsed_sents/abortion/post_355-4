(ROOT
  (S
    (NP (CD One) (NN day))
    (, ,)
    (NP (PRP you))
    (VP
      (VP (VBP wake)
        (PRT (RP up)))
      (CC and)
      (VP (VBP find)
        (NP
          (NP (DT a) (NN human))
          (VP (VBG being)
            (VP (VBN attached)
              (PP (TO to)
                (NP
                  (NP (PRP you))
                  (PP (IN via)
                    (NP (JJ life-support) (NN system))))))))))
    (. .)))

num(day-2, One-1)
tmod(wake-5, day-2)
nsubj(wake-5, you-4)
nsubj(find-8, you-4)
root(ROOT-0, wake-5)
prt(wake-5, up-6)
conj_and(wake-5, find-8)
