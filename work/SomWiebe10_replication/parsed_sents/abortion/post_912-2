(ROOT
  (S (CC But)
    (NP (PRP it))
    (VP (VBZ remains)
      (NP (DT a) (JJ personal) (NN issue))
      (, ,)
      (SBAR (IN that)
        (S
          (NP (RB only)
            (NP (DT the) (NN woman))
            (CC and)
            (NP (PRP$ her) (NN doctor)))
          (VP (MD can)
            (ADVP (RB truly))
            (VP (VB understand))))))
    (. .)))

cc(issue-6, But-1)
nsubj(issue-6, it-2)
cop(issue-6, remains-3)
det(issue-6, a-4)
amod(issue-6, personal-5)
root(ROOT-0, issue-6)
complm(understand-17, that-8)
advmod(woman-11, only-9)
det(woman-11, the-10)
nsubj(understand-17, woman-11)
poss(doctor-14, her-13)
conj_and(woman-11, doctor-14)
nsubj(understand-17, doctor-14)
aux(understand-17, can-15)
advmod(understand-17, truly-16)
ccomp(issue-6, understand-17)

