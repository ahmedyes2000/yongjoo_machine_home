(ROOT
  (S
    (NP (PRP I))
    (VP (MD ca) (RB n't)
      (VP (VB keep)
        (S
          (VP (VBG arguing)
            (PP (IN with)
              (NP (PRP you)))))
        (SBAR (IN if)
          (S
            (NP (PRP you))
            (VP (VBP continue)
              (S
                (VP (TO to)
                  (VP (VB take)
                    (NP (DT the) (NN debate))
                    (PP (IN off)
                      (NP (NN topic)))))))))))
    (. .)))

nsubj(keep-4, I-1)
aux(keep-4, ca-2)
neg(keep-4, n't-3)
root(ROOT-0, keep-4)
xcomp(keep-4, arguing-5)
prep_with(arguing-5, you-7)
mark(continue-10, if-8)
nsubj(continue-10, you-9)
xsubj(take-12, you-9)
advcl(keep-4, continue-10)
aux(take-12, to-11)
xcomp(continue-10, take-12)
det(debate-14, the-13)
dobj(take-12, debate-14)
prep_off(take-12, topic-16)

