(ROOT
  (S
    (PP (IN At)
      (NP (CD 21) (NNS days)))
    (, ,)
    (NP (DT the) (NN heartbeat))
    (VP (MD can)
      (VP (VB be)
        (VP (VBN heard))))
    (. .)))

num(days-3, 21-2)
prep_at(heard-9, days-3)
det(heartbeat-6, the-5)
nsubjpass(heard-9, heartbeat-6)
aux(heard-9, can-7)
auxpass(heard-9, be-8)
root(ROOT-0, heard-9)

