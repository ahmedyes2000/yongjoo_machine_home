(ROOT
  (S
    (NP (PRP I))
    (VP (VBP do) (RB not)
      (VP (VB believe)
        (SBAR (IN that)
          (S
            (VP (VBZ makes)
              (SBAR
                (S
                  (NP (PRP it))
                  (VP (VBP ok)
                    (S
                      (VP (TO to)
                        (VP (VB kill)
                          (NP (DT that) (NN person)))))))))))))
    (. .)))

nsubj(believe-4, I-1)
aux(believe-4, do-2)
neg(believe-4, not-3)
root(ROOT-0, believe-4)
complm(makes-6, that-5)
ccomp(believe-4, makes-6)
nsubj(ok-8, it-7)
xsubj(kill-10, it-7)
ccomp(makes-6, ok-8)
aux(kill-10, to-9)
xcomp(ok-8, kill-10)
det(person-12, that-11)
dobj(kill-10, person-12)

