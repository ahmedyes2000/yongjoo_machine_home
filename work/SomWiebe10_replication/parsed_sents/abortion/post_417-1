(ROOT
  (S
    (ADVP (RB Corpse))
    (, ,)
    (PP (IN by)
      (NP (NN definition)))
    (, ,)
    (VP (VBZ refers)
      (PP (TO to)
        (NP
          (NP (NN something))
          (SBAR
            (WHNP (WDT that))
            (S
              (VP
                (VP (VBD used)
                  (S
                    (VP (TO to)
                      (VP (VB be)
                        (ADJP (JJ alive))))))
                (CC and)
                (VP (VBZ is)
                  (ADVP (RB no) (RB longer)))))))))
    (. .)))

advmod(refers-6, Corpse-1)
prep_by(refers-6, definition-4)
root(ROOT-0, refers-6)
prep_to(refers-6, something-8)
nsubj(used-10, something-8)
nsubj(is-15, something-8)
rcmod(something-8, used-10)
aux(alive-13, to-11)
cop(alive-13, be-12)
xcomp(used-10, alive-13)
rcmod(something-8, is-15)
conj_and(used-10, is-15)
advmod(longer-17, no-16)
advmod(is-15, longer-17)

