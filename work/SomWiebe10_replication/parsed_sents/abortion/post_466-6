(ROOT
  (S
    (NP
      (NP (DT The)
        (ADJP (RB only) (JJ acceptable))
        (NNS times))
      (PP (IN for)
        (NP (DT an) (NN abortion))))
    (VP (VBP are)
      (UCP
        (NP (NN rape))
        (CC and)
        (SBAR (IN if)
          (S
            (S
              (VP (VBG carrying)
                (NP (DT the) (NN child))))
            (VP (MD could)
              (VP (VB kill)
                (CC or)
                (VB harm)
                (NP (PRP you))
                (ADVP (RB severely))))))))
    (. .)))

det(times-4, The-1)
advmod(acceptable-3, only-2)
amod(times-4, acceptable-3)
nsubj(rape-9, times-4)
det(abortion-7, an-6)
prep_for(times-4, abortion-7)
cop(rape-9, are-8)
root(ROOT-0, rape-9)
mark(kill-16, if-11)
