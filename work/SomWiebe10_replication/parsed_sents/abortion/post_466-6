csubj(harm-18, carrying-12)
det(child-14, the-13)
dobj(carrying-12, child-14)
aux(kill-16, could-15)
conj_and(rape-9, kill-16)
conj_and(rape-9, harm-18)
conj_or(kill-16, harm-18)
