(ROOT
  (S
    (NP (JJ Many) (NNS abortions))
    (VP (VBP are)
      (VP (VBN performed)
        (NP (JJ AFTER) (DT the) (JJ first) (NN trimester))
        (PRN (: --)
          (SBAR
            (ADVP (RB right) (RB up))
            (IN until)
            (S
              (NP (DT the) (NN baby))
              (VP (VBZ is)
                (ADJP (JJ viable)
                  (PP (IN outside)
                    (NP (DT the) (NN womb)))))))
          (: --))
        (PP (VBG including)
          (PP (IN by)
            (NP (JJ partial-birth) (NNS abortions))))))
    (. .)))

amod(abortions-2, Many-1)
nsubjpass(performed-4, abortions-2)
auxpass(performed-4, are-3)
root(ROOT-0, performed-4)
amod(trimester-8, AFTER-5)
det(trimester-8, the-6)
amod(trimester-8, first-7)
dobj(performed-4, trimester-8)
advmod(up-11, right-10)
advmod(viable-16, up-11)
dep(viable-16, until-12)
det(baby-14, the-13)
nsubj(viable-16, baby-14)
cop(viable-16, is-15)
parataxis(performed-4, viable-16)
det(womb-19, the-18)
prep_outside(viable-16, womb-19)
prep(performed-4, including-21)
pcomp(including-21, by-22)
amod(abortions-24, partial-birth-23)
prep_by(including-21, abortions-24)

