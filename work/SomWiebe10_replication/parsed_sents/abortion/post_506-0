(ROOT
  (S
    (NP
      (NP (DT Those))
      (SBAR
        (WHNP (WP who))
        (S
          (NP (NN abortion))
          (VP (MD should)
            (ADVP (RB never))
            (VP (VB be)
              (VP (VBN allowed)
                (PP (IN under)
                  (NP (DT any) (NN circumstance)))))))))
    (VP (VBP are)
      (PP (IN in)
        (NP (DT a) (JJ significant) (NN minority))))
    (. .)))

dobj(allowed-7, Those-1)
nsubj(are-11, Those-1)
rel(allowed-7, who-2)
nsubjpass(allowed-7, abortion-3)
aux(allowed-7, should-4)
neg(allowed-7, never-5)
auxpass(allowed-7, be-6)
rcmod(Those-1, allowed-7)
det(circumstance-10, any-9)
