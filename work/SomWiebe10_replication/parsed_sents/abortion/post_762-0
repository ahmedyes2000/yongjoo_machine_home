(ROOT
  (SBARQ
    (WHNP
      (WHADJP (WRB How)
        (ADJP (IN about)))
      (NP
        (NP (DT every) (NN sperm)
          (CC or)
          (NN egg))
        (NP (DT that))))
    (SQ (VBZ is)
      (VP (VBN wasted)))
    (. ?)))

advmod(about-2, How-1)
amod(sperm-4, about-2)
det(sperm-4, every-3)
nsubjpass(wasted-9, sperm-4)
conj_or(sperm-4, egg-6)
nsubjpass(wasted-9, egg-6)
dep(sperm-4, that-7)
auxpass(wasted-9, is-8)
root(ROOT-0, wasted-9)

