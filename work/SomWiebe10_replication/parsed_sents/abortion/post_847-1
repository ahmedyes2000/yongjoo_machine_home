(ROOT
  (SBARQ
    (SBAR (IN If)
      (S
        (NP (PRP it))
        (VP (VBZ is) (RB n't)
          (ADJP (JJ alive)))))
    (, ,)
    (WHADVP (WRB how))
    (SQ (MD can)
      (NP (PRP you))
      (VP (VB kill)
        (NP (PRP it))))
    (. ?)))

mark(alive-5, If-1)
nsubj(alive-5, it-2)
cop(alive-5, is-3)
neg(alive-5, n't-4)
advcl(kill-10, alive-5)
advmod(kill-10, how-7)
aux(kill-10, can-8)
nsubj(kill-10, you-9)
root(ROOT-0, kill-10)
dobj(kill-10, it-11)

