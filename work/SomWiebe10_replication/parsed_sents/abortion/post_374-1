(ROOT
  (S
    (PP (IN In)
      (NP (DT this) (NN format)))
    (NP (PRP it))
    (ADVP (RB typically))
    (VP (VBZ indicates)
      (NP
        (NP (DT the) (NNS words))
        (SBAR
          (WHPP (TO to)
            (WHNP (WDT which)))
          (S
            (NP (DT the) (NN author))
            (VP (VBZ is)
              (VP (VBG responding)))))))
    (. .)))

det(format-3, this-2)
prep_in(indicates-6, format-3)
nsubj(indicates-6, it-4)
advmod(indicates-6, typically-5)
root(ROOT-0, indicates-6)
det(words-8, the-7)
dobj(indicates-6, words-8)
rel(responding-14, which-10)
det(author-12, the-11)
nsubj(responding-14, author-12)
aux(responding-14, is-13)
rcmod(words-8, responding-14)

