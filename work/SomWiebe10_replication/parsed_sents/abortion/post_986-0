(ROOT
  (S
    (NP (EX There))
    (VP (VBZ is)
      (NP
        (NP (DT a) (NN difference))
        (PP (IN between)
          (S
            (VP (VBG refuting)
              (CC and)
              (VBG ignoring))))))
    (. .)))

expl(is-2, There-1)
root(ROOT-0, is-2)
det(difference-4, a-3)
nsubj(is-2, difference-4)
prepc_between(difference-4, refuting-6)
prepc_between(difference-4, ignoring-8)
conj_and(refuting-6, ignoring-8)

