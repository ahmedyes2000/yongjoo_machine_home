(ROOT
  (S
    (PP (IN On)
      (NP (DT the) (CD one) (NN hand)))
    (, ,)
    (NP (DT the) (JJ first))
    (VP (VBZ has)
      (NP (DT an) (JJ inalienable)
        (ADJP
          (ADJP (JJ right)
            (PP (TO to)
              (NP (PRP$ his))))
          (PRN (-LRB- -LRB-)
            (PP (CC or)
              (NP (PRP her)))
            (-RRB- -RRB-)))
        (NN property)))
    (. .)))

det(hand-4, the-2)
num(hand-4, one-3)
prep_on(has-8, hand-4)
det(first-7, the-6)
nsubj(has-8, first-7)
root(ROOT-0, has-8)
det(property-18, an-9)
amod(property-18, inalienable-10)
amod(property-18, right-11)
prep_to(right-11, his-13)
dep(right-11, or-15)
dep(or-15, her-16)
dobj(has-8, property-18)

