(ROOT
  (FRAG
    (ADVP (RB Hmmm))
    (: ...)
    (SBAR (IN so)
      (S
        (SBAR (IN if)
          (S
            (NP (PRP I))
            (VP (VBP do) (RB n't)
              (VP (VB believe)
                (SBAR (IN that)
                  (S
                    (NP (NN thievery))
                    (VP (VBZ is)
                      (ADJP (JJ wrong)))))))))
        (, ,)
        (NP (PRP I))
        (VP (MD should)
          (ADVP (RB just))
          (VP (VB choose)
            (S (RB not)
              (VP (TO to)
                (VP (VB do)
                  (NP (PRP it)))))))))
    (. ?)))

root(ROOT-0, Hmmm-1)
mark(choose-17, so-3)
mark(believe-8, if-4)
nsubj(believe-8, I-5)
aux(believe-8, do-6)
neg(believe-8, n't-7)
advcl(choose-17, believe-8)
complm(wrong-12, that-9)
nsubj(wrong-12, thievery-10)
cop(wrong-12, is-11)
ccomp(believe-8, wrong-12)
nsubj(choose-17, I-14)
xsubj(do-20, I-14)
aux(choose-17, should-15)
advmod(choose-17, just-16)
dep(Hmmm-1, choose-17)
neg(do-20, not-18)
aux(do-20, to-19)
xcomp(choose-17, do-20)
dobj(do-20, it-21)

