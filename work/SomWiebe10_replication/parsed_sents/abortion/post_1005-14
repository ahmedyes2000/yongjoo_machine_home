(ROOT
  (S
    (VP (VB Do)
      (NP (PRP$ your) (NN research))
      (ADVP (RB first))
      (SBAR (IN before)
        (S
          (NP (PRP you))
          (VP (VBP make)
            (NP
              (NP (NNS assertions))
              (PP (IN like)
                (NP (DT this)))))
          (, ,)
          (PP (IN Because) (IN for)
            (NP
              (NP (DT those))
              (PP (IN of)
                (NP (PRP us)))
              (SBAR
                (WHNP (WP who))
                (S
                  (VP (VBP have)))))))))
    (. .)))

root(ROOT-0, Do-1)
poss(research-3, your-2)
dobj(Do-1, research-3)
advmod(Do-1, first-4)
mark(make-7, before-5)
nsubj(make-7, you-6)
advcl(Do-1, make-7)
dobj(make-7, assertions-8)
prep_like(assertions-8, this-10)
dep(make-7, Because-12)
prep_for(make-7, those-14)
nsubj(have-18, those-14)
prep_of(those-14, us-16)
rcmod(those-14, have-18)

