(ROOT
  (S
    (NP (NNP Prev))
    (, ,)
    (VP (VB show)
      (NP (PRP me))
      (SBAR
        (WHADVP (WRB how))
        (S
          (S
            (NP (DT this))
            (VP (VBZ has)
              (S
                (VP (TO to)
                  (VP (VB deal)
                    (ADVP (RB directly))
                    (PP (IN with)
                      (NP
                        (NP (NNS abortions))
                        (VP (VBG being)
                          (VP (VBN banned))))))))))
          (CC and)
          (S
            (NP (PRP I))
            (VP (MD will)
              (VP (VB answer)
                (NP (PRP it))))))))
    (. .)))

nsubj(show-3, Prev-1)
root(ROOT-0, show-3)
dobj(show-3, me-4)
advmod(has-7, how-5)
nsubj(has-7, this-6)
xsubj(deal-9, this-6)
dep(show-3, has-7)
aux(deal-9, to-8)
xcomp(has-7, deal-9)
advmod(deal-9, directly-10)
prep_with(deal-9, abortions-12)
auxpass(banned-14, being-13)
partmod(abortions-12, banned-14)
nsubj(answer-18, I-16)
aux(answer-18, will-17)
dep(show-3, answer-18)
conj_and(has-7, answer-18)
dobj(answer-18, it-19)

