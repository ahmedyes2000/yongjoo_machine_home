(ROOT
  (SINV
    (PP (IN Under)
      (NP (DT no) (NNS circumstances)))
    (VP (MD should))
    (NP
      (NP (DT a) (NN woman) (POS 's))
      (NN right)
      (S
        (VP (TO to)
          (VP (VB control)
            (S
              (NP (PRP$ her) (JJ own) (NN body))
              (VP (VB be)
                (VP (VBN curtailed)
                  (PP (IN in)
                    (NP (DT this) (NN way))))))))))
    (. .)))

det(circumstances-3, no-2)
prep_under(should-4, circumstances-3)
root(ROOT-0, should-4)
det(woman-6, a-5)
poss(right-8, woman-6)
nsubj(should-4, right-8)
aux(control-10, to-9)
infmod(right-8, control-10)
poss(body-13, her-11)
amod(body-13, own-12)
nsubjpass(curtailed-15, body-13)
auxpass(curtailed-15, be-14)
ccomp(control-10, curtailed-15)
det(way-18, this-17)
prep_in(curtailed-15, way-18)

