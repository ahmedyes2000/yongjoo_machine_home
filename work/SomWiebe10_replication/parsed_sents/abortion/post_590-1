(ROOT
  (S
    (NP (DT A) (NN person))
    (VP (MD can)
      (ADVP (RB naturally))
      (VP
        (VP (VB have)
          (NP (DT a) (NN heart) (NN attack)))
        (CC and)
        (VP (VB die))))
    (. .)))

det(person-2, A-1)
nsubj(have-5, person-2)
nsubj(die-10, person-2)
aux(have-5, can-3)
advmod(have-5, naturally-4)
root(ROOT-0, have-5)
det(attack-8, a-6)
nn(attack-8, heart-7)
dobj(have-5, attack-8)
conj_and(have-5, die-10)

