(ROOT
  (S
    (NP (DT That))
    (VP (VBZ 's)
      (NP
        (NP (DT a) (JJ religious) (NN belief))
        (, ,)
        (VP (RB not) (VBN supported)
          (PP (IN by)
            (NP (JJ scientific) (NN evidence))))))
    (. .)))

nsubj(belief-5, That-1)
cop(belief-5, 's-2)
det(belief-5, a-3)
amod(belief-5, religious-4)
root(ROOT-0, belief-5)
neg(supported-8, not-7)
partmod(belief-5, supported-8)
amod(evidence-11, scientific-10)
agent(supported-8, evidence-11)

