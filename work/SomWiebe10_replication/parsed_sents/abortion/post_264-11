(ROOT
  (S
    (ADVP (RB now))
    (NP (NNP lest))
    (VP (VBP say)
      (SBAR (IN that)
        (S
          (NP (NN story))
          (VP (VBZ is)
            (ADJP (JJ true))))))
    (. .)))

advmod(say-3, now-1)
nsubj(say-3, lest-2)
root(ROOT-0, say-3)
complm(true-7, that-4)
nsubj(true-7, story-5)
cop(true-7, is-6)
ccomp(say-3, true-7)

