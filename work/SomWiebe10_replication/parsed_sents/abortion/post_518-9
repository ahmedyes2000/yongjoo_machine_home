(ROOT
  (S
    (PP (IN For)
      (NP (PRP me)))
    (NP (NN life))
    (VP (VBZ is) (RB not)
      (VP (VBN based)
        (NP
          (NP (PRP$ my) (NN interpretation))
          (PP (IN of)
            (SBAR
              (WHADVP (WRB when))
              (S
                (NP (DT the) (NN soul))
                (VP (VBZ inhabits)
                  (NP (DT the) (NN body)))))))))
    (. .)))

prep_for(based-6, me-2)
nsubjpass(based-6, life-3)
auxpass(based-6, is-4)
neg(based-6, not-5)
root(ROOT-0, based-6)
poss(interpretation-8, my-7)
dobj(based-6, interpretation-8)
advmod(inhabits-13, when-10)
det(soul-12, the-11)
nsubj(inhabits-13, soul-12)
prepc_of(interpretation-8, inhabits-13)
det(body-15, the-14)
dobj(inhabits-13, body-15)

