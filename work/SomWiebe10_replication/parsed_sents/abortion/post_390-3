(ROOT
  (S
    (NP (DT A) (JJ human) (NN person))
    (VP (VBZ is)
      (NP
        (NP (DT a) (JJ human) (NN being))
        (SBAR
          (WHNP (WDT that))
          (S
            (VP (VBZ is)
              (ADVP (RB now))
              (VP (VBG interacting)
                (PP (IN with)
                  (NP (PRP$ its) (NN environment)))))))))
    (. .)))

det(person-3, A-1)
amod(person-3, human-2)
nsubj(being-7, person-3)
cop(being-7, is-4)
det(being-7, a-5)
amod(being-7, human-6)
root(ROOT-0, being-7)
nsubj(interacting-11, being-7)
aux(interacting-11, is-9)
advmod(interacting-11, now-10)
rcmod(being-7, interacting-11)
poss(environment-14, its-13)
prep_with(interacting-11, environment-14)

