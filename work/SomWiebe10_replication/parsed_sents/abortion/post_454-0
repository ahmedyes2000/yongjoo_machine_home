(ROOT
  (S
    (NP
      (NP (NNP Women))
      (SBAR
        (WHNP (WP who))
        (S
          (VP (VBP are)
            (VP (VBN opposed)
              (PP (TO to)
                (NP (NN abortion))))))))
    (VP (MD should) (RB n't)
      (VP (VB have)
        (NP (NNS abortions))))
    (. .)))

nsubjpass(opposed-4, Women-1)
nsubj(have-9, Women-1)
auxpass(opposed-4, are-3)
rcmod(Women-1, opposed-4)
prep_to(opposed-4, abortion-6)
aux(have-9, should-7)
neg(have-9, n't-8)
root(ROOT-0, have-9)
dobj(have-9, abortions-10)

