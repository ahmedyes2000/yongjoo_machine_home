(ROOT
  (S
    (ADVP (RB Once) (RB again))
    (, ,)
    (NP (DT that))
    (VP (VBZ 's) (RB not)
      (NP (PRP$ my) (NN decision)))
    (. .)))

advmod(again-2, Once-1)
advmod(decision-8, again-2)
nsubj(decision-8, that-4)
cop(decision-8, 's-5)
neg(decision-8, not-6)
poss(decision-8, my-7)
root(ROOT-0, decision-8)

(ROOT
  (S
    (NP (DT The) (NN choice))
    (ADVP (RB however))
    (VP (MD should)
      (VP (VB be)
        (ADVP (RB there)
          (PP (IN for)
            (NP
              (NP (NNS people))
              (SBAR
                (WHNP (WP who))
                (S
                  (VP (VBP demand)
                    (NP (PRP it))))))))))
    (. .)))

det(choice-2, The-1)
nsubj(be-5, choice-2)
advmod(be-5, however-3)
aux(be-5, should-4)
root(ROOT-0, be-5)
advmod(be-5, there-6)
prep_for(there-6, people-8)
nsubj(demand-10, people-8)
rcmod(people-8, demand-10)
dobj(demand-10, it-11)

