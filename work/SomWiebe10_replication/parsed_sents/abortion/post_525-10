(ROOT
  (S
    (NP (DT This))
    (VP (VBZ is)
      (NP
        (NP (CD one) (NN reason))
        (SBAR
          (WHADVP (WRB why))
          (S
            (NP (NNP Vietnam))
            (VP (VBD was)
              (ADJP (RB so) (VBN opposed)))))))
    (. .)))

nsubj(reason-4, This-1)
cop(reason-4, is-2)
num(reason-4, one-3)
root(ROOT-0, reason-4)
advmod(opposed-9, why-5)
nsubjpass(opposed-9, Vietnam-6)
auxpass(opposed-9, was-7)
advmod(opposed-9, so-8)
rcmod(reason-4, opposed-9)

