(ROOT
  (S
    (NP (RB Just) (NN visit) (NNS \/))
    (VP (VBP work)
      (PP (IN in)
        (NP
          (NP (DT a) (NN hospital))
          (PP (IN for)
            (NP (DT a) (NN day))))))
    (. .)))

advmod(\/-3, Just-1)
nn(\/-3, visit-2)
nsubj(work-4, \/-3)
root(ROOT-0, work-4)
det(hospital-7, a-6)
prep_in(work-4, hospital-7)
det(day-10, a-9)
prep_for(hospital-7, day-10)

