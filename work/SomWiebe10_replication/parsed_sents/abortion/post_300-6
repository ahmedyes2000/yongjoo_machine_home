(ROOT
  (S
    (VP (VB Sigh) (, ,)
      (SBAR (RB as) (IN if)
        (S
          (NP (PRP we))
          (VP (VBP need)
            (NP
              (NP (JJR more) (NNS kids))
              (PP (IN like)
                (NP (DT that))))
            (PRT (RP around))))))
    (. .)))

root(ROOT-0, Sigh-1)
advmod(need-6, as-3)
dep(need-6, if-4)
nsubj(need-6, we-5)
ccomp(Sigh-1, need-6)
amod(kids-8, more-7)
dobj(need-6, kids-8)
prep_like(kids-8, that-10)
prt(need-6, around-11)

