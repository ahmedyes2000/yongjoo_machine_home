(ROOT
  (S
    (NP (PRP You))
    (ADVP (RB better))
    (VP (VBP study)
      (NP (PRP$ your) (NN history) (NN dear) (NN child)))
    (. .)))

nsubj(study-3, You-1)
advmod(study-3, better-2)
root(ROOT-0, study-3)
poss(child-7, your-4)
nn(child-7, history-5)
nn(child-7, dear-6)
dobj(study-3, child-7)

