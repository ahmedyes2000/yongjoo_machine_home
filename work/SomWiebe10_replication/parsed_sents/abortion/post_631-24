(ROOT
  (S
    (PP (IN From)
      (NP
        (NP (NN research))
        (PP (IN in)
          (NP (DT these) (NNS areas)))))
    (, ,)
    (NP (PRP it))
    (VP (VBZ is)
      (ADJP (JJ clear))
      (SBAR (IN that)
        (S
          (NP (JJ certain) (NNS things))
          (VP (VBP are)
            (VP (VBN required)
              (PP (IN in)
                (NP
                  (NP (NN order))
                  (PP (IN for)
                    (NP (DT the) (NN heart)))))
              (S
                (VP (TO to)
                  (VP (VB beat)))))))))
    (. .)))

prep_from(clear-9, research-2)
det(areas-5, these-4)
prep_in(research-2, areas-5)
nsubj(clear-9, it-7)
cop(clear-9, is-8)
root(ROOT-0, clear-9)
complm(required-14, that-10)
amod(things-12, certain-11)
nsubjpass(required-14, things-12)
xsubj(beat-21, things-12)
auxpass(required-14, are-13)
ccomp(clear-9, required-14)
prep_in(required-14, order-16)
det(heart-19, the-18)
prep_for(order-16, heart-19)
aux(beat-21, to-20)
xcomp(required-14, beat-21)

