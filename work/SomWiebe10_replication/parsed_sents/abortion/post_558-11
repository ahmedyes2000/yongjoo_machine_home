(ROOT
  (S
    (PP (IN In)
      (NP (DT the) (JJ real) (NN world)))
    (NP (JJ reproductive) (NNS issues))
    (VP (VBP get)
      (ADJP
        (ADJP (JJ messy))
        (CC and)
        (ADJP (RB very) (RB ethically) (JJ complex)))
      (, ,)
      (S
        (VP (VBG making)
          (S
            (NP (NN blanket) (NN legislation))
            (ADJP (RB very) (JJ difficult))))))
    (. .)))

det(world-4, the-2)
amod(world-4, real-3)
prep_in(messy-8, world-4)
amod(issues-6, reproductive-5)
nsubj(messy-8, issues-6)
nsubj(complex-12, issues-6)
dep(messy-8, get-7)
root(ROOT-0, messy-8)
advmod(complex-12, very-10)
advmod(complex-12, ethically-11)
conj_and(messy-8, complex-12)
xcomp(messy-8, making-14)
nn(legislation-16, blanket-15)
nsubj(difficult-18, legislation-16)
advmod(difficult-18, very-17)
xcomp(making-14, difficult-18)

