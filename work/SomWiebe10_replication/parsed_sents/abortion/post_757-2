(ROOT
  (S
    (S
      (NP (PRP It))
      (VP (VBZ 's)
        (NP (DT a) (JJ terrible) (NN idea))))
    (, ,)
    (CC but)
    (S
      (NP (DT some) (NNS people))
      (ADVP (RB just))
      (VP (VBP do) (RB n't)
        (VP (VB like)
          (NP
            (NP (DT the) (NN idea))
            (PP (IN of)
              (NP (NN abstinence))))
          (ADVP (RB either)))))
    (. .)))

nsubj(idea-5, It-1)
cop(idea-5, 's-2)
det(idea-5, a-3)
amod(idea-5, terrible-4)
root(ROOT-0, idea-5)
det(people-9, some-8)
nsubj(like-13, people-9)
