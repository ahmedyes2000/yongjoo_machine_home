(ROOT
  (S
    (NP (DT This))
    (VP (VBZ is) (RB not)
      (NP (DT a) (JJ legal) (NN argument)))
    (. .)))

nsubj(argument-6, This-1)
cop(argument-6, is-2)
neg(argument-6, not-3)
det(argument-6, a-4)
amod(argument-6, legal-5)
root(ROOT-0, argument-6)

(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ 's)
      (NP (DT a) (NNS values) (NN debate)))
    (. .)))

nsubj(debate-5, It-1)
cop(debate-5, 's-2)
det(debate-5, a-3)
nn(debate-5, values-4)
root(ROOT-0, debate-5)

(ROOT
  (S ('' '')
    (NP (JJ Should) (NN abortion))
    (VP (VBP be)
      (ADJP (JJ legal)))
    (. ?)))

amod(abortion-3, Should-2)
nsubj(legal-5, abortion-3)
cop(legal-5, be-4)
root(ROOT-0, legal-5)

