(ROOT
  (S
    (NP (NNP He\/she))
    (VP (VBZ is) (RB n't)
      (NP
        (NP (NN part))
        (PP (IN of)
          (NP (PRP$ their) (NN mother)))))
    (. .)))

nsubj(part-4, He\/she-1)
cop(part-4, is-2)
neg(part-4, n't-3)
root(ROOT-0, part-4)
poss(mother-7, their-6)
prep_of(part-4, mother-7)

