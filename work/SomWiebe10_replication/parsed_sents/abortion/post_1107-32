(ROOT
  (S
    (VP (VB Slow)
      (NP (PRP$ our) (NN stampede))
      (PP (IN into)
        (NP (JJ social) (NN anarchy)))
      (SBAR (IN if)
        (S
          (NP (PRP you))
          (VP (MD will)))))
    (. .)))

root(ROOT-0, Slow-1)
poss(stampede-3, our-2)
dobj(Slow-1, stampede-3)
amod(anarchy-6, social-5)
prep_into(Slow-1, anarchy-6)
mark(will-9, if-7)
nsubj(will-9, you-8)
advcl(Slow-1, will-9)

