(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ takes)
      (ADJP
        (ADJP (RB considerably) (JJR longer))
        (PP (IN than)
          (NP (DT a) (JJ few) (NNS seconds))))
      (S
        (VP (TO to)
          (VP (VB ignite)
            (NP
              (NP (DT the) (NN operation))
              (PP (IN of)
                (NP (PRP$ his) (NN consciousness))))))))
    (. .)))

nsubj(takes-2, It-1)
xsubj(ignite-10, It-1)
root(ROOT-0, takes-2)
advmod(longer-4, considerably-3)
acomp(takes-2, longer-4)
det(seconds-8, a-6)
amod(seconds-8, few-7)
prep_than(longer-4, seconds-8)
aux(ignite-10, to-9)
xcomp(takes-2, ignite-10)
det(operation-12, the-11)
dobj(ignite-10, operation-12)
poss(consciousness-15, his-14)
prep_of(operation-12, consciousness-15)

