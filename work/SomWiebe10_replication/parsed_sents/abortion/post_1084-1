(ROOT
  (S
    (VP (VB Shame)
      (PP (IN on)
        (NP
          (NP (DT the) (NNS people))
          (SBAR
            (WHNP (WP who))
            (S
              (VP (VBP say)
                (SBAR
                  (S
                    (NP (PRP it))
                    (VP (MD should)
                      (VP (VB be)
                        (VP (VBN banned))))))))))))
    (. .)))

root(ROOT-0, Shame-1)
det(people-4, the-3)
prep_on(Shame-1, people-4)
nsubj(say-6, people-4)
rcmod(people-4, say-6)
nsubjpass(banned-10, it-7)
aux(banned-10, should-8)
auxpass(banned-10, be-9)
ccomp(say-6, banned-10)

