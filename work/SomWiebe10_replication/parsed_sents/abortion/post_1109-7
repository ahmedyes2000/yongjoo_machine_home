(ROOT
  (S
    (NP (NNP Everything))
    (VP (VBZ becomes)
      (NP
        (NP (DT a) (NN matter))
        (PP (IN of)
          (NP (JJ personal) (NN preference)
            (CC and)
            (NN taste)))))
    (. .)))

nsubj(matter-4, Everything-1)
cop(matter-4, becomes-2)
det(matter-4, a-3)
root(ROOT-0, matter-4)
amod(preference-7, personal-6)
prep_of(matter-4, preference-7)
prep_of(matter-4, taste-9)
conj_and(preference-7, taste-9)

