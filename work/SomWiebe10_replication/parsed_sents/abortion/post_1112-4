(ROOT
  (S
    (NP (PRP They))
    (ADVP (DT all))
    (VP (VBP devalue)
      (NP (JJ human) (NN life))
      (PP (IN by)
        (S
          (VP (VBG reducing)
            (NP (PRP it))
            (PP (TO to)
              (NP (NN something)))
            (PP (CC less)
              (PP (IN than)
                (SBAR
                  (WHNP (WP what))
                  (S
                    (NP (PRP it))
                    (VP (VBZ is))))))))))
    (. .)))

nsubj(devalue-3, They-1)
advmod(devalue-3, all-2)
root(ROOT-0, devalue-3)
amod(life-5, human-4)
dobj(devalue-3, life-5)
prepc_by(devalue-3, reducing-7)
dobj(reducing-7, it-8)
prep_to(reducing-7, something-10)
cc(reducing-7, less-11)
dobj(is-15, what-13)
nsubj(is-15, it-14)
prepc_than(reducing-7, is-15)

(ROOT
  (S
    (S
      (NP (PRP You))
      (VP (VBP agree)
        (SBAR (IN that)
          (S
            (NP (NN man))
            (VP (VBZ has)
              (NP (DT a) (NN nature)))))))
    (CC and)
    (S
      (NP (PRP it))
      (VP (VBZ is)
        (VP (VBN fixed))))
    (. .)))

nsubj(agree-2, You-1)
root(ROOT-0, agree-2)
complm(has-5, that-3)
nsubj(has-5, man-4)
ccomp(agree-2, has-5)
det(nature-7, a-6)
dobj(has-5, nature-7)
nsubjpass(fixed-11, it-9)
auxpass(fixed-11, is-10)
conj_and(agree-2, fixed-11)

