(ROOT
  (S
    (NP
      (NP (NNS Arguments))
      (PP (IN against)
        (NP (NN abortion))))
    (VP (VBP use)
      (NP (NN scare) (NNS tactics))
      (PP (IN like)
        (S
          (VP (VBG killing)
            (NP (JJ cute) (NNS babies))
            (PP (IN in)
              (NP
                (NP (PRP$ their) (NN mother) (POS 's))
                (NNS arms)))))))
    (. .)))

nsubj(use-4, Arguments-1)
prep_against(Arguments-1, abortion-3)
root(ROOT-0, use-4)
nn(tactics-6, scare-5)
dobj(use-4, tactics-6)
prepc_like(use-4, killing-8)
amod(babies-10, cute-9)
dobj(killing-8, babies-10)
poss(mother-13, their-12)
poss(arms-15, mother-13)
prep_in(killing-8, arms-15)

