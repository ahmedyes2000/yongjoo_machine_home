(ROOT
  (FRAG
    (ADJP (JJ thats))
    (SBAR
      (WHNP (WP what))
      (S
        (NP (DT the) (JJ other) (NN debater))
        (VP (VBD said))))
    (. .)))

root(ROOT-0, thats-1)
dobj(said-6, what-2)
det(debater-5, the-3)
amod(debater-5, other-4)
nsubj(said-6, debater-5)
dep(thats-1, said-6)

