(ROOT
  (S
    (NP (PRP We))
    (VP (VBP are)
      (VP (VBG debating)
        (NP (NN abortion))
        (ADVP (RB not)
          (NP (DT the) (NN war)))
        (PP (IN in)
          (NP (NNP iraq)))))
    (. .)))

nsubj(debating-3, We-1)
aux(debating-3, are-2)
root(ROOT-0, debating-3)
dobj(debating-3, abortion-4)
neg(debating-3, not-5)
det(war-7, the-6)
pobj(not-5, war-7)
prep_in(debating-3, iraq-9)

