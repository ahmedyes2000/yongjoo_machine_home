(ROOT
  (S ('' '')
    (NP (NNP Webster))
    (VP (VBZ 's)
      (VP (VBN Revised)
        (NP (NNP Unabridged) (CD 1996))))
    (. .)))

nsubjpass(Revised-4, Webster-2)
auxpass(Revised-4, 's-3)
root(ROOT-0, Revised-4)
dobj(Revised-4, Unabridged-5)
num(Unabridged-5, 1996-6)

