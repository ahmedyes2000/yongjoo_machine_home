(ROOT
  (S
    (NP (DT That))
    (VP (VBZ 's) (RB not)
      (ADVP (RB really))
      (NP
        (NP (DT the) (NN point))
        (ADVP (RB though))))
    (. .)))

nsubj(point-6, That-1)
cop(point-6, 's-2)
neg(point-6, not-3)
advmod(point-6, really-4)
det(point-6, the-5)
root(ROOT-0, point-6)
advmod(point-6, though-7)

