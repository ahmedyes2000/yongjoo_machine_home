(ROOT
  (S ('' '')
    (NP (DT The) (JJ intentional) (NN taking))
    (ADVP (IN of)
      (NP (JJ innocent) (NNS lives)))
    (VP (MD can)
      (ADVP (RB never))
      (VP (VB be)
        (VP (VBN approved)
          (PP (IN by)
            (NP (DT the) (NNP Church))))))
    (. .)))

det(taking-4, The-2)
amod(taking-4, intentional-3)
nsubjpass(approved-11, taking-4)
advmod(approved-11, of-5)
amod(lives-7, innocent-6)
pobj(of-5, lives-7)
aux(approved-11, can-8)
neg(approved-11, never-9)
auxpass(approved-11, be-10)
root(ROOT-0, approved-11)
det(Church-14, the-13)
agent(approved-11, Church-14)

