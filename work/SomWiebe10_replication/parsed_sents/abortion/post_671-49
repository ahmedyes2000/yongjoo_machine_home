(ROOT
  (S
    (NP (DT This))
    (VP (VBZ is)
      (NP
        (NP (DT a) (NN democracy))
        (, ,)
        (PP (IN for)
          (NP
            (NP (NNP God) (POS 's))
            (NN sake)))
        (, ,)
        (NP
          (NP (RB not) (DT a) (JJ cruel) (JJ theocratic) (NN totalitarian) (NN regime))
          (PP (IN like)
            (NP (NNP Iran))))))
    (. .)))

nsubj(democracy-4, This-1)
cop(democracy-4, is-2)
det(democracy-4, a-3)
root(ROOT-0, democracy-4)
poss(sake-9, God-7)
prep_for(democracy-4, sake-9)
neg(regime-16, not-11)
det(regime-16, a-12)
amod(regime-16, cruel-13)
amod(regime-16, theocratic-14)
nn(regime-16, totalitarian-15)
appos(democracy-4, regime-16)
prep_like(regime-16, Iran-18)

