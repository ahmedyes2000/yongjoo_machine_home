(ROOT
  (S
    (S
      (NP (DT This))
      (VP (VBZ is)
        (SBAR
          (WHADVP (WRB why))
          (S
            (S
              (VP (VBG extending)
                (NP (DT a) (NN right))
                (PP (TO to)
                  (NP (NN life)))))
            (VP (VBZ is)
              (PP (IN of)
                (NP (JJ utmost) (NN importance))))))))
    (: ;)
    (S
      (NP
        (NP (DT the) (NN future))
        (PP (IN of)
          (NP (DT the) (NN unborn))))
      (VP (VBZ depends)
        (PP (IN on)
          (NP (PRP it)))))
    (. .)))

nsubj(is-2, This-1)
root(ROOT-0, is-2)
advmod(is-9, why-3)
csubj(is-9, extending-4)
det(right-6, a-5)
dobj(extending-4, right-6)
prep_to(extending-4, life-8)
advcl(is-2, is-9)
amod(importance-12, utmost-11)
prep_of(is-9, importance-12)
det(future-15, the-14)
nsubj(depends-19, future-15)
det(unborn-18, the-17)
prep_of(future-15, unborn-18)
parataxis(is-2, depends-19)
prep_on(depends-19, it-21)

