(ROOT
  (S
    (S
      (NP (PRP It))
      (VP (MD may)
        (VP (VB come)
          (NP (DT a) (JJ few) (NNS weeks))
          (PP (IN after)
            (NP (NN conception)
              (CC or)
              (NNS years)))
          (PP (IN after)
            (NP (PRP$ our) (NN birth))))))
    (CC but)
    (S
      (NP (PRP it))
      (VP (MD will)
        (VP (VB happen))))
    (. .)))

nsubj(come-3, It-1)
aux(come-3, may-2)
root(ROOT-0, come-3)
det(weeks-6, a-4)
amod(weeks-6, few-5)
tmod(come-3, weeks-6)
prep_after(come-3, conception-8)
prep_after(come-3, years-10)
conj_or(conception-8, years-10)
poss(birth-13, our-12)
prep_after(come-3, birth-13)
nsubj(happen-17, it-15)
aux(happen-17, will-16)
conj_but(come-3, happen-17)

