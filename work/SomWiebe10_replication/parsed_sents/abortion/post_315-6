  (FRAG (CC Or)
    (SBAR
      (WHNP
        (WHADJP (WRB how) (JJ many))
        (NNS times))
      (S
        (NP (PRP you))
        (VP (VBP 've)
          (VP (VBN given)
            (NP (NN bone) (NN marrow))
            (SBAR (RB so) (IN that)
              (S
                (NP (DT a) (NN child))
