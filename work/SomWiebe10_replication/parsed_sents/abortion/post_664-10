(ROOT
  (S
    (NP (PDT All) (PRP$ our) (NNS laws))
    (VP (VBP are)
      (VP (VBN based)
        (PP (IN on)
          (NP (JJ moral) (NNS axioms)))))
    (. .)))

predet(laws-3, All-1)
poss(laws-3, our-2)
nsubjpass(based-5, laws-3)
auxpass(based-5, are-4)
root(ROOT-0, based-5)
amod(axioms-8, moral-7)
prep_on(based-5, axioms-8)

