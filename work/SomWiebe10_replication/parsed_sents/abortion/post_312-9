(ROOT
  (S
    (NP (DT The) (JJ biological) (NN heritage))
    (VP (VBZ is)
      (NP
        (NP (DT the) (RBS most) (JJ verifiable) (NN criterion))
        (PP (IN for)
          (NP (NN personhood)))))
    (. .)))

det(heritage-3, The-1)
amod(heritage-3, biological-2)
nsubj(criterion-8, heritage-3)
cop(criterion-8, is-4)
det(criterion-8, the-5)
advmod(criterion-8, most-6)
amod(criterion-8, verifiable-7)
root(ROOT-0, criterion-8)
prep_for(criterion-8, personhood-10)

