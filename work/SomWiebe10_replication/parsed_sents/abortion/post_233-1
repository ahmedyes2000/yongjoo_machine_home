(ROOT
  (S
    (NP (PRP You))
    (VP (VBP 're)
      (VP (VBG going)
        (S
          (VP (TO to)
            (VP (VB have)
              (S
                (VP (TO to)
                  (VP (VB give)
                    (NP (PRP me))
                    (NP (DT a) (NN link))))))))))
    (. .)))

nsubj(going-3, You-1)
xsubj(have-5, You-1)
aux(going-3, 're-2)
root(ROOT-0, going-3)
aux(have-5, to-4)
xcomp(going-3, have-5)
aux(give-7, to-6)
xcomp(have-5, give-7)
iobj(give-7, me-8)
det(link-10, a-9)
dobj(give-7, link-10)

