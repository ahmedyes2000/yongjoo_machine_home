  (S ('' '')
    (S
      (NP (PRP I))
      (VP (VBP 'm) (RB not)
        (ADJP (JJ obligated)
          (S
            (VP (TO to)
              (VP (VB save)
                (NP (DT a) (NN life))))))))
    (, ,)
    (CC but)
    (S
      (NP (PRP I))
      (VP (VBP am)
        (VP (VBN obligated)
          (S (RB not)
            (VP (TO to)
              (VP (VB take)
                (NP (CD one))))))))
    (. .)))

