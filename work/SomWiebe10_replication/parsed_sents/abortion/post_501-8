(ROOT
  (S
    (NP (NNP Adoption) (, ,))
    (VP
      (VP (VBZ is)
        (NP (DT a) (JJ viable) (NN alternative)))
      (CC and)
      (VP (MD should)
        (VP (VB be)
          (VP (VBN examined)))))
    (. .)))

nsubj(alternative-6, Adoption-1)
nsubjpass(examined-10, Adoption-1)
cop(alternative-6, is-3)
det(alternative-6, a-4)
amod(alternative-6, viable-5)
root(ROOT-0, alternative-6)
aux(examined-10, should-8)
auxpass(examined-10, be-9)
conj_and(alternative-6, examined-10)

