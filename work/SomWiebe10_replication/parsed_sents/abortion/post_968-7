(ROOT
  (S
    (NP (PRP You))
    (VP (MD should)
      (VP (VB help)
        (S
          (NP (NN everyone))
          (VP (VB stay)
            (ADJP (JJ healthy)
              (CC and)
              (JJ alive))))))
    (. .)))

nsubj(help-3, You-1)
aux(help-3, should-2)
root(ROOT-0, help-3)
nsubj(healthy-6, everyone-4)
nsubj(alive-8, everyone-4)
cop(healthy-6, stay-5)
ccomp(help-3, healthy-6)
ccomp(help-3, alive-8)
conj_and(healthy-6, alive-8)

