(ROOT
  (S (RB Then)
    (NP (EX there))
    (VP (VBP are)
      (NP
        (NP (DT those))
        (SBAR
          (WHNP (WP who))
          (S
            (VP (VBP say)
              (SBAR (IN that)
                (S
                  (NP (DT the) (NN zygote))
                  (VP (VBZ is)
                    (NP (DT a) (JJ new) (NN life))
                    (SBAR (IN because)
                      (S
                        (NP (PRP it))
                        (VP (MD will)
                          (ADVP (RB eventually))
                          (VP (VB be)
                            (NP (DT an) (JJ independent) (NN life))))))))))))))
    (. .)))

advmod(are-3, Then-1)
expl(are-3, there-2)
root(ROOT-0, are-3)
nsubj(are-3, those-4)
nsubj(say-6, those-4)
rcmod(those-4, say-6)
complm(life-13, that-7)
det(zygote-9, the-8)
