(ROOT
  (S
    (ADVP (RB Just))
    (VP (VB remember)
      (SBAR (IN that)
        (S
          (NP (NNP God))
          (VP (VBZ loves)
            (NP
              (NP (DT the) (NN person))
              (VP (VBG getting)
                (NP
                  (NP (DT the) (NN abortion))
                  (CC AND)
                  (NP (DT the) (JJ beautiful) (, ,) (JJ innocent) (JJ little) (NNS babies)))))))))
    (. .)))

advmod(remember-2, Just-1)
root(ROOT-0, remember-2)
complm(loves-5, that-3)
nsubj(loves-5, God-4)
ccomp(remember-2, loves-5)
det(person-7, the-6)
dobj(loves-5, person-7)
partmod(person-7, getting-8)
det(abortion-10, the-9)
dobj(getting-8, abortion-10)
det(babies-17, the-12)
amod(babies-17, beautiful-13)
amod(babies-17, innocent-15)
amod(babies-17, little-16)
dobj(getting-8, babies-17)
conj_and(abortion-10, babies-17)

