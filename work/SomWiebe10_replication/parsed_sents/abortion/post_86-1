(ROOT
  (S
    (NP
      (NP (VBG Denying) (NN someone) (NN life))
      (PP (RB because) (IN of)
        (NP
          (NP (DT the) (NNS circumstances))
          (PP (IN of)
            (NP (PRP$ their) (NN conception))))))
    (VP (VBZ is)
      (ADJP (JJ unfair)))
    (. .)))

amod(life-3, Denying-1)
nn(life-3, someone-2)
nsubj(unfair-12, life-3)
det(circumstances-7, the-6)
prep_because_of(life-3, circumstances-7)
poss(conception-10, their-9)
prep_of(circumstances-7, conception-10)
cop(unfair-12, is-11)
root(ROOT-0, unfair-12)

