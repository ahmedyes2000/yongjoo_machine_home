(ROOT
  (SQ (VBD did)
    (NP (PRP you))
    (VP (VB choose)
      (S
        (VP (TO to)
          (VP (VB be)
            (NP (DT a) (NN man))))))
    (. ?)))

aux(choose-3, did-1)
nsubj(choose-3, you-2)
xsubj(man-7, you-2)
root(ROOT-0, choose-3)
aux(man-7, to-4)
cop(man-7, be-5)
det(man-7, a-6)
xcomp(choose-3, man-7)

(ROOT
  (S
    (NP (PRP we))
    (VP (VBD had)
      (NP
        (NP (DT no) (NN choice))
        (PP (IN in)
          (NP (PRP$ our) (NN sex)))))
    (. .)))

nsubj(had-2, we-1)
root(ROOT-0, had-2)
det(choice-4, no-3)
dobj(had-2, choice-4)
poss(sex-7, our-6)
prep_in(choice-4, sex-7)

