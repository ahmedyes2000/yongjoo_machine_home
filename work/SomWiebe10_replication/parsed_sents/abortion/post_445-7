(ROOT
  (S ('' '')
    (NP (PRP I))
    (VP (VBP 'm)
      (VP (VBN ok)
        (PP (IN with)
          (NP (NN someone)))
        (S
          (VP (VBG choosing)
            (NP (DT an) (NN abortion))))))
    (. .)))

nsubjpass(ok-4, I-2)
auxpass(ok-4, 'm-3)
root(ROOT-0, ok-4)
prep_with(ok-4, someone-6)
xcomp(ok-4, choosing-7)
det(abortion-9, an-8)
dobj(choosing-7, abortion-9)

