(ROOT
  (S
    (S
      (NP (PRP I))
      (VP (VBP say)
        (SBAR
          (S
            (NP (PRP they))
            (VP (MD should))))))
    (, ,)
    (CC but)
    (S
      (NP (PRP it))
      (VP (VBZ depends)))
    (. .)))

nsubj(say-2, I-1)
root(ROOT-0, say-2)
nsubj(should-4, they-3)
ccomp(say-2, should-4)
nsubj(depends-8, it-7)
conj_but(say-2, depends-8)

