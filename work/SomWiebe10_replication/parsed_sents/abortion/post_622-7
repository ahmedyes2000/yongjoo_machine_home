(ROOT
  (S
    (NP (PRP You))
    (VP (VBP are)
      (ADVP (RB also))
      (VP (VBG assuming)
        (SBAR (IN that) (IN because)
          (S
            (NP (PRP I))
            (VP (VBP base)
              (NP (PRP$ my) (NNS beliefs))
              (PP (IN on)
                (NP (JJ Christian) (NNS principles)))
              (SBAR (IN that)
                (S
                  (NP (PRP I))
                  (VP (MD must)
                    (VP (VB be)
                      (ADJP (JJ wrong)))))))))))
    (. .)))

nsubj(assuming-4, You-1)
aux(assuming-4, are-2)
advmod(assuming-4, also-3)
root(ROOT-0, assuming-4)
complm(base-8, that-5)
dep(base-8, because-6)
nsubj(base-8, I-7)
ccomp(assuming-4, base-8)
poss(beliefs-10, my-9)
dobj(base-8, beliefs-10)
amod(principles-13, Christian-12)
prep_on(base-8, principles-13)
complm(wrong-18, that-14)
nsubj(wrong-18, I-15)
aux(wrong-18, must-16)
cop(wrong-18, be-17)
ccomp(base-8, wrong-18)

