      (NP (PRP We))
      (ADVP (RB also))
      (VP
        (ADVP (RB now))
        (VBP know)
        (SBAR (IN that)
          (S
            (NP (DT these) (NNS fetuses))
            (VP (MD can)
              (ADVP (RB indeed))
              (VP (VB feel)
                (NP (NN pain))))))))
    (, ,)
    (ADVP (RB therefore))
    (S
      (NP (PRP it))
      (VP (VBZ is)
        (NP
