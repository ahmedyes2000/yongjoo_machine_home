(ROOT
  (S
    (SBAR (IN If)
      (S
        (S
          (NP (PRP$ your))
          (VP (VBP are)
            (PP (IN for)
              (NP (NN abortion)))))
        (S
          (INTJ (VB please))
          (VP (VB tell)
            (NP (PRP me))
            (SBAR
              (WHNP (WP what))
              (S
                (NP (DT that))
                (VP (VBZ is)
                  (NP (PRP they)))))))))
    (VP (VBP are)
      (VP (VBG sucking)
        (PRT (RP out))
        (PP (IN into)
          (NP (DT the) (NN trash)))))
    (. .)))

mark(are-3, If-1)
nsubj(are-3, your-2)
csubj(sucking-14, are-3)
prep_for(are-3, abortion-5)
dep(tell-7, please-6)
dep(are-3, tell-7)
dobj(tell-7, me-8)
dobj(they-12, what-9)
nsubj(they-12, that-10)
cop(they-12, is-11)
dep(tell-7, they-12)
aux(sucking-14, are-13)
root(ROOT-0, sucking-14)
prt(sucking-14, out-15)
det(trash-18, the-17)
prep_into(sucking-14, trash-18)

