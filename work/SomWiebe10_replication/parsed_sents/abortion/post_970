(ROOT
  (S
    (ADVP (RB Yes))
    (NP (PRP it))
    (VP (MD should)
      (VP (VB be)
        (VP (VBN banned))))
    (. .)))

advmod(banned-5, Yes-1)
nsubjpass(banned-5, it-2)
aux(banned-5, should-3)
auxpass(banned-5, be-4)
root(ROOT-0, banned-5)

(ROOT
  (S
    (NP (NNS People))
    (VP (MD should) (RB n't)
      (VP (VB be)
        (VP (VBG having)
          (NP (NN sex))
          (SBAR (IN if)
            (S
              (NP (PRP they))
              (VP (VBP do) (RB n't)
                (VP (VB wanna)
                  (NP (DT a) (NN baby)))))))))
    (. .)))

nsubj(having-5, People-1)
aux(having-5, should-2)
neg(having-5, n't-3)
aux(having-5, be-4)
root(ROOT-0, having-5)
dobj(having-5, sex-6)
mark(wanna-11, if-7)
nsubj(wanna-11, they-8)
aux(wanna-11, do-9)
neg(wanna-11, n't-10)
advcl(having-5, wanna-11)
det(baby-13, a-12)
dobj(wanna-11, baby-13)

(ROOT
  (S (CC Or)
    (NP (PRP it))
    (VP (MD should)
      (VP (VB be)
        (VP (VBN protected))))
    (. .)))

cc(protected-5, Or-1)
nsubjpass(protected-5, it-2)
aux(protected-5, should-3)
auxpass(protected-5, be-4)
root(ROOT-0, protected-5)

(ROOT
  (S
    (NP (VBG Killing) (NNS babies))
    (VP (VBZ is)
      (NP (DT a) (NN crime)))
    (. .)))

amod(babies-2, Killing-1)
nsubj(crime-5, babies-2)
cop(crime-5, is-3)
det(crime-5, a-4)
root(ROOT-0, crime-5)

