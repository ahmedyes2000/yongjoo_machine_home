(ROOT
  (S
    (INTJ (UH No))
    (, ,)
    (NP (PRP you))
    (VP (VBP are)
      (ADVP (NN way) (IN out))
      (PP (IN in)
        (NP (JJ left) (NN field))))
    (. .)))

dep(are-4, No-1)
nsubj(are-4, you-3)
root(ROOT-0, are-4)
dep(out-6, way-5)
advmod(are-4, out-6)
amod(field-9, left-8)
prep_in(are-4, field-9)

