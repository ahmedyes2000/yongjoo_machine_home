(ROOT
  (S
    (NP (DT This))
    (VP (VBZ is)
      (NP
        (NP (DT an) (NN issue))
        (PP (IN of)
          (NP (JJ individual) (NNS rights)))))
    (. .)))

nsubj(issue-4, This-1)
cop(issue-4, is-2)
det(issue-4, an-3)
root(ROOT-0, issue-4)
amod(rights-7, individual-6)
prep_of(issue-4, rights-7)

