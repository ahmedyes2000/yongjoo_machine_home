(ROOT
  (S
    (S
      (NP (EX there))
      (VP (VBZ is)
        (NP
          (NP (DT no) (NN question))
          (PP (TO to)
            (SBAR
              (WHADVP (WRB when))
              (S
                (NP (NN life))
                (VP (VBZ begins))))))))
    (: :)
    (S
      (NP (PRP it))
      (VP (VBZ 's)
        (NP (NN life))
        (PP (IN throughout))))
    (. .)))

expl(is-2, there-1)
root(ROOT-0, is-2)
det(question-4, no-3)
