(ROOT
  (S
    (PP (IN At)
      (NP (DT every) (NN stage)))
    (, ,)
    (S
      (NP (EX there))
      (VP (VBZ is)
        (NP (NN life))))
    (, ,)
    (CC and)
    (S
      (NP (DT that) (NN life))
      (VP (VBZ has)
        (NP (JJ human) (NN DNA))))
    (. .)))

det(stage-3, every-2)
prep_at(is-6, stage-3)
expl(is-6, there-5)
root(ROOT-0, is-6)
nsubj(is-6, life-7)
det(life-11, that-10)
nsubj(has-12, life-11)
conj_and(is-6, has-12)
amod(DNA-14, human-13)
dobj(has-12, DNA-14)

