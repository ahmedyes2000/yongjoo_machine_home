(ROOT
  (S
    (NP (PRP I))
    (VP (VBP am)
      (ADJP (JJ happy)
        (SBAR (IN that)
          (S
            (NP (NN someone))
            (VP (VBD said)
              (NP (DT this)))))))
    (. .)))

nsubj(happy-3, I-1)
cop(happy-3, am-2)
root(ROOT-0, happy-3)
complm(said-6, that-4)
nsubj(said-6, someone-5)
ccomp(happy-3, said-6)
dobj(said-6, this-7)

