(ROOT
  (S
    (S
      (NP (NNP Irrationality))
      (VP (MD can)
        (VP (VB run))))
    (, ,)
    (CC but)
    (S
      (NP (PRP it))
      (VP (MD ca) (RB n't)
        (VP (VB hide))))
    (. .)))

nsubj(run-3, Irrationality-1)
aux(run-3, can-2)
root(ROOT-0, run-3)
nsubj(hide-9, it-6)
aux(hide-9, ca-7)
neg(hide-9, n't-8)
conj_but(run-3, hide-9)

