(ROOT
  (FRAG
    (WHADVP (WRB Why))
    (RB not)
    (VP
      (VP (VB strive)
        (PP (IN for)
          (NP (DT the) (NN ideal))))
      (CONJP (RB instead) (IN of))
      (VP (VB settle)
        (PP (IN for)
          (NP (NN something)))
        (ADVP (RBR less))))
    (. .)))

dep(not-2, Why-1)
root(ROOT-0, not-2)
dep(not-2, strive-3)
det(ideal-6, the-5)
prep_for(strive-3, ideal-6)
dep(not-2, settle-9)
conj_negcc(strive-3, settle-9)
prep_for(settle-9, something-11)
