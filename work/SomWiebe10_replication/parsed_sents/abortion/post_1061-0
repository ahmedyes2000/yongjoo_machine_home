(ROOT
  (S
    (ADVP (RB Very))
    (NP
      (NP (RB little))
      (PP (IN of)
        (NP (DT all) (NNS abortions))))
    (VP (VBP are)
      (VP (VBN performed)
        (PP (RB because) (IN of)
          (NP
            (NP (DT the) (NN baby))
            (VP (VBG threatening)
              (NP
                (NP (DT the) (NN mother) (POS 's))
                (NN life)))))))
    (. .)))

advmod(performed-7, Very-1)
nsubjpass(performed-7, little-2)
det(abortions-5, all-4)
prep_of(little-2, abortions-5)
auxpass(performed-7, are-6)
root(ROOT-0, performed-7)
det(baby-11, the-10)
prep_because_of(performed-7, baby-11)
partmod(baby-11, threatening-12)
det(mother-14, the-13)
poss(life-16, mother-14)
dobj(threatening-12, life-16)

