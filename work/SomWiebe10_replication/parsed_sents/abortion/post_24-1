(ROOT
  (S
    (SBAR
      (WHNP (WP What))
      (S
        (VP (VBZ makes)
          (S
            (NP (DT the) (NNS mothers) (NN life))
            (ADJP (RBR more) (JJ important))))))
    (VP (VBZ is)
      (SBAR (IN that)
        (S
          (NP (PRP she))
          (VP
            (VP (VBZ is)
              (NP (DT a) (JJ full-grown) (NN adult)))
            (CC and)
            (VP (VBZ has)
              (NP (DT the) (NN ability)
                (S
                  (VP (TO to)
                    (VP (VB make)
                      (NP (JJR more) (NNS children)))))))))))
    (. .)))

