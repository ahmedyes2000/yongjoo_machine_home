  (S
    (NP (PRP I))
    (VP (MD 'd)
      (VP (VB like)
        (S
          (VP (TO to)
            (VP (VB see)
              (NP (DT those) (NNS statistics))
              (PRN (, ,)
                (INTJ (VB please))))))))
    (. .)))

