  (S
    (PP (IN In)
      (NP (DT some) (NNS cases)))
    (NP (DT an) (NN abortion))
    (VP (MD may)
      (VP (VB be)
        (NP
          (NP (NN life))
          (VP (VBG saving)
            (PP (TO to)
              (NP (DT the) (JJ pregnant) (NN woman)))))))
    (. .)))

det(cases-3, some-2)
prep_in(life-8, cases-3)
det(abortion-5, an-4)
