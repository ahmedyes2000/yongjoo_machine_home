(ROOT
  (S
    (ADVP (RB Therefore))
    (NP
      (NP (DT no) (NN reason))
      (SBAR
        (S
          (NP (PRP you))
          (VP (VBD gave)
            (PP (IN for)
              (NP (NN abortion)))))))
    (VP (VBZ is)
      (ADJP (JJ adequate)))
    (. .)))

advmod(adequate-9, Therefore-1)
det(reason-3, no-2)
dobj(gave-5, reason-3)
nsubj(adequate-9, reason-3)
nsubj(gave-5, you-4)
rcmod(reason-3, gave-5)
prep_for(gave-5, abortion-7)
cop(adequate-9, is-8)
root(ROOT-0, adequate-9)

