(ROOT
  (S
    (NP (NNP Plainsman))
    (, ,)
    (ADVP (RB Perhaps))
    (NP (PRP I))
    (VP (VBD failed)
      (PP (TO to)
        (NP (NN word)))
      (NP (PRP$ my) (NN post)
        (S
          (VP (TO to)
            (VP
              (ADVP (RB correctly))
              (VB show)
              (NP
                (NP (DT the) (NN basis))
                (PP (IN of)
                  (NP (PRP$ my) (JJ pro-choice) (NN stance)))))))))
    (. .)))

nsubj(failed-5, Plainsman-1)
advmod(failed-5, Perhaps-3)
nsubj(failed-5, I-4)
root(ROOT-0, failed-5)
prep_to(failed-5, word-7)
poss(post-9, my-8)
dobj(failed-5, post-9)
