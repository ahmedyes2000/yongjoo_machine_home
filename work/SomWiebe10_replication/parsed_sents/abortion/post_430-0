(ROOT
  (S
    (NP (DT An) (NN embryo))
    (VP (VBZ requires)
      (NP (DT a) (NN host)
        (S
          (VP (TO to)
            (VP (VB survive))))))
    (. .)))

det(embryo-2, An-1)
nsubj(requires-3, embryo-2)
root(ROOT-0, requires-3)
det(host-5, a-4)
dobj(requires-3, host-5)
aux(survive-7, to-6)
infmod(host-5, survive-7)

