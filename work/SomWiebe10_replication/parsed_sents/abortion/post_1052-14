(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP we))
        (VP (VBP make)
          (S
            (NP (NN abortion))
            (ADJP (JJ illegal))))))
    (, ,)
    (NP (NN adoption) (NNS agencies))
    (VP (MD might)
      (VP (VB start)
        (S
          (VP (TO to)
            (VP (VB look)
              (PP (IN like)
                (NP (NN dog) (NNS pounds))))))))
    (. .)))

mark(make-3, If-1)
nsubj(make-3, we-2)
advcl(start-10, make-3)
nsubj(illegal-5, abortion-4)
xcomp(make-3, illegal-5)
nn(agencies-8, adoption-7)
nsubj(start-10, agencies-8)
xsubj(look-12, agencies-8)
aux(start-10, might-9)
root(ROOT-0, start-10)
aux(look-12, to-11)
xcomp(start-10, look-12)
nn(pounds-15, dog-14)
prep_like(look-12, pounds-15)

