(ROOT
  (S
    (NP (PRP We))
    (VP (VBP are)
      (NP
        (NP (DT all) (CD one) (NN family))
        (PP (IN in)
          (NP (PRP$ His) (NNS eyes)))))
    (. .)))

nsubj(family-5, We-1)
cop(family-5, are-2)
det(family-5, all-3)
num(family-5, one-4)
root(ROOT-0, family-5)
poss(eyes-8, His-7)
prep_in(family-5, eyes-8)

