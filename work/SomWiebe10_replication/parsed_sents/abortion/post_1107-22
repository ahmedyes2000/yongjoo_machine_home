(ROOT
  (S (CC And)
    (NP (NNS kids))
    (VP (MD should)
      (ADVP (RB never))
      (VP (VB get)
        (NP (DT the) (NN death) (NN penalty))
        (SBAR (IN because)
          (S
            (NP (PRP$ their) (NNS parents))
            (VP (VBP do) (RB n't)
              (VP (VB want)
                (S
                  (VP (TO to)
                    (VP (VB take)
                      (NP (NN responsibility))
                      (PP (IN for)
                        (NP (PRP them))))))))))))
    (. .)))

cc(get-5, And-1)
nsubj(get-5, kids-2)
aux(get-5, should-3)
neg(get-5, never-4)
root(ROOT-0, get-5)
det(penalty-8, the-6)
nn(penalty-8, death-7)
dobj(get-5, penalty-8)
mark(want-14, because-9)
poss(parents-11, their-10)
nsubj(want-14, parents-11)
xsubj(take-16, parents-11)
aux(want-14, do-12)
neg(want-14, n't-13)
advcl(get-5, want-14)
aux(take-16, to-15)
xcomp(want-14, take-16)
dobj(take-16, responsibility-17)
prep_for(take-16, them-19)

