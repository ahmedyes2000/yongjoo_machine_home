(ROOT
  (S
    (NP (DT The) (JJ other) (NN question))
    (VP (VBZ is)
      (SBARQ
        (WHADVP (WRB why))
        (SQ (VBZ is)
          (NP (DT this) (`` ``) (NN sin) ('' ''))
          (ADJP (RBR more) (JJ important)
            (PP (IN than)
              (NP (PDT all) (DT the) (NNS others)))))))
    (. .)))

det(question-3, The-1)
amod(question-3, other-2)
nsubj(is-4, question-3)
root(ROOT-0, is-4)
advmod(important-12, why-5)
cop(important-12, is-6)
det(sin-9, this-7)
nsubj(important-12, sin-9)
advmod(important-12, more-11)
dep(is-4, important-12)
predet(others-16, all-14)
