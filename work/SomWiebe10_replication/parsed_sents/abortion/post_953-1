(ROOT
  (S (RB So)
    (VP (VB let)
      (S
        (NP (PRP me))
        (VP (VB modify)
          (NP
            (SBAR
              (WHNP (WP what))
              (S
                (NP (PRP I))
                (VP (VBD said))))
            (: ;)
            (NP
              (NP (DT the) (NNS reasons))
              (VP
                (ADVP (RB commonly))
                (VBN given)
                (SBAR (IN for)
                  (S
                    (NP (NNS abortions))
                    (VP (VBP are)
                      (PP (IN in)
                        (ADJP (JJ adequate))))))))))))
    (. .)))

advmod(let-2, So-1)
root(ROOT-0, let-2)
nsubj(modify-4, me-3)
ccomp(let-2, modify-4)
dobj(said-7, what-5)
rel(said-7, what-5)
nsubj(said-7, I-6)
dep(reasons-10, said-7)
det(reasons-10, the-9)
dobj(modify-4, reasons-10)
advmod(given-12, commonly-11)
partmod(reasons-10, given-12)
mark(are-15, for-13)
nsubj(are-15, abortions-14)
advcl(given-12, are-15)
prep_in(are-15, adequate-17)

