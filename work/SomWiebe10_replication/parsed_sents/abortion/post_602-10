  (S
    (NP (PRP We))
    (VP (MD could)
      (VP (VB say)
        (SBAR (IN that)
          (S
            (NP (DT the) (NN inconvenience))
            (VP (VBZ is)
              (NP
                (NP (DT a) (JJ direct) (NN consequence))
                (PP (IN of)
                  (NP (PRP$ her) (JJ adult) (NN decision)))))))))
    (. .)))

