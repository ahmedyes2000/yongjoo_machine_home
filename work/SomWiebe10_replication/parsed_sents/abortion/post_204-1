(ROOT
  (S
    (PP (IN On)
      (NP
        (NP (DT both) (NNS ends))
        (PRN (-LRB- -LRB-)
          (NP (NN abortion)
            (CC and)
            (NNS euthanasia))
          (-RRB- -RRB-))))
    (NP (NN life))
    (VP (VBZ needs)
      (S
        (VP (TO to)
          (VP (VB be)
            (VP (VBN protected))))))
    (. .)))

det(ends-3, both-2)
prep_on(needs-10, ends-3)
appos(ends-3, abortion-5)
appos(ends-3, euthanasia-7)
conj_and(abortion-5, euthanasia-7)
nsubj(needs-10, life-9)
