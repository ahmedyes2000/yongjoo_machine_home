(ROOT
  (S
    (NP (DT That) (NN money))
    (VP (MD should)
      (VP (VB have)
        (VP
          (VP (VBN gone)
            (PP (TO to)
              (NP
                (NP (NNS things))
                (PP (IN like)
                  (NP (NNS homlessness))))))
          (CC or)
          (VP
            (NP (WRB how))
            (PP (IN about)
              (NP (DT the) (JJ foster) (NN care) (NN system)))))))
    (. .)))

det(money-2, That-1)
nsubj(gone-5, money-2)
aux(gone-5, should-3)
aux(gone-5, have-4)
