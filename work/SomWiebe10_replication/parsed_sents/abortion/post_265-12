  (S
    (ADVP (RB Maybe))
    (NP (NNS people))
    (VP (MD should)
      (VP (VB decide)
        (PP (IN as)
          (NP (DT a) (NN couple)))
        (SBAR
          (WHADVP (WRB where))
          (S
            (NP (PRP they))
            (VP (VBP stand)
              (PP (IN on)
                (NP (DT the) (NN abortion) (NN debate)))
              (PP (IN before)
                (S
                  (VP (VBG engaging)
                    (PP (IN in)
                      (NP (NN sex)))))))))))
    (. .)))

