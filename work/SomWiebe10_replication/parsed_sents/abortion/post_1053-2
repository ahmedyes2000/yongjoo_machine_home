(ROOT
  (S
    (ADVP (IN At) (JJS least))
    (NP (PRP you))
    (VP (MD will)
      (VP (VB be)
        (VP (VBG preserving)
          (NP (DT a) (JJ human) (NN life))
          (PP (RB rather) (IN than)
            (S
              (VP (VBG killing)
                (NP (CD one))))))))
    (. .)))

advmod(preserving-6, At-1)
pobj(At-1, least-2)
nsubj(preserving-6, you-3)
aux(preserving-6, will-4)
aux(preserving-6, be-5)
root(ROOT-0, preserving-6)
det(life-9, a-7)
amod(life-9, human-8)
dobj(preserving-6, life-9)
advmod(preserving-6, rather-10)
prepc_than(preserving-6, killing-12)
dobj(killing-12, one-13)

