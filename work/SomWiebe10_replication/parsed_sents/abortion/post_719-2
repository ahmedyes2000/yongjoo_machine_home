(ROOT
  (S
    (PP (IN At)
      (NP (DT the) (JJ bare) (NN minimum)))
    (NP (NN abortion))
    (VP (VBZ is)
      (NP
        (NP (DT the) (NN taking))
        (PP (IN of)
          (NP
            (NP (CD one) (NN person) (POS 's))
            (NN life)))))
    (. .)))

det(minimum-4, the-2)
amod(minimum-4, bare-3)
prep_at(taking-8, minimum-4)
nsubj(taking-8, abortion-5)
cop(taking-8, is-6)
det(taking-8, the-7)
root(ROOT-0, taking-8)
num(person-11, one-10)
poss(life-13, person-11)
