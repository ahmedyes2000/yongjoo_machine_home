(ROOT
  (S
    (S
      (NP (PRP You))
      (VP (VBP give)
        (NP (DT no) (NNS reasons))
        (PP (TO to)
          (PP (IN for)
            (NP (PRP$ your) (NN opinion))))))
    (: ...)
    (S
      (INTJ (VB please))
      (VP (VB do)
        (ADVP (RB so))))
    (. .)))

nsubj(give-2, You-1)
root(ROOT-0, give-2)
det(reasons-4, no-3)
dobj(give-2, reasons-4)
prep(give-2, to-5)
pcomp(to-5, for-6)
poss(opinion-8, your-7)
prep_for(to-5, opinion-8)
dep(do-11, please-10)
parataxis(give-2, do-11)
advmod(do-11, so-12)

(ROOT
  (S
    (VP (VB Im)
      (ADJP (JJ interested)
        (PP (IN in)
          (NP (DT this) (NN topic)))))))

root(ROOT-0, Im-1)
acomp(Im-1, interested-2)
det(topic-5, this-4)
prep_in(interested-2, topic-5)

