(ROOT
  (S
    (ADJP (VB No.))
    (. .)))

root(ROOT-0, No.-1)

(ROOT
  (SBARQ
    (SBAR (IN Because)
      (S
        (NP (PRP it))
        (VP (VBZ 's) (RB not)
          (NP (RB truly) (DT a))
          (S
            (VP (VBG being)
              (ADVP (RB yet)))))))
    (, ,)
    (SQ (VBZ is)
      (NP (PRP it)))
    (. ?)))

mark(a-6, Because-1)
nsubj(a-6, it-2)
cop(a-6, 's-3)
neg(a-6, not-4)
advmod(a-6, truly-5)
advcl(is-10, a-6)
xcomp(a-6, being-7)
advmod(being-7, yet-8)
root(ROOT-0, is-10)
nsubj(is-10, it-11)

