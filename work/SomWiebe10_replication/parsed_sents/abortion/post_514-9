  (S
    (SBAR (IN That)
      (FRAG
        (ADJP (JJ sad))))
    (, ,)
    (NP
      (NP (NN era))
      (, ,)
      (PP (IN of)
        (NP (NN ignorance)
          (CC and)
          (NN arrogance)))
      (, ,))
    (VP (VBZ is)
      (ADJP (RP over)))
    (. .)))

complm(sad-2, That-1)
