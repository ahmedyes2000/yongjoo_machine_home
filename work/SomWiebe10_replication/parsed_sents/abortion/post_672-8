(ROOT
  (S
    (ADVP (RB As) (RB long)
      (SBAR (IN as)
        (S
          (NP (PRP we))
          (VP (VBP 're)
            (VP (VBG talking)
              (PP (IN about)
                (NP (NN logic))))))))
    (, ,)
    (NP (PRP we))
    (VP (MD may)
      (ADVP (RB as) (RB well))
      (VP (VB examine)
        (NP (JJ actual) (JJ logical) (NNS constructs))))
    (. .)))

advmod(long-2, As-1)
advmod(examine-14, long-2)
mark(talking-6, as-3)
