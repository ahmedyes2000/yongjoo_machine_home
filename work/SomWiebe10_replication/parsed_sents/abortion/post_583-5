  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP (VBP think)
          (SBAR
            (S
              (NP (NN war))
              (VP (VBZ is)
                (ADVP (RB sometimes))
                (VP (VBN justified)
                  (ADVP (RB then)))))))))
    (NP (PRP I))
    (VP (VBP do) (RB n't)
      (VP (VB see)
        (SBAR
          (WHADVP (WRB how))
          (S
            (NP (DT this))
            (VP (VBZ is) (`` ``)
              (NP
                (NP (DT the) (NN issue))
                (ADVP (RB here))))))))
    (. .)))

mark(think-3, If-1)
nsubj(think-3, you-2)
advcl(see-12, think-3)
nsubjpass(justified-7, war-4)
