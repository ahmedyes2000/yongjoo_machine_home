(ROOT
  (S
    (NP (JJS Most) (NNS mammals))
    (VP (VBP have)
      (NP (DT a) (NN right)
        (S
          (VP (TO to)
            (VP (VB humane)
              (NP (NN treatment)))))))
    (. .)))

amod(mammals-2, Most-1)
nsubj(have-3, mammals-2)
root(ROOT-0, have-3)
det(right-5, a-4)
dobj(have-3, right-5)
aux(humane-7, to-6)
infmod(right-5, humane-7)
dobj(humane-7, treatment-8)

