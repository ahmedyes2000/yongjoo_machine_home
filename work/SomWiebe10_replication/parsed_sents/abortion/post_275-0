(ROOT
  (S
    (NP
      (NP (DT The) (NN morning))
      (PP (IN after)
        (NP (NN pill))))
    (VP (VBZ has)
      (NP (DT a) (NN failure) (NN rate)))
    (. .)))

det(morning-2, The-1)
nsubj(has-5, morning-2)
prep_after(morning-2, pill-4)
root(ROOT-0, has-5)
det(rate-8, a-6)
nn(rate-8, failure-7)
dobj(has-5, rate-8)

