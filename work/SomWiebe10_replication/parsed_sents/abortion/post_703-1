  (S
    (SBAR (IN If)
      (S
        (S
          (NP (PRP it))
          (VP (VBD were)
            (ADVP (RB just))
            (PP (IN about)
              (NP (NNS eggs)
                (CC and)
                (NN sperm)))))
        (, ,) (RB then)
        (S
          (NP (PRP we))
          (VP (VBD got)
            (NP
              (NP (NNS lots))
              (PP (IN of)
                (NP (NN life))))
            (S
              (VP (TO to)
                (VP (VB save))))))))
    (, ,)
    (NP (NNS lots))
    (ADVP (RBR more) (IN than))
    (VP (VBZ is)
      (ADJP (JJ possible)))
    (. .)))

