(ROOT
  (S
    (NP (NN murder))
    (VP (VBZ is) (RB not)
      (ADJP (JJ legal))
      (SBAR (IN so)
        (S
          (NP (FW i))
          (VP (VBP 'm) (RB not)
            (ADJP (JJ sure))
            (SBAR
              (WHADVP (WRB why))
              (S
                (NP (NN abortion))
                (VP (VBZ is)
                  (ADVP (RB even))
                  (NP
                    (NP (DT a) (NN debateRoe))
                    (PP (IN of)
                      (NP (NNP roe)
                        (CC vs.)
                        (NNP wade)))))))))))
    (. .)))

nsubj(legal-4, murder-1)
cop(legal-4, is-2)
neg(legal-4, not-3)
root(ROOT-0, legal-4)
mark(sure-9, so-5)
nsubj(sure-9, i-6)
cop(sure-9, 'm-7)
neg(sure-9, not-8)
advcl(legal-4, sure-9)
advmod(debateRoe-15, why-10)
