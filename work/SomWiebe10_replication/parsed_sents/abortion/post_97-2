(ROOT
  (S
    (NP (DT This))
    (VP (VBZ avoids)
      (NP (PDT both) (DT the) (NN suffering))
      (PP
        (PP (IN of)
          (NP (DT the) (NNS parents)))
        (CC and)
        (PP (IN of)
          (NP (DT the) (NN child)))))
    (. .)))

nsubj(avoids-2, This-1)
root(ROOT-0, avoids-2)
predet(suffering-5, both-3)
det(suffering-5, the-4)
dobj(avoids-2, suffering-5)
det(parents-8, the-7)
prep_of(avoids-2, parents-8)
det(child-12, the-11)
prep_of(avoids-2, child-12)
