(ROOT
  (NP
    (NP (NNP Personhood))
    (: :)
    (NP
      (NP (DT the) (NN quality))
      (PP (IN of)
        (S
          (VP (VBG being)
            (NP (DT a) (JJ human) (NN individual))))))
    (. .)))

root(ROOT-0, Personhood-1)
det(quality-4, the-3)
dep(Personhood-1, quality-4)
cop(individual-9, being-6)
det(individual-9, a-7)
amod(individual-9, human-8)
prepc_of(quality-4, individual-9)

