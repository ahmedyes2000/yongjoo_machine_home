(ROOT
  (S
    (NP (PRP You))
    (VP (MD will)
      (VP (VB say)
        (SBAR (IN that)
          (S
            (NP
              (NP (JJ hurts) (NNS people))
              (CC and)
              (NP (NN sex)))
            (VP (VBZ does) (RB n't))))))
    (. .)))

nsubj(say-3, You-1)
aux(say-3, will-2)
root(ROOT-0, say-3)
complm(does-9, that-4)
amod(people-6, hurts-5)
nsubj(does-9, people-6)
conj_and(people-6, sex-8)
nsubj(does-9, sex-8)
ccomp(say-3, does-9)
neg(does-9, n't-10)

