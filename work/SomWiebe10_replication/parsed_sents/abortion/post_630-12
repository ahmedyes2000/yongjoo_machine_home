(ROOT
  (S
    (NP (PRP I))
    (VP (VBP share)
      (NP
        (NP (JJR more))
        (PP (IN of)
          (NP
            (NP (DT the) (JJ true) (NNS values))
            (PP (IN of)
              (NP
                (NP (NN conservatism))
                (PRN (-LRB- -LRB-)
                  (NP (JJ old-school))
                  (-RRB- -RRB-)))))))
      (SBAR (IN than)
        (S
          (NP (PRP I))
          (VP (VBP do)
            (NP (NN socialism))))))
    (. .)))

nsubj(share-2, I-1)
root(ROOT-0, share-2)
dobj(share-2, more-3)
det(values-7, the-5)
