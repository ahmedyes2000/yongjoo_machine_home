  (S
    (NP (DT The) (JJ biological) (NN heritage) (NN usage))
    (VP (VBZ constitutes)
      (NP
        (NP (DT an) (JJ alternate) (NN definition))
        (PP (IN of)
          (NP (DT the) (NN word)))))
    (. .)))

det(usage-4, The-1)
amod(usage-4, biological-2)
nn(usage-4, heritage-3)
nsubj(constitutes-5, usage-4)
root(ROOT-0, constitutes-5)
det(definition-8, an-6)
