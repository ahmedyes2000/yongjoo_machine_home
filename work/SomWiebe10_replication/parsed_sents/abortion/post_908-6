(ROOT
  (S
    (S
      (NP (PRP He)
        (CC or)
        (PRP she))
      (VP (VBZ is)
        (ADVP (RB also))
        (VP (VBG breaking)
          (NP (DT the) (JJ first) (NN commandment)))))
    (: :)
    (S
      (NP (PRP You))
      (VP (MD shall)
        (VP (VB have)
          (NP (DT no) (JJ other) (NNS gods)))))
    (. .)))

nsubj(breaking-6, He-1)
conj_or(He-1, she-3)
nsubj(breaking-6, she-3)
aux(breaking-6, is-4)
advmod(breaking-6, also-5)
root(ROOT-0, breaking-6)
det(commandment-9, the-7)
amod(commandment-9, first-8)
dobj(breaking-6, commandment-9)
nsubj(have-13, You-11)
aux(have-13, shall-12)
parataxis(breaking-6, have-13)
det(gods-16, no-14)
amod(gods-16, other-15)
dobj(have-13, gods-16)

