(ROOT
  (S
    (SBAR (IN Since)
      (S
        (NP (RB almost) (DT every) (NN circumstance))
        (VP (VBZ is)
          (ADJP (JJ unique)))))
    (NP (DT no) (JJ specific) (NN rule))
    (VP (MD can)
      (VP (VB be)
        (VP
          (ADVP (RB fairly))
          (VBN applied)
          (SBAR (IN that)
            (S
              (VP (MD would)
                (VP (VB limit)
                  (PP (IN against)
                    (NP (NN abortion))))))))))
    (. .)))

mark(unique-6, Since-1)
advmod(circumstance-4, almost-2)
det(circumstance-4, every-3)
nsubj(unique-6, circumstance-4)
cop(unique-6, is-5)
advcl(applied-13, unique-6)
det(rule-9, no-7)
amod(rule-9, specific-8)
nsubjpass(applied-13, rule-9)
aux(applied-13, can-10)
auxpass(applied-13, be-11)
advmod(applied-13, fairly-12)
root(ROOT-0, applied-13)
complm(limit-16, that-14)
aux(limit-16, would-15)
ccomp(applied-13, limit-16)
prep_against(limit-16, abortion-18)

