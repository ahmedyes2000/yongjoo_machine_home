(ROOT
  (S
    (NP (NN Abortion) (NNS opponents))
    (VP (MD might)
      (ADVP (RB as) (RB well))
      (VP (VB consider)
        (S
          (NP (NNP Mother) (NNP Nature))
          (NP
            (NP (DT the) (NN world) (POS 's))
            (JJS largest) (NN abortion) (NN mill)))))
    (. .)))

nn(opponents-2, Abortion-1)
nsubj(consider-6, opponents-2)
aux(consider-6, might-3)
advmod(well-5, as-4)
mwe(well-5, as-4)
advmod(consider-6, well-5)
root(ROOT-0, consider-6)
nn(Nature-8, Mother-7)
nsubj(mill-14, Nature-8)
det(world-10, the-9)
poss(mill-14, world-10)
amod(mill-14, largest-12)
nn(mill-14, abortion-13)
xcomp(consider-6, mill-14)

