(ROOT
  (SQ (MD Are) (RB n't)
    (NP (PRP you))
    (VP (VB glad)
      (S
        (NP (PRP$ your) (NN mother))
        (VP (VB put)
          (NP (PRP$ your) (NN life))
          (PP (IN before)
            (NP (NNS hers))))))
    (. ?)))

aux(glad-4, Are-1)
neg(glad-4, n't-2)
nsubj(glad-4, you-3)
root(ROOT-0, glad-4)
poss(mother-6, your-5)
nsubj(put-7, mother-6)
ccomp(glad-4, put-7)
poss(life-9, your-8)
dobj(put-7, life-9)
prep_before(put-7, hers-11)

