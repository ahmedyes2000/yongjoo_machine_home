(ROOT
  (S
    (SBAR
      (WHADVP (WRB When))
      (S
        (NP (NN abortion))
        (VP (VBZ occurs)
          (PP (IN for)
            (NP (DT no) (NN reason))))))
    (, ,)
    (NP (PRP it))
    (VP (VBZ is)
      (VP (VBN considered)
        (PP (IN as)
          (NP (NN murder)))))
    (. .)))

advmod(occurs-3, When-1)
nsubj(occurs-3, abortion-2)
advcl(considered-10, occurs-3)
det(reason-6, no-5)
prep_for(occurs-3, reason-6)
nsubjpass(considered-10, it-8)
auxpass(considered-10, is-9)
root(ROOT-0, considered-10)
prep_as(considered-10, murder-12)

