(ROOT
  (S
    (S
      (NP (PRP You))
      (VP (MD should)
        (VP (VB have)
          (NP (DT a) (VBG living) (NN will))
          (SBAR
            (SBAR
              (WHNP (WDT that))
              (S
                (VP (VBZ gives)
                  (PRT (RP away))
                  (NP
                    (NP (PRP$ your) (NNS eyes))
                    (, ,)
                    (CC and)
                    (NP (PRP$ your) (NN heart))))))
            (CC and)
            (SBAR
              (WHNP (WDT whatever))
              (S
                (ADVP (RB else))
                (VP (VBZ is)
                  (ADJP (JJ salvageable)
                    (PP (IN in)
                      (NP
                        (NP (DT the) (NN event))
                        (SBAR
                          (S
                            (NP (PRP you))
                            (VP (VBP 're)
                              (VP (VBN made)
                                (S
                                  (NP (NN brain))
                                  (ADJP (JJ dead)))))))))))))))))
    (, ,)
    (CC or)
    (S
      (NP (PRP it))
      (VP (VBZ 's)
        (ADJP (JJ clear)
          (SBAR
            (S
              (NP (PRP you))
              (VP (MD wo) (RB n't)
                (VP (VB live))))))))
    (. .)))

