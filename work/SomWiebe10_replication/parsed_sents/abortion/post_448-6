(ROOT
  (S
    (NP (PRP You))
    (VP (VBP tell)
      (NP (NNS women))
      (SBAR (IN that)
        (S
          (NP (PRP it))
          (VP (VBZ 's)
            (NP (PRP$ their) (NN fault))
            (SBAR (IN that)
              (S
                (NP (PRP they))
                (VP (VBD got)
                  (ADJP (JJ pregnant)))))))))
    (. .)))

nsubj(tell-2, You-1)
root(ROOT-0, tell-2)
dobj(tell-2, women-3)
complm(fault-8, that-4)
nsubj(fault-8, it-5)
cop(fault-8, 's-6)
poss(fault-8, their-7)
ccomp(tell-2, fault-8)
complm(pregnant-12, that-9)
nsubj(pregnant-12, they-10)
dep(pregnant-12, got-11)
ccomp(fault-8, pregnant-12)

