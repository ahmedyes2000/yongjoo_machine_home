(ROOT
  (SINV
    (S
      (INTJ (UH Yes) (, ,) (UH yes))
      (NP (PRP you))
      (ADVP (RB apparently))
      (VP (VBP are) (, ,)
        (PP (IN for)
          (NP (PRP I)))
        (ADVP (CC but))))
    (VP (VBP mirror))
    (NP (PRP$ your) (NN argument))
    (. .)))

dep(are-6, Yes-1)
dep(Yes-1, yes-3)
nsubj(are-6, you-4)
advmod(are-6, apparently-5)
dep(mirror-11, are-6)
prep_for(are-6, I-9)
advmod(are-6, but-10)
root(ROOT-0, mirror-11)
poss(argument-13, your-12)
nsubj(mirror-11, argument-13)

