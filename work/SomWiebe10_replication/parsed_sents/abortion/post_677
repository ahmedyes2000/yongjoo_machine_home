(ROOT
  (S (CC Or)
    (NP (PRP they))
    (VP (MD could)
      (VP
        (VP (VB move)
          (PP (TO to)
            (NP (NNP Afghanistan))))
        (CC and)
        (VP (VB join)
          (NP
            (NP (DT some))
            (PP (IN of)
              (NP (DT the) (NNS radicals))))
          (ADVP (RB there)))))
    (. .)))

