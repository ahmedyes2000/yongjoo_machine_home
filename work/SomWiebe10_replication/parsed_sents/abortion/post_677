nsubj(written-5, Well-1)
root(ROOT-0, said-2)
advmod(written-5, well-4)
conj_and(said-2, written-5)

(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (JJ other) (NNS folks))
        (VP (VBP want))))
    (NP (PRP it))
    (VP (VBD changed)
      (SBAR
        (S
          (NP (PRP they))
          (VP (VBP need)
            (S
              (VP (TO to)
                (VP
                  (VP (VB change)
                    (NP (DT the) (NN law)))
                  (CC and)
                  (VP (VB leave)
                    (NP (NN everyone) (RB else))
                    (ADVP (RB alone))))))))))
    (. .)))

mark(want-4, If-1)
