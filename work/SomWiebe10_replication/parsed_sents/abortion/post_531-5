(ROOT
  (S
    (NP (DT This))
    (, ,)
    (PP (IN in)
      (NP (PRP itself)))
    (, ,)
    (VP (VBZ is)
      (ADJP (RB completely)
        (PP (IN against)
          (NP (NN everything))))
      (SBAR (IN that)
        (S
          (NP (NNP America))
          (, ,)
          (PP (IN in)
            (NP (PRP$ my) (NN opinion)))
          (, ,)
          (VP (VBZ stands)
            (PP (IN for))))))
    (. .)))

nsubj(completely-7, This-1)
prep_in(completely-7, itself-4)
cop(completely-7, is-6)
root(ROOT-0, completely-7)
prep_against(completely-7, everything-9)
complm(stands-17, that-10)
nsubj(stands-17, America-11)
poss(opinion-15, my-14)
prep_in(stands-17, opinion-15)
ccomp(completely-7, stands-17)
prep(stands-17, for-18)

