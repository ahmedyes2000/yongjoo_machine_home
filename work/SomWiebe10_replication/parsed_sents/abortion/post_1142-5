(ROOT
  (S (CC But)
    (SBAR
      (WHNP (WP what))
      (S
        (NP (DT a) (JJ awful) (NN one))
        (VP (TO to)
          (VP (VB make)))))
    (, ,)
    (NP (PRP they))
    (VP (MD will)
      (VP (VB live)
        (PP (IN with)
          (NP (PRP it)))
        (PP (IN for)
          (NP
            (NP (DT the) (NN rest))
            (PP (IN of)
              (NP (PRP$ their) (NN life)))))))
    (. .)))

