(ROOT
  (S
    (PP (IN In)
      (NP (DT a) (JJ free) (NN society)))
    (, ,)
    (NP (NN abortion))
    (VP (VBZ is)
      (ADVP (RB truly))
      (NP
        (NP (DT a) (NN matter))
        (PP (IN of)
          (NP (JJ personal) (NN belief)))))
    (. .)))

det(society-4, a-2)
amod(society-4, free-3)
prep_in(matter-10, society-4)
nsubj(matter-10, abortion-6)
cop(matter-10, is-7)
advmod(matter-10, truly-8)
det(matter-10, a-9)
root(ROOT-0, matter-10)
amod(belief-13, personal-12)
prep_of(matter-10, belief-13)

