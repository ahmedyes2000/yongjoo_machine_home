(ROOT
  (S (CC But)
    (NP (PRP you))
    (VP (MD ca) (RB n't)
      (VP (VB kill)
        (NP (DT another) (JJ human) (NN being))
        (SBAR (RB just) (IN because)
          (S
            (NP (PRP$ their) (NN existence))
            (VP (VBZ makes)
              (S
                (NP (PRP$ your) (NN life))
                (ADJP
                  (ADJP (JJ physically))
                  (CC or)
                  (ADJP (RB emotionally) (JJ burdensome)))))))))
    (. .)))

cc(kill-5, But-1)
nsubj(kill-5, you-2)
aux(kill-5, ca-3)
neg(kill-5, n't-4)
root(ROOT-0, kill-5)
det(being-8, another-6)
amod(being-8, human-7)
dobj(kill-5, being-8)
advmod(makes-13, just-9)
dep(makes-13, because-10)
poss(existence-12, their-11)
nsubj(makes-13, existence-12)
dep(kill-5, makes-13)
poss(life-15, your-14)
nsubj(physically-16, life-15)
nsubj(burdensome-19, life-15)
xcomp(makes-13, physically-16)
advmod(burdensome-19, emotionally-18)
xcomp(makes-13, burdensome-19)
conj_or(physically-16, burdensome-19)

