(ROOT
  (S
    (NP (DT The) (NN issue))
    (VP (VBZ is) (RB not)
      (SBAR
        (SBAR
          (WHADVP (WRB when))
          (S
            (NP (NN life))
            (VP (VBZ begins))))
        (, ,)
        (CC or)
        (SBAR (IN if)
          (S
            (NP (EX there))
            (VP (VBZ is)
              (NP
                (NP (DT a) (NN life))
                (PP (IN at)
                  (NP (DT all)))))))))
    (. .)))

det(issue-2, The-1)
nsubj(is-3, issue-2)
root(ROOT-0, is-3)
neg(is-3, not-4)
advmod(begins-7, when-5)
nsubj(begins-7, life-6)
dep(is-3, begins-7)
mark(is-12, if-10)
expl(is-12, there-11)
dep(is-3, is-12)
conj_or(begins-7, is-12)
det(life-14, a-13)
