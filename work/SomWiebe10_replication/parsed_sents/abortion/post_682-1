(ROOT
  (S
    (VP (VB Thank)
      (NP (PRP you))
      (PP (IN for)
        (S
          (VP (VBG being)
            (ADJP (RB so) (JJ thoughtful))
            (PP (VBG regarding)
              (NP (DT this) (NN subject)))))))
    (. .)))

root(ROOT-0, Thank-1)
dobj(Thank-1, you-2)
cop(thoughtful-6, being-4)
advmod(thoughtful-6, so-5)
prepc_for(Thank-1, thoughtful-6)
det(subject-9, this-8)
prep_regarding(thoughtful-6, subject-9)

