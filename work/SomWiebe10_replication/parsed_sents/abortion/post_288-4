(ROOT
  (S (CC And)
    (ADVP (RB also))
    (, ,)
    (NP (DT a) (NN murderer))
    (VP (VBZ is)
      (NP
        (NP (NN someone))
        (SBAR
          (WHNP (WP who))
          (S
            (VP (VBZ takes)
              (SBAR
                (S
                  (NP (DT another))
                  (VP (VBZ 's)
                    (NP (NN life)))
                  (. .))))))))
    (. .)))

cc(someone-7, And-1)
advmod(someone-7, also-2)
det(murderer-5, a-4)
nsubj(someone-7, murderer-5)
cop(someone-7, is-6)
root(ROOT-0, someone-7)
nsubj(takes-9, someone-7)
rcmod(someone-7, takes-9)
nsubj(life-12, another-10)
cop(life-12, 's-11)
ccomp(takes-9, life-12)

