(ROOT
  (S
    (NP (DT Some) (NNS women))
    (VP (VBP have)
      (VP (VBN decided)
        (S
          (VP (TO to)
            (VP (VB have)
              (NP (DT the) (NN baby))
              (PP (IN after)
                (NP (DT a) (NN rape))))))))
    (. .)))

det(women-2, Some-1)
nsubj(decided-4, women-2)
xsubj(have-6, women-2)
aux(decided-4, have-3)
root(ROOT-0, decided-4)
aux(have-6, to-5)
xcomp(decided-4, have-6)
det(baby-8, the-7)
dobj(have-6, baby-8)
det(rape-11, a-10)
prep_after(have-6, rape-11)

