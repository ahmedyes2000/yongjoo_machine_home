(ROOT
  (S
    (PP (IN In)
      (NP (DT some)
        (ADJP (JJ unfortunate))
        (NNS cases)))
    (, ,)
    (NP (DT those) (NNS rights))
    (VP (VBP are)
      (PP (IN in)
        (NP (NN opposition))))
    (. .)))

det(cases-4, some-2)
amod(cases-4, unfortunate-3)
prep_in(are-8, cases-4)
det(rights-7, those-6)
nsubj(are-8, rights-7)
root(ROOT-0, are-8)
prep_in(are-8, opposition-10)

