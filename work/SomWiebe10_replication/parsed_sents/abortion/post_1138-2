(ROOT
  (S
    (NP (PRP You))
    (VP (VBD were)
      (ADVP (RB once)
        (PP (IN under)
          (NP
            (ADJP
              (NP (CD 10) (NNS weeks))
              (JJ old)
              (PP (IN in)
                (NP (PRP$ your) (NNS mothers))))
            (NN womb))
          (ADVP (RB as) (RB well)))))
    (. .)))

nsubj(were-2, You-1)
root(ROOT-0, were-2)
advmod(were-2, once-3)
num(weeks-6, 10-5)
npadvmod(old-7, weeks-6)
amod(womb-11, old-7)
poss(mothers-10, your-9)
prep_in(old-7, mothers-10)
prep_under(once-3, womb-11)
advmod(well-13, as-12)
mwe(well-13, as-12)
advmod(once-3, well-13)

