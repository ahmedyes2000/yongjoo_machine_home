(ROOT
  (S
    (S
      (NP (DT The) (NN baby))
      (VP (VBD was)
        (VP (VBG living))))
    (CC and)
    (S
      (NP (PRP she))
      (VP (VBD watched)
        (SBAR
          (S
            (NP (PRP it))
            (VP (VBP die))))))
    (. .)))

det(baby-2, The-1)
nsubj(living-4, baby-2)
aux(living-4, was-3)
root(ROOT-0, living-4)
nsubj(watched-7, she-6)
conj_and(living-4, watched-7)
nsubj(die-9, it-8)
ccomp(watched-7, die-9)

