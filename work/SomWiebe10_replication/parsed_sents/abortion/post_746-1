(ROOT
  (S
    (NP (PRP She))
    (VP (MD can)
      (VP (VB give)
        (NP (DT the) (NN child))
        (PP (TO to)
          (NP
            (NP (NN someone))
            (SBAR
              (WHNP (WP who))
              (S
                (VP (VBZ wants)
                  (NP (PRP it)))))))))
    (. .)))

nsubj(give-3, She-1)
aux(give-3, can-2)
root(ROOT-0, give-3)
det(child-5, the-4)
dobj(give-3, child-5)
prep_to(give-3, someone-7)
nsubj(wants-9, someone-7)
rcmod(someone-7, wants-9)
dobj(wants-9, it-10)

