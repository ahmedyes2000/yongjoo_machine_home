  (S
    (S
      (NP (DT The) (JJ new) (NN life))
      (VP (VBZ is)
        (ADJP (JJ reliant)
          (PP (IN on)
            (NP
              (NP (DT the) (NN mother) (POS 's))
              (NN body))))))
    (, ,)
    (CC but)
    (S
      (NP (PRP it))
      (VP (VBZ is)
        (NP (DT a) (JJ separate) (NN life))))
    (. .)))

