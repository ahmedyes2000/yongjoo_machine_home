(ROOT
  (SBARQ
    (WHADVP (WRB Where) (RB exactly))
    (SQ (VBP are)
      (NP (PRP you))
      (VP (VBG getting)
        (NP (DT this))))
    (. ?)))

advmod(exactly-2, Where-1)
advmod(getting-5, exactly-2)
aux(getting-5, are-3)
nsubj(getting-5, you-4)
root(ROOT-0, getting-5)
dobj(getting-5, this-6)

