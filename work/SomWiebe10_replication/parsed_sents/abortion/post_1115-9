  (S
    (NP (PRP It))
    (VP (VBZ 's)
      (NP (RB quite) (DT a) (NN jump)
        (S
          (VP (TO to)
            (VP (VB say)
              (SBAR (IN that)
                (S
                  (NP (NN sex))
                  (VP (VBZ is)
                    (ADVP (RB only))
                    (VP (VBN ok)
                      (PP (IN in)
                        (NP (DT a) (JJ lifelong) (JJ heterosexual) (NN marriage)))
                      (PP (IN except)
                        (PP (IN for)
                          (NP (JJ religious) (NNS reasons)))))))))))))
    (. .)))

