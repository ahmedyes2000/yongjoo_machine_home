(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is)
      (NP
        (NP
          (NP (DT an)
            (ADJP (JJ unfortunate))
            (NN fact))
          (PP (IN of)
            (NP (NN life)))
          (, ,)
          (ADJP (JJ similar)
            (PP (TO to)
              (NP (NN war))))
          (, ,)
          (ADJP (JJ similar)
            (PP (TO to)
              (NP (NN racism)))))
        (, ,)
        (ADJP (JJ similar) (TO to) (JJ obesity))))
    (. .)))

nsubj(fact-5, It-1)
cop(fact-5, is-2)
det(fact-5, an-3)
amod(fact-5, unfortunate-4)
root(ROOT-0, fact-5)
prep_of(fact-5, life-7)
amod(fact-5, similar-9)
prep_to(similar-9, war-11)
amod(fact-5, similar-13)
prep_to(similar-13, racism-15)
amod(obesity-19, similar-17)
dep(obesity-19, to-18)
amod(fact-5, obesity-19)

