(ROOT
  (S
    (SBAR (RB Just) (IN because)
      (S
        (NP (PRP you))
        (VP (MD may)
          (VP (VB be)
            (VP (VBN adopted))))))
    (VP (VBZ does) (RB not)
      (VP (VB mean)
        (SBAR
          (S
            (NP (PRP you))
            (ADVP (RB ll))
            (VP (VBP have)
              (NP (DT a)
                (ADJP (JJ miserable) (JJ unsuccessful))
                (NN life)))))))
    (. .)))

advmod(adopted-6, Just-1)
dep(adopted-6, because-2)
nsubjpass(adopted-6, you-3)
aux(adopted-6, may-4)
auxpass(adopted-6, be-5)
advcl(mean-9, adopted-6)
csubj(mean-9, adopted-6)
aux(mean-9, does-7)
neg(mean-9, not-8)
root(ROOT-0, mean-9)
nsubj(have-12, you-10)
advmod(have-12, ll-11)
ccomp(mean-9, have-12)
det(life-16, a-13)
amod(unsuccessful-15, miserable-14)
amod(life-16, unsuccessful-15)
dobj(have-12, life-16)

