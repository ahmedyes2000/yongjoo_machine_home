(ROOT
  (S
    (NP (PRP I))
    (VP (VBP do) (RB n't)
      (VP (VB say)
        (NP
          (NP (DT that)
            (ADJP (RBR better) (JJ sexual))
            (NN education) (NNS discredits) (NN abortion))
          (PP (IN as)
            (NP (DT a) (NN possibility))))))
    (. .)))

nsubj(say-4, I-1)
aux(say-4, do-2)
neg(say-4, n't-3)
root(ROOT-0, say-4)
det(abortion-10, that-5)
advmod(sexual-7, better-6)
amod(abortion-10, sexual-7)
nn(abortion-10, education-8)
nn(abortion-10, discredits-9)
dobj(say-4, abortion-10)
det(possibility-13, a-12)
prep_as(abortion-10, possibility-13)

