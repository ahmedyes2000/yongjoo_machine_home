(ROOT
  (S
    (NP (DT The) (JJ rational) (NN animal))
    (ADVP (RB alone))
    (VP (VBZ has)
      (NP (DT a) (JJ conceptual) (NN consciousness)))
    (. .)))

det(animal-3, The-1)
amod(animal-3, rational-2)
nsubj(has-5, animal-3)
advmod(has-5, alone-4)
root(ROOT-0, has-5)
det(consciousness-8, a-6)
amod(consciousness-8, conceptual-7)
dobj(has-5, consciousness-8)

