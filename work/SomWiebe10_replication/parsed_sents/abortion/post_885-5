(ROOT
  (S
    (NP (PRP$ Your) (NN decision))
    (VP (MD should)
      (VP (VB happen)
        (NP
          (NP (NNP BEFORE))
          (SBAR
            (S
              (NP (PRP you))
              (VP (VBP find)
                (NP (PRP yourself))
                (PP (IN in)
                  (NP (DT this) (NN position)))))))))
    (. .)))

poss(decision-2, Your-1)
nsubj(happen-4, decision-2)
aux(happen-4, should-3)
root(ROOT-0, happen-4)
dobj(happen-4, BEFORE-5)
nsubj(find-7, you-6)
rcmod(BEFORE-5, find-7)
dobj(find-7, yourself-8)
det(position-11, this-10)
prep_in(find-7, position-11)

