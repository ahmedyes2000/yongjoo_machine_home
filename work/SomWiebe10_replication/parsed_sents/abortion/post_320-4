(ROOT
  (S
    (PP (IN In)
      (NP (DT both) (NNS situations)))
    (, ,)
    (NP (NN someone))
    (VP (VBZ ends)
      (ADJP (IN up) (JJ dead)))
    (. .)))

det(situations-3, both-2)
prep_in(ends-6, situations-3)
nsubj(ends-6, someone-5)
root(ROOT-0, ends-6)
dep(dead-8, up-7)
acomp(ends-6, dead-8)

