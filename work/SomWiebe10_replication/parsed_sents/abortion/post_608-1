(ROOT
  (S
    (NP (NNP Jon))
    (VP (VBZ is)
      (ADJP (JJ right))
      (SBAR (IN that)
        (S
          (NP (DT the) (NN issue))
          (VP (VBZ is) (RB not)
            (SBAR
              (WHADVP (WRB when))
              (S
                (NP (NN life))
                (VP (VBZ begins))))
            (, ,)
            (SBAR (IN because)
              (S
                (NP (PRP we))
                (VP (VBP know)
                  (NP (DT that)))))))))
    (. .)))

nsubj(right-3, Jon-1)
cop(right-3, is-2)
root(ROOT-0, right-3)
