(ROOT
  (S
    (NP (NNP The))
    (`` ``)
    (S
      (VP (VBG doing) ('' '')))
    (VP (VBZ is)
      (ADVP (RB also))
      (ADVP (RB largely))
      (SBAR
        (WHNP (WP what))
        (S
          (NP (PRP we))
          (VP (VBP are)
            (PP (IN beyond)
              (NP
                (NP (DT a) (NN collection))
                (PP (IN of)
                  (NP (NN dna)))))))))
    (. .)))

nsubj(is-5, The-1)
dep(is-5, doing-3)
root(ROOT-0, is-5)
advmod(is-5, also-6)
advmod(is-5, largely-7)
dobj(are-10, what-8)
nsubj(are-10, we-9)
ccomp(is-5, are-10)
det(collection-13, a-12)
