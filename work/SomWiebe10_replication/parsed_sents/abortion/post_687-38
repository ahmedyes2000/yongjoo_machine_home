(ROOT
  (S
    (NP (DT That))
    (VP (VBZ 's)
      (ADVP (RB also))
      (NP (DT the) (NN reason))
      (SBAR (IN that)
        (S
          (NP (PRP I))
          (VP (VBP have)
            (NP (DT no) (NN problem))
            (PP (IN with)
              (NP
                (NP (NNS exceptions))
                (PP (IN for)
                  (NP
                    (NP (DT the) (NN life))
                    (PP (IN of)
                      (NP (DT the) (NN mother)))))))))))
    (. .)))

nsubj(reason-5, That-1)
cop(reason-5, 's-2)
advmod(reason-5, also-3)
det(reason-5, the-4)
root(ROOT-0, reason-5)
complm(have-8, that-6)
nsubj(have-8, I-7)
ccomp(reason-5, have-8)
det(problem-10, no-9)
dobj(have-8, problem-10)
prep_with(have-8, exceptions-12)
det(life-15, the-14)
prep_for(exceptions-12, life-15)
det(mother-18, the-17)
prep_of(life-15, mother-18)

