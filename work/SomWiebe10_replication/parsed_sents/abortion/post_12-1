(ROOT
  (S
    (S
      (NP (PRP We))
      (VP (VBP do)
        (NP
          (NP (DT this))
          (PP (IN with)
            (NP (PRP$ our) (NNS enemies))))
        (PP (IN in)
          (NP (NN wartime)))))
    (, ,)
    (CC or)
    (S
      (PP (IN with)
        (NP (NN anyone)))
      (NP (PRP we))
      (VP (MD would)
        (VP (VB like)
          (S
            (VP (TO to)
              (VP (VB enslave)
                (CC or)
                (VB exploit)))))))
    (. .)))

nsubj(do-2, We-1)
root(ROOT-0, do-2)
dobj(do-2, this-3)
poss(enemies-6, our-5)
prep_with(this-3, enemies-6)
prep_in(do-2, wartime-8)
prep_with(like-15, anyone-12)
nsubj(like-15, we-13)
xsubj(enslave-17, we-13)
aux(like-15, would-14)
conj_or(do-2, like-15)
aux(enslave-17, to-16)
xcomp(like-15, enslave-17)
xcomp(like-15, exploit-19)
conj_or(enslave-17, exploit-19)

