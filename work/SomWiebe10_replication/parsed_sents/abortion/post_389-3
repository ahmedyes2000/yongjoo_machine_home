(ROOT
  (S
    (NP
      (NP (DT Every) (NN organism))
      (SBAR
        (WHNP (WDT that))
        (S
          (VP (VBZ has)
            (NP (DT a) (NN brain))))))
    (, ,)
    (VP (VBZ has)
      (NP (NN brain) (NN activity)))
    (. .)))

det(organism-2, Every-1)
nsubj(has-4, organism-2)
nsubj(has-8, organism-2)
rcmod(organism-2, has-4)
det(brain-6, a-5)
dobj(has-4, brain-6)
root(ROOT-0, has-8)
nn(activity-10, brain-9)
dobj(has-8, activity-10)

