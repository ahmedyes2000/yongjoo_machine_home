(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is)
      (ADJP (JJ important)
        (S
          (VP (TO to)
            (VP (VB protect)
              (NP (PDT such) (DT a) (JJ voiceless) (NN minority))
              (PP (IN in)
                (NP (NN society))))))))
    (. .)))

nsubj(important-3, It-1)
cop(important-3, is-2)
root(ROOT-0, important-3)
aux(protect-5, to-4)
xcomp(important-3, protect-5)
predet(minority-9, such-6)
det(minority-9, a-7)
amod(minority-9, voiceless-8)
dobj(protect-5, minority-9)
prep_in(protect-5, society-11)

