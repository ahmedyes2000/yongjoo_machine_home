(ROOT
  (S (CC Either)
    (NP
      (NP (DT that))
      (CC or)
      (NP (DT the) (NN government)))
    (VP (MD would)
      (VP (VB become)
        (NP (DT a) (NN laughing) (NN stock))))
    (. .)))

dep(stock-10, Either-1)
nsubj(stock-10, that-2)
det(government-5, the-4)
conj_or(that-2, government-5)
nsubj(stock-10, government-5)
aux(stock-10, would-6)
cop(stock-10, become-7)
det(stock-10, a-8)
nn(stock-10, laughing-9)
root(ROOT-0, stock-10)

