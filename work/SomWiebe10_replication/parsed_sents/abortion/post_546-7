(ROOT
  (S
    (NP (NNS People))
    (VP (VBP die)
      (NP (DT every) (JJ single) (NN day))
      (PP (IN from)
        (NP
          (ADJP (RB completely) (JJ random))
          (NNS issues))))
    (. .)))

nsubj(die-2, People-1)
root(ROOT-0, die-2)
det(day-5, every-3)
amod(day-5, single-4)
tmod(die-2, day-5)
advmod(random-8, completely-7)
amod(issues-9, random-8)
prep_from(die-2, issues-9)

