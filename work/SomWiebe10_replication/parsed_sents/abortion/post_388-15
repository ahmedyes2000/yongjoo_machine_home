(ROOT
  (S
    (PP (IN From)
      (NP
        (NP (DT that) (NN point))
        (ADVP (RB forward))))
    (NP (PRP they))
    (VP (VBP continue)
      (NP (PRP$ their) (NN growth)
        (CC and)
        (NN development))
      (S
        (VP (TO to)
          (VP (VB improve)
            (NP
              (NP (PRP$ their) (NNS chances))
              (PP (IN of)
                (S
                  (VP (VBG surviving)
                    (PP (IN on)
                      (NP (PRP$ their) (JJ own)))
                    (PP (IN outside)
                      (NP (DT the) (NN womb)))))))))))
    (. .)))

det(point-3, that-2)
prep_from(continue-6, point-3)
advmod(point-3, forward-4)
nsubj(continue-6, they-5)
root(ROOT-0, continue-6)
poss(growth-8, their-7)
dobj(continue-6, growth-8)
dobj(continue-6, development-10)
conj_and(growth-8, development-10)
aux(improve-12, to-11)
xcomp(continue-6, improve-12)
poss(chances-14, their-13)
dobj(improve-12, chances-14)
prepc_of(chances-14, surviving-16)
poss(own-19, their-18)
prep_on(surviving-16, own-19)
det(womb-22, the-21)
prep_outside(surviving-16, womb-22)

