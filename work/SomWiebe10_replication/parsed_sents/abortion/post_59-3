(ROOT
  (S
    (NP (NNP Abortion))
    (VP (VBZ is)
      (ADVP (RB sometimes))
      (VP (VBN justified)
        (PP (IN as)
          (NP
            (NP (DT the) (JJR better) (NN choice))
            (PP (IN between)
              (NP (NNS evils)))))))
    (. .)))

nsubjpass(justified-4, Abortion-1)
auxpass(justified-4, is-2)
advmod(justified-4, sometimes-3)
root(ROOT-0, justified-4)
det(choice-8, the-6)
amod(choice-8, better-7)
prep_as(justified-4, choice-8)
prep_between(choice-8, evils-10)

