(ROOT
  (S
    (NP (NNP OK))
    (VP (VBD agreed)
      (SBAR
        (S
          (NP (PRP you))
          (VP (VBP have)
            (S
              (VP (TO to)
                (VP (VB establish)
                  (NP (NN intent)))))))))
    (. .)))

nsubj(agreed-2, OK-1)
root(ROOT-0, agreed-2)
nsubj(have-4, you-3)
xsubj(establish-6, you-3)
ccomp(agreed-2, have-4)
aux(establish-6, to-5)
xcomp(have-4, establish-6)
dobj(establish-6, intent-7)

