  (S
    (SBAR (RB Just)
      (S
        (VP (VBP bear)
          (PP (IN with)
            (NP (PRP me)))
          (PP (IN for)
            (NP (DT a) (NN moment))))))
    (NP (PRP I))
    (VP (VBP see)
      (SBAR
        (S
          (NP (PRP I))
          (VP (VBP have)
            (S
              (VP (TO to)
                (VP (VB hold)
                  (NP (PRP$ your) (NN hand)
                    (S
                      (VP (TO to)
                        (VP (VB get)
                          (S
                            (NP (PRP you))
                            (VP (VB see)
                              (NP (DT this)))))))))))))))
    (. .)))

