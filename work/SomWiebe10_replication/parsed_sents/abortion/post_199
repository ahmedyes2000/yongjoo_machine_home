(ROOT
  (S
    (NP (DT The) (NNP Church))
    (ADVP (RB only))
    (VP (VBZ makes)
      (NP
        (NP (DT a) (JJ formal) (NN declaration))
        (PP (IN of)
          (NP
            (NP (NN infallibility))
            (PP
              (PP (IN in)
                (NP (JJ extraordinary) (NNS circumstances)))
              (, ,)
              (CC and)
              (PP (IN in)
                (NP
                  (NP (JJ particular))
                  (, ,)
                  (PP (VBG regarding)
                    (NP
                      (NP (NNS matters))
                      (PP (IN of)
                        (NP
                          (NP (JJ widespread) (NN doubt))
                          (PP (IN among)
                            (NP (DT the) (NN faithful))))))))))))))
    (. .)))

