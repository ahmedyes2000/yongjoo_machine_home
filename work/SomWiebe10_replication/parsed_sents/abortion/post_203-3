(ROOT
  (SQ (VBZ Is)
    (NP (DT a) (NN person))
    (NP
      (NP (DT that))
      (SBAR
        (S
          (VP (VBZ starts)
            (S
              (NP (JJ unnecessary) (NNS wars))
              (ADJP (JJ pro-life)))))))
    (. ?)))

cop(that-4, Is-1)
det(person-3, a-2)
nsubj(that-4, person-3)
root(ROOT-0, that-4)
rcmod(that-4, starts-5)
amod(wars-7, unnecessary-6)
nsubj(pro-life-8, wars-7)
xcomp(starts-5, pro-life-8)

