(ROOT
  (S
    (NP (NNP Rape))
    (VP
      (VP (VBZ is)
        (ADJP (JJ wrong)))
      (CC but)
      (ADVP (RB so))
      (VP (VBZ is)
        (NP (NN murder))))
    (. .)))

nsubj(wrong-3, Rape-1)
cop(wrong-3, is-2)
root(ROOT-0, wrong-3)
advmod(wrong-3, so-5)
conj_but(wrong-3, so-5)
cop(murder-7, is-6)
conj_but(wrong-3, murder-7)

(ROOT
  (SBARQ
    (WHNP (WDT What)
      (NP (JJ moral) (NN stand)))
    (SQ (VBP are)
      (NP (PRP you))
      (VP (VBG taking)))
    (. ?)))

det(stand-3, What-1)
amod(stand-3, moral-2)
dobj(taking-6, stand-3)
aux(taking-6, are-4)
nsubj(taking-6, you-5)
root(ROOT-0, taking-6)

