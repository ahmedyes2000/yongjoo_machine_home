(ROOT
  (S
    (SBAR (RB Not) (IN if)
      (S
        (NP (PRP they))
        (VP (VBP do)
          (NP (PRP it))
          (PP (IN for)
            (NP (DT the) (NN right))))))
    (VP (VBZ reasons))
    (. .)))

dep(do-4, Not-1)
dep(do-4, if-2)
nsubj(do-4, they-3)
advcl(reasons-9, do-4)
csubj(reasons-9, do-4)
dobj(do-4, it-5)
det(right-8, the-7)
prep_for(do-4, right-8)
root(ROOT-0, reasons-9)

