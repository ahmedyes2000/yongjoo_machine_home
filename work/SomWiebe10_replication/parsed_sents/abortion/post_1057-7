(ROOT
  (S
    (NP (PRP I))
    (VP (VBP 'm)
      (ADJP (VBN convinced)
        (SBAR (IN that)
          (S
            (NP (DT an) (NN abortion))
            (VP (VBZ is)
              (VP (VBN unjustified)
                (PP (IN for)
                  (NP
                    (NP (DT the) (NNS reasons))
                    (VP
                      (ADVP (RB commonly))
                      (VBN given))))))))))
    (. .)))

nsubjpass(convinced-3, I-1)
auxpass(convinced-3, 'm-2)
root(ROOT-0, convinced-3)
complm(unjustified-8, that-4)
det(abortion-6, an-5)
nsubjpass(unjustified-8, abortion-6)
auxpass(unjustified-8, is-7)
ccomp(convinced-3, unjustified-8)
det(reasons-11, the-10)
prep_for(unjustified-8, reasons-11)
advmod(given-13, commonly-12)
partmod(reasons-11, given-13)

