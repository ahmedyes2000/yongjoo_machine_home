(ROOT
  (S
    (NP (EX There))
    (VP (VBZ is)
      (NP
        (NP (DT no) (JJ rational) (NN sense))
        (SBAR
          (WHPP (IN in)
            (WHNP (WDT which)))
          (S
            (NP (PDT such) (DT a) (NN thing))
            (VP (VBZ is)
              (NP (DT a) (NN person)))))))
    (. .)))

expl(is-2, There-1)
root(ROOT-0, is-2)
det(sense-5, no-3)
amod(sense-5, rational-4)
nsubj(is-2, sense-5)
rel(person-13, which-7)
predet(thing-10, such-8)
det(thing-10, a-9)
nsubj(person-13, thing-10)
cop(person-13, is-11)
det(person-13, a-12)
rcmod(sense-5, person-13)

