  (S
    (SBAR (IN If)
      (S
        (NP (PRP they))
        (VP (VBP are)
          (NP
            (NP (NN part))
            (PP (IN of)
              (NP (DT the) (NN government)))))))
    (NP (PRP they))
    (VP (VBP are)
      (ADJP (JJ corrupt)))
    (. .)))

mark(part-4, If-1)
nsubj(part-4, they-2)
