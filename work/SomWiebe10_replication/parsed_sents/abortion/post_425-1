(ROOT
  (S (CC But)
    (NP (PRP$ your) (NN argument))
    (VP (MD can) (RB not)
      (VP (VB answer)
        (NP (DT this))))
    (. .)))

cc(answer-6, But-1)
poss(argument-3, your-2)
nsubj(answer-6, argument-3)
aux(answer-6, can-4)
neg(answer-6, not-5)
root(ROOT-0, answer-6)
