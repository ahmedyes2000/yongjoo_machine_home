(ROOT
  (S
    (S
      (NP (PRP You))
      (VP (VBP combine)
        (NP (DT the) (CD two))))
    (CC and)
    (S
      (NP (PRP you))
      (VP (VBP have)
        (NP (DT a) (JJ new) (NN life))))
    (. .)))

nsubj(combine-2, You-1)
root(ROOT-0, combine-2)
det(two-4, the-3)
dobj(combine-2, two-4)
nsubj(have-7, you-6)
conj_and(combine-2, have-7)
det(life-10, a-8)
amod(life-10, new-9)
dobj(have-7, life-10)

