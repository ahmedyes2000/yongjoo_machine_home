(ROOT
  (S (CC And)
    (NP (PRP I))
    (VP (VBP stand)
      (PP (IN by)
        (NP
          (NP (NN everything) (NN els))
          (SBAR
            (S
              (NP (PRP I))
              (VP (VBD said)))))))
    (. .)))

cc(stand-3, And-1)
nsubj(stand-3, I-2)
root(ROOT-0, stand-3)
nn(els-6, everything-5)
prep_by(stand-3, els-6)
dobj(said-8, els-6)
nsubj(said-8, I-7)
rcmod(els-6, said-8)

