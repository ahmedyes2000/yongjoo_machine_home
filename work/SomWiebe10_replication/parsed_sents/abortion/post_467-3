(ROOT
  (S
    (S
      (NP (DT These))
      (VP (VBP are)
        (NP (JJ real) (NNS issues))))
    (, ,)
    (CC but)
    (S
      (NP (PRP they))
      (VP (MD should) (RB n't)
        (VP (VB be)
          (VP (VBN dealt)
            (PP (IN with)
              (PP (IN by)
                (S
                  (VP (VBG banning)
                    (NP (NN abortion))))))))))
    (. .)))

nsubj(issues-4, These-1)
cop(issues-4, are-2)
amod(issues-4, real-3)
root(ROOT-0, issues-4)
nsubjpass(dealt-11, they-7)
aux(dealt-11, should-8)
neg(dealt-11, n't-9)
auxpass(dealt-11, be-10)
conj_but(issues-4, dealt-11)
prep(dealt-11, with-12)
pcomp(with-12, by-13)
prepc_by(with-12, banning-14)
dobj(banning-14, abortion-15)

