(ROOT
  (S
    (PP (IN Of)
      (NP (NN course)))
    (NP (NNS condoms))
    (VP (VBP are)
      (ADJP (RBR more) (JJ effective)
        (PP (IN than)
          (NP (JJ unprotected) (NN sex)))))
    (. .)))

prep_of(effective-6, course-2)
nsubj(effective-6, condoms-3)
cop(effective-6, are-4)
advmod(effective-6, more-5)
root(ROOT-0, effective-6)
amod(sex-9, unprotected-8)
prep_than(effective-6, sex-9)

