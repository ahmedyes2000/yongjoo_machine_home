(ROOT
  (S (IN So)
    (SBAR (IN if)
      (S
        (NP (DT a) (NN someone))
        (VP (VBZ buys)
          (NP (DT another) (NN person)))))
    (, ,)
    (NP (PRP we))
    (VP (VBP have)
      (NP (DT a) (NN problem)))
    (. .)))

dep(have-10, So-1)
mark(buys-5, if-2)
det(someone-4, a-3)
nsubj(buys-5, someone-4)
advcl(have-10, buys-5)
det(person-7, another-6)
dobj(buys-5, person-7)
nsubj(have-10, we-9)
root(ROOT-0, have-10)
det(problem-12, a-11)
dobj(have-10, problem-12)

