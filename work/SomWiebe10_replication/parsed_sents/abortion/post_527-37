(ROOT
  (S
    (NP (PRP you))
    (VP (VBP say)
      (SBAR
        (S
          (NP (DT the) (NN child))
          (VP (VBZ has)
            (NP (DT no) (NN choice))))))
    (. .)))

nsubj(say-2, you-1)
root(ROOT-0, say-2)
det(child-4, the-3)
nsubj(has-5, child-4)
ccomp(say-2, has-5)
det(choice-7, no-6)
dobj(has-5, choice-7)

