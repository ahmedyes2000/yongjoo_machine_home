(ROOT
  (S
    (NP (DT This))
    (VP (VBZ is) (RB n't)
      (NP
        (NP
          (NP (DT a) (NN woman) (POS 's))
          (NN issue))
        (PP (IN for)
          (NP (DT this) (JJ very) (NN reason)))))
    (. .)))

nsubj(issue-7, This-1)
cop(issue-7, is-2)
neg(issue-7, n't-3)
det(woman-5, a-4)
poss(issue-7, woman-5)
root(ROOT-0, issue-7)
det(reason-11, this-9)
amod(reason-11, very-10)
prep_for(issue-7, reason-11)

