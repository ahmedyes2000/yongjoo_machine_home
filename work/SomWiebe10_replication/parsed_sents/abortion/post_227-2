(ROOT
  (S
    (NP (DT The) (JJ pro-choice) (NNS congressmen))
    (VP (VBD voted)
      (PP (IN with)
        (NP
          (NP (DT the) (NN church))
          (PP (IN on)
            (NP (NN everything)))))
      (PP (IN except) (IN on)
        (NP
          (NP (NN opposition))
          (PP (TO to)
            (NP (NN abortion))))))
    (. .)))

det(congressmen-3, The-1)
amod(congressmen-3, pro-choice-2)
nsubj(voted-4, congressmen-3)
root(ROOT-0, voted-4)
det(church-7, the-6)
prep_with(voted-4, church-7)
prep_on(church-7, everything-9)
dep(voted-4, except-10)
prep_on(voted-4, opposition-12)
prep_to(opposition-12, abortion-14)

