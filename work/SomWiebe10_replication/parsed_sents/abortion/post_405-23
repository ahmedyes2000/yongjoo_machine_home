(ROOT
  (S
    (NP (JJ Potential) (NNS beings))
    (VP (VBP have)
      (NP (DT no) (NNS rights))
      (PP
        (PP (IN over)
          (NP (JJ actual) (NNS beings)))
        (, ,) (RB not)
        (PP (IN in)
          (NP (DT any) (JJ valid)
            (ADJP (JJ ethical)
              (CC or)
              (JJ political))
            (NN system)))))
    (. .)))

amod(beings-2, Potential-1)
nsubj(have-3, beings-2)
root(ROOT-0, have-3)
det(rights-5, no-4)
dobj(have-3, rights-5)
amod(beings-8, actual-7)
prep_over(have-3, beings-8)
neg(have-3, not-10)
det(system-17, any-12)
amod(system-17, valid-13)
amod(system-17, ethical-14)
conj_or(ethical-14, political-16)
amod(system-17, political-16)
prep_in(have-3, system-17)

