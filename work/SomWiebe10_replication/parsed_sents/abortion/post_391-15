(ROOT
  (SINV (CC Nor)
    (VBZ is)
    (NP (NN thought))
    (NP (DT the) (NN response)
      (S
        (VP (TO to)
          (VP (VB stimuli)))))
    (. .)))

cc(thought-3, Nor-1)
cop(thought-3, is-2)
root(ROOT-0, thought-3)
det(response-5, the-4)
dep(thought-3, response-5)
aux(stimuli-7, to-6)
infmod(response-5, stimuli-7)

