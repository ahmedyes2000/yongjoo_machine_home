(ROOT
  (S
    (S
      (NP (PRP I))
      (VP (VBP do)
        (NP
          (NP (NN care))
          (PP (IN about)
            (NP (DT the) (NN mother))))))
    (CC and)
    (S
      (NP (PRP I))
      (VP (VBP care)
        (PP (IN about)
          (NP (PRP you)))))
    (. .)))

nsubj(do-2, I-1)
root(ROOT-0, do-2)
dobj(do-2, care-3)
det(mother-6, the-5)
prep_about(care-3, mother-6)
nsubj(care-9, I-8)
conj_and(do-2, care-9)
prep_about(care-9, you-11)

