(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ 's)
      (NP (PDT such) (DT a) (NN shame))
      (SBAR (IN that)
        (S
          (NP (NN someone))
          (VP (MD would)
            (VP (VB do)
              (NP (DT that)))))))
    (. .)))

nsubj(shame-5, It-1)
cop(shame-5, 's-2)
predet(shame-5, such-3)
det(shame-5, a-4)
root(ROOT-0, shame-5)
complm(do-9, that-6)
nsubj(do-9, someone-7)
aux(do-9, would-8)
ccomp(shame-5, do-9)
dobj(do-9, that-10)

