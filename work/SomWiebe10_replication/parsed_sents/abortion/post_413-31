(ROOT
  (FRAG (IN If)
    (SBAR (IN so)
      (S
        (ADVP (RB then))
        (NP (PRP we))
        (VP (MD can)
          (CC and)
          (MD should)
          (VP (VB come)
            (PP (TO to)
              (NP (DT the) (JJ same) (NN conclusion)))))))
    (. .)))

root(ROOT-0, If-1)
mark(come-8, so-2)
advmod(come-8, then-3)
nsubj(come-8, we-4)
aux(come-8, can-5)
dep(If-1, should-7)
aux(come-8, should-7)
conj_and(come-8, should-7)
dep(If-1, come-8)
det(conclusion-12, the-10)
amod(conclusion-12, same-11)
prep_to(come-8, conclusion-12)

