(ROOT
  (S ('' '')
    (S
      (NP
        (NP (DT A) (NN piece))
        (PP (IN of)
          (NP (NN protoplasm))))
      (VP (VBZ has)
        (NP (DT no) (NNS rights))))
    (CC and)
    (S
      (NP
        (NP (DT no) (NN life))
        (PP (IN in)
          (NP (DT the) (NN human))))
      (VP (VBP sense)
        (PP (IN of)
          (NP (DT the) (NN term)))))
    (. .)))

det(piece-3, A-2)
nsubj(has-6, piece-3)
prep_of(piece-3, protoplasm-5)
root(ROOT-0, has-6)
det(rights-8, no-7)
dobj(has-6, rights-8)
det(life-11, no-10)
nsubj(sense-15, life-11)
det(human-14, the-13)
prep_in(life-11, human-14)
conj_and(has-6, sense-15)
det(term-18, the-17)
prep_of(sense-15, term-18)

