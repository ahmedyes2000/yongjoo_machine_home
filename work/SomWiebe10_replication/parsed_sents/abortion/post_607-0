(ROOT
  (S
    (NP (PRP You))
    (VP (VBP have)
      (NP
        (NP (DT a)
          (ADJP (RB very) (JJ cold))
          (NN view))
        (PP (IN of)
          (NP
            (NP (DT the) (NNS rights))
            (PP (IN of)
              (NP (DT a) (NN rape) (NN victim)))))))
    (. .)))

nsubj(have-2, You-1)
root(ROOT-0, have-2)
det(view-6, a-3)
advmod(cold-5, very-4)
amod(view-6, cold-5)
dobj(have-2, view-6)
det(rights-9, the-8)
prep_of(view-6, rights-9)
det(victim-13, a-11)
nn(victim-13, rape-12)
