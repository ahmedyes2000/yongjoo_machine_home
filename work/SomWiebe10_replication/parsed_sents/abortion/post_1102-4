  (S (IN So) (, ,)
    (SBAR (IN if)
      (S
        (NP (PRP I))
        (VP (VBP make)
          (S
            (NP (DT the) (NN choice))
            (VP (TO to)
              (VP (VB kill)
                (NP (PRP$ my) (NN neighbor))
                (S
                  (VP (VBG using)
                    (NP (PRP$ my) (JJ own) (JJ moral) (NN judgment))))))))))
    (: ;)
    (NP (PRP it))
    (VP (VBZ 's)
      (ADJP (JJ ok)))
    (. ?)))

