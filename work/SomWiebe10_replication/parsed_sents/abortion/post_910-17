(ROOT
  (S
    (NP (DT This) (NN information))
    (VP (MD should)
      (VP (VB be)
        (VP
          (VP (VBN publicized)
            (ADVP (RBR more)))
          (, ,)
          (CC and)
          (VP (VBN made)
            (NP (JJ mandatory))
            (PP (IN before)
              (S
                (VP (VBG going)
                  (PRT (RP through))
                  (PP (IN with)
                    (NP (DT the) (NN procedure))))))))))
    (. .)))

det(information-2, This-1)
nsubjpass(publicized-5, information-2)
nsubjpass(made-9, information-2)
