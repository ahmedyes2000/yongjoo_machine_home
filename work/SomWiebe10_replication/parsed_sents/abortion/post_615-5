(ROOT
  (S
    (NP
      (NP (DT The) (NN idea))
      (SBAR (IN that)
        (S
          (NP (EX there))
          (VP (VBZ is)
            (NP
              (NP (DT no) (JJ such) (NN thing))
              (PP (IN as)
                (NP (JJ new) (NN life))))))))
    (VP (VBZ is)
      (NP
        (NP (DT an) (NN oft))
        (VP (VBN repeated)
          (NP (JJ pro-choice) (NN trope)))))
    (. .)))

det(idea-2, The-1)
nsubj(is-12, idea-2)
complm(is-5, that-3)
expl(is-5, there-4)
dep(idea-2, is-5)
det(thing-8, no-6)
amod(thing-8, such-7)
nsubj(is-5, thing-8)
amod(life-11, new-10)
prep_as(thing-8, life-11)
root(ROOT-0, is-12)
det(oft-14, an-13)
attr(is-12, oft-14)
partmod(oft-14, repeated-15)
amod(trope-17, pro-choice-16)
dobj(repeated-15, trope-17)

