(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP you))
        (VP (VBP 're)
          (ADJP (RB truly) (JJ Catholic)
            (SBAR
              (S
                (NP (PRP you))
                (VP (VBP accept)
                  (NP (NN everything)))))))))
    (NP (DT the) (NNP Vatican))
    (VP (VBZ says)
      (PP (IN without)
        (NP (NN question))))
    (. .)))

mark(Catholic-5, If-1)
nsubj(Catholic-5, you-2)
cop(Catholic-5, 're-3)
advmod(Catholic-5, truly-4)
advcl(says-11, Catholic-5)
nsubj(accept-7, you-6)
dep(Catholic-5, accept-7)
dobj(accept-7, everything-8)
det(Vatican-10, the-9)
nsubj(says-11, Vatican-10)
root(ROOT-0, says-11)
prep_without(says-11, question-13)

