(ROOT
  (S
    (NP (DT That))
    (VP (VBZ is) (RB not)
      (ADVP (RB even))
      (NP (DT the) (NN argument)))
    (. .)))

nsubj(argument-6, That-1)
cop(argument-6, is-2)
neg(argument-6, not-3)
advmod(argument-6, even-4)
det(argument-6, the-5)
root(ROOT-0, argument-6)

(ROOT
  (S
    (NP (PRP We))
    (VP (VBP are)
      (VP (VBG debating)
        (NP (NN abortion))
        (ADVP (RB not)
          (NP (DT the) (NN war)))
        (PP (IN in)
          (NP (NNP iraq)))))
    (. .)))

nsubj(debating-3, We-1)
aux(debating-3, are-2)
root(ROOT-0, debating-3)
dobj(debating-3, abortion-4)
neg(debating-3, not-5)
det(war-7, the-6)
pobj(not-5, war-7)
prep_in(debating-3, iraq-9)

