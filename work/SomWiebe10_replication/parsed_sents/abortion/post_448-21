(ROOT
  (S
    (NP (PRP They))
    (VP (VBP have)
      (NP (DT no) (NN family)
        (S
          (VP (TO to)
            (VP (VB help)
              (NP (PRP them)))))))
    (. .)))

nsubj(have-2, They-1)
root(ROOT-0, have-2)
det(family-4, no-3)
dobj(have-2, family-4)
aux(help-6, to-5)
infmod(family-4, help-6)
dobj(help-6, them-7)

