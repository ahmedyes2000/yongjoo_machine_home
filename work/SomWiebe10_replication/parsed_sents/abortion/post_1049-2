(ROOT
  (S
    (VP (VB Tell)
      (NP (PRP me)))
    (. !)))

root(ROOT-0, Tell-1)
dobj(Tell-1, me-2)

(ROOT
  (S
    (NP (PRP I))
    (VP (VBP want)
      (S
        (VP (TO to)
          (VP (VB know)))))
    (. .)))

nsubj(want-2, I-1)
xsubj(know-4, I-1)
root(ROOT-0, want-2)
aux(know-4, to-3)
xcomp(want-2, know-4)

