(ROOT
  (S
    (NP (DT That))
    (VP (VBZ 's)
      (SBAR
        (WHADVP (WRB why))
        (S
          (NP (PRP it))
          (VP (VBZ 's)
            (ADJP (JJ hypocritical))))))
    (. .)))

nsubj('s-2, That-1)
root(ROOT-0, 's-2)
advmod(hypocritical-6, why-3)
nsubj(hypocritical-6, it-4)
cop(hypocritical-6, 's-5)
advcl('s-2, hypocritical-6)

