(ROOT
  (S
    (NP (JJ Extraordinary) (NNP Universal) (NNP Magisterium) (NNS Bishops))
    (VP (VBN united)
      (PP (IN with)
        (NP (DT the) (NNP Pope)))
      (PP (IN at)
        (NP (DT an) (NNP Ecumenical) (NNP Council))))
    (. .)))

amod(Bishops-4, Extraordinary-1)
nn(Bishops-4, Universal-2)
nn(Bishops-4, Magisterium-3)
nsubj(united-5, Bishops-4)
root(ROOT-0, united-5)
det(Pope-8, the-7)
prep_with(united-5, Pope-8)
det(Council-12, an-10)
nn(Council-12, Ecumenical-11)
prep_at(united-5, Council-12)

