(ROOT
  (S
    (ADVP (RB Clearly))
    (, ,)
    (PP (RB just) (IN by)
      (S
        (VP (VBG being)
          (ADVP (RB here)))))
    (NP (PRP you))
    (VP (VBP dispute)
      (NP (PRP$ your) (JJ own) (NN statement)))
    (. .)))

advmod(dispute-8, Clearly-1)
advmod(dispute-8, just-3)
prepc_by(dispute-8, being-5)
advmod(being-5, here-6)
nsubj(dispute-8, you-7)
root(ROOT-0, dispute-8)
poss(statement-11, your-9)
amod(statement-11, own-10)
dobj(dispute-8, statement-11)

