  (S
    (NP (NNP Pope) (NNP Benedict) (NNP XVI))
    (VP (VBZ is)
      (VP (VBG using)
        (NP
          (NP (DT the) (JJ established) (JJ moral) (NN reasoning) (NN prinicple))
          (PP (IN of)
            (NP (NN cooperation))))
        (PP (IN with)
          (NP (NN evil)))
        (S
          (VP (TO to)
            (VP (VB make)
              (NP (PRP$ his) (NN statement)))))))
    (. .)))

