(ROOT
  (S
    (NP (DT A) (NN person))
    (VP (VBZ gets)
      (NP (PRP$ their) (JJ own) (NN DNA))
      (PP (IN at)
        (NP (NN conception))))
    (. .)))

det(person-2, A-1)
nsubj(gets-3, person-2)
root(ROOT-0, gets-3)
poss(DNA-6, their-4)
amod(DNA-6, own-5)
dobj(gets-3, DNA-6)
prep_at(gets-3, conception-8)

