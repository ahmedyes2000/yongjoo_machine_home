(ROOT
  (PP (IN at)
    (NP
      (NP (DT the) (NN moment))
      (PP (IN of)
        (NP (NN conception)))
      (SBAR
        (WHNP (WDT that))
        (S
          (VP (VBZ 's)
            (NP
              (NP
                (NP (JJ human) (NN life))
                (PP (IN at)
                  (NP
                    (NP (PRP it) (POS 's))
                    (JJS weakest) (NN point))))
              (CONJP (IN if) (RB just))
              (NP
                (NP (DT a) (NN ball))
                (PP (IN of)
                  (NP (NNS cells)))))))
        (, ,)))
    (: ...)))

