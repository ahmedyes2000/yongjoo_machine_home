            (WHNP (WP who))
            (S
              (VP (VBN composed)
                (S
                  (NP (DT the) (NN constitution))
                  (VP (VBZ is)
                    (ADJP (RB quite) (JJ clear))))))))))
    (VP (VBD unquestioned)
      (NP
        (NP (NN right))
        (PP (IN of)
          (NP (NN self-determination)
            (CC and)
            (NN liberty))))
      (S
        (VP (TO to)
          (VP (VB exercise)
            (NP (DT the) (JJ same))))))
    (. .)))

