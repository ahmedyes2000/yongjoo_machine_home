(ROOT
  (S
    (NP (NNP Society))
    (NP (NN today))
    (VP (VBZ has)
      (VP (VBD watered)
        (PRT (RP down))
        (NP (NNP Truth))
        (PP (IN as)
          (PP (TO to)
            (NP
              (NP
                (NP (PRP it) (POS 's))
                (JJ true) (NN meaning))
              (PP (IN from)
                (NP (NNS years))))))
        (ADVP (IN ago))))
    (. .)))

nsubj(watered-4, Society-1)
nsubj(watered-4, today-2)
aux(watered-4, has-3)
root(ROOT-0, watered-4)
prt(watered-4, down-5)
dobj(watered-4, Truth-6)
prepc_as_to(watered-4, to-8)
poss(meaning-12, it-9)
amod(meaning-12, true-11)
pobj(watered-4, meaning-12)
prep_from(meaning-12, years-14)
advmod(watered-4, ago-15)

