(ROOT
  (S
    (NP (PRP$ His) (NN research)
      (CC and)
      (NN teaching) (NNS interests))
    (VP (VBP are)
      (ADVP (RB mainly))
      (PP (IN in)
        (NP (NNS ethics) (, ,) (NNS metaphysics)
          (CC and)
          (NN epistemology))))
    (. .)))

poss(research-2, His-1)
nsubj(are-6, research-2)
nn(interests-5, teaching-4)
conj_and(research-2, interests-5)
nsubj(are-6, interests-5)
root(ROOT-0, are-6)
advmod(are-6, mainly-7)
prep_in(are-6, ethics-9)
prep_in(are-6, metaphysics-11)
conj_and(ethics-9, metaphysics-11)
prep_in(are-6, epistemology-13)
conj_and(ethics-9, epistemology-13)

