(ROOT
  (S
    (PP (IN In)
      (NP (JJ other) (NNS words)))
    (, ,)
    (NP
      (NP (DT the) (NN government))
      (PP (IN of)
        (NP
          (NP (DT the) (NNP United) (NNPS States))
          (PP (IN of)
            (NP (NNP America))))))
    (VP (VBZ is)
      (ADVP (RB not)
        (PRN (, ,)
          (PP (IN in)
            (NP (DT any) (NN sense)))
          (, ,))
        (S
          (VP (VBN founded)
            (PP (IN on)
              (NP (DT the) (JJ Christian) (NN religion)))))))
    (. .)))

amod(words-3, other-2)
prep_in(is-13, words-3)
det(government-6, the-5)
nsubj(is-13, government-6)
det(States-10, the-8)
nn(States-10, United-9)
prep_of(government-6, States-10)
prep_of(States-10, America-12)
root(ROOT-0, is-13)
neg(is-13, not-14)
dep(not-14, in-16)
det(sense-18, any-17)
pobj(in-16, sense-18)
dep(not-14, founded-20)
det(religion-24, the-22)
