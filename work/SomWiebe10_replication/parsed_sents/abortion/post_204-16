(ROOT
  (S
    (NP (PRP You))
    (VP (VBP use)
      (S
        (NP (DT the) (NN term))
        (`` ``)
        (NP (JJ full) (JJ fledged) (JJ human) (NN being))
        ('' '')))
    (. .)))

nsubj(use-2, You-1)
root(ROOT-0, use-2)
det(term-4, the-3)
dep(being-9, term-4)
amod(being-9, full-6)
amod(being-9, fledged-7)
amod(being-9, human-8)
dep(use-2, being-9)

