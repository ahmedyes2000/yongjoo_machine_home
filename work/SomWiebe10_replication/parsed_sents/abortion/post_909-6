  (S
    (NP (DT A) (JJ recent) (NN studied))
    (VP (VBD showed)
      (SBAR (IN that)
        (S
          (NP
            (NP (NNS women))
            (SBAR
              (WHNP (WP who))
              (S
                (VP
                  (VP (VBD were)
                    (VP (VBN raped)))
                  (CC and)
                  (VP (VBD had)
                    (NP (DT an) (NN abortion)))))))
          (VP (VBD had)
            (NP
              (NP (DT a) (JJR harder) (NN time) (NN healing))
