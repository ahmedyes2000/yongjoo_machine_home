(ROOT
  (S (CC And)
    (NP (DT this))
    (VP (VBZ is)
      (NP (DT a) (JJ fine) (NN way)
        (S
          (VP (TO to)
            (VP (VB teach)
              (NP (JJ sexual) (NN education)))))))
    (. .)))

cc(way-6, And-1)
nsubj(way-6, this-2)
cop(way-6, is-3)
det(way-6, a-4)
amod(way-6, fine-5)
root(ROOT-0, way-6)
aux(teach-8, to-7)
infmod(way-6, teach-8)
amod(education-10, sexual-9)
dobj(teach-8, education-10)

