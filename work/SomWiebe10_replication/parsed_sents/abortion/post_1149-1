(ROOT
  (S
    (NP (PRP you))
    (VP (VBP are)
      (NP
        (NP (DT the) (NN kind))
        (PP (IN of)
          (NP
            (NP (NNS people))
            (VP (VBG i)
              (ADVP (RB pray))
              (PP (IN for)))))))
    (. .)))

nsubj(kind-4, you-1)
cop(kind-4, are-2)
det(kind-4, the-3)
root(ROOT-0, kind-4)
prep_of(kind-4, people-6)
partmod(people-6, i-7)
advmod(i-7, pray-8)
prep(i-7, for-9)

