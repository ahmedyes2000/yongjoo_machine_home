(ROOT
  (S
    (ADVP (RB Surely))
    (NP
      (NP (DT the) (NN majority))
      (PP (IN of)
        (NP (PRP you))))
    (NP (DT all))
    (VP (MD would)
      (VP (VB agree)
        (PP (IN with)
          (NP (PRP me)))
        (SBAR (IN that)
          (S
            (NP (DT a) (NN child))
            (VP (VBZ has)
              (NP (PRP$ its) (NNS rights)))))
        (SBAR (RB even) (IN if)
          (S
            (NP (PRP it))
            (VP (MD can) (RB not)
              (VP (VB understand)
                (NP (PRP them))))))))
    (. !)))

advmod(agree-8, Surely-1)
det(majority-3, the-2)
nsubj(agree-8, majority-3)
prep_of(majority-3, you-5)
nsubj(agree-8, all-6)
aux(agree-8, would-7)
root(ROOT-0, agree-8)
