(ROOT
  (S
    (NP (PRP We))
    (VP (VBP 've)
      (VP (VBN taken)
        (NP
          (NP (DT a) (NN number))
          (PP (IN of)
            (NP
              (NP (NNS steps))
              (ADVP (RB backwards)
                (PP (IN in)
                  (NP (DT some) (JJ inane) (NN desire)))))))
        (S
          (VP (TO to)
            (VP (VB please)
              (NP (NNS men)))))))
    (. .)))

nsubj(taken-3, We-1)
aux(taken-3, 've-2)
root(ROOT-0, taken-3)
det(number-5, a-4)
dobj(taken-3, number-5)
prep_of(number-5, steps-7)
advmod(steps-7, backwards-8)
det(desire-12, some-10)
amod(desire-12, inane-11)
prep_in(backwards-8, desire-12)
aux(please-14, to-13)
xcomp(taken-3, please-14)
dobj(please-14, men-15)

