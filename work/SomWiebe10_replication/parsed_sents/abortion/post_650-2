(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ changes)
      (SBAR (IN as)
        (S
          (NP (PRP we))
          (VP (VBP learn)
            (SBAR
              (S
                (NP (NNS things))
                (VP (VBP are)
                  (ADJP (JJ different)))))))))
    (. .)))

nsubj(changes-2, It-1)
root(ROOT-0, changes-2)
mark(learn-5, as-3)
nsubj(learn-5, we-4)
