(ROOT
  (S ('' '')
    (S
      (NP (RB First) (DT a))
      (VP (VBP question)))
    (: :)
    (S
      (NP (PRP You))
      (VP (VBD used)
        (S
          (NP (DT the) (NN term))
          (`` ``)
          (ADJP (JJ miraculous))
          ('' ''))))
    (. .)))

advmod(a-3, First-2)
nsubj(question-4, a-3)
root(ROOT-0, question-4)
nsubj(used-7, You-6)
parataxis(question-4, used-7)
det(term-9, the-8)
dep(miraculous-11, term-9)
dep(used-7, miraculous-11)

