(ROOT
  (S
    (S
      (NP (PRP You))
      (VP (VBP say)))
    (: ;)
    (S
      (NP (EX There))
      (VP (VBP are)
        (NP
          (NP (JJ various) (JJ good) (NNS reasons))
          (PP (IN for)
            (S
              (VP (VBG terminating)
                (NP (DT a) (NN pregnancy))))))))
    (. .)))

nsubj(say-2, You-1)
root(ROOT-0, say-2)
expl(are-5, There-4)
parataxis(say-2, are-5)
amod(reasons-8, various-6)
amod(reasons-8, good-7)
nsubj(are-5, reasons-8)
prepc_for(reasons-8, terminating-10)
det(pregnancy-12, a-11)
dobj(terminating-10, pregnancy-12)

