(ROOT
  (S
    (NP (PRP I))
    (VP (MD would) (RB rather)
      (VP (VB be)
        (NP
          (NP (JJ pro) (NN life))
          (PP (IN than)
            (NP (JJ pro) (NN choice))))))
    (. .)))

nsubj(life-6, I-1)
aux(life-6, would-2)
advmod(life-6, rather-3)
cop(life-6, be-4)
amod(life-6, pro-5)
root(ROOT-0, life-6)
amod(choice-9, pro-8)
prep_than(life-6, choice-9)

