(ROOT
  (S
    (NP (DT This))
    (VP (VBZ is)
      (PP (IN about)
        (NP (NN abortion)))
      (, ,)
      (S
        (VP
          (VP (VB go)
            (ADVP (RB ahead)))
          (, ,)
          (VP (VB try))
          (CC and)
          (VP (VB make)
            (S
              (NP (PRP it))
              (ADJP (JJ illegal)))))))
    (. .)))

nsubj(is-2, This-1)
root(ROOT-0, is-2)
prep_about(is-2, abortion-4)
ccomp(is-2, go-6)
advmod(go-6, ahead-7)
ccomp(is-2, try-9)
conj_and(go-6, try-9)
ccomp(is-2, make-11)
conj_and(go-6, make-11)
nsubj(illegal-13, it-12)
xcomp(make-11, illegal-13)

