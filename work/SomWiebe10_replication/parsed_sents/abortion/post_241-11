(ROOT
  (S
    (VP (VB Work)
      (S
        (VP (TO to)
          (VP (VB make)
            (S
              (NP (PRP it))
              (NP
                (NP (DT a) (JJR better) (NN world))
                (SBAR
                  (WHADVP (WRB where))
                  (S
                    (NP (JJ few) (NNS children))
                    (VP (VBP are)
                      (PP (IN in)
                        (NP (JJ foster) (NN care))))))))))))
    (. .)))

root(ROOT-0, Work-1)
aux(make-3, to-2)
xcomp(Work-1, make-3)
nsubj(world-7, it-4)
det(world-7, a-5)
amod(world-7, better-6)
xcomp(make-3, world-7)
