(ROOT
  (S
    (S
      (NP (PRP we))
      (VP (VBP 're) (RB not)
        (VP (VBG saying)
          (NP
            (NP (NN u))
            (SBAR
              (S
                (VP (MD can)
                  (VP (VB stab)
                    (NP (DT a) (JJ pregnant) (NN person))))))))))
    (CC but)
    (S
      (NP (PRP we))
      (VP (VBP 're)
        (VP (VBG saying)
          (SBAR (IN that)
            (S
              (NP (DT the) (JJ pregnant) (NN person))
              (VP (MD should)
                (VP (VB have)
                  (NP (DT the) (NN option)
                    (S
                      (VP (TO to)
                        (VP (VB abort))))))))))))
    (. .)))

