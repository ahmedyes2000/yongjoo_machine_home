(ROOT
  (S
    (SBAR
      (WHADVP (WRB When))
      (S
        (NP (DT that))
        (VP (MD can)
          (VP (VB happen)))))
    (, ,)
    (ADVP (RB then))
    (NP (PRP you))
    (VP (MD will)
      (VP (VB be)
        (PP (IN on)
          (NP (JJ equal) (NN footing)))
        (PP (IN with)
          (NP
            (NP (NNS women))
            (PP (IN on)
              (NP (DT the) (JJ reproductive) (NN issue)))))))
    (. .)))

advmod(happen-4, When-1)
nsubj(happen-4, that-2)
aux(happen-4, can-3)
advcl(be-9, happen-4)
advmod(be-9, then-6)
nsubj(be-9, you-7)
aux(be-9, will-8)
root(ROOT-0, be-9)
amod(footing-12, equal-11)
prep_on(be-9, footing-12)
prep_with(be-9, women-14)
det(issue-18, the-16)
amod(issue-18, reproductive-17)
prep_on(women-14, issue-18)

