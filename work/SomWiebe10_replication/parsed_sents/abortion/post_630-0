(ROOT
  (NP
    (NP (DT A) (JJ little) (NN bit))
    (PP (IN of)
      (NP
        (NP (DT this))
        (CC and)
        (NP
          (NP (DT a) (JJ little) (NN bit))
          (PP (IN of)
            (NP (DT that))))))
    (: ...)))

det(bit-3, A-1)
amod(bit-3, little-2)
root(ROOT-0, bit-3)
prep_of(bit-3, this-5)
det(bit-9, a-7)
amod(bit-9, little-8)
prep_of(bit-3, bit-9)
conj_and(this-5, bit-9)
prep_of(bit-9, that-11)

