(ROOT
  (S
    (NP (PRP I))
    (VP (MD could) (RB not)
      (VP (VB hold)
        (NP (DT this) (NN discussion))
        (PP (IN with)
          (NP
            (NP (DT any))
            (PP (IN of)
              (NP (DT the) (JJR lower) (NNS animals)))))))
    (. .)))

nsubj(hold-4, I-1)
aux(hold-4, could-2)
neg(hold-4, not-3)
root(ROOT-0, hold-4)
det(discussion-6, this-5)
dobj(hold-4, discussion-6)
prep_with(hold-4, any-8)
det(animals-12, the-10)
amod(animals-12, lower-11)
prep_of(any-8, animals-12)

