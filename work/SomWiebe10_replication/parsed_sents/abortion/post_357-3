(ROOT
  (S
    (ADVP (RB Furthermore))
    (, ,)
    (NP
      (NP (NNS ethics))
      (CC and)
      (NP (NN morality)))
    (VP (VBP are)
      (NP
        (ADJP
          (ADJP
            (ADVP (RB far) (RBR more))
            (VBN complicated))
          (CC and)
          (ADJP (VBN confused)))
        (NNS issues))
      (SBAR (IN than)
        (S
          (NP (DT a) (NN dictionary))
          (VP (MD would)
            (VP (VB lead)
              (S
                (NP (PRP you))
                (VP (TO to)
                  (VP (VB believe)))))))))
    (. .)))

advmod(issues-12, Furthermore-1)
nsubj(issues-12, ethics-3)
conj_and(ethics-3, morality-5)
nsubj(issues-12, morality-5)
cop(issues-12, are-6)
advmod(more-8, far-7)
advmod(complicated-9, more-8)
amod(issues-12, complicated-9)
conj_and(complicated-9, confused-11)
amod(issues-12, confused-11)
root(ROOT-0, issues-12)
mark(lead-17, than-13)
det(dictionary-15, a-14)
nsubj(lead-17, dictionary-15)
aux(lead-17, would-16)
advcl(issues-12, lead-17)
nsubj(believe-20, you-18)
aux(believe-20, to-19)
xcomp(lead-17, believe-20)

