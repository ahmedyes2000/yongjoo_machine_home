(ROOT
  (S
    (NP (DT The) (CD two) (NNS events))
    (VP (VBP are) (RB not)
      (ADJP (RB morally) (JJ equivalent)))
    (. .)))

det(events-3, The-1)
num(events-3, two-2)
nsubj(equivalent-7, events-3)
cop(equivalent-7, are-4)
neg(equivalent-7, not-5)
advmod(equivalent-7, morally-6)
root(ROOT-0, equivalent-7)

