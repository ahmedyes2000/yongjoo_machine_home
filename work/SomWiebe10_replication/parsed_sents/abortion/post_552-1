(ROOT
  (FRAG
    (ADVP (RB Just) (RB as))
    (: :)
    (SBAR (IN if)
      (S
        (NP (PRP you))
        (VP (VBP think)
          (SBAR
            (S
              (NP (NN slavery))
              (VP (VBZ is)
                (ADJP (JJ wrong)
                  (SBAR
                    (S
                      (VP (VBP do) (RB n't)
                        (VP (VB get)
                          (NP (CD one)))))))))))))
    (. .)))

advmod(as-2, Just-1)
root(ROOT-0, as-2)
mark(think-6, if-4)
nsubj(think-6, you-5)
dep(as-2, think-6)
nsubj(wrong-9, slavery-7)
cop(wrong-9, is-8)
ccomp(think-6, wrong-9)
aux(get-12, do-10)
neg(get-12, n't-11)
dep(wrong-9, get-12)
dobj(get-12, one-13)

