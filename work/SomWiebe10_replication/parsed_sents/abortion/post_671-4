(ROOT
  (S ('' '')
    (NP (PRP we))
    (VP (VBP 're) (RB not)
      (VP (VBG referring)
        (PP (TO to)
          (NP
            (NP (DT the) (NN event) (NNS millions))
            (PP (IN of)
              (NP (NNS years)))))
        (ADVP (IN ago))))
    (. .)))

nsubj(referring-5, we-2)
aux(referring-5, 're-3)
neg(referring-5, not-4)
root(ROOT-0, referring-5)
det(millions-9, the-7)
nn(millions-9, event-8)
prep_to(referring-5, millions-9)
prep_of(millions-9, years-11)
advmod(referring-5, ago-12)

