(ROOT
  (NP
    (NP (NNP First))
    (, ,)
    (NP
      (NP (PRP$ my) (NNS apologies))
      (PP (IN for)
        (S
          (VP (VBG creating)
            (NP (DT some) (NN confusion))))))
    (. .)))

root(ROOT-0, First-1)
poss(apologies-4, my-3)
appos(First-1, apologies-4)
prepc_for(apologies-4, creating-6)
det(confusion-8, some-7)
dobj(creating-6, confusion-8)

