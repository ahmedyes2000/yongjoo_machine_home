(ROOT
  (SBARQ
    (WHADVP (WRB When))
    (SQ (VBZ does)
      (NP (NNP HUMAN) (NN life))
      (VP (VB begin)))
    (. ?)))

advmod(begin-5, When-1)
aux(begin-5, does-2)
nn(life-4, HUMAN-3)
nsubj(begin-5, life-4)
root(ROOT-0, begin-5)

(ROOT
  (S
    (S
      (VP (MD should)
        (VP (VB be)
          (NP (DT the) (NN question)))))
    (VP (VBD asked))
    (. .)))

aux(question-4, should-1)
cop(question-4, be-2)
det(question-4, the-3)
csubj(asked-5, question-4)
root(ROOT-0, asked-5)

