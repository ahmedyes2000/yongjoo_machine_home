(ROOT
  (S
    (S
      (NP (DT That))
      (VP (VBZ is)
        (PP (IN like)
          (S
            (VP (VBG saying)
              (SBAR
                (S
                  (NP (NN rape))
                  (VP (VBZ is)
                    (ADJP (JJ wrong))))))))))
    (CC but)
    (S
      (NP (NN incest))
      (VP (VBZ is)
        (ADJP (JJ okay))))
    (. .)))

nsubj(is-2, That-1)
root(ROOT-0, is-2)
prepc_like(is-2, saying-4)
nsubj(wrong-7, rape-5)
cop(wrong-7, is-6)
ccomp(saying-4, wrong-7)
nsubj(okay-11, incest-9)
cop(okay-11, is-10)
conj_but(is-2, okay-11)

