(ROOT
  (SBARQ
    (WHNP (WP What))
    (SQ (VBZ is)
      (NP (PRP it)))
    (. ?)))

attr(is-2, What-1)
root(ROOT-0, is-2)
nsubj(is-2, it-3)

(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (PRP it))
        (VP (VBZ 's) (RB not)
          (ADJP (JJ human)
            (PP (IN than)
              (NP (DT no) (NN justification)))))))
    (VP (VBZ is)
      (ADJP (JJ necessary)))
    (. .)))

mark(human-5, If-1)
nsubj(human-5, it-2)
cop(human-5, 's-3)
neg(human-5, not-4)
csubj(necessary-10, human-5)
det(justification-8, no-7)
prep_than(human-5, justification-8)
cop(necessary-10, is-9)
root(ROOT-0, necessary-10)

