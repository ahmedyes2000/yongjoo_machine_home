(ROOT
  (S
    (PP (IN In)
      (NP (NN fact)))
    (, ,)
    (NP
      (NP (NN eye))
      (PP (IN for)
        (NP (DT an) (NN eye))))
    (VP (VBZ is)
      (NP
        (NP (DT a) (JJ legal) (NN punishment))
        (PP (IN for)
          (NP
            (NP (DT any) (NN adult))
            (VP (VBG injuring)
              (NP (DT any) (JJ other) (NN adult))
              (ADVP (RB willfully)))))))
    (. .)))

prep_in(punishment-11, fact-2)
nsubj(punishment-11, eye-4)
det(eye-7, an-6)
