(ROOT
  (S
    (NP (NNP Abortion))
    (VP (VBZ is)
      (NP
        (NP (DT the) (NN killing))
        (PP (IN of)
          (NP (DT a) (NN child)))))
    (. .)))

nsubj(killing-4, Abortion-1)
cop(killing-4, is-2)
det(killing-4, the-3)
root(ROOT-0, killing-4)
det(child-7, a-6)
prep_of(killing-4, child-7)

