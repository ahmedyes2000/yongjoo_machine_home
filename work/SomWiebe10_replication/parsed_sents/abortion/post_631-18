(ROOT
  (S
    (NP (DT The) (JJ initial) (NNS contractions))
    (VP (VBP are)
      (ADJP (JJ peristaltic))
      (: --)
      (SBAR (IN that)
        (S
          (VP (VBZ is) (, ,)
            (S
              (NP (PRP they))
              (VP (VB proceed)
                (PP (IN in)
                  (NP
                    (NP (DT a) (JJ wave-like) (NN fashion))
                    (PP (IN along)
                      (NP
                        (NP (DT the) (NN length))
                        (PP (IN of)
                          (NP (DT the) (NN heart)))))))))))))
    (. .)))

det(contractions-3, The-1)
amod(contractions-3, initial-2)
nsubj(peristaltic-5, contractions-3)
cop(peristaltic-5, are-4)
root(ROOT-0, peristaltic-5)
complm(is-8, that-7)
ccomp(peristaltic-5, is-8)
parataxis(peristaltic-5, is-8)
nsubj(proceed-11, they-10)
ccomp(is-8, proceed-11)
det(fashion-15, a-13)
amod(fashion-15, wave-like-14)
