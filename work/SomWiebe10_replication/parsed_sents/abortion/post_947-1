(ROOT
  (S
    (NP (DT The) (JJ double) (NN homicide) (NN law))
    (VP (VBD was)
      (ADVP (RB only))
      (VP (VBN raised)
        (S
          (VP (TO to)
            (VP (VB illustrate)
              (NP (DT this)))))))
    (. .)))

det(law-4, The-1)
amod(law-4, double-2)
nn(law-4, homicide-3)
nsubjpass(raised-7, law-4)
xsubj(illustrate-9, law-4)
auxpass(raised-7, was-5)
advmod(raised-7, only-6)
root(ROOT-0, raised-7)
aux(illustrate-9, to-8)
xcomp(raised-7, illustrate-9)
dobj(illustrate-9, this-10)

