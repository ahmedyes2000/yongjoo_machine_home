(ROOT
  (S
    (NP (NNP Dad))
    (ADVP (RB always))
    (VP (VBD said)
      (SBAR (IN that)
        (S
          (NP (PRP we))
          (VP (VBP catch)
            (NP (JJR more) (NNS flies))
            (PP (IN with)
              (NP (NN honey)))
            (SBAR (IN than)
              (S
                (NP (PRP we))
                (VP (VBP do)
                  (NP (NN vinegar)))))))))
    (. !)))

nsubj(said-3, Dad-1)
advmod(said-3, always-2)
root(ROOT-0, said-3)
complm(catch-6, that-4)
nsubj(catch-6, we-5)
ccomp(said-3, catch-6)
amod(flies-8, more-7)
dobj(catch-6, flies-8)
prep_with(catch-6, honey-10)
mark(do-13, than-11)
nsubj(do-13, we-12)
advcl(catch-6, do-13)
dobj(do-13, vinegar-14)

