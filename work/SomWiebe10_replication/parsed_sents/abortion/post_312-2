(ROOT
  (S
    (NP (PRP I))
    (ADVP (RB therefore))
    (VP (VBP challenge)
      (S
        (NP (PRP you))
        (, ,)
        (VP (VB prove)
          (SBAR (IN that)
            (S
              (NP (PRP you))
              (VP (VBP have)
                (NP (DT a) (NN soul))))))))
    (. .)))

nsubj(challenge-3, I-1)
advmod(challenge-3, therefore-2)
root(ROOT-0, challenge-3)
nsubj(prove-6, you-4)
ccomp(challenge-3, prove-6)
complm(have-9, that-7)
nsubj(have-9, you-8)
ccomp(prove-6, have-9)
det(soul-11, a-10)
dobj(have-9, soul-11)

