(ROOT
  (S
    (NP (NNP Poor) (NNS women))
    (VP (VBP are)
      (ADVP (RB disproportionately))
      (VP (VBN deprived)
        (NP (NN choice))
        (SBAR
          (WHADVP (WRB when))
          (S
            (NP (NN abortion))
            (VP (VBZ is)
              (ADJP (JJ illegal)))))))
    (. .)))

nn(women-2, Poor-1)
nsubjpass(deprived-5, women-2)
auxpass(deprived-5, are-3)
advmod(deprived-5, disproportionately-4)
root(ROOT-0, deprived-5)
dobj(deprived-5, choice-6)
advmod(illegal-10, when-7)
nsubj(illegal-10, abortion-8)
cop(illegal-10, is-9)
advcl(deprived-5, illegal-10)

