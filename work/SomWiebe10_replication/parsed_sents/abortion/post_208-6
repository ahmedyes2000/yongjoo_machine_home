(ROOT
  (S
    (ADVP (RB However))
    (, ,)
    (NP (JJ reasonable) (NNS people))
    (VP (MD can)
      (VP (VB disagree)
        (PP (IN about)
          (WHNP (WDT which)))
        (NP
          (NP (NNS candidates))
          ('' '')
          (SBAR
            (S
              (NP (NNS policies))
              (VP (MD will)
                (VP (VB do)
                  (NP (JJR more))
                  (S
                    (VP (TO to)
                      (VP (VB help)
                        (NP (DT the) (JJ poor))))))))))))
    (. .)))

advmod(disagree-6, However-1)
amod(people-4, reasonable-3)
nsubj(disagree-6, people-4)
aux(disagree-6, can-5)
root(ROOT-0, disagree-6)
prep_about(disagree-6, which-8)
dobj(disagree-6, candidates-9)
nsubj(do-13, policies-11)
aux(do-13, will-12)
rcmod(candidates-9, do-13)
dobj(do-13, more-14)
aux(help-16, to-15)
xcomp(do-13, help-16)
det(poor-18, the-17)
dobj(help-16, poor-18)

