(ROOT
  (S
    (NP (DT The) (NN failure) (NN rate))
    (VP (VBZ is)
      (ADVP (RB only)
        (NP
          (QP (RB about) (CD .01))
          (NN %)))
      (NP (CD -1) (NN %)))
    (. .)))

det(rate-3, The-1)
nn(rate-3, failure-2)
nsubj(%-10, rate-3)
cop(%-10, is-4)
advmod(%-10, only-5)
quantmod(.01-7, about-6)
num(%-8, .01-7)
pobj(only-5, %-8)
num(%-10, -1-9)
root(ROOT-0, %-10)

