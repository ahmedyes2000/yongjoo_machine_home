(ROOT
  (S
    (NP (DT Those))
    (VP (MD can)
      (VP (VB be)
        (VP (VBN considered)
          (NP (`` ``)
            (NP (NNS chances))
            ('' '')
            (PP (IN at)
              (NP (NN life)))))))
    (. .)))

nsubjpass(considered-4, Those-1)
aux(considered-4, can-2)
auxpass(considered-4, be-3)
root(ROOT-0, considered-4)
dobj(considered-4, chances-6)
prep_at(chances-6, life-9)

