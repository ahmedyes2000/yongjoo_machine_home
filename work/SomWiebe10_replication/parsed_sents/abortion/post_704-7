(ROOT
  (S
    (NP (DT That))
    (VP (VBZ is)
      (NP
        (NP (DT a) (JJ big) (NN one))
        (, ,)
        (SBAR
          (WHADVP (WRB where))
          (S
            (NP (DT the) (NN fetus))
            (VP (MD will)
              (VP (VB die)
                (SBAR (IN if)
                  (S
                    (NP (DT the) (NNS chromosomes))
                    (VP (MD ca) (RB n't)
                      (VP (VB dance)
                        (ADVP (RB together))))))))))))
    (. .)))

nsubj(one-5, That-1)
cop(one-5, is-2)
det(one-5, a-3)
amod(one-5, big-4)
root(ROOT-0, one-5)
advmod(die-11, where-7)
det(fetus-9, the-8)
nsubj(die-11, fetus-9)
aux(die-11, will-10)
rcmod(one-5, die-11)
mark(dance-17, if-12)
det(chromosomes-14, the-13)
nsubj(dance-17, chromosomes-14)
aux(dance-17, ca-15)
neg(dance-17, n't-16)
advcl(die-11, dance-17)
advmod(dance-17, together-18)

