(ROOT
  (S
    (PP (VBG According)
      (PP (TO to)
        (NP (PRP$ your) (NN model))))
    (, ,)
    (NP
      (NP (DT the) (NN woman))
      (SBAR
        (WHNP (WP who))
        (S
          (VP (VBZ becomes)
            (ADJP (JJ pregnant))))))
    (VP (VBZ CEASES)
      (S
        (VP (TO to)
          (VP (VB have)
            (NP (JJ human) (NNS rights))
            (SBAR (IN until)
              (S
                (S
                  (NP (DT a) (NN baby))
                  (VP (VBZ is)
                    (VP (VBN delivered))))
                (CC or)
                (S
                  (NP (DT the) (NN pregnancy))
                  (ADVP (RB spontaneously))
                  (VP (VBZ aborts)))))))))
    (. .)))

