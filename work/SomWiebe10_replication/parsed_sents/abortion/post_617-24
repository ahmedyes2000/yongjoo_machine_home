(ROOT
  (S
    (ADVP (RB Frankly))
    (, ,)
    (NP (PRP I))
    (VP (VBP am)
      (VP (VBN confused)
        (PP (IN by)
          (NP (DT this)))))
    (. .)))

advmod(confused-5, Frankly-1)
