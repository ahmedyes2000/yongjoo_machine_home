(ROOT
  (FRAG
    (SBAR (IN If)
      (S
        (NP (DT the) (NN church))
        (VP (VBD was)
          (VP (VBG doing)
            (NP (PDT such) (DT a) (JJ good) (NN job)))))
      (. .))
    (. .)))

mark(doing-5, If-1)
det(church-3, the-2)
nsubj(doing-5, church-3)
aux(doing-5, was-4)
root(ROOT-0, doing-5)
predet(job-9, such-6)
det(job-9, a-7)
amod(job-9, good-8)
dobj(doing-5, job-9)

