(ROOT
  (S
    (NP (DT The) (NN mother))
    (, ,)
    (ADVP (RB therefore))
    (, ,)
    (VP (VBZ has)
      (NP (DT a) (JJ special) (NN responsibility)
        (S
          (VP (TO to)
            (VP (VB keep)
              (NP
                (NP
                  (NP (PRP$ her) (NN child))
                  (ADJP
                    (ADJP (JJ alive))
                    (CC and)
                    (ADJP (RB not) (JJ abort))))
                (: ;)
                (NP (CD 2))))))))
    (. .)))

det(mother-2, The-1)
nsubj(has-6, mother-2)
advmod(has-6, therefore-4)
root(ROOT-0, has-6)
det(responsibility-9, a-7)
amod(responsibility-9, special-8)
dobj(has-6, responsibility-9)
aux(keep-11, to-10)
infmod(responsibility-9, keep-11)
poss(child-13, her-12)
dobj(keep-11, child-13)
amod(child-13, alive-14)
neg(abort-17, not-16)
amod(child-13, abort-17)
conj_and(alive-14, abort-17)
dep(child-13, 2-19)

