(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is) (, ,)
      (PP (IN in)
        (NP (JJ scientific) (NNS terms)))
      (, ,)
      (SBAR
        (NP
          (NP (DT every) (NN bit))
          (PP (IN as)
            (NP (JJ much) (NN human))))
        (IN as)
        (S
          (NP (PRP you))
          (VP (VBP are)))))
    (. .)))

nsubj(is-2, It-1)
root(ROOT-0, is-2)
amod(terms-6, scientific-5)
prep_in(is-2, terms-6)
det(bit-9, every-8)
dep(are-15, bit-9)
amod(human-12, much-11)
prep_as(bit-9, human-12)
dep(are-15, as-13)
nsubj(are-15, you-14)
ccomp(is-2, are-15)

