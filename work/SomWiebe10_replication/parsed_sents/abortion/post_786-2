(ROOT
  (S
    (NP
      (NP (NN Anyone))
      (VP (VBG saying)
        (SBAR
          (S
            (NP (PRP they))
            (VP (VBP do) (RB n't)
              (VP (VB believe)
                (PP (IN in)
                  (NP (NNP God)))))))))
    (VP (VBZ has)
      (ADVP (RB never))
      (VP (VBN read)
        (NP (PRP it))))
    (. .)))

nsubj(read-11, Anyone-1)
partmod(Anyone-1, saying-2)
nsubj(believe-6, they-3)
aux(believe-6, do-4)
neg(believe-6, n't-5)
ccomp(saying-2, believe-6)
prep_in(believe-6, God-8)
aux(read-11, has-9)
neg(read-11, never-10)
root(ROOT-0, read-11)
dobj(read-11, it-12)

