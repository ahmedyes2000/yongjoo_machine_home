(ROOT
  (S
    (NP (PDT Both) (PRP$ their) (NNS babies))
    (VP (VBD were)
      (ADVP (RB still))
      (PP (IN in)
        (NP (PRP$ their) (NNS wombs))))
    (. .)))

predet(babies-3, Both-1)
poss(babies-3, their-2)
nsubj(were-4, babies-3)
root(ROOT-0, were-4)
advmod(were-4, still-5)
poss(wombs-8, their-7)
prep_in(were-4, wombs-8)

