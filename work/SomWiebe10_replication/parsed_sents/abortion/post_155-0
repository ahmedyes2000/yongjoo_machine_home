(ROOT
  (S
    (NP (NNP Partial-birth) (NNS abortions))
    (VP (VBP are)
      (VP (VBN performed)
        (SBAR
          (WHADVP (WRB when))
          (S
            (NP (JJ fetal) (NNS tissues))
            (VP (VBP are)
              (ADJP (JJR harder)
                (S
                  (VP (TO to)
                    (VP (VB dismember))))))))))
    (. .)))

nn(abortions-2, Partial-birth-1)
nsubjpass(performed-4, abortions-2)
auxpass(performed-4, are-3)
root(ROOT-0, performed-4)
advmod(harder-9, when-5)
amod(tissues-7, fetal-6)
nsubj(harder-9, tissues-7)
cop(harder-9, are-8)
advcl(performed-4, harder-9)
aux(dismember-11, to-10)
