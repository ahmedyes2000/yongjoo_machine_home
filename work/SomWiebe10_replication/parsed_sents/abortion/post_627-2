(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is)
      (ADJP (JJ incumbent)
        (PP (IN upon)
          (NP (PRP them))))
      (S
        (VP (TO to)
          (VP (VB do)
            (ADVP (RB just))
            (NP (DT the) (JJ opposite))))))
    (. .)))

nsubj(incumbent-3, It-1)
xsubj(do-7, It-1)
cop(incumbent-3, is-2)
root(ROOT-0, incumbent-3)
prep_upon(incumbent-3, them-5)
aux(do-7, to-6)
xcomp(incumbent-3, do-7)
advmod(do-7, just-8)
det(opposite-10, the-9)
dobj(do-7, opposite-10)

