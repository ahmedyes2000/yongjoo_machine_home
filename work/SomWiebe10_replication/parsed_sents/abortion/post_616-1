(ROOT
  (S
    (NP (DT These))
    (VP (VBP are)
      (NP
        (NP (PRP$ your) (NNS opinions) (, ,) (RB not) (JJ incontrovertible) (NNS truths))
        (, ,)
        (SBAR (IN as)
          (S
            (NP (PRP you))
            (VP (VBP present)
              (NP (PRP them)))))))
    (. .)))

nsubj(truths-8, These-1)
cop(truths-8, are-2)
poss(truths-8, your-3)
nn(truths-8, opinions-4)
neg(truths-8, not-6)
amod(truths-8, incontrovertible-7)
root(ROOT-0, truths-8)
mark(present-12, as-10)
nsubj(present-12, you-11)
dep(truths-8, present-12)
dobj(present-12, them-13)

