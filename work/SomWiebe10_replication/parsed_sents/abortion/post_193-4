(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is)
      (NP
        (NP (NNP God) (POS 's))
        (NN command)
        (S
          (VP (TO to)
            (VP (VB love)
              (NP (DT each) (JJ other))
              (PP (IN as)
                (NP (NNS brothers)
                  (CC &)
                  (NNS sisters))))))))
    (. .)))

nsubj(command-5, It-1)
cop(command-5, is-2)
poss(command-5, God-3)
root(ROOT-0, command-5)
aux(love-7, to-6)
infmod(command-5, love-7)
det(other-9, each-8)
dobj(love-7, other-9)
prep_as(love-7, brothers-11)
prep_as(love-7, sisters-13)
conj_and(brothers-11, sisters-13)

