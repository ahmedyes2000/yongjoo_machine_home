(ROOT
  (S
    (NP (PRP I))
    (VP (VBP agree)
      (SBAR
        (S
          (NP (PRP they))
          (VP (MD should)
            (VP (VB think)
              (PRT (RP about)))))))
    (. .)))

nsubj(agree-2, I-1)
root(ROOT-0, agree-2)
nsubj(think-5, they-3)
aux(think-5, should-4)
ccomp(agree-2, think-5)
prt(think-5, about-6)

