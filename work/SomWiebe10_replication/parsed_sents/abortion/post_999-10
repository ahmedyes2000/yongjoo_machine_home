  (S (CC But)
    (SBAR (IN if)
      (S
        (NP (DT some) (JJ poor) (JJ young) (NN girl))
        (VP (VBZ thinks)
          (SBAR (IN that)
            (S
              (VP (VBZ 's)
                (NP (PRP$ her) (JJS best) (NN option))))))))
    (, ,)
    (ADVP (RB then))
    (NP (DT that))
    (VP (VBZ 's)
      (NP (PRP$ her) (NN choice)))
    (. .)))

