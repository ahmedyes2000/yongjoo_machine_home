(ROOT
  (S
    (NP (EX There))
    (VP (VBZ is)
      (ADVP (RB certainly))
      (VP (NN question)
        (PP (TO to)
          (SBAR
            (WHADVP (WRB when))
            (S
              (NP (NN life))
              (VP (VBZ begins)))))))
    (. .)))

expl(is-2, There-1)
root(ROOT-0, is-2)
advmod(is-2, certainly-3)
dep(is-2, question-4)
advmod(begins-8, when-6)
nsubj(begins-8, life-7)
prepc_to(question-4, begins-8)

