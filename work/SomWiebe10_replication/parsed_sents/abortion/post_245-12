(ROOT
  (S
    (NP
      (NP (DT The) (NN problem))
      (PP (IN with)
        (S
          (VP (VBG saying)
            (SBAR (IN that)
              (S
                (NP (DT a) (NN woman))
                (VP (VBZ has)
                  (NP (DT the) (NNS rights))
                  (PP (TO to)
                    (NP (PRP$ her) (NN womb))))))))))
    (VP (VBZ ignores)
      (NP (DT the) (JJ potential) (NN child)))
    (. .)))

det(problem-2, The-1)
nsubj(ignores-14, problem-2)
prepc_with(problem-2, saying-4)
complm(has-8, that-5)
det(woman-7, a-6)
nsubj(has-8, woman-7)
ccomp(saying-4, has-8)
det(rights-10, the-9)
dobj(has-8, rights-10)
poss(womb-13, her-12)
prep_to(has-8, womb-13)
root(ROOT-0, ignores-14)
det(child-17, the-15)
amod(child-17, potential-16)
dobj(ignores-14, child-17)

