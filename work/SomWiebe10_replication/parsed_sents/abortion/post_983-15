  (S
    (SBAR (IN If)
      (S
        (NP (PRP I))
        (VP (VBP 'm)
          (NP (PRP$ His) (NN follower))
          (SBAR (IN than)
            (S
              (NP (PRP I))
              (VP (MD 'll)
                (VP (VB do)
                  (NP (WP what)))))))))
    (NP (PRP he))
    (VP (VBZ commands)
      (NP (PRP me)))
    (. .)))

