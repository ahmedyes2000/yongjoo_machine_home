(ROOT
  (S
    (NP (DT The) (JJ Jewish) (NN Holocaust))
    (VP (VBZ is)
      (VP (VBN considered)
        (NP (DT a) (NN life) (NN issue))))
    (. .)))

det(Holocaust-3, The-1)
amod(Holocaust-3, Jewish-2)
nsubjpass(considered-5, Holocaust-3)
auxpass(considered-5, is-4)
root(ROOT-0, considered-5)
det(issue-8, a-6)
nn(issue-8, life-7)
dobj(considered-5, issue-8)

(ROOT
  (S
    (NP (NNP Life) (NNS issues))
    (ADVP (RB also))
    (VP (VBP include)
      (NP (NN abortion)
        (CC and)
        (NN euthanasia)))
    (. .)))

nn(issues-2, Life-1)
nsubj(include-4, issues-2)
advmod(include-4, also-3)
root(ROOT-0, include-4)
dobj(include-4, abortion-5)
dobj(include-4, euthanasia-7)
conj_and(abortion-5, euthanasia-7)

