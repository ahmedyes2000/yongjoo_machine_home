(ROOT
  (S
    (ADVP (RB Obviously))
    (NP
      (NP (RB not))
      (PRN (, ,)
        (ADVP (RB otherwise))
        (PP
          (ADVP (RB just))
          (IN about)
          (NP (DT every) (NN male)))))
    (VP (MD would)
      (VP (VB be)
        (VP (VBN convicted)
          (PP (IN as)
            (NP (DT a) (JJ mass) (NN murderer))))))
    (. .)))

advmod(convicted-11, Obviously-1)
nsubjpass(convicted-11, not-2)
dep(about-6, otherwise-4)
advmod(about-6, just-5)
dep(not-2, about-6)
det(male-8, every-7)
pobj(about-6, male-8)
aux(convicted-11, would-9)
auxpass(convicted-11, be-10)
root(ROOT-0, convicted-11)
det(murderer-15, a-13)
amod(murderer-15, mass-14)
prep_as(convicted-11, murderer-15)

