(ROOT
  (S
    (S
      (NP (EX There))
      (VP (VBP are)
        (NP
          (NP (JJ many) (NNS firsts))
          (PP (IN in)
            (NP
              (NP (DT each) (NN cycle))
              (PP (IN of)
                (NP (NN life))))))))
    (, ,)
    (S
      (NP
        (NP (DT each))
        (PP (IN of)
          (NP (PRP them))))
      (ADJP (RB equally) (JJ necessary)
        (PP (IN for)
          (NP
            (NP (DT the) (NN continuity))
            (PP (IN of)
              (NP (NN life)))))))
    (. .)))

expl(are-2, There-1)
root(ROOT-0, are-2)
amod(firsts-4, many-3)
nsubj(are-2, firsts-4)
det(cycle-7, each-6)
prep_in(firsts-4, cycle-7)
prep_of(cycle-7, life-9)
dep(necessary-15, each-11)
prep_of(each-11, them-13)
advmod(necessary-15, equally-14)
ccomp(are-2, necessary-15)
det(continuity-18, the-17)
prep_for(necessary-15, continuity-18)
prep_of(continuity-18, life-20)

