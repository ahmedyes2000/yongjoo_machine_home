(ROOT
  (S
    (NP (NNP Life))
    (VP (VBZ is)
      (NP (NN life))
      (SBAR (IN whether)
        (S
          (NP (PRP we))
          (VP (VBP believe)
            (PP (IN in)
              (NP (PRP it)))))
        (CC or)
        (RB not)))
    (. .)))

nsubj(life-3, Life-1)
cop(life-3, is-2)
root(ROOT-0, life-3)
complm(believe-6, whether-4)
nsubj(believe-6, we-5)
ccomp(life-3, believe-6)
prep_in(believe-6, it-8)
ccomp(life-3, not-10)
conj_or(believe-6, not-10)

