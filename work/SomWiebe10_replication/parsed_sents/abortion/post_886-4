(ROOT
  (S (CC But)
    (NP
      (NP (NNS millions))
      (PP (IN of)
        (NP (NNS women))))
    (VP
      (VP (VBP have)
        (VP (VBN had)
          (NP (NNS abortions))))
      (CC and)
      (VP (VBP are)
        (VP (VBG living)
          (NP
            (ADJP (RB perfectly) (JJ healthy) (, ,) (JJ happy) (, ,) (JJ successful))
            (NNS lives)))))
    (. .)))

nsubj(had-6, millions-2)
nsubj(living-10, millions-2)
prep_of(millions-2, women-4)
aux(had-6, have-5)
root(ROOT-0, had-6)
dobj(had-6, abortions-7)
aux(living-10, are-9)
conj_and(had-6, living-10)
advmod(successful-16, perfectly-11)
amod(successful-16, healthy-12)
amod(successful-16, happy-14)
amod(lives-17, successful-16)
dobj(living-10, lives-17)

