(ROOT
  (S
    (NP (DT The) (JJ human) (NN mind))
    (VP (VBZ is)
      (ADJP
        (ADJP (JJ independent))
        (: --)
        (ADJP (RB metaphysically) (JJ independent))))
    (. .)))

det(mind-3, The-1)
amod(mind-3, human-2)
nsubj(independent-5, mind-3)
cop(independent-5, is-4)
root(ROOT-0, independent-5)
advmod(independent-8, metaphysically-7)
dep(independent-5, independent-8)

