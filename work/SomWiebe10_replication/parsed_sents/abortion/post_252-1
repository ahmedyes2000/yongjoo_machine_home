(ROOT
  (S
    (NP (NNP Rights))
    (VP (VBP belong)
      (NP (JJ only) (NN man)))
    (. .)))

nsubj(belong-2, Rights-1)
root(ROOT-0, belong-2)
amod(man-4, only-3)
dobj(belong-2, man-4)

(ROOT
  (S
    (NP (RB Only) (NN man))
    (VP (VBZ possesses)
      (NP (DT the) (JJ rational) (NN faculty)))
    (. .)))

advmod(man-2, Only-1)
nsubj(possesses-3, man-2)
root(ROOT-0, possesses-3)
det(faculty-6, the-4)
amod(faculty-6, rational-5)
dobj(possesses-3, faculty-6)

