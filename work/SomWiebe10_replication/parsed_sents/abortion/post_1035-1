(ROOT
  (S
    (VP (VB Do) (RB n't)
      (VP (VB abstract)
        (NP
          (NP (DT a) (NN fragment))
          (PP (IN of)
            (NP
              (NP (DT a) (NN statement))
              (PP (IN for)
                (NP (PRP$ your) (NN propaganda))))))))
    (. .)))

aux(abstract-3, Do-1)
neg(abstract-3, n't-2)
root(ROOT-0, abstract-3)
det(fragment-5, a-4)
dobj(abstract-3, fragment-5)
det(statement-8, a-7)
prep_of(fragment-5, statement-8)
poss(propaganda-11, your-10)
prep_for(statement-8, propaganda-11)

