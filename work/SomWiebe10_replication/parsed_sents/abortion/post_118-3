(ROOT
  (S
    (NP (DT Each) (NN miscarriage))
    (VP (MD would)
      (VP (VB have)
        (S
          (VP (TO to)
            (VP (VB be)
              (VP (VBN investigated)))))))
    (. .)))

det(miscarriage-2, Each-1)
nsubj(have-4, miscarriage-2)
xsubj(investigated-7, miscarriage-2)
aux(have-4, would-3)
root(ROOT-0, have-4)
