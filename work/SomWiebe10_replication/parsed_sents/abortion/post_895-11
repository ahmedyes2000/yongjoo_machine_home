(ROOT
  (S
    (NP (PRP I))
    (VP (VBP 'm) (RB not)
      (VP (VBN allowed)
        (S
          (VP (TO to)
            (VP (VB steal)
              (PP (IN from)
                (NP (PRP you))))))))
    (. .)))

nsubjpass(allowed-4, I-1)
xsubj(steal-6, I-1)
auxpass(allowed-4, 'm-2)
neg(allowed-4, not-3)
root(ROOT-0, allowed-4)
aux(steal-6, to-5)
xcomp(allowed-4, steal-6)
prep_from(steal-6, you-8)

