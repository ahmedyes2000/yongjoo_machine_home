(ROOT
  (S
    (NP (DT The) (JJ whole) (NN article))
    (VP (MD can)
      (VP (VB be)
        (VP (VBN found)
          (ADVP (RB here)))))
    (. .)))

det(article-3, The-1)
amod(article-3, whole-2)
nsubjpass(found-6, article-3)
aux(found-6, can-4)
auxpass(found-6, be-5)
root(ROOT-0, found-6)
advmod(found-6, here-7)

