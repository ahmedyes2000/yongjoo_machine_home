(ROOT
  (FRAG
    (NP (PRP$ My) (JJ sole) (NN reason))
    (: :)
    (S
      (NP (PRP They))
      (VP (VBD did) (RB not)
        (VP (VB choose)
          (S
            (VP (TO to)
              (VP (VB have)
                (NP (NN sex))))))))
    (. .)))

poss(reason-3, My-1)
amod(reason-3, sole-2)
root(ROOT-0, reason-3)
nsubj(choose-8, They-5)
xsubj(have-10, They-5)
aux(choose-8, did-6)
neg(choose-8, not-7)
dep(reason-3, choose-8)
aux(have-10, to-9)
xcomp(choose-8, have-10)
dobj(have-10, sex-11)

