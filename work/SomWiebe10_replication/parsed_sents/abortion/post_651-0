(ROOT
  (S
    (PP (IN Of)
      (NP (NN course)))
    (NP (PRP they))
    (VP (VBP change)
      (PRT (RP over))
      (NP (NN time))
      (, ,)
      (PP (IN as)
        (NP (JJ medical) (NN knowledge) (NNS increases))))
    (. .)))

prep_of(change-4, course-2)
nsubj(change-4, they-3)
root(ROOT-0, change-4)
prt(change-4, over-5)
dobj(change-4, time-6)
amod(increases-11, medical-9)
nn(increases-11, knowledge-10)
prep_as(change-4, increases-11)

