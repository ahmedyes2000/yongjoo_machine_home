(ROOT
  (S
    (S
      (SBAR
        (WHADVP (WRB When))
        (S
          (NP (NN life))
          (VP (VBZ begins))))
      (VP (VBZ is)
        (NP (DT a) (JJ scientific) (NN fact))))
    (, ,) (IN so)
    (S
      (NP (DT an) (NN opinion) (NN poll))
      (ADVP (RB really))
      (VP (VBZ does) (RB n't)
        (VP (VB help)
          (S
            (NP (DT that))
            (ADJP (JJ much))))))
    (. .)))

advmod(begins-3, When-1)
nsubj(begins-3, life-2)
advcl(fact-7, begins-3)
csubj(fact-7, begins-3)
cop(fact-7, is-4)
det(fact-7, a-5)
amod(fact-7, scientific-6)
root(ROOT-0, fact-7)
dep(fact-7, so-9)
det(poll-12, an-10)
nn(poll-12, opinion-11)
nsubj(help-16, poll-12)
advmod(help-16, really-13)
aux(help-16, does-14)
neg(help-16, n't-15)
ccomp(fact-7, help-16)
nsubj(much-18, that-17)
xcomp(help-16, much-18)

