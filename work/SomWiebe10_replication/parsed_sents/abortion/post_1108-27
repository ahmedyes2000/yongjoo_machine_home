(ROOT
  (S
    (NP (NN Nobody))
    (VP (VBZ is) (`` ``)
      (VP (VBG telling)
        (NP (NNS kids))
        (S
          (VP (TO to)
            (VP (VB have)
              (NP (NN sex) ('' '')))))))
    (. .)))

nsubj(telling-4, Nobody-1)
aux(telling-4, is-2)
root(ROOT-0, telling-4)
