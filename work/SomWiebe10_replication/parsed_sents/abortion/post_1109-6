(ROOT
  (S
    (SBAR (IN Because)
      (S
        (NP (EX there))
        (VP (VBZ is)
          (NP (DT no) (NN way) (NN man)))))
    (VP (MD ought)
      (S
        (VP (TO to)
          (VP (VB be)
            (VP (CC and)
              (NP (DT no) (NN norm))
              (PP (IN from)
                (SBAR
                  (WHNP (WDT which))
                  (S
                    (VP (TO to)
                      (VP (VB deviate)
                        (PP (IN from))))))))))))
    (. .)))

mark(is-3, Because-1)
expl(is-3, there-2)
csubj(ought-7, is-3)
det(man-6, no-4)
nn(man-6, way-5)
nsubj(is-3, man-6)
root(ROOT-0, ought-7)
aux(norm-12, to-8)
aux(norm-12, be-9)
cc(norm-12, and-10)
det(norm-12, no-11)
xcomp(ought-7, norm-12)
nsubj(deviate-16, which-14)
aux(deviate-16, to-15)
prepc_from(norm-12, deviate-16)
prep(deviate-16, from-17)

