(ROOT
  (S
    (NP
      (NP (DT All) (NNS abortions))
      (SBAR
        (WHADVP
          (ADVP (DT no) (NN matter))
          (WRB when))
        (S
          (NP (PRP they))
          (VP (VBP occur)))))
    (VP (VBP are)
      (NP
        (ADJP (RB very) (JJ sad))
        (NNS affairs)))
    (. .)))

det(abortions-2, All-1)
tmod(occur-7, abortions-2)
nsubj(affairs-11, abortions-2)
dep(matter-4, no-3)
advmod(when-5, matter-4)
advmod(occur-7, when-5)
nsubj(occur-7, they-6)
dep(abortions-2, occur-7)
cop(affairs-11, are-8)
advmod(sad-10, very-9)
amod(affairs-11, sad-10)
root(ROOT-0, affairs-11)

