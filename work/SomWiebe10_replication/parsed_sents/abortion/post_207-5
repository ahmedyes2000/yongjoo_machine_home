(ROOT
  (S
    (NP (PRP We))
    (VP (VBP value)
      (NP
        (NP (DT the) (NN life))
        (PP (DT both) (IN of)
          (NP
            (NP (DT the) (NN born))
            (CC and)
            (NP (NN unborn))))))
    (. .)))

nsubj(value-2, We-1)
root(ROOT-0, value-2)
det(life-4, the-3)
dobj(value-2, life-4)
dep(life-4, both-5)
det(born-8, the-7)
prep_of(life-4, born-8)
prep_of(life-4, unborn-10)
conj_and(born-8, unborn-10)

