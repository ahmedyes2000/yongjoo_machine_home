  (S
    (PP (IN By)
      (NP (DT the) (NN way)))
    (, ,)
    (NP (DT a) (NN child))
    (VP (VBZ has)
      (NP
        (NP (JJ discernible) (NNS fingers))
        (PP (IN at)
          (NP
            (NP
              (NP (CD 3) (NNS weeks))
              (NP (NN pregnancy)))
            (: ;)
            (SBAR
              (WHNP (WDT that))
              (S
                (VP (VBZ 's)
                  (SBAR
                    (WHADVP (WRB why))
                    (S
                      (NP (JJS most) (NN abortion) (NNS procedures))
                      (VP (VBP involve)
                        (NP
                          (NP (DT the) (NN quartering))
                          (PP (IN of)
                            (NP (DT the) (NN child))))))))))))))
    (. .)))

det(way-3, the-2)
prep_by(has-7, way-3)
det(child-6, a-5)
nsubj(has-7, child-6)
