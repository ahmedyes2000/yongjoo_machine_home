(ROOT
  (S
    (NP (PRP I))
    (ADVP (RB even))
    (VP (VBD had)
      (NP
        (NP (DT a) (NN girlfriend))
        (SBAR
          (WHNP (WP who))
          (S
            (VP (VBD had)
              (NP (DT an) (NN abortion)))))))
    (. .)))

nsubj(had-3, I-1)
advmod(had-3, even-2)
root(ROOT-0, had-3)
det(girlfriend-5, a-4)
dobj(had-3, girlfriend-5)
nsubj(had-7, girlfriend-5)
rcmod(girlfriend-5, had-7)
det(abortion-9, an-8)
dobj(had-7, abortion-9)

