(ROOT
  (S
    (NP
      (NP (DT The) (JJ whole) (NN purpose))
      (PP (IN of)
        (NP (DT the) (NN procedure))))
    (VP (VBZ is)
      (S
        (VP (TO to)
          (VP (VB kill)
            (NP (NN someone))))))
    (. .)))

det(purpose-3, The-1)
amod(purpose-3, whole-2)
nsubj(is-7, purpose-3)
xsubj(kill-9, purpose-3)
det(procedure-6, the-5)
prep_of(purpose-3, procedure-6)
root(ROOT-0, is-7)
aux(kill-9, to-8)
xcomp(is-7, kill-9)
dobj(kill-9, someone-10)

