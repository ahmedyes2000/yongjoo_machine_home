(ROOT
  (S
    (SBAR
      (WHADVP (WRB When))
      (S
        (NP (PRP it))
        (VP (VBZ is)
          (VP (VBN born)
            (PP (IN into)
              (NP (DT a) (JJ different) (NN environment)))))))
    (, ,)
    (NP (DT that) (NN potential))
    (VP (VBZ is)
      (VP (VBN activated)))
    (. .)))

advmod(born-4, When-1)
nsubjpass(born-4, it-2)
auxpass(born-4, is-3)
advcl(activated-13, born-4)
det(environment-8, a-6)
amod(environment-8, different-7)
prep_into(born-4, environment-8)
det(potential-11, that-10)
nsubjpass(activated-13, potential-11)
auxpass(activated-13, is-12)
root(ROOT-0, activated-13)

