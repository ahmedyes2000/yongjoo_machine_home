  (S
    (SBAR
      (WHNP (WP What))
      (S
        (VP (VBZ is)
          (ADJP (JJ necessary)))))
    (ADVP (RB here))
    (VP (VBZ is)
      (S
        (VP (TO to)
          (VP (VB use)
            (S
              (NP
                (NP (DT all) (JJ available) (NN knowledge))
                (PP (IN of)
                  (NP
                    (NP (NN science))
                    (, ,)
                    (NP (NN phylosophy))
                    (CC and)
                    (NP (DT any) (NN subject)))))
              (ADJP (JJ available)
                (S
                  (VP (TO to)
                    (VP (VB give)
                      (NP (DT an) (NN answer))
                      (PP (TO to)
                        (NP (DT those) (CD two) (NNS questions))))))))))))
    (. .)))

