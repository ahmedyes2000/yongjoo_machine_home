(ROOT
  (S
    (NP (DT These) (NNS things))
    (VP (VBP are)
      (UCP
        (ADJP (RB very) (JJ powerful))
        (, ,)
        (CC and)
        (NP (JJ real) (NNS forces))))
    (. .)))

det(things-2, These-1)
nsubj(powerful-5, things-2)
cop(powerful-5, are-3)
advmod(powerful-5, very-4)
root(ROOT-0, powerful-5)
amod(forces-9, real-8)
conj_and(powerful-5, forces-9)

