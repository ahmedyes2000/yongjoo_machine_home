(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ takes)
      (NP (CD TWO) (NNS people))
      (S
        (VP (TO to)
          (VP (VB make)
            (NP (NN sex))))))
    (. .)))

nsubj(takes-2, It-1)
root(ROOT-0, takes-2)
num(people-4, TWO-3)
dobj(takes-2, people-4)
aux(make-6, to-5)
xcomp(takes-2, make-6)
dobj(make-6, sex-7)

