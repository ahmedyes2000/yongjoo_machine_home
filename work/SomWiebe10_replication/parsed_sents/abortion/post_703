(ROOT
  (S
    (S
      (NP (DT This))
      (VP (VBZ is)
        (NP
          (NP (DT a) (JJ ill))
          (VP (VBN posed)
            (S
              (VP (NN question)
                (PRN
                  (ADVP (RB anyway))
                  (ADVP (RB obviously)))
                (NP (PRP$ its)
                  (ADJP (RB not) (RB just))
                  (NN life))))))))
    (NP (PRP we))
    (VP (VBP are)
      (VP
        (VP (VBN concerned)
          (PP (IN with)))
        (, ,)
        (CC but)
        (VP
          (NP (JJ potential) (JJ individual) (JJ human) (NN life)))))
    (. .)))

