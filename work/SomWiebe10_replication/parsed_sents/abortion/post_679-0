(ROOT
  (S
    (NP (PRP You))
    (VP (VBD were)
      (VP (VBG doing)
        (ADVP (RB so) (RB well) (RB up))
        (PP (IN until)
          (NP (RB here)))))
    (. .)))

nsubj(doing-3, You-1)
aux(doing-3, were-2)
root(ROOT-0, doing-3)
advmod(up-6, so-4)
advmod(up-6, well-5)
advmod(doing-3, up-6)
prep(doing-3, until-7)
pobj(until-7, here-8)

