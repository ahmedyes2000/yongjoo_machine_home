(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (NN science))
        (VP (VBZ says)
          (SBAR
            (S
              (NP (PRP it))
              (VP (VBZ 's)
                (NP
                  (NP (DT a) (NN sentient))
                  (VP (VBG being)))))))))
    (, ,)
    (NP (PRP I))
    (VP (MD 'll)
      (VP (VB be)
        (PP (IN against)
          (NP (PRP it)))))
    (. .)))

mark(says-3, If-1)
nsubj(says-3, science-2)
advcl(be-12, says-3)
nsubj(sentient-7, it-4)
cop(sentient-7, 's-5)
det(sentient-7, a-6)
ccomp(says-3, sentient-7)
partmod(sentient-7, being-8)
nsubj(be-12, I-10)
aux(be-12, 'll-11)
root(ROOT-0, be-12)
prep_against(be-12, it-14)

