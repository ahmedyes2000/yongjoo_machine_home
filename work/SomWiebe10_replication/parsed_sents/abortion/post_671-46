      (S
        (NP (PRP you))
        (VP (VBP believe)
          (PP (IN in)
            (NP
              (NP (DT a) (NN god))
              (CC and)
              (NP (DT a) (NN soul))))
          (, ,)
          (SBAR
            (SBARQ
              (WHADVP (WRB how))
              (SQ (VBP do)
                (NP (PRP you))
                (VP (VB know)
                  (SBAR
                    (WHADVP (WRB when))
                    (S
                      (NP (DT that) (NN god))
                      (VP (VBZ sticks)
                        (NP (DT the) (NN soul))
                        (PP (IN into)
                          (NP (DT the) (NN body))))))))
              (. ?))))))))

dep(even-2, But-1)
root(ROOT-0, even-2)
