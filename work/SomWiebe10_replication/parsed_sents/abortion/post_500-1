(ROOT
  (S
    (S
      (ADVP (RB There))
      (NP (PRP you))
      (VP (VBD were)
        (ADJP (JJ bad))))
    (CC and)
    (S
      (NP (DT the) (NN baby))
      (VP (VBZ is)
        (NP (PRP$ your) (NN punishment))))
    (. ?)))

advmod(bad-4, There-1)
nsubj(bad-4, you-2)
cop(bad-4, were-3)
root(ROOT-0, bad-4)
det(baby-7, the-6)
nsubj(punishment-10, baby-7)
cop(punishment-10, is-8)
poss(punishment-10, your-9)
