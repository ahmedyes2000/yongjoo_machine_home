(ROOT
  (S ('' '')
    (PP (IN If)
      (S
        (NP (PRP it))
        (VP (VBZ is)
          (NP (DT a) (JJ human) (NN life))
          (ADVP (RB then))
          (NP
            (NP (DT no) (NN reason))
            (PP (IN for)
              (NP (NN abortion)))))))
    (VP (VBZ is)
      (ADJP (JJ adequate)))
    (. .)))

nsubj(life-7, it-3)
cop(life-7, is-4)
det(life-7, a-5)
amod(life-7, human-6)
prepc_if(adequate-14, life-7)
advmod(life-7, then-8)
det(reason-10, no-9)
attr(life-7, reason-10)
prep_for(reason-10, abortion-12)
cop(adequate-14, is-13)
root(ROOT-0, adequate-14)

