        (SBAR
          (S
            (NP (NN life))
            (VP (VBZ begins)
              (PP (IN before)
                (NP (NN conception))))))))
    (, ,)
    (NP (PRP I))
    (VP (VBZ decree)
      (SBAR (IN that)
        (S
          (NP (DT all) (NN birth) (NN control))
          (VP (VBZ is)
            (ADVP (RB here))
            (PP (IN by)
              (ADJP (VBN banned)))))))
    (. .)))

