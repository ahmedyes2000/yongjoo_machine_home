(ROOT
  (S (`` ``)
    (NP (NNP Life))
    (VP (VBZ is)
      (NP (NN life))
      (SBAR (IN whether)
        (S
          (NP (PRP we))
          (VP (VBP believe)
            (PP (IN in)
              (NP (PRP it)))))
        (CC or)
        (RB not)))
    (. .)))

nsubj(life-4, Life-2)
cop(life-4, is-3)
root(ROOT-0, life-4)
complm(believe-7, whether-5)
nsubj(believe-7, we-6)
ccomp(life-4, believe-7)
