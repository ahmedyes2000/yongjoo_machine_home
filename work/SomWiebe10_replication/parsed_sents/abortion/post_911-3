(ROOT
  (S
    (S
      (NP (DT That))
      (VP (VBD said)))
    (, ,)
    (NP (NNS abortions))
    (VP (VBP are)
      (ADJP
        (ADJP (JJ completely))
        (CC and)
        (ADJP (RB absolutely) (JJ necessary)))
      (PP (RB even)
        (NP (JJ late) (NN term) (NNS abortions))))
    (. .)))

nsubj(said-2, That-1)
ccomp(completely-6, said-2)
nsubj(completely-6, abortions-4)
nsubj(necessary-9, abortions-4)
cop(completely-6, are-5)
root(ROOT-0, completely-6)
advmod(necessary-9, absolutely-8)
conj_and(completely-6, necessary-9)
prep(completely-6, even-10)
amod(abortions-13, late-11)
nn(abortions-13, term-12)
pobj(even-10, abortions-13)

