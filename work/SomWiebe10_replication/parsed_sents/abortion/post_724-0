(ROOT
  (S
    (PP (IN in)
      (NP (PRP$ my) (NN opinion)))
    (NP (PRP it))
    (VP (MD should)
      (VP (VB be)
        (SBAR (IN because) (IN if)
          (S
            (NP (PRP you))
            (VP (VBP do) (RB n't)
              (VP (VB want)
                (S
                  (S
                    (VP (TO to)
                      (VP (VB get)
                        (ADJP (JJ pregnant)))))
                  (SBARQ
                    (WHADVP (WRB why))
                    (SQ (VBP are)
                      (NP (PRP you))
                      (VP (VBG having)
                        (NP (JJ sexual) (NN activity))))))))))))))

poss(opinion-3, my-2)
prep_in(be-6, opinion-3)
nsubj(be-6, it-4)
aux(be-6, should-5)
root(ROOT-0, be-6)
mark(want-12, because-7)
dep(want-12, if-8)
nsubj(want-12, you-9)
aux(want-12, do-10)
neg(want-12, n't-11)
advcl(be-6, want-12)
aux(pregnant-15, to-13)
dep(pregnant-15, get-14)
dep(want-12, pregnant-15)
advmod(having-19, why-16)
aux(having-19, are-17)
nsubj(having-19, you-18)
dep(pregnant-15, having-19)
amod(activity-21, sexual-20)
dobj(having-19, activity-21)

