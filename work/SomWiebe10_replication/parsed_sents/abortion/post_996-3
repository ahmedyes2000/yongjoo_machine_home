(ROOT
  (S
    (NP (DT This))
    (VP (VBZ is)
      (NP
        (NP (DT every) (NN time))
        (SBAR
          (S
            (NP (DT a) (NN guy))
            (VP (VBZ cums))))))
    (. .)))

nsubj(time-4, This-1)
cop(time-4, is-2)
det(time-4, every-3)
root(ROOT-0, time-4)
dobj(cums-7, time-4)
det(guy-6, a-5)
nsubj(cums-7, guy-6)
rcmod(time-4, cums-7)

