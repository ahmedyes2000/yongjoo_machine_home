  (SBARQ
    (PP (IN With)
      (NP
        (NP (DT that))
        (PP (IN in)
          (NP (NN mind)))))
    (, ,)
    (WHADVP (WRB how))
    (SQ (MD can)
      (VP (VB make)
        (NP (NNS arguments))
        (SBAR (IN that)
          (S
            (NP (PRP it))
            (VP (VBZ is) (RB not)
              (NP (DT a) (NN person)))))))
    (. ?)))

