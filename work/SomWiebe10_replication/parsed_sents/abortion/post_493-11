(ROOT
  (S
    (PP (IN In)
      (NP (JJ such) (NN case)))
    (, ,)
    (NP (DT the) (NN mother))
    (VP (VBZ has)
      (NP (DT the) (NN right)
        (S
          (VP (TO to)
            (VP (VB abort)
              (PP (IN at)
                (NP
                  (NP (DT a) (NN time)
                    (CC and)
                    (NN place))
                  (PP (IN of)
                    (NP (PRP$ her) (NN choosing))))))))))
    (. .)))

amod(case-3, such-2)
prep_in(has-7, case-3)
det(mother-6, the-5)
nsubj(has-7, mother-6)
root(ROOT-0, has-7)
det(right-9, the-8)
dobj(has-7, right-9)
aux(abort-11, to-10)
