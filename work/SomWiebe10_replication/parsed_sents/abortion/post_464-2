(ROOT
  (S
    (NP (PRP You))
    (VP (MD can)
      (VP (VB determine)
        (SBAR (IN whether)
          (CC or)
          (RB not)
          (S
            (NP (PRP they))
            (VP (VBP are)
              (VP (VBG doing)
                (NP (PRP it))
                (PP (IN for)
                  (FRAG
                    (NP (NN convenience))
                    (CC or)
                    (RB not)))))))))
    (. .)))

nsubj(determine-3, You-1)
aux(determine-3, can-2)
root(ROOT-0, determine-3)
complm(doing-9, whether-4)
ccomp(determine-3, not-6)
conj_or(doing-9, not-6)
nsubj(doing-9, they-7)
aux(doing-9, are-8)
ccomp(determine-3, doing-9)
dobj(doing-9, it-10)
prep(doing-9, for-11)
dep(for-11, convenience-12)
dep(convenience-12, or-13)
neg(convenience-12, not-14)

