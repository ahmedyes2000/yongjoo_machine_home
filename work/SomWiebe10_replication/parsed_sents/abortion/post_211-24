(ROOT
  (S
    (NP (PRP We))
    (VP
      (VP (VBP protect)
        (NP
          (NP (DT the) (NNS lives))
          (PP (IN of)
            (NP (PRP$ our) (NNS pets) (, ,) (RB even) (JJ stray) (NNS animals)))))
      (, ,)
      (CC but)
      (VP (VBP offer)
        (NP
          (NP (NN nothing))
          (PP (IN for)
            (NP (PRP$ our) (JJ own) (NNS children))))))
    (. .)))

nsubj(protect-2, We-1)
nsubj(offer-14, We-1)
root(ROOT-0, protect-2)
det(lives-4, the-3)
dobj(protect-2, lives-4)
poss(animals-11, our-6)
nn(animals-11, pets-7)
advmod(animals-11, even-9)
amod(animals-11, stray-10)
prep_of(lives-4, animals-11)
conj_but(protect-2, offer-14)
dobj(offer-14, nothing-15)
poss(children-19, our-17)
amod(children-19, own-18)
prep_for(nothing-15, children-19)

