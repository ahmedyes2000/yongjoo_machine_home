(ROOT
  (S
    (ADVP (RB Now))
    (, ,)
    (SBAR
      (WHADVP (WRB how))
      (S
        (VP (MD can))))
    (NP (PRP we))
    (VP (VBP stop)
      (NP (NN abortion)))
    (. ?)))

