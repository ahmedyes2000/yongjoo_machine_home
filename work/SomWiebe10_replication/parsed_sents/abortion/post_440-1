(ROOT
  (S
    (NP (NNP Science))
    (VP (MD can) (RB not)
      (VP (VB discover)
        (CC or)
        (VB identify)
        (NP (DT a) (NN soul))))
    (. .)))

nsubj(discover-4, Science-1)
nsubj(identify-6, Science-1)
aux(discover-4, can-2)
neg(discover-4, not-3)
root(ROOT-0, discover-4)
conj_or(discover-4, identify-6)
det(soul-8, a-7)
dobj(discover-4, soul-8)

