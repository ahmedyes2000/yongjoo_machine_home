(ROOT
  (S
    (NP (PRP I))
    (VP (VBP am) (RB not)
      (VP (VBG saying)
        (NP
          (NP (DT the) (JJ old) (NN man))
          (SBAR
            (S
              (VP (VBZ does) (RB not)
                (VP (VB deserve)
                  (S
                    (VP (TO to)
                      (VP (VB live)))))))))))
    (. .)))

nsubj(saying-4, I-1)
aux(saying-4, am-2)
neg(saying-4, not-3)
root(ROOT-0, saying-4)
det(man-7, the-5)
amod(man-7, old-6)
dobj(saying-4, man-7)
aux(deserve-10, does-8)
neg(deserve-10, not-9)
rcmod(man-7, deserve-10)
aux(live-12, to-11)
xcomp(deserve-10, live-12)

