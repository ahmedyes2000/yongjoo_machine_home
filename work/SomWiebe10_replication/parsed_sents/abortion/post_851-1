(ROOT
  (S
    (NP (PRP One))
    (VP (MD can)
      (VP
        (VP (VB be)
          (ADJP (RB very) (JJ stupid)))
        (CC and)
        (VP (VB have)
          (NP (DT no) (NN sex) (NN drive)))))
    (. .)))

nsubj(stupid-5, One-1)
nsubj(have-7, One-1)
aux(stupid-5, can-2)
cop(stupid-5, be-3)
advmod(stupid-5, very-4)
root(ROOT-0, stupid-5)
conj_and(stupid-5, have-7)
det(drive-10, no-8)
nn(drive-10, sex-9)
dobj(have-7, drive-10)

