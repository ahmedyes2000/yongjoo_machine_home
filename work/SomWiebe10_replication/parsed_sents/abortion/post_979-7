(ROOT
  (S
    (NP (DT That))
    (VP (VBZ 's)
      (SBAR
        (WHADVP (WRB how) (RB long))
        (S
          (NP (PRP it))
          (VP (MD will)
            (VP (VB take)
              (S
                (VP (TO to)
                  (VP (VB pay)
                    (NP (DT the) (NN debt))))))))))
    (. .)))

nsubj('s-2, That-1)
root(ROOT-0, 's-2)
advmod(long-4, how-3)
advmod(take-7, long-4)
nsubj(take-7, it-5)
xsubj(pay-9, it-5)
aux(take-7, will-6)
ccomp('s-2, take-7)
aux(pay-9, to-8)
xcomp(take-7, pay-9)
det(debt-11, the-10)
dobj(pay-9, debt-11)

