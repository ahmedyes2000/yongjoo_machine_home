(ROOT
  (S
    (ADVP (RB However))
    (, ,)
    (VP (VBG forbidding)
      (NP (DT the) (NN right)
        (S
          (VP (TO to)
            (VP (VB choose))))))
    (VP (VBZ does)
      (NP (RB exactly) (DT that)))
    (. .)))

advmod(forbidding-3, However-1)
root(ROOT-0, forbidding-3)
det(right-5, the-4)
dobj(forbidding-3, right-5)
aux(choose-7, to-6)
infmod(right-5, choose-7)
dep(forbidding-3, does-8)
advmod(that-10, exactly-9)
dobj(does-8, that-10)

