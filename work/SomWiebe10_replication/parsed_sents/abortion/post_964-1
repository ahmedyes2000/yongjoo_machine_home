  (S
    (NP (PRP I))
    (VP (MD 'll)
      (VP (VB let)
        (S
          (NP (DT the) (NN story))
          (VP (VB speak)
            (PP (IN for)
              (NP
                (NP (PRP it) (POS 's))
                (NN self)))))))
    (. .)))

