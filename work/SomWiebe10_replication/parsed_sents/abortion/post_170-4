(ROOT
  (S
    (NP (DT That))
    (VP (VBZ is)
      (NP
        (NP (DT a) (JJ scientific) (NN fact))
        (, ,) (RB not)
        (NP (DT a) (JJ religious) (NN belief))))
    (. .)))

nsubj(fact-5, That-1)
cop(fact-5, is-2)
det(fact-5, a-3)
amod(fact-5, scientific-4)
root(ROOT-0, fact-5)
neg(fact-5, not-7)
det(belief-10, a-8)
amod(belief-10, religious-9)
dep(fact-5, belief-10)

