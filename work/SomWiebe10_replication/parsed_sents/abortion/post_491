    (NP (DT The) (NN thing))
    (VP (VBZ has)
      (VP (VBN been)
        (VP (VBG growning)
          (PP (IN by)
            (NP (CD 40) (NN mill)))
          (NP (DT every) (NN year))
          (SBAR (IN since)
            (S
              (NP (PRP it))
              (VP (VBZ has)
                (VP (VBN been)
                  (VP (VBN gagged)))))))))
    (. .)))

