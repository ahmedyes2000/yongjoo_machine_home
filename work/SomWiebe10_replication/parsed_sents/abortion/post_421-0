(ROOT
  (S
    (VP (VBZ is)
      (S
        (VP (TO to)
          (VP (VB make)
            (PRT (RP up))
            (NP (NNS definitions))
            (SBAR (IN as)
              (S
                (NP (PRP you))
                (VP (VBP go)
                  (PRT (RP along)))))))))
    (. .)))

root(ROOT-0, is-1)
aux(make-3, to-2)
xcomp(is-1, make-3)
prt(make-3, up-4)
dobj(make-3, definitions-5)
mark(go-8, as-6)
nsubj(go-8, you-7)
advcl(make-3, go-8)
prt(go-8, along-9)

