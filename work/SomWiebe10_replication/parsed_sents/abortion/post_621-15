(ROOT
  (S
    (NP
      (NP (DT The) (NN thought))
      (PP (IN of)
        (S
          (ADVP (RB actually))
          (VP (VBG intervening)))))
    (VP (VBZ is)
      (ADJP (JJ unfathomable))
      (SBAR
        (WHADVP (WRB when))
        (S
          (NP (PRP it))
          (VP (VBZ has)
            (NP (JJ actual) (JJ human) (NNS faces))))))
    (. .)))

det(thought-2, The-1)
nsubj(unfathomable-7, thought-2)
advmod(intervening-5, actually-4)
prepc_of(thought-2, intervening-5)
cop(unfathomable-7, is-6)
root(ROOT-0, unfathomable-7)
advmod(has-10, when-8)
nsubj(has-10, it-9)
advcl(unfathomable-7, has-10)
amod(faces-13, actual-11)
amod(faces-13, human-12)
dobj(has-10, faces-13)

