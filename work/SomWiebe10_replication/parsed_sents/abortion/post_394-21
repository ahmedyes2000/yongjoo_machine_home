(ROOT
  (S
    (NP (DT Every) (JJ human) (NN being))
    (VP (VBZ has)
      (NP
        (NP (JJ individual) (NNS rights))
        (PP (IN of)
          (NP (PRP$ their) (JJ own) (NN body)))))
    (. .)))

det(being-3, Every-1)
amod(being-3, human-2)
nsubj(has-4, being-3)
root(ROOT-0, has-4)
amod(rights-6, individual-5)
dobj(has-4, rights-6)
poss(body-10, their-8)
amod(body-10, own-9)
prep_of(rights-6, body-10)

