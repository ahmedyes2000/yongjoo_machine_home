(ROOT
  (S
    (S
      (NP (PRP$ Your) (NN position))
      (VP (VBZ is)
        (NP
          (NP (DT the) (NN one))
          (SBAR
            (S
              (NP (DT the) (NNP Pharisee))
              (VP (VBD took)))))))
    (CC and)
    (S
      (NP (DT the) (CD one) (NNS Jesus))
      (VP (VBD condemned)))
    (. .)))

