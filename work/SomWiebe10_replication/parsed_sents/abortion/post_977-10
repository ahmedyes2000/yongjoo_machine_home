(ROOT
  (FRAG
    (NP (RB about) (JJ judging))
    (: :)
    (S
      (NP
        (NP (DT The) (NN passage))
        (PP (IN of)
          (NP (NN scripture))))
      (VP (VBD were)
        (NP (NNP Jesus) (NNS deals))
        (PP (IN with)
          (S
            (VP (VBG judging)
              (SBAR
                (S
                  (VP
                    (VP (VBZ does) (RB n't)
                      (VP (VB condemn)
                        (S
                          (VP (VBG judging)))))
                    (CC nor)
                    (VP (VBD forbid)
                      (NP (PRP it)))))))))))
    (. .)))

advmod(judging-2, about-1)
root(ROOT-0, judging-2)
det(passage-5, The-4)
nsubj(deals-10, passage-5)
prep_of(passage-5, scripture-7)
