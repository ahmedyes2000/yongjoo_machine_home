(ROOT
  (S ('' '')
    (NP (DT That) (DT the) (NNS Christ))
    (VP (MD would)
      (VP (VB become)
        (SBAR
          (S
            (NP (DT a) (NN baby))
            (VP (VBZ shows)
              (SBAR
                (WHADVP (RB just) (WRB how))
                (S
                  (NP (JJ sacred) (NN life))
                  (VP (VBZ is)))))))))
    (. .)))

predet(Christ-4, That-2)
det(Christ-4, the-3)
nsubj(become-6, Christ-4)
aux(become-6, would-5)
root(ROOT-0, become-6)
det(baby-8, a-7)
nsubj(shows-9, baby-8)
ccomp(become-6, shows-9)
advmod(how-11, just-10)
advmod(is-14, how-11)
amod(life-13, sacred-12)
nsubj(is-14, life-13)
ccomp(shows-9, is-14)

