(ROOT
  (S
    (NP (PRP One))
    (VP (MD can)
      (VP (VB get)
        (NP
          (NP
            (ADJP (RB highly) (VBN varied))
            (NNS results))
          (PP (IN in)
            (NP (NNS polls))))
        (PP (VBG depending)
          (PP (IN on)
            (SBAR
              (WHADVP (WRB how))
              (S
                (NP (DT the) (NN question))
                (VP (VBZ is)
                  (VP (VBN asked)))))))))
    (. .)))

nsubj(get-3, One-1)
aux(get-3, can-2)
root(ROOT-0, get-3)
advmod(varied-5, highly-4)
amod(results-6, varied-5)
dobj(get-3, results-6)
prep_in(results-6, polls-8)
prepc_depending_on(get-3, on-10)
advmod(asked-15, how-11)
det(question-13, the-12)
nsubjpass(asked-15, question-13)
auxpass(asked-15, is-14)
pcomp(get-3, asked-15)

