(ROOT
  (S ('' '')
    (NP (DT That))
    (VP (VBZ is)
      (SBAR
        (WHADVP (WRB why))
        (S
          (NP (PRP we))
          (VP (MD should)
            (VP (VB work)
              (S
                (VP (TO to)
                  (VP (VB reduce)
                    (NP (NNS abortions))))))))))
    (. .)))

nsubj(is-3, That-2)
root(ROOT-0, is-3)
advmod(work-7, why-4)
nsubj(work-7, we-5)
xsubj(reduce-9, we-5)
aux(work-7, should-6)
advcl(is-3, work-7)
aux(reduce-9, to-8)
xcomp(work-7, reduce-9)
dobj(reduce-9, abortions-10)

