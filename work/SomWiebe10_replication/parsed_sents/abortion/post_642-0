(ROOT
  (S
    (NP (PRP I))
    (VP (VBP am)
      (ADVP (RB broadly)
        (PP (IN in)
          (NP
            (NP (NN favor))
            (PP (IN of)
              (NP (JJ contentious) (NNS issues))))))
      (VP (VBG being)
        (VP (VBN decided)
          (PP
            (PP (IN in)
              (NP (JJR smaller) (NNS jurisdictions)))
            (, ,) (RB rather)
            (PP (IN than)
              (ADVP (RB nationally)))))))
    (. .)))

nsubjpass(decided-10, I-1)
aux(decided-10, am-2)
advmod(decided-10, broadly-3)
prep_in(broadly-3, favor-5)
amod(issues-8, contentious-7)
prep_of(favor-5, issues-8)
auxpass(decided-10, being-9)
root(ROOT-0, decided-10)
amod(jurisdictions-13, smaller-12)
prep_in(decided-10, jurisdictions-13)
advmod(decided-10, rather-15)
prep(decided-10, than-16)
pcomp(than-16, nationally-17)

