(ROOT
  (S
    (NP (PRP I))
    (VP (MD would)
      (ADVP (RB just))
      (VP (VB like)
        (S
          (VP (TO to)
            (VP (VB add)
              (NP (CD one) (JJR more) (NN thing)))))))
    (. .)))

nsubj(like-4, I-1)
xsubj(add-6, I-1)
aux(like-4, would-2)
advmod(like-4, just-3)
root(ROOT-0, like-4)
aux(add-6, to-5)
xcomp(like-4, add-6)
num(thing-9, one-7)
amod(thing-9, more-8)
dobj(add-6, thing-9)

