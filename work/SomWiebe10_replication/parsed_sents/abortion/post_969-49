(ROOT
  (S
    (NP (PRP$ My) (JJ own) (NN mother))
    (VP
      (VP (VBZ has)
        (NP (CD 6) (NNS children)))
      (CC and)
      (VP (VBZ has)
        (ADVP (RB never))
        (VP (VBN had)
          (NP (DT an) (NN abortion))
          (ADVP (RB !!)))))))

poss(mother-3, My-1)
amod(mother-3, own-2)
nsubj(has-4, mother-3)
nsubj(had-10, mother-3)
root(ROOT-0, has-4)
num(children-6, 6-5)
dobj(has-4, children-6)
aux(had-10, has-8)
neg(had-10, never-9)
