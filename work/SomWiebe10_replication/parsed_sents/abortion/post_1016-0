(ROOT
  (S
    (INTJ (UH No))
    (VP (VB answer)
      (PP (TO to)
        (NP
          (NP (PRP$ my) (NN retort))
          (, ,) (RB just)
          (NP (DT a) (JJ down) (NN vote)))))
    (. .)))

dep(answer-2, No-1)
root(ROOT-0, answer-2)
poss(retort-5, my-4)
prep_to(answer-2, retort-5)
advmod(retort-5, just-7)
det(vote-10, a-8)
amod(vote-10, down-9)
dep(retort-5, vote-10)

