(ROOT
  (S
    (S
      (NP (EX There))
      (VP (VBP are)
        (NP (JJ many) (NNS ways))
        (SBAR (IN that)
          (S
            (NP (PRP you))
            (VP (MD can)
              (VP (VB help)
                (NP (NN stop) (NN abortion))))))))
    (, ,) (IN so)
    (S
      (NP (PRP I))
      (VP (VBP hope)
        (SBAR (IN that)
          (S
            (NP (PRP you))
            (VP
              (VP (VBP take)
                (NP (DT a) (NN while)))
              (CC and)
              (VP (VBP do)
                (NP (DT that))))))))
    (. .)))

expl(are-2, There-1)
root(ROOT-0, are-2)
amod(ways-4, many-3)
nsubj(are-2, ways-4)
complm(help-8, that-5)
nsubj(help-8, you-6)
aux(help-8, can-7)
ccomp(are-2, help-8)
nn(abortion-10, stop-9)
dobj(help-8, abortion-10)
dep(are-2, so-12)
nsubj(hope-14, I-13)
ccomp(are-2, hope-14)
complm(take-17, that-15)
nsubj(take-17, you-16)
nsubj(do-21, you-16)
ccomp(hope-14, take-17)
det(while-19, a-18)
dobj(take-17, while-19)
ccomp(hope-14, do-21)
conj_and(take-17, do-21)
dobj(do-21, that-22)

