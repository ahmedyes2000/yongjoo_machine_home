(ROOT
  (S
    (NP (DT These) (NNS facts))
    (ADVP (RB alone))
    (VP (MD should)
      (VP (VB convince)
        (NP (NN anyone))
        (S
          (VP (TO to)
            (VP (VB eliminate)
              (NP
                (NP (DT the) (NN idea))
                (PP (IN of)
                  (NP
                    (NP (NN pregnancy) (NN termination))
                    (PP (RB out) (IN of)
                      (NP (PRP$ their) (NN mind)))))))))))
    (. .)))

det(facts-2, These-1)
nsubj(convince-5, facts-2)
advmod(convince-5, alone-3)
aux(convince-5, should-4)
root(ROOT-0, convince-5)
dobj(convince-5, anyone-6)
aux(eliminate-8, to-7)
xcomp(convince-5, eliminate-8)
det(idea-10, the-9)
dobj(eliminate-8, idea-10)
nn(termination-13, pregnancy-12)
prep_of(idea-10, termination-13)
poss(mind-17, their-16)
prep_out_of(termination-13, mind-17)

