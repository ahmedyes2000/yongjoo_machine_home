(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is)
      (NP
        (NP (DT a) (NN ball))
        (PP (IN of)
          (S
            (VP
              (ADVP (RB rapidly))
              (VBG dividing)
              (NP (NNS cells)))))))
    (. .)))

nsubj(ball-4, It-1)
cop(ball-4, is-2)
det(ball-4, a-3)
root(ROOT-0, ball-4)
advmod(dividing-7, rapidly-6)
prepc_of(ball-4, dividing-7)
dobj(dividing-7, cells-8)

