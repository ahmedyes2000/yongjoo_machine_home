(ROOT
  (S
    (NP (DT The) (JJ scientific) (NN fact))
    (VP (VBZ is)
      (SBAR (IN that)
        (S
          (NP (DT a) (JJ new) (JJ human) (NN life))
          (VP (VBZ is)
            (VP (VBN created)
              (PP (IN at)
                (NP
                  (NP (DT the) (NN moment))
                  (PP (IN of)
                    (NP (NN conception))))))))))
    (. .)))

det(fact-3, The-1)
amod(fact-3, scientific-2)
nsubj(is-4, fact-3)
root(ROOT-0, is-4)
complm(created-11, that-5)
det(life-9, a-6)
amod(life-9, new-7)
amod(life-9, human-8)
nsubjpass(created-11, life-9)
auxpass(created-11, is-10)
ccomp(is-4, created-11)
det(moment-14, the-13)
prep_at(created-11, moment-14)
prep_of(moment-14, conception-16)

