(ROOT
  (S
    (NP (PRP We))
    (ADVP (RB fervently))
    (VP (VBP hope)
      (SBAR (IN that)
        (S
          (NP
            (NP (NN none))
            (PP (IN of)
              (NP
                (NP (DT the) (NNS casualties))
                (, ,)
                (NP (DT this) (NN time))
                (, ,))))
          (VP (VBP are)
            (ADJP (RB completely) (JJ innocent))))))
    (. .)))

nsubj(hope-3, We-1)
advmod(hope-3, fervently-2)
root(ROOT-0, hope-3)
complm(innocent-15, that-4)
nsubj(innocent-15, none-5)
det(casualties-8, the-7)
prep_of(none-5, casualties-8)
det(time-11, this-10)
appos(casualties-8, time-11)
cop(innocent-15, are-13)
advmod(innocent-15, completely-14)
ccomp(hope-3, innocent-15)

