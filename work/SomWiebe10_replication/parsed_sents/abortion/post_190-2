(ROOT
  (S
    (S
      (NP (EX There))
      (VP (VBZ is) (RB not)
        (NP
          (NP (DT a) (JJ unanimous) (NN tradition))
          (PP (IN on)
            (NP (DT this) (NN point))))))
    (CC and)
    (S
      (NP (NNS authors))
      (VP (VBP are)
        (ADVP (RB as) (RB yet))
        (PP (IN in)
          (NP (NN disagreement)))))
    (. .)))

expl(is-2, There-1)
root(ROOT-0, is-2)
neg(is-2, not-3)
det(tradition-6, a-4)
amod(tradition-6, unanimous-5)
nsubj(is-2, tradition-6)
det(point-9, this-8)
prep_on(tradition-6, point-9)
nsubj(are-12, authors-11)
conj_and(is-2, are-12)
advmod(yet-14, as-13)
advmod(are-12, yet-14)
prep_in(are-12, disagreement-16)

