(ROOT
  (S
    (NP
      (NP (NNP Random) (NNS events))
      (PP (IN in)
        (NP
          (NP (JJ meiosis) (NN lead))
          (PP (TO to)
            (NP (DT a) (JJ unique) (JJ genetic))))))
    (VP (VBP make)
      (PRT (RP up))
      (PP (IN for)
        (NP (DT each) (NN gamete))))
    (. .)))

nn(events-2, Random-1)
nsubj(make-10, events-2)
amod(lead-5, meiosis-4)
prep_in(events-2, lead-5)
det(genetic-9, a-7)
amod(genetic-9, unique-8)
prep_to(lead-5, genetic-9)
root(ROOT-0, make-10)
prt(make-10, up-11)
det(gamete-14, each-13)
prep_for(make-10, gamete-14)

