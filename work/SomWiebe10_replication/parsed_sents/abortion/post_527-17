(ROOT
  (S
    (NP (NNS THIS))
    (VP (VBZ is)
      (NP (DT the) (NN freedom))
      (SBAR (IN that)
        (S
          (NP (FW i))
          (VP (VBP am)
            (VP (VBG speak)
              (PP (IN of)))))))
    (. .)))

nsubj(freedom-4, THIS-1)
cop(freedom-4, is-2)
det(freedom-4, the-3)
root(ROOT-0, freedom-4)
complm(speak-8, that-5)
nsubj(speak-8, i-6)
aux(speak-8, am-7)
ccomp(freedom-4, speak-8)
prep(speak-8, of-9)

