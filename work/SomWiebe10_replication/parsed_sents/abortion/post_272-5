(ROOT
  (FRAG
    (WHADVP (WRB How))
    (PP (IN about)
      (NP
        (NP (DT a) (NN heart) (NN beat))
        (CC or)
        (NP (DT a) (JJ first) (NN breath)))
      (. .))))

root(ROOT-0, How-1)
dep(How-1, about-2)
det(beat-5, a-3)
nn(beat-5, heart-4)
pobj(about-2, beat-5)
det(breath-9, a-7)
amod(breath-9, first-8)
pobj(about-2, breath-9)
conj_or(beat-5, breath-9)

