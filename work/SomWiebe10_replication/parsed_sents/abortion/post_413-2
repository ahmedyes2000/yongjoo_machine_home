(ROOT
  (S
    (NP (PRP I))
    (VP (VBD did)
      (S
        (ADVP (RB however))
        (VP (VB read)
          (NP (DT the) (NNS comments))
          (SBAR (IN before)
            (S
              (NP (PRP I))
              (VP (VBD posted)
                (SBAR
                  (S
                    (NP (PRP mine)
                      (CC and)
                      (PRP I))
                    (VP (VBD did)
                      (VP (`` ``) (VB ignore) ('' '')
                        (NP (PRP$ your) (NN comment)))))))))))
      (SBAR
        (ADVP (RB essentially))
        (IN because)
        (S
          (NP (PRP you))
          (VP (VBD changed)
            (NP (DT the) (NN topic))))))
    (. .)))

