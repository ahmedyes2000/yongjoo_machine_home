(ROOT
  (S
    (NP (NNP Religion))
    (ADVP (RB never))
    (VP
      (VP (VBZ has))
      (, ,)
      (CC and)
      (ADVP (IN in) (NN fact))
      (, ,)
      (VP (VBZ has)
        (ADVP (RB only))
        (VP
          (ADVP (RB ever))
          (VBN acted)
          (PP (IN against)
            (NP (PRP it))))))
    (. .)))

nsubj(has-3, Religion-1)
nsubj(acted-12, Religion-1)
neg(has-3, never-2)
root(ROOT-0, has-3)
advmod(has-3, in-6)
conj_and(has-3, in-6)
dep(in-6, fact-7)
aux(acted-12, has-9)
advmod(acted-12, only-10)
advmod(acted-12, ever-11)
conj_and(has-3, acted-12)
prep_against(acted-12, it-14)

