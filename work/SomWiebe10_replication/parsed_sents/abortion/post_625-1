(ROOT
  (S
    (VP (VB Non)
      (ADJP (RB so) (JJ blind))
      (PP (IN as)
        (NP
          (NP (DT those))
          (SBAR
            (WHNP (WP who))
            (S
              (VP
                (VP (VBP have)
                  (NP (NNS eyes)))
                (CC but)
                (VP (MD can) (RB not)
                  (VP (VB see)))))))))
    (. .)))

root(ROOT-0, Non-1)
advmod(blind-3, so-2)
acomp(Non-1, blind-3)
prep_as(Non-1, those-5)
nsubj(have-7, those-5)
nsubj(see-12, those-5)
rcmod(those-5, have-7)
dobj(have-7, eyes-8)
aux(see-12, can-10)
neg(see-12, not-11)
rcmod(those-5, see-12)
conj_but(have-7, see-12)

