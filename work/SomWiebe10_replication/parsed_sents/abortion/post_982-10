(ROOT
  (S
    (NP (DT The) (JJ same))
    (VP
      (VP (VBZ goes)
        (PP (IN for)
          (NP (NNS gays))))
      (CONJP (RB as) (RB well))
      (VP (VBG concerning)
        (NP (NN harassment))))
    (. .)))

det(same-2, The-1)
nsubj(goes-3, same-2)
nsubj(concerning-8, same-2)
root(ROOT-0, goes-3)
prep_for(goes-3, gays-5)
conj_and(goes-3, concerning-8)
dobj(concerning-8, harassment-9)

