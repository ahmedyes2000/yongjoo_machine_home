(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is)
      (NP (DT a) (NN crime)
        (S
          (VP (TO to)
            (VP (VB sterilize)
              (NP (NN someone))
              (PP (IN against)
                (NP (PRP$ their) (NN will))))))))
    (. .)))

nsubj(crime-4, It-1)
cop(crime-4, is-2)
det(crime-4, a-3)
root(ROOT-0, crime-4)
aux(sterilize-6, to-5)
infmod(crime-4, sterilize-6)
dobj(sterilize-6, someone-7)
poss(will-10, their-9)
prep_against(sterilize-6, will-10)

