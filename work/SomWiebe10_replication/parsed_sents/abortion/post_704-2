  (S
    (PP (IN In)
      (NP
        (NP (DT the) (JJ unbroken) (NN chain))
        (PP (IN of)
          (NP (NN descent)))))
    (NP (EX there))
    (VP (VBP are)
      (NP
        (NP (DT a) (NN series))
        (PP (IN of)
          (NP
            (NP (JJ miraculous) (NNS transformations))
            (, ,)
            (SBAR
              (WHNP (CD one)
                (WHPP (IN of)
                  (WHNP (WDT which))))
              (S
                (VP (VBZ is)
                  (NP (NN conception)))))))))
    (. .)))

