(ROOT
  (S
    (NP (EX There))
    (VP (VBP are)
      (NP
        (NP (NNS millions))
        (PP (IN of)
          (NP
            (NP (NNS women))
            (VP (VBG wanting)
              (S
                (VP (TO to)
                  (VP
                    (VP (VB have)
                      (NP (NNS babies)))
                    (CC and)
                    (VP (VBP are)
                      (VP (VBG facing)
                        (NP (JJ real) (NNS obstacles)
                          (CC and)
                          (NNS traumas))
                        (PP (IN as)
                          (NP
                            (NP (DT the) (NNS numbers))
                            (PP (IN above)
                              (NP (NN show)))))))))))))))
    (. .)))

expl(are-2, There-1)
root(ROOT-0, are-2)
nsubj(are-2, millions-3)
prep_of(millions-3, women-5)
xsubj(have-8, women-5)
partmod(women-5, wanting-6)
aux(have-8, to-7)
xcomp(wanting-6, have-8)
dobj(have-8, babies-9)
aux(facing-12, are-11)
xcomp(wanting-6, facing-12)
conj_and(have-8, facing-12)
amod(obstacles-14, real-13)
dobj(facing-12, obstacles-14)
dobj(facing-12, traumas-16)
conj_and(obstacles-14, traumas-16)
det(numbers-19, the-18)
prep_as(facing-12, numbers-19)
prep_above(numbers-19, show-21)

