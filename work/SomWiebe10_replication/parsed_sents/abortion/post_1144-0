      (NP
        (QP (RB Only) (CD 1))
        (NN %))
      (PP (IN of)
        (NP
          (NP (DT all) (NNS abortions))
          (PP (IN in)
            (NP (DT the) (NNP United) (NNPS States))))))
    (VP (VBP are)
      (NP
        (NP (DT a) (NN result))
        (PP (IN of)
          (NP (NN rape)
            (CC or)
            (NN incest)))))
    (. .)))

quantmod(1-2, Only-1)
num(%-3, 1-2)
nsubj(result-13, %-3)
det(abortions-6, all-5)
