(ROOT
  (S
    (ADVP (RB However))
    (, ,)
    (NP (PRP I))
    (VP (VBP do)
      (VP (VB understand)
        (NP (DT the) (NN need))
        (PP (IN for)
          (S
            (VP (VBG allowing)
              (NP
                (NP (NNS abortions))
                (, ,) (RB hence)
                (NP (DT a) (NN solution)))
              (PP (VBN based)
                (PP (IN on)
                  (NP (NN compromise)))))))))
    (. .)))

advmod(understand-5, However-1)
nsubj(understand-5, I-3)
aux(understand-5, do-4)
root(ROOT-0, understand-5)
det(need-7, the-6)
dobj(understand-5, need-7)
prepc_for(understand-5, allowing-9)
dobj(allowing-9, abortions-10)
