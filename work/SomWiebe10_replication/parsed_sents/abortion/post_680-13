  (S
    (NP
      (NP (DT This) (NN business))
      (PP (IN about)
        (NP
          (NP (JJ human) (NN life))
          (PP (IN with)
            (NP (DT the) (NN potential)
              (S
                (VP
                  (VP (TO to)
                    (VP (VB be)
                      (ADJP (RB fully) (JJ human))))
                  (CC or)
                  (VP (TO to)
                    (VP (VB be)
                      (NP (DT a) (JJ human) (NN being)))))))))))
    (VP (VBZ seems)
      (PP (IN from)
        (NP (PRP$ my) (NN POV)
          (S
            (VP (TO to)
              (VP (VB be)
                (ADJP
                  (ADVP (RB strictly))
                  (JJ applicable)
                  (PP (TO to)
                    (NP (CD one) (NNS species))))))))))
    (. .)))

