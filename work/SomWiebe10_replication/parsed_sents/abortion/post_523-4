(ROOT
  (S
    (NP
      (NP (NN life))
      (PP (IN without)
        (NP (NN liberty))))
    (VP (VBZ is)
      (NP (DT a) (JJ poor) (, ,) (JJ sad) (NN life)))
    (. .)))

nsubj(life-9, life-1)
prep_without(life-1, liberty-3)
cop(life-9, is-4)
det(life-9, a-5)
amod(life-9, poor-6)
amod(life-9, sad-8)
root(ROOT-0, life-9)

