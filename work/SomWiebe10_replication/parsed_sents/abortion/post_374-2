(ROOT
  (S
    (S (IN So)
      (NP (PRP you))
      (VP (MD should)
        (VP (VB be)
          (VP (VBG addressing)
            (NP
              (NP (DT the) (NN author))
              (PP (IN of)
                (NP (DT the) (JJ original) (NN post))))))))
    (, ,)
    (CC but)
    (S
      (ADVP (RB then))
      (NP (PRP you))
      (VP (MD would)
        (VP (VB be)
          (VP (VBG making)
            (NP (PRP$ my) (NN case))))))
    (. .)))

dep(addressing-5, So-1)
nsubj(addressing-5, you-2)
aux(addressing-5, should-3)
aux(addressing-5, be-4)
