(ROOT
  (S
    (SBAR (IN If)
      (S
        (NP (DT the) (NN person))
        (VP (MD can) (RB not)
          (VP (VB be)
            (VP (VBN isolated)
              (PP (IN from)
                (NP (NN society))))))))
    (, ,)
    (NP
      (NP (RB then) (DT the) (NN value))
      (PP (IN of)
        (NP
          (NP (DT the) (JJ many) (NNS lives))
          (PP (IN in)
            (NP (NN society))))))
    (VP (VBZ needs)
      (S
        (VP (TO to)
          (VP (VB have)
            (NP (JJR greater) (NN weight))
            (SBAR (IN than)
              (S
                (NP (DT the) (CD one) (NN person))
                (VP (VBZ s)
                  (NP (NN life)))))))))
    (. .)))

