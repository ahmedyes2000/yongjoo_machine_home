  (S
    (ADVP (RB Instead))
    (, ,)
    (NP (DT the) (NN solution))
    (VP (VBZ is)
      (S
        (VP (TO to)
          (VP (VB legalize)
            (NP
              (NP (DT all) (NNS forms))
              (PP (IN of)
                (NP (NN abortion))))))))
    (. .)))

advmod(is-5, Instead-1)
det(solution-4, the-3)
nsubj(is-5, solution-4)
xsubj(legalize-7, solution-4)
root(ROOT-0, is-5)
aux(legalize-7, to-6)
