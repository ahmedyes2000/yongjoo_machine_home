(ROOT
  (S
    (NP (PRP It))
    (VP (VBZ is)
      (ADJP (JJ true))
      (SBAR (IN that)
        (S
          (NP
            (NP (NNS laws))
            (VP (VBN enacted)
              (S
                (VP (TO to)
                  (VP (VB protect)
                    (NP (NN privacy) (NNS rights))
                    (ADVP (RB also)))))))
          (VP (VBP provide)
            (NP
              (NP (NN opportunity))
              (PP (IN for)
                (NP (JJ secret) (NN wrongdoing))))))))
    (. .)))

nsubj(true-3, It-1)
cop(true-3, is-2)
root(ROOT-0, true-3)
complm(provide-12, that-4)
xsubj(protect-8, laws-5)
nsubj(provide-12, laws-5)
partmod(laws-5, enacted-6)
aux(protect-8, to-7)
xcomp(enacted-6, protect-8)
nn(rights-10, privacy-9)
dobj(protect-8, rights-10)
advmod(protect-8, also-11)
ccomp(true-3, provide-12)
dobj(provide-12, opportunity-13)
amod(wrongdoing-16, secret-15)
prep_for(opportunity-13, wrongdoing-16)

