(ROOT
  (S
    (S
      (NP (NNS Soldiers))
      (VP (VBP are)
        (ADJP (JJ combatants))))
    (CC and)
    (S
      (ADVP (RB sometimes))
      (NP (PRP they))
      (VP (VBP kill)
        (NP (DT each) (JJ other))))
    (. .)))

nsubj(combatants-3, Soldiers-1)
cop(combatants-3, are-2)
root(ROOT-0, combatants-3)
advmod(kill-7, sometimes-5)
nsubj(kill-7, they-6)
conj_and(combatants-3, kill-7)
det(other-9, each-8)
dobj(kill-7, other-9)

