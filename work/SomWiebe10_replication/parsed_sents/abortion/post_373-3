    (PP (IN With)
      (NP (PRP$ my) (NN age) (NN group)))
    (, ,)
    (S
      (NP (DT that))
      (VP (VBZ is)
        (ADJP (RB so) (VBN gone))))
    (, ,)
    (CC and)
    (S
      (NP (PRP we))
      (VP (VBP are)
        (ADVP (RB so) (RB much))
        (ADVP (RB together))
        (PP (IN on)
          (NP (DT a) (JJ common) (NN ground)))))
    (. .)))

