(ROOT
  (S (CC and)
    (NP (DT this))
    (ADVP (RB then))
    (VP (VBZ applies)
      (PP (TO to)
        (NP
          (NP (DT all) (NNS people))
          (PP (VBG including)
            (NP
              (NP (DT the) (NN life))
              (SBAR (IN that)
                (S
                  (NP (NN freedom))
                  (VP (VBD was)
                    (VP (VBN tossed)
                      (PRT (RP aside))
                      (PP (IN for)))))))))))
    (. .)))

cc(applies-4, and-1)
nsubj(applies-4, this-2)
