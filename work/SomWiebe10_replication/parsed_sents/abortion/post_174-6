(ROOT
  (S
    (NP (PRP I))
    (ADVP (RB also))
    (VP (VBP think)
      (SBAR
        (S
          (NP
            (NP (DT the) (NN statement))
            (PP (IN by)
              (NP (DT the) (NNS bishops))))
          (VP (MD will)
            (VP (VB answer)
              (NP
                (NP (DT some))
                (PP (IN of)
                  (NP
                    (NP (PRP$ your) (NNS questions))
                    (VP (VBN raised)
                      (PP (IN in)
                        (NP (PRP$ your) (NN reply)))
                      (PP (TO to)
                        (NP (PRP me))))))))))))
    (. .)))

nsubj(think-3, I-1)
advmod(think-3, also-2)
root(ROOT-0, think-3)
det(statement-5, the-4)
nsubj(answer-10, statement-5)
det(bishops-8, the-7)
prep_by(statement-5, bishops-8)
aux(answer-10, will-9)
ccomp(think-3, answer-10)
dobj(answer-10, some-11)
poss(questions-14, your-13)
prep_of(some-11, questions-14)
partmod(questions-14, raised-15)
poss(reply-18, your-17)
prep_in(raised-15, reply-18)
prep_to(raised-15, me-20)

