(ROOT
  (S
    (NP (NNP Abstinence))
    (ADVP (RB only))
    (VP (VBD worked)
      (ADVP (RB well))
      (PP (IN in)
        (NP
          (NP (DT this) (NN country))
          (PP (IN for)
            (NP (NNP awhile)))))
      (S
        (VP (TO to)
          (VP (VB curb)
            (NP (DT these) (NNS problems))))))
    (. .)))

nsubj(worked-3, Abstinence-1)
xsubj(curb-11, Abstinence-1)
advmod(worked-3, only-2)
root(ROOT-0, worked-3)
advmod(worked-3, well-4)
det(country-7, this-6)
