(ROOT
  (S
    (ADVP (RB Also))
    (, ,)
    (NP (PRP I))
    (VP (VBP need)
      (S
        (VP (TO to)
          (VP (VB know)
            (SBAR
              (WHNP (WP what))
              (S
                (NP
                  (NP (PRP$ your) (NN definition))
                  (PP (IN of)
                    (S
                      (VP (VBG judging)))))
                (VP (VBZ is) (, ,)
                  (SBAR (IN since)
                    (S
                      (NP (PRP you))
                      (VP (VBP disagree)
                        (PP (IN with)
                          (NP (NN mine)))))))))))))
    (. .)))

