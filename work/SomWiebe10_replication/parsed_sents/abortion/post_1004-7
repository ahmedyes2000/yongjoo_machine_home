(ROOT
  (S
    (NP (DT No) (NN one))
    (VP (VBZ is)
      (PP (IN for)
        (NP (NN abortion)))
      (SBAR (IN once)
        (S
          (NP (DT the) (NN child))
          (VP (VBZ is)
            (ADJP (JJ self-aware))))))
    (. .)))

det(one-2, No-1)
nsubj(is-3, one-2)
root(ROOT-0, is-3)
prep_for(is-3, abortion-5)
mark(self-aware-10, once-6)
det(child-8, the-7)
nsubj(self-aware-10, child-8)
cop(self-aware-10, is-9)
advcl(is-3, self-aware-10)

