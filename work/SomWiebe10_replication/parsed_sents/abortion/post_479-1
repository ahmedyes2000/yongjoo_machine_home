(ROOT
  (S
    (NP (NNP THAT))
    (VP (MD should)
      (ADVP (RB never))
      (VP (VB be)
        (VP (VBN allowed)
          (PP (IN in)
            (NP (DT a) (JJ true) (NN democracy))))))
    (. .)))

nsubjpass(allowed-5, THAT-1)
aux(allowed-5, should-2)
neg(allowed-5, never-3)
auxpass(allowed-5, be-4)
root(ROOT-0, allowed-5)
det(democracy-9, a-7)
amod(democracy-9, true-8)
prep_in(allowed-5, democracy-9)

