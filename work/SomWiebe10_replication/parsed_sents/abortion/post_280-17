(ROOT
  (S
    (ADVP (RB Therefore))
    (, ,)
    (NP
      (NP (DT those))
      (SBAR
        (WHNP (WP who))
        (S
          (VP (VBP perform)
            (NP (NNS abortions))))))
    (VP (VBP are) (RB not)
      (NP (NNS murderers)))
    (. .)))

advmod(murderers-9, Therefore-1)
nsubj(perform-5, those-3)
nsubj(murderers-9, those-3)
rcmod(those-3, perform-5)
dobj(perform-5, abortions-6)
cop(murderers-9, are-7)
neg(murderers-9, not-8)
root(ROOT-0, murderers-9)

