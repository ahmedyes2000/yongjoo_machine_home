(ROOT
  (S
    (NP (PRP I))
    (VP (VBP 've)
      (ADVP (RB always))
      (VP (VBN found)
        (SBAR (IN that)
          (S
            (NP (DT a) (NN bit))
            (ADJP (JJ odd))))))
    (. .)))

nsubj(found-4, I-1)
aux(found-4, 've-2)
advmod(found-4, always-3)
root(ROOT-0, found-4)
complm(odd-8, that-5)
det(bit-7, a-6)
dep(odd-8, bit-7)
dep(found-4, odd-8)

